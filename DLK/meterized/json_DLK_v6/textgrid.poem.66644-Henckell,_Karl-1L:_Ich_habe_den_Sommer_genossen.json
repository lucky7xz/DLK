{"textgrid.poem.66644": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe den Sommer genossen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe den Sommer genossen", "tokens": ["Ich", "ha\u00b7be", "den", "Som\u00b7mer", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In selbstgen\u00fcgsamer Ruh,", "tokens": ["In", "selbst\u00b7ge\u00b7n\u00fcg\u00b7sa\u00b7mer", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den roten Vorhang geschlossen,", "tokens": ["Den", "ro\u00b7ten", "Vor\u00b7hang", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dr\u00e4ngte die Sonne sich zu.", "tokens": ["Dr\u00e4ng\u00b7te", "die", "Son\u00b7ne", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Verstohlen spielten die Strahlen", "tokens": ["Ver\u00b7stoh\u00b7len", "spiel\u00b7ten", "die", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum blinkenden Ofenring,", "tokens": ["Zum", "blin\u00b7ken\u00b7den", "O\u00b7fen\u00b7ring", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf rosaroten Sandalen", "tokens": ["Auf", "ro\u00b7sa\u00b7ro\u00b7ten", "San\u00b7da\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Sonne durchs Zimmer ging.", "tokens": ["Die", "Son\u00b7ne", "durchs", "Zim\u00b7mer", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So lag ich in stillender K\u00fchle,", "tokens": ["So", "lag", "ich", "in", "stil\u00b7len\u00b7der", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So lag ich in reifender Rast", "tokens": ["So", "lag", "ich", "in", "rei\u00b7fen\u00b7der", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Auf golddurchwobenem Pf\u00fchle", "tokens": ["Auf", "gold\u00b7durch\u00b7wo\u00b7be\u00b7nem", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und habe mich selber erfa\u00dft.", "tokens": ["Und", "ha\u00b7be", "mich", "sel\u00b7ber", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ich habe gelauscht und getrunken", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7lauscht", "und", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Meiner Jugend wirbelnden Gischt,", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "wir\u00b7beln\u00b7den", "Gischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Da ist es in sch\u00e4umenden Funken", "tokens": ["Da", "ist", "es", "in", "sch\u00e4u\u00b7men\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Noch einmal emporgezischt.", "tokens": ["Noch", "ein\u00b7mal", "em\u00b7por\u00b7ge\u00b7zischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "All mein verwegenes Wollen,", "tokens": ["All", "mein", "ver\u00b7we\u00b7ge\u00b7nes", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "All mein messianischer Mut,", "tokens": ["All", "mein", "mes\u00b7si\u00b7a\u00b7ni\u00b7scher", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Rollen h\u00f6rt' ich und grollen", "tokens": ["Rol\u00b7len", "h\u00f6rt'", "ich", "und", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die gl\u00fchende Willensflut ...", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7de", "Wil\u00b7lens\u00b7flut", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich habe den Sommer genossen", "tokens": ["Ich", "ha\u00b7be", "den", "Som\u00b7mer", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In selbstgen\u00fcgsamer Ruh,", "tokens": ["In", "selbst\u00b7ge\u00b7n\u00fcg\u00b7sa\u00b7mer", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den roten Vorhang geschlossen,", "tokens": ["Den", "ro\u00b7ten", "Vor\u00b7hang", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dr\u00e4ngte die Sonne sich zu.", "tokens": ["Dr\u00e4ng\u00b7te", "die", "Son\u00b7ne", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Nun sch\u00fcttet kristallene Bl\u00e4ue", "tokens": ["Nun", "sch\u00fct\u00b7tet", "kris\u00b7tal\u00b7le\u00b7ne", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herbst in mein offnes Gemach,", "tokens": ["Der", "Herbst", "in", "mein", "off\u00b7nes", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schwebend in eigener Treue", "tokens": ["Schwe\u00b7bend", "in", "ei\u00b7ge\u00b7ner", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schau' ich der sinkenden Sonne nach.", "tokens": ["Schau'", "ich", "der", "sin\u00b7ken\u00b7den", "Son\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}