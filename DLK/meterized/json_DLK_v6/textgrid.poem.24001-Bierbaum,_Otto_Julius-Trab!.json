{"textgrid.poem.24001": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Trab!", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern wohl m\u00f6chte mich die Braune.", "tokens": ["Gern", "wohl", "m\u00f6ch\u00b7te", "mich", "die", "Brau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch ich soll erst karressieren,", "tokens": ["Doch", "ich", "soll", "erst", "kar\u00b7res\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Redebutterbr\u00f6de schmieren;", "tokens": ["Re\u00b7de\u00b7but\u00b7ter\u00b7br\u00f6\u00b7de", "schmie\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dazu hab ich keine Laune.", "tokens": ["Da\u00b7zu", "hab", "ich", "kei\u00b7ne", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm und k\u00fc\u00df und sei vern\u00fcnftig,", "tokens": ["Komm", "und", "k\u00fc\u00df", "und", "sei", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "KON", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiel nicht lange erst die Spr\u00f6de!", "tokens": ["Spiel", "nicht", "lan\u00b7ge", "erst", "die", "Spr\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nste Schmeichelbutterbr\u00f6de", "tokens": ["Sch\u00f6ns\u00b7te", "Schmei\u00b7chel\u00b7but\u00b7ter\u00b7br\u00f6\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch mehr bekommst du k\u00fcnftig.", "tokens": ["Und", "noch", "mehr", "be\u00b7kommst", "du", "k\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}