{"textgrid.poem.43138": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl k\u00f6nnt Ihr m\u00e4keln jetzt an Wort und That,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl k\u00f6nnt Ihr m\u00e4keln jetzt an Wort und That,", "tokens": ["Wohl", "k\u00f6nnt", "Ihr", "m\u00e4\u00b7keln", "jetzt", "an", "Wort", "und", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt mich verdammen, seht, es r\u00fchrt mich nimmer;", "tokens": ["K\u00f6nnt", "mich", "ver\u00b7dam\u00b7men", ",", "seht", ",", "es", "r\u00fchrt", "mich", "nim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVINF", "$,", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hasche nicht nach Eurem feilen Rath", "tokens": ["Ich", "ha\u00b7sche", "nicht", "nach", "Eu\u00b7rem", "fei\u00b7len", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und morscher Tugend fahlen Moderschimmer!", "tokens": ["Und", "mor\u00b7scher", "Tu\u00b7gend", "fah\u00b7len", "Mo\u00b7der\u00b7schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich trachte nimmermehr nach Eurer Lieb',", "tokens": ["Ich", "trach\u00b7te", "nim\u00b7mer\u00b7mehr", "nach", "Eu\u00b7rer", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich werde liebearm und einsam schreiten,", "tokens": ["Ich", "wer\u00b7de", "lie\u00b7be\u00b7arm", "und", "ein\u00b7sam", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch jene Waffe, die einst fort mich trieb,", "tokens": ["Doch", "je\u00b7ne", "Waf\u00b7fe", ",", "die", "einst", "fort", "mich", "trieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "ADV", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie wird nun stumpf von meinem Panzer gleiten.", "tokens": ["Sie", "wird", "nun", "stumpf", "von", "mei\u00b7nem", "Pan\u00b7zer", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O, ich war elend! \u2013 jeder b\u00f6se Zug", "tokens": ["O", ",", "ich", "war", "e\u00b7lend", "!", "\u2013", "je\u00b7der", "b\u00f6\u00b7se", "Zug"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$.", "$(", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In Euren kalten Larven mahnt mich wieder,", "tokens": ["In", "Eu\u00b7ren", "kal\u00b7ten", "Lar\u00b7ven", "mahnt", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie Jeder von Euch t\u00fcckisch nach mir schlug,", "tokens": ["Wie", "Je\u00b7der", "von", "Euch", "t\u00fc\u00b7ckisch", "nach", "mir", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In mir vernichtet meine reinsten Lieder!", "tokens": ["In", "mir", "ver\u00b7nich\u00b7tet", "mei\u00b7ne", "reins\u00b7ten", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}