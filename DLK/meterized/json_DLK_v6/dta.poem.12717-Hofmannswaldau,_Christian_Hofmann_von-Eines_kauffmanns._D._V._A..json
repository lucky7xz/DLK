{"dta.poem.12717": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines kauffmanns.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Omonat voller angst! der sonst Augustus heist,", "tokens": ["O\u00b7mo\u00b7nat", "vol\u00b7ler", "angst", "!", "der", "sonst", "Au\u00b7gus\u00b7tus", "heist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur die gr\u00e4ber mehrt! man k\u00f6nnt\u2019 ihn anders nennen,", "tokens": ["Und", "nur", "die", "gr\u00e4\u00b7ber", "mehrt", "!", "man", "k\u00f6nnt'", "ihn", "an\u00b7ders", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und vor den angst-monath mit gutem recht erkennen;", "tokens": ["Und", "vor", "den", "angst\u00b7mo\u00b7nath", "mit", "gu\u00b7tem", "recht", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00f6rt aber, wie es uns Herr \u2012 \u2012 itzt verweist:", "tokens": ["H\u00f6rt", "a\u00b7ber", ",", "wie", "es", "uns", "Herr", "\u2012", "\u2012", "itzt", "ver\u00b7weist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "PPER", "PRF", "NN", "$(", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mir, spricht er, hat August nichts als die angst genommen,", "tokens": ["Mir", ",", "spricht", "er", ",", "hat", "Au\u00b7gust", "nichts", "als", "die", "angst", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VAFIN", "NN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum la\u00dft ihm, was er hat vom K\u00e4yser selbst bekommen.", "tokens": ["Drum", "la\u00dft", "ihm", ",", "was", "er", "hat", "vom", "K\u00e4y\u00b7ser", "selbst", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}