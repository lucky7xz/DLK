{"textgrid.poem.48741": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "28. An den Flu\u00df Moskaw, als er schiede", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fleu\u00df sanfte, wie du tust, in beiden Ufern hin,", "tokens": ["Fleu\u00df", "sanf\u00b7te", ",", "wie", "du", "tust", ",", "in", "bei\u00b7den", "U\u00b7fern", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "fleu\u00df deine Stadt vorbei, die gro\u00dfe, die gepreiste,", "tokens": ["fleu\u00df", "dei\u00b7ne", "Stadt", "vor\u00b7bei", ",", "die", "gro\u00b7\u00dfe", ",", "die", "ge\u00b7preis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die nun das ander Mal sich uns so gut erweiste,", "tokens": ["die", "nun", "das", "an\u00b7der", "Mal", "sich", "uns", "so", "gut", "er\u00b7weis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "PRF", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "durch welcher Urlaub wir nun in den Aufgang ziehn,", "tokens": ["durch", "wel\u00b7cher", "Ur\u00b7laub", "wir", "nun", "in", "den", "Auf\u00b7gang", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Verbleib' ich so gesund, als wie ich itzo bin,", "tokens": ["Ver\u00b7bleib'", "ich", "so", "ge\u00b7sund", ",", "als", "wie", "ich", "it\u00b7zo", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "$,", "KOUS", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und komm' ich wieder heim, als wie ich ab verreiste,", "tokens": ["und", "komm'", "ich", "wie\u00b7der", "heim", ",", "als", "wie", "ich", "ab", "ver\u00b7reis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PWAV", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so sei dirs zugesagt mit Mund und ganzem Geiste,", "tokens": ["so", "sei", "dirs", "zu\u00b7ge\u00b7sagt", "mit", "Mund", "und", "gan\u00b7zem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da solt mir nimmermehr nicht kommen aus dem Sinn'.", "tokens": ["da", "solt", "mir", "nim\u00b7mer\u00b7mehr", "nicht", "kom\u00b7men", "aus", "dem", "Sinn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich will dich so bekant als meine ", "tokens": ["Ich", "will", "dich", "so", "be\u00b7kant", "als", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die itzund \u00fcber mir nicht allzusehr wird lachen,", "tokens": ["die", "it\u00b7zund", "\u00fc\u00b7ber", "mir", "nicht", "all\u00b7zu\u00b7sehr", "wird", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PTKNEG", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "weil ich fast nicht denk heim, ein halbverlorner Sohn.", "tokens": ["weil", "ich", "fast", "nicht", "denk", "heim", ",", "ein", "halb\u00b7ver\u00b7lor\u00b7ner", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nim diese Hand voll Klee, im Mangel der Violen,", "tokens": ["Nim", "die\u00b7se", "Hand", "voll", "Klee", ",", "im", "Man\u00b7gel", "der", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADJD", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu treuen G\u00fcnsten an. Ich dichte schon den Ton.", "tokens": ["zu", "treu\u00b7en", "G\u00fcns\u00b7ten", "an", ".", "Ich", "dich\u00b7te", "schon", "den", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lauf, Erato, alsbald, die Zither herzuholen!", "tokens": ["Lauf", ",", "E\u00b7ra\u00b7to", ",", "als\u00b7bald", ",", "die", "Zi\u00b7ther", "her\u00b7zu\u00b7ho\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}