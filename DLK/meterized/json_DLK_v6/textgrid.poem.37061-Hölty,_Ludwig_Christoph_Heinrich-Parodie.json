{"textgrid.poem.37061": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Parodie", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets wohne Gleichmuth, wohne Zufriedenheit", "tokens": ["Stets", "woh\u00b7ne", "Gleich\u00b7muth", ",", "woh\u00b7ne", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deiner Seele, wann dir der Recensent", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", ",", "wann", "dir", "der", "Re\u00b7cen\u00b7sent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Weihrauchk\u00f6rnchen streuet, oder", "tokens": ["Ein", "Weih\u00b7rauch\u00b7k\u00f6rn\u00b7chen", "streu\u00b7et", ",", "o\u00b7der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sp\u00f6ttischen Tadel und Grobheit ausstr\u00f6mt.", "tokens": ["Sp\u00f6t\u00b7ti\u00b7schen", "Ta\u00b7del", "und", "Grob\u00b7heit", "aus\u00b7str\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Des ernsten Weisen, welcher bis an den Bart", "tokens": ["Des", "erns\u00b7ten", "Wei\u00b7sen", ",", "wel\u00b7cher", "bis", "an", "den", "Bart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In B\u00fcchern sitzet, Bogen auf Bogen schreibt,", "tokens": ["In", "B\u00fc\u00b7chern", "sit\u00b7zet", ",", "Bo\u00b7gen", "auf", "Bo\u00b7gen", "schreibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des T\u00e4ndlers, und des frohen Zechers,", "tokens": ["Des", "T\u00e4nd\u00b7lers", ",", "und", "des", "fro\u00b7hen", "Ze\u00b7chers", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Harret die Klaue des Knochenmannes.", "tokens": ["Har\u00b7ret", "die", "Klau\u00b7e", "des", "Kno\u00b7chen\u00b7man\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Drum la\u00df ins Zimmer, wo dir der Ofen und", "tokens": ["Drum", "la\u00df", "ins", "Zim\u00b7mer", ",", "wo", "dir", "der", "O\u00b7fen", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "KON"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Lehnstuhl winken, blauer Wacholderduft", "tokens": ["Der", "Lehn\u00b7stuhl", "win\u00b7ken", ",", "blau\u00b7er", "Wa\u00b7chol\u00b7der\u00b7duft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Rauchfa\u00df str\u00f6mt, und Fr\u00fchlingsscenen,", "tokens": ["Vom", "Rauch\u00b7fa\u00df", "str\u00f6mt", ",", "und", "Fr\u00fch\u00b7lings\u00b7sce\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u00f6gel und Blumen die W\u00e4nde schm\u00fccken,", "tokens": ["V\u00f6\u00b7gel", "und", "Blu\u00b7men", "die", "W\u00e4n\u00b7de", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dir Knasterrollen, Pfeifen und Fidibus,", "tokens": ["Dir", "Knas\u00b7ter\u00b7rol\u00b7len", ",", "Pfei\u00b7fen", "und", "Fi\u00b7di\u00b7bus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Trunke bringen, den die Levante zeugt,", "tokens": ["Zum", "Trun\u00b7ke", "brin\u00b7gen", ",", "den", "die", "Le\u00b7van\u00b7te", "zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bevor die Parce deinen Faden,", "tokens": ["Be\u00b7vor", "die", "Par\u00b7ce", "dei\u00b7nen", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mitten im Fluge der Spindel, k\u00fcrzet.", "tokens": ["Mit\u00b7ten", "im", "Flu\u00b7ge", "der", "Spin\u00b7del", ",", "k\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dann wird die theure Bibliotheck verkauft,", "tokens": ["Dann", "wird", "die", "theu\u00b7re", "Bib\u00b7liot\u00b7heck", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die zentnerschweren B\u00fccher in Folio,", "tokens": ["Die", "zent\u00b7ner\u00b7schwe\u00b7ren", "B\u00fc\u00b7cher", "in", "Fo\u00b7lio", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Dichter, die mit goldnen Titeln,", "tokens": ["Die", "Dich\u00b7ter", ",", "die", "mit", "gold\u00b7nen", "Ti\u00b7teln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldenen Blumen, und Schnitten prangen.", "tokens": ["Gol\u00b7de\u00b7nen", "Blu\u00b7men", ",", "und", "Schnit\u00b7ten", "pran\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Des Todes Sichel m\u00e4het dein Leben ab,", "tokens": ["Des", "To\u00b7des", "Si\u00b7chel", "m\u00e4\u00b7het", "dein", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du magst mit Klopstocks Schwunge der Ewigkeit", "tokens": ["Du", "magst", "mit", "Klops\u00b7tocks", "Schwun\u00b7ge", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entgegen fliegen, oder braunem", "tokens": ["Ent\u00b7ge\u00b7gen", "flie\u00b7gen", ",", "o\u00b7der", "brau\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pfeffer und W\u00fcrze zur H\u00fclle dienen.", "tokens": ["Pfef\u00b7fer", "und", "W\u00fcr\u00b7ze", "zur", "H\u00fcl\u00b7le", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "O Freund, der Pre\u00dfen Ewigkeit ist ein Traum,", "tokens": ["O", "Freund", ",", "der", "Pre\u00b7\u00dfen", "E\u00b7wig\u00b7keit", "ist", "ein", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Schicksal st\u00fcrzet, fr\u00fch oder sp\u00e4t, das Lied", "tokens": ["Das", "Schick\u00b7sal", "st\u00fcr\u00b7zet", ",", "fr\u00fch", "o\u00b7der", "sp\u00e4t", ",", "das", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des schalen Reimers, und des Dichters", "tokens": ["Des", "scha\u00b7len", "Rei\u00b7mers", ",", "und", "des", "Dich\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der Verge\u00dfenheit Nacht hinunter.", "tokens": ["In", "der", "Ver\u00b7ge\u00b7\u00dfen\u00b7heit", "Nacht", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}