{"textgrid.poem.46358": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Daphne", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist der nahe Wald", "tokens": ["Es", "ist", "der", "na\u00b7he", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Musen Aufenthalt;", "tokens": ["Der", "Mu\u00b7sen", "Auf\u00b7ent\u00b7halt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist die Wiese hier", "tokens": ["Es", "ist", "die", "Wie\u00b7se", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Flora Lustrevier;", "tokens": ["Der", "Flo\u00b7ra", "Lus\u00b7tre\u00b7vier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sch\u00f6n singt im tiefen Thal", "tokens": ["Sch\u00f6n", "singt", "im", "tie\u00b7fen", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Lied die Nachtigal;", "tokens": ["Ihr", "Lied", "die", "Nach\u00b7ti\u00b7gal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sch\u00f6n flie\u00dft die Havel dort", "tokens": ["Sch\u00f6n", "flie\u00dft", "die", "Ha\u00b7vel", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NE", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Um kleine Inseln fort;", "tokens": ["Um", "klei\u00b7ne", "In\u00b7seln", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch nichts d\u00fcnkt mir so sch\u00f6n", "tokens": ["Doch", "nichts", "d\u00fcnkt", "mir", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Als jene heitre H\u00f6hn,", "tokens": ["Als", "je\u00b7ne", "heit\u00b7re", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da seh ich Daphnen gehn.", "tokens": ["Da", "seh", "ich", "Daph\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}