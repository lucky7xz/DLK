{"textgrid.poem.50682": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Eitle Dankbarkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df f\u00fcr erwies'ne Dienst' oft Kremon mich gepriesen,", "tokens": ["Da\u00df", "f\u00fcr", "er\u00b7wies'\u00b7ne", "Dienst'", "oft", "Kre\u00b7mon", "mich", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist keine Dankbarkeit und nichts, als Gaukelei:", "tokens": ["Ist", "kei\u00b7ne", "Dank\u00b7bar\u00b7keit", "und", "nichts", ",", "als", "Gau\u00b7ke\u00b7lei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "PIS", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er trachtet, darzuthun, was ", "tokens": ["Er", "trach\u00b7tet", ",", "dar\u00b7zu\u00b7thun", ",", "was"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVIZU", "$,", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr, da\u00df ", "tokens": ["Mehr", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["PIS", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}}}}}