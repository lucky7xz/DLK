{"dta.poem.312": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie gestorben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Grausahmes Verh\u00e4ngni\u00df! h\u00f6llische Pein/", "tokens": ["Grau\u00b7sah\u00b7mes", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "h\u00f6l\u00b7li\u00b7sche", "Pein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00df ich ein Ballen des Gl\u00fcckes denn seyn?", "tokens": ["Mu\u00df", "ich", "ein", "Bal\u00b7len", "des", "Gl\u00fc\u00b7ckes", "denn", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "KON", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der bald erh\u00f6ht zur blauen Zinnen steiget/", "tokens": ["Der", "bald", "er\u00b7h\u00f6ht", "zur", "blau\u00b7en", "Zin\u00b7nen", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald zu der Grufft der H\u00f6llen sich hinneiget.", "tokens": ["Bald", "zu", "der", "Grufft", "der", "H\u00f6l\u00b7len", "sich", "hin\u00b7nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ha! strenges Gl\u00fcck", "tokens": ["Ha", "!", "stren\u00b7ges", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch deine T\u00fcck", "tokens": ["Durch", "dei\u00b7ne", "T\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ist mir geraubet worden/", "tokens": ["Ist", "mir", "ge\u00b7rau\u00b7bet", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein halbes Hertz/", "tokens": ["Mein", "hal\u00b7bes", "Hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So da\u00df der Schmertz", "tokens": ["So", "da\u00df", "der", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Mich bringt ins Todes-Orden.", "tokens": ["Mich", "bringt", "ins", "To\u00b7des\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ersterbet ihr Geister/ k\u00fcsset den Tod", "tokens": ["E\u00b7rster\u00b7bet", "ihr", "Geis\u00b7ter", "/", "k\u00fcs\u00b7set", "den", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So endet das Sterben/ Jam\u0303er und Noht/", "tokens": ["So", "en\u00b7det", "das", "Ster\u00b7ben", "/", "Jam\u0303er", "und", "Noht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hier findet ihr vor die gehabten L\u00fcste", "tokens": ["Hier", "fin\u00b7det", "ihr", "vor", "die", "ge\u00b7hab\u00b7ten", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Drachen-Haus/ und Schlangen-volle W\u00fcste/", "tokens": ["Ein", "Dra\u00b7chen\u00b7Haus", "/", "und", "Schlan\u00b7gen\u00b7vol\u00b7le", "W\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt stinckt die Welt/", "tokens": ["Jetzt", "stinckt", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mein Geist nichts h\u00e4lt", "tokens": ["Mein", "Geist", "nichts", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Von ihren falschen Freuden.", "tokens": ["Von", "ih\u00b7ren", "fal\u00b7schen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es wird verflucht", "tokens": ["Es", "wird", "ver\u00b7flucht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die leere Frucht/", "tokens": ["Die", "lee\u00b7re", "Frucht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Damit sie mich will weiden.", "tokens": ["Da\u00b7mit", "sie", "mich", "will", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}