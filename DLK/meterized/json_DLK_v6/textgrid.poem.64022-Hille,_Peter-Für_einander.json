{"textgrid.poem.64022": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr einander", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kreis von Erde das ist ein Zauberkreis", "tokens": ["Ein", "Kreis", "von", "Er\u00b7de", "das", "ist", "ein", "Zau\u00b7ber\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00dcber alle Hexenkunst.", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", "He\u00b7xen\u00b7kunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese deine Erde dr\u00e4ngt sich dir empor", "tokens": ["Die\u00b7se", "dei\u00b7ne", "Er\u00b7de", "dr\u00e4ngt", "sich", "dir", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "PRF", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und aufsteigt deines Geistes Domes Chor.", "tokens": ["Und", "auf\u00b7steigt", "dei\u00b7nes", "Geis\u00b7tes", "Do\u00b7mes", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du wirst von ihr: dein Leben", "tokens": ["Du", "wirst", "von", "ihr", ":", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird sie rauschend \u00fcberschweben.", "tokens": ["Wird", "sie", "rau\u00b7schend", "\u00fc\u00b7bersc\u00b7hwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von ihr gr\u00fc\u00dfen", "tokens": ["Und", "von", "ihr", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Hoch in frohem Wipfelhaupte.", "tokens": ["Hoch", "in", "fro\u00b7hem", "Wip\u00b7fel\u00b7haup\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "An weitem, blauen, heiterreifen Himmel.", "tokens": ["An", "wei\u00b7tem", ",", "blau\u00b7en", ",", "hei\u00b7ter\u00b7rei\u00b7fen", "Him\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und willst du ruhen,", "tokens": ["Und", "willst", "du", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So schl\u00e4gst du aus der Brust", "tokens": ["So", "schl\u00e4gst", "du", "aus", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dir der Erde breite, warme Falte;", "tokens": ["Dir", "der", "Er\u00b7de", "brei\u00b7te", ",", "war\u00b7me", "Fal\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wie ein Krieger seinen Mantel schl\u00e4gt,", "tokens": ["Wie", "ein", "Krie\u00b7ger", "sei\u00b7nen", "Man\u00b7tel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Den feldgewohnten,", "tokens": ["Den", "feld\u00b7ge\u00b7wohn\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "In fest geronnen scharfer Nacht", "tokens": ["In", "fest", "ge\u00b7ron\u00b7nen", "schar\u00b7fer", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Um seines Heldenherzens m\u00fcd' Erkalten,", "tokens": ["Um", "sei\u00b7nes", "Hel\u00b7den\u00b7her\u00b7zens", "m\u00fcd'", "Er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df frischer Morgenwind", "tokens": ["Da\u00df", "fri\u00b7scher", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Noch die Glut mag finden und erwecken.", "tokens": ["Noch", "die", "Glut", "mag", "fin\u00b7den", "und", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Die wachen Sterne aber h\u00fcten treu", "tokens": ["Die", "wa\u00b7chen", "Ster\u00b7ne", "a\u00b7ber", "h\u00fc\u00b7ten", "treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Im Lebenslager all den starken knappen Heldenschlaf.", "tokens": ["Im", "Le\u00b7bens\u00b7la\u00b7ger", "all", "den", "star\u00b7ken", "knap\u00b7pen", "Hel\u00b7den\u00b7schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}