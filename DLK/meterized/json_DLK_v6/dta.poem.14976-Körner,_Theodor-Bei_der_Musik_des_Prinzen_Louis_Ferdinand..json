{"dta.poem.14976": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Musik des Prinzen Louis Ferdinand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Muthig schwellen sie ans volle Herz,", "tokens": ["Mut\u00b7hig", "schwel\u00b7len", "sie", "ans", "vol\u00b7le", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In die Seele f\u00fchl' ich sie mir dringen,", "tokens": ["In", "die", "See\u00b7le", "f\u00fchl'", "ich", "sie", "mir", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wecken mir den vaterl\u00e4nd'schen Schmerz.", "tokens": ["We\u00b7cken", "mir", "den", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7schen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und mit ihren fr\u00fch gepr\u00fcften Schwingen,", "tokens": ["Und", "mit", "ih\u00b7ren", "fr\u00fch", "ge\u00b7pr\u00fcf\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00e4mpfen sie im Sturme himmelw\u00e4rts,", "tokens": ["K\u00e4mp\u00b7fen", "sie", "im", "Stur\u00b7me", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Doch sie tragen nur ein dunkles Sehnen,", "tokens": ["Doch", "sie", "tra\u00b7gen", "nur", "ein", "dunk\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nicht den Geist aus diesem Land der Thr\u00e4nen.", "tokens": ["Nicht", "den", "Geist", "aus", "die\u00b7sem", "Land", "der", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Allgewaltig h\u00e4lt ihn noch das Leben,", "tokens": ["All\u00b7ge\u00b7wal\u00b7tig", "h\u00e4lt", "ihn", "noch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Taucht die Fl\u00fcgel in den stygschen Flu\u00df.", "tokens": ["Taucht", "die", "Fl\u00fc\u00b7gel", "in", "den", "styg\u00b7schen", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Es ist nicht der K\u00fcnste freies Schweben,", "tokens": ["Es", "ist", "nicht", "der", "K\u00fcns\u00b7te", "frei\u00b7es", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht verkl\u00e4rter Geister Weiheku\u00df.", "tokens": ["Nicht", "ver\u00b7kl\u00e4r\u00b7ter", "Geis\u00b7ter", "Wei\u00b7he\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Noch dem Erdgeist ist er Preis gegeben,", "tokens": ["Noch", "dem", "Erd\u00b7geist", "ist", "er", "Preis", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem Staube k\u00e4mpft der Genius,", "tokens": ["Mit", "dem", "Stau\u00b7be", "k\u00e4mpft", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Rei\u00dft er auch im Rausche der Gedanken,", "tokens": ["Rei\u00dft", "er", "auch", "im", "Rau\u00b7sche", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Oft sich blutend los aus seinen Schranken.", "tokens": ["Oft", "sich", "blu\u00b7tend", "los", "aus", "sei\u00b7nen", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dann ergreift ihn ein bachantisch W\u00fcthen,", "tokens": ["Dann", "er\u00b7greift", "ihn", "ein", "ba\u00b7ch\u00b7an\u00b7tisch", "W\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wilde Melodienblitze spr\u00fchn,", "tokens": ["Wil\u00b7de", "Me\u00b7lo\u00b7dien\u00b7blit\u00b7ze", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aus dem Tode ruft er Strahlenbl\u00fcthen,", "tokens": ["Aus", "dem", "To\u00b7de", "ruft", "er", "Strah\u00b7len\u00b7bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und zertritt sie kalt, sobald sie bl\u00fchn.", "tokens": ["Und", "zer\u00b7tritt", "sie", "kalt", ",", "so\u00b7bald", "sie", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn die letzten Funken bleich vergl\u00fchten,", "tokens": ["Wenn", "die", "letz\u00b7ten", "Fun\u00b7ken", "bleich", "ver\u00b7gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hebt er sich noch einmal, stolz und k\u00fchn,", "tokens": ["Hebt", "er", "sich", "noch", "ein\u00b7mal", ",", "stolz", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und versinkt dann mit gewalt'gem Schauren,", "tokens": ["Und", "ver\u00b7sinkt", "dann", "mit", "ge\u00b7walt'\u00b7gem", "Schau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In den alten Kampf mit dem Centauren.", "tokens": ["In", "den", "al\u00b7ten", "Kampf", "mit", "dem", "Cen\u00b7tau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wilder Geist! jetzt hast du \u00fcberwunden,", "tokens": ["Wil\u00b7der", "Geist", "!", "jetzt", "hast", "du", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Nacht verschmilzt in Morgenroth,", "tokens": ["Dei\u00b7ne", "Nacht", "ver\u00b7schmilzt", "in", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ausgek\u00e4mpft sind deiner Pr\u00fcfung Stunden,", "tokens": ["Aus\u00b7ge\u00b7k\u00e4mpft", "sind", "dei\u00b7ner", "Pr\u00fc\u00b7fung", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leer der Kelch, den dir das Schicksal bot.", "tokens": ["Leer", "der", "Kelch", ",", "den", "dir", "das", "Schick\u00b7sal", "bot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kunst und Leben hat den Kranz gewunden,", "tokens": ["Kunst", "und", "Le\u00b7ben", "hat", "den", "Kranz", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf die Locken dr\u00fcckte ihn der Tod.", "tokens": ["Auf", "die", "Lo\u00b7cken", "dr\u00fcck\u00b7te", "ihn", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deinen Grabstein kann die Zeit zermalmen,", "tokens": ["Dei\u00b7nen", "Grab\u00b7stein", "kann", "die", "Zeit", "zer\u00b7mal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch die Lorbeern werden dort zu Palmen.", "tokens": ["Doch", "die", "Lor\u00b7beern", "wer\u00b7den", "dort", "zu", "Pal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und dein Sehnen klagte nicht vergebens,", "tokens": ["Und", "dein", "Seh\u00b7nen", "klag\u00b7te", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einmal wards in deiner Seele Tag,", "tokens": ["Ein\u00b7mal", "wards", "in", "dei\u00b7ner", "See\u00b7le", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als dein Herz am k\u00fchnsten Ziel des Strebens", "tokens": ["Als", "dein", "Herz", "am", "k\u00fchns\u00b7ten", "Ziel", "des", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kalt und blutend auf der Wahlstatt lag.", "tokens": ["Kalt", "und", "blu\u00b7tend", "auf", "der", "Wahl\u00b7statt", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sterbend l\u00f6\u00dfte sich der Sturm des Lebens,", "tokens": ["Ster\u00b7bend", "l\u00f6\u00df\u00b7te", "sich", "der", "Sturm", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sterbend l\u00f6\u00dfte sich der Harfe Schlag,", "tokens": ["Ster\u00b7bend", "l\u00f6\u00df\u00b7te", "sich", "der", "Har\u00b7fe", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Himmels siegverkl\u00e4rte S\u00f6hne", "tokens": ["Und", "des", "Him\u00b7mels", "sieg\u00b7ver\u00b7kl\u00e4r\u00b7te", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Trugen dich ins freie Land der T\u00f6ne.", "tokens": ["Tru\u00b7gen", "dich", "ins", "frei\u00b7e", "Land", "der", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}