{"textgrid.poem.59996": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: So in Prosa und in Reimen", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So in Prosa und in Reimen", "tokens": ["So", "in", "Pro\u00b7sa", "und", "in", "Rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heult der wilde Bajuvar,", "tokens": ["Heult", "der", "wil\u00b7de", "Ba\u00b7ju\u00b7var", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es heulen die \u00bbGeheimen\u00ab:", "tokens": ["Und", "es", "heu\u00b7len", "die", "\u00bb", "Ge\u00b7hei\u00b7men", "\u00ab", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "$(", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbayerland ist in Gefahr!\u00ab", "tokens": ["\u00bb", "bay\u00b7er\u00b7land", "ist", "in", "Ge\u00b7fahr", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, vergebens baute jener", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "bau\u00b7te", "je\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ludovik die Propyl\u00e4n,", "tokens": ["Lu\u00b7do\u00b7vik", "die", "Pro\u00b7py\u00b7l\u00e4n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Sprache der Athener", "tokens": ["Denn", "die", "Spra\u00b7che", "der", "A\u00b7the\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird man niemals hier verstehn.", "tokens": ["Wird", "man", "nie\u00b7mals", "hier", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie die Narren dir's ver\u00fcbeln,", "tokens": ["Wie", "die", "Nar\u00b7ren", "dir's", "ver\u00b7\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's den P\u00f6bel ba\u00df verdrie\u00dft,", "tokens": ["Wie's", "den", "P\u00f6\u00b7bel", "ba\u00df", "ver\u00b7drie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er seinen Schmutz in K\u00fcbeln", "tokens": ["Wie", "er", "sei\u00b7nen", "Schmutz", "in", "K\u00fc\u00b7beln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimpfend \u00fcber dich ergie\u00dft;", "tokens": ["Schimp\u00b7fend", "\u00fc\u00b7ber", "dich", "er\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil Horazens schwarze Vettel", "tokens": ["Weil", "Ho\u00b7ra\u00b7zens", "schwar\u00b7ze", "Vet\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht mit dir zu Pferde sitzt;", "tokens": ["Nicht", "mit", "dir", "zu", "Pfer\u00b7de", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil einmal ein Bankozettel", "tokens": ["Weil", "ein\u00b7mal", "ein", "Ban\u00b7ko\u00b7zet\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "In der Muse H\u00e4nden blitzt;", "tokens": ["In", "der", "Mu\u00b7se", "H\u00e4n\u00b7den", "blitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weil des reichen Schachs Kamele", "tokens": ["Weil", "des", "rei\u00b7chen", "Schachs", "Ka\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeitig angelangt einmal,", "tokens": ["Zei\u00b7tig", "an\u00b7ge\u00b7langt", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh Firdusi seine Seele", "tokens": ["Eh", "Fir\u00b7du\u00b7si", "sei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgehaucht in Not und Qual;", "tokens": ["Aus\u00b7ge\u00b7haucht", "in", "Not", "und", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weil einmal ein goldner Regen", "tokens": ["Weil", "ein\u00b7mal", "ein", "gold\u00b7ner", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In den Scho\u00df des K\u00fcnstlers f\u00e4llt \u2013", "tokens": ["In", "den", "Scho\u00df", "des", "K\u00fcnst\u00b7lers", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruiniere meinetwegen", "tokens": ["Ru\u00b7i\u00b7nie\u00b7re", "mei\u00b7net\u00b7we\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle K\u00f6nige der Welt.", "tokens": ["Al\u00b7le", "K\u00f6\u00b7ni\u00b7ge", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hol den Hort der Nibelungen,", "tokens": ["Hol", "den", "Hort", "der", "Ni\u00b7be\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den versunknen, aus dem Rhein!", "tokens": ["Den", "ver\u00b7sun\u00b7knen", ",", "aus", "dem", "Rhein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was Orpheus einst gesungen,", "tokens": ["Und", "was", "Or\u00b7pheus", "einst", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt es dir unm\u00f6glich sein?", "tokens": ["Sollt", "es", "dir", "un\u00b7m\u00f6g\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tiger, Affen, Schweinehunde,", "tokens": ["Ti\u00b7ger", ",", "Af\u00b7fen", ",", "Schwei\u00b7ne\u00b7hun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meyerb\u00e4ren macht' er zahm;", "tokens": ["Me\u00b7yer\u00b7b\u00e4\u00b7ren", "macht'", "er", "zahm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leider hab ich keine Kunde,", "tokens": ["Lei\u00b7der", "hab", "ich", "kei\u00b7ne", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sich Sanchos Tier benahm.", "tokens": ["Wie", "sich", "San\u00b7chos", "Tier", "be\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber la\u00df des Esels Knirschen", "tokens": ["A\u00b7ber", "la\u00df", "des", "E\u00b7sels", "Knir\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich nicht st\u00f6ren im Genu\u00df!", "tokens": ["Dich", "nicht", "st\u00f6\u00b7ren", "im", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "I\u00df, mit wem du willst, die Kirschen,", "tokens": ["I\u00df", ",", "mit", "wem", "du", "willst", ",", "die", "Kir\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PWS", "PPER", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber Zukunftsmusikus!", "tokens": ["Lie\u00b7ber", "Zu\u00b7kunfts\u00b7mu\u00b7si\u00b7kus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nur empfehl ich dir das eine:", "tokens": ["Nur", "emp\u00b7fehl", "ich", "dir", "das", "ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PDS", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du fertig, sag ade!", "tokens": ["Bist", "du", "fer\u00b7tig", ",", "sag", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VVFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Warte nicht, bis man die Steine", "tokens": ["War\u00b7te", "nicht", ",", "bis", "man", "die", "Stei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "An den Kopf dir wirft \u2013 o weh!", "tokens": ["An", "den", "Kopf", "dir", "wirft", "\u2013", "o", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$(", "FM", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Suche niemals mehr auf solcher", "tokens": ["Su\u00b7che", "nie\u00b7mals", "mehr", "auf", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde dir ein Lorbeerblatt,", "tokens": ["Er\u00b7de", "dir", "ein", "Lor\u00b7beer\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinge selbst das Vlies, das Kolcher,", "tokens": ["Hin\u00b7ge", "selbst", "das", "Vlies", ",", "das", "Kol\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber jedem Tor der Stadt!", "tokens": ["\u00dc\u00b7ber", "je\u00b7dem", "Tor", "der", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}