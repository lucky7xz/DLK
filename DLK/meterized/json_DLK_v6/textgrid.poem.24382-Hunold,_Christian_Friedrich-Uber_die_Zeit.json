{"textgrid.poem.24382": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pfeil geht zwar geschwind/ die Luft saust schnell vorbey/", "tokens": ["Ein", "Pfeil", "geht", "zwar", "ge\u00b7schwind", "/", "die", "Luft", "saust", "schnell", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$(", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wolcken lauffen sehr/ der Blitz fahrt in die Eichen/", "tokens": ["Die", "Wol\u00b7cken", "lauf\u00b7fen", "sehr", "/", "der", "Blitz", "fahrt", "in", "die", "Ei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprich/ ob was schnellers noch/ als seine Strahlen sey?", "tokens": ["Sprich", "/", "ob", "was", "schnel\u00b7lers", "noch", "/", "als", "sei\u00b7ne", "Strah\u00b7len", "sey", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PIS", "ADV", "ADV", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Blitz/ Pfeil/ Lufft/ Wolcken sind der Zeit nicht zu vergleichen.", "tokens": ["Blitz", "/", "Pfeil", "/", "Lufft", "/", "Wol\u00b7cken", "sind", "der", "Zeit", "nicht", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "VAFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie streicht geschwind dahin/ kein Auge kan es sehn:", "tokens": ["Sie", "streicht", "ge\u00b7schwind", "da\u00b7hin", "/", "kein", "Au\u00b7ge", "kan", "es", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$(", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Meer/ Wind und Wetter sind von Menschen aufzuhalten/", "tokens": ["Meer", "/", "Wind", "und", "Wet\u00b7ter", "sind", "von", "Men\u00b7schen", "auf\u00b7zu\u00b7hal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VAFIN", "APPR", "NN", "VVIZU", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Die Zeit von keinem nicht: sie l\u00e4\u00dft auch K\u00e4yser stehn/", "tokens": ["Die", "Zeit", "von", "kei\u00b7nem", "nicht", ":", "sie", "l\u00e4\u00dft", "auch", "K\u00e4y\u00b7ser", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht \u00fcber einen Blick vermag in F\u00fcrst zu walten.", "tokens": ["Nicht", "\u00fc\u00b7ber", "ei\u00b7nen", "Blick", "ver\u00b7mag", "in", "F\u00fcrst", "zu", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer kauffte nicht die Zeit vor Millionen ein?", "tokens": ["Wer", "kauff\u00b7te", "nicht", "die", "Zeit", "vor", "Mil\u00b7lion\u00b7en", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Doch geht sie/ weil sie mehr als g\u00fclden ist zu sch\u00e4tzen.", "tokens": ["Doch", "geht", "sie", "/", "weil", "sie", "mehr", "als", "g\u00fcl\u00b7den", "ist", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PIAT", "KOKOM", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer sich der Zeit bedient/ kan reich in Armuth seyn.", "tokens": ["Wer", "sich", "der", "Zeit", "be\u00b7dient", "/", "kan", "reich", "in", "Ar\u00b7muth", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVPP", "$(", "VMFIN", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bey zeiten kan die Zeit in Gl\u00fcck und Ehren setzen.", "tokens": ["Bey", "zei\u00b7ten", "kan", "die", "Zeit", "in", "Gl\u00fcck", "und", "Eh\u00b7ren", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum edle Menschen braucht anitzt der edlen Zeit:", "tokens": ["Drum", "ed\u00b7le", "Men\u00b7schen", "braucht", "a\u00b7nitzt", "der", "ed\u00b7len", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gar lange wird der Sand nicht in dem Glase bleiben;", "tokens": ["Gar", "lan\u00b7ge", "wird", "der", "Sand", "nicht", "in", "dem", "Gla\u00b7se", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sucht die Wei\u00dfheit mehr als die Erg\u00f6tzlichkeit/", "tokens": ["Und", "sucht", "die", "Wei\u00df\u00b7heit", "mehr", "als", "die", "Er\u00b7g\u00f6tz\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vertreibt die Zeit doch nicht/ sie wird sich selbst vertreiben.", "tokens": ["Ver\u00b7treibt", "die", "Zeit", "doch", "nicht", "/", "sie", "wird", "sich", "selbst", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "$(", "PPER", "VAFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}