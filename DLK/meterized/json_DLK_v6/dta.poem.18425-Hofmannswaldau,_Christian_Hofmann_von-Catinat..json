{"dta.poem.18425": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Catinat.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit der best\u00fcrtzte Po dein w\u00fctend schwerd versp\u00fcrt/", "tokens": ["Seit", "der", "be\u00b7st\u00fcrtz\u00b7te", "Po", "dein", "w\u00fc\u00b7tend", "schwerd", "ver\u00b7sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat er offtermal dis klage-wort gef\u00fchrt:", "tokens": ["So", "hat", "er", "off\u00b7ter\u00b7mal", "dis", "kla\u00b7ge\u00b7wort", "ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PDS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es liegt ein Phaeton bereits in mir begraben/", "tokens": ["Es", "liegt", "ein", "Phae\u00b7ton", "be\u00b7reits", "in", "mir", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Soll dieser denn umsonst mein land verw\u00fcstet haben?", "tokens": ["Soll", "die\u00b7ser", "denn", "um\u00b7sonst", "mein", "land", "ver\u00b7w\u00fcs\u00b7tet", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "KON", "ADV", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iam