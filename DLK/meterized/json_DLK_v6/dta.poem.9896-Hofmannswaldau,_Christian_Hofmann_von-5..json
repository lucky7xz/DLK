{"dta.poem.9896": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ein c\u00f6rper ohne schatten bin sehr sch\u00f6n und ungemein/", "tokens": ["Ich", "ein", "c\u00f6r\u00b7per", "oh\u00b7ne", "schat\u00b7ten", "bin", "sehr", "sch\u00f6n", "und", "un\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ADJA", "VAFIN", "ADV", "ADJD", "KON", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "N\u00fctzlich allem was nur lebet/ kan darbey auch sch\u00e4dlich seyn.", "tokens": ["N\u00fctz\u00b7lich", "al\u00b7lem", "was", "nur", "le\u00b7bet", "/", "kan", "dar\u00b7bey", "auch", "sch\u00e4d\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PWS", "ADV", "VVFIN", "$(", "VMFIN", "PAV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich bin eines c\u00f6rpers auge/ da mir fehlt der augen licht/", "tokens": ["Ich", "bin", "ei\u00b7nes", "c\u00f6r\u00b7pers", "au\u00b7ge", "/", "da", "mir", "fehlt", "der", "au\u00b7gen", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und kan andern klarheit geben/ nur ich selber sehe nicht.", "tokens": ["Und", "kan", "an\u00b7dern", "klar\u00b7heit", "ge\u00b7ben", "/", "nur", "ich", "sel\u00b7ber", "se\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "VVINF", "$(", "ADV", "PPER", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}