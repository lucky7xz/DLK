{"dta.poem.4288": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirsch im Netz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In erb\u00e4rmlicher Gestalt sieht man hier, in Todes-", "tokens": ["In", "er\u00b7b\u00e4rm\u00b7li\u00b7cher", "Ge\u00b7stalt", "sieht", "man", "hier", ",", "in", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADV", "$,", "APPR", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Z\u00fcgen,", "tokens": ["Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Einen fest-verstrickten Hirsch, meistens schon zerfleischet,", "tokens": ["Ei\u00b7nen", "fest\u00b7ver\u00b7strick\u00b7ten", "Hirsch", ",", "meis\u00b7tens", "schon", "zer\u00b7flei\u00b7schet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "liegen.", "tokens": ["lie\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sein bethr\u00e4ntes Auge bricht, unser Ohr glaubt das Ge-", "tokens": ["Sein", "be\u00b7thr\u00e4n\u00b7tes", "Au\u00b7ge", "bricht", ",", "un\u00b7ser", "Ohr", "glaubt", "das", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+---", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "r\u00e4usch", "tokens": ["r\u00e4usch"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der zersplitterten Gebeine, und von dem zerquetschten", "tokens": ["Der", "zer\u00b7split\u00b7ter\u00b7ten", "Ge\u00b7bei\u00b7ne", ",", "und", "von", "dem", "zer\u00b7quetschten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Fleisch", "tokens": ["Fleisch"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Jeden blut\u2019gen Ri\u00df zu h\u00f6ren, auch ein immer st\u00e4rker", "tokens": ["Je\u00b7den", "blut'\u00b7gen", "Ri\u00df", "zu", "h\u00f6\u00b7ren", ",", "auch", "ein", "im\u00b7mer", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "ADV", "ART", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Stehnen", "tokens": ["Steh\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Bey dem w\u00fcrgenden Geschrey seiner M\u00f6rder. Seine", "tokens": ["Bey", "dem", "w\u00fcr\u00b7gen\u00b7den", "Ge\u00b7schrey", "sei\u00b7ner", "M\u00f6r\u00b7der", ".", "Sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$.", "PPOSAT"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Sehnen,", "tokens": ["Seh\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Sammt den Adern, sieht man offen, seht, wie quillt der", "tokens": ["Sammt", "den", "A\u00b7dern", ",", "sieht", "man", "of\u00b7fen", ",", "seht", ",", "wie", "quillt", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PIS", "ADJD", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Schwei\u00df herf\u00fcr!", "tokens": ["Schwei\u00df", "her\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Meynt man doch all\u2019 Augenblick, das beflossene Papier", "tokens": ["Meynt", "man", "doch", "all'", "Au\u00b7gen\u00b7blick", ",", "das", "be\u00b7flos\u00b7se\u00b7ne", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Werde roht und schmutzig werden. Durch die Aehnlich-", "tokens": ["Wer\u00b7de", "roht", "und", "schmut\u00b7zig", "wer\u00b7den", ".", "Durch", "die", "A\u00b7ehn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VAINF", "$.", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "keit verf\u00fchret,", "tokens": ["keit", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "F\u00fchlen alle, die es sehen, da\u00df sie Gram und Mitleid", "tokens": ["F\u00fch\u00b7len", "al\u00b7le", ",", "die", "es", "se\u00b7hen", ",", "da\u00df", "sie", "Gram", "und", "Mit\u00b7leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "r\u00fchret.", "tokens": ["r\u00fch\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Dennoch w\u00e4hret es nicht lange; denn es f\u00e4llt zum Trost uns", "tokens": ["Den\u00b7noch", "w\u00e4h\u00b7ret", "es", "nicht", "lan\u00b7ge", ";", "denn", "es", "f\u00e4llt", "zum", "Trost", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "KON", "PPER", "VVFIN", "APPRART", "NN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "ein:", "tokens": ["ein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Ist doch, was wir hier bedauren, eine nur gemahlte Pein.", "tokens": ["Ist", "doch", ",", "was", "wir", "hier", "be\u00b7dau\u00b7ren", ",", "ei\u00b7ne", "nur", "ge\u00b7mahl\u00b7te", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}