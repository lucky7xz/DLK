{"dta.poem.21793": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Brenn/ aber lindre auch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Was hab' ich/ kleiner/ dir getahn", "tokens": ["Was", "hab'", "ich", "/", "klei\u00b7ner", "/", "dir", "ge\u00b7tahn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$(", "ADJD", "$(", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df ich nicht Ruhe haben kan/", "tokens": ["da\u00df", "ich", "nicht", "Ru\u00b7he", "ha\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "willstu mich denn zu Aschen bren\u0303en?", "tokens": ["will\u00b7stu", "mich", "denn", "zu", "A\u00b7schen", "bre\u00f1en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin ohn di\u00df ent\u00e4dert bleich", "tokens": ["Ich", "bin", "ohn", "di\u00df", "en\u00b7t\u00e4\u00b7dert", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDS", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und einem schwarzen Schatten gleich", "tokens": ["und", "ei\u00b7nem", "schwar\u00b7zen", "Schat\u00b7ten", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "von meinen Br\u00fcdern kaum zu kennen.", "tokens": ["von", "mei\u00b7nen", "Br\u00fc\u00b7dern", "kaum", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich gebe dir die Sieges-Fahn\u2019", "tokens": ["Ich", "ge\u00b7be", "dir", "die", "Sie\u00b7ges\u00b7Fahn'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und flehe dich in Demuht an/", "tokens": ["und", "fle\u00b7he", "dich", "in", "De\u00b7muht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "la\u00df deinen Diener nicht verderben.", "tokens": ["la\u00df", "dei\u00b7nen", "Die\u00b7ner", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es gibt dir/ Amor/ schlechten Prei\u00df", "tokens": ["Es", "gibt", "dir", "/", "A\u00b7mor", "/", "schlech\u00b7ten", "Prei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NE", "$(", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wenn/ der sich nicht zu retten wei\u00df", "tokens": ["wenn", "/", "der", "sich", "nicht", "zu", "ret\u00b7ten", "wei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "PRELS", "PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Genade zieret einen Held.", "tokens": ["Ge\u00b7na\u00b7de", "zie\u00b7ret", "ei\u00b7nen", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich r\u00e4ume dir des Herzens Feld/", "tokens": ["Ich", "r\u00e4u\u00b7me", "dir", "des", "Her\u00b7zens", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mein blosser Busem steht dir offen.", "tokens": ["mein", "blos\u00b7ser", "Bu\u00b7sem", "steht", "dir", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieh ein/ und g\u00f6nn mir nur die Lehn", "tokens": ["Zieh", "ein", "/", "und", "g\u00f6nn", "mir", "nur", "die", "Lehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "$(", "KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich wil dir zu Gebote stehn/", "tokens": ["ich", "wil", "dir", "zu", "Ge\u00b7bo\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "was hastu mehr von mir zu hoffen.", "tokens": ["was", "has\u00b7tu", "mehr", "von", "mir", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer wird/ hastu mich umgebracht/", "tokens": ["Wer", "wird", "/", "has\u00b7tu", "mich", "um\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "alsdenn erheben deine Macht?", "tokens": ["als\u00b7denn", "er\u00b7he\u00b7ben", "dei\u00b7ne", "Macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wird von deinen Tahten singen?", "tokens": ["Wer", "wird", "von", "dei\u00b7nen", "Tah\u00b7ten", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd\u2019 ich noch etwas \u00fcbrig sein:", "tokens": ["Werd'", "ich", "noch", "et\u00b7was", "\u00fcb\u00b7rig", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Staub kan dir nicht Nuzzen brin-", "tokens": ["Mein", "Staub", "kan", "dir", "nicht", "Nuz\u00b7zen", "brin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}