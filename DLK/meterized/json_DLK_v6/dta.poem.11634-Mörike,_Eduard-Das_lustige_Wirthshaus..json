{"dta.poem.11634": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.85", "sv:0.14"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Doch seht mir nur dort das Professorsvolk an!", "tokens": ["Doch", "seht", "mir", "nur", "dort", "das", "Pro\u00b7fes\u00b7sors\u00b7volk", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das jauchzet und tanzet und hopft!", "tokens": ["Das", "jauch\u00b7zet", "und", "tan\u00b7zet", "und", "hopft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Storch und der Pfau und die Eule voran!", "tokens": ["Der", "Storch", "und", "der", "Pfau", "und", "die", "Eu\u00b7le", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein Seel, sie sind Alle bezopft!", "tokens": ["Mein", "Seel", ",", "sie", "sind", "Al\u00b7le", "be\u00b7zopft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}