{"textgrid.poem.56425": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da st\u00fcrzt ein W\u00e4chter mit Geschrei herein", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da st\u00fcrzt ein W\u00e4chter mit Geschrei herein", "tokens": ["Da", "st\u00fcrzt", "ein", "W\u00e4ch\u00b7ter", "mit", "Ge\u00b7schrei", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Lagers Ruhe. \u00bbZu den Waffen, auf.\u00ab", "tokens": ["In", "La\u00b7gers", "Ru\u00b7he", ".", "\u00bb", "Zu", "den", "Waf\u00b7fen", ",", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "$.", "$(", "APPR", "ART", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle sehn erschreckt auf seinen Lauf,", "tokens": ["Und", "al\u00b7le", "sehn", "er\u00b7schreckt", "auf", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der schreiend eilt schon fern durch ihre Reihn.", "tokens": ["Der", "schrei\u00b7end", "eilt", "schon", "fern", "durch", "ih\u00b7re", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie stehen auf, sie legen ihre Hand", "tokens": ["Sie", "ste\u00b7hen", "auf", ",", "sie", "le\u00b7gen", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor ihre Augen. Doch sie sehen nicht.", "tokens": ["Vor", "ih\u00b7re", "Au\u00b7gen", ".", "Doch", "sie", "se\u00b7hen", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Rauch der Felder bei\u00dft sie ins Gesicht.", "tokens": ["Der", "Rauch", "der", "Fel\u00b7der", "bei\u00dft", "sie", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie sehen nichts als Rauch und Felderbrand.", "tokens": ["Sie", "se\u00b7hen", "nichts", "als", "Rauch", "und", "Fel\u00b7der\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf eines Thrakers Schultern wird gehoben", "tokens": ["Auf", "ei\u00b7nes", "Thra\u00b7kers", "Schul\u00b7tern", "wird", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein kleiner Skythe, da\u00df er weiter s\u00e4he.", "tokens": ["Ein", "klei\u00b7ner", "Sky\u00b7the", ",", "da\u00df", "er", "wei\u00b7ter", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er schaut ins Land von seiner Warte oben.", "tokens": ["Er", "schaut", "ins", "Land", "von", "sei\u00b7ner", "War\u00b7te", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die andern dr\u00e4ngen sich in ihre N\u00e4he.", "tokens": ["Die", "an\u00b7dern", "dr\u00e4n\u00b7gen", "sich", "in", "ih\u00b7re", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein gro\u00dfer Kreis hat sich um sie geschoben", "tokens": ["Ein", "gro\u00b7\u00dfer", "Kreis", "hat", "sich", "um", "sie", "ge\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wartet schweigend, was er wohl ersp\u00e4he.", "tokens": ["Und", "war\u00b7tet", "schwei\u00b7gend", ",", "was", "er", "wohl", "er\u00b7sp\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}