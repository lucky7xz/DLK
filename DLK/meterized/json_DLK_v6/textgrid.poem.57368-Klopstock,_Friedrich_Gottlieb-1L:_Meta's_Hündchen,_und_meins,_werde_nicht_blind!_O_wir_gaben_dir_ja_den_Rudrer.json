{"textgrid.poem.57368": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meta's H\u00fcndchen, und meins, werde nicht blind! O wir gaben dir ja den Rudrer", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meta's H\u00fcndchen, und meins, werde nicht blind! O wir gaben dir ja den Rudrer", "tokens": ["Me\u00b7ta's", "H\u00fcnd\u00b7chen", ",", "und", "meins", ",", "wer\u00b7de", "nicht", "blind", "!", "O", "wir", "ga\u00b7ben", "dir", "ja", "den", "Rud\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "NE", "$,", "VAFIN", "PTKNEG", "ADJD", "$.", "NE", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+--+--+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Von der Sch\u00fcte, der mein lieber", "tokens": ["Von", "der", "Sch\u00fc\u00b7te", ",", "der", "mein", "lie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Klopstock! mich nante, zum Arzt.", "tokens": ["Klops\u00b7tock", "!", "mich", "nan\u00b7te", ",", "zum", "Arzt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Blind, begleitest du mich, nehm' ich den Zaum,", "tokens": ["Blind", ",", "be\u00b7glei\u00b7test", "du", "mich", ",", "nehm'", "ich", "den", "Zaum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Niemals wieder, ersp\u00e4hst zuwedelnd", "tokens": ["Nie\u00b7mals", "wie\u00b7der", ",", "er\u00b7sp\u00e4hst", "zu\u00b7we\u00b7delnd"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Von dem Gaule, wie von hohen", "tokens": ["Von", "dem", "Gau\u00b7le", ",", "wie", "von", "ho\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Riesenhunden du thust,", "tokens": ["Rie\u00b7sen\u00b7hun\u00b7den", "du", "thust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Laune zu spielen nicht mehr. Robepierr'!", "tokens": ["Lau\u00b7ne", "zu", "spie\u00b7len", "nicht", "mehr", ".", "Ro\u00b7be\u00b7pi\u00b7err'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PTKNEG", "ADV", "$.", "NE", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem, dem wedle nicht zu, denn hier bist", "tokens": ["Dem", ",", "dem", "wed\u00b7le", "nicht", "zu", ",", "denn", "hier", "bist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KON", "ADV", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du verloren; denn mit H\u00f6llen-", "tokens": ["Du", "ver\u00b7lo\u00b7ren", ";", "denn", "mit", "H\u00f6l\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$.", "KON", "APPR", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hunden spielet man nicht.", "tokens": ["Hun\u00b7den", "spie\u00b7let", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Eile nicht, lerne noch diess: Belle den Mond", "tokens": ["Ei\u00b7le", "nicht", ",", "ler\u00b7ne", "noch", "diess", ":", "Bel\u00b7le", "den", "Mond"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "ADV", "VVFIN", "$.", "NN", "ART", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ja nicht an! Ihn umschwebt noch Kord\u00e4.", "tokens": ["Ja", "nicht", "an", "!", "Ihn", "um\u00b7schwebt", "noch", "Kor\u00b7d\u00e4", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn du jemals da hinaufbellst,", "tokens": ["Wenn", "du", "je\u00b7mals", "da", "hin\u00b7auf\u00b7bellst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kleiner, so brenn' ich dich blind!", "tokens": ["Klei\u00b7ner", ",", "so", "brenn'", "ich", "dich", "blind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Und dann l\u00e4gest du da, zweifeltest gar", "tokens": ["Und", "dann", "l\u00e4\u00b7gest", "du", "da", ",", "zwei\u00b7fel\u00b7test", "gar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ob dereinst zu dem Freund' Ulyssens,", "tokens": ["Ob", "de\u00b7reinst", "zu", "dem", "Freund'", "U\u00b7lys\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und des Dulders Elisama", "tokens": ["Und", "des", "Dul\u00b7ders", "E\u00b7li\u00b7sa\u00b7ma"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Treuem Hunde du k\u00e4mst.", "tokens": ["Treu\u00b7em", "Hun\u00b7de", "du", "k\u00e4mst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}