{"dta.poem.9881": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nacht-lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt da die nacht tritt ihre herrschafft an/", "tokens": ["Jetzt", "da", "die", "nacht", "tritt", "ih\u00b7re", "herr\u00b7schafft", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und glantz und licht mit braunem mantel decket/", "tokens": ["Und", "glantz", "und", "licht", "mit", "brau\u00b7nem", "man\u00b7tel", "de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da uns der schlaff die m\u00fcden glieder strecket/", "tokens": ["Da", "uns", "der", "schlaff", "die", "m\u00fc\u00b7den", "glie\u00b7der", "stre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einsamkeit f\u00fcr lust tritt auff die bahn;", "tokens": ["Und", "ein\u00b7sam\u00b7keit", "f\u00fcr", "lust", "tritt", "auff", "die", "bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So scheinet doch der sch\u00f6nheit holde kertze/", "tokens": ["So", "schei\u00b7net", "doch", "der", "sch\u00f6n\u00b7heit", "hol\u00b7de", "kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und treibet uns den flammen-brand ins hertze.", "tokens": ["Und", "trei\u00b7bet", "uns", "den", "flam\u00b7men\u00b7brand", "ins", "hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach Lisillis! erblick uns gn\u00e4dig an/", "tokens": ["Ach", "Li\u00b7sil\u00b7lis", "!", "er\u00b7blick", "uns", "gn\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVIMP", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sieh die h\u00f6chst-verpflichten knechte stehen/", "tokens": ["Und", "sieh", "die", "h\u00f6chst\u00b7ver\u00b7pflich\u00b7ten", "knech\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alles zwingt/ zwang uns hieher zu gehen;", "tokens": ["Der", "al\u00b7les", "zwingt", "/", "zwang", "uns", "hie\u00b7her", "zu", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "VVFIN", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sagen dirs: die liebe hats gethan.", "tokens": ["Wir", "sa\u00b7gen", "dirs", ":", "die", "lie\u00b7be", "hats", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn deine sonne scheint/ so mag der sterne", "tokens": ["Wenn", "dei\u00b7ne", "son\u00b7ne", "scheint", "/", "so", "mag", "der", "ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hell-gl\u00e4ntzend licht sich machen in die ferne.", "tokens": ["Hell\u00b7gl\u00e4nt\u00b7zend", "licht", "sich", "ma\u00b7chen", "in", "die", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVINF", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jhr augen/ denen sonn und monde weicht;", "tokens": ["Ihr", "au\u00b7gen", "/", "de\u00b7nen", "sonn", "und", "mon\u00b7de", "weicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "ADV", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jhr spiegel-gla\u00df der tieff-verliebten seelen;", "tokens": ["Ihr", "spie\u00b7gel\u00b7gla\u00df", "der", "tief\u00b7fver\u00b7lieb\u00b7ten", "see\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kan auch ein mensch die heisse glut verbelen/", "tokens": ["Kan", "auch", "ein", "mensch", "die", "heis\u00b7se", "glut", "ver\u00b7be\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die uns geschwind/ eh man es hofft/ beschleicht/", "tokens": ["Die", "uns", "ge\u00b7schwind", "/", "eh", "man", "es", "hofft", "/", "be\u00b7schleicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$(", "KOUS", "PIS", "PPER", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die unsre freyheit wirffet auff die bahre", "tokens": ["Die", "uns\u00b7re", "frey\u00b7heit", "wirf\u00b7fet", "auff", "die", "bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jm sch\u00f6nsten May/ im lentzen frischer jahre?", "tokens": ["Jm", "sch\u00f6ns\u00b7ten", "May", "/", "im", "lent\u00b7zen", "fri\u00b7scher", "jah\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}