{"dta.poem.21165": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Dose .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Hausger\u00e4th bey Thoren und bey Weisen,", "tokens": ["Du", "Haus\u00b7ge\u00b7r\u00e4\u00b7th", "bey", "Tho\u00b7ren", "und", "bey", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich, Dose, soll die Leyer dankbar preisen,", "tokens": ["Dich", ",", "Do\u00b7se", ",", "soll", "die", "Le\u00b7yer", "dank\u00b7bar", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Ceremoniel im Lehnstuhl angekettet,", "tokens": ["Vom", "Ce\u00b7re\u00b7mo\u00b7niel", "im", "Lehn\u00b7stuhl", "an\u00b7ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hast du oft unbemerkt vom Sprechen mich errettet.", "tokens": ["Hast", "du", "oft", "un\u00b7be\u00b7merkt", "vom", "Spre\u00b7chen", "mich", "er\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn ich gef\u00fchlt, wie steif ich da gesessen,", "tokens": ["Wenn", "ich", "ge\u00b7f\u00fchlt", ",", "wie", "steif", "ich", "da", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beym Dummkopf stumm, so nahm ich nur vermessen", "tokens": ["Beym", "Dumm\u00b7kopf", "stumm", ",", "so", "nahm", "ich", "nur", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und voller Stolz Rappee; und ohne mein Bem\u00fchen", "tokens": ["Und", "vol\u00b7ler", "Stolz", "Rap\u00b7pee", ";", "und", "oh\u00b7ne", "mein", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NE", "$.", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sah ich das finstre Weib, die Langeweile, fliehen.", "tokens": ["Sah", "ich", "das", "finst\u00b7re", "Weib", ",", "die", "Lan\u00b7ge\u00b7wei\u00b7le", ",", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es fehlt uns nie an Zuflucht in dem Leben.", "tokens": ["Es", "fehlt", "uns", "nie", "an", "Zu\u00b7flucht", "in", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der F\u00e4cher ward dem Frauenvolk gegeben;", "tokens": ["Der", "F\u00e4\u00b7cher", "ward", "dem", "Frau\u00b7en\u00b7volk", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschickt darauf zu sehn, ihn auf und zu zu machen,", "tokens": ["Ge\u00b7schickt", "da\u00b7rauf", "zu", "sehn", ",", "ihn", "auf", "und", "zu", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PAV", "PTKZU", "VVINF", "$,", "PPER", "PTKVZ", "KON", "PTKZU", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bewahrt die Kl\u00fcgsten oft vor Plaudern und vor Lachen.", "tokens": ["Be\u00b7wahrt", "die", "Kl\u00fcgs\u00b7ten", "oft", "vor", "Plau\u00b7dern", "und", "vor", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein gutes Gl\u00fcck hat uns die Dos\u2019 erfunden.", "tokens": ["Ein", "gu\u00b7tes", "Gl\u00fcck", "hat", "uns", "die", "Dos'", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sey mein Trost in langen trocknen Stunden!", "tokens": ["Sie", "sey", "mein", "Trost", "in", "lan\u00b7gen", "trock\u00b7nen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Schicksal! soll ich oft mich bey Visiten qu\u00e4len,", "tokens": ["O", "Schick\u00b7sal", "!", "soll", "ich", "oft", "mich", "bey", "Vi\u00b7si\u00b7ten", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "ADV", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df nur nie Rappee der treuen Dose fehlen!", "tokens": ["So", "la\u00df", "nur", "nie", "Rap\u00b7pee", "der", "treu\u00b7en", "Do\u00b7se", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}