{"textgrid.poem.41587": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Als der Herr in Sidons Land gekommen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Herr in Sidons Land gekommen,", "tokens": ["Als", "der", "Herr", "in", "Si\u00b7dons", "Land", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Naht ein kanan\u00e4isch Weiblein sich.", "tokens": ["Naht", "ein", "ka\u00b7na\u00b7n\u00e4\u00b7isch", "Weib\u00b7lein", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbherr!\u00ab spricht sie in Demut und in Frommen,", "tokens": ["\u00bb", "herr", "!", "\u00ab", "spricht", "sie", "in", "De\u00b7mut", "und", "in", "From\u00b7men", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbherr! erbarme meiner Tochter dich.", "tokens": ["\u00bb", "herr", "!", "er\u00b7bar\u00b7me", "mei\u00b7ner", "Toch\u00b7ter", "dich", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sieh, sie liegt daheim in gro\u00dfen Peinen,", "tokens": ["Sieh", ",", "sie", "liegt", "da\u00b7heim", "in", "gro\u00b7\u00dfen", "Pei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn es wohnt in ihr ein b\u00f6ser Geist.\u00ab", "tokens": ["Denn", "es", "wohnt", "in", "ihr", "ein", "b\u00f6\u00b7ser", "Geist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und voll Trauer hebt sie an zu weinen,", "tokens": ["Und", "voll", "Trau\u00b7er", "hebt", "sie", "an", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als der Herr sie strenge von sich weist.", "tokens": ["Als", "der", "Herr", "sie", "stren\u00b7ge", "von", "sich", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Doch sie schaut in seiner Augen Prachten,", "tokens": ["Doch", "sie", "schaut", "in", "sei\u00b7ner", "Au\u00b7gen", "Prach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ihr treues Herz bleibt ungeschreckt,", "tokens": ["Und", "ihr", "treu\u00b7es", "Herz", "bleibt", "un\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einem H\u00fcndlein gleich will sie sich achten,", "tokens": ["Ei\u00b7nem", "H\u00fcnd\u00b7lein", "gleich", "will", "sie", "sich", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das die Kr\u00fcmlein von der Erde leckt,", "tokens": ["Das", "die", "Kr\u00fcm\u00b7lein", "von", "der", "Er\u00b7de", "leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihre Demut hat sich durchgerungen:", "tokens": ["Ih\u00b7re", "De\u00b7mut", "hat", "sich", "durch\u00b7ge\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbweib, dein Glaub' hat dir geholfen,\u00ab spricht", "tokens": ["\u00bb", "weib", ",", "dein", "Glaub'", "hat", "dir", "ge\u00b7hol\u00b7fen", ",", "\u00ab", "spricht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "$(", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Jesu s\u00fc\u00dfe Stimme, und bezwungen", "tokens": ["Je\u00b7su", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", ",", "und", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Weicht der finstre Geist dem Gnadenlicht.", "tokens": ["Weicht", "der", "finst\u00b7re", "Geist", "dem", "Gna\u00b7den\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Kann nur Demut uns den Segen bringen,", "tokens": ["Kann", "nur", "De\u00b7mut", "uns", "den", "Se\u00b7gen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ich schn\u00f6der Wurm der Sterblichkeit", "tokens": ["Und", "ich", "schn\u00f6\u00b7der", "Wurm", "der", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine noch, es m\u00fcsse mir gelingen,", "tokens": ["Mei\u00b7ne", "noch", ",", "es", "m\u00fcs\u00b7se", "mir", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da ich von der Demut noch so weit?", "tokens": ["Da", "ich", "von", "der", "De\u00b7mut", "noch", "so", "weit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hab' ich nur ein kleines Leid getragen,", "tokens": ["Hab'", "ich", "nur", "ein", "klei\u00b7nes", "Leid", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einen Heller meiner gro\u00dfen Schuld,", "tokens": ["Ei\u00b7nen", "Hel\u00b7ler", "mei\u00b7ner", "gro\u00b7\u00dfen", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "F\u00fchl' ich schon ein leises Wohlbehagen", "tokens": ["F\u00fchl'", "ich", "schon", "ein", "lei\u00b7ses", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00dcber meine St\u00e4rke und Geduld.", "tokens": ["\u00dc\u00b7ber", "mei\u00b7ne", "St\u00e4r\u00b7ke", "und", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Seele mein, hast du denn ganz vergessen", "tokens": ["See\u00b7le", "mein", ",", "hast", "du", "denn", "ganz", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner S\u00fcnden, dunkel wie die Nacht,", "tokens": ["Dei\u00b7ner", "S\u00fcn\u00b7den", ",", "dun\u00b7kel", "wie", "die", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hast den Quell im Sande stolz gemessen,", "tokens": ["Hast", "den", "Quell", "im", "San\u00b7de", "stolz", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der weiten W\u00fcste nicht gedacht?", "tokens": ["Und", "der", "wei\u00b7ten", "W\u00fcs\u00b7te", "nicht", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ach, wie t\u00e4uschte dich die Eigenliebe", "tokens": ["Ach", ",", "wie", "t\u00e4uschte", "dich", "die", "Ei\u00b7gen\u00b7lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00dcber dein Beginnen sonder Treu,", "tokens": ["\u00dc\u00b7ber", "dein", "Be\u00b7gin\u00b7nen", "son\u00b7der", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eine M\u00fccke f\u00e4ngst du auf im Siebe,", "tokens": ["Ei\u00b7ne", "M\u00fc\u00b7cke", "f\u00e4ngst", "du", "auf", "im", "Sie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Das Kamel verschlingst du sonder Scheu.", "tokens": ["Das", "Ka\u00b7mel", "ver\u00b7schlingst", "du", "son\u00b7der", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Denkst wohl gar Verdienste zu gewinnen,", "tokens": ["Denkst", "wohl", "gar", "Ver\u00b7diens\u00b7te", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4hnst um dich der Siegespalmen Gr\u00fcn,", "tokens": ["W\u00e4hnst", "um", "dich", "der", "Sie\u00b7ge\u00b7spal\u00b7men", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, was du auch immer magst beginnen,", "tokens": ["Ach", ",", "was", "du", "auch", "im\u00b7mer", "magst", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Kr\u00e4fte \u00e4u\u00dferstes Bem\u00fchn,", "tokens": ["Dei\u00b7ner", "Kr\u00e4f\u00b7te", "\u00e4u\u00b7\u00dfers\u00b7tes", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nntest tausend Jahr dem Herrn du dienen,", "tokens": ["K\u00f6nn\u00b7test", "tau\u00b7send", "Jahr", "dem", "Herrn", "du", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In Zerknirschung b\u00fc\u00dfend fort und fort,", "tokens": ["In", "Zer\u00b7knir\u00b7schung", "b\u00fc\u00b7\u00dfend", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deine Frevel kannst du nimmer s\u00fchnen,", "tokens": ["Dei\u00b7ne", "Fre\u00b7vel", "kannst", "du", "nim\u00b7mer", "s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Gnade bleibt dein einz'ges Hoffnungswort.", "tokens": ["Gna\u00b7de", "bleibt", "dein", "einz'\u00b7ges", "Hoff\u00b7nungs\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie wenig hast du nicht gelitten", "tokens": ["Und", "wie", "we\u00b7nig", "hast", "du", "nicht", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Reue bittrer L\u00e4utrungsglut,", "tokens": ["In", "der", "Reu\u00b7e", "bit\u00b7trer", "L\u00e4ut\u00b7rungs\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wie lau und schw\u00e4chlich nicht gestritten", "tokens": ["Und", "wie", "lau", "und", "schw\u00e4ch\u00b7lich", "nicht", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "KON", "ADJD", "PTKNEG", "VVPP"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Gegen deiner innern Feinde Wut!", "tokens": ["Ge\u00b7gen", "dei\u00b7ner", "in\u00b7nern", "Fein\u00b7de", "Wut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kannst du eine Viertelstunde nennen,", "tokens": ["Kannst", "du", "ei\u00b7ne", "Vier\u00b7tel\u00b7stun\u00b7de", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo du ganz und gar dem Herrn geh\u00f6rt,", "tokens": ["Wo", "du", "ganz", "und", "gar", "dem", "Herrn", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Keine W\u00fcnsche dich von Jesu trennen,", "tokens": ["Kei\u00b7ne", "W\u00fcn\u00b7sche", "dich", "von", "Je\u00b7su", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kein Gedanke dein Gebet gest\u00f6rt?", "tokens": ["Kein", "Ge\u00b7dan\u00b7ke", "dein", "Ge\u00b7bet", "ge\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ach, mit jedem meiner Seufzer treten", "tokens": ["Ach", ",", "mit", "je\u00b7dem", "mei\u00b7ner", "Seuf\u00b7zer", "tre\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PIAT", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neue S\u00fcnden vor dein Angesicht.", "tokens": ["Neu\u00b7e", "S\u00fcn\u00b7den", "vor", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herr, ich bin nicht wert, zu dir zu beten,", "tokens": ["Herr", ",", "ich", "bin", "nicht", "wert", ",", "zu", "dir", "zu", "be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schone mein, du starker Gott im Licht.", "tokens": ["Scho\u00b7ne", "mein", ",", "du", "star\u00b7ker", "Gott", "im", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "PPER", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O! mich fa\u00dft ein ungeheurer Schrecken,", "tokens": ["O", "!", "mich", "fa\u00dft", "ein", "un\u00b7ge\u00b7heu\u00b7rer", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df ich so vermessen mich erk\u00fchnt,", "tokens": ["Da\u00df", "ich", "so", "ver\u00b7mes\u00b7sen", "mich", "er\u00b7k\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Weh, mein ganzes Leben ist ein Flecken,", "tokens": ["Weh", ",", "mein", "gan\u00b7zes", "Le\u00b7ben", "ist", "ein", "Fle\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Jede Stunde hat den Tod verdient.", "tokens": ["Je\u00b7de", "Stun\u00b7de", "hat", "den", "Tod", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dennoch, dennoch darfst du nicht verzagen,", "tokens": ["Den\u00b7noch", ",", "den\u00b7noch", "darfst", "du", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht in deines tiefsten Elends Drang,", "tokens": ["Nicht", "in", "dei\u00b7nes", "tiefs\u00b7ten", "E\u00b7lends", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mu\u00dft die Schmerzen grimm, die in dir nagen,", "tokens": ["Mu\u00dft", "die", "Schmer\u00b7zen", "grimm", ",", "die", "in", "dir", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fesseln mit der Hoffnung s\u00fc\u00dfem Zwang.", "tokens": ["Fes\u00b7seln", "mit", "der", "Hoff\u00b7nung", "s\u00fc\u00b7\u00dfem", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jesus will es, und du mu\u00dft vollbringen,", "tokens": ["Je\u00b7sus", "will", "es", ",", "und", "du", "mu\u00dft", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ob dich seine Milde fast zerdr\u00fcckt,", "tokens": ["Ob", "dich", "sei\u00b7ne", "Mil\u00b7de", "fast", "zer\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Darfst nicht trotzend in Verzweiflung ringen,", "tokens": ["Darfst", "nicht", "trot\u00b7zend", "in", "Ver\u00b7zwei\u00b7flung", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie der eigne Wille dich ber\u00fcckt.", "tokens": ["Wie", "der", "eig\u00b7ne", "Wil\u00b7le", "dich", "be\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wie der Pharus an dem Seegestade", "tokens": ["Wie", "der", "Pha\u00b7rus", "an", "dem", "See\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frieden leuchtet durch der St\u00fcrme Wut,", "tokens": ["Frie\u00b7den", "leuch\u00b7tet", "durch", "der", "St\u00fcr\u00b7me", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Strahlt so mildiglich das Kreuz der Gnade,", "tokens": ["Strahlt", "so", "mil\u00b7dig\u00b7lich", "das", "Kreuz", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Drum nur Mut, bedr\u00e4ngte Seele, Mut!", "tokens": ["Drum", "nur", "Mut", ",", "be\u00b7dr\u00e4ng\u00b7te", "See\u00b7le", ",", "Mut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ADV", "NN", "$,", "ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Halte fest in Demut und Vertrauen", "tokens": ["Hal\u00b7te", "fest", "in", "De\u00b7mut", "und", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seele mein, mit deiner ganzen Macht,", "tokens": ["See\u00b7le", "mein", ",", "mit", "dei\u00b7ner", "gan\u00b7zen", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Siehe, wie f\u00fcnf rote Sonnen schauen", "tokens": ["Sie\u00b7he", ",", "wie", "f\u00fcnf", "ro\u00b7te", "Son\u00b7nen", "schau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOKOM", "CARD", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Jesu Wunden durch die w\u00fcste Nacht.", "tokens": ["Je\u00b7su", "Wun\u00b7den", "durch", "die", "w\u00fcs\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und wie einst die Arche trug das Leben", "tokens": ["Und", "wie", "einst", "die", "Ar\u00b7che", "trug", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch der S\u00fcnde allgemeinen Tod,", "tokens": ["Durch", "der", "S\u00fcn\u00b7de", "all\u00b7ge\u00b7mei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird das s\u00fc\u00dfe Kreuz mich rettend heben,", "tokens": ["Wird", "das", "s\u00fc\u00b7\u00dfe", "Kreuz", "mich", "ret\u00b7tend", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn entsetzlich das Verderben droht.", "tokens": ["Wenn", "ent\u00b7setz\u00b7lich", "das", "Ver\u00b7der\u00b7ben", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ja, ich will auf Jesu Worte bauen,", "tokens": ["Ja", ",", "ich", "will", "auf", "Je\u00b7su", "Wor\u00b7te", "bau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seh ich gleich nicht ihn, und nur die Nacht,", "tokens": ["Seh", "ich", "gleich", "nicht", "ihn", ",", "und", "nur", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PPER", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fest nur, fest in Demut und Vertrauen,", "tokens": ["Fest", "nur", ",", "fest", "in", "De\u00b7mut", "und", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Seele mein, mit deiner ganzen Macht.", "tokens": ["See\u00b7le", "mein", ",", "mit", "dei\u00b7ner", "gan\u00b7zen", "Macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}