{"textgrid.poem.30978": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum glauben ist nicht m\u00fcglich, die Sinnen zu bezwingen;", "tokens": ["Zum", "glau\u00b7ben", "ist", "nicht", "m\u00fcg\u00b7lich", ",", "die", "Sin\u00b7nen", "zu", "be\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum heucheln ists wol m\u00f6glich, die Sinnen anzubringen.", "tokens": ["Zum", "heu\u00b7cheln", "ists", "wol", "m\u00f6g\u00b7lich", ",", "die", "Sin\u00b7nen", "an\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}