{"dta.poem.16788": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "147.  Die Ewge Gnadenwahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach zweifele doch nicht: Sep nur au\u00df GOtt gebohrn/", "tokens": ["Ach", "zwei\u00b7fe\u00b7le", "doch", "nicht", ":", "Sep", "nur", "au\u00df", "Gott", "ge\u00b7bohrn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PTKNEG", "$.", "NE", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So bistu ewiglich zum Leben au\u00dferkohrn.", "tokens": ["So", "bis\u00b7tu", "e\u00b7wig\u00b7lich", "zum", "Le\u00b7ben", "au\u00b7\u00dfer\u00b7kohrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}