{"textgrid.poem.56095": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und der Engel sprach und gab sich M\u00fch", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Engel sprach und gab sich M\u00fch", "tokens": ["Und", "der", "En\u00b7gel", "sprach", "und", "gab", "sich", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "PRF", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "an dem Mann, der seine F\u00e4uste ballte:", "tokens": ["an", "dem", "Mann", ",", "der", "sei\u00b7ne", "F\u00e4us\u00b7te", "ball\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber siehst du nicht an jeder Falte,", "tokens": ["A\u00b7ber", "siehst", "du", "nicht", "an", "je\u00b7der", "Fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "da\u00df sie k\u00fchl ist wie die Gottesfr\u00fch.", "tokens": ["da\u00df", "sie", "k\u00fchl", "ist", "wie", "die", "Got\u00b7tes\u00b7fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Doch der andre sah ihn finster an,", "tokens": ["Doch", "der", "and\u00b7re", "sah", "ihn", "fins\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "murmelnd nur: Was hat sie so verwandelt?", "tokens": ["mur\u00b7melnd", "nur", ":", "Was", "hat", "sie", "so", "ver\u00b7wan\u00b7delt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch da schrie der Engel: Zimmermann,", "tokens": ["Doch", "da", "schrie", "der", "En\u00b7gel", ":", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "merkst du's noch nicht, da\u00df der Herrgott handelt?", "tokens": ["merkst", "du's", "noch", "nicht", ",", "da\u00df", "der", "Herr\u00b7gott", "han\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Weil du Bretter machst, in deinem Stolze,", "tokens": ["Weil", "du", "Bret\u00b7ter", "machst", ",", "in", "dei\u00b7nem", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "willst du wirklich ", "tokens": ["willst", "du", "wirk\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "der bescheiden aus dem gleichen Holze", "tokens": ["der", "be\u00b7schei\u00b7den", "aus", "dem", "glei\u00b7chen", "Hol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bl\u00e4tter treiben macht und Knospen schwelln?", "tokens": ["Bl\u00e4t\u00b7ter", "trei\u00b7ben", "macht", "und", "Knos\u00b7pen", "schwelln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Er begriff. Und wie er jetzt die Blicke,", "tokens": ["Er", "be\u00b7griff", ".", "Und", "wie", "er", "jetzt", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWAV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "recht erschrocken, zu dem Engel hob,", "tokens": ["recht", "er\u00b7schro\u00b7cken", ",", "zu", "dem", "En\u00b7gel", "hob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "war der fort. Da schob er seine dicke", "tokens": ["war", "der", "fort", ".", "Da", "schob", "er", "sei\u00b7ne", "di\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00fctze langsam ab. Dann sang er lob.", "tokens": ["M\u00fct\u00b7ze", "lang\u00b7sam", "ab", ".", "Dann", "sang", "er", "lob", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}