{"textgrid.poem.44432": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Paganini", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00e4rst ein M\u00f6rder nicht? Selbstm\u00f6rder du!", "tokens": ["Du", "w\u00e4rst", "ein", "M\u00f6r\u00b7der", "nicht", "?", "Selbst\u00b7m\u00f6r\u00b7der", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$.", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was \u00f6ffnest du des Busens sichres Haus", "tokens": ["Was", "\u00f6ff\u00b7nest", "du", "des", "Bu\u00b7sens", "sich\u00b7res", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6\u00dft sie aus, die unverh\u00fcllte Seele,", "tokens": ["Und", "st\u00f6\u00dft", "sie", "aus", ",", "die", "un\u00b7ver\u00b7h\u00fcll\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stellst sie hin, den Gaffern eine Lust?", "tokens": ["Und", "stellst", "sie", "hin", ",", "den", "Gaf\u00b7fern", "ei\u00b7ne", "Lust", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00e4hrst mit dem Dolch nach ihr und triffst;", "tokens": ["F\u00e4hrst", "mit", "dem", "Dolch", "nach", "ihr", "und", "triffst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und weinst und klagst darob", "tokens": ["Und", "weinst", "und", "klagst", "da\u00b7rob"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und z\u00e4hlst mit Tr\u00e4nen ihre blutgen Tropfen?", "tokens": ["Und", "z\u00e4hlst", "mit", "Tr\u00e4\u00b7nen", "ih\u00b7re", "blut\u00b7gen", "Trop\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Drauf h\u00f6hnst du sie und dich", "tokens": ["Drauf", "h\u00f6hnst", "du", "sie", "und", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Aufjubelnd laut in gellendem Gel\u00e4chter.", "tokens": ["Auf\u00b7ju\u00b7belnd", "laut", "in", "gel\u00b7len\u00b7dem", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du nicht ein M\u00f6rder? Frevler du am Ich!", "tokens": ["Du", "nicht", "ein", "M\u00f6r\u00b7der", "?", "Frev\u00b7ler", "du", "am", "Ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "$.", "NN", "PPER", "APPRART", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Des eignen Leibs, der eignen Seele M\u00f6rder;", "tokens": ["Des", "eig\u00b7nen", "Leibs", ",", "der", "eig\u00b7nen", "See\u00b7le", "M\u00f6r\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und auch der meine \u2013 doch ich weich dir aus!", "tokens": ["Und", "auch", "der", "mei\u00b7ne", "\u2013", "doch", "ich", "weich", "dir", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPOSAT", "$(", "KON", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}