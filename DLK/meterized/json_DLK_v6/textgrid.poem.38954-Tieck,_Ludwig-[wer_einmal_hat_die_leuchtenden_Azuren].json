{"textgrid.poem.38954": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[wer einmal hat die leuchtenden Azuren]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einmal hat die leuchtenden Azuren,", "tokens": ["Wer", "ein\u00b7mal", "hat", "die", "leuch\u00b7ten\u00b7den", "A\u00b7zu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchsp\u00e4ht mit seinem Adlerblick, dem k\u00fchnen,", "tokens": ["Durch\u00b7sp\u00e4ht", "mit", "sei\u00b7nem", "Ad\u00b7ler\u00b7blick", ",", "dem", "k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der irrt nicht auf den hohen Himmelsb\u00fchnen,", "tokens": ["Der", "irrt", "nicht", "auf", "den", "ho\u00b7hen", "Him\u00b7mels\u00b7b\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sich, kennt er die schaffenden Naturen.", "tokens": ["Wie", "sich", ",", "kennt", "er", "die", "schaf\u00b7fen\u00b7den", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Muthigen Schritt's geht auf den Sternenfluren", "tokens": ["Mut\u00b7hi\u00b7gen", "Schritt's", "geht", "auf", "den", "Ster\u00b7nen\u00b7flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Er l\u00e4chelnd, mit dem All sich zu vers\u00fchnen,", "tokens": ["Er", "l\u00e4\u00b7chelnd", ",", "mit", "dem", "All", "sich", "zu", "ver\u00b7s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wei\u00df wie Blumen bl\u00fchn und Pflanzen gr\u00fcnen,", "tokens": ["Er", "wei\u00df", "wie", "Blu\u00b7men", "bl\u00fchn", "und", "Pflan\u00b7zen", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Licht gl\u00e4nzt, gehn, fliegen, schwimmen Kreaturen.", "tokens": ["Licht", "gl\u00e4nzt", ",", "gehn", ",", "flie\u00b7gen", ",", "schwim\u00b7men", "Kre\u00b7a\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Derselbe Mann, den ich mu\u00df tief verehren,", "tokens": ["Der\u00b7sel\u00b7be", "Mann", ",", "den", "ich", "mu\u00df", "tief", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Derselbe Mann, der so beschuht zum wandern,", "tokens": ["Der\u00b7sel\u00b7be", "Mann", ",", "der", "so", "be\u00b7schuht", "zum", "wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "APPRART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Derselbe Mann, auf dieser hohen Leiter,", "tokens": ["Der\u00b7sel\u00b7be", "Mann", ",", "auf", "die\u00b7ser", "ho\u00b7hen", "Lei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kommt mit der Zeit vielleicht noch immer weiter,", "tokens": ["Kommt", "mit", "der", "Zeit", "viel\u00b7leicht", "noch", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist, Wunder, o ein Mensch nur wie wir andern,", "tokens": ["Ist", ",", "Wun\u00b7der", ",", "o", "ein", "Mensch", "nur", "wie", "wir", "an\u00b7dern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "FM", "ART", "NN", "ADV", "KOKOM", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch mehr, kann dieses Lob beinah entbehren.", "tokens": ["Noch", "mehr", ",", "kann", "die\u00b7ses", "Lob", "bei\u00b7nah", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VMFIN", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}