{"textgrid.poem.52127": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sey, da\u00df K\u00f6nig stammt und kommt vom Worte k\u00f6nnen", "tokens": ["Es", "sey", ",", "da\u00df", "K\u00f6\u00b7nig", "stammt", "und", "kommt", "vom", "Wor\u00b7te", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch man kan Gewalt und Macht und Recht beginnen:", "tokens": ["Da\u00b7durch", "man", "kan", "Ge\u00b7walt", "und", "Macht", "und", "Recht", "be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VMFIN", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sey, da\u00df sich das Wort vom K\u00fchnen r\u00fchmt und schreibt,", "tokens": ["Es", "sey", ",", "da\u00df", "sich", "das", "Wort", "vom", "K\u00fch\u00b7nen", "r\u00fchmt", "und", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist es, da\u00df K\u00f6nigs Marck ein Marck von beyden bleibt.", "tokens": ["Ist", "es", ",", "da\u00df", "K\u00f6\u00b7nigs", "Marck", "ein", "Marck", "von", "bey\u00b7den", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "NN", "NN", "ART", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was nennt das Marck ihr nun von den Arctoischen Waffen,", "tokens": ["Was", "nennt", "das", "Marck", "ihr", "nun", "von", "den", "Arc\u00b7to\u00b7i\u00b7schen", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den K\u00f6nigs Marckt den Marckt von K\u00fchen und von Schaaffen:", "tokens": ["Den", "K\u00f6\u00b7nigs", "Marckt", "den", "Marckt", "von", "K\u00fc\u00b7hen", "und", "von", "Schaaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Arg macht ihr es, der Held ist k\u00fchn und wird euch k\u00fchn,", "tokens": ["Arg", "macht", "ihr", "es", ",", "der", "Held", "ist", "k\u00fchn", "und", "wird", "euch", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ihr zun Ochsen theils des Plato werdet fliehn.", "tokens": ["Da\u00df", "ihr", "zun", "Och\u00b7sen", "theils", "des", "Pla\u00b7to", "wer\u00b7det", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NE", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von euch wird manche Marck noch K\u00f6nigs Marck bekommen,", "tokens": ["Von", "euch", "wird", "man\u00b7che", "Marck", "noch", "K\u00f6\u00b7nigs", "Marck", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "ADV", "NN", "NN", "VVINF", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den merckt es, welcher M\u00e4rckt' in Deutschland eingenommen,", "tokens": ["Den", "merckt", "es", ",", "wel\u00b7cher", "M\u00e4r\u00b7ckt'", "in", "Deutschland", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAT", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bringt L\u00e4nder, bringt zuletzt selbst K\u00f6nigreiche bey,", "tokens": ["Bringt", "L\u00e4n\u00b7der", ",", "bringt", "zu\u00b7letzt", "selbst", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Last weg den Marckt, drauff lest, was denn sein Nahme sey.", "tokens": ["Last", "weg", "den", "Marckt", ",", "drauff", "lest", ",", "was", "denn", "sein", "Nah\u00b7me", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PAV", "VVFIN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}