{"dta.poem.13040": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ezu dem wer bin dann ich? weil ich nichts bergen will;", "tokens": ["\u201e", "zu", "dem", "wer", "bin", "dann", "ich", "?", "weil", "ich", "nichts", "ber\u00b7gen", "will", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "PWS", "VAFIN", "ADV", "PPER", "$.", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier schwieg der gute Grei\u00df, und alle waren still.", "tokens": ["Hier", "schwieg", "der", "gu\u00b7te", "Grei\u00df", ",", "und", "al\u00b7le", "wa\u00b7ren", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drauf fieng er wieder an: \u201eh\u00e4tt ich dann nichts zu weisen?", "tokens": ["Drauf", "fi\u00b7eng", "er", "wie\u00b7der", "an", ":", "\u201e", "h\u00e4tt", "ich", "dann", "nichts", "zu", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$(", "VAFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "280\u201dKennt ihr den guten Rath dann nicht, mich, diesen Greisen?", "tokens": ["\"", "Kennt", "ihr", "den", "gu\u00b7ten", "Rath", "dann", "nicht", ",", "mich", ",", "die\u00b7sen", "Grei\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$,", "PPER", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas hab ich f\u00fcr das Heil des Vaterlands gethan?", "tokens": ["\u201e", "was", "hab", "ich", "f\u00fcr", "das", "Heil", "des", "Va\u00b7ter\u00b7lands", "ge\u00b7than", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edoch nein: man fange nicht von mir zu reden an.", "tokens": ["\u201e", "doch", "nein", ":", "man", "fan\u00b7ge", "nicht", "von", "mir", "zu", "re\u00b7den", "an", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKANT", "$.", "PIS", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}