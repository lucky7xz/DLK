{"textgrid.poem.41339": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Fallacia causae non causae", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotz einer Elster schwatzt Ursin,", "tokens": ["Trotz", "ei\u00b7ner", "Els\u00b7ter", "schwatzt", "Ur\u00b7sin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keine Grabschrift l\u00fcgt, wie er:", "tokens": ["Und", "kei\u00b7ne", "Grab\u00b7schrift", "l\u00fcgt", ",", "wie", "er", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem j\u00fcdisch schreienden Gingrin", "tokens": ["Dem", "j\u00fc\u00b7disch", "schrei\u00b7en\u00b7den", "Gin\u00b7grin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00e4llt, auch im Schlaf, das Schweigen schwer.", "tokens": ["F\u00e4llt", ",", "auch", "im", "Schlaf", ",", "das", "Schwei\u00b7gen", "schwer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie, deren Mund nichts sprachlos macht,", "tokens": ["Sie", ",", "de\u00b7ren", "Mund", "nichts", "sprach\u00b7los", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie reden heut, als mit Bedacht,", "tokens": ["Sie", "re\u00b7den", "heut", ",", "als", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verbindlich, sparsamer und leiser.", "tokens": ["Ver\u00b7bind\u00b7lich", ",", "spar\u00b7sa\u00b7mer", "und", "lei\u00b7ser", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind heute beide Thoren weiser?", "tokens": ["Sind", "heu\u00b7te", "bei\u00b7de", "Tho\u00b7ren", "wei\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O nein! Beim Frost der letzten Nacht", "tokens": ["O", "nein", "!", "Beim", "Frost", "der", "letz\u00b7ten", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ward jener taub, und dieser heiser.", "tokens": ["Ward", "je\u00b7ner", "taub", ",", "und", "die\u00b7ser", "hei\u00b7ser", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJD", "$,", "KON", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}