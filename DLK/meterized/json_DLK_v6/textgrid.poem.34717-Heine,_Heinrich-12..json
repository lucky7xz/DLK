{"textgrid.poem.34717": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ich sehne mich nach Tr\u00e4nen,", "tokens": ["Ach", ",", "ich", "seh\u00b7ne", "mich", "nach", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebestr\u00e4nen, schmerzenmild,", "tokens": ["Lie\u00b7be\u00b7str\u00e4\u00b7nen", ",", "schmer\u00b7zen\u00b7mild", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich f\u00fcrchte, dieses Sehnen", "tokens": ["Und", "ich", "f\u00fcrch\u00b7te", ",", "die\u00b7ses", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird am Ende noch erf\u00fcllt.", "tokens": ["Wird", "am", "En\u00b7de", "noch", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, der Liebe s\u00fc\u00dfes Elend", "tokens": ["Ach", ",", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfes", "E\u00b7lend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Liebe bittre Lust", "tokens": ["Und", "der", "Lie\u00b7be", "bitt\u00b7re", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleicht sich wieder, himmlisch qu\u00e4lend,", "tokens": ["Schleicht", "sich", "wie\u00b7der", ",", "himm\u00b7lisch", "qu\u00e4\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die kaum genesne Brust.", "tokens": ["In", "die", "kaum", "ge\u00b7nes\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}