{"textgrid.poem.40825": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ew'ge Mond im Dom der N\u00e4chte schimmert,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ew'ge Mond im Dom der N\u00e4chte schimmert,", "tokens": ["Der", "ew'\u00b7ge", "Mond", "im", "Dom", "der", "N\u00e4ch\u00b7te", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ew'ge Lamp' im Klosterkirchlein flimmert;", "tokens": ["Die", "ew'\u00b7ge", "Lamp'", "im", "Klos\u00b7ter\u00b7kirch\u00b7lein", "flim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Horch Mitternacht! Von den zw\u00f6lf Schl\u00e4gen gellen", "tokens": ["Horch", "Mit\u00b7ter\u00b7nacht", "!", "Von", "den", "zw\u00f6lf", "Schl\u00e4\u00b7gen", "gel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ART", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der M\u00f6nche S\u00e4rge, wie einst ihre Zellen!", "tokens": ["Der", "M\u00f6n\u00b7che", "S\u00e4r\u00b7ge", ",", "wie", "einst", "ih\u00b7re", "Zel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie zur Hora einst, entsteigt den Bahren", "tokens": ["Und", "wie", "zur", "Ho\u00b7ra", "einst", ",", "ent\u00b7steigt", "den", "Bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein dunkles Heer in schleppenden Talaren,", "tokens": ["Ein", "dunk\u00b7les", "Heer", "in", "schlep\u00b7pen\u00b7den", "Ta\u00b7la\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voran die Kirchenfahne mit dem Kranze", "tokens": ["Vo\u00b7ran", "die", "Kir\u00b7chen\u00b7fah\u00b7ne", "mit", "dem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ein gewaltig Kreuz auf hoher Lanze.", "tokens": ["Und", "ein", "ge\u00b7wal\u00b7tig", "Kreuz", "auf", "ho\u00b7her", "Lan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In langem Zug, gesenkten Auges, schweigend,", "tokens": ["In", "lan\u00b7gem", "Zug", ",", "ge\u00b7senk\u00b7ten", "Au\u00b7ges", ",", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Langsam und feierlich zum Chore steigend,", "tokens": ["Lang\u00b7sam", "und", "fei\u00b7er\u00b7lich", "zum", "Cho\u00b7re", "stei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jetzt braust ihr Lied, und Orgelklang gewittert,", "tokens": ["Jetzt", "braust", "ihr", "Lied", ",", "und", "Or\u00b7gel\u00b7klang", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Wand und Pfeiler bebt, die Kuppel zittert:", "tokens": ["Da\u00df", "Wand", "und", "Pfei\u00b7ler", "bebt", ",", "die", "Kup\u00b7pel", "zit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbweh! Was wir bauten, ist in Schutt geschmettert!", "tokens": ["\u00bb", "weh", "!", "Was", "wir", "bau\u00b7ten", ",", "ist", "in", "Schutt", "ge\u00b7schmet\u00b7tert", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weh! Was wir s\u00e4ten, hat der Sturm entbl\u00e4ttert!", "tokens": ["Weh", "!", "Was", "wir", "s\u00e4\u00b7ten", ",", "hat", "der", "Sturm", "ent\u00b7bl\u00e4t\u00b7tert", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Loos all' unsres Lebens und Gebetes,", "tokens": ["Das", "Loos", "all'", "uns\u00b7res", "Le\u00b7bens", "und", "Ge\u00b7be\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mensch zertritt es, und der Wind verweht es!\u00ab", "tokens": ["Der", "Mensch", "zer\u00b7tritt", "es", ",", "und", "der", "Wind", "ver\u00b7weht", "es", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort unten wandeln zwei verblichne Meister!", "tokens": ["Dort", "un\u00b7ten", "wan\u00b7deln", "zwei", "ver\u00b7blich\u00b7ne", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sind des Bildners und des Malers Geister,", "tokens": ["Das", "sind", "des", "Bild\u00b7ners", "und", "des", "Ma\u00b7lers", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt vor zerfallnen Marmorbildern stehend,", "tokens": ["Jetzt", "vor", "zer\u00b7fall\u00b7nen", "Mar\u00b7mor\u00b7bil\u00b7dern", "ste\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt manch entf\u00e4rbtes Altarblatt besehend:", "tokens": ["Jetzt", "manch", "ent\u00b7f\u00e4rb\u00b7tes", "Al\u00b7tar\u00b7blatt", "be\u00b7se\u00b7hend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbweh dir, o Zeit! Verst\u00fcmmelt wie ein wilder,", "tokens": ["\u00bb", "weh", "dir", ",", "o", "Zeit", "!", "Ver\u00b7st\u00fcm\u00b7melt", "wie", "ein", "wil\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "FM", "NN", "$.", "VVFIN", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Muthwill'ger Bube hast du unsre Bilder!", "tokens": ["Muthwill'\u00b7ger", "Bu\u00b7be", "hast", "du", "uns\u00b7re", "Bil\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weh euch, o Staub und Moose! Euer Weben,", "tokens": ["Weh", "euch", ",", "o", "Staub", "und", "Moo\u00b7se", "!", "Eu\u00b7er", "We\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Bahrtuch ist's von unsres Geistes Leben!\u00ab", "tokens": ["Das", "Bahr\u00b7tuch", "ist's", "von", "uns\u00b7res", "Geis\u00b7tes", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und wieder trat aus einem schlichten Grabe", "tokens": ["Und", "wie\u00b7der", "trat", "aus", "ei\u00b7nem", "schlich\u00b7ten", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mann mit Zirkel, Winkelma\u00df und Stabe;", "tokens": ["Ein", "Mann", "mit", "Zir\u00b7kel", ",", "Win\u00b7kel\u00b7ma\u00df", "und", "Sta\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er setzte sich auf morsche Quadernst\u00fccke,", "tokens": ["Er", "setz\u00b7te", "sich", "auf", "mor\u00b7sche", "Qua\u00b7dern\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Arkad' und Kuppel ma\u00dfen seine Blicke:", "tokens": ["Ar\u00b7kad'", "und", "Kup\u00b7pel", "ma\u00b7\u00dfen", "sei\u00b7ne", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbweh! Stolzer S\u00e4ulen Zier liegt rings gebrochen!", "tokens": ["\u00bb", "weh", "!", "Stol\u00b7zer", "S\u00e4u\u00b7len", "Zier", "liegt", "rings", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADJA", "NN", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ist's, als w\u00e4ren's meine eignen Knochen!", "tokens": ["Mir", "ist's", ",", "als", "w\u00e4\u00b7ren's", "mei\u00b7ne", "eig\u00b7nen", "Kno\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer untergeht im Werk all seines Lebens,", "tokens": ["Wer", "un\u00b7ter\u00b7geht", "im", "Werk", "all", "sei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der stirbt wohl zwiefach, ach, und lebt vergebens!\u00ab", "tokens": ["Der", "stirbt", "wohl", "zwie\u00b7fach", ",", "ach", ",", "und", "lebt", "ver\u00b7ge\u00b7bens", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "ITJ", "$,", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Inde\u00df stand l\u00e4chelnd mitten unter ihnen", "tokens": ["In\u00b7de\u00df", "stand", "l\u00e4\u00b7chelnd", "mit\u00b7ten", "un\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der helle Mond und sprach mit heit'ren Mienen:", "tokens": ["Der", "hel\u00b7le", "Mond", "und", "sprach", "mit", "heit'\u00b7ren", "Mie\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich wall' als Geist der Sonn' in dieser Stunde,", "tokens": ["\u00bb", "ich", "wall'", "als", "Geist", "der", "Sonn'", "in", "die\u00b7ser", "Stun\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "KOKOM", "NN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so spricht sie zu euch aus meinem Munde:", "tokens": ["Und", "so", "spricht", "sie", "zu", "euch", "aus", "mei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Ich wandle meine Bahn seit Jahr und Jahren,", "tokens": ["Ich", "wand\u00b7le", "mei\u00b7ne", "Bahn", "seit", "Jahr", "und", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer hat des Leides mehr als ich erfahren?", "tokens": ["Wer", "hat", "des", "Lei\u00b7des", "mehr", "als", "ich", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was nennt ihr eures Lebens Preis vergebens?", "tokens": ["Was", "nennt", "ihr", "eu\u00b7res", "Le\u00b7bens", "Preis", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O seht den schn\u00f6den Preis all meines Lebens!", "tokens": ["O", "seht", "den", "schn\u00f6\u00b7den", "Preis", "all", "mei\u00b7nes", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ich bin das Licht! \u2013 Die Welt liegt noch in N\u00e4chten!", "tokens": ["Ich", "bin", "das", "Licht", "!", "\u2013", "Die", "Welt", "liegt", "noch", "in", "N\u00e4ch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin die Freiheit! \u2013 Sie ist voll von Knechten!", "tokens": ["Ich", "bin", "die", "Frei\u00b7heit", "!", "\u2013", "Sie", "ist", "voll", "von", "Knech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin die Liebe! \u2013 Sie ist hassestrunken!", "tokens": ["Ich", "bin", "die", "Lie\u00b7be", "!", "\u2013", "Sie", "ist", "has\u00b7ses\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin die Wahrheit! \u2013 Sie in Trug versunken!\u00ab", "tokens": ["Ich", "bin", "die", "Wahr\u00b7heit", "!", "\u2013", "Sie", "in", "Trug", "ver\u00b7sun\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "PPER", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und wie er's spra