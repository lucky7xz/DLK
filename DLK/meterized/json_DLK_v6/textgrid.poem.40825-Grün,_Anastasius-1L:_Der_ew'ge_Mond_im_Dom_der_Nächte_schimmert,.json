", "Lam\u00b7pe", "matt", "und", "mat\u00b7ter", "flim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Leichenstein' im fahlen Zwielicht ragen,", "tokens": ["Die", "Lei\u00b7chen\u00b7stein'", "im", "fah\u00b7len", "Zwie\u00b7licht", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Osten graut's, mich d\u00fcnkt, es will bald tagen.", "tokens": ["Im", "Os\u00b7ten", "graut's", ",", "mich", "d\u00fcnkt", ",", "es", "will", "bald", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}