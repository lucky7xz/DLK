{"dta.poem.20717": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die                  \n  g\u00f6ttlichverkannte Phillis im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst der Hirsch voll Liebesflamme                 ", "tokens": ["Als", "j\u00fcngst", "der", "Hirsch", "voll", "Lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Thier verfolgte, das ihn floh,", "tokens": ["Sein", "Thier", "ver\u00b7folg\u00b7te", ",", "das", "ihn", "floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da stand an einem Birkenstamme", "tokens": ["Da", "stand", "an", "ei\u00b7nem", "Bir\u00b7kens\u00b7tam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der alte J\u00e4ger Sylvio.", "tokens": ["Der", "al\u00b7te", "J\u00e4\u00b7ger", "Syl\u00b7vio", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ein Greis ist er, und noch verstocken", "tokens": ["Ein", "Greis", "ist", "er", ",", "und", "noch", "ver\u00b7sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihm des Lebens S\u00e4fte nicht,", "tokens": ["In", "ihm", "des", "Le\u00b7bens", "S\u00e4f\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm flattern taubenwei\u00dfe Locken", "tokens": ["Ihm", "flat\u00b7tern", "tau\u00b7ben\u00b7wei\u00b7\u00dfe", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um rosenfarbnes Angesicht.", "tokens": ["Um", "ro\u00b7sen\u00b7farb\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Betrachtend stand vom Messerschnitte", "tokens": ["Be\u00b7trach\u00b7tend", "stand", "vom", "Mes\u00b7ser\u00b7schnit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwachsen in den starren Baum:", "tokens": ["Ver\u00b7wach\u00b7sen", "in", "den", "star\u00b7ren", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da rauschten meiner Phillis Tritte", "tokens": ["Da", "rauschten", "mei\u00b7ner", "Phil\u00b7lis", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vorbei und ihres Kleides Saum.", "tokens": ["Vor\u00b7bei", "und", "ih\u00b7res", "Klei\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zur\u00fcck fuhr Sylvio der Alte,", "tokens": ["Zu\u00b7r\u00fcck", "fuhr", "Syl\u00b7vio", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr G\u00f6tter! rief er, ja sie rauscht", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "rief", "er", ",", "ja", "sie", "rauscht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An mir vorbei, die wohlgestalte", "tokens": ["An", "mir", "vor\u00b7bei", ",", "die", "wohl\u00b7ge\u00b7stal\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Diana, die das Wild belauscht.               ", "tokens": ["Di\u00b7a\u00b7na", ",", "die", "das", "Wild", "be\u00b7lauscht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Welch eine Miene! welche Wangen!", "tokens": ["Welch", "ei\u00b7ne", "Mie\u00b7ne", "!", "wel\u00b7che", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Ph\u00f6bus! welch ein Angesicht!", "tokens": ["O", "Ph\u00f6\u00b7bus", "!", "welch", "ein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kann den Hirsch durch Blicke fangen", "tokens": ["Sie", "kann", "den", "Hirsch", "durch", "Bli\u00b7cke", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brauchet Pfeil und Bogen nicht.", "tokens": ["Und", "brau\u00b7chet", "Pfeil", "und", "Bo\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Fleuch nicht, Diana! bleib, ich schlachte", "tokens": ["Fleuch", "nicht", ",", "Di\u00b7a\u00b7na", "!", "bleib", ",", "ich", "schlach\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "NE", "$.", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Opfer dir ein junges Reh,", "tokens": ["Zum", "Op\u00b7fer", "dir", "ein", "jun\u00b7ges", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ich so zahm wie L\u00e4mmer machte,", "tokens": ["Das", "ich", "so", "zahm", "wie", "L\u00e4m\u00b7mer", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ist es wie neuer Schnee.", "tokens": ["So", "wei\u00df", "ist", "es", "wie", "neu\u00b7er", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich will dir einen Eber w\u00fcrgen,", "tokens": ["Ich", "will", "dir", "ei\u00b7nen", "E\u00b7ber", "w\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der trotzig durch Mor\u00e4ste brach,", "tokens": ["Der", "trot\u00b7zig", "durch", "Mo\u00b7r\u00e4s\u00b7te", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem ich vergebens auf Gebirgen", "tokens": ["Dem", "ich", "ver\u00b7ge\u00b7bens", "auf", "Ge\u00b7bir\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in Geb\u00fcschen sp\u00e4hte nach.", "tokens": ["Und", "in", "Ge\u00b7b\u00fc\u00b7schen", "sp\u00e4h\u00b7te", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich will ihn finden, o Diane!", "tokens": ["Ich", "will", "ihn", "fin\u00b7den", ",", "o", "Di\u00b7a\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Liebesg\u00f6tter helfen mir.", "tokens": ["Drei", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "hel\u00b7fen", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Wurfspie\u00df trift trotz seinem Zahne,", "tokens": ["Mein", "Wurf\u00b7spie\u00df", "trift", "trotz", "sei\u00b7nem", "Zah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu F\u00fc\u00dfen soll er bluten dir \u2014", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "soll", "er", "blu\u00b7ten", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ha! welch ein Blick! mich zu verachten?", "tokens": ["Ha", "!", "welch", "ein", "Blick", "!", "mich", "zu", "ver\u00b7ach\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "$.", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun will ich dir auf diesem Block", "tokens": ["Nun", "will", "ich", "dir", "auf", "die\u00b7sem", "Block"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein wei\u00dfes Reh zum Opfer schlachten,", "tokens": ["Kein", "wei\u00b7\u00dfes", "Reh", "zum", "Op\u00b7fer", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, einen schwarzen Ziegenbock.", "tokens": ["Nein", ",", "ei\u00b7nen", "schwar\u00b7zen", "Zie\u00b7gen\u00b7bock", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}