{"textgrid.poem.43465": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was kann aus Ru\u00dfland kommen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kann aus Ru\u00dfland kommen", "tokens": ["Was", "kann", "aus", "Ru\u00df\u00b7land", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu unserm Nutz und Frommen?", "tokens": ["Zu", "un\u00b7serm", "Nutz", "und", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die russische Cultur", "tokens": ["Die", "rus\u00b7si\u00b7sche", "Cul\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die pa\u00dft f\u00fcr Ru\u00dfland nur.", "tokens": ["Die", "pa\u00dft", "f\u00fcr", "Ru\u00df\u00b7land", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wachsen zu viel Ruthen,", "tokens": ["Da", "wach\u00b7sen", "zu", "viel", "Ru\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Kantschu, Peitschen, Knuten;", "tokens": ["Und", "Kant\u00b7schu", ",", "Peit\u00b7schen", ",", "Knu\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei meiner Seel' und Haut!", "tokens": ["Bei", "mei\u00b7ner", "Seel'", "und", "Haut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schlecht schmeckt ein solches Kraut.", "tokens": ["Schlecht", "schmeckt", "ein", "sol\u00b7ches", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was kann uns Ru\u00dfland fruchten", "tokens": ["Was", "kann", "uns", "Ru\u00df\u00b7land", "fruch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "NE", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinem Talg und Juchten?", "tokens": ["Mit", "sei\u00b7nem", "Talg", "und", "Juch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lichter stinken sehr,", "tokens": ["Die", "Lich\u00b7ter", "stin\u00b7ken", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Juchten noch viel mehr.", "tokens": ["Die", "Juch\u00b7ten", "noch", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf F\u00fcchs' und Zobel schie\u00dfen", "tokens": ["Auf", "F\u00fcchs'", "und", "Zo\u00b7bel", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und die Gedanken spie\u00dfen,", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "spie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist zwar nicht fein und zart,", "tokens": ["Ist", "zwar", "nicht", "fein", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch echte Russenart.", "tokens": ["Doch", "ech\u00b7te", "Rus\u00b7sen\u00b7art", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer etwas frei gesprochen,", "tokens": ["Wer", "et\u00b7was", "frei", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hat schon viel verbrochen,", "tokens": ["Der", "hat", "schon", "viel", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df nach Sibirien gehn,", "tokens": ["Mu\u00df", "nach", "Si\u00b7bi\u00b7ri\u00b7en", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dort mal umzusehn.", "tokens": ["Sich", "dort", "mal", "um\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum woll' uns Gott bewahren", "tokens": ["Drum", "woll'", "uns", "Gott", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor jedem wei\u00dfen Zaren", "tokens": ["Vor", "je\u00b7dem", "wei\u00b7\u00dfen", "Za\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Ru\u00dflands Herrlichkeit", "tokens": ["Und", "Ru\u00df\u00b7lands", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr jetzt und allezeit!", "tokens": ["F\u00fcr", "jetzt", "und", "al\u00b7le\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}