{"textgrid.poem.39380": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel, sagt man, kann Gewalt erleiden.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel, sagt man, kann Gewalt erleiden.", "tokens": ["Der", "Him\u00b7mel", ",", "sagt", "man", ",", "kann", "Ge\u00b7walt", "er\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "$,", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O dr\u00e4ngen meiner Blicke Liebespfeile", "tokens": ["O", "dr\u00e4n\u00b7gen", "mei\u00b7ner", "Bli\u00b7cke", "Lie\u00b7be\u00b7spfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wolken durch, da\u00df ich an deinem Heile,", "tokens": ["Die", "Wol\u00b7ken", "durch", ",", "da\u00df", "ich", "an", "dei\u00b7nem", "Hei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geliebtes Kind, mein Herz doch m\u00f6chte weiden!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "mein", "Herz", "doch", "m\u00f6ch\u00b7te", "wei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dftest von der treuen Mutter scheiden:", "tokens": ["Du", "mu\u00df\u00b7test", "von", "der", "treu\u00b7en", "Mut\u00b7ter", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward eine Mutter droben dir zu Theile?", "tokens": ["Ward", "ei\u00b7ne", "Mut\u00b7ter", "dro\u00b7ben", "dir", "zu", "Thei\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sagt dir Tr\u00f6stung, die dein Mitleid heile,", "tokens": ["Wer", "sagt", "dir", "Tr\u00f6s\u00b7tung", ",", "die", "dein", "Mit\u00b7leid", "hei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du so fern herabschaust auf uns beiden?", "tokens": ["Wenn", "du", "so", "fern", "her\u00b7ab\u00b7schaust", "auf", "uns", "bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein heil'ges Wort hat Botschaft ja gesendet,", "tokens": ["Ein", "heil'\u00b7ges", "Wort", "hat", "Bot\u00b7schaft", "ja", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort walt' ein weiblich Bild der Muttertriebe,", "tokens": ["Dort", "walt'", "ein", "weib\u00b7lich", "Bild", "der", "Mut\u00b7ter\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz der Welt, in ewigem Umarmen.", "tokens": ["Das", "Herz", "der", "Welt", ",", "in", "e\u00b7wi\u00b7gem", "Um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O, wenn von ernster Glorie Strahl geblendet,", "tokens": ["O", ",", "wenn", "von", "erns\u00b7ter", "Glo\u00b7rie", "Strahl", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die zarte Seele flieht zum Schoo\u00df der Liebe:", "tokens": ["Die", "zar\u00b7te", "See\u00b7le", "flieht", "zum", "Schoo\u00df", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Birg du, Maria, sie in deinen Armen.", "tokens": ["Birg", "du", ",", "Ma\u00b7ria", ",", "sie", "in", "dei\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}