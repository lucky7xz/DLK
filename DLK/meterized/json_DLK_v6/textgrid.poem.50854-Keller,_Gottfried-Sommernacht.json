{"textgrid.poem.50854": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wallt das Korn weit in die Runde,", "tokens": ["Es", "wallt", "das", "Korn", "weit", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ein Meer dehnt es sich aus;", "tokens": ["Und", "wie", "ein", "Meer", "dehnt", "es", "sich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch liegt auf seinem stillen Grunde", "tokens": ["Doch", "liegt", "auf", "sei\u00b7nem", "stil\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Seegew\u00fcrm noch andrer Graus:", "tokens": ["Nicht", "See\u00b7ge\u00b7w\u00fcrm", "noch", "an\u00b7drer", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da tr\u00e4umen Blumen nur von Kr\u00e4nzen", "tokens": ["Da", "tr\u00e4u\u00b7men", "Blu\u00b7men", "nur", "von", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und trinken der Gestirne Schein.", "tokens": ["Und", "trin\u00b7ken", "der", "Ge\u00b7stir\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O goldnes Meer, dein friedlich Gl\u00e4nzen", "tokens": ["O", "gold\u00b7nes", "Meer", ",", "dein", "fried\u00b7lich", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Saugt meine Seele gierig ein!", "tokens": ["Saugt", "mei\u00b7ne", "See\u00b7le", "gie\u00b7rig", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In meiner Heimat gr\u00fcnen Talen,", "tokens": ["In", "mei\u00b7ner", "Hei\u00b7mat", "gr\u00fc\u00b7nen", "Ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da herrscht ein alter sch\u00f6ner Brauch;", "tokens": ["Da", "herrscht", "ein", "al\u00b7ter", "sch\u00f6\u00b7ner", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann hell die Sommersterne strahlen,", "tokens": ["Wann", "hell", "die", "Som\u00b7mers\u00b7ter\u00b7ne", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gl\u00fchwurm schimmert durch den Strauch:", "tokens": ["Der", "Gl\u00fch\u00b7wurm", "schim\u00b7mert", "durch", "den", "Strauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann geht ein Fl\u00fcstern und ein Winken,", "tokens": ["Dann", "geht", "ein", "Fl\u00fcs\u00b7tern", "und", "ein", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das sich dem \u00c4hrenfelde naht,", "tokens": ["Das", "sich", "dem", "\u00c4h\u00b7ren\u00b7fel\u00b7de", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da geht ein n\u00e4chtlich Silberblinken", "tokens": ["Da", "geht", "ein", "n\u00e4cht\u00b7lich", "Sil\u00b7ber\u00b7blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Sicheln durch die goldne Saat.", "tokens": ["Von", "Si\u00b7cheln", "durch", "die", "gold\u00b7ne", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das sind die Bursche, jung und wacker,", "tokens": ["Das", "sind", "die", "Bur\u00b7sche", ",", "jung", "und", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sammeln sich im Feld zuhauf", "tokens": ["Die", "sam\u00b7meln", "sich", "im", "Feld", "zu\u00b7hauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und suchen den gereiften Acker", "tokens": ["Und", "su\u00b7chen", "den", "ge\u00b7reif\u00b7ten", "A\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Witwe oder Waise auf,", "tokens": ["Der", "Wit\u00b7we", "o\u00b7der", "Wai\u00b7se", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die keines Vaters, keiner Br\u00fcder", "tokens": ["Die", "kei\u00b7nes", "Va\u00b7ters", ",", "kei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und keines Knechtes Hilfe wei\u00df \u2013", "tokens": ["Und", "kei\u00b7nes", "Knech\u00b7tes", "Hil\u00b7fe", "wei\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr schneiden sie den Segen nieder,", "tokens": ["Ihr", "schnei\u00b7den", "sie", "den", "Se\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die reinste Lust ziert ihren Flei\u00df.", "tokens": ["Die", "reins\u00b7te", "Lust", "ziert", "ih\u00b7ren", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon sind die Garben fest gebunden", "tokens": ["Schon", "sind", "die", "Gar\u00b7ben", "fest", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00f6n in einen Kranz gebracht;", "tokens": ["Und", "sch\u00f6n", "in", "ei\u00b7nen", "Kranz", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lieblich flohn die stillen Stunden,", "tokens": ["Wie", "lieb\u00b7lich", "flohn", "die", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ein Spiel in k\u00fchler Nacht!", "tokens": ["Es", "war", "ein", "Spiel", "in", "k\u00fch\u00b7ler", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun wird geschw\u00e4rmt und hell gesungen", "tokens": ["Nun", "wird", "ge\u00b7schw\u00e4rmt", "und", "hell", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Garbenkreis, bis Morgenduft", "tokens": ["Im", "Gar\u00b7ben\u00b7kreis", ",", "bis", "Mor\u00b7gen\u00b7duft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die nimmerm\u00fcden, braunen Jungen", "tokens": ["Die", "nim\u00b7mer\u00b7m\u00fc\u00b7den", ",", "brau\u00b7nen", "Jun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur eignen schweren Arbeit ruft.", "tokens": ["Zur", "eig\u00b7nen", "schwe\u00b7ren", "Ar\u00b7beit", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}