: "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir mit der Hoffnung oft verschwand.", "tokens": ["Mir", "mit", "der", "Hoff\u00b7nung", "oft", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "War ich denn manchem Mangel nahe,", "tokens": ["War", "ich", "denn", "man\u00b7chem", "Man\u00b7gel", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja sah ich keine Rettung mehr,", "tokens": ["Ja", "sah", "ich", "kei\u00b7ne", "Ret\u00b7tung", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wu\u00dft ich nicht, wie mir geschahe,", "tokens": ["So", "wu\u00dft", "ich", "nicht", ",", "wie", "mir", "ge\u00b7scha\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du gabst aus deinem Vorrath her,", "tokens": ["Du", "gabst", "aus", "dei\u00b7nem", "Vor\u00b7rath", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Milde deiner Vorsehung", "tokens": ["Die", "Mil\u00b7de", "dei\u00b7ner", "Vor\u00b7se\u00b7hung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "