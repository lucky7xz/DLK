{"dta.poem.13408": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne neiget sich in Westen,", "tokens": ["Die", "Son\u00b7ne", "nei\u00b7get", "sich", "in", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und geht den Gegenf\u00fcssern auf,", "tokens": ["Und", "geht", "den", "Ge\u00b7gen\u00b7f\u00fcs\u00b7sern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich armer Mensch! ich wei\u00df am besten", "tokens": ["Ich", "ar\u00b7mer", "Mensch", "!", "ich", "wei\u00df", "am", "bes\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meinen ganzen Lebenslauf!", "tokens": ["Um", "mei\u00b7nen", "gan\u00b7zen", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! mein Gewissen tr\u00f6stet nicht!", "tokens": ["Ach", "!", "mein", "Ge\u00b7wis\u00b7sen", "tr\u00f6s\u00b7tet", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "HeRR! geh nicht mit mir ins Gericht!", "tokens": ["HeRR", "!", "geh", "nicht", "mit", "mir", "ins", "Ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Wie wohl ich dieses Tages Stunden", "tokens": ["Wie", "wohl", "ich", "die\u00b7ses", "Ta\u00b7ges", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meiner Wohlfahrt angewandt,", "tokens": ["Zu", "mei\u00b7ner", "Wohl\u00b7fahrt", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sich die D\u00e4mmrung eingefunden,", "tokens": ["Bis", "sich", "die", "D\u00e4mm\u00b7rung", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist GOtt und mir allein bekannt;", "tokens": ["Ist", "Gott", "und", "mir", "al\u00b7lein", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bin ich von Schuld und S\u00fcnden rein?", "tokens": ["Bin", "ich", "von", "Schuld", "und", "S\u00fcn\u00b7den", "rein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Fleisch spricht ja! der Geist sagt: nein!", "tokens": ["Das", "Fleisch", "spricht", "ja", "!", "der", "Geist", "sagt", ":", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich hatte fr\u00fch, bey dem Erwachen,", "tokens": ["Ich", "hat\u00b7te", "fr\u00fch", ",", "bey", "dem", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und als es wiederum getagt,", "tokens": ["Und", "als", "es", "wie\u00b7de\u00b7rum", "ge\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Himmel mir zum Freund zu machen,", "tokens": ["Den", "Him\u00b7mel", "mir", "zum", "Freund", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Leben zugesagt,", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "zu\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von alten S\u00fcnden abzustehn,", "tokens": ["Von", "al\u00b7ten", "S\u00fcn\u00b7den", "ab\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hielt ich es aber auch? wie sch\u00f6n!", "tokens": ["Hielt", "ich", "es", "a\u00b7ber", "auch", "?", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es wird mir angst! ach! wie so bange!", "tokens": ["Es", "wird", "mir", "angst", "!", "ach", "!", "wie", "so", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$.", "XY", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jtzt seh ich meiner S\u00fcnden Heer!", "tokens": ["Jtzt", "seh", "ich", "mei\u00b7ner", "S\u00fcn\u00b7den", "Heer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich an zu rechnen fange,", "tokens": ["Und", "wenn", "ich", "an", "zu", "rech\u00b7nen", "fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So find ich sie, wie Sand am Meer,", "tokens": ["So", "find", "ich", "sie", ",", "wie", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Ungl\u00fcck! h\u00f6rt! ja ja! sie schreyn:", "tokens": ["O", "Un\u00b7gl\u00fcck", "!", "h\u00f6rt", "!", "ja", "ja", "!", "sie", "schreyn", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "$.", "ADV", "ADV", "$.", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott wird nunmehro Richter seyn!", "tokens": ["Gott", "wird", "nun\u00b7meh\u00b7ro", "Rich\u00b7ter", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo soll ich hin vor deinem Geiste,", "tokens": ["Wo", "soll", "ich", "hin", "vor", "dei\u00b7nem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allgegenw\u00e4rtiger! wohin?", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ger", "!", "wo\u00b7hin", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "PWAV", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und wenn ich in die H\u00f6lle reiste,", "tokens": ["Und", "wenn", "ich", "in", "die", "H\u00f6l\u00b7le", "reis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, wollt ich in den Himmel fliehn,", "tokens": ["Ja", ",", "wollt", "ich", "in", "den", "Him\u00b7mel", "fliehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schnell als man gedenken kan,", "tokens": ["So", "schnell", "als", "man", "ge\u00b7den\u00b7ken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich tr\u00e4f Dich allenthalben an.", "tokens": ["Ich", "tr\u00e4f", "Dich", "al\u00b7len\u00b7thal\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du bist gerecht! ich mu\u00df mich sch\u00e4men,", "tokens": ["Du", "bist", "ge\u00b7recht", "!", "ich", "mu\u00df", "mich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "GoTT, grosser GOtt, du bist gerecht,", "tokens": ["GoTT", ",", "gros\u00b7ser", "Gott", ",", "du", "bist", "ge\u00b7recht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kannst mir nun den Himmel nehmen,", "tokens": ["Du", "kannst", "mir", "nun", "den", "Him\u00b7mel", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der S\u00fcnden treuer Knecht,", "tokens": ["Ich", "bin", "der", "S\u00fcn\u00b7den", "treu\u00b7er", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als der sich selber zuerkennt,", "tokens": ["Als", "der", "sich", "sel\u00b7ber", "zu\u00b7er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df unter ihm die H\u00f6lle brennt.", "tokens": ["Da\u00df", "un\u00b7ter", "ihm", "die", "H\u00f6l\u00b7le", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie brennt! dein Zorn hat sie entz\u00fcndet,", "tokens": ["Sie", "brennt", "!", "dein", "Zorn", "hat", "sie", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Eifer selbsten angesteckt,", "tokens": ["Dein", "Ei\u00b7fer", "selbs\u00b7ten", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr! deine Rach ist wohlgegr\u00fcndet,", "tokens": ["Herr", "!", "dei\u00b7ne", "Rach", "ist", "wohl\u00b7ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mein B\u00f6slichthun hat sie erweckt,", "tokens": ["Mein", "B\u00f6s\u00b7licht\u00b7hun", "hat", "sie", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und wenn sie itzt mit mir verf\u00e4hrt,", "tokens": ["Und", "wenn", "sie", "itzt", "mit", "mir", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin es leider allzuwerth.", "tokens": ["Ich", "bin", "es", "lei\u00b7der", "all\u00b7zu\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Allein Du hast doch auch gesaget:", "tokens": ["Al\u00b7lein", "Du", "hast", "doch", "auch", "ge\u00b7sa\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du seyest voll Barmherzigkeit,", "tokens": ["Du", "se\u00b7yest", "voll", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Drum h\u00f6re, was mein Glaube waget;", "tokens": ["Drum", "h\u00f6\u00b7re", ",", "was", "mein", "Glau\u00b7be", "wa\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein reuendes Gewissen schreyt:", "tokens": ["Mein", "reu\u00b7en\u00b7des", "Ge\u00b7wis\u00b7sen", "schreyt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verschlie\u00df die H\u00f6lle noch einmahl", "tokens": ["Ver\u00b7schlie\u00df", "die", "H\u00f6l\u00b7le", "noch", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor meiner S\u00fcnden grosser Zahl.", "tokens": ["Vor", "mei\u00b7ner", "S\u00fcn\u00b7den", "gros\u00b7ser", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sey gn\u00e4dig, HErr! ach! Gnade! Gnade!", "tokens": ["Sey", "gn\u00e4\u00b7dig", ",", "Herr", "!", "ach", "!", "Gna\u00b7de", "!", "Gna\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$.", "XY", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertilge meine Missethat!", "tokens": ["Ver\u00b7til\u00b7ge", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was in dem ersten Wasserbade", "tokens": ["Was", "in", "dem", "ers\u00b7ten", "Was\u00b7ser\u00b7ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00fcnder angelobet hat,", "tokens": ["Der", "S\u00fcn\u00b7der", "an\u00b7ge\u00b7lo\u00b7bet", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Versprech ich itzt mit Herz und Mund,", "tokens": ["Ver\u00b7sprech", "ich", "itzt", "mit", "Herz", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darum erneure jenen Bund.", "tokens": ["Da\u00b7rum", "er\u00b7neu\u00b7re", "je\u00b7nen", "Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit Thr\u00e4nen fall ich Dir zu Fusse,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "fall", "ich", "Dir", "zu", "Fus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergieb, HErr! habe doch Geduld!", "tokens": ["Ver\u00b7gieb", ",", "Herr", "!", "ha\u00b7be", "doch", "Ge\u00b7duld", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erz\u00fcrnter Richter! meine Busse", "tokens": ["Er\u00b7z\u00fcrn\u00b7ter", "Rich\u00b7ter", "!", "mei\u00b7ne", "Bus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestehet die gemachte Schuld;", "tokens": ["Ge\u00b7ste\u00b7het", "die", "ge\u00b7mach\u00b7te", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr! werde Vater! und denn sprich:", "tokens": ["Herr", "!", "wer\u00b7de", "Va\u00b7ter", "!", "und", "denn", "sprich", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "NN", "$.", "KON", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mein Sohn, Kind, hat bezahlt f\u00fcr dich.", "tokens": ["Mein", "Sohn", ",", "Kind", ",", "hat", "be\u00b7zahlt", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00df dieses Trostwort mich empfinden:", "tokens": ["La\u00df", "die\u00b7ses", "Trost\u00b7wort", "mich", "emp\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geh hin, die Schuld ist ausgethan!", "tokens": ["Geh", "hin", ",", "die", "Schuld", "ist", "aus\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deinen Geist mein Herz entz\u00fcnden,", "tokens": ["La\u00df", "dei\u00b7nen", "Geist", "mein", "Herz", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es gel\u00e4utert werden kann,", "tokens": ["Da\u00df", "es", "ge\u00b7l\u00e4u\u00b7tert", "wer\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit es Satan, Fleisch und Welt", "tokens": ["Da\u00b7mit", "es", "Sa\u00b7tan", ",", "Fleisch", "und", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht wieder in die Grube f\u00e4llt.", "tokens": ["Nicht", "wie\u00b7der", "in", "die", "Gru\u00b7be", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dein gn\u00e4dig Ohr neig zu dem Liede,", "tokens": ["Dein", "gn\u00e4\u00b7dig", "Ohr", "neig", "zu", "dem", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Andacht, Sch\u00f6pfer! bringt es Dir,", "tokens": ["Die", "An\u00b7dacht", ",", "Sch\u00f6p\u00b7fer", "!", "bringt", "es", "Dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn lieg und schlaf ich sanft im Friede,", "tokens": ["Denn", "lieg", "und", "schlaf", "ich", "sanft", "im", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "HeRR! Du alleine hilfst ja mir,", "tokens": ["HeRR", "!", "Du", "al\u00b7lei\u00b7ne", "hilfst", "ja", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ADV", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast mit mir Vertrag gemacht,", "tokens": ["Du", "hast", "mit", "mir", "Ver\u00b7trag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum hoff ich eine gute Nacht.", "tokens": ["Drum", "hoff", "ich", "ei\u00b7ne", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Vorhero sey, HErr meiner Tage!", "tokens": ["Vor\u00b7he\u00b7ro", "sey", ",", "Herr", "mei\u00b7ner", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dir noch ein Opfer zubereit,", "tokens": ["Dir", "noch", "ein", "Op\u00b7fer", "zu\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Du f\u00fcr mancher Noth und Plage", "tokens": ["Da\u00df", "Du", "f\u00fcr", "man\u00b7cher", "Noth", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich hast besch\u00fctzt, davon befreyt;", "tokens": ["Mich", "hast", "be\u00b7sch\u00fctzt", ",", "da\u00b7von", "be\u00b7freyt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch unaussprechlicher Gewinn!", "tokens": ["Welch", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein ist, da\u00df ich noch wirklich bin.", "tokens": ["Dein", "ist", ",", "da\u00df", "ich", "noch", "wirk\u00b7lich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "GoTT, mein Erhalter, ja, mein Leben!", "tokens": ["GoTT", ",", "mein", "Er\u00b7hal\u00b7ter", ",", "ja", ",", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PTKANT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Dir kommt alle Wohlthat her,", "tokens": ["Von", "Dir", "kommt", "al\u00b7le", "Wohlt\u00b7hat", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesundheit hast Du mir gegeben,", "tokens": ["Ge\u00b7sund\u00b7heit", "hast", "Du", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Witz, Nahrung, Kleider, ja weit mehr,", "tokens": ["Witz", ",", "Nah\u00b7rung", ",", "Klei\u00b7der", ",", "ja", "weit", "mehr", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als ich erzehlen kann und wei\u00df,", "tokens": ["Als", "ich", "er\u00b7zeh\u00b7len", "kann", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punc