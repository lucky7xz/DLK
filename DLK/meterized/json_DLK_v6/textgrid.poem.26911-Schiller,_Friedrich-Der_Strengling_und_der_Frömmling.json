{"textgrid.poem.26911": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Strengling und der Fr\u00f6mmling", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jener fodert durchaus, da\u00df dir das Gute mi\u00dffalle,", "tokens": ["Je\u00b7ner", "fo\u00b7dert", "durc\u00b7haus", ",", "da\u00df", "dir", "das", "Gu\u00b7te", "mi\u00df\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dieser will gar, da\u00df du liebst, was dir von Herzen mi\u00dff\u00e4llt.", "tokens": ["Die\u00b7ser", "will", "gar", ",", "da\u00df", "du", "liebst", ",", "was", "dir", "von", "Her\u00b7zen", "mi\u00df\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Mu\u00df ich w\u00e4hlen, so seis in Gottes Namen die Tugend,", "tokens": ["Mu\u00df", "ich", "w\u00e4h\u00b7len", ",", "so", "seis", "in", "Got\u00b7tes", "Na\u00b7men", "die", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADV", "ADV", "APPR", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn ich kann einmal nicht lieben, was abgeschmackt ist.", "tokens": ["Denn", "ich", "kann", "ein\u00b7mal", "nicht", "lie\u00b7ben", ",", "was", "ab\u00b7ge\u00b7schmackt", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}