{"textgrid.poem.65422": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rosen, ihrem Haar entrissen, wollen nicht mehr bl\u00fchen,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosen, ihrem Haar entrissen, wollen nicht mehr bl\u00fchen,", "tokens": ["Ro\u00b7sen", ",", "ih\u00b7rem", "Haar", "ent\u00b7ris\u00b7sen", ",", "wol\u00b7len", "nicht", "mehr", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVPP", "$,", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Funkenw\u00fcrmchen, ihm entflogen, k\u00f6nnen nicht mehr gl\u00fchen:", "tokens": ["Fun\u00b7ken\u00b7w\u00fcrm\u00b7chen", ",", "ihm", "ent\u00b7flo\u00b7gen", ",", "k\u00f6n\u00b7nen", "nicht", "mehr", "gl\u00fc\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "$,", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Und, mein armes Herz, du solltest dich zu l\u00f6sen wagen?", "tokens": ["Und", ",", "mein", "ar\u00b7mes", "Herz", ",", "du", "soll\u00b7test", "dich", "zu", "l\u00f6\u00b7sen", "wa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Nur nach ihrer Locken Wallen kannst du hier noch schlagen.", "tokens": ["Nur", "nach", "ih\u00b7rer", "Lo\u00b7cken", "Wal\u00b7len", "kannst", "du", "