{"textgrid.poem.63668": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Allerseelen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was s\u00e4uselt in den L\u00fcften?", "tokens": ["Was", "s\u00e4u\u00b7selt", "in", "den", "L\u00fcf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was singt im Gem\u00fct?", "tokens": ["Was", "singt", "im", "Ge\u00b7m\u00fct", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ist's Fr\u00fchling, ist's Liebe,", "tokens": ["Ist's", "Fr\u00fch\u00b7ling", ",", "ist's", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was neu mir erbl\u00fcht?", "tokens": ["Was", "neu", "mir", "er\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ach, h\u00f6rst du nicht sausen", "tokens": ["Ach", ",", "h\u00f6rst", "du", "nicht", "sau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Herbstwind im Baum?", "tokens": ["Den", "Herbst\u00b7wind", "im", "Baum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach, ging nicht die Liebe", "tokens": ["Ach", ",", "ging", "nicht", "die", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dahin wie ein Traum?", "tokens": ["Da\u00b7hin", "wie", "ein", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "KOKOM", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Vom H\u00fcgel, darunter", "tokens": ["Vom", "H\u00fc\u00b7gel", ",", "da\u00b7run\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "PAV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie schlummert schon lang,", "tokens": ["Sie", "schlum\u00b7mert", "schon", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Klingt leise das Liedchen,", "tokens": ["Klingt", "lei\u00b7se", "das", "Lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das einst sie dir sang:", "tokens": ["Das", "einst", "sie", "dir", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Es findet die Liebe", "tokens": ["Es", "fin\u00b7det", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Grabe nicht Ruh;", "tokens": ["Im", "Gra\u00b7be", "nicht", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Am Tag Allerseelen", "tokens": ["Am", "Tag", "Al\u00b7ler\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da winkt sie dir zu.", "tokens": ["Da", "winkt", "sie", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Am Tag Allerseelen", "tokens": ["Am", "Tag", "Al\u00b7ler\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da weckt sie der Schmerz,", "tokens": ["Da", "weckt", "sie", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da sucht sie den Liebsten", "tokens": ["Da", "sucht", "sie", "den", "Liebs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und k\u00fc\u00dft ihn aufs Herz.", "tokens": ["Und", "k\u00fc\u00dft", "ihn", "aufs", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}