{"textgrid.poem.59046": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "265. Ach Jesu!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Such Heiligkeit, sonst geht's nicht gut,", "tokens": ["Such", "Hei\u00b7lig\u00b7keit", ",", "sonst", "geht's", "nicht", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch such kein Heil in deinen H\u00e4nden!", "tokens": ["Doch", "such", "kein", "Heil", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ruht der Mut in Christi Blut", "tokens": ["Es", "ruht", "der", "Mut", "in", "Chris\u00b7ti", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht in seinen eignen St\u00e4nden.", "tokens": ["Und", "nicht", "in", "sei\u00b7nen", "eig\u00b7nen", "St\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}