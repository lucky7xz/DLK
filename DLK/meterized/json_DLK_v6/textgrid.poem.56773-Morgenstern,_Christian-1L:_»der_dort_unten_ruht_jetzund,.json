{"textgrid.poem.56773": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder dort unten ruht jetzund,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder dort unten ruht jetzund,", "tokens": ["\u00bb", "der", "dort", "un\u00b7ten", "ruht", "je\u00b7tzund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sein Schatten stie\u00df ihn in den Grund.", "tokens": ["sein", "Schat\u00b7ten", "stie\u00df", "ihn", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am steilen Fels den schmalen Gang", "tokens": ["Am", "stei\u00b7len", "Fels", "den", "schma\u00b7len", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "klomm verwegen er entlang.", "tokens": ["klomm", "ver\u00b7we\u00b7gen", "er", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scharf lag auf ihm das Mittagslicht,", "tokens": ["Scharf", "lag", "auf", "ihm", "das", "Mit\u00b7tags\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "der Schwei\u00df rann ihm \u00fcbers Gesicht.", "tokens": ["der", "Schwei\u00df", "rann", "ihm", "\u00fc\u00b7bers", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da blieb er, sich zu trocknen, stehn \u2013", "tokens": ["Da", "blieb", "er", ",", "sich", "zu", "trock\u00b7nen", ",", "stehn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRF", "APPR", "ADJA", "$,", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mu\u00df dabei seinen Schatten sehn.", "tokens": ["mu\u00df", "da\u00b7bei", "sei\u00b7nen", "Schat\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Und wie er ihn sieht, reckt sich der", "tokens": ["Und", "wie", "er", "ihn", "sieht", ",", "reckt", "sich", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "von der Wand gegen ihn her.", "tokens": ["von", "der", "Wand", "ge\u00b7gen", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Den Wandrer fasset bittre Not,", "tokens": ["Den", "Wand\u00b7rer", "fas\u00b7set", "bitt\u00b7re", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "er f\u00fchlet, neben ihm steht der Tod", "tokens": ["er", "f\u00fch\u00b7let", ",", "ne\u00b7ben", "ihm", "steht", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "und dr\u00e4ngt ihn in das tiefe Grab", "tokens": ["und", "dr\u00e4ngt", "ihn", "in", "das", "tie\u00b7fe", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "der wilden Felsenschlucht hinab.", "tokens": ["der", "wil\u00b7den", "Fel\u00b7sen\u00b7schlucht", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er sinkt zusammen in kaltem Schwei\u00df,", "tokens": ["Er", "sinkt", "zu\u00b7sam\u00b7men", "in", "kal\u00b7tem", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "alles dreht sich mit ihm im Kreis.", "tokens": ["al\u00b7les", "dreht", "sich", "mit", "ihm", "im", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Er pre\u00dft die Stirn an den kalten Stein", "tokens": ["Er", "pre\u00dft", "die", "Stirn", "an", "den", "kal\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "und denkt an Weib und Kinderlein.", "tokens": ["und", "denkt", "an", "Weib", "und", "Kin\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Aber der Tod hatt' gewonnen Spiel", "tokens": ["A\u00b7ber", "der", "Tod", "hatt'", "ge\u00b7won\u00b7nen", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "und schob und stie\u00df ihn, bis da\u00df er fiel.", "tokens": ["und", "schob", "und", "stie\u00df", "ihn", ",", "bis", "da\u00df", "er", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$,", "KON", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Eine Dirn aus unserm Dorf hat's geschaut,", "tokens": ["Ei\u00b7ne", "Dirn", "aus", "un\u00b7serm", "Dorf", "hat's", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "ein fremder Maler den Stein aufgebaut,", "tokens": ["ein", "frem\u00b7der", "Ma\u00b7ler", "den", "Stein", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "die Verse sind von der alten Kathrein.", "tokens": ["die", "Ver\u00b7se", "sind", "von", "der", "al\u00b7ten", "Ka\u00b7th\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "Sprecht: Armer Wandrer, wir denken Dein!\u00ab", "tokens": ["Sprecht", ":", "Ar\u00b7mer", "Wand\u00b7rer", ",", "wir", "den\u00b7ken", "Dein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}