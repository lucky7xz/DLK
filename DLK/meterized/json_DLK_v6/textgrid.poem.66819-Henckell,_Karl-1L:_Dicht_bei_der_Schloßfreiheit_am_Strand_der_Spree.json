{"textgrid.poem.66819": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dicht bei der Schlo\u00dffreiheit am Strand der Spree", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht bei der Schlo\u00dffreiheit am Strand der Spree", "tokens": ["Dicht", "bei", "der", "Schlo\u00df\u00b7frei\u00b7heit", "am", "Strand", "der", "Spree"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Strahlt in die Abendd\u00e4mmrung ein Caf\u00e9.", "tokens": ["Strahlt", "in", "die", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "ein", "Ca\u00b7f\u00e9", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Zeit noch zu vertr\u00f6deln bis zum Feste,", "tokens": ["Die", "Zeit", "noch", "zu", "ver\u00b7tr\u00f6\u00b7deln", "bis", "zum", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Fenster g\u00e4hnt ein Frack mit wei\u00dfer Weste.", "tokens": ["Am", "Fens\u00b7ter", "g\u00e4hnt", "ein", "Frack", "mit", "wei\u00b7\u00dfer", "Wes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein T\u00fcllkost\u00fcm, das durch Enth\u00fcllung siegt,", "tokens": ["Ein", "T\u00fcll\u00b7kos\u00b7t\u00fcm", ",", "das", "durch", "Ent\u00b7h\u00fcl\u00b7lung", "siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Saugt Sorbet, in des Mantels Wurf geschmiegt.", "tokens": ["Saugt", "Sor\u00b7bet", ",", "in", "des", "Man\u00b7tels", "Wurf", "ge\u00b7schmiegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und viele noble Leute rekeln rings", "tokens": ["Und", "vie\u00b7le", "nob\u00b7le", "Leu\u00b7te", "re\u00b7keln", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und l\u00f6sen sicher das Problem der Sphinx", "tokens": ["Und", "l\u00f6\u00b7sen", "si\u00b7cher", "das", "Prob\u00b7lem", "der", "Sphinx"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der neuen Zeit von ihren Polstersitzen", "tokens": ["Der", "neu\u00b7en", "Zeit", "von", "ih\u00b7ren", "Pols\u00b7ter\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Noch heute Abend, eh die Sterne blitzen.", "tokens": ["Noch", "heu\u00b7te", "A\u00b7bend", ",", "eh", "die", "Ster\u00b7ne", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So eifrig trinken sie die Tulpe Bier", "tokens": ["So", "eif\u00b7rig", "trin\u00b7ken", "sie", "die", "Tul\u00b7pe", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und lesen Artur Levysohns Papier.", "tokens": ["Und", "le\u00b7sen", "Ar\u00b7tur", "Le\u00b7vy\u00b7sohns", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gesellschaftss\u00e4ulen \u2013 wahre Seelenweide", "tokens": ["Ge\u00b7sell\u00b7schafts\u00b7s\u00e4u\u00b7len", "\u2013", "wah\u00b7re", "See\u00b7len\u00b7wei\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Die Kellner sehn dem Ballherrn blendend \u00e4hnlich,", "tokens": ["Die", "Kell\u00b7ner", "sehn", "dem", "Ball\u00b7herrn", "blen\u00b7dend", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und wie dem Gaste wird's auch ihnen g\u00e4hnlich,", "tokens": ["Und", "wie", "dem", "Gas\u00b7te", "wird's", "auch", "ih\u00b7nen", "g\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Bis sich ihr Herrscherantlitz hold belebt,", "tokens": ["Bis", "sich", "ihr", "Herr\u00b7scher\u00b7ant\u00b7litz", "hold", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wenn grad ein Trinkgeld klopft und sich erhebt.", "tokens": ["Wenn", "grad", "ein", "Trink\u00b7geld", "klopft", "und", "sich", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Da treten \u2013 sind die Kerle wohl verr\u00fcckt? \u2013", "tokens": ["Da", "tre\u00b7ten", "\u2013", "sind", "die", "Ker\u00b7le", "wohl", "ver\u00b7r\u00fcckt", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zwei M\u00e4nner ein, Hut in die Hand gedr\u00fcckt,", "tokens": ["Zwei", "M\u00e4n\u00b7ner", "ein", ",", "Hut", "in", "die", "Hand", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$,", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Im braunen Arbeitsrock, adrett und frisch,", "tokens": ["Im", "brau\u00b7nen", "Ar\u00b7beits\u00b7rock", ",", "ad\u00b7rett", "und", "frisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und setzen still sich an den n\u00e4chsten Tisch.", "tokens": ["Und", "set\u00b7zen", "still", "sich", "an", "den", "n\u00e4chs\u00b7ten", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Zahlkellner schleicht auf Mardersohlen her.", "tokens": ["Zahl\u00b7kell\u00b7ner", "schleicht", "auf", "Mar\u00b7der\u00b7soh\u00b7len", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "\u00bbzwei Gl\u00e4ser Bier!\u00ab Zahlkellner wolkenschwer.", "tokens": ["\u00bb", "zwei", "Gl\u00e4\u00b7ser", "Bier", "!", "\u00ab", "Zahl\u00b7kell\u00b7ner", "wol\u00b7ken\u00b7schwer", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "CARD", "NN", "NN", "$.", "$(", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein Wink vom Wirt. Der Ballfrack dreht sich um", "tokens": ["Ein", "Wink", "vom", "Wirt", ".", "Der", "Ball\u00b7frack", "dreht", "sich", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und knurrt zum T\u00fcll. \u00bbEin nettes Publikum!\u00ab", "tokens": ["Und", "knurrt", "zum", "T\u00fcll", ".", "\u00bb", "Ein", "net\u00b7tes", "Pub\u00b7li\u00b7kum", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "T\u00fcll f\u00e4llt die Sorbetr\u00f6hre aus dem Munde.", "tokens": ["T\u00fcll", "f\u00e4llt", "die", "Sor\u00b7be\u00b7tr\u00f6h\u00b7re", "aus", "dem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Zahlkellner dr\u00e4uend. (\u00bbUnversch\u00e4mte Hunde!\u00ab)", "tokens": ["Zahl\u00b7kell\u00b7ner", "dr\u00e4u\u00b7end", ".", "(", "\u00bb", "Un\u00b7ver\u00b7sch\u00e4m\u00b7te", "Hun\u00b7de", "!", "\u00ab", ")"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVPP", "$.", "$(", "$(", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der Gott der Kellner und der Klassen spricht:", "tokens": ["Der", "Gott", "der", "Kell\u00b7ner", "und", "der", "Klas\u00b7sen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "\u00bbbedaure leider ... bitte ... das geht nicht!", "tokens": ["\u00bb", "be\u00b7dau\u00b7re", "lei\u00b7der", "...", "bit\u00b7te", "...", "das", "geht", "nicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$(", "PTKANT", "$(", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Sie sehn ... Man w\u00fcnscht ... Sie sind am falschen Ort.\u00ab", "tokens": ["Sie", "sehn", "...", "Man", "w\u00fcnscht", "...", "Sie", "sind", "am", "fal\u00b7schen", "Ort", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "$(", "PIS", "VVFIN", "$(", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die stehen auf und gehen schweigend fort ...", "tokens": ["Die", "ste\u00b7hen", "auf", "und", "ge\u00b7hen", "schwei\u00b7gend", "fort", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Auf ihrem Platze hockt mit einem Sprung", "tokens": ["Auf", "ih\u00b7rem", "Plat\u00b7ze", "hockt", "mit", "ei\u00b7nem", "Sprung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der Ha\u00df und schl\u00fcrft ein Fa\u00df Verbitterung.", "tokens": ["Der", "Ha\u00df", "und", "schl\u00fcrft", "ein", "Fa\u00df", "Ver\u00b7bit\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}