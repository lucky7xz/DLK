{"dta.poem.19497": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das wei\u00dfe Spitzchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blendendes Spitzchen blickt \u00fcber den Wald,", "tokens": ["Ein", "blen\u00b7den\u00b7des", "Spitz\u00b7chen", "blickt", "\u00fc\u00b7ber", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das ruft mich, das zieht mich, das thut mir Gewalt:", "tokens": ["Das", "ruft", "mich", ",", "das", "zieht", "mich", ",", "das", "thut", "mir", "Ge\u00b7walt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ewas schaffst du noch unten im Menschengew\u00fchl?", "tokens": ["\u201e", "was", "schaffst", "du", "noch", "un\u00b7ten", "im", "Men\u00b7schen\u00b7ge\u00b7w\u00fchl", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier oben ist's einsam! Hier oben ist's k\u00fchl!", "tokens": ["Hier", "o\u00b7ben", "ist's", "ein\u00b7sam", "!", "Hier", "o\u00b7ben", "ist's", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "$.", "ADV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Der See mir zu F\u00fc\u00dfen hat heut sich enteist,", "tokens": ["Der", "See", "mir", "zu", "F\u00fc\u00b7\u00dfen", "hat", "heut", "sich", "ent\u00b7eist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VAFIN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er kr\u00e4uselt sich, fluthet, er wandert, er reist,", "tokens": ["Er", "kr\u00e4u\u00b7selt", "sich", ",", "flut\u00b7het", ",", "er", "wan\u00b7dert", ",", "er", "reist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Die Moosbank des Felsens ist dir schon bereit,", "tokens": ["Die", "Moos\u00b7bank", "des", "Fel\u00b7sens", "ist", "dir", "schon", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihr ist's zum ewigen Schnee nicht mehr weit!\u201c", "tokens": ["Von", "ihr", "ist's", "zum", "e\u00b7wi\u00b7gen", "Schnee", "nicht", "mehr", "weit", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Spitzchen, es ruft mich, sobald ich erwacht,", "tokens": ["Das", "Spitz\u00b7chen", ",", "es", "ruft", "mich", ",", "so\u00b7bald", "ich", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Mittag, am Abend, im Traum noch der Nacht.", "tokens": ["Am", "Mit\u00b7tag", ",", "am", "A\u00b7bend", ",", "im", "Traum", "noch", "der", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "So komm ich denn morgen! Nun la\u00df mich in Ruh!", "tokens": ["So", "komm", "ich", "denn", "mor\u00b7gen", "!", "Nun", "la\u00df", "mich", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erst schlie\u00df' ich die B\u00fccher, die Schreine noch zu.", "tokens": ["Erst", "schlie\u00df'", "ich", "die", "B\u00fc\u00b7cher", ",", "die", "Schrei\u00b7ne", "noch", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Leis wandelt in L\u00fcften ein Heerdegel\u00e4ut:", "tokens": ["Leis", "wan\u00b7delt", "in", "L\u00fcf\u00b7ten", "ein", "Heer\u00b7de\u00b7ge\u00b7l\u00e4ut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201ela\u00df offen die Truhen! Komm lieber noch heut.\u201c", "tokens": ["\u201e", "la\u00df", "of\u00b7fen", "die", "Tru\u00b7hen", "!", "Komm", "lie\u00b7ber", "noch", "heut", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADJD", "ART", "NN", "$.", "VVFIN", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}