{"textgrid.poem.35828": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, ich will Ihn nicht verlassen,", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, ich will Ihn nicht verlassen,", "tokens": ["Nein", ",", "ich", "will", "Ihn", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der nach mir die Arme streckt! \u2013", "tokens": ["Der", "nach", "mir", "die", "Ar\u00b7me", "streckt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ja, ich will Sein Kreuz umfassen,", "tokens": ["Ja", ",", "ich", "will", "Sein", "Kreuz", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mich lang genug geschreckt.", "tokens": ["Das", "mich", "lang", "ge\u00b7nug", "ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur vergebens sucht' ich Freuden", "tokens": ["Nur", "ver\u00b7ge\u00b7bens", "sucht'", "ich", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier im weiten Erdenreich;", "tokens": ["Hier", "im", "wei\u00b7ten", "Er\u00b7den\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer sah ich Ihn verscheiden,", "tokens": ["Im\u00b7mer", "sah", "ich", "Ihn", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! so blutend und so bleich.", "tokens": ["Ach", "!", "so", "blu\u00b7tend", "und", "so", "bleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Sein Herz \u2013 o weh der S\u00fcnde!", "tokens": ["Und", "Sein", "Herz", "\u2013", "o", "weh", "der", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "FM", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dies treuste Herz betr\u00fcbt! \u2013", "tokens": ["Die", "dies", "treus\u00b7te", "Herz", "be\u00b7tr\u00fcbt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PDS", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandte sich zum b\u00f6sen Kinde,", "tokens": ["Wand\u00b7te", "sich", "zum", "b\u00f6\u00b7sen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat es noch im Tod geliebt.", "tokens": ["Hat", "es", "noch", "im", "Tod", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als Er schon ausgelitten,", "tokens": ["Und", "als", "Er", "schon", "aus\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meint' es noch Sein Herz so gut,", "tokens": ["Meint'", "es", "noch", "Sein", "Herz", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward vom scharfen Speer durchschnitten,", "tokens": ["Ward", "vom", "schar\u00b7fen", "Speer", "durch\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gab mir noch Sein letztes Blut.", "tokens": ["Gab", "mir", "noch", "Sein", "letz\u00b7tes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, uns\u00e4glich gro\u00dfe Liebe!", "tokens": ["O", ",", "un\u00b7s\u00e4g\u00b7lich", "gro\u00b7\u00dfe", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, unendlich liebend Herz!", "tokens": ["O", ",", "un\u00b7end\u00b7lich", "lie\u00b7bend", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weh! wer unempfindlich bliebe,", "tokens": ["Weh", "!", "wer", "un\u00b7emp\u00b7find\u00b7lich", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn besch\u00e4mten Stein und Erz.", "tokens": ["Ihn", "be\u00b7sch\u00e4m\u00b7ten", "Stein", "und", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer dies treuste Herz verachtet,", "tokens": ["Wer", "dies", "treus\u00b7te", "Herz", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das uns ewig Heil erwarb,", "tokens": ["Das", "uns", "e\u00b7wig", "Heil", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! der hat ", "tokens": ["Ach", "!", "der", "hat"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ART", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Der f\u00fcr ihn am Kreuze starb. \u2013", "tokens": ["Der", "f\u00fcr", "ihn", "am", "Kreu\u00b7ze", "starb", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00f6ge denn die Welt es wissen,", "tokens": ["M\u00f6\u00b7ge", "denn", "die", "Welt", "es", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in eitler Lust sich bl\u00e4ht,", "tokens": ["Die", "in", "eit\u00b7ler", "Lust", "sich", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mein Herz von Schmerz zerrissen", "tokens": ["Da\u00df", "mein", "Herz", "von", "Schmerz", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterm Kreuz des Heilands steht;", "tokens": ["Un\u00b7term", "Kreuz", "des", "Hei\u00b7lands", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df ich sie veracht' und hasse,", "tokens": ["Da\u00df", "ich", "sie", "ver\u00b7acht'", "und", "has\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie mir auch Kronen beut,", "tokens": ["Ob", "sie", "mir", "auch", "Kro\u00b7nen", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich gern Sein Kreuz umfasse,", "tokens": ["Da\u00df", "ich", "gern", "Sein", "Kreuz", "um\u00b7fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm zu folgen bin bereit.", "tokens": ["Ihm", "zu", "fol\u00b7gen", "bin", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mag sie prunken, mag sie schimmern,", "tokens": ["Mag", "sie", "prun\u00b7ken", ",", "mag", "sie", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis sie einst in Asche f\u00e4llt;", "tokens": ["Bis", "sie", "einst", "in", "A\u00b7sche", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenig wird mich das bek\u00fcmmern:", "tokens": ["We\u00b7nig", "wird", "mich", "das", "be\u00b7k\u00fcm\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesu Herz ist meine Welt.", "tokens": ["Je\u00b7su", "Herz", "ist", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}