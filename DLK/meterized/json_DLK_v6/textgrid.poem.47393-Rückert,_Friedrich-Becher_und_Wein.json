{"textgrid.poem.47393": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Becher und Wein", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt Ohren meinem Spruche,", "tokens": ["Gebt", "Oh\u00b7ren", "mei\u00b7nem", "Spru\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vernehmt und trinket nur,", "tokens": ["Ver\u00b7nehmt", "und", "trin\u00b7ket", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Bruchst\u00fcck aus dem Buche", "tokens": ["Ein", "Bruchs\u00b7t\u00fcck", "aus", "dem", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Weisheit der Natur.", "tokens": ["Der", "Weis\u00b7heit", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es fiel ein Strahl der Sonne", "tokens": ["Es", "fiel", "ein", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zugleich mit Adams Fall,", "tokens": ["Zu\u00b7gleich", "mit", "A\u00b7dams", "Fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlustig seiner Wonne,", "tokens": ["Ver\u00b7lus\u00b7tig", "sei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ward, erstarrt, Metall.", "tokens": ["Und", "ward", ",", "er\u00b7starrt", ",", "Me\u00b7tall", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hing das Gold in Kl\u00fcften,", "tokens": ["Es", "hing", "das", "Gold", "in", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohin das Licht nicht drang,", "tokens": ["Wo\u00b7hin", "das", "Licht", "nicht", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehnte sich, den L\u00fcften", "tokens": ["Und", "sehn\u00b7te", "sich", ",", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu k\u00fcnden Glanz und Klang.", "tokens": ["Zu", "k\u00fcn\u00b7den", "Glanz", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kam, um zu erl\u00f6sen", "tokens": ["Da", "kam", ",", "um", "zu", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Bruder aus der Nacht,", "tokens": ["Den", "Bru\u00b7der", "aus", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gefahren mit Get\u00f6sen", "tokens": ["Ge\u00b7fah\u00b7ren", "mit", "Ge\u00b7t\u00f6\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bergmann in den Schacht.", "tokens": ["Der", "Berg\u00b7mann", "in", "den", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da ward die Starrheit milde,", "tokens": ["Da", "ward", "die", "Star\u00b7rheit", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als in des K\u00fcnstlers Hand", "tokens": ["Als", "in", "des", "K\u00fcnst\u00b7lers", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gl\u00e4nzendes Gebilde,", "tokens": ["Ein", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein t\u00f6nendes, entstand.", "tokens": ["Ein", "t\u00f6\u00b7nen\u00b7des", ",", "ent\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war ein leer' Gef\u00e4\u00dfe", "tokens": ["Es", "war", "ein", "leer'", "Ge\u00b7f\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gab nur hohlen Klang;", "tokens": ["Und", "gab", "nur", "hoh\u00b7len", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da fehlte der gem\u00e4\u00dfe", "tokens": ["Da", "fehl\u00b7te", "der", "ge\u00b7m\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehalt der Form nicht lang.", "tokens": ["Ge\u00b7halt", "der", "Form", "nicht", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Denn als im Sonnenstrahle", "tokens": ["Denn", "als", "im", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Mark der Rebe schmolz,", "tokens": ["Das", "Mark", "der", "Re\u00b7be", "schmolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward die goldne Schale", "tokens": ["Da", "ward", "die", "gold\u00b7ne", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf goldnen Inhalt stolz.", "tokens": ["Auf", "gold\u00b7nen", "In\u00b7halt", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Becher gab ein T\u00f6nen,", "tokens": ["Der", "Be\u00b7cher", "gab", "ein", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wein begeistert schwoll,", "tokens": ["Der", "Wein", "be\u00b7geis\u00b7tert", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Empfindend, da\u00df vers\u00f6hnen", "tokens": ["Emp\u00b7fin\u00b7dend", ",", "da\u00df", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Streit er soll.", "tokens": ["Des", "Le\u00b7bens", "Streit", "er", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es spiegelte der Himmel", "tokens": ["Es", "spie\u00b7gel\u00b7te", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich in der klaren Flut,", "tokens": ["Sich", "in", "der", "kla\u00b7ren", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und irdisches Gewimmel", "tokens": ["Und", "ir\u00b7di\u00b7sches", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trank heitern Lebensmut.", "tokens": ["Trank", "hei\u00b7tern", "Le\u00b7bens\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erhebt den Blick, ihr Zecher,", "tokens": ["Er\u00b7hebt", "den", "Blick", ",", "ihr", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und trinkt, dem Lichte hold,", "tokens": ["Und", "trinkt", ",", "dem", "Lich\u00b7te", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus goldnem Sonnenbecher", "tokens": ["Aus", "gold\u00b7nem", "Son\u00b7nen\u00b7be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschmolznes Sonnengold.", "tokens": ["Ge\u00b7schmolz\u00b7nes", "Son\u00b7nen\u00b7gold", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}