{"textgrid.poem.62207": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[o Gott, Du wei\u00dft am besten, was uns frommt]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott, Du wei\u00dft am besten, was uns frommt", "tokens": ["O", "Gott", ",", "Du", "wei\u00dft", "am", "bes\u00b7ten", ",", "was", "uns", "frommt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gut ist alles, was von Deiner G\u00fcte kommt,", "tokens": ["Und", "gut", "ist", "al\u00b7les", ",", "was", "von", "Dei\u00b7ner", "G\u00fc\u00b7te", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein die Menschen sind so schwach:", "tokens": ["Al\u00b7lein", "die", "Men\u00b7schen", "sind", "so", "schwach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh' ihnen lieber alles nach!", "tokens": ["Sieh'", "ih\u00b7nen", "lie\u00b7ber", "al\u00b7les", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}