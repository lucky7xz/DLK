{"dta.poem.19182": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sinne, welchen Gott die obre Stelle gab,", "tokens": ["Die", "Sin\u00b7ne", ",", "wel\u00b7chen", "Gott", "die", "ob\u00b7re", "Stel\u00b7le", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehn auf die untere mit zuviel Stolz herab.", "tokens": ["Sehn", "auf", "die", "un\u00b7te\u00b7re", "mit", "zu\u00b7viel", "Stolz", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie sehn vor lauter Stolz nicht ein auf hohem Pf\u00fchl,", "tokens": ["Sie", "sehn", "vor", "lau\u00b7ter", "Stolz", "nicht", "ein", "auf", "ho\u00b7hem", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie nichts sind als ein besondertes Gef\u00fchl.", "tokens": ["Da\u00df", "sie", "nichts", "sind", "als", "ein", "be\u00b7son\u00b7der\u00b7tes", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Das Auge f\u00fchlt das Licht, und sieht, vom Licht ber\u00fchrt;", "tokens": ["Das", "Au\u00b7ge", "f\u00fchlt", "das", "Licht", ",", "und", "sieht", ",", "vom", "Licht", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch Ersch\u00fcttrung wird der Schall ins Ohr gef\u00fchrt.", "tokens": ["Und", "durch", "Er\u00b7sch\u00fct\u00b7trung", "wird", "der", "Schall", "ins", "Ohr", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}