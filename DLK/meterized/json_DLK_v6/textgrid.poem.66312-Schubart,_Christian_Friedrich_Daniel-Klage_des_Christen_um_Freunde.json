{"textgrid.poem.66312": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Klage des Christen um Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche wilde Trauer,", "tokens": ["Wel\u00b7che", "wil\u00b7de", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Welcher Todesschauer", "tokens": ["Wel\u00b7cher", "To\u00b7des\u00b7schau\u00b7er"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Herrschet um mich her?", "tokens": ["Herr\u00b7schet", "um", "mich", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mit zerstreutem Haare", "tokens": ["Mit", "zer\u00b7streu\u00b7tem", "Haa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Stehet an der Bahre", "tokens": ["Ste\u00b7het", "an", "der", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der Betr\u00fcbten Heer.", "tokens": ["Der", "Be\u00b7tr\u00fcb\u00b7ten", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wilder Schmerz", "tokens": ["Wil\u00b7der", "Schmerz"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Zerrei\u00dft ihr Herz;", "tokens": ["Zer\u00b7rei\u00dft", "ihr", "Herz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gro\u00dfe Thr\u00e4nentropfen hangen", "tokens": ["Gro\u00b7\u00dfe", "Thr\u00e4\u00b7nen\u00b7trop\u00b7fen", "han\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Blutig an den Wangen.", "tokens": ["Blu\u00b7tig", "an", "den", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Heidnisches Get\u00fcmmel", "tokens": ["Heid\u00b7ni\u00b7sches", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jammert laut gen Himmel", "tokens": ["Jam\u00b7mert", "laut", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und bet\u00e4ubt mein Ohr.", "tokens": ["Und", "be\u00b7t\u00e4ubt", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Abgeh\u00e4rmte Mienen,", "tokens": ["Ab\u00b7ge\u00b7h\u00e4rm\u00b7te", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Todesangst in ihnen,", "tokens": ["To\u00b7de\u00b7sangst", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blicken aus dem Flor.", "tokens": ["Bli\u00b7cken", "aus", "dem", "Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie sie sich", "tokens": ["Wie", "sie", "sich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "So j\u00e4mmerlich", "tokens": ["So", "j\u00e4m\u00b7mer\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mit hervorgepre\u00dften Blicken", "tokens": ["Mit", "her\u00b7vor\u00b7ge\u00b7pre\u00df\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In die Gr\u00e4ber b\u00fccken!", "tokens": ["In", "die", "Gr\u00e4\u00b7ber", "b\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar die Thr\u00e4nenb\u00e4che", "tokens": ["Zwar", "die", "Thr\u00e4\u00b7nen\u00b7b\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zeugen von der Schw\u00e4che", "tokens": ["Zeu\u00b7gen", "von", "der", "Schw\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Menschlicher Natur;", "tokens": ["Menschli\u00b7cher", "Na\u00b7tur", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch am Tag der Leiden", "tokens": ["Doch", "am", "Tag", "der", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Christen ", "tokens": ["Chris\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Seid ihr blind,", "tokens": ["Seid", "ihr", "blind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wie Heiden sind,", "tokens": ["Wie", "Hei\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die, wenn Bruderleichen brennen,", "tokens": ["Die", ",", "wenn", "Bru\u00b7der\u00b7lei\u00b7chen", "bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Keine Hoffnung kennen?", "tokens": ["Kei\u00b7ne", "Hoff\u00b7nung", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wahre Christen wehren", "tokens": ["Wah\u00b7re", "Chris\u00b7ten", "weh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ungest\u00fcmen Z\u00e4hren", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7men", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einen freien Lauf.", "tokens": ["Ei\u00b7nen", "frei\u00b7en", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Weckt man denn mit Sehnen,", "tokens": ["Weckt", "man", "denn", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00e4nderingen, Thr\u00e4nen", "tokens": ["H\u00e4n\u00b7de\u00b7rin\u00b7gen", ",", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Einen Todten auf?", "tokens": ["Ei\u00b7nen", "Tod\u00b7ten", "auf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Seelen, sagt,", "tokens": ["See\u00b7len", ",", "sagt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die ihr verzagt:", "tokens": ["Die", "ihr", "ver\u00b7zagt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ist denn eine Welt verdorben,", "tokens": ["Ist", "denn", "ei\u00b7ne", "Welt", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn ein Mensch gestorben?", "tokens": ["Wenn", "ein", "Mensch", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nur, wenn Christen hoffen,", "tokens": ["Nur", ",", "wenn", "Chris\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steht der Himmel offen;", "tokens": ["Steht", "der", "Him\u00b7mel", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber S\u00fcndern nicht,", "tokens": ["A\u00b7ber", "S\u00fcn\u00b7dern", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die verzweifeln wollen,", "tokens": ["Die", "ver\u00b7zwei\u00b7feln", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn sie warten sollen,", "tokens": ["Wenn", "sie", "war\u00b7ten", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Bis der Tr\u00f6ster spricht.", "tokens": ["Bis", "der", "Tr\u00f6s\u00b7ter", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ruht der Freund,", "tokens": ["Ruht", "der", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Den ihr beweint,", "tokens": ["Den", "ihr", "be\u00b7weint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Wenn er selig abgeschieden,", "tokens": ["Wenn", "er", "se\u00b7lig", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ruht er nicht im Frieden?", "tokens": ["Ruht", "er", "nicht", "im", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Zwar auch Christen weinen", "tokens": ["Zwar", "auch", "Chris\u00b7ten", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ueber den Gebeinen,", "tokens": ["Ue\u00b7ber", "den", "Ge\u00b7bei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die die Gruft verschlie\u00dft;", "tokens": ["Die", "die", "Gruft", "ver\u00b7schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dich nur eine Z\u00e4hre,", "tokens": ["Dich", "nur", "ei\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die dem Freund zur Ehre", "tokens": ["Die", "dem", "Freund", "zur", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und nicht ewig flie\u00dft.", "tokens": ["Und", "nicht", "e\u00b7wig", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Denn ihr Blick", "tokens": ["Denn", "ihr", "Blick"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Strahlt nach dem Gl\u00fcck,", "tokens": ["Strahlt", "nach", "dem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punc