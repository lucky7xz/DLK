pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bald wirst du", "tokens": ["Bald", "wirst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Zur stillen Ruh'", "tokens": ["Zur", "stil\u00b7len", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In das Land des Friedens gehen,", "tokens": ["In", "das", "Land", "des", "Frie\u00b7dens", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und den Bruder sehen.", "tokens": ["Und", "den", "Bru\u00b7der", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Br\u00fcder treffen dorten", "tokens": ["Br\u00fc\u00b7der", "tref\u00b7fen", "dor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "An des Himmels Pforten", "tokens": ["An", "des", "Him\u00b7mels", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihre Br\u00fcder an;", "tokens": ["Ih\u00b7re", "Br\u00fc\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und ein David brennet,", "tokens": ["Und", "ein", "Da\u00b7vid", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measu