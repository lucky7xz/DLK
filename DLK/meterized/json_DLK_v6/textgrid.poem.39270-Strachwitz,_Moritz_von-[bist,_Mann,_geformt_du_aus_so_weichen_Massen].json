{"textgrid.poem.39270": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[bist, Mann, geformt du aus so weichen Massen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist, Mann, geformt du aus so weichen Massen,", "tokens": ["Bist", ",", "Mann", ",", "ge\u00b7formt", "du", "aus", "so", "wei\u00b7chen", "Mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "VVPP", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dir die Schwertwucht l\u00e4hmt die Weiberarme?", "tokens": ["Da\u00df", "dir", "die", "Schwert\u00b7wucht", "l\u00e4hmt", "die", "Wei\u00b7be\u00b7rar\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kannst du nicht stehn im dichten Waffenschwarme,", "tokens": ["Kannst", "du", "nicht", "stehn", "im", "dich\u00b7ten", "Waf\u00b7fen\u00b7schwar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Gott des Kampfes Wetter losgelassen?", "tokens": ["Wenn", "Gott", "des", "Kamp\u00b7fes", "Wet\u00b7ter", "los\u00b7ge\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ha! nimmer soll dein Angesicht erblassen,", "tokens": ["Ha", "!", "nim\u00b7mer", "soll", "dein", "An\u00b7ge\u00b7sicht", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gib Worte k\u00fchn des freien Herzens Harme,", "tokens": ["Gib", "Wor\u00b7te", "k\u00fchn", "des", "frei\u00b7en", "Her\u00b7zens", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Worten Schwertesstreiche, zorneswarme,", "tokens": ["Den", "Wor\u00b7ten", "Schwer\u00b7tes\u00b7strei\u00b7che", ",", "zor\u00b7nes\u00b7war\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Schmach und Unrecht krallend dich umfassen.", "tokens": ["Wenn", "Schmach", "und", "Un\u00b7recht", "kral\u00b7lend", "dich", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn nicht allein auf blut'gem Schlachtenfelde", "tokens": ["Denn", "nicht", "al\u00b7lein", "auf", "blut'\u00b7gem", "Schlach\u00b7ten\u00b7fel\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ziemt's k\u00fchn zu wallen durch des Streites N\u00e4chte,", "tokens": ["Ziemt's", "k\u00fchn", "zu", "wal\u00b7len", "durch", "des", "Strei\u00b7tes", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, auch die Harfe mag zum Schwerte werden;", "tokens": ["Nein", ",", "auch", "die", "Har\u00b7fe", "mag", "zum", "Schwer\u00b7te", "wer\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VMFIN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn da\u00df den Klang des Heldenschwerts er melde,", "tokens": ["Denn", "da\u00df", "den", "Klang", "des", "Hel\u00b7den\u00b7schwerts", "er", "mel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im edlen Streit mit Waffenliedern fechte \u2013", "tokens": ["Im", "ed\u00b7len", "Streit", "mit", "Waf\u00b7fen\u00b7lie\u00b7dern", "fech\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist die Pflicht des S\u00e4ngers auf der Erden.", "tokens": ["Das", "ist", "die", "Pflicht", "des", "S\u00e4n\u00b7gers", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}