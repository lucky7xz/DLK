{"textgrid.poem.32888": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie, Damon, du beklagest dich,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie, Damon, du beklagest dich,", "tokens": ["Wie", ",", "Da\u00b7mon", ",", "du", "be\u00b7kla\u00b7gest", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NE", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du weit st\u00e4rker liebst, als ich!", "tokens": ["Da\u00df", "du", "weit", "st\u00e4r\u00b7ker", "liebst", ",", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich, was hast du noch je gewagt?", "tokens": ["Sprich", ",", "was", "hast", "du", "noch", "je", "ge\u00b7wagt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von mir gefordert? ich versagt?", "tokens": ["Von", "mir", "ge\u00b7for\u00b7dert", "?", "ich", "ver\u00b7sagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du k\u00f6mmst, entgegen lauf ich dir:", "tokens": ["Du", "k\u00f6mmst", ",", "ent\u00b7ge\u00b7gen", "lauf", "ich", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sprichst, es wallt mein Herz in mir:", "tokens": ["Du", "sprichst", ",", "es", "wallt", "mein", "Herz", "in", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O fordre mehr! dann siehe zu,", "tokens": ["O", "ford\u00b7re", "mehr", "!", "dann", "sie\u00b7he", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "ADV", "VVIMP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer st\u00e4rker liebt, ich, oder du?", "tokens": ["Wer", "st\u00e4r\u00b7ker", "liebt", ",", "ich", ",", "o\u00b7der", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PPER", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}