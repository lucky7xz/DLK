{"textgrid.poem.55340": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Zu Gem\u00e4lden einer Kapelle", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie Moses, kaum geboren,", "tokens": ["So", "wie", "Mo\u00b7ses", ",", "kaum", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gewissem Tode bestimmt,", "tokens": ["Ge\u00b7wis\u00b7sem", "To\u00b7de", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wunderbar ward gerettet:", "tokens": ["Wun\u00b7der\u00b7bar", "ward", "ge\u00b7ret\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So mancher, schon halb verloren,", "tokens": ["So", "man\u00b7cher", ",", "schon", "halb", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da der Feind eindrang ergrimmt,", "tokens": ["Da", "der", "Feind", "ein\u00b7drang", "er\u00b7grimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Ward wieder froh und gl\u00fccklich gebettet.", "tokens": ["Ward", "wie\u00b7der", "froh", "und", "gl\u00fcck\u00b7lich", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Johannes erst in der W\u00fcste predigt:", "tokens": ["Jo\u00b7han\u00b7nes", "erst", "in", "der", "W\u00fcs\u00b7te", "pre\u00b7digt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbseht Gottes Lamm, das von S\u00fcnden erledigt.\u00ab", "tokens": ["\u00bb", "seht", "Got\u00b7tes", "Lamm", ",", "das", "von", "S\u00fcn\u00b7den", "er\u00b7le\u00b7digt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NN", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun deutet er in die himmlischen Auen:", "tokens": ["Nun", "deu\u00b7tet", "er", "in", "die", "himm\u00b7li\u00b7schen", "Au\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbdort sollt ihr den Herrn, den erl\u00f6senden, schauen.\u00ab", "tokens": ["\u00bb", "dort", "sollt", "ihr", "den", "Herrn", ",", "den", "er\u00b7l\u00f6\u00b7sen\u00b7den", ",", "schau\u00b7en", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$.", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}