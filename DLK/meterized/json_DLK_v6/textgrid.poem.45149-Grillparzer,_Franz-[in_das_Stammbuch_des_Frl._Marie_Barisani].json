{"textgrid.poem.45149": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in das Stammbuch des Frl. Marie Barisani]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Uhr, die man als Braut dir zum Geschenk gemacht,", "tokens": ["Die", "Uhr", ",", "die", "man", "als", "Braut", "dir", "zum", "Ge\u00b7schenk", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die ich dir, noch fremd so schwerer Kunde,", "tokens": ["Und", "die", "ich", "dir", ",", "noch", "fremd", "so", "schwe\u00b7rer", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PPER", "$,", "ADV", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum erstenmal in Gang gebracht,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "in", "Gang", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sei mit deinem holden Selbst im Bunde,", "tokens": ["Sie", "sei", "mit", "dei\u00b7nem", "hol\u00b7den", "Selbst", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Ordnung Bild bei Tag und Nacht,", "tokens": ["Der", "Ord\u00b7nung", "Bild", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zeige stets, wie du, die gute Stunde.", "tokens": ["Und", "zei\u00b7ge", "stets", ",", "wie", "du", ",", "die", "gu\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}