{"textgrid.poem.34113": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die alten Naturdichter und die neuen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft ihr, warum euch die K\u00e4fer, die Butterblumen so gl\u00fccken?", "tokens": ["Wi\u00dft", "ihr", ",", "wa\u00b7rum", "euch", "die", "K\u00e4\u00b7fer", ",", "die", "But\u00b7ter\u00b7blu\u00b7men", "so", "gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Weil ihr die Menschen nicht kennt, weil ihr die Sterne nicht seht!", "tokens": ["Weil", "ihr", "die", "Men\u00b7schen", "nicht", "kennt", ",", "weil", "ihr", "die", "Ster\u00b7ne", "nicht", "seht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schautet ihr tief in die Herzen, wie k\u00f6nntet ihr schw\u00e4rmen f\u00fcr K\u00e4fer?", "tokens": ["Schau\u00b7tet", "ihr", "tief", "in", "die", "Her\u00b7zen", ",", "wie", "k\u00f6nn\u00b7tet", "ihr", "schw\u00e4r\u00b7men", "f\u00fcr", "K\u00e4\u00b7fer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "S\u00e4ht ihr das Sonnensystem, sagt doch, was w\u00e4r' euch ein Strau\u00df?", "tokens": ["S\u00e4ht", "ihr", "das", "Son\u00b7nen\u00b7sys\u00b7tem", ",", "sagt", "doch", ",", "was", "w\u00e4r'", "euch", "ein", "Strau\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADV", "$,", "PWS", "VAFIN", "PPER", "ART", "NE", "$."], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Aber das mu\u00dfte so sein; damit ihr das Kleine vortrefflich", "tokens": ["A\u00b7ber", "das", "mu\u00df\u00b7te", "so", "sein", ";", "da\u00b7mit", "ihr", "das", "Klei\u00b7ne", "vor\u00b7treff\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "ADV", "VAINF", "$.", "KOUS", "PPER", "ART", "ADJA", "ADJD"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Liefertet, hat die Natur klug euch das Gro\u00dfe entr\u00fcckt.", "tokens": ["Lie\u00b7fer\u00b7tet", ",", "hat", "die", "Na\u00b7tur", "klug", "euch", "das", "Gro\u00b7\u00dfe", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}