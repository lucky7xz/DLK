{"textgrid.poem.66825": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traurig tr\u00e4umt der Riesenschornstein,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig tr\u00e4umt der Riesenschornstein,", "tokens": ["Trau\u00b7rig", "tr\u00e4umt", "der", "Rie\u00b7sen\u00b7schorn\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und der Blitzableiter meint:", "tokens": ["Und", "der", "Blitz\u00b7ab\u00b7lei\u00b7ter", "meint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u00bbmeyer, Rosenstern und Kornstein", "tokens": ["\u00bb", "me\u00b7yer", ",", "Ro\u00b7sens\u00b7tern", "und", "Korns\u00b7tein"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "$,", "NN", "KON", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Rauchen nimmer, wie mir scheint.", "tokens": ["Rau\u00b7chen", "nim\u00b7mer", ",", "wie", "mir", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollte die Fabrik verkracht sein,", "tokens": ["Soll\u00b7te", "die", "Fab\u00b7rik", "ver\u00b7kracht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Oder w\u00e4r' es blo\u00df ein Streik?", "tokens": ["O\u00b7der", "w\u00e4r'", "es", "blo\u00df", "ein", "Streik", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig wird die Schlacht sein ...\u00ab \u2013", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", "wird", "die", "Schlacht", "sein", "...", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbschweige, Blitzableiter, schweig!", "tokens": ["\u00bb", "schwei\u00b7ge", ",", "Blitz\u00b7ab\u00b7lei\u00b7ter", ",", "schweig", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blo\u00df ein Streik, blo\u00df und nichts weiter,", "tokens": ["Blo\u00df", "ein", "Streik", ",", "blo\u00df", "und", "nichts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "KON", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du spindeld\u00fcrrer Fant!", "tokens": ["O", "du", "spin\u00b7del\u00b7d\u00fcr\u00b7rer", "Fant", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig, traurig, Blitzableiter,", "tokens": ["Trau\u00b7rig", ",", "trau\u00b7rig", ",", "Blitz\u00b7ab\u00b7lei\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Kampf, der hier entbrannt.", "tokens": ["Ist", "der", "Kampf", ",", "der", "hier", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die f\u00fcr Lohn und Leben k\u00e4mpfen,", "tokens": ["Die", "f\u00fcr", "Lohn", "und", "Le\u00b7ben", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finden Feinde sonder Zahl,", "tokens": ["Fin\u00b7den", "Fein\u00b7de", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winden sich in Hungerkr\u00e4mpfen,", "tokens": ["Win\u00b7den", "sich", "in", "Hun\u00b7ger\u00b7kr\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie siegen nicht einmal.", "tokens": ["Und", "sie", "sie\u00b7gen", "nicht", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Siehst du, Lump, die neuen Leute,", "tokens": ["Siehst", "du", ",", "Lump", ",", "die", "neu\u00b7en", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Ferne massenhaft", "tokens": ["Aus", "der", "Fer\u00b7ne", "mas\u00b7sen\u00b7haft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Milit\u00e4rgeleite", "tokens": ["Un\u00b7ter", "Mi\u00b7li\u00b7t\u00e4r\u00b7ge\u00b7lei\u00b7te"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hergelotste Arbeitskraft?", "tokens": ["Her\u00b7ge\u00b7lots\u00b7te", "Ar\u00b7beits\u00b7kraft", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Morgen mu\u00df ich wieder schloten,", "tokens": ["Mor\u00b7gen", "mu\u00df", "ich", "wie\u00b7der", "schlo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Rost dich fr\u00e4\u00dfe, Wicht!", "tokens": ["Da\u00df", "der", "Rost", "dich", "fr\u00e4\u00b7\u00dfe", ",", "Wicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mir \u00fcber Nacht die roten", "tokens": ["Da\u00df", "mir", "\u00fc\u00b7ber", "Nacht", "die", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammen leckten ums Gesicht! ...\u00ab", "tokens": ["Flam\u00b7men", "leck\u00b7ten", "ums", "Ge\u00b7sicht", "!", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbmeyer, Rosenstern und Kornstein!\u00ab", "tokens": ["\u00bb", "me\u00b7yer", ",", "Ro\u00b7sens\u00b7tern", "und", "Korns\u00b7tein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "--+---+-", "measure": "anapaest.init"}, "line.2": {"text": "Kleinlaut Franklins Spr\u00f6\u00dfling mault:", "tokens": ["Klein\u00b7laut", "Fran\u00b7klins", "Spr\u00f6\u00df\u00b7ling", "mault", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbjetzt ist euer Riesenschornstein", "tokens": ["\u00bb", "jetzt", "ist", "eu\u00b7er", "Rie\u00b7sen\u00b7schorn\u00b7stein"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sozialistisch angefault.\u00ab", "tokens": ["So\u00b7zi\u00b7a\u00b7lis\u00b7tisch", "an\u00b7ge\u00b7fault", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}