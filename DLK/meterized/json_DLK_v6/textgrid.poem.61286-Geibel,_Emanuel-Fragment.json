{"textgrid.poem.61286": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fragment", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist lau, die Schw\u00e4ne kreisen,", "tokens": ["Die", "Nacht", "ist", "lau", ",", "die", "Schw\u00e4\u00b7ne", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entschlummert scheinen Bl\u00fct' und Blatt,", "tokens": ["Ent\u00b7schlum\u00b7mert", "schei\u00b7nen", "Bl\u00fct'", "und", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lehn' dich auf des Gel\u00e4nders Eisen,", "tokens": ["Lehn'", "dich", "auf", "des", "Ge\u00b7l\u00e4n\u00b7ders", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dort zeigt am sch\u00f6nsten sich die Stadt.", "tokens": ["Dort", "zeigt", "am", "sch\u00f6ns\u00b7ten", "sich", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Siehst du den H\u00e4userkreis, den dunkeln,", "tokens": ["Siehst", "du", "den", "H\u00e4u\u00b7ser\u00b7kreis", ",", "den", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus welchem tausend Lichter funkeln,", "tokens": ["Aus", "wel\u00b7chem", "tau\u00b7send", "Lich\u00b7ter", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die tief sich spiegeln in der Flut?", "tokens": ["Die", "tief", "sich", "spie\u00b7geln", "in", "der", "Flut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So ist's, wenn mit geschliffnen Kanten", "tokens": ["So", "ist's", ",", "wenn", "mit", "ge\u00b7schliff\u00b7nen", "Kan\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Kranz von blitzenden Demanten", "tokens": ["Ein", "Kranz", "von", "blit\u00b7zen\u00b7den", "De\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf blauem Sammetkissen ruht.", "tokens": ["Auf", "blau\u00b7em", "Sam\u00b7met\u00b7kis\u00b7sen", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm n\u00e4her! Sieh, wie hier in Massen", "tokens": ["Komm", "n\u00e4\u00b7her", "!", "Sieh", ",", "wie", "hier", "in", "Mas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$.", "NE", "$,", "PWAV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menschenwoge sich ergie\u00dft!", "tokens": ["Die", "Men\u00b7schen\u00b7wo\u00b7ge", "sich", "er\u00b7gie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies sind die H\u00e4user, sind die Gassen,", "tokens": ["Dies", "sind", "die", "H\u00e4u\u00b7ser", ",", "sind", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man erwirbt, wo man genie\u00dft.", "tokens": ["Wo", "man", "er\u00b7wirbt", ",", "wo", "man", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von lichtem Kerzenglanz umflossen", "tokens": ["Von", "lich\u00b7tem", "Ker\u00b7zen\u00b7glanz", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ruht hier im Prunkgew\u00f6lb' erschlossen", "tokens": ["Ruht", "hier", "im", "Prunk\u00b7ge\u00b7w\u00f6lb'", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der fernsten Zonen Schmuck und Zier;", "tokens": ["Der", "ferns\u00b7ten", "Zo\u00b7nen", "Schmuck", "und", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und horch, aus jenen S\u00e4ulenhallen", "tokens": ["Und", "horch", ",", "aus", "je\u00b7nen", "S\u00e4u\u00b7len\u00b7hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durchs Klirren der Pokale schallen", "tokens": ["Durchs", "Klir\u00b7ren", "der", "Po\u00b7ka\u00b7le", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der G\u00e4ste Lieder. Lauschen wir!", "tokens": ["Der", "G\u00e4s\u00b7te", "Lie\u00b7der", ".", "Lau\u00b7schen", "wir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbla\u00dft andre beten, andre fasten!", "tokens": ["\u00bb", "la\u00dft", "and\u00b7re", "be\u00b7ten", ",", "and\u00b7re", "fas\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PIS", "VVFIN", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr unsre Stirn der Freude Kranz!", "tokens": ["F\u00fcr", "uns\u00b7re", "Stirn", "der", "Freu\u00b7de", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns f\u00fchren hunderttausend Masten", "tokens": ["Uns", "f\u00fch\u00b7ren", "hun\u00b7dert\u00b7tau\u00b7send", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die G\u00f6tter her: Genu\u00df und Glanz.", "tokens": ["Die", "G\u00f6t\u00b7ter", "her", ":", "Ge\u00b7nu\u00df", "und", "Glanz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schafft die Welt an allen Enden", "tokens": ["Es", "schafft", "die", "Welt", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr unser Fest mit tausend H\u00e4nden,", "tokens": ["F\u00fcr", "un\u00b7ser", "Fest", "mit", "tau\u00b7send", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wahl des K\u00f6stlichsten ist schwer;", "tokens": ["Die", "Wahl", "des", "K\u00f6st\u00b7lichs\u00b7ten", "ist", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Die H\u00fcgel zollen s\u00fc\u00dfe Weine,", "tokens": ["Die", "H\u00fc\u00b7gel", "zol\u00b7len", "s\u00fc\u00b7\u00dfe", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Berge geben Gold und Steine,", "tokens": ["Die", "Ber\u00b7ge", "ge\u00b7ben", "Gold", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und seine Perlen gibt das Meer.", "tokens": ["Und", "sei\u00b7ne", "Per\u00b7len", "gibt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schaut dies Gemach an! Die Tapeten", "tokens": ["Schaut", "dies", "Ge\u00b7mach", "an", "!", "Die", "Ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat China bunt uns ausgespannt;", "tokens": ["Hat", "Chi\u00b7na", "bunt", "uns", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der farb'ge Teppich, drauf wir treten,", "tokens": ["Der", "fa\u00b7rb'\u00b7ge", "Tep\u00b7pich", ",", "drauf", "wir", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kommt aus der Smyrnioten Hand;", "tokens": ["Kommt", "aus", "der", "Smyr\u00b7ni\u00b7o\u00b7ten", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Holzwerk, das ge\u00e4dert gl\u00e4nzet,", "tokens": ["Das", "Holz\u00b7werk", ",", "das", "ge\u00b7\u00e4\u00b7dert", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat einst als laub'ger Wald umkr\u00e4nzet", "tokens": ["Hat", "einst", "als", "laub'\u00b7ger", "Wald", "um\u00b7kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den hohen Bord von Martinique!", "tokens": ["Den", "ho\u00b7hen", "Bord", "von", "Mar\u00b7ti\u00b7ni\u00b7que", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Antwerpen wob des Vorhangs Sammet,", "tokens": ["Ant\u00b7wer\u00b7pen", "wob", "des", "Vor\u00b7hangs", "Sam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und aus Venedigs Spiegel flammet", "tokens": ["Und", "aus", "Ve\u00b7ne\u00b7digs", "Spie\u00b7gel", "flam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NE", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Die Ampel von Paris zur\u00fcck.", "tokens": ["Die", "Am\u00b7pel", "von", "Pa\u00b7ris", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum la\u00dft uns keinen K\u00f6nig neiden!", "tokens": ["Drum", "la\u00dft", "uns", "kei\u00b7nen", "K\u00f6\u00b7nig", "nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ihn die Macht, f\u00fcr uns die Lust!", "tokens": ["F\u00fcr", "ihn", "die", "Macht", ",", "f\u00fcr", "uns", "die", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mag er in Waffenschmuck sich kleiden,", "tokens": ["Mag", "er", "in", "Waf\u00b7fen\u00b7schmuck", "sich", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Seiden weicher schl\u00e4ft die Brust;", "tokens": ["In", "Sei\u00b7den", "wei\u00b7cher", "schl\u00e4ft", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mag er um Schwei\u00df sich Ruhm erkaufen!", "tokens": ["Mag", "er", "um", "Schwei\u00df", "sich", "Ruhm", "er\u00b7kau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was frommt ihm, wenn die Zeit verlaufen,", "tokens": ["Was", "frommt", "ihm", ",", "wenn", "die", "Zeit", "ver\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Lorbeerkranz, der Thronen Sturz?", "tokens": ["Der", "Lor\u00b7beer\u00b7kranz", ",", "der", "Thro\u00b7nen", "Sturz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir wollen, wo die Tafeln brechen,", "tokens": ["Wir", "wol\u00b7len", ",", "wo", "die", "Ta\u00b7feln", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den ros'gen Augenblick verzechen;", "tokens": ["Den", "ros'\u00b7gen", "Au\u00b7gen\u00b7blick", "ver\u00b7ze\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Grab ist schwarz, das Leben kurz.", "tokens": ["Das", "Grab", "ist", "schwarz", ",", "das", "Le\u00b7ben", "kurz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und schafft Musik zum reichen Tische!", "tokens": ["Und", "schafft", "Mu\u00b7sik", "zum", "rei\u00b7chen", "Ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie flute halbgeh\u00f6rt dahin,", "tokens": ["Sie", "flu\u00b7te", "halb\u00b7ge\u00b7h\u00f6rt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein k\u00fchles Bad erfrische", "tokens": ["Und", "wie", "ein", "k\u00fch\u00b7les", "Bad", "er\u00b7fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NE", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verhallend sie den hei\u00dfen Sinn.", "tokens": ["Ver\u00b7hal\u00b7lend", "sie", "den", "hei\u00b7\u00dfen",