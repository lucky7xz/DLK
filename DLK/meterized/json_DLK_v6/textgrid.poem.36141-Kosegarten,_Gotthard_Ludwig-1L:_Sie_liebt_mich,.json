{"textgrid.poem.36141": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie liebt mich,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie liebt mich,", "tokens": ["Sie", "liebt", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sie liebt mich!!", "tokens": ["Sie", "liebt", "mich", "!!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Welch Zittern ergreift mich!", "tokens": ["Welch", "Zit\u00b7tern", "er\u00b7greift", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Welch Sturm zerr\u00fcttet mir die fliegende Brust! \u2013", "tokens": ["Welch", "Sturm", "zer\u00b7r\u00fct\u00b7tet", "mir", "die", "flie\u00b7gen\u00b7de", "Brust", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Sie liebt mich!", "tokens": ["Sie", "liebt", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sie liebt mich!", "tokens": ["Sie", "liebt", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Welch' Trunkenheit fa\u00dft mich,", "tokens": ["Welch'", "Trun\u00b7ken\u00b7heit", "fa\u00dft", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Welch str\u00f6mendes Leben, und paradiesische Lust! \u2013", "tokens": ["Welch", "str\u00f6\u00b7men\u00b7des", "Le\u00b7ben", ",", "und", "pa\u00b7ra\u00b7die\u00b7si\u00b7sche", "Lust", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Sie liebt mich!", "tokens": ["Sie", "liebt", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sie liebt mich! \u2013", "tokens": ["Sie", "liebt", "mich", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie fass' ich die Wonne,", "tokens": ["Wie", "fass'", "ich", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die hohe unaussprechliche Wonne,", "tokens": ["Die", "ho\u00b7he", "un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df meine Wonna mich liebet!", "tokens": ["Da\u00df", "mei\u00b7ne", "Won\u00b7na", "mich", "lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wonne, du herrliche", "tokens": ["Won\u00b7ne", ",", "du", "herr\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Schmetternde, sch\u00fctternde,", "tokens": ["Schmet\u00b7tern\u00b7de", ",", "sch\u00fct\u00b7tern\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Du unaussprechliche! kann ich dich fassen,", "tokens": ["Du", "un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "!", "kann", "ich", "dich", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df meine himmlische Wonna mich liebet? \u2013", "tokens": ["Da\u00df", "mei\u00b7ne", "himm\u00b7li\u00b7sche", "Won\u00b7na", "mich", "lie\u00b7bet", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wonna, Wonna,", "tokens": ["Won\u00b7na", ",", "Won\u00b7na", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "----", "measure": "unknown.measure.zero"}, "line.2": {"text": "Meine himmlische Wonna,", "tokens": ["Mei\u00b7ne", "himm\u00b7li\u00b7sche", "Won\u00b7na", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebest du mich? \u2013", "tokens": ["Lie\u00b7best", "du", "mich", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ja, du liebst mich!", "tokens": ["Ja", ",", "du", "liebst", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du liebst mich! \u2013", "tokens": ["Du", "liebst", "mich", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Brennend und weinend,", "tokens": ["Bren\u00b7nend", "und", "wei\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit Stammeln und Stocken,", "tokens": ["Mit", "Stam\u00b7meln", "und", "Sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit Zittern und Beben.", "tokens": ["Mit", "Zit\u00b7tern", "und", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mit tausend K\u00fcssen,", "tokens": ["Mit", "tau\u00b7send", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Tausend br\u00fcnstigen gl\u00fchenden K\u00fcssen,", "tokens": ["Tau\u00b7send", "br\u00fcns\u00b7ti\u00b7gen", "gl\u00fc\u00b7hen\u00b7den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Hast du mir die Wonne geschworen, geweint:", "tokens": ["Hast