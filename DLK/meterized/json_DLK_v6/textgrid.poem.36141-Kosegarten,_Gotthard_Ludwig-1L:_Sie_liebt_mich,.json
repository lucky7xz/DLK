.di"}, "line.5": {"text": "Mein Auge weint.", "tokens": ["Mein", "Au\u00b7ge", "weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sieh, meine Wonna,", "tokens": ["Sieh", ",", "mei\u00b7ne", "Won\u00b7na", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie sanft mein Auge weint, da\u00df du mich liebst.", "tokens": ["Wie", "sanft", "mein", "Au\u00b7ge", "weint", ",", "da\u00df", "du", "mich", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O, ich liebe dich ewig.", "tokens": ["O", ",", "ich", "lie\u00b7be", "dich", "e\u00b7wig", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Ewig, ewig", "tokens": ["E\u00b7wig", ",", "e\u00b7wig"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Liebt dich meine Seele \u2013 \u2013", "tokens": ["Liebt", "dich", "mei\u00b7ne", "See\u00b7le", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Warum weinet meine Wonna!", "tokens": ["Wa\u00b7rum", "wei\u00b7net", "mei\u00b7ne", "Won\u00b7na", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol