ren,", "tokens": ["Du", "hast", "mir's", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Du hast mir's geweint,", "tokens": ["Du", "hast", "mir's", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Da\u00df ewig, ewig die Meinige, du! \u2013 \u2013", "tokens": ["Da\u00df", "e\u00b7wig", ",", "e\u00b7wig", "die", "Mei\u00b7ni\u00b7ge", ",", "du", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADJD", "ART", "PPOSS", "$,", "PPER", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wonna, die Meine!", "tokens": ["Won\u00b7na", ",", "die", "Mei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.10": {"text": "Meine Wonna! Sie ist die Meine!", "tokens": ["Mei\u00b7ne", "Won\u00b7na", "!", "Sie", "ist", "die", "Mei\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Mir s\u00e4uselt's ihr Odem,", "tokens": ["Mir", "s\u00e4u\u00b7selt's", "ihr", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mir rauscht es ihr Liebesku\u00df.", "tokens": ["Mir", "rauscht", "es", "ihr", "Lie\u00b7bes\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Mir lispelt's jedes halb hergestammeltes Wort:", "tokens": ["Mir", "lis\u00b7pelt's", "je\u00b7des", "halb", "her\u00b7ge\u00b7stam\u00b7mel\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "\u00bbich bin die Deine!", "tokens": ["\u00bb", "ich", "bin", "die", "Dei\u00b7ne", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ewig, ewig die Deine!!!\u00ab", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "die", "Dei\u00b7ne", "!!!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "PPOSAT", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "O, du, die mich liebet,", "tokens": ["O", ",", "du", ",", "die", "mich", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "O, du, die die Meine ist,", "tokens": ["O", ",", "du", ",", "die", "die", "Mei\u00b7ne", "ist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie f\u00fchl' ich's so m\u00e4chtig,", "tokens": ["Wie", "f\u00fchl'", "ich's", "so", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "