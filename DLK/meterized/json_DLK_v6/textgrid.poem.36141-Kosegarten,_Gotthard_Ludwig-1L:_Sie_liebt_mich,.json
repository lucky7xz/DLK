: "Wo bist du? \u2013 \u2013", "tokens": ["Wo", "bist", "du", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Fern hinter Gebirgen,", "tokens": ["Fern", "hin\u00b7ter", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fern hinter zehn tausend feuerflammenden Oceanen", "tokens": ["Fern", "hin\u00b7ter", "zehn", "tau\u00b7send", "feu\u00b7er\u00b7flam\u00b7men\u00b7den", "O\u00b7cea\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "CARD", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Hindurch die Gebirge!", "tokens": ["Hin\u00b7durch", "die", "Ge\u00b7bir\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hindurch die Flammenmeere!", "tokens": ["Hin\u00b7durch", "die", "Flam\u00b7men\u00b7mee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn Wonna liebt mich, liebt mich ewig,", "tokens": ["Denn", "Won\u00b7na", "liebt", "mich", ",", "liebt", "mich", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ewig, ewig!!", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "!!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": 