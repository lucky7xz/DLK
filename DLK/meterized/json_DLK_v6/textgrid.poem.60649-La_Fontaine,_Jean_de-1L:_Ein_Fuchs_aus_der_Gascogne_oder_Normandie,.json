{"textgrid.poem.60649": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fuchs aus der Gascogne oder Normandie,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fuchs aus der Gascogne oder Normandie,", "tokens": ["Ein", "Fuchs", "aus", "der", "Gas\u00b7cog\u00b7ne", "o\u00b7der", "Nor\u00b7man\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Verhungernd fast, hat Trauben am Spalier erschaut.", "tokens": ["Ver\u00b7hun\u00b7gernd", "fast", ",", "hat", "Trau\u00b7ben", "am", "Spa\u00b7lier", "er\u00b7schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "$,", "VAFIN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hingen hoch \u2013 doch ach, wie k\u00f6stlich lockten sie", "tokens": ["Sie", "hin\u00b7gen", "hoch", "\u2013", "doch", "ach", ",", "wie", "k\u00f6st\u00b7lich", "lock\u00b7ten", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "ADV", "ADV", "$,", "PWAV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit ihrer reifen zartbehauchten Haut!", "tokens": ["Mit", "ih\u00b7rer", "rei\u00b7fen", "zart\u00b7be\u00b7hauch\u00b7ten", "Haut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das w\u00e4r ein Mahl, wie\u2019s unserm Burschen wohl behagte.", "tokens": ["Das", "w\u00e4r", "ein", "Mahl", ",", "wie's", "un\u00b7serm", "Bur\u00b7schen", "wohl", "be\u00b7hag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch unerreichbar hing die s\u00fc\u00dfe Traubenglut.", "tokens": ["Doch", "un\u00b7er\u00b7reich\u00b7bar", "hing", "die", "s\u00fc\u00b7\u00dfe", "Trau\u00b7beng\u00b7lut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum rief er: \u00bbPfui, wie gr\u00fcn! Die sind f\u00fcr Lumpen gut!\u00ab", "tokens": ["Drum", "rief", "er", ":", "\u00bb", "Pfui", ",", "wie", "gr\u00fcn", "!", "Die", "sind", "f\u00fcr", "Lum\u00b7pen", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "PWAV", "ADJD", "$.", "PDS", "VAFIN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und war's nicht besser so, als da\u00df er sich beklagte?", "tokens": ["Und", "wa\u00b7r's", "nicht", "bes\u00b7ser", "so", ",", "als", "da\u00df", "er", "sich", "be\u00b7klag\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}