{"dta.poem.2954": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "153.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im sch\u00f6nsten Herbst, wo klar so Mond als Sonne war,", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Herbst", ",", "wo", "klar", "so", "Mond", "als", "Son\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV", "NN", "KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Klar \u00fcber Sonn' und Mond sah ich ein Sternenpaar", "tokens": ["Klar", "\u00fc\u00b7ber", "Sonn'", "und", "Mond", "sah", "ich", "ein", "Ster\u00b7nen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Bruderj\u00fcnglingen, die, wenn sie F\u00fcrstens\u00f6hne", "tokens": ["Von", "Bru\u00b7der\u00b7j\u00fcng\u00b7lin\u00b7gen", ",", "die", ",", "wenn", "sie", "F\u00fcrs\u00b7ten\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "NN"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht w\u00e4ren, edel doch ich nennt' an G\u00fct' und Sch\u00f6ne.", "tokens": ["Nicht", "w\u00e4\u00b7ren", ",", "e\u00b7del", "doch", "ich", "nennt'", "an", "G\u00fct'", "und", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "$,", "ADJD", "KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Vater preis' ich nicht um seinen F\u00fcrstenhut,", "tokens": ["Den", "Va\u00b7ter", "preis'", "ich", "nicht", "um", "sei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Vater preis' ich ihn der S\u00f6hne sch\u00f6n und gut.", "tokens": ["Als", "Va\u00b7ter", "preis'", "ich", "ihn", "der", "S\u00f6h\u00b7ne", "sch\u00f6n", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich will euch profezein, euch aber bitt' ich fein", "tokens": ["Ich", "will", "euch", "pro\u00b7fe\u00b7zein", ",", "euch", "a\u00b7ber", "bitt'", "ich", "fein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "$,", "PPER", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es so zu machen, da\u00df die Profezie treff' ein:", "tokens": ["Es", "so", "zu", "ma\u00b7chen", ",", "da\u00df", "die", "Pro\u00b7fe\u00b7zie", "treff'", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr werdet w\u00fcrdig seyn des Ranges, weil, entfernt", "tokens": ["Ihr", "wer\u00b7det", "w\u00fcr\u00b7dig", "seyn", "des", "Ran\u00b7ges", ",", "weil", ",", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "ART", "NN", "$,", "KOUS", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom F\u00fcrstlichen, ihr erst habt Menschliches gelernt.", "tokens": ["Vom", "F\u00fcrst\u00b7li\u00b7chen", ",", "ihr", "erst", "habt", "Menschli\u00b7ches", "ge\u00b7lernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}}}}