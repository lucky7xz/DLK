{"dta.poem.765": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "I.  Morgen Sonnet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die ewig helle schar wil nun ihr licht ver- ", "tokens": ["Die", "e\u00b7wig", "hel\u00b7le", "schar", "wil", "nun", "ihr", "licht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schlissen/", "tokens": ["schlis\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Diane steht erbla\u00dft; die Morgenr\u00f6tte ", "tokens": ["Di\u00b7a\u00b7ne", "steht", "er\u00b7bla\u00dft", ";", "die", "Mor\u00b7gen\u00b7r\u00f6t\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "VVPP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "lacht", "tokens": ["lacht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Den grawen Himmel an/ der sanffte", "tokens": ["Den", "gra\u00b7wen", "Him\u00b7mel", "an", "/", "der", "sanff\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wind erwacht/", "tokens": ["Wind", "er\u00b7wacht", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vnd reitzt das Federvolck/ den newen Tag", "tokens": ["Vnd", "reitzt", "das", "Fe\u00b7der\u00b7volck", "/", "den", "ne\u00b7wen", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "zu gr\u00fcssen.", "tokens": ["zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Das leben dieser welt/ eilt schon die welt zu k\u00fcssen/", "tokens": ["Das", "le\u00b7ben", "die\u00b7ser", "welt", "/", "eilt", "schon", "die", "welt", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$(", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd steckt sein Haupt empor/ man siht der Stral\u1ebd pracht", "tokens": ["Vnd", "steckt", "sein", "Haupt", "em\u00b7por", "/", "man", "siht", "der", "Stra\u00b7l\u1ebd", "pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "PIS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun blinckern auf der See: O dreymal h\u00f6chste Macht", "tokens": ["Nun", "blin\u00b7ckern", "auf", "der", "See", ":", "O", "drey\u00b7mal", "h\u00f6chs\u00b7te", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$.", "NE", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erleuchte den/ der sich jtzt beugt vor deinen F\u00fcssen.", "tokens": ["Er\u00b7leuch\u00b7te", "den", "/", "der", "sich", "jtzt", "beugt", "vor", "dei\u00b7nen", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$(", "PRELS", "PRF", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vertreib die dicke Nacht/ die meine Seel vmbgibt/", "tokens": ["Ver\u00b7treib", "die", "di\u00b7cke", "Nacht", "/", "die", "mei\u00b7ne", "Seel", "vmb\u00b7gibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Erquicke mein gem\u00fct/ vnd st\u00e4rcke mein vertrawen.", "tokens": ["Er\u00b7qui\u00b7cke", "mein", "ge\u00b7m\u00fct", "/", "vnd", "st\u00e4r\u00b7cke", "mein", "ver\u00b7tra\u00b7wen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVPP", "$(", "KON", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib/ da\u00df ich diesen Tag/ in deinem dinst allein", "tokens": ["Gib", "/", "da\u00df", "ich", "die\u00b7sen", "Tag", "/", "in", "dei\u00b7nem", "dinst", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "PDAT", "NN", "$(", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zubring; vnd wenn mein End\u2019 vnd jener Tag bricht ein", "tokens": ["Zu\u00b7bring", ";", "vnd", "wenn", "mein", "End'", "vnd", "je\u00b7ner", "Tag", "bricht", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "KOUS", "PPOSAT", "NN", "KON", "PDAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich dich meine Sonn/ meiu Lichtm\u00f6g ewig schawen.", "tokens": ["Da\u00df", "ich", "dich", "mei\u00b7ne", "Sonn", "/", "mei\u00b7u", "Licht\u00b7m\u00f6g", "e\u00b7wig", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}