{"textgrid.poem.61096": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Ungenannten", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du den Blick mir zugewandt voll G\u00fcte,", "tokens": ["Die", "du", "den", "Blick", "mir", "zu\u00b7ge\u00b7wandt", "voll", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PPER", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da mich die andern in den h\u00f6fisch glatten,", "tokens": ["Da", "mich", "die", "an\u00b7dern", "in", "den", "h\u00f6\u00b7fisch", "glat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Prunkvollen S\u00e4len stolz vergessen hatten,", "tokens": ["Prunk\u00b7vol\u00b7len", "S\u00e4\u00b7len", "stolz", "ver\u00b7ges\u00b7sen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dank' ich deinem freundlichen Gem\u00fcte!", "tokens": ["Wie", "dank'", "ich", "dei\u00b7nem", "freund\u00b7li\u00b7chen", "Ge\u00b7m\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du botest l\u00e4chelnd mir des Herzens Bl\u00fcte,", "tokens": ["Du", "bo\u00b7test", "l\u00e4\u00b7chelnd", "mir", "des", "Her\u00b7zens", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dfem Wort erquicktest du den Matten;", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Wort", "er\u00b7quick\u00b7test", "du", "den", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mag ein Quell in hoher Palmen Schatten", "tokens": ["So", "mag", "ein", "Quell", "in", "ho\u00b7her", "Pal\u00b7men", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Pilger laben, der von Durst entgl\u00fchte.", "tokens": ["Den", "Pil\u00b7ger", "la\u00b7ben", ",", "der", "von", "Durst", "ent\u00b7gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch! Nicht folgen darf ich jenem Gl\u00fccke,", "tokens": ["Und", "doch", "!", "Nicht", "fol\u00b7gen", "darf", "ich", "je\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PTKNEG", "VVINF", "VMFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das deine Gunst so reich mir zugewogen;", "tokens": ["Das", "dei\u00b7ne", "Gunst", "so", "reich", "mir", "zu\u00b7ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich h\u00e4lt das Herz, mich h\u00e4lt die Pflicht zur\u00fccke.", "tokens": ["Mich", "h\u00e4lt", "das", "Herz", ",", "mich", "h\u00e4lt", "die", "Pflicht", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn zwischen uns ist eine Kluft gezogen,", "tokens": ["Denn", "zwi\u00b7schen", "uns", "ist", "ei\u00b7ne", "Kluft", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich verbinden l\u00e4\u00dft durch keine Br\u00fccke,", "tokens": ["Die", "sich", "ver\u00b7bin\u00b7den", "l\u00e4\u00dft", "durch", "kei\u00b7ne", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die noch keiner gl\u00fccklich \u00fcberflogen.", "tokens": ["Und", "die", "noch", "kei\u00b7ner", "gl\u00fcck\u00b7lich", "\u00fc\u00b7berf\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}