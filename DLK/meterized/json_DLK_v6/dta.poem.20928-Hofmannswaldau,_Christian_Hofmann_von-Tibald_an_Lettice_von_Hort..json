{"dta.poem.20928": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tibald an Lettice von Hort.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt meine Todte noch? ist nichts an ihr ver-", "tokens": ["Lebt", "mei\u00b7ne", "Tod\u00b7te", "noch", "?", "ist", "nichts", "an", "ihr", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$.", "VAFIN", "PIS", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "blichen?", "tokens": ["bli\u00b7chen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Greift die Verwesung ihr nicht Brust und", "tokens": ["Greift", "die", "Ver\u00b7we\u00b7sung", "ihr", "nicht", "Brust", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "PTKNEG", "NN", "KON"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lippen an?", "tokens": ["Lip\u00b7pen", "an", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Du sturbest nur der Welt/ mir bistu nicht entwichen/", "tokens": ["Du", "stur\u00b7best", "nur", "der", "Welt", "/", "mir", "bis\u00b7tu", "nicht", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du weist wohl/ da\u00df dein F\u00fcrst dich nicht entbehren kan.", "tokens": ["Du", "weist", "wohl", "/", "da\u00df", "dein", "F\u00fcrst", "dich", "nicht", "ent\u00b7beh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich darf dir noch zur Zeit nicht eine Grabschrifft ma-", "tokens": ["Ich", "darf", "dir", "noch", "zur", "Zeit", "nicht", "ei\u00b7ne", "Grab\u00b7schrifft", "ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Ich denck itzt an den Sarg/ und an den Marmel nicht/", "tokens": ["Ich", "denck", "itzt", "an", "den", "Sarg", "/", "und", "an", "den", "Mar\u00b7mel", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$(", "KON", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du kanst mit frischem Muth itzt deines Grabes lachen/", "tokens": ["Du", "kanst", "mit", "fri\u00b7schem", "Muth", "itzt", "dei\u00b7nes", "Gra\u00b7bes", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und schaust noch/ dem du gleichst/ das sch\u00f6ne Tages-", "tokens": ["Und", "schaust", "noch", "/", "dem", "du", "gleichst", "/", "das", "sch\u00f6\u00b7ne", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$(", "PRELS", "PPER", "ADV", "$(", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Licht.", "tokens": ["Licht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Zu Flammen magstu wohl/ doch nicht zu Asche werden/", "tokens": ["Zu", "Flam\u00b7men", "mags\u00b7tu", "wohl", "/", "doch", "nicht", "zu", "A\u00b7sche", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "$(", "ADV", "PTKNEG", "APPR", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann jene zieren dich/ di\u00df ist zu fr\u00fch vor dich/", "tokens": ["Dann", "je\u00b7ne", "zie\u00b7ren", "dich", "/", "di\u00df", "ist", "zu", "fr\u00fch", "vor", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "$(", "PDS", "VAFIN", "PTKA", "ADJD", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Mund/ den sch\u00f6nen Arm/ die Anmuth der Ge-", "tokens": ["Den", "Mund", "/", "den", "sch\u00f6\u00b7nen", "Arm", "/", "die", "An\u00b7muth", "der", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.16": {"text": "behrden/", "tokens": ["behr\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Begehret nicht der Todt/ er l\u00e4\u00dft es noch vor mich.", "tokens": ["Be\u00b7ge\u00b7hret", "nicht", "der", "Todt", "/", "er", "l\u00e4\u00dft", "es", "noch", "vor", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du bist der werthe Zoll/ den mir die Sch\u00f6nheit giebet/", "tokens": ["Du", "bist", "der", "wert\u00b7he", "Zoll", "/", "den", "mir", "die", "Sch\u00f6n\u00b7heit", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wann durch mein Hertzogthum/ sie ihre Wahren", "tokens": ["Wann", "durch", "mein", "Hert\u00b7zog\u00b7thum", "/", "sie", "ih\u00b7re", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$(", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "f\u00fchrt/", "tokens": ["f\u00fchrt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Es scheint/ der Himmel selbst hat deinen Leib geliebet/", "tokens": ["Es", "scheint", "/", "der", "Him\u00b7mel", "selbst", "hat", "dei\u00b7nen", "Leib", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dieweil er ihn so reich mit seinen Gaben ziehrt.", "tokens": ["Die\u00b7weil", "er", "ihn", "so", "reich", "mit", "sei\u00b7nen", "Ga\u00b7ben", "ziehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kan nun des Himmels Hand sich deiner nicht enthal-", "tokens": ["Kan", "nun", "des", "Him\u00b7mels", "Hand", "sich", "dei\u00b7ner", "nicht", "ent\u00b7ha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "PRF", "PPOSAT", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "ten/", "tokens": ["ten", "/"], "token_info": ["word", "punct"], "pos": ["FM", "$("], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Wie solte denn der Mensch dir ungewogen seyn?", "tokens": ["Wie", "sol\u00b7te", "denn", "der", "Mensch", "dir", "un\u00b7ge\u00b7wo\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Liebe heist mich itzt des Himmels statt verwalten/", "tokens": ["Die", "Lie\u00b7be", "heist", "mich", "itzt", "des", "Him\u00b7mels", "statt", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich stelle mich bey dir mit meinem Hertzen ein.", "tokens": ["Ich", "stel\u00b7le", "mich", "bey", "dir", "mit", "mei\u00b7nem", "Hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie aber lebestu? Was kanstu sch\u00f6nes schauen?", "tokens": ["Wie", "a\u00b7ber", "le\u00b7be\u00b7stu", "?", "Was", "kans\u00b7tu", "sch\u00f6\u00b7nes", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$.", "PWS", "VMFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nichts als die Einsamkeit/ des Todes Ebenbild/", "tokens": ["Nichts", "als", "die", "Ein\u00b7sam\u00b7keit", "/", "des", "To\u00b7des", "E\u00b7ben\u00b7bild", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.30": {"text": "Du siehest ihre Hand ein Schlo\u00df der Schwermuth", "tokens": ["Du", "sie\u00b7hest", "ih\u00b7re", "Hand", "ein", "Schlo\u00df", "der", "Schwer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "bauen/", "tokens": ["bau\u00b7en", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Und bist wie mich bedeucht mit Schwermuth selbst er-", "tokens": ["Und", "bist", "wie", "mich", "be\u00b7deucht", "mit", "Schwer\u00b7muth", "selbst", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "PPER", "VVFIN", "APPR", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "f\u00fcllt;", "tokens": ["f\u00fcllt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Ich wolte dich also in diesen Schatten legen/", "tokens": ["Ich", "wol\u00b7te", "dich", "al\u00b7so", "in", "die\u00b7sen", "Schat\u00b7ten", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dieweil ich deinen Leib hab allzuwerth geacht/", "tokens": ["Die\u00b7weil", "ich", "dei\u00b7nen", "Leib", "hab", "all\u00b7zu\u00b7werth", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es wird ein Diamant von seiner Hoheit wegen/", "tokens": ["Es", "wird", "ein", "Di\u00b7a\u00b7mant", "von", "sei\u00b7ner", "Ho\u00b7heit", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mit Riegeln wohlverwahrt/ und unter Schl\u00f6sser", "tokens": ["Mit", "Rie\u00b7geln", "wohl\u00b7ver\u00b7wahrt", "/", "und", "un\u00b7ter", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$(", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "bracht.", "tokens": ["bracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Das Licht ist nicht vor dich/ du kennst den Lauf der", "tokens": ["Das", "Licht", "ist", "nicht", "vor", "dich", "/", "du", "kennst", "den", "Lauf", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "$(", "PPER", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Zeiten/", "tokens": ["Zei\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Des Hofes Auge sieht vor mich und dich zu scharf/", "tokens": ["Des", "Ho\u00b7fes", "Au\u00b7ge", "sieht", "vor", "mich", "und", "dich", "zu", "scharf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "KON", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Es wei\u00df der Baum der Gunst sich hier nicht recht zu", "tokens": ["Es", "wei\u00df", "der", "Baum", "der", "Gunst", "sich", "hier", "nicht", "recht", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "ADV", "PTKNEG", "ADJD", "PTKZU"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "breiten/", "tokens": ["brei\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["ADJA", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Ach das ein Hertzog nicht die Satzung brechen darf.", "tokens": ["Ach", "das", "ein", "Hert\u00b7zog", "nicht", "die", "Sat\u00b7zung", "bre\u00b7chen", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "ART", "NE", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wir sollen F\u00fcrsten seyn und dienen den Gesetzen/", "tokens": ["Wir", "sol\u00b7len", "F\u00fcrs\u00b7ten", "seyn", "und", "die\u00b7nen", "den", "Ge\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Man b\u00fccket sich vor uns/ und st\u00f6hrt doch unser Lust/", "tokens": ["Man", "b\u00fc\u00b7cket", "sich", "vor", "uns", "/", "und", "st\u00f6hrt", "doch", "un\u00b7ser", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPER", "$(", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wir k\u00f6nnen ohne Fleck uns nirgends recht ergetzen/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "oh\u00b7ne", "Fleck", "uns", "nir\u00b7gends", "recht", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und was man Freyheit heist/ das bleibt uns unbewust.", "tokens": ["Und", "was", "man", "Frey\u00b7heit", "heist", "/", "das", "bleibt", "uns", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "NN", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "