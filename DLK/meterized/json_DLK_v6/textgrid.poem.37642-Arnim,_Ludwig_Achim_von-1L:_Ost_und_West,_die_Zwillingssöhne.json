{"textgrid.poem.37642": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ost und West, die Zwillingss\u00f6hne", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ost und West, die Zwillingss\u00f6hne", "tokens": ["Ost", "und", "West", ",", "die", "Zwil\u00b7lings\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Buhlten um ein Jungfr\u00e4ulein,", "tokens": ["Buhl\u00b7ten", "um", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00c4hnlich klangen ihre T\u00f6ne", "tokens": ["\u00c4hn\u00b7lich", "klan\u00b7gen", "ih\u00b7re", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Sch\u00f6nen Fensterlein.", "tokens": ["Vor", "der", "Sch\u00f6\u00b7nen", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Luft hie\u00df ihre leichte Sch\u00f6ne,", "tokens": ["Luft", "hie\u00df", "ih\u00b7re", "leich\u00b7te", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Federn trug sie auf dem Haupt,", "tokens": ["Fe\u00b7dern", "trug", "sie", "auf", "dem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie ew'ge Myrthe kr\u00f6ne,", "tokens": ["Da\u00df", "sie", "ew'\u00b7ge", "Myr\u00b7the", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ihr Fenster myrthumlaubt.", "tokens": ["Ist", "ihr", "Fens\u00b7ter", "myrt\u00b7hum\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lange steht sie so im Glanze,", "tokens": ["Lan\u00b7ge", "steht", "sie", "so", "im", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr sind beide einerlei,", "tokens": ["Ihr", "sind", "bei\u00b7de", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie verwechselt beid' im Tanze,", "tokens": ["Sie", "ver\u00b7wech\u00b7selt", "beid'", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also \u00e4hnlich sind die zwei.", "tokens": ["Al\u00b7so", "\u00e4hn\u00b7lich", "sind", "die", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so weit wird es noch kommen,", "tokens": ["Und", "so", "weit", "wird", "es", "noch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie stiftet Bruderzwist;", "tokens": ["Da\u00df", "sie", "stif\u00b7tet", "Bru\u00b7der\u00b7zwist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihren Zweifeln zu entkommen,", "tokens": ["Ih\u00b7ren", "Zwei\u00b7feln", "zu", "ent\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denket sie auf eine List.", "tokens": ["Den\u00b7ket", "sie", "auf", "ei\u00b7ne", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Einen Mann, den mu\u00df ich haben,", "tokens": ["Ei\u00b7nen", "Mann", ",", "den", "mu\u00df", "ich", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denkt das arme Jungferlein,", "tokens": ["Denkt", "das", "ar\u00b7me", "Jung\u00b7fer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mir kann das Herz erlaben,", "tokens": ["Der", "mir", "kann", "das", "Herz", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ich bin nicht gern allein.", "tokens": ["Denn", "ich", "bin", "nicht", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zweifelnd denkt sie an die K\u00fcnste,", "tokens": ["Zwei\u00b7felnd", "denkt", "sie", "an", "die", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr Mutter ", "tokens": ["Die", "ihr", "Mut\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Macht am Freitag Weihrauchd\u00fcnste,", "tokens": ["Macht", "am", "Frei\u00b7tag", "Weih\u00b7rauch\u00b7d\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kocht den Zaubertrank am Heerd.", "tokens": ["Kocht", "den", "Zau\u00b7ber\u00b7trank", "am", "Heerd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Deckt dann vor dem Bett ein Tischlein,", "tokens": ["Deckt", "dann", "vor", "dem", "Bett", "ein", "Tisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt zwei blanke Teller drauf,", "tokens": ["Setzt", "zwei", "blan\u00b7ke", "Tel\u00b7ler", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zwei Gl\u00e4ser und zwei Fischlein,", "tokens": ["Und", "zwei", "Gl\u00e4\u00b7ser", "und", "zwei", "Fisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "KON", "CARD", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gleich als k\u00e4m' ein Gast in's Haus.", "tokens": ["Gleich", "als", "k\u00e4m'", "ein", "Gast", "in's", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbwer dann zu dir k\u00f6nnt von allen,", "tokens": ["\u00bb", "wer", "dann", "zu", "dir", "k\u00f6nnt", "von", "al\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "APPR", "PPER", "VVFIN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Mutter ihr gesagt,", "tokens": ["Hat", "die", "Mut\u00b7ter", "ihr", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der St\u00e4rkste im Gefallen,", "tokens": ["Ist", "der", "St\u00e4rks\u00b7te", "im", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der sei dir zugesagt.\u00ab", "tokens": ["Und", "der", "sei", "dir", "zu\u00b7ge\u00b7sagt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbder sei deiner Liebe Meister,", "tokens": ["\u00bb", "der", "sei", "dei\u00b7ner", "Lie\u00b7be", "Meis\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4chtig deiner Sch\u00f6nheit Kraft,", "tokens": ["M\u00e4ch\u00b7tig", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es wollen stets die Geister,", "tokens": ["Denn", "es", "wol\u00b7len", "stets", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df das M\u00e4chtigste sich schafft.\u00ab", "tokens": ["Da\u00df", "das", "M\u00e4ch\u00b7tigs\u00b7te", "sich", "schafft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRF", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Es ist Nacht, die beiden Lauten", "tokens": ["Es", "ist", "Nacht", ",", "die", "bei\u00b7den", "Lau\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingen vor dem Fensterlein,", "tokens": ["Klin\u00b7gen", "vor", "dem", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann schaut sie ihren Trauten;", "tokens": ["Und", "dann", "schaut", "sie", "ih\u00b7ren", "Trau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweigend tritt er zu ihr ein.", "tokens": ["Schwei\u00b7gend", "tritt", "er", "zu", "ihr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ob es Ost, ob's West gewesen,", "tokens": ["Ob", "es", "Ost", ",", "ob's", "West", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "KOUS", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denket sie vergebens nun.", "tokens": ["Den\u00b7ket", "sie", "ver\u00b7ge\u00b7bens", "nun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleicher waren nie zwei Wesen,", "tokens": ["Glei\u00b7cher", "wa\u00b7ren", "nie", "zwei", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Zweifel will nicht ruhn!", "tokens": ["Die\u00b7ser", "Zwei\u00b7fel", "will", "nicht", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Spricht er nicht, er kann doch sehen,", "tokens": ["Spricht", "er", "nicht", ",", "er", "kann", "doch", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie ihn zum Tische winkt,", "tokens": ["Wie", "sie", "ihn", "zum", "Ti\u00b7sche", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie f\u00fchlt des Athems Wehen,", "tokens": ["Und", "sie", "f\u00fchlt", "des", "A\u00b7thems", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er aus dem Becher trinkt.", "tokens": ["Wie", "er", "aus", "dem", "Be\u00b7cher", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie er's T\u00fcchlein wohlgefalten", "tokens": ["Wie", "er's", "T\u00fcch\u00b7lein", "wohl\u00b7ge\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt vom blanken Teller ab,", "tokens": ["Nimmt", "vom", "blan\u00b7ken", "Tel\u00b7ler", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft die Speisen doch erkalten,", "tokens": ["L\u00e4\u00dft", "die", "Spei\u00b7sen", "doch", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschm\u00e4het ihre Gab.", "tokens": ["Und", "ver\u00b7schm\u00e4\u00b7het", "ih\u00b7re", "Gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Dennoch mu\u00df sie nun empfangen,", "tokens": ["Den\u00b7noch", "mu\u00df", "sie", "nun", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh er sie in's Bette f\u00fchrt,", "tokens": ["Eh", "er", "sie", "in's", "Bet\u00b7te", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Gabe ohn' Verlangen,", "tokens": ["Ei\u00b7ne", "Ga\u00b7be", "ohn'", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die als Zeichen ihr geb\u00fchrt.", "tokens": ["Die", "als", "Zei\u00b7chen", "ihr", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Abgebrannt sind beide Lichter,", "tokens": ["Ab\u00b7ge\u00b7brannt", "sind", "bei\u00b7de", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Freund sitzt noch bei ihr,", "tokens": ["Und", "der", "Freund", "sitzt", "noch", "bei", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Macht so drohende Gesichter,", "tokens": ["Macht", "so", "dro\u00b7hen\u00b7de", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie flieht zur Kammerth\u00fcr.", "tokens": ["Da\u00df", "sie", "flieht", "zur", "Kam\u00b7mert\u00b7h\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Er das Messer aus dem G\u00fcrtel", "tokens": ["Er", "das", "Mes\u00b7ser", "aus", "dem", "G\u00fcr\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehet und ganz stille sitzt,", "tokens": ["Zie\u00b7het", "und", "ganz", "stil\u00b7le", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mond aus seinem Viertel", "tokens": ["Und", "der", "Mond", "aus", "sei\u00b7nem", "Vier\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauet, wie es herrlich blitzt.", "tokens": ["Schau\u00b7et", ",", "wie", "es", "herr\u00b7lich", "blitzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Nein, sie wagt es nicht zu nehmen,", "tokens": ["Nein", ",", "sie", "wagt", "es", "nicht", "zu", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es vorgeschrieben ist,", "tokens": ["Wie", "es", "vor\u00b7ge\u00b7schrie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei es Schrecken, sei es Sch\u00e4men,", "tokens": ["Sei", "es", "Schre\u00b7cken", ",", "sei", "es", "Sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verw\u00fcnschet jetzt die List.", "tokens": ["Sie", "ver\u00b7w\u00fcn\u00b7schet", "jetzt", "die", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Sie entschl\u00fcpfet in die Kammer.", "tokens": ["Sie", "ent\u00b7schl\u00fcp\u00b7fet", "in", "die", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er das Messer wirft nach ihr;", "tokens": ["Er", "das", "Mes\u00b7ser", "wirft", "nach", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als er flieht mit schwerem Jammer,", "tokens": ["Als", "er", "flieht", "mit", "schwe\u00b7rem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt das Messer in der Th\u00fcr.", "tokens": ["Steckt", "das", "Mes\u00b7ser", "in", "der", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Morgens kommen beide Br\u00fcder", "tokens": ["Mor\u00b7gens", "kom\u00b7men", "bei\u00b7de", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie zu gr\u00fc\u00dfen, doch dem West", "tokens": ["Sie", "zu", "gr\u00fc\u00b7\u00dfen", ",", "doch", "dem", "West"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fehlt das Messer, seine Lieder", "tokens": ["Fehlt", "das", "Mes\u00b7ser", ",", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagen ein gest\u00f6rtes Fest.", "tokens": ["Kla\u00b7gen", "ein", "ge\u00b7st\u00f6r\u00b7tes", "Fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Das ihm Traume ihn gequ\u00e4let", "tokens": ["Das", "ihm", "Trau\u00b7me", "ihn", "ge\u00b7qu\u00e4\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergangen ist zu nichts,", "tokens": ["Und", "ver\u00b7gan\u00b7gen", "ist", "zu", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PTKA", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich alles hat verfehlet", "tokens": ["Weil", "sich", "al\u00b7les", "hat", "ver\u00b7feh\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Schrecken des Gesichts.", "tokens": ["In", "dem", "Schre\u00b7cken", "des", "Ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Tr\u00f6stend giebt sie ihm die H\u00e4nde,", "tokens": ["Tr\u00f6s\u00b7tend", "giebt", "sie", "ihm", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcsset ihm die m\u00fcde Brust,", "tokens": ["K\u00fcs\u00b7set", "ihm", "die", "m\u00fc\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es drehen sich die W\u00e4nde", "tokens": ["Und", "es", "dre\u00b7hen", "sich", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald in hoher Hochzeitlust.", "tokens": ["Bald", "in", "ho\u00b7her", "Hoch\u00b7zeit\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Doch kein Kind will ihn erfreuen", "tokens": ["Doch", "kein", "Kind", "will", "ihn", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er w\u00fcnschet es so sehr,", "tokens": ["Und", "er", "w\u00fcn\u00b7schet", "es", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie sich mit Zaubereien", "tokens": ["Bis", "sie", "sich", "mit", "Zau\u00b7be\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzt in schrecklichen Verkehr.", "tokens": ["Setzt", "in", "schreck\u00b7li\u00b7chen", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "K\u00f6nnte sie's voraus nur wissen,", "tokens": ["K\u00f6nn\u00b7te", "sie's", "vo\u00b7raus", "nur", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Kindbett mu\u00df sie b\u00fc\u00dfen", "tokens": ["In", "dem", "Kind\u00b7bett", "mu\u00df", "sie", "b\u00fc\u00b7\