"line.3": {"text": "Und die Qual ihn neu umschwebet,", "tokens": ["Und", "die", "Qual", "ihn", "neu", "um\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihn jene Nacht durchgl\u00fcht.", "tokens": ["Die", "ihn", "je\u00b7ne", "Nacht", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "\u00bbalso du bist es gewesen,", "tokens": ["\u00bb", "al\u00b7so", "du", "bist", "es", "ge\u00b7we\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VAFIN", "PPER", "VAPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die mich jene Nacht geplagt,", "tokens": ["Die", "mich", "je\u00b7ne", "Nacht", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich nie vom Schreck genesen,", "tokens": ["Da\u00df", "ich", "nie", "vom", "Schreck", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, das sei hier Gott geklagt.\u00ab", "tokens": ["O", ",", "das", "sei", "hier", "Gott", "ge\u00b7klagt", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "ADV", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "\u00bbhast du mich voraus gequ\u00e4let,", "tokens": ["\u00bb", "hast", "du", "mich", "vo\u00b7raus", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lang im schweren Liebesdienst,", "tokens": ["Lang", "im", "schwe\u00b7ren", "Lie\u00b7bes\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Straf ich dich, nun wir verm\u00e4hlet,", "tokens": ["Straf", "ich", "dich", ",", "nun", "wir", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich zahl' wie du's verdienst.\u00ab", "tokens": ["Und", "ich", "zahl'", "wie", "du's", "ver\u00b7dienst", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADJD", "KOKOM", "PIS", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "\u00bbhab' ich auch nicht wollen speisen", "tokens": ["\u00bb", "hab'", "ich", "auch", "nicht", "wol\u00b7len", "spei\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Fische Zauberei,", "tokens": ["Von", "der", "Fi\u00b7sche", "Zau\u00b7be\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist geh\u00e4rtet doch dies Eisen,", "tokens": ["Ist", "ge\u00b7h\u00e4r