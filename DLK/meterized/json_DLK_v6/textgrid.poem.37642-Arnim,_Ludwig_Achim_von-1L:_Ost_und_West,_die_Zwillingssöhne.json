, "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber nicht der harte Zorn,", "tokens": ["A\u00b7ber", "nicht", "der", "har\u00b7te", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als des Herzens Ri\u00df verwunden,", "tokens": ["Als", "des", "Her\u00b7zens", "Ri\u00df", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmerzt im Fleische ihm der Dorn.", "tokens": ["Schmerzt", "im", "Flei\u00b7sche", "ihm", "der", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.34": {"line.1": {"text": "Wenn sie weint bei seinen Schl\u00e4gen,", "tokens": ["Wenn", "sie", "weint", "bei", "sei\u00b7nen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt er ihr das Messer nur,", "tokens": ["Zeigt", "er", "ihr", "das", "Mes\u00b7ser", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht dann: Ohne Lieb' kein Segen,", "tokens": ["Spricht", "dann", ":", "Oh\u00b7ne", "Lieb'", "kein", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure":