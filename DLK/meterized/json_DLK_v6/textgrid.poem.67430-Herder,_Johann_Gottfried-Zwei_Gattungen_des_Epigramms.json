{"textgrid.poem.67430": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Gattungen des Epigramms", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir ist das Epigramm die kleine gesch\u00e4ftige Biene,", "tokens": ["Dir", "ist", "das", "E\u00b7pig\u00b7ramm", "die", "klei\u00b7ne", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die auf Blumen umher flieget und sauset und sticht;", "tokens": ["Die", "auf", "Blu\u00b7men", "um\u00b7her", "flie\u00b7get", "und", "sau\u00b7set", "und", "sticht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PTKVZ", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mir ist das Epigramm die kleine knospende Rose,", "tokens": ["Mir", "ist", "das", "E\u00b7pig\u00b7ramm", "die", "klei\u00b7ne", "knos\u00b7pen\u00b7de", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die aus Dornengeb\u00fcsch Nektarerfrischungen haucht.", "tokens": ["Die", "aus", "Dor\u00b7nen\u00b7ge\u00b7b\u00fcsch", "Nek\u00b7ta\u00b7rer\u00b7fri\u00b7schun\u00b7gen", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "La\u00df uns Beide sie denn in ", "tokens": ["La\u00df", "uns", "Bei\u00b7de", "sie", "denn", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "PPER", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier sind Blumen, o Freund; sende die Bienen dazu!", "tokens": ["Hier", "sind", "Blu\u00b7men", ",", "o", "Freund", ";", "sen\u00b7de", "die", "Bie\u00b7nen", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "FM", "NN", "$.", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}