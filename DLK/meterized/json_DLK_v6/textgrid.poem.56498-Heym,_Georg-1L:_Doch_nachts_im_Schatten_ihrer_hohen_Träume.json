{"textgrid.poem.56498": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch nachts im Schatten ihrer hohen Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch nachts im Schatten ihrer hohen Tr\u00e4ume", "tokens": ["Doch", "nachts", "im", "Schat\u00b7ten", "ih\u00b7rer", "ho\u00b7hen", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie unter gro\u00dfer Eichen k\u00fchlem Dach", "tokens": ["Wie", "un\u00b7ter", "gro\u00b7\u00dfer", "Ei\u00b7chen", "k\u00fch\u00b7lem", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klingt um sie laut das Dunkel hundertfach", "tokens": ["Klingt", "um", "sie", "laut", "das", "Dun\u00b7kel", "hun\u00b7dert\u00b7fach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Sterne fahren singend durch die R\u00e4ume", "tokens": ["Und", "Ster\u00b7ne", "fah\u00b7ren", "sin\u00b7gend", "durch", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Hauche Gottes durch das All getrieben", "tokens": ["Vom", "Hau\u00b7che", "Got\u00b7tes", "durch", "das", "All", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit goldnen Federn in die Nacht gespreizt,", "tokens": ["Mit", "gold\u00b7nen", "Fe\u00b7dern", "in", "die", "Nacht", "ge\u00b7spreizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kometen, die mit tr\u00fcbem Schrei zerstieben,", "tokens": ["Ko\u00b7me\u00b7ten", ",", "die", "mit", "tr\u00fc\u00b7bem", "Schrei", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der traurig ihre schlaffen Ohren beizt.", "tokens": ["Der", "trau\u00b7rig", "ih\u00b7re", "schlaf\u00b7fen", "Oh\u00b7ren", "beizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie horchen auf des Waldes Ruhe unten", "tokens": ["Sie", "hor\u00b7chen", "auf", "des", "Wal\u00b7des", "Ru\u00b7he", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie in den Wurzeln blau der Schlummer schwillt", "tokens": ["Wie", "in", "den", "Wur\u00b7zeln", "blau", "der", "Schlum\u00b7mer", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und auf der Erde schweres Atmen drunten,", "tokens": ["Und", "auf", "der", "Er\u00b7de", "schwe\u00b7res", "At\u00b7men", "drun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das langsam ihre gro\u00dfen H\u00f6hlen f\u00fcllt.", "tokens": ["Das", "lang\u00b7sam", "ih\u00b7re", "gro\u00b7\u00dfen", "H\u00f6h\u00b7len", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wieder klingt's in ihren Frieden leise,", "tokens": ["Und", "wie\u00b7der", "klingt's", "in", "ih\u00b7ren", "Frie\u00b7den", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn das verborgne Silber wachsend schw\u00e4rt,", "tokens": ["Wenn", "das", "ver\u00b7borg\u00b7ne", "Sil\u00b7ber", "wach\u00b7send", "schw\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und das Ger\u00e4usch der Sonne auf der Reise,", "tokens": ["Und", "das", "Ge\u00b7r\u00e4usch", "der", "Son\u00b7ne", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unten \u00fcber weite Meere f\u00e4hrt.", "tokens": ["Die", "un\u00b7ten", "\u00fc\u00b7ber", "wei\u00b7te", "Mee\u00b7re", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auf einmal h\u00f6ren sie die St\u00fcrme wehen", "tokens": ["Auf", "ein\u00b7mal", "h\u00f6\u00b7ren", "sie", "die", "St\u00fcr\u00b7me", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und laute Glocke l\u00e4uten durch die Nacht.", "tokens": ["Und", "lau\u00b7te", "Glo\u00b7cke", "l\u00e4u\u00b7ten", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie m\u00f6chten gern dem Schall entgegengehen,", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "gern", "dem", "Schall", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erh\u00f6rt, entfesselt, in das Licht gebracht.", "tokens": ["Er\u00b7h\u00f6rt", ",", "ent\u00b7fes\u00b7selt", ",", "in", "das", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch pl\u00f6tzlich bricht es ab. Und nur ein Zittern", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "bricht", "es", "ab", ".", "Und", "nur", "ein", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist rund im Raum, das sie im Ohre nagt,", "tokens": ["Ist", "rund", "im", "Raum", ",", "das", "sie", "im", "Oh\u00b7re", "nagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie tief in seinem Sarge im Verwittern", "tokens": ["Wie", "tief", "in", "sei\u00b7nem", "Sar\u00b7ge", "im", "Ver\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Toter weint und seine Trauer klagt.", "tokens": ["Ein", "To\u00b7ter", "weint", "und", "sei\u00b7ne", "Trau\u00b7er", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein L\u00e4cheln kraut sie dann, da\u00df sie noch leben,", "tokens": ["Ein", "L\u00e4\u00b7cheln", "kraut", "sie", "dann", ",", "da\u00df", "sie", "noch", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schlummers Sabber h\u00e4ngt sich an ihr Kinn", "tokens": ["Des", "Schlum\u00b7mers", "Sab\u00b7ber", "h\u00e4ngt", "sich", "an", "ihr", "Kinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jemand kommt mit Fingern leicht, die schweben", "tokens": ["Und", "je\u00b7mand", "kommt", "mit", "Fin\u00b7gern", "leicht", ",", "die", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "ADJD", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ihrem Rettichkopf wie Fliegen hin.", "tokens": ["Auf", "ih\u00b7rem", "Ret\u00b7tich\u00b7kopf", "wie", "Flie\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}