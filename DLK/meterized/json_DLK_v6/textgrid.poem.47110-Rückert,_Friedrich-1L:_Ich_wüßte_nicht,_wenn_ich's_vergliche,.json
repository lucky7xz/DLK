{"textgrid.poem.47110": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich w\u00fc\u00dfte nicht, wenn ich's vergliche,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fc\u00dfte nicht, wenn ich's vergliche,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "nicht", ",", "wenn", "ich's", "ver\u00b7gli\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob meins, ob deins ein gr\u00f6\u00dfres Reich?", "tokens": ["Ob", "meins", ",", "ob", "deins", "ein", "gr\u00f6\u00df\u00b7res", "Reich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sind des Sanges Himmelsstriche", "tokens": ["Es", "sind", "des", "San\u00b7ges", "Him\u00b7mels\u00b7stri\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl dem Gebiet der Anmut gleich.", "tokens": ["Wohl", "dem", "Ge\u00b7biet", "der", "An\u00b7mut", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Paradiese, die uns gl\u00e4nzen,", "tokens": ["Zwei", "Pa\u00b7ra\u00b7die\u00b7se", ",", "die", "uns", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das deine mein und meines deins,", "tokens": ["Das", "dei\u00b7ne", "mein", "und", "mei\u00b7nes", "deins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "PPOSAT", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die gegenseitig sich begrenzen,", "tokens": ["Die", "ge\u00b7gen\u00b7sei\u00b7tig", "sich", "be\u00b7gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und beide sind zusammen eins.", "tokens": ["Und", "bei\u00b7de", "sind", "zu\u00b7sam\u00b7men", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo deiner Liebe Zauber endet,", "tokens": ["Wo", "dei\u00b7ner", "Lie\u00b7be", "Zau\u00b7ber", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt meines Liedes Glanzwelt an;", "tokens": ["Hebt", "mei\u00b7nes", "Lie\u00b7des", "Glanz\u00b7welt", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo die Seele hin sich wendet", "tokens": ["Und", "wo", "die", "See\u00b7le", "hin", "sich", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihr ein Himmel aufgethan.", "tokens": ["Ist", "ihr", "ein", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}