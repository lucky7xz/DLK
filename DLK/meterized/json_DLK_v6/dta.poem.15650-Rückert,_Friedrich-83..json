{"dta.poem.15650": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00e4tt' uns k\u00f6nnen Gott f\u00fcr Rechnungen ersparen", "tokens": ["Was", "h\u00e4tt'", "uns", "k\u00f6n\u00b7nen", "Gott", "f\u00fcr", "Rech\u00b7nun\u00b7gen", "er\u00b7spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "VMFIN", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ungleichen Uebergriffs von Sonn- und Mondenjahren;", "tokens": ["Un\u00b7glei\u00b7chen", "Ue\u00b7berg\u00b7riffs", "von", "Sonn", "und", "Mon\u00b7den\u00b7jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' er geordnet so f\u00fcr uns des Himmels Lauf,", "tokens": ["H\u00e4tt'", "er", "ge\u00b7ord\u00b7net", "so", "f\u00fcr", "uns", "des", "Him\u00b7mels", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ohne Br\u00fcche Jahr, Monat und Tag gieng auf.", "tokens": ["Da\u00df", "oh\u00b7ne", "Br\u00fc\u00b7che", "Jahr", ",", "Mo\u00b7nat", "und", "Tag", "gieng", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er wollt' es nicht, warum? Es steht in seinem Buch,", "tokens": ["Er", "wollt'", "es", "nicht", ",", "wa\u00b7rum", "?", "Es", "steht", "in", "sei\u00b7nem", "Buch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er die Ganzheit ist, und unsre Welt ein Bruch.", "tokens": ["Da\u00df", "er", "die", "Ganz\u00b7heit", "ist", ",", "und", "uns\u00b7re", "Welt", "ein", "Bruch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}