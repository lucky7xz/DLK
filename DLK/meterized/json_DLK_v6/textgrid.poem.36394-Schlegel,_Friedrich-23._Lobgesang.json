{"textgrid.poem.36394": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23. Lobgesang", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wacht auf, ihr s\u00fc\u00dfen V\u00f6gelein,", "tokens": ["Wacht", "auf", ",", "ihr", "s\u00fc\u00b7\u00dfen", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Nachtigallen kleine,", "tokens": ["Ihr", "Nach\u00b7ti\u00b7gal\u00b7len", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr auf gr\u00fcnen Zweigelein,", "tokens": ["Die", "ihr", "auf", "gr\u00fc\u00b7nen", "Zwei\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim ersten Morgenscheine", "tokens": ["Beim", "ers\u00b7ten", "Mor\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu singen r\u00fcst' eu'r Schn\u00e4belein,", "tokens": ["Zu", "sin\u00b7gen", "r\u00fcst'", "eu'r", "Schn\u00e4\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gedreht aus Elfenbeine.", "tokens": ["Ge\u00b7dreht", "aus", "El\u00b7fen\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun stimmet eure Pfeifelein", "tokens": ["Nun", "stim\u00b7met", "eu\u00b7re", "Pfei\u00b7fe\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieblich und so reine,", "tokens": ["So", "lieb\u00b7lich", "und", "so", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun singt ein s\u00fc\u00dfes Liedelein,", "tokens": ["Nun", "singt", "ein", "s\u00fc\u00b7\u00dfes", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stimmt fr\u00f6hlich \u00fcbereine;", "tokens": ["Stimmt", "fr\u00f6h\u00b7lich", "\u00fc\u00b7be\u00b7rei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott will von euch gelobet sein", "tokens": ["Gott", "will", "von", "euch", "ge\u00b7lo\u00b7bet", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von allen in Gemeine.", "tokens": ["Von", "al\u00b7len", "in", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr seid die s\u00fc\u00dfen S\u00e4ngerlein,", "tokens": ["Ihr", "seid", "die", "s\u00fc\u00b7\u00dfen", "S\u00e4n\u00b7ger\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ihn bestellt alleine;", "tokens": ["F\u00fcr", "ihn", "be\u00b7stellt", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Singt, singt ihr s\u00fc\u00dfen Schw\u00e4tzerlein,", "tokens": ["Singt", ",", "singt", "ihr", "s\u00fc\u00b7\u00dfen", "Schw\u00e4t\u00b7zer\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Nachtigallen kleine,", "tokens": ["Ihr", "Nach\u00b7ti\u00b7gal\u00b7len", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft schallen eure Stimmelein", "tokens": ["La\u00dft", "schal\u00b7len", "eu\u00b7re", "Stim\u00b7mel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum gr\u00fcnen Wald hereine.", "tokens": ["Zum", "gr\u00fc\u00b7nen", "Wald", "her\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sind viel klare Br\u00fcnnelein", "tokens": ["Da", "sind", "viel", "kla\u00b7re", "Br\u00fcn\u00b7nel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefa\u00dft in Marmorsteine;", "tokens": ["Ge\u00b7fa\u00dft", "in", "Mar\u00b7mor\u00b7stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sp\u00fclet eure Z\u00fcngelein", "tokens": ["Da", "sp\u00fc\u00b7let", "eu\u00b7re", "Z\u00fcn\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich, und ein vor eine;", "tokens": ["Zu\u00b7gleich", ",", "und", "ein", "vor", "ei\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ART", "APPR", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sp\u00fclt ihr so eure Z\u00fcngelein,", "tokens": ["Sp\u00fclt", "ihr", "so", "eu\u00b7re", "Z\u00fcn\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So singt ihr noch so reine.", "tokens": ["So", "singt", "ihr", "noch", "so", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Habt ihr kein eignes Liedelein,", "tokens": ["Habt", "ihr", "kein", "eig\u00b7nes", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lernet nur das meine;", "tokens": ["So", "ler\u00b7net", "nur", "das", "mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Euch gn\u00fcgt mit diesem Liedelein,", "tokens": ["Euch", "gn\u00fcgt", "mit", "die\u00b7sem", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bed\u00fcrft der andern keine.", "tokens": ["Be\u00b7d\u00fcrft", "der", "an\u00b7dern", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Singt nur allein, gelobt sei Gott,", "tokens": ["Singt", "nur", "al\u00b7lein", ",", "ge\u00b7lobt", "sei", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott Zebaoth alleine.", "tokens": ["Gott", "Ze\u00b7bao\u00b7th", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zu tausend mal gelobt sei Gott,", "tokens": ["Zu", "tau\u00b7send", "mal", "ge\u00b7lobt", "sei", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott Zebaoth alleine,", "tokens": ["Gott", "Ze\u00b7bao\u00b7th", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wiederum wohl ohne Zahl", "tokens": ["Und", "wie\u00b7de\u00b7rum", "wohl", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelobt sei Gott alleine,", "tokens": ["Ge\u00b7lobt", "sei", "Gott", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und immerfort wohl tausendmal", "tokens": ["Und", "im\u00b7mer\u00b7fort", "wohl", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott Zebaoth alleine.", "tokens": ["Gott", "Ze\u00b7bao\u00b7th", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Singt nur dies einz'ge Liedelein,", "tokens": ["Singt", "nur", "dies", "einz'\u00b7ge", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Liedlein das ich meine;", "tokens": ["Das", "Lied\u00b7lein", "das", "ich", "mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Singt es ihr frommen V\u00f6gelein,", "tokens": ["Singt", "es", "ihr", "from\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ich f\u00fcr Freuden weine.", "tokens": ["Dann", "ich", "f\u00fcr", "Freu\u00b7den", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz verwund't dies Liedelein,", "tokens": ["Mein", "Herz", "ver\u00b7wund't", "dies", "Lie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Liedlein das ich meine.", "tokens": ["Das", "Lied\u00b7lein", "das", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So flieget durch die W\u00e4lderlein,", "tokens": ["So", "flie\u00b7get", "durch", "die", "W\u00e4l\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt Tag und Nacht bei eine;", "tokens": ["Bleibt", "Tag", "und", "Nacht", "bei", "ei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Singt immer nur das Liedelein", "tokens": ["Singt", "im\u00b7mer", "nur", "das", "Lie\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Sonn- und Mondenscheine:", "tokens": ["Bei", "Sonn", "und", "Mon\u00b7den\u00b7schei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gelobt sei Gott, Gott Zebaoth,", "tokens": ["Ge\u00b7lobt", "sei", "Gott", ",", "Gott", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gelobt sei Gott alleine.", "tokens": ["Ge\u00b7lobt", "sei", "Gott", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sonn', Mond und kleine Sternelein", "tokens": ["Sonn'", ",", "Mond", "und", "klei\u00b7ne", "Ster\u00b7ne\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich festiglich vermeine,", "tokens": ["Ich", "fes\u00b7tig\u00b7lich", "ver\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit samt der Erde Pfl\u00e4nzelein,", "tokens": ["Mit", "samt", "der", "Er\u00b7de", "Pfl\u00e4n\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Laub, Blumen, Berg' und Haine", "tokens": ["Laub", ",", "Blu\u00b7men", ",", "Ber\u00b7g'", "und", "Hai\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Beginnen ein sch\u00f6n's T\u00e4nzelein", "tokens": ["Be\u00b7gin\u00b7nen", "ein", "sch\u00f6n's", "T\u00e4n\u00b7zel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im fr\u00f6hlichen Vereine.", "tokens": ["Im", "fr\u00f6h\u00b7li\u00b7chen", "Ver\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Freud' bringen wird's den Engelein,", "tokens": ["Freud'", "brin\u00b7gen", "wird's", "den", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den B\u00f6sen bringt es Peine;", "tokens": ["Den", "B\u00f6\u00b7sen", "bringt", "es", "Pei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum singt ihr lieben V\u00f6gelein,", "tokens": ["Drum", "singt", "ihr", "lie\u00b7ben", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Nachtigallen kleine.", "tokens": ["Ihr", "Nach\u00b7ti\u00b7gal\u00b7len", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Also will Gott gelobet sein,", "tokens": ["Al\u00b7so", "will", "Gott", "ge\u00b7lo\u00b7bet", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott Zebaoth alleine.", "tokens": ["Gott", "Ze\u00b7bao\u00b7th", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}