{"textgrid.poem.55918": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, wie sich alles auftut: so sind wir;", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie sich alles auftut: so sind wir;", "tokens": ["Sieh", ",", "wie", "sich", "al\u00b7les", "auf\u00b7tut", ":", "so", "sind", "wir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PRF", "PIS", "VVPP", "$.", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "denn wir sind nichts als solche Seligkeit.", "tokens": ["denn", "wir", "sind", "nichts", "als", "sol\u00b7che", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was Blut und Dunkel war in einem Tier,", "tokens": ["Was", "Blut", "und", "Dun\u00b7kel", "war", "in", "ei\u00b7nem", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das wuchs in uns zur Seele an und schreit", "tokens": ["das", "wuchs", "in", "uns", "zur", "See\u00b7le", "an", "und", "schreit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "als Seele weiter. Und es schreit nach dir.", "tokens": ["als", "See\u00b7le", "wei\u00b7ter", ".", "Und", "es", "schreit", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du freilich nimmst es nur in dein Gesicht", "tokens": ["Du", "frei\u00b7lich", "nimmst", "es", "nur", "in", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als sei es Landschaft: sanft und ohne Gier.", "tokens": ["als", "sei", "es", "Land\u00b7schaft", ":", "sanft", "und", "oh\u00b7ne", "Gier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "NN", "$.", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und darum meinen wir, du bist es nicht,", "tokens": ["Und", "da\u00b7rum", "mei\u00b7nen", "wir", ",", "du", "bist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "nach dem es schreit. Und doch, bist du nicht der,", "tokens": ["nach", "dem", "es", "schreit", ".", "Und", "doch", ",", "bist", "du", "nicht", "der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "KON", "ADV", "$,", "VAFIN", "PPER", "PTKNEG", "ART", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "an den wir uns ganz ohne Rest verl\u00f6ren?", "tokens": ["an", "den", "wir", "uns", "ganz", "oh\u00b7ne", "Rest", "ver\u00b7l\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und werden wir in irgend einem ", "tokens": ["Und", "wer\u00b7den", "wir", "in", "ir\u00b7gend", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit uns geht das Unendliche ", "tokens": ["Mit", "uns", "geht", "das", "Un\u00b7end\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Du aber sei, du Mund, da\u00df wir es h\u00f6ren,", "tokens": ["Du", "a\u00b7ber", "sei", ",", "du", "Mund", ",", "da\u00df", "wir", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PPER", "NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "du aber, du Uns-Sagender: du sei.", "tokens": ["du", "a\u00b7ber", ",", "du", "Un\u00b7sSa\u00b7gen\u00b7der", ":", "du", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "NN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}