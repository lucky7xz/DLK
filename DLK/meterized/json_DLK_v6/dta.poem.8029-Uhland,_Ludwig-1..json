{"dta.poem.8029": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ebester Ritter von Kastilien!", "tokens": ["\u201e", "bes\u00b7ter", "Rit\u00b7ter", "von", "Kas\u00b7ti\u00b7li\u00b7en", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "NE", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wann die fernen Berge tosen,", "tokens": ["Wann", "die", "fer\u00b7nen", "Ber\u00b7ge", "to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein\u2019 ich, deinen Kampf zu h\u00f6ren:", "tokens": ["Mein'", "ich", ",", "dei\u00b7nen", "Kampf", "zu", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es ist des Donners Rollen.", "tokens": ["Doch", "es", "ist", "des", "Don\u00b7ners", "Rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann es hinter jenen H\u00f6hen", "tokens": ["Wann", "es", "hin\u00b7ter", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Roth und golden gl\u00fcht am Morgen,", "tokens": ["Roth", "und", "gol\u00b7den", "gl\u00fcht", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein\u2019 ich, da\u00df du wollst erscheinen:", "tokens": ["Mein'", "ich", ",", "da\u00df", "du", "wollst", "er\u00b7schei\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch es kommt herauf die Sonne.\u201c", "tokens": ["Doch", "es", "kommt", "her\u00b7auf", "die", "Son\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}