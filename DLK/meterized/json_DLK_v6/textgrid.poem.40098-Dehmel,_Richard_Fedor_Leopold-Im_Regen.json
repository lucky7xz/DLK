{"textgrid.poem.40098": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Im Regen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In langen Tropfen rinnt es um mich nieder,", "tokens": ["In", "lan\u00b7gen", "Trop\u00b7fen", "rinnt", "es", "um", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie schlagen prasselnd durch die schlaffen Bl\u00e4tter,", "tokens": ["sie", "schla\u00b7gen", "pras\u00b7selnd", "durch", "die", "schlaf\u00b7fen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die V\u00f6gel str\u00e4uben triefend das Gefieder:", "tokens": ["die", "V\u00f6\u00b7gel", "str\u00e4u\u00b7ben", "trie\u00b7fend", "das", "Ge\u00b7fie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es stimmt zu mir! es ist ein artig Wetter!", "tokens": ["es", "stimmt", "zu", "mir", "!", "es", "ist", "ein", "ar\u00b7tig", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tr\u00fcbsel'ger rauscht es in den L\u00fcften immer,", "tokens": ["Tr\u00fcb\u00b7sel'\u00b7ger", "rauscht", "es", "in", "den", "L\u00fcf\u00b7ten", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.2": {"text": "der Himmel br\u00fctend scheint zu \u00fcberlegen", "tokens": ["der", "Him\u00b7mel", "br\u00fc\u00b7tend", "scheint", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das Loos der Erde \u2013 nirgend st\u00f6rt ein Schimmer:", "tokens": ["das", "Loos", "der", "Er\u00b7de", "\u2013", "nir\u00b7gend", "st\u00f6rt", "ein", "Schim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "versunken Laub und Licht, \u2013 nur Regen, Regen.", "tokens": ["ver\u00b7sun\u00b7ken", "Laub", "und", "Licht", ",", "\u2013", "nur", "Re\u00b7gen", ",", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "$(", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Welt f\u00fchlt gr\u00e4mlich ihres Alters Schwere:", "tokens": ["Die", "Welt", "f\u00fchlt", "gr\u00e4m\u00b7lich", "ih\u00b7res", "Al\u00b7ters", "Schwe\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kein Schein von Freude rings, kein Hauch von Trauer.", "tokens": ["kein", "Schein", "von", "Freu\u00b7de", "rings", ",", "kein", "Hauch", "von", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "ADV", "$,", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ziellos starr' ich, \u2013 schreit' ich, \u2013 fort, \u2013 ins Leere:", "tokens": ["Und", "ziel\u00b7los", "starr'", "ich", ",", "\u2013", "schreit'", "ich", ",", "\u2013", "fort", ",", "\u2013", "ins", "Lee\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PTKVZ", "$,", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in mir und um mich grau \u2013 \u2013 und immer grauer.", "tokens": ["in", "mir", "und", "um", "mich", "grau", "\u2013", "\u2013", "und", "im\u00b7mer", "grau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "PPER", "ADJD", "$(", "$(", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}