{"textgrid.poem.47406": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als einst von deiner Sch\u00f6ne]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst von deiner Sch\u00f6ne,", "tokens": ["Als", "einst", "von", "dei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O meine s\u00fc\u00dfe Wonne,", "tokens": ["O", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Strahl entz\u00fcckter Ahnung", "tokens": ["Ein", "Strahl", "ent\u00b7z\u00fcck\u00b7ter", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch alle Himmel hin,", "tokens": ["Durch", "al\u00b7le", "Him\u00b7mel", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch die ", "tokens": ["Durch", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Sich breitete \u2013 geboren", "tokens": ["Sich", "brei\u00b7te\u00b7te", "\u2013", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["PRF", "VVFIN", "$(", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ward eine neue Gottheit;", "tokens": ["Ward", "ei\u00b7ne", "neu\u00b7e", "Got\u00b7theit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Die Liebe war's, der Herzen", "tokens": ["Die", "Lie\u00b7be", "wa\u00b7r's", ",", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gewaltige K\u00f6nigin.", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber den Himmel schwang sie", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Him\u00b7mel", "schwang", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den flammenspr\u00fch'nden Zepter", "tokens": ["Den", "flam\u00b7men\u00b7spr\u00fch'n\u00b7den", "Zep\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrer stolzen Hand;", "tokens": ["Mit", "ih\u00b7rer", "stol\u00b7zen", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allein die Engel standen", "tokens": ["Al\u00b7lein", "die", "En\u00b7gel", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Inmitten ihrer Feuer", "tokens": ["In\u00b7mit\u00b7ten", "ih\u00b7rer", "Feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eiskalt und unentbrannt.", "tokens": ["Eis\u00b7kalt", "und", "un\u00b7ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da fa\u00dfte Zorn die G\u00f6ttin;", "tokens": ["Da", "fa\u00df\u00b7te", "Zorn", "die", "G\u00f6t\u00b7tin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie flog zur Erde nieder,", "tokens": ["Sie", "flog", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu f\u00fchlender Menschen Herzen", "tokens": ["Zu", "f\u00fch\u00b7len\u00b7der", "Men\u00b7schen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die Fittige gewandt.", "tokens": ["Die", "Fit\u00b7ti\u00b7ge", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Seit jenem Tage spr\u00fchen,", "tokens": ["Seit", "je\u00b7nem", "Ta\u00b7ge", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Seit jenem Tage gl\u00fchen", "tokens": ["Seit", "je\u00b7nem", "Ta\u00b7ge", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Flammen ihres Zepters", "tokens": ["Die", "Flam\u00b7men", "ih\u00b7res", "Zep\u00b7ters"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Durch alles ird'sche Land.", "tokens": ["Durch", "al\u00b7les", "ird'\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}