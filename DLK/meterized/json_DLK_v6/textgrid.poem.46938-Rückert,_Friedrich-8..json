{"textgrid.poem.46938": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich m\u00f6cht' ich sehn, der du in dumpfem Zorne", "tokens": ["Dich", "m\u00f6cht'", "ich", "sehn", ",", "der", "du", "in", "dum\u00b7pfem", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt, alter Rhein, ziehst deine Flutenbahnen", "tokens": ["Jetzt", ",", "al\u00b7ter", "Rhein", ",", "ziehst", "dei\u00b7ne", "Flu\u00b7ten\u00b7bah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NE", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Meerniederw\u00e4rts, da dich zum Unterthanen", "tokens": ["Meer\u00b7nie\u00b7der\u00b7w\u00e4rts", ",", "da", "dich", "zum", "Un\u00b7ter\u00b7tha\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Fremdling zwang das Schicksal, das verworrne;", "tokens": ["Dem", "Fremd\u00b7ling", "zwang", "das", "Schick\u00b7sal", ",", "das", "ver\u00b7worr\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dich m\u00f6cht' ich sehn, wann \u00fcber deinem Borne", "tokens": ["Dich", "m\u00f6cht'", "ich", "sehn", ",", "wann", "\u00fc\u00b7ber", "dei\u00b7nem", "Bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du einst des ersten deutschen Heerzugs Fahnen", "tokens": ["Du", "einst", "des", "ers\u00b7ten", "deut\u00b7schen", "Heer\u00b7zugs", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADJA", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Siehst wieder flattern, und im Freiheitsahnen", "tokens": ["Siehst", "wie\u00b7der", "flat\u00b7tern", ",", "und", "im", "Frei\u00b7hei\u00b7tsah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dich richtest auf mit neugewachs'nem Horne;", "tokens": ["Dich", "rich\u00b7test", "auf", "mit", "neu\u00b7ge\u00b7wachs'\u00b7nem", "Hor\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und rufst mit lautem Ruf aus deinem Schilfe", "tokens": ["Und", "rufst", "mit", "lau\u00b7tem", "Ruf", "aus", "dei\u00b7nem", "Schil\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den Deinen zu, ein weitvernommner Rufer:", "tokens": ["Den", "Dei\u00b7nen", "zu", ",", "ein", "weit\u00b7ver\u00b7nomm\u00b7ner", "Ru\u00b7fer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auf, ihr Tritonen, auf, ihr Knechtschaftsdulder!", "tokens": ["Auf", ",", "ihr", "Tri\u00b7to\u00b7nen", ",", "auf", ",", "ihr", "Knecht\u00b7schafts\u00b7dul\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Herbei ihr alle zu vereinter Hilfe!", "tokens": ["Her\u00b7bei", "ihr", "al\u00b7le", "zu", "ver\u00b7ein\u00b7ter", "Hil\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Siegjauchzend tragt mir an das linke Ufer", "tokens": ["Sieg\u00b7jauch\u00b7zend", "tragt", "mir", "an", "das", "lin\u00b7ke", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das erste deutsche Schiff auf eurer Schulter!", "tokens": ["Das", "ers\u00b7te", "deut\u00b7sche", "Schiff", "auf", "eu\u00b7rer", "Schul\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}