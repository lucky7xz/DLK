{"textgrid.poem.56352": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Verbrennung der t\u00fcrkischen Flotte zu Tschesme", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stellt willig euch nicht taub und blind, es r\u00e4cht sich.", "tokens": ["Stellt", "wil\u00b7lig", "euch", "nicht", "taub", "und", "blind", ",", "es", "r\u00e4cht", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der m\u00e4cht'ge Sultan mu\u00dft es selbst erfahren", "tokens": ["Der", "m\u00e4cht'\u00b7ge", "Sul\u00b7tan", "mu\u00dft", "es", "selbst", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein tausend sieben hundert acht und sechzig.", "tokens": ["Ein", "tau\u00b7send", "sie\u00b7ben", "hun\u00b7dert", "acht", "und", "sech\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "CARD", "CARD", "CARD", "KON", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es machten ihm in dem und n\u00e4chsten Jahren", "tokens": ["Es", "mach\u00b7ten", "ihm", "in", "dem", "und", "n\u00e4chs\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Viel Ungemach die unbeschnittnen Hunde,", "tokens": ["Viel", "Un\u00b7ge\u00b7mach", "die", "un\u00b7be\u00b7schnitt\u00b7nen", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gar im Krieg ihm \u00fcberlegen waren.", "tokens": ["Die", "gar", "im", "Krieg", "ihm", "\u00fc\u00b7berl\u00b7e\u00b7gen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und seinem Divan gab geheime Kunde", "tokens": ["Und", "sei\u00b7nem", "Di\u00b7van", "gab", "ge\u00b7hei\u00b7me", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein andrer Hund, Gesandter einer Macht,", "tokens": ["Ein", "an\u00b7drer", "Hund", ",", "Ge\u00b7sand\u00b7ter", "ei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die eben mit den Russen nicht im Bunde:", "tokens": ["Die", "e\u00b7ben", "mit", "den", "Rus\u00b7sen", "nicht", "im", "Bun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es sei ihm sichern Ortes hinterbracht,", "tokens": ["Es", "sei", "ihm", "si\u00b7chern", "Or\u00b7tes", "hin\u00b7ter\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit welchen Pl\u00e4nen sich die Zarin br\u00fcste,", "tokens": ["Mit", "wel\u00b7chen", "Pl\u00e4\u00b7nen", "sich", "die", "Za\u00b7rin", "br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zur Tat gediehen, eh man sich's gedacht;", "tokens": ["Zur", "Tat", "ge\u00b7die\u00b7hen", ",", "eh", "man", "sich's", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUS", "PIS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie in den Ostsee-H\u00e4fen sie sich r\u00fcste,", "tokens": ["Wie", "in", "den", "Ost\u00b7see\u00b7H\u00e4\u00b7fen", "sie", "sich", "r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und eine Flotte, bald zur Fahrt bereit,", "tokens": ["Und", "ei\u00b7ne", "Flot\u00b7te", ",", "bald", "zur", "Fahrt", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bedrohe fernher Griechenlandes K\u00fcste.", "tokens": ["Be\u00b7dro\u00b7he", "fern\u00b7her", "Grie\u00b7chen\u00b7lan\u00b7des", "K\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Darauf die Herrn: er m\u00f6g in k\u00fcnft'ger Zeit", "tokens": ["Da\u00b7rauf", "die", "Herrn", ":", "er", "m\u00f6g", "in", "k\u00fcnft'\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "$.", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sich h\u00fcten, mit so unversch\u00e4mter L\u00fcge", "tokens": ["Sich", "h\u00fc\u00b7ten", ",", "mit", "so", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das Ohr zu kr\u00e4nken Seiner Herrlichkeit.", "tokens": ["Das", "Ohr", "zu", "kr\u00e4n\u00b7ken", "Sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der hohe Sultan wisse zur Gen\u00fcge:", "tokens": ["Der", "ho\u00b7he", "Sul\u00b7tan", "wis\u00b7se", "zur", "Ge\u00b7n\u00fc\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Von dorther sei ins Mittell\u00e4nd'sche Meer", "tokens": ["Von", "dor\u00b7ther", "sei", "ins", "Mit\u00b7tel\u00b7l\u00e4n\u00b7d'\u00b7sche", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.21": {"text": "Kein Wasserweg, der eine Flotte tr\u00fcge.", "tokens": ["Kein", "Was\u00b7ser\u00b7weg", ",", "der", "ei\u00b7ne", "Flot\u00b7te", "tr\u00fc\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Drauf er entr\u00fcstet ob der neuen M\u00e4r:", "tokens": ["Drauf", "er", "ent\u00b7r\u00fcs\u00b7tet", "ob", "der", "neu\u00b7en", "M\u00e4r", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u00bbseht scharf die beigelegten Charten an,", "tokens": ["\u00bb", "seht", "scharf", "die", "bei\u00b7ge\u00b7leg\u00b7ten", "Char\u00b7ten", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Es ist nicht, wie ihr sagt, ihr irret sehr.", "tokens": ["Es", "ist", "nicht", ",", "wie", "ihr", "sagt", ",", "ihr", "ir\u00b7ret", "sehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die Nordsee, der Kanal, der Ozean", "tokens": ["Die", "Nord\u00b7see", ",", "der", "Ka\u00b7nal", ",", "der", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Er\u00f6ffnen um Europa weit im Kreise", "tokens": ["Er\u00b7\u00f6ff\u00b7nen", "um", "Eu\u00b7ro\u00b7pa", "weit", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Zu Herkulss\u00e4ulen eine feuchte Bahn.\u00ab", "tokens": ["Zu", "Her\u00b7kuls\u00b7s\u00e4u\u00b7len", "ei\u00b7ne", "feuch\u00b7te", "Bahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Drauf sie: \u00bbDu nennst uns fabelhafter Weise", "tokens": ["Drauf", "sie", ":", "\u00bb", "Du", "nennst", "uns", "fa\u00b7bel\u00b7haf\u00b7ter", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "$.", "$(", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Den Herkules, den gibt es nicht; vor allen", "tokens": ["Den", "Her\u00b7ku\u00b7les", ",", "den", "gibt", "es", "nicht", ";", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PIAT"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Ist aber unser Herrscher gro\u00df und weise.", "tokens": ["Ist", "a\u00b7ber", "un\u00b7ser", "Herr\u00b7scher", "gro\u00df", "und", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Drum h\u00fcte dich beschwerlich ihm zu fallen,", "tokens": ["Drum", "h\u00fc\u00b7te", "dich", "be\u00b7schwer\u00b7lich", "ihm", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Du bist gewarnt, er l\u00e4\u00dft, ungl\u00e4ub'ger Christ,", "tokens": ["Du", "bist", "ge\u00b7warnt", ",", "er", "l\u00e4\u00dft", ",", "un\u00b7gl\u00e4ub'\u00b7ger", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sich solche ", "tokens": ["Sich", "sol\u00b7che"], "token_info": ["word", "word"], "pos": ["PRF", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Es blieb bei dem Bescheid. Ihr aber wi\u00dft,", "tokens": ["Es", "blieb", "bei", "dem", "Be\u00b7scheid", ".", "Ihr", "a\u00b7ber", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Was doch sich bald zu Tschesme zugetragen,", "tokens": ["Was", "doch", "sich", "bald", "zu", "Tsches\u00b7me", "zu\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "ADV", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wo jener Stolz zu Rauch geworden ist.", "tokens": ["Wo", "je\u00b7ner", "Stolz", "zu", "Rauch", "ge\u00b7wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ihr wi\u00dft es ja, und wollt uns dennoch sagen:", "tokens": ["Ihr", "wi\u00dft", "es", "ja", ",", "und", "wollt", "uns", "den\u00b7noch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die Nacht ist gut, worin wir euch umschlungen,", "tokens": ["Die", "Nacht", "ist", "gut", ",", "wo\u00b7rin", "wir", "euch", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Es darf und wird euch keine Sonne tagen;", "tokens": ["Es", "darf", "und", "wird", "euch", "kei\u00b7ne", "Son\u00b7ne", "ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wir halten nichts von euren ", "tokens": ["Wir", "hal\u00b7ten", "nichts", "von", "eu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}