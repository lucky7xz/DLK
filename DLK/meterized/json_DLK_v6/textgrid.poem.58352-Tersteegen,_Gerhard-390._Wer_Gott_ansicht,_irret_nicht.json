{"textgrid.poem.58352": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "390. Wer Gott ansicht, irret nicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du rein und sicher gehen,", "tokens": ["Willst", "du", "rein", "und", "si\u00b7cher", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft du nicht beiseite sehen,", "tokens": ["Mu\u00dft", "du", "nicht", "bei\u00b7sei\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Gem\u00fctes still Gesicht", "tokens": ["Des", "Ge\u00b7m\u00fc\u00b7tes", "still", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblich sei in Gott gericht;", "tokens": ["Lieb\u00b7lich", "sei", "in", "Gott", "ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df dich keine Furcht verwirren,", "tokens": ["La\u00df", "dich", "kei\u00b7ne", "Furcht", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer so wandelt, kann nicht irren!", "tokens": ["Wer", "so", "wan\u00b7delt", ",", "kann", "nicht", "ir\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dies einf\u00e4ltige Gemerk", "tokens": ["Dies", "ein\u00b7f\u00e4l\u00b7ti\u00b7ge", "Ge\u00b7merk"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heiligt dich und all dein Werk.", "tokens": ["Hei\u00b7ligt", "dich", "und", "all", "dein", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}