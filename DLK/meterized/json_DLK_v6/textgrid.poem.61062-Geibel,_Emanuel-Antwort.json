{"textgrid.poem.61062": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst mich, liebe Kleine,", "tokens": ["Du", "fragst", "mich", ",", "lie\u00b7be", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum ich sing' und weine,", "tokens": ["Wa\u00b7rum", "ich", "sing'", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du fragest, was mich schmerzt?", "tokens": ["Du", "fra\u00b7gest", ",", "was", "mich", "schmerzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich habe den Lenz vers\u00e4umet,", "tokens": ["Ich", "ha\u00b7be", "den", "Lenz", "ver\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich habe die Jugend vertr\u00e4umet,", "tokens": ["Ich", "ha\u00b7be", "die", "Ju\u00b7gend", "ver\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ich habe die Liebe verscherzt.", "tokens": ["Ich", "ha\u00b7be", "die", "Lie\u00b7be", "ver\u00b7scherzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Mir schwoll der Becher am Munde,", "tokens": ["Mir", "schwoll", "der", "Be\u00b7cher", "am", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich hatte nicht Durst zur Stunde,", "tokens": ["Ich", "hat\u00b7te", "nicht", "Durst", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lie\u00df vor\u00fcber ihn gehn;", "tokens": ["Ich", "lie\u00df", "vor\u00b7\u00fc\u00b7ber", "ihn", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mir winkt' im gr\u00fcnen Laube", "tokens": ["Mir", "winkt'", "im", "gr\u00fc\u00b7nen", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Granate, Feig' und Traube,", "tokens": ["Gra\u00b7na\u00b7te", ",", "Feig'", "und", "Trau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Doch hab' ich sie lassen stehn.", "tokens": ["Doch", "hab'", "ich", "sie", "las\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und als nun kam der Abend,", "tokens": ["Und", "als", "nun", "kam", "der", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonn' im Glanz begrabend,", "tokens": ["Die", "Sonn'", "im", "Glanz", "be\u00b7gra\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war mein Durst erwacht;", "tokens": ["Da", "war", "mein", "Durst", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aber der Becher der Wonnen,", "tokens": ["A\u00b7ber", "der", "Be\u00b7cher", "der", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Die Fr\u00fcchte waren zerronnen,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "wa\u00b7ren", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und dunkelte rings die Nacht.", "tokens": ["Und", "dun\u00b7kel\u00b7te", "rings", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Welt hat mich verlassen;", "tokens": ["Die", "Welt", "hat", "mich", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun sing' ich auf den Gassen", "tokens": ["Nun", "sing'", "ich", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Lied, wie tief es schmerzt:", "tokens": ["Mein", "Lied", ",", "wie", "tief", "es", "schmerzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbich habe den Lenz vers\u00e4umet,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "den", "Lenz", "ver\u00b7s\u00e4u\u00b7met", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich habe die Jugend vertr\u00e4umet,", "tokens": ["Ich", "ha\u00b7be", "die", "Ju\u00b7gend", "ver\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ich habe die Liebe verscherzt.\u00ab", "tokens": ["Ich", "ha\u00b7be", "die", "Lie\u00b7be", "ver\u00b7scherzt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}