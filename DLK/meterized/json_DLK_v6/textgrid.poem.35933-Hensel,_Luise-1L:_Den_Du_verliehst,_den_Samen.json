{"textgrid.poem.35933": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Du verliehst, den Samen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Du verliehst, den Samen", "tokens": ["Den", "Du", "ver\u00b7liehst", ",", "den", "Sa\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Streut' ich mit r\u00fcst'gen H\u00e4nden", "tokens": ["Streut'", "ich", "mit", "r\u00fcst'\u00b7gen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einst in des Lenzes Tagen", "tokens": ["Einst", "in", "des", "Len\u00b7zes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In frisches Gartenland.", "tokens": ["In", "fri\u00b7sches", "Gar\u00b7ten\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich that's in ", "tokens": ["Ich", "that's", "in"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Du wolltest Segen spenden", "tokens": ["Du", "woll\u00b7test", "Se\u00b7gen", "spen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Sonn' und Regen senden,", "tokens": ["Und", "Sonn'", "und", "Re\u00b7gen", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df reich die Saat erstand.", "tokens": ["Da\u00df", "reich", "die", "Saat", "er\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun in des Herbstes Tagen", "tokens": ["Nun", "in", "des", "Herbs\u00b7tes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kehr' ich von fernen Wegen", "tokens": ["Kehr'", "ich", "von", "fer\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcck zu diesen Feldern,", "tokens": ["Zu\u00b7r\u00fcck", "zu", "die\u00b7sen", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich Dir einst gebaut,", "tokens": ["Die", "ich", "Dir", "einst", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und froh mein Herz mu\u00df schlagen,", "tokens": ["Und", "froh", "mein", "Herz", "mu\u00df", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn reicher Aehrensegen", "tokens": ["Denn", "rei\u00b7cher", "A\u00b7eh\u00b7ren\u00b7se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wallt golden mir entgegen", "tokens": ["Wallt", "gol\u00b7den", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wohin das Auge schaut.", "tokens": ["Wo\u00b7hin", "das", "Au\u00b7ge", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Dank, Herr! Deinen Hulden,", "tokens": ["O", "Dank", ",", "Herr", "!", "Dei\u00b7nen", "Hul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df vor des Sturmes W\u00fcthen", "tokens": ["Da\u00df", "vor", "des", "Stur\u00b7mes", "W\u00fct\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und D\u00fcrr' und Wurmes-Nagen", "tokens": ["Und", "D\u00fcrr'", "und", "Wur\u00b7mes\u00b7Na\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sch\u00fctzte Deine Hand", "tokens": ["Sie", "sch\u00fctz\u00b7te", "Dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trotz meiner Schw\u00e4che Schulden!", "tokens": ["Trotz", "mei\u00b7ner", "Schw\u00e4\u00b7che", "Schul\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O, wollest ferner h\u00fcten", "tokens": ["O", ",", "wol\u00b7lest", "fer\u00b7ner", "h\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Fr\u00fcchte und die Bl\u00fcthen", "tokens": ["Die", "Fr\u00fcch\u00b7te", "und", "die", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Dir geweihten Land! Amen.", "tokens": ["Im", "Dir", "ge\u00b7weih\u00b7ten", "Land", "!", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "PPER", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}