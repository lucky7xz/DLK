{"textgrid.poem.62441": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir stehen hier aufs Sterben,", "tokens": ["Wir", "ste\u00b7hen", "hier", "aufs", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tod ist uns ein Spott.", "tokens": ["Der", "Tod", "ist", "uns", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df uns den Himmel erben,", "tokens": ["La\u00df", "uns", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du ewig treuer Gott.", "tokens": ["Du", "e\u00b7wig", "treu\u00b7er", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sind wir gleich voller Schulden", "tokens": ["Sind", "wir", "gleich", "vol\u00b7ler", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ohne gro\u00dfen Ruhm,", "tokens": ["Und", "oh\u00b7ne", "gro\u00b7\u00dfen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir sind dein Eigenthum,", "tokens": ["Wir", "sind", "dein", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und du bist reich an Hulden.", "tokens": ["Und", "du", "bist", "reich", "an", "Hul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fern von den Thermopylen", "tokens": ["Fern", "von", "den", "Ther\u00b7mo\u00b7py\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kommt uns ein ernstes Wort,", "tokens": ["Kommt", "uns", "ein", "erns\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo wackre Streiter fielen", "tokens": ["Wo", "wack\u00b7re", "Strei\u00b7ter", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ihres Landes Hort;", "tokens": ["Als", "ih\u00b7res", "Lan\u00b7des", "Hort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Heiden haben k\u00f6nnten", "tokens": ["Was", "Hei\u00b7den", "ha\u00b7ben", "k\u00f6nn\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit festem treuem Muth,", "tokens": ["Mit", "fes\u00b7tem", "treu\u00b7em", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das h\u00f6chste sel'ge Gut", "tokens": ["Das", "h\u00f6chs\u00b7te", "sel'\u00b7ge", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wirst du den Christen g\u00f6nnen.", "tokens": ["Wirst", "du", "den", "Chris\u00b7ten", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die f\u00fcr den Christ gestritten,", "tokens": ["Die", "f\u00fcr", "den", "Christ", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie scheinen herzuschau'n,", "tokens": ["Sie", "schei\u00b7nen", "her\u00b7zu\u00b7schau'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glaubenstod erlitten,", "tokens": ["Die", "Glau\u00b7ben\u00b7stod", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die M\u00e4nner, Kinder, Frau'n,", "tokens": ["Die", "M\u00e4n\u00b7ner", ",", "Kin\u00b7der", ",", "Frau'n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihren Marterzeichen;", "tokens": ["Mit", "ih\u00b7ren", "Mar\u00b7ter\u00b7zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die sel'ge Zeugenschaar", "tokens": ["Die", "sel'\u00b7ge", "Zeu\u00b7gen\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Scheint auch f\u00fcr unser Haar", "tokens": ["Scheint", "auch", "f\u00fcr", "un\u00b7ser", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Palmen herzureichen.", "tokens": ["Die", "Pal\u00b7men", "her\u00b7zu\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der uns vorangeschritten", "tokens": ["Der", "uns", "vor\u00b7an\u00b7ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Herzog in dem Schmerz,", "tokens": ["Ein", "Her\u00b7zog", "in", "dem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Der Herr ist in der Mitten", "tokens": ["Der", "Herr", "ist", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spricht an jedes Herz.", "tokens": ["Und", "spricht", "an", "je\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welt liegt in den Ketten", "tokens": ["Die", "Welt", "liegt", "in", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der b\u00f6sen dunkeln Macht,", "tokens": ["Der", "b\u00f6\u00b7sen", "dun\u00b7keln", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die H\u00f6lle z\u00fcrnt und wacht,", "tokens": ["Die", "H\u00f6l\u00b7le", "z\u00fcrnt", "und", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wer will die Welt erretten?", "tokens": ["Wer", "will", "die", "Welt", "er\u00b7ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist ein sch\u00f6nes Kriegen", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7nes", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In solchem heil'gen Ha\u00df,", "tokens": ["In", "sol\u00b7chem", "heil'\u00b7gen", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch erschlagen liegen", "tokens": ["Und", "auch", "er\u00b7schla\u00b7gen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im gr\u00fcnen k\u00fchlen Gras.", "tokens": ["Im", "gr\u00fc\u00b7nen", "k\u00fch\u00b7len", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "All' Sehnen und all' Streben,", "tokens": ["All'", "Seh\u00b7nen", "und", "all'", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie wird es leicht gestillt,", "tokens": ["Wie", "wird", "es", "leicht", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bei Feldmusik entquillt", "tokens": ["Bei", "Feld\u00b7mu\u00b7sik", "ent\u00b7quillt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Brust das warme Leben.", "tokens": ["Der", "Brust", "das", "war\u00b7me", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir haben uns verschworen", "tokens": ["Wir", "ha\u00b7ben", "uns", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrs Heil der ganzen Welt, \u2013", "tokens": ["F\u00fcrs", "Heil", "der", "gan\u00b7zen", "Welt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wird zum Licht geboren,", "tokens": ["Der", "wird", "zum", "Licht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer heute r\u00fchmlich f\u00e4llt.", "tokens": ["Wer", "heu\u00b7te", "r\u00fchm\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist ein leichtes Sterben,", "tokens": ["Das", "ist", "ein", "leich\u00b7tes", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ist ein s\u00fc\u00dfer Tod,", "tokens": ["Das", "ist", "ein", "s\u00fc\u00b7\u00dfer", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn's gilt aus bittrer Noth", "tokens": ["Wenn's", "gilt", "aus", "bit\u00b7trer", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die ew'ge Lust zu erben.", "tokens": ["Die", "ew'\u00b7ge", "Lust", "zu", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}