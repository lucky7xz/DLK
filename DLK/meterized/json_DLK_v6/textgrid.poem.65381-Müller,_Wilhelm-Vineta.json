{"textgrid.poem.65381": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vineta", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Meeres tiefem, tiefem Grunde", "tokens": ["Aus", "des", "Mee\u00b7res", "tie\u00b7fem", ",", "tie\u00b7fem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Klingen Abendglocken dumpf und matt,", "tokens": ["Klin\u00b7gen", "A\u00b7bend\u00b7glo\u00b7cken", "dumpf", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Uns zu geben wunderbare Kunde", "tokens": ["Uns", "zu", "ge\u00b7ben", "wun\u00b7der\u00b7ba\u00b7re", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der sch\u00f6nen alten Wunderstadt.", "tokens": ["Von", "der", "sch\u00f6\u00b7nen", "al\u00b7ten", "Wun\u00b7der\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In der Fluthen Schoo\u00df hinabgesunken,", "tokens": ["In", "der", "Flut\u00b7hen", "Schoo\u00df", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blieben unten ihre Tr\u00fcmmer stehn.", "tokens": ["Blie\u00b7ben", "un\u00b7ten", "ih\u00b7re", "Tr\u00fcm\u00b7mer", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Zinnen lassen goldne Funken", "tokens": ["Ih\u00b7re", "Zin\u00b7nen", "las\u00b7sen", "gold\u00b7ne", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wiederscheinend auf dem Spiegel sehn.", "tokens": ["Wie\u00b7der\u00b7schei\u00b7nend", "auf", "dem", "Spie\u00b7gel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und der Schiffer, der den Zauberschimmer", "tokens": ["Und", "der", "Schif\u00b7fer", ",", "der", "den", "Zau\u00b7ber\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einmal sah im hellen Abendroth,", "tokens": ["Ein\u00b7mal", "sah", "im", "hel\u00b7len", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nach derselben Stelle schifft er immer,", "tokens": ["Nach", "der\u00b7sel\u00b7ben", "Stel\u00b7le", "schifft", "er", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ob auch rings umher die Klippe droht.", "tokens": ["Ob", "auch", "rings", "um\u00b7her", "die", "Klip\u00b7pe", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus des Herzens tiefem, tiefem Grunde", "tokens": ["Aus", "des", "Her\u00b7zens", "tie\u00b7fem", ",", "tie\u00b7fem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Klingt es mir, wie Glocken, dumpf und matt.", "tokens": ["Klingt", "es", "mir", ",", "wie", "Glo\u00b7cken", ",", "dumpf", "und", "matt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, sie geben wunderbare Kunde", "tokens": ["Ach", ",", "sie", "ge\u00b7ben", "wun\u00b7der\u00b7ba\u00b7re", "Kun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Liebe, die geliebt es hat.", "tokens": ["Von", "der", "Lie\u00b7be", ",", "die", "ge\u00b7liebt", "es", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Eine sch\u00f6ne Welt ist da versunken,", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Welt", "ist", "da", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre Tr\u00fcmmer blieben unten stehn,", "tokens": ["Ih\u00b7re", "Tr\u00fcm\u00b7mer", "blie\u00b7ben", "un\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lassen sich als goldne Himmelsfunken", "tokens": ["Las\u00b7sen", "sich", "als", "gold\u00b7ne", "Him\u00b7mels\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oft im Spiegel meiner Tr\u00e4ume sehn.", "tokens": ["Oft", "im", "Spie\u00b7gel", "mei\u00b7ner", "Tr\u00e4u\u00b7me", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und dann m\u00f6cht' ich tauchen in die Tiefen,", "tokens": ["Und", "dann", "m\u00f6cht'", "ich", "tau\u00b7chen", "in", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich versenken in den Wiederschein,", "tokens": ["Mich", "ver\u00b7sen\u00b7ken", "in", "den", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mir ist, als ob mich Engel riefen", "tokens": ["Und", "mir", "ist", ",", "als", "ob", "mich", "En\u00b7gel", "rie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "NE", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In die alte Wunderstadt herein.", "tokens": ["In", "die", "al\u00b7te", "Wun\u00b7der\u00b7stadt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}