{"textgrid.poem.60343": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Neid der Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4che rauschen", "tokens": ["Die", "B\u00e4\u00b7che", "rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Fr\u00fchlingssonne,", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hell singen die V\u00f6gel,", "tokens": ["Hell", "sin\u00b7gen", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es lauschen die Bl\u00fcten,", "tokens": ["Es", "lau\u00b7schen", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und sprachlos ringen", "tokens": ["Und", "sprach\u00b7los", "rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sich Wonned\u00fcfte", "tokens": ["Sich", "Won\u00b7ne\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["PRF", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Aus ihrem Busen;", "tokens": ["Aus", "ih\u00b7rem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und ich mu\u00df trauern,", "tokens": ["Und", "ich", "mu\u00df", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Denn nimmer strahlt mir", "tokens": ["Denn", "nim\u00b7mer", "strahlt", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Dein Aug, o Geliebte! \u2013", "tokens": ["Dein", "Aug", ",", "o", "Ge\u00b7lieb\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Nicht \u00fcber den Wellen", "tokens": ["Nicht", "\u00fc\u00b7ber", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Des Ozeanes,", "tokens": ["Des", "O\u00b7zea\u00b7nes", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Nicht \u00fcber den Sternen", "tokens": ["Nicht", "\u00fc\u00b7ber", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und nicht im Lande", "tokens": ["Und", "nicht", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Der Phantasien", "tokens": ["Der", "Phan\u00b7ta\u00b7si\u00b7en"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.16": {"text": "Ist meine Heimat;", "tokens": ["Ist", "mei\u00b7ne", "Hei\u00b7mat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Ich finde sie nur", "tokens": ["Ich", "fin\u00b7de", "sie", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "In deinem Auge!", "tokens": ["In", "dei\u00b7nem", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Was je mir freudig", "tokens": ["Was", "je", "mir", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Beseelte das Leben,", "tokens": ["Be\u00b7seel\u00b7te", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Was nach dem Tode", "tokens": ["Was", "nach", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Mir weckte die Sehnsucht,", "tokens": ["Mir", "weck\u00b7te", "die", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Entschwundner Kindheit", "tokens": ["Ent\u00b7schwund\u00b7ner", "Kind\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Fr\u00f6hliche Tage", "tokens": ["Fr\u00f6h\u00b7li\u00b7che", "Ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Und meiner Jugend", "tokens": ["Und", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Himmlische Tr\u00e4ume,", "tokens": ["Himm\u00b7li\u00b7sche", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Von meinen Toten", "tokens": ["Von", "mei\u00b7nen", "To\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Trauliche Gr\u00fc\u00dfe", "tokens": ["Trau\u00b7li\u00b7che", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Und meiner Gottheit", "tokens": ["Und", "mei\u00b7ner", "Got\u00b7theit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "St\u00e4rkenden Anblick,", "tokens": ["St\u00e4r\u00b7ken\u00b7den", "An\u00b7blick", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Das alles find ich", "tokens": ["Das", "al\u00b7les", "find", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "In deinem Auge,", "tokens": ["In", "dei\u00b7nem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "O meine Geliebte!", "tokens": ["O", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Nun bist du ferne,", "tokens": ["Nun", "bist", "du", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Und bitter beneiden", "tokens": ["Und", "bit\u00b7ter", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Mu\u00df jeden Stein ich", "tokens": ["Mu\u00df", "je\u00b7den", "Stein", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Und jede Blume,", "tokens": ["Und", "je\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Beneiden die kalten", "tokens": ["Be\u00b7nei\u00b7den", "die", "kal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Menschen und Sterne,", "tokens": ["Men\u00b7schen", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "An die du vergeudest", "tokens": ["An", "die", "du", "ver\u00b7geu\u00b7dest"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.41": {"text": "Die s\u00fc\u00dfen Blicke.", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}