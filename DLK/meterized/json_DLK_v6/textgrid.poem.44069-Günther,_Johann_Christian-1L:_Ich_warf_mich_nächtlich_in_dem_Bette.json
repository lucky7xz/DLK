{"textgrid.poem.44069": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich warf mich n\u00e4chtlich in dem Bette", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich warf mich n\u00e4chtlich in dem Bette", "tokens": ["Ich", "warf", "mich", "n\u00e4cht\u00b7lich", "in", "dem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dachte traurig hin und her,", "tokens": ["Und", "dach\u00b7te", "trau\u00b7rig", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woran ich mich vers\u00fcndigt h\u00e4tte", "tokens": ["Wo\u00b7ran", "ich", "mich", "ver\u00b7s\u00fcn\u00b7digt", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was doch mein Verh\u00e4ngn\u00fc\u00df w\u00e4r.", "tokens": ["Und", "was", "doch", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah den Troz der reichen Thoren,", "tokens": ["Ich", "sah", "den", "Troz", "der", "rei\u00b7chen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Sauflied fiel mir in die Ohren,", "tokens": ["Ihr", "Sauf\u00b7lied", "fiel", "mir", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schlugen Sorg und Gott in Wind.", "tokens": ["Sie", "schlu\u00b7gen", "Sorg", "und", "Gott", "in", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich lies den Glauben ziemlich wancken", "tokens": ["Ich", "lies", "den", "Glau\u00b7ben", "ziem\u00b7lich", "wan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und kam auf artige Gedancken,", "tokens": ["Und", "kam", "auf", "ar\u00b7ti\u00b7ge", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die kl\u00fcger bleiben, was sie sind.", "tokens": ["Die", "kl\u00fc\u00b7ger", "blei\u00b7ben", ",", "was", "sie", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dar\u00fcber zeigte mir der Schlummer", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "zeig\u00b7te", "mir", "der", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ungew\u00f6hnlich Traumgesicht:", "tokens": ["Ein", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "Traum\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Weibsbild lief vor Neid und Kummer", "tokens": ["Ein", "Weibs\u00b7bild", "lief", "vor", "Neid", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schien erbermlich zugericht;", "tokens": ["Und", "schien", "er\u00b7berm\u00b7lich", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dachte sich mit Schreyn und Z\u00e4hren", "tokens": ["Sie", "dach\u00b7te", "sich", "mit", "Schreyn", "und", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des nahen Elends zu erwehren.", "tokens": ["Des", "na\u00b7hen", "E\u00b7lends", "zu", "er\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Schr\u00f6cken gab ihr Kraft zur Flucht;", "tokens": ["Das", "Schr\u00f6\u00b7cken", "gab", "ihr", "Kraft", "zur", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allein die Armuth hielt sie wieder", "tokens": ["Al\u00b7lein", "die", "Ar\u00b7muth", "hielt", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und ri\u00df sie bey der Hecke nieder,", "tokens": ["Und", "ri\u00df", "sie", "bey", "der", "He\u00b7cke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In der sie H\u00fclf und Rath gesucht.", "tokens": ["In", "der", "sie", "H\u00fclf", "und", "Rath", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Hunger fra\u00df in Fleisch und Beine,", "tokens": ["Der", "Hun\u00b7ger", "fra\u00df", "in", "Fleisch", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die L\u00fcgen sogen Marck und Blut,", "tokens": ["Die", "L\u00fc\u00b7gen", "so\u00b7gen", "Marck", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Laster warfen Pfeil und Steine,", "tokens": ["Die", "Las\u00b7ter", "war\u00b7fen", "Pfeil", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thorheit sprach den Frevel gut.", "tokens": ["Die", "Thor\u00b7heit", "sprach", "den", "Fre\u00b7vel", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Unschuld sah die Angst von weiten", "tokens": ["Die", "Un\u00b7schuld", "sah", "die", "Angst", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wollt und ward auf allen Seiten", "tokens": ["Und", "wollt", "und", "ward", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "KON", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Aberglauben weggejagt.", "tokens": ["Vom", "A\u00b7berg\u00b7lau\u00b7ben", "weg\u00b7ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Zeit kam auch mit ihrer L\u00e4nge", "tokens": ["Die", "Zeit", "kam", "auch", "mit", "ih\u00b7rer", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sprach: O halt nur im Gedr\u00e4nge,", "tokens": ["Und", "sprach", ":", "O", "halt", "nur", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist noch nicht genug geplagt.", "tokens": ["Du", "bist", "noch", "nicht", "ge\u00b7nug", "ge\u00b7plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Gegend von dem Trauerspiele", "tokens": ["Die", "Ge\u00b7gend", "von", "dem", "Trau\u00b7er\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wies in der N\u00e4h ein lustig Feld:", "tokens": ["Wies", "in", "der", "N\u00e4h", "ein", "lus\u00b7tig", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diesem lacht- und scherzten viele,", "tokens": ["Auf", "die\u00b7sem", "lacht", "und", "scherz\u00b7ten", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "TRUNC", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenn man etwan Hochzeit h\u00e4lt.", "tokens": ["Wie", "wenn", "man", "et\u00b7wan", "Hoch\u00b7zeit", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es waren Freund und Anverwandten,", "tokens": ["Es", "wa\u00b7ren", "Freund", "und", "An\u00b7ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die unsrer \u00c4rmsten Noth wohl kannten.", "tokens": ["Die", "uns\u00b7rer", "\u00c4rms\u00b7ten", "Noth", "wohl", "kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie rief, sie schrie, sie weint und bat", "tokens": ["Sie", "rief", ",", "sie", "schrie", ",", "sie", "weint", "und", "bat"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und streckte die zerfleischten Armen,", "tokens": ["Und", "streck\u00b7te", "die", "zer\u00b7fleischten", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Nicht einer war, der aus Erbarmen", "tokens": ["Nicht", "ei\u00b7ner", "war", ",", "der", "aus", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "VAFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nur wenig Schritte n\u00e4her trat.", "tokens": ["Nur", "we\u00b7nig", "Schrit\u00b7te", "n\u00e4\u00b7her", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drauf \u00e4chzte sie zum lezten Mahle:", "tokens": ["Drauf", "\u00e4chz\u00b7te", "sie", "zum", "lez\u00b7ten", "Mah\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach Himmel, hilf mir aus der Noth!", "tokens": ["Ach", "Him\u00b7mel", ",", "hilf", "mir", "aus", "der", "Noth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er that es mit dem sch\u00e4rfsten Strahle,", "tokens": ["Er", "that", "es", "mit", "dem", "sch\u00e4rfs\u00b7ten", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Mitleid war ihr schneller Tod.", "tokens": ["Sein", "Mit\u00b7leid", "war", "ihr", "schnel\u00b7ler", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Feinde schleppten ihre Leiche", "tokens": ["Die", "Fein\u00b7de", "schlepp\u00b7ten", "ih\u00b7re", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Wege, Sand, Morast und Str\u00e4uche,", "tokens": ["Durch", "We\u00b7ge", ",", "Sand", ",", "Mo\u00b7rast", "und", "Str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Grabmahl war ein w\u00fcster Ort.", "tokens": ["Ihr", "Grab\u00b7mahl", "war", "ein", "w\u00fcs\u00b7ter", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Aug erschrack vor solchem Grimme", "tokens": ["Mein", "Aug", "er\u00b7schrack", "vor", "sol\u00b7chem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wachte gleich vor dieser Stimme:", "tokens": ["Und", "wach\u00b7te", "gleich", "vor", "die\u00b7ser", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So schickt man deine Jugend fort.", "tokens": ["So", "schickt", "man", "dei\u00b7ne", "Ju\u00b7gend", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}