{"textgrid.poem.37229": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[die Selbstkritik hat viel f\u00fcr sich]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Selbstkritik hat viel f\u00fcr sich.", "tokens": ["Die", "Selbst\u00b7kri\u00b7tik", "hat", "viel", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesetzt den Fall, ich tadle mich,", "tokens": ["Ge\u00b7setzt", "den", "Fall", ",", "ich", "tad\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hab ich erstens den Gewinn,", "tokens": ["So", "hab", "ich", "ers\u00b7tens", "den", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich so h\u00fcbsch bescheiden bin;", "tokens": ["Da\u00df", "ich", "so", "h\u00fcbsch", "be\u00b7schei\u00b7den", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zum zweiten denken sich die Leut,", "tokens": ["Zum", "zwei\u00b7ten", "den\u00b7ken", "sich", "die", "Leut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mann ist lauter Redlichkeit;", "tokens": ["Der", "Mann", "ist", "lau\u00b7ter", "Red\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch schnapp ich drittens diesen Bissen", "tokens": ["Auch", "schnapp", "ich", "drit\u00b7tens", "die\u00b7sen", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vorweg den andern Kritik\u00fcssen;", "tokens": ["Vor\u00b7weg", "den", "an\u00b7dern", "Kri\u00b7ti\u00b7k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und viertens hoff ich au\u00dferdem", "tokens": ["Und", "vier\u00b7tens", "hoff", "ich", "au\u00b7\u00dfer\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Widerspruch, der mir genehm.", "tokens": ["Auf", "Wi\u00b7der\u00b7spruch", ",", "der", "mir", "ge\u00b7nehm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So kommt es denn zuletzt heraus,", "tokens": ["So", "kommt", "es", "denn", "zu\u00b7letzt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich ganz famoses Haus.", "tokens": ["Da\u00df", "ich", "ganz", "fa\u00b7mo\u00b7ses", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}