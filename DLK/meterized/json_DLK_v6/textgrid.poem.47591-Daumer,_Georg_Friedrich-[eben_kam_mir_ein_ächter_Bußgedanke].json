{"textgrid.poem.47591": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eben kam mir ein \u00e4chter Bu\u00dfgedanke]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eben kam mir ein \u00e4chter Bu\u00dfgedanke;", "tokens": ["E\u00b7ben", "kam", "mir", "ein", "\u00e4ch\u00b7ter", "Bu\u00df\u00b7ge\u00b7dan\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Kehren wollt' ich in heilsam enge Schranke;", "tokens": ["Keh\u00b7ren", "wollt'", "ich", "in", "heil\u00b7sam", "en\u00b7ge", "Schran\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch mein Liebchen, es lugt herein mit hellen", "tokens": ["Doch", "mein", "Lieb\u00b7chen", ",", "es", "lugt", "her\u00b7ein", "mit", "hel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "APPR", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schelmenaugen und merket, da\u00df ich kranke,", "tokens": ["Schel\u00b7men\u00b7au\u00b7gen", "und", "mer\u00b7ket", ",", "da\u00df", "ich", "kran\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schleicht dann n\u00e4her und lacht mich aus und schmeichelt", "tokens": ["Schleicht", "dann", "n\u00e4\u00b7her", "und", "lacht", "mich", "aus", "und", "schmei\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und umgarnet die Seele mir, die schwanke.", "tokens": ["Und", "um\u00b7gar\u00b7net", "die", "See\u00b7le", "mir", ",", "die", "schwan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ach, wie lacht mich ihr M\u00fcndchen an, das rothe,", "tokens": ["Ach", ",", "wie", "lacht", "mich", "ihr", "M\u00fcnd\u00b7chen", "an", ",", "das", "ro\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, wie rei\u00dft mich ihr Br\u00fcstchen hin, das blanke!", "tokens": ["Ach", ",", "wie", "rei\u00dft", "mich", "ihr", "Br\u00fcst\u00b7chen", "hin", ",", "das", "blan\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sag', o Himmel, o sage selbst, wie bliebe", "tokens": ["Sag'", ",", "o", "Him\u00b7mel", ",", "o", "sa\u00b7ge", "selbst", ",", "wie", "blie\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "FM", "VVFIN", "ADV", "$,", "PWAV", "VVFIN"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Frei das Innere hier von argem Wanke?", "tokens": ["Frei", "das", "In\u00b7ne\u00b7re", "hier", "von", "ar\u00b7gem", "Wan\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Du, das Gold der realen Wonne heischend,", "tokens": ["Du", ",", "das", "Gold", "der", "re\u00b7a\u00b7len", "Won\u00b7ne", "hei\u00b7schend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Giebst so luftige Waare nur zum Danke!", "tokens": ["Giebst", "so", "luf\u00b7ti\u00b7ge", "Waa\u00b7re", "nur", "zum", "Dan\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADV", "APPRART", "PTKANT", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Drum erlaube mir immer noch ein wenig,", "tokens": ["Drum", "er\u00b7lau\u00b7be", "mir", "im\u00b7mer", "noch", "ein", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "PIS", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df ein traulicher Arm mich s\u00fc\u00df umranke!", "tokens": ["Da\u00df", "ein", "trau\u00b7li\u00b7cher", "Arm", "mich", "s\u00fc\u00df", "um\u00b7ran\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}