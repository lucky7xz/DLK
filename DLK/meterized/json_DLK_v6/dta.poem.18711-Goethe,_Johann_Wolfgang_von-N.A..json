{"dta.poem.18711": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du auf dem Guten ruhst,", "tokens": ["Wenn", "du", "auf", "dem", "Gu\u00b7ten", "ruhst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer werd\u2019 ich\u2019s tadeln,", "tokens": ["Nim\u00b7mer", "werd'", "ich's", "ta\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn du gar das Gute thust,", "tokens": ["Wenn", "du", "gar", "das", "Gu\u00b7te", "thust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh das soll dich adeln;", "tokens": ["Sieh", "das", "soll", "dich", "a\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hast du aber deinen Zaun", "tokens": ["Hast", "du", "a\u00b7ber", "dei\u00b7nen", "Zaun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Um dein Gut gezogen,", "tokens": ["Um", "dein", "Gut", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Leb ich frey und lebe traun", "tokens": ["Leb", "ich", "frey", "und", "le\u00b7be", "traun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "VVFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keineswegs betrogen.", "tokens": ["Kei\u00b7nes\u00b7wegs", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denn die Menschen sie sind gut,", "tokens": ["Denn", "die", "Men\u00b7schen", "sie", "sind", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrden besser bleiben,", "tokens": ["W\u00fcr\u00b7den", "bes\u00b7ser", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sollte nicht wie\u2019s einer thut", "tokens": ["Soll\u00b7te", "nicht", "wie's", "ei\u00b7ner", "thut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "NN", "ART", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der Andre treiben.", "tokens": ["Auch", "der", "And\u00b7re", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf dem Weg da ists ein Wort,", "tokens": ["Auf", "dem", "Weg", "da", "ists", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand wird\u2019s verdammen:", "tokens": ["Nie\u00b7mand", "wird's", "ver\u00b7dam\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wollen wir an Einen Ort,", "tokens": ["Wol\u00b7len", "wir", "an", "Ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun! wir gehn zusammen.", "tokens": ["Nun", "!", "wir", "gehn", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vieles wird sich da und hie", "tokens": ["Vie\u00b7les", "wird", "sich", "da", "und", "hie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PRF", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns entgegen stellen.", "tokens": ["Uns", "ent\u00b7ge\u00b7gen", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In der Liebe mag man nie", "tokens": ["In", "der", "Lie\u00b7be", "mag", "man", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Helfer und Gesellen,", "tokens": ["Hel\u00b7fer", "und", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geld und Ehre h\u00e4tte man", "tokens": ["Geld", "und", "Eh\u00b7re", "h\u00e4t\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gern allein zur Spende", "tokens": ["Gern", "al\u00b7lein", "zur", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und der Wein, der treue Mann,", "tokens": ["Und", "der", "Wein", ",", "der", "treu\u00b7e", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Der entzweyt am Ende.", "tokens": ["Der", "ent\u00b7zweyt", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hat doch \u00fcber solches Zeug", "tokens": ["Hat", "doch", "\u00fc\u00b7ber", "sol\u00b7ches", "Zeug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hafis auch gesprochen,", "tokens": ["Ha\u00b7fis", "auch", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber manchen dummen Streich", "tokens": ["Ue\u00b7ber", "man\u00b7chen", "dum\u00b7men", "Streich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich den Kopf zerbrochen,", "tokens": ["Sich", "den", "Kopf", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und ich seh nicht was es frommt", "tokens": ["Und", "ich", "seh", "nicht", "was", "es", "frommt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Welt zu laufen,", "tokens": ["Aus", "der", "Welt", "zu", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Magst du, wenn das Schlimmste kommt,", "tokens": ["Magst", "du", ",", "wenn", "das", "Schlimms\u00b7te", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus einmal dich raufen.", "tokens": ["Aus", "ein\u00b7mal", "dich", "rau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}