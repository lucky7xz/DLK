{"textgrid.poem.62416": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Bei seines Vaters Tod", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf' in deiner engen Kammer,", "tokens": ["Schlaf'", "in", "dei\u00b7ner", "en\u00b7gen", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber alter Vater, schlaf',", "tokens": ["Lie\u00b7ber", "al\u00b7ter", "Va\u00b7ter", ",", "schlaf'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fccklich, da\u00df nach langem Jammer", "tokens": ["Gl\u00fcck\u00b7lich", ",", "da\u00df", "nach", "lan\u00b7gem", "Jam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch dich frohe Zeitung traf.", "tokens": ["Noch", "dich", "fro\u00b7he", "Zei\u00b7tung", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dank dir, da\u00df in unsre Herzen", "tokens": ["Dank", "dir", ",", "da\u00df", "in", "uns\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du der Ehre Muth gelegt,", "tokens": ["Du", "der", "Eh\u00b7re", "Muth", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wol Hunger, Durst und Schmerzen,", "tokens": ["Der", "wol", "Hun\u00b7ger", ",", "Durst", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Knechtschaft nie und Schande tr\u00e4gt.", "tokens": ["Knecht\u00b7schaft", "nie", "und", "Schan\u00b7de", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn auch Fremde dich begraben,", "tokens": ["Wenn", "auch", "Frem\u00b7de", "dich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaf' in freier Erde nun,", "tokens": ["Schlaf'", "in", "frei\u00b7er", "Er\u00b7de", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieber Vater, schau', wir haben", "tokens": ["Lie\u00b7ber", "Va\u00b7ter", ",", "schau'", ",", "wir", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt ein be\u00dfres Werk zu thun.", "tokens": ["Jetzt", "ein", "be\u00df\u00b7res", "Werk", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann erst, wenn die deutschen Auen", "tokens": ["Dann", "erst", ",", "wenn", "die", "deut\u00b7schen", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Keine Feinde mehr entweihn,", "tokens": ["Kei\u00b7ne", "Fein\u00b7de", "mehr", "ent\u00b7weihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen wir dein Grabmal bauen,", "tokens": ["Wol\u00b7len", "wir", "dein", "Grab\u00b7mal", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreiben deinen Leichenstein.", "tokens": ["Schrei\u00b7ben", "dei\u00b7nen", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oben in den blauen Hallen,", "tokens": ["O\u00b7ben", "in", "den", "blau\u00b7en", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den V\u00e4tern weile du,", "tokens": ["Bei", "den", "V\u00e4\u00b7tern", "wei\u00b7le", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Waffenruf soll schallen", "tokens": ["Un\u00b7ser", "Waf\u00b7fen\u00b7ruf", "soll", "schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis in deine sel'ge Ruh'.", "tokens": ["Bis", "in", "dei\u00b7ne", "sel'\u00b7ge", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}