{"dta.poem.9517": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-gedancken.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kom Cynthia der fr\u00fchling trit heran/", "tokens": ["Kom", "Cyn\u00b7thia", "der", "fr\u00fch\u00b7ling", "trit", "he\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die blume will nicht in der knospe bleiben/", "tokens": ["Die", "blu\u00b7me", "will", "nicht", "in", "der", "knos\u00b7pe", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nachtigal stimmt ihre lieder an/", "tokens": ["Die", "nach\u00b7ti\u00b7gal", "stimmt", "ih\u00b7re", "lie\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und will die zeit mit Liebligkeit vertreiben;", "tokens": ["Und", "will", "die", "zeit", "mit", "Lieb\u00b7lig\u00b7keit", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der weinstock weint zum zeugen seiner lust/", "tokens": ["Der", "wein\u00b7stock", "weint", "zum", "zeu\u00b7gen", "sei\u00b7ner", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Ph\u00f6bus ihn mit neuen augen gr\u00fcsset/", "tokens": ["Da\u00df", "Ph\u00f6\u00b7bus", "ihn", "mit", "neu\u00b7en", "au\u00b7gen", "gr\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Feld/ thal und berg schm\u00fcckt lenden/ schoo\u00df und brust/", "tokens": ["Feld", "/", "thal", "und", "berg", "schm\u00fcckt", "len\u00b7den", "/", "schoo\u00df", "und", "brust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "KON", "NE", "VVFIN", "VVINF", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wenn Flora sie mit buntem munde k\u00fcsset.", "tokens": ["Wenn", "Flo\u00b7ra", "sie", "mit", "bun\u00b7tem", "mun\u00b7de", "k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es r\u00fchren sich die kr\u00e4ffte der natur/", "tokens": ["Es", "r\u00fch\u00b7ren", "sich", "die", "kr\u00e4ff\u00b7te", "der", "na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So durch den frost vor diesem war verschlossen/", "tokens": ["So", "durch", "den", "frost", "vor", "die\u00b7sem", "war", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PDAT", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Was adern hat/ kommt auff der liebe spur/", "tokens": ["Was", "a\u00b7dern", "hat", "/", "kommt", "auff", "der", "lie\u00b7be", "spur", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und was der thau des himmels hat begossen.", "tokens": ["Und", "was", "der", "thau", "des", "him\u00b7mels", "hat", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Cupido reist auff seine laute zu/", "tokens": ["Cu\u00b7pi\u00b7do", "reist", "auff", "sei\u00b7ne", "lau\u00b7te", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die saiten sind des leibes beste sehnen/", "tokens": ["Die", "sai\u00b7ten", "sind", "des", "lei\u00b7bes", "bes\u00b7te", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich/ Cynthia/ verliehre trost und ruh/", "tokens": ["Ich", "/", "Cyn\u00b7thia", "/", "ver\u00b7lieh\u00b7re", "trost", "und", "ruh", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "$(", "VVFIN", "ADJD", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Bewegen dich nicht meine heissen thr\u00e4nen?", "tokens": ["Be\u00b7we\u00b7gen", "dich", "nicht", "mei\u00b7ne", "heis\u00b7sen", "thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Komm Cynthia/ bewege geist und fu\u00df/", "tokens": ["Komm", "Cyn\u00b7thia", "/", "be\u00b7we\u00b7ge", "geist", "und", "fu\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "ADJA", "NN", "KON", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Und komm mit mir in einen schlechten garten/", "tokens": ["Und", "komm", "mit", "mir", "in", "ei\u00b7nen", "schlech\u00b7ten", "gar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der blumen schaar empfindet fast verdru\u00df/", "tokens": ["Der", "blu\u00b7men", "schaar", "emp\u00b7fin\u00b7det", "fast", "ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So lang auff dich/ mein sch\u00f6nes licht/ zu warten", "tokens": ["So", "lang", "auff", "dich", "/", "mein", "sch\u00f6\u00b7nes", "licht", "/", "zu", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Was noch der frost mit schlechten banden dr\u00fcckt/", "tokens": ["Was", "noch", "der", "frost", "mit", "schlech\u00b7ten", "ban\u00b7den", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und eine lusst des weissen nordens bindet/", "tokens": ["Und", "ei\u00b7ne", "lusst", "des", "weis\u00b7sen", "nor\u00b7dens", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wird alsobald trl\u00f6set und erquickt/", "tokens": ["Wird", "al\u00b7so\u00b7bald", "tr\u00b7l\u00f6\u00b7set", "und", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wenn sich der glantz von deiner sonnen findet/", "tokens": ["Wenn", "sich", "der", "glantz", "von", "dei\u00b7ner", "son\u00b7nen", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Trit fr\u00f6lich zu/ denck auff die blumen nicht/", "tokens": ["Trit", "fr\u00f6\u00b7lich", "zu", "/", "denck", "auff", "die", "blu\u00b7men", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "$(", "VVIMP", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Dieselben durch die f\u00fcsse zu verderben/", "tokens": ["Die\u00b7sel\u00b7ben", "durch", "die", "f\u00fcs\u00b7se", "zu", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Es ist ihr wunsch also seyn zuger", "tokens": ["Es", "ist", "ihr", "wunsch", "al\u00b7so", "seyn", "zu\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Und durch den fu\u00df/ der himmlisch ist/ zu sterben.", "tokens": ["Und", "durch", "den", "fu\u00df", "/", "der", "himm\u00b7lisch", "ist", "/", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PTKVZ", "$(", "ART", "ADJD", "VAFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Doch liebest du vielmehr die s\u00fcsse rub/", "tokens": ["Doch", "lie\u00b7best", "du", "viel\u00b7mehr", "die", "s\u00fcs\u00b7se", "rub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und bist gesinnt dich in das gra\u00df zu setzen/", "tokens": ["Und", "bist", "ge\u00b7sinnt", "dich", "in", "das", "gra\u00df", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So l\u00e4st es dir auch diese freyheit zu/", "tokens": ["So", "l\u00e4st", "es", "dir", "auch", "die\u00b7se", "frey\u00b7heit", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PDAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wie solte sie das sch\u00f6ne theil verletzen.", "tokens": ["Wie", "sol\u00b7te", "sie", "das", "sch\u00f6\u00b7ne", "theil", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "So trachte nur/ weil erd und himmel lacht/", "tokens": ["So", "trach\u00b7te", "nur", "/", "weil", "erd", "und", "him\u00b7mel", "lacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Die sch\u00f6ne zeit nicht schl\u00e4frig zu verliehren.", "tokens": ["Die", "sch\u00f6\u00b7ne", "zeit", "nicht", "schl\u00e4f\u00b7rig", "zu", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Erwege doch/ was jener sperling macht/", "tokens": ["Er\u00b7we\u00b7ge", "doch", "/", "was", "je\u00b7ner", "sper\u00b7ling", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PWS", "PDS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Es will der schalck uns in die schule f\u00fchren.", "tokens": ["Es", "will", "der", "schalck", "uns", "in", "die", "schu\u00b7le", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Schau was man dort mit jener schmate thut/", "tokens": ["Schau", "was", "man", "dort", "mit", "je\u00b7ner", "schma\u00b7te", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PIS", "ADV", "APPR", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Man nimt den stock und pfropffet in die krinne/", "tokens": ["Man", "nimt", "den", "stock", "und", "pfropf\u00b7fet", "in", "die", "krin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Es regt in mir sich adern geist und blut/", "tokens": ["Es", "regt", "in", "mir", "sich", "a\u00b7dern", "geist", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PRF", "VVINF", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Komm/ da\u00df ich auch dergleichen spiel beginne.", "tokens": ["Komm", "/", "da\u00df", "ich", "auch", "derg\u00b7lei\u00b7chen", "spiel", "be\u00b7gin\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}