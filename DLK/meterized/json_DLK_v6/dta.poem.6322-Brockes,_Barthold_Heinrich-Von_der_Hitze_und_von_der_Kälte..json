{"dta.poem.6322": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Feuer scheidet, theilt, vereinet, wei\u00df zu l\u00e4utern,", "tokens": ["Das", "Feu\u00b7er", "schei\u00b7det", ",", "theilt", ",", "ver\u00b7ei\u00b7net", ",", "wei\u00df", "zu", "l\u00e4u\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Probiret die Metallen; Ein Chymist", "tokens": ["Pro\u00b7bi\u00b7ret", "die", "Me\u00b7tal\u00b7len", ";", "Ein", "Chy\u00b7mist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ergr\u00fcbelt und erforscht, was in den Kr\u00e4utern", "tokens": ["Er\u00b7gr\u00fc\u00b7belt", "und", "er\u00b7forscht", ",", "was", "in", "den", "Kr\u00e4u\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Mineralien verborgen ist.", "tokens": ["Und", "Mi\u00b7ne\u00b7ra\u00b7li\u00b7en", "ver\u00b7bor\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man sieht, da\u00df C\u00f6rper mehr und minder sich zertrennen,", "tokens": ["Man", "sieht", ",", "da\u00df", "C\u00f6r\u00b7per", "mehr", "und", "min\u00b7der", "sich", "zer\u00b7tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NE", "ADV", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem ein st\u00e4rckeres und ein gelindes Brennen", "tokens": ["Nach\u00b7dem", "ein", "st\u00e4r\u00b7cke\u00b7res", "und", "ein", "ge\u00b7lin\u00b7des", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie dr\u00e4nget und bek\u00e4mpfft: nachdem die Gluht sich mehrt,", "tokens": ["Sie", "dr\u00e4n\u00b7get", "und", "be\u00b7k\u00e4mpfft", ":", "nach\u00b7dem", "die", "Gluht", "sich", "mehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sind ihre Bande auch ver\u00e4ndert und zerst\u00f6hrt.", "tokens": ["Sind", "ih\u00b7re", "Ban\u00b7de", "auch", "ver\u00b7\u00e4n\u00b7dert", "und", "zer\u00b7st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}