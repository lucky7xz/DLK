{"textgrid.poem.49675": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ernst Moritz Arndt", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df deine Treue stark geblieben,", "tokens": ["Da\u00df", "dei\u00b7ne", "Treu\u00b7e", "stark", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Deutschland arm am Boden lag,", "tokens": ["Als", "Deutschland", "arm", "am", "Bo\u00b7den", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wird heut' geredet und geschrieben", "tokens": ["Wird", "heut'", "ge\u00b7re\u00b7det", "und", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klingt beim lauten Festgelag.", "tokens": ["Und", "klingt", "beim", "lau\u00b7ten", "Fest\u00b7ge\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Genehmigte Begeisterungen", "tokens": ["Ge\u00b7neh\u00b7mig\u00b7te", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tut dir ein treuer Diener kund;", "tokens": ["Tut", "dir", "ein", "treu\u00b7er", "Die\u00b7ner", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das N\u00fctzliche, was du gesungen,", "tokens": ["Das", "N\u00fctz\u00b7li\u00b7che", ",", "was", "du", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gilt ihm als eigentlicher Grund.", "tokens": ["Gilt", "ihm", "als", "ei\u00b7gent\u00b7li\u00b7cher", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Wenn so Ergebenheiten stammeln", "tokens": ["Wenn", "so", "Er\u00b7ge\u00b7ben\u00b7hei\u00b7ten", "stam\u00b7meln"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und Hochgef\u00fchl die Herzen bl\u00e4ht,", "tokens": ["Und", "Hoch\u00b7ge\u00b7f\u00fchl", "die", "Her\u00b7zen", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann l\u00e4\u00dft sich auch was telegrammeln", "tokens": ["Dann", "l\u00e4\u00dft", "sich", "auch", "was", "te\u00b7le\u00b7gram\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "PIS", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zum sch\u00f6nen Schlu\u00df an Majest\u00e4t.", "tokens": ["Zum", "sch\u00f6\u00b7nen", "Schlu\u00df", "an", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das mag die vielen nun begl\u00fccken,", "tokens": ["Das", "mag", "die", "vie\u00b7len", "nun", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "PIAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alle Feste laut begehn;", "tokens": ["Die", "al\u00b7le", "Fes\u00b7te", "laut", "be\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns aber la\u00df dein Bildnis schm\u00fccken", "tokens": ["Uns", "a\u00b7ber", "la\u00df", "dein", "Bild\u00b7nis", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Heimatliebe recht verstehn!", "tokens": ["Und", "Hei\u00b7mat\u00b7lie\u00b7be", "recht", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir danken dir in diesen Stunden,", "tokens": ["Wir", "dan\u00b7ken", "dir", "in", "die\u00b7sen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dich dein Glaube nie verlie\u00df,", "tokens": ["Da\u00df", "dich", "dein", "Glau\u00b7be", "nie", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du beim Volke Trost gefunden,", "tokens": ["Da\u00df", "du", "beim", "Vol\u00b7ke", "Trost", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als dich ein Jammerf\u00fcrst verstie\u00df.", "tokens": ["Als", "dich", "ein", "Jam\u00b7mer\u00b7f\u00fcrst", "ver\u00b7stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}