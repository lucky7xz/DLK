{"textgrid.poem.37252": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es stand vor eines Hauses Tor]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stand vor eines Hauses Tor", "tokens": ["Es", "stand", "vor", "ei\u00b7nes", "Hau\u00b7ses", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Esel mit gespitztem Ohr,", "tokens": ["Ein", "E\u00b7sel", "mit", "ge\u00b7spitz\u00b7tem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00e4ute sich sein B\u00fcndel Heu", "tokens": ["Der", "k\u00e4u\u00b7te", "sich", "sein", "B\u00fcn\u00b7del", "Heu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenvoll und still entzwei. \u2013", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "und", "still", "ent\u00b7zwei", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun kommen da und bleiben stehn", "tokens": ["Nun", "kom\u00b7men", "da", "und", "blei\u00b7ben", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der naseweisen Buben zween,", "tokens": ["Der", "na\u00b7se\u00b7wei\u00b7sen", "Bu\u00b7ben", "zween", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die auch sogleich, indem sie lachen,", "tokens": ["Die", "auch", "sog\u00b7leich", ",", "in\u00b7dem", "sie", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verha\u00dfte Redensarten machen,", "tokens": ["Ver\u00b7ha\u00df\u00b7te", "Re\u00b7dens\u00b7ar\u00b7ten", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Womit man denn bezwecken wollte,", "tokens": ["Wo\u00b7mit", "man", "denn", "be\u00b7zwe\u00b7cken", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich der Esel \u00e4rgern sollte. \u2013", "tokens": ["Da\u00df", "sich", "der", "E\u00b7sel", "\u00e4r\u00b7gern", "soll\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dieser hocherfahrne Greis", "tokens": ["Doch", "die\u00b7ser", "ho\u00b7cher\u00b7fahr\u00b7ne", "Greis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschrieb nur einen halben Kreis,", "tokens": ["Be\u00b7schrieb", "nur", "ei\u00b7nen", "hal\u00b7ben", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verhielt sich stumm und zeigte itzt", "tokens": ["Ver\u00b7hielt", "sich", "stumm", "und", "zeig\u00b7te", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seite, wo der Wedel sitzt.", "tokens": ["Die", "Sei\u00b7te", ",", "wo", "der", "We\u00b7del", "sitzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}