{"dta.poem.18616": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber das  C onterfait eines  \n Frauenzimmers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hier dieses Conterfait hat zwar viel Seltenheiten/", "tokens": ["Hier", "die\u00b7ses", "Con\u00b7ter\u00b7fait", "hat", "zwar", "viel", "Sel\u00b7ten\u00b7hei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch Auge sieht es auch wohl vor erdichtet an.", "tokens": ["Manch", "Au\u00b7ge", "sieht", "es", "auch", "wohl", "vor", "er\u00b7dich\u00b7tet", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Schade/ da\u00df die Kunst der Tugend Kostbarkeiten/", "tokens": ["Doch", "Scha\u00b7de", "/", "da\u00df", "die", "Kunst", "der", "Tu\u00b7gend", "Kost\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "KOUS", "ART", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht wie den kleinsten Theil der Sch\u00f6nheit mahlen kan.", "tokens": ["Nicht", "wie", "den", "kleins\u00b7ten", "Theil", "der", "Sch\u00f6n\u00b7heit", "mah\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}