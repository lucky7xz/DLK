{"textgrid.poem.36707": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Blandusische Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandre nur vor, es umgr\u00fcnen dich wild der Sabiner Gebirge,", "tokens": ["Wand\u00b7re", "nur", "vor", ",", "es", "um\u00b7gr\u00fc\u00b7nen", "dich", "wild", "der", "Sa\u00b7bi\u00b7ner", "Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Folge dem Strom, es entragt einsam Licenza dem Fels,", "tokens": ["Fol\u00b7ge", "dem", "Strom", ",", "es", "en\u00b7tragt", "ein\u00b7sam", "Li\u00b7cen\u00b7za", "dem", "Fels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Unter Kastanien erreichst du den Kies, wo der dicht'rische Quell rinnt;", "tokens": ["Un\u00b7ter", "Kas\u00b7ta\u00b7ni\u00b7en", "er\u00b7reichst", "du", "den", "Kies", ",", "wo", "der", "dicht'\u00b7ri\u00b7sche", "Quell", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Trink! noch sprudelt der Quell, aber der Genius fehlt.", "tokens": ["Trink", "!", "noch", "spru\u00b7delt", "der", "Quell", ",", "a\u00b7ber", "der", "Ge\u00b7nius", "fehlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}