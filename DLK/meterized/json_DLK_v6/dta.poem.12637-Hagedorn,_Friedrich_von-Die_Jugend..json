{"dta.poem.12637": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Jugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt auch ich durch Gram und Leid", "tokens": ["Sollt", "auch", "ich", "durch", "Gram", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Leib verzehren,", "tokens": ["Mei\u00b7nen", "Leib", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und des Lebens Fr\u00f6hlichkeit,", "tokens": ["Und", "des", "Le\u00b7bens", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich leb\u2019, entbehren?", "tokens": ["Weil", "ich", "leb'", ",", "ent\u00b7beh\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freunde! nein; es stehet fest,", "tokens": ["Freun\u00b7de", "!", "nein", ";", "es", "ste\u00b7het", "fest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Jugend Ueberrest", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Ue\u00b7ber\u00b7rest"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll mir Lust gew\u00e4hren.", "tokens": ["Soll", "mir", "Lust", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Quellen tausendfacher Lust:", "tokens": ["Quel\u00b7len", "tau\u00b7send\u00b7fa\u00b7cher", "Lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jugend! Sch\u00f6nheit! Liebe!", "tokens": ["Ju\u00b7gend", "!", "Sch\u00f6n\u00b7heit", "!", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jhr erweckt in meiner Brust", "tokens": ["Ihr", "er\u00b7weckt", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmeichelhafte Triebe.", "tokens": ["Schmei\u00b7chel\u00b7haf\u00b7te", "Trie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kein Genu\u00df ergr\u00fcbelt sich;", "tokens": ["Kein", "Ge\u00b7nu\u00df", "er\u00b7gr\u00fc\u00b7belt", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich wei\u00df gnug, indem ich mich", "tokens": ["Ich", "wei\u00df", "gnug", ",", "in\u00b7dem", "ich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Jm Empfinden \u00fcbe.", "tokens": ["Jm", "Emp\u00b7fin\u00b7den", "\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft die Sch\u00f6nen dieser Flur", "tokens": ["La\u00dft", "die", "Sch\u00f6\u00b7nen", "die\u00b7ser", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Wahl erfahren:", "tokens": ["Eu\u00b7re", "Wahl", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich besinge Phyllis nur,", "tokens": ["Ich", "be\u00b7sin\u00b7ge", "Phyl\u00b7lis", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die von achtzehn Jahren,", "tokens": ["Die", "von", "acht\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freundlich, sinnreich, schlau zur Lust,", "tokens": ["Freund\u00b7lich", ",", "sinn\u00b7reich", ",", "schlau", "zur", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df von Stirne, Hals und Brust,", "tokens": ["Wei\u00df", "von", "Stir\u00b7ne", ",", "Hals", "und", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwarz von Aug und Haaren.", "tokens": ["Schwarz", "von", "Aug", "und", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Der mein Thun zu meistern denkt", "tokens": ["Der", "mein", "Thun", "zu", "meis\u00b7tern", "denkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Predigt tauben Ohren.", "tokens": ["Pre\u00b7digt", "tau\u00b7ben", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schm\u00e4hen hat mich nie gekr\u00e4nkt:", "tokens": ["Schm\u00e4\u00b7hen", "hat", "mich", "nie", "ge\u00b7kr\u00e4nkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ist der gebohren,", "tokens": ["Wo", "ist", "der", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Welcher allen wohlgef\u00e4llt?", "tokens": ["Wel\u00b7cher", "al\u00b7len", "wohl\u00b7ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und woraus besteht die Welt?", "tokens": ["Und", "wo\u00b7raus", "be\u00b7steht", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Mehrentheils aus Thoren.", "tokens": ["Meh\u00b7ren\u00b7theils", "aus", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wer den Wehrt der Freyheit kennt,", "tokens": ["Wer", "den", "Wehrt", "der", "Frey\u00b7heit", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt aus ihr die Lehre,", "tokens": ["Nimmt", "aus", "ihr", "die", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df was die Natur verg\u00f6nnt", "tokens": ["Da\u00df", "was", "die", "Na\u00b7tur", "ver\u00b7g\u00f6nnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Wohl vermehre.", "tokens": ["Un\u00b7ser", "Wohl", "ver\u00b7meh\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wi\u00dft: es bleibt ein freyer Mann,", "tokens": ["Wi\u00dft", ":", "es", "bleibt", "ein", "frey\u00b7er", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den kein Kl\u00fcgeln binden kann,", "tokens": ["Den", "kein", "Kl\u00fc\u00b7geln", "bin\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seiner Zeiten Ehre.", "tokens": ["Sei\u00b7ner", "Zei\u00b7ten", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}