{"textgrid.poem.66676": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut in der Stadt schnell im Vor\u00fcbergehen \u2013", "tokens": ["Heut", "in", "der", "Stadt", "schnell", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Luft trotz schwachen Regens dr\u00fcckend schw\u00fcl! \u2013", "tokens": ["Die", "Luft", "trotz", "schwa\u00b7chen", "Re\u00b7gens", "dr\u00fc\u00b7ckend", "schw\u00fcl", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hab ich ein kleines Stra\u00dfenbild gesehen,", "tokens": ["Hab", "ich", "ein", "klei\u00b7nes", "Stra\u00b7\u00dfen\u00b7bild", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erquickt mein \u00fcbermenschliches Gef\u00fchl.", "tokens": ["Er\u00b7quickt", "mein", "\u00fc\u00b7ber\u00b7menschli\u00b7ches", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Polizist hie\u00df einen Kutscher drehen,", "tokens": ["Ein", "Po\u00b7li\u00b7zist", "hie\u00df", "ei\u00b7nen", "Kut\u00b7scher", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der falsch getrabt, in gr\u00f6bstem Z\u00fcrcher Stil.", "tokens": ["Der", "falsch", "ge\u00b7trabt", ",", "in", "gr\u00f6bs\u00b7tem", "Z\u00fcr\u00b7cher", "Stil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Rosselenker schwoll die Zornesader,", "tokens": ["Dem", "Ros\u00b7se\u00b7len\u00b7ker", "schwoll", "die", "Zor\u00b7ne\u00b7sa\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wort gab das Wort, in Flammen stand der Hader.", "tokens": ["Wort", "gab", "das", "Wort", ",", "in", "Flam\u00b7men", "stand", "der", "Ha\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und blitzschnell, eh ein Augenblick verronnen,", "tokens": ["Und", "blitz\u00b7schnell", ",", "eh", "ein", "Au\u00b7gen\u00b7blick", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Peitsche traf die hohe Polizei,", "tokens": ["Die", "Peit\u00b7sche", "traf", "die", "ho\u00b7he", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00d6l go\u00df ins Feuer gleich in ganzen Tonnen", "tokens": ["\u00d6l", "go\u00df", "ins", "Feu\u00b7er", "gleich", "in", "gan\u00b7zen", "Ton\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ma\u00dflose Wut, wahnwitzige Raserei.", "tokens": ["Ma\u00df\u00b7lo\u00b7se", "Wut", ",", "wahn\u00b7wit\u00b7zi\u00b7ge", "Ra\u00b7se\u00b7rei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Fast hat die Hermandad den Bock gewonnen,", "tokens": ["Fast", "hat", "die", "Her\u00b7man\u00b7dad", "den", "Bock", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie h\u00e4lt sich mutig \u2013 rings herum Geschrei!", "tokens": ["Sie", "h\u00e4lt", "sich", "mu\u00b7tig", "\u2013", "rings", "he\u00b7rum", "Ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$(", "ADV", "APZR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die St\u00f6\u00dfe prasseln \u2013 wildes Armgerenke!", "tokens": ["Die", "St\u00f6\u00b7\u00dfe", "pras\u00b7seln", "\u2013", "wil\u00b7des", "Arm\u00b7ge\u00b7ren\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Ordnungsw\u00e4chter packt die Handgelenke.", "tokens": ["Der", "Ord\u00b7nungs\u00b7w\u00e4ch\u00b7ter", "packt", "die", "Hand\u00b7ge\u00b7len\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon wieder frei. Da von der andern Seite", "tokens": ["Schon", "wie\u00b7der", "frei", ".", "Da", "von", "der", "an\u00b7dern", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Knirps von Postmensch klettert hinterr\u00fccks \u2013", "tokens": ["Ein", "Knirps", "von", "Post\u00b7mensch", "klet\u00b7tert", "hin\u00b7ter\u00b7r\u00fccks", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesetzeshelfer in dem blutigen Streite \u2013", "tokens": ["Ge\u00b7set\u00b7ze\u00b7shel\u00b7fer", "in", "dem", "blu\u00b7ti\u00b7gen", "Strei\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und trifft die Wucht des riesigen Genicks", "tokens": ["Und", "trifft", "die", "Wucht", "des", "rie\u00b7si\u00b7gen", "Ge\u00b7nicks"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und bleut mit feiger Faust des Nackens Breite,", "tokens": ["Und", "bleut", "mit", "fei\u00b7ger", "Faust", "des", "Na\u00b7ckens", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Augenpuff beraubt den Mann des Blicks \u2013", "tokens": ["Ein", "Au\u00b7gen\u00b7puff", "be\u00b7raubt", "den", "Mann", "des", "Blicks", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Polizist legt rasch die Eisenschellen", "tokens": ["Der", "Po\u00b7li\u00b7zist", "legt", "rasch", "die", "Ei\u00b7sen\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihm an und rei\u00dft vom Thron den Staatsrebellen.", "tokens": ["Ihm", "an", "und", "rei\u00dft", "vom", "Thron", "den", "Staats\u00b7re\u00b7bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Angebinde noch von Gafferf\u00e4usten", "tokens": ["Ein", "An\u00b7ge\u00b7bin\u00b7de", "noch", "von", "Gaf\u00b7fer\u00b7f\u00e4us\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem wehrlos Hingeschleiften ins Gesicht:", "tokens": ["Dem", "wehr\u00b7los", "Hin\u00b7ge\u00b7schleif\u00b7ten", "ins", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbder chaibe Hund! Wie konnt' er sich erdreusten!\u00ab", "tokens": ["\u00bb", "der", "chai\u00b7be", "Hund", "!", "Wie", "konnt'", "er", "sich", "erd\u00b7reus\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Wache fort und fort zum Weltgericht!", "tokens": ["Zur", "Wa\u00b7che", "fort", "und", "fort", "zum", "Welt\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Szene liest sich morgen in den \u00bbNeuesten\u00ab", "tokens": ["Die", "Sze\u00b7ne", "liest", "sich", "mor\u00b7gen", "in", "den", "\u00bb", "Neu\u00b7es\u00b7ten", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "$(", "NN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ganz h\u00fcbsch. Der Metzger zur Frau Metzgern spricht:", "tokens": ["Ganz", "h\u00fcbsch", ".", "Der", "Metz\u00b7ger", "zur", "Frau", "Metz\u00b7gern", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "\u00bbmietskutscherblut flo\u00df in der Storchengasse.", "tokens": ["\u00bb", "miets\u00b7kut\u00b7scher\u00b7blut", "flo\u00df", "in", "der", "Stor\u00b7chen\u00b7gas\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geschieht ihm recht. So'n Polizist hat Rasse.\u00ab", "tokens": ["Ge\u00b7schieht", "ihm", "recht", ".", "So'n", "Po\u00b7li\u00b7zist", "hat", "Ras\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "NN", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und weiter nichts? Welch winzig Zwischenf\u00e4llchen!", "tokens": ["Und", "wei\u00b7ter", "nichts", "?", "Welch", "win\u00b7zig", "Zwi\u00b7schen\u00b7f\u00e4ll\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$.", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist so was \u00fcberhaupt der Rede wert?", "tokens": ["Ist", "so", "was", "\u00fc\u00b7ber\u00b7haupt", "der", "Re\u00b7de", "wert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entr\u00fcstungsr\u00f6\u00dflein, bleib in deinem St\u00e4llchen!", "tokens": ["Ent\u00b7r\u00fcs\u00b7tungs\u00b7r\u00f6\u00df\u00b7lein", ",", "bleib", "in", "dei\u00b7nem", "St\u00e4ll\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zornb\u00fcbchen, setz dich nicht aufs hohe Pferd!", "tokens": ["Zorn\u00b7b\u00fcb\u00b7chen", ",", "setz", "dich", "nicht", "aufs", "ho\u00b7he", "Pferd", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Das fehlte! Doch aus winzigen Schneeb\u00e4llchen", "tokens": ["Das", "fehl\u00b7te", "!", "Doch", "aus", "win\u00b7zi\u00b7gen", "Schnee\u00b7b\u00e4ll\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "W\u00e4chst lautlos die Lawine, die verheert", "tokens": ["W\u00e4chst", "laut\u00b7los", "die", "La\u00b7wi\u00b7ne", ",", "die", "ver\u00b7heert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und j\u00e4h verschlingt, die wei\u00dfe Schneehy\u00e4ne,", "tokens": ["Und", "j\u00e4h", "ver\u00b7schlingt", ",", "die", "wei\u00b7\u00dfe", "Schnee\u00b7hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Hirten samt Schalmei ... ", "tokens": ["Den", "Hir\u00b7ten", "samt", "Schal\u00b7mei", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}