{"textgrid.poem.60102": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war ein M\u00e4nnchen, fein und hold", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein M\u00e4nnchen, fein und hold", "tokens": ["Es", "war", "ein", "M\u00e4nn\u00b7chen", ",", "fein", "und", "hold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Sitten und Gebehrden;", "tokens": ["Von", "Sit\u00b7ten", "und", "Ge\u00b7behr\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm tr\u00e4umt' es in der Nacht, er sollt'", "tokens": ["Ihm", "tr\u00e4umt'", "es", "in", "der", "Nacht", ",", "er", "sollt'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein zweyter ", "tokens": ["Ein", "zwey\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Er nahm sich eine ", "tokens": ["Er", "nahm", "sich", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die oft ihr blaues Wunder sah,", "tokens": ["Die", "oft", "ihr", "blau\u00b7es", "Wun\u00b7der", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn, klein in Holz geschnitten,", "tokens": ["Wenn", ",", "klein", "in", "Holz", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die alten Riesen stritten.", "tokens": ["Die", "al\u00b7ten", "Rie\u00b7sen", "strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun fand er die Philister zwar", "tokens": ["Nun", "fand", "er", "die", "Phi\u00b7lis\u00b7ter", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allm\u00e4hlig ausgestorben,", "tokens": ["All\u00b7m\u00e4h\u00b7lig", "aus\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich selber, trotz dem langen Haar,", "tokens": ["Sich", "sel\u00b7ber", ",", "trotz", "dem", "lan\u00b7gen", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum starken Mann verdorben;", "tokens": ["Zum", "star\u00b7ken", "Mann", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und auch die W\u00e4lderchen umher", "tokens": ["Und", "auch", "die", "W\u00e4l\u00b7der\u00b7chen", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von L\u00f6wen und von Tigern leer;", "tokens": ["Von", "L\u00f6\u00b7wen", "und", "von", "Ti\u00b7gern", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Konnt aber ihm geb\u00fchren", "tokens": ["Konnt", "a\u00b7ber", "ihm", "ge\u00b7b\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch etwas aufzusp\u00fcren.", "tokens": ["Noch", "et\u00b7was", "auf\u00b7zu\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sofort beginnt er seinen Lauf", "tokens": ["So\u00b7fort", "be\u00b7ginnt", "er", "sei\u00b7nen", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im leichten Moder\u00f6cklein;", "tokens": ["Im", "leich\u00b7ten", "Mo\u00b7de\u00b7r\u00f6ck\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er sucht ein Ungeheuer auf;", "tokens": ["Er", "sucht", "ein", "Un\u00b7ge\u00b7heu\u00b7er", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begegnet ihm ein B\u00f6cklein:", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "ein", "B\u00f6c\u00b7klein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das arme B\u00f6cklein greift er bald,", "tokens": ["Das", "ar\u00b7me", "B\u00f6c\u00b7klein", "greift", "er", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erw\u00fcrgt, zerrei\u00dft es mit Gewalt,", "tokens": ["Er\u00b7w\u00fcrgt", ",", "zer\u00b7rei\u00dft", "es", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie einen grimmen Leuen,", "tokens": ["Wie", "ei\u00b7nen", "grim\u00b7men", "Leu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein M\u00e4dchen zu erfreuen.", "tokens": ["Sein", "M\u00e4d\u00b7chen", "zu", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch in der Folge wollt's ihm schier", "tokens": ["Doch", "in", "der", "Fol\u00b7ge", "wollt's", "ihm", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Abentheuern mangeln:", "tokens": ["An", "A\u00b7bent\u00b7heu\u00b7ern", "man\u00b7geln", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hob er einst die Kammerth\u00fcr", "tokens": ["Da", "hob", "er", "einst", "die", "Kam\u00b7mert\u00b7h\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des M\u00e4dchen aus den Angeln,", "tokens": ["Des", "M\u00e4d\u00b7chen", "aus", "den", "An\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und trug sie, nach gemeiner Sag,", "tokens": ["Und", "trug", "sie", ",", "nach", "ge\u00b7mei\u00b7ner", "Sag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinauf auf einen Taubenschlag,", "tokens": ["Hin\u00b7auf", "auf", "ei\u00b7nen", "Tau\u00b7ben\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "De\u00df, als die Mutter zankte,", "tokens": ["De\u00df", ",", "als", "die", "Mut\u00b7ter", "zank\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das M\u00e4dchen sch\u00f6n ihm dankte.", "tokens": ["Das", "M\u00e4d\u00b7chen", "sch\u00f6n", "ihm", "dank\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht selten sprach er r\u00e4thselhaft", "tokens": ["Nicht", "sel\u00b7ten", "sprach", "er", "r\u00e4th\u00b7sel\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ganz gemeinen Dingen;", "tokens": ["Von", "ganz", "ge\u00b7mei\u00b7nen", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es fehlt' ihm einzig nur die Kraft,", "tokens": ["Es", "fehlt'", "ihm", "ein\u00b7zig", "nur", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die F\u00fcchse zu bezwingen:", "tokens": ["Die", "F\u00fcch\u00b7se", "zu", "be\u00b7zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da jagt' er einen Feuerbrand", "tokens": ["Da", "jagt'", "er", "ei\u00b7nen", "Feu\u00b7er\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinem Pudel durch das Land;", "tokens": ["Mit", "sei\u00b7nem", "Pu\u00b7del", "durch", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Himmel lie\u00df in Gnaden", "tokens": ["Der", "Him\u00b7mel", "lie\u00df", "in", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Fr\u00fcchte sonder Schaden.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "son\u00b7der", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weswegen man ihm nichts verdarb", "tokens": ["Wes\u00b7we\u00b7gen", "man", "ihm", "nichts", "ver\u00b7darb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Augen und an Locken;", "tokens": ["An", "Au\u00b7gen", "und", "an", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als er sanft im Bettlein starb,", "tokens": ["Und", "als", "er", "sanft", "im", "Bet\u00b7tlein", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb Alles unerschrocken.", "tokens": ["Blieb", "Al\u00b7les", "un\u00b7er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dies M\u00e4hrchen hab ich euch geweiht,", "tokens": ["Dies", "M\u00e4hr\u00b7chen", "hab", "ich", "euch", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr kleinen Simsons unsrer Zeit!", "tokens": ["Ihr", "klei\u00b7nen", "Sim\u00b7sons", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und euern Amazonen,", "tokens": ["Und", "eu\u00b7ern", "A\u00b7maz\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die k\u00fcssend euch belohnen.", "tokens": ["Die", "k\u00fcs\u00b7send", "euch", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}