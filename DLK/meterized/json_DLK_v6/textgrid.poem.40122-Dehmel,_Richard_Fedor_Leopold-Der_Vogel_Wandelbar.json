{"textgrid.poem.40122": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Der Vogel Wandelbar", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War einst ein V\u00f6glein Wandelbar,", "tokens": ["War", "einst", "ein", "V\u00f6\u00b7glein", "Wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an welchem Alles seltsam war.", "tokens": ["an", "wel\u00b7chem", "Al\u00b7les", "selt\u00b7sam", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein rechter Wildfang wollt' es sein", "tokens": ["Ein", "rech\u00b7ter", "Wild\u00b7fang", "wollt'", "es", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und hatte doch ein Humpelbein,", "tokens": ["und", "hat\u00b7te", "doch", "ein", "Hum\u00b7pel\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "das arme lust'ge V\u00f6glein.", "tokens": ["das", "ar\u00b7me", "lust'\u00b7ge", "V\u00f6\u00b7glein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Allein das V\u00f6glein hatte auch", "tokens": ["Al\u00b7lein", "das", "V\u00f6\u00b7glein", "hat\u00b7te", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein wundersam Gefieder;", "tokens": ["ein", "wun\u00b7der\u00b7sam", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das schillerte wie Purpurschaum,", "tokens": ["das", "schil\u00b7ler\u00b7te", "wie", "Pur\u00b7pur\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und auf der Brust der weiche Flaum", "tokens": ["und", "auf", "der", "Brust", "der", "wei\u00b7che", "Flaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wie ein Perlmuttermieder.", "tokens": ["wie", "ein", "Perl\u00b7mut\u00b7ter\u00b7mie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Vom vielen Zwitschern eigner Art", "tokens": ["Vom", "vie\u00b7len", "Zwit\u00b7schern", "eig\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Schn\u00e4blein ihm ganz silbrig ward,", "tokens": ["das", "Schn\u00e4b\u00b7lein", "ihm", "ganz", "silb\u00b7rig", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und seiner Aeuglein Scheinen", "tokens": ["und", "sei\u00b7ner", "A\u00b7e\u00b7u\u00b7glein", "Schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gar lieblich launisch wechselte", "tokens": ["gar", "lieb\u00b7lich", "lau\u00b7nisch", "wech\u00b7sel\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "gleich blauen Edelsteinen.", "tokens": ["gleich", "blau\u00b7en", "E\u00b7del\u00b7stei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So trug sich V\u00f6glein Wandelbar", "tokens": ["So", "trug", "sich", "V\u00f6\u00b7glein", "Wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Au\u00dfen sonderlich f\u00fcrwahr,", "tokens": ["von", "Au\u00b7\u00dfen", "son\u00b7der\u00b7lich", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch was das Sonderlichste war:", "tokens": ["doch", "was", "das", "Son\u00b7der\u00b7lichs\u00b7te", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "tief Innen trug's ", "tokens": ["tief", "In\u00b7nen", "trug's"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "ein Herz von lautrem Golde!", "tokens": ["ein", "Herz", "von", "laut\u00b7rem", "Gol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Alles war dem V\u00f6glein gut,", "tokens": ["Und", "Al\u00b7les", "war", "dem", "V\u00f6\u00b7glein", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie's h\u00fcpfelte und gl\u00e4nzte, \u2013", "tokens": ["wie's", "h\u00fcp\u00b7fel\u00b7te", "und", "gl\u00e4nz\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "$("], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "und Jeder nahm's in seine Hut:", "tokens": ["und", "Je\u00b7der", "nahm's", "in", "sei\u00b7ne", "Hut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "solang 's im sichern Hofe sa\u00df,", "tokens": ["so\u00b7lang", "'s", "im", "si\u00b7chern", "Ho\u00b7fe", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "er ihm das Nest umgrenzte.", "tokens": ["er", "ihm", "das", "Nest", "um\u00b7grenz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bis unser V\u00f6glein langsam", "tokens": ["Bis", "un\u00b7ser", "V\u00f6\u00b7glein", "lang\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sich wuchs zu einem Vogel aus;", "tokens": ["sich", "wuchs", "zu", "ei\u00b7nem", "Vo\u00b7gel", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da mu\u00dft' es aus dem warmen Haus", "tokens": ["da", "mu\u00dft'", "es", "aus", "dem", "war\u00b7men", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "allein ins weite Land hinaus.", "tokens": ["al\u00b7lein", "ins", "wei\u00b7te", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das schien ihm, ach, so bangsam.", "tokens": ["Das", "schien", "ihm", ",", "ach", ",", "so", "bang\u00b7sam", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ITJ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Andern liefen gar so schnell,", "tokens": ["Die", "An\u00b7dern", "lie\u00b7fen", "gar", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Ihre zu erjagen;", "tokens": ["das", "Ih\u00b7re", "zu", "er\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da kommt mit seinem Wackelschritt", "tokens": ["da", "kommt", "mit", "sei\u00b7nem", "Wa\u00b7ckel\u00b7schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "solch armes Entlein nicht gut ", "tokens": ["solch", "ar\u00b7mes", "Ent\u00b7lein", "nicht", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "PTKNEG", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und \u2013 mu\u00df den Spott noch tragen.", "tokens": ["und", "\u2013", "mu\u00df", "den", "Spott", "noch", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie flie\u00dfen es und traten es", "tokens": ["Sie", "flie\u00b7\u00dfen", "es", "und", "tra\u00b7ten", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und rupften es gescheit,", "tokens": ["und", "rupf\u00b7ten", "es", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und in dem wilden Dr\u00e4ngen", "tokens": ["und", "in", "dem", "wil\u00b7den", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "blieb bald sein schutzlos Schimmerkleid", "tokens": ["blieb", "bald", "sein", "schutz\u00b7los", "Schim\u00b7mer\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "an Busch und Dornen h\u00e4ngen.", "tokens": ["an", "Busch", "und", "Dor\u00b7nen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zwar Mancher blieb auch stehen:", "tokens": ["Zwar", "Man\u00b7cher", "blieb", "auch", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "vermahnten dann und schalten", "tokens": ["ver\u00b7mahn\u00b7ten", "dann", "und", "schal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "den ungeschickten Wandelbar,", "tokens": ["den", "un\u00b7ge\u00b7schick\u00b7ten", "Wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wu\u00dften doch, da\u00df ", "tokens": ["und", "wu\u00df\u00b7ten", "doch", ",", "da\u00df"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "und \u2013 blieben selbst die Alten!", "tokens": ["und", "\u2013", "blie\u00b7ben", "selbst", "die", "Al\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch endlich war es ihm gegl\u00fcckt,", "tokens": ["Doch", "end\u00b7lich", "war", "es", "ihm", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit letzten Kr\u00e4ften, arg zerpfl\u00fcckt,", "tokens": ["mit", "letz\u00b7ten", "Kr\u00e4f\u00b7ten", ",", "arg", "zer\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein B\u00e4umlein zu erschwingen;", "tokens": ["ein", "B\u00e4um\u00b7lein", "zu", "er\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da dacht' er endlich auszuruhn", "tokens": ["da", "dacht'", "er", "end\u00b7lich", "aus\u00b7zu\u00b7ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und sich in Schutz zu bringen.", "tokens": ["und", "sich", "in", "Schutz", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Verwandelt war nun ganz und gar", "tokens": ["Ver\u00b7wan\u00b7delt", "war", "nun", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der arme Vogel Wandelbar,", "tokens": ["der", "ar\u00b7me", "Vo\u00b7gel", "Wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so funkelnd einst; nur hier und da", "tokens": ["so", "fun\u00b7kelnd", "einst", ";", "nur", "hier", "und", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "$.", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein glei\u00dfend Federlein noch sah", "tokens": ["ein", "glei\u00b7\u00dfend", "Fe\u00b7der\u00b7lein", "noch", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "aus seinem grauen Kittel.", "tokens": ["aus", "sei\u00b7nem", "grau\u00b7en", "Kit\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und auch der Aeuglein helles Licht", "tokens": ["Und", "auch", "der", "A\u00b7e\u00b7u\u00b7glein", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "war bla\u00df wie welk Vergi\u00dfmeinnicht;", "tokens": ["war", "bla\u00df", "wie", "welk", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "war ihm geblieben noch vonklein,", "tokens": ["war", "ihm", "ge\u00b7blie\u00b7ben", "noch", "von\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "wenn's auch nur schwach noch zirpte.", "tokens": ["wenn's", "auch", "nur", "schwach", "noch", "zirp\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So sa\u00df er fern denn vom Gew\u00fchl", "tokens": ["So", "sa\u0