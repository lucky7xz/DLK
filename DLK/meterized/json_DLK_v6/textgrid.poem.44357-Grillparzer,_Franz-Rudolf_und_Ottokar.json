{"textgrid.poem.44357": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Rudolf und Ottokar", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im ersiegten Ungarlager", "tokens": ["Im", "er\u00b7sieg\u00b7ten", "Un\u00b7gar\u00b7la\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der B\u00f6hmen hoher K\u00f6nig,", "tokens": ["Steht", "der", "B\u00f6h\u00b7men", "ho\u00b7her", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines Landes Hort und S\u00e4ule,", "tokens": ["Sei\u00b7nes", "Lan\u00b7des", "Hort", "und", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrst Przemisl Ottokar.", "tokens": ["F\u00fcrst", "Pr\u00b7ze\u00b7misl", "Ot\u00b7to\u00b7kar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hoch empor das Haupt geworfen,", "tokens": ["Hoch", "em\u00b7por", "das", "Haupt", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht er da im blanken Harnisch,", "tokens": ["Steht", "er", "da", "im", "blan\u00b7ken", "Har\u00b7nisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz, vom unbehelmten Scheitel,", "tokens": ["Schwarz", ",", "vom", "un\u00b7be\u00b7helm\u00b7ten", "Schei\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallt herab sein dunkles Haar.", "tokens": ["Wallt", "her\u00b7ab", "sein", "dunk\u00b7les", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mit aufgespannten Sinnen", "tokens": ["Und", "mit", "auf\u00b7ge\u00b7spann\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht die Flucht er seiner Feinde,", "tokens": ["Sieht", "die", "Flucht", "er", "sei\u00b7ner", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt den Jubel er der Seinen,", "tokens": ["H\u00f6rt", "den", "Ju\u00b7bel", "er", "der", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlt er das Gef\u00fchl des Siegs.", "tokens": ["F\u00fchlt", "er", "das", "Ge\u00b7f\u00fchl", "des", "Siegs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da kommt Rudolf angeschritten,", "tokens": ["Da", "kommt", "Ru\u00b7dolf", "an\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der von Habsburg, Schenk des K\u00f6nigs,", "tokens": ["Der", "von", "Habs\u00b7burg", ",", "Schenk", "des", "K\u00f6\u00b7nigs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Degen in der Scheide,", "tokens": ["Sei\u00b7nen", "De\u00b7gen", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen zweiten in der Hand.", "tokens": ["Ei\u00b7nen", "zwei\u00b7ten", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und zu seines K\u00f6nigs F\u00fc\u00dfen", "tokens": ["Und", "zu", "sei\u00b7nes", "K\u00f6\u00b7nigs", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Legt er hin die blanke Waffe,", "tokens": ["Legt", "er", "hin", "die", "blan\u00b7ke", "Waf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprechend: \u00bbnimm dies Siegeszeichen,", "tokens": ["Spre\u00b7chend", ":", "\u00bb", "nimm", "dies", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "$(", "VVIMP", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm des Ungark\u00f6nigs Schwert!", "tokens": ["Nimm", "des", "Un\u00b7gar\u00b7k\u00f6\u00b7nigs", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf der Flucht hat ers verloren", "tokens": ["Auf", "der", "Flucht", "hat", "ers", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was ihm dies Schwert gewonnen,", "tokens": ["Und", "was", "ihm", "dies", "Schwert", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "\u00d6streich und die Mark von Steier", "tokens": ["\u00d6s\u00b7treich", "und", "die", "Mark", "von", "Stei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist jetzt dein, wie dieses Schwert.\u00ab", "tokens": ["Ist", "jetzt", "dein", ",", "wie", "die\u00b7ses", "Schwert", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "$,", "PWAV", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Rasch darnach mit beiden H\u00e4nden", "tokens": ["Rasch", "dar\u00b7nach", "mit", "bei\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Greift der F\u00fcrst und h\u00e4lts und jubelnd", "tokens": ["Greift", "der", "F\u00fcrst", "und", "h\u00e4lts", "und", "ju\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NE", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft er aus: \u00bbmein Feind im Staube!", "tokens": ["Ruft", "er", "aus", ":", "\u00bb", "mein", "Feind", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hebt die Hand und mit dem Finger,", "tokens": ["Hebt", "die", "Hand", "und", "mit", "dem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch gepanzert aus dem Treffen,", "tokens": ["Noch", "ge\u00b7pan\u00b7zert", "aus", "dem", "Tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt er in die H\u00f6h und spricht:", "tokens": ["Zeigt", "er", "in", "die", "H\u00f6h", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Der gew\u00e4ltigt die Gewaltgen,", "tokens": ["Der", "ge\u00b7w\u00e4l\u00b7tigt", "die", "Ge\u00b7walt\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem das Gl\u00fcck des B\u00f6hmenk\u00f6nigs,", "tokens": ["Dem", "das", "Gl\u00fcck", "des", "B\u00f6h\u00b7men\u00b7k\u00f6\u00b7nigs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was des Ungark\u00f6nigs Gl\u00fcck.\u00ab", "tokens": ["Was", "des", "Un\u00b7gar\u00b7k\u00f6\u00b7nigs", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und der stolze F\u00fcrst der B\u00f6hmen", "tokens": ["Und", "der", "stol\u00b7ze", "F\u00fcrst", "der", "B\u00f6h\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttert leis in sich zusammen,", "tokens": ["Sch\u00fct\u00b7tert", "leis", "in", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht auf ihn jetzt, der gesprochen,", "tokens": ["Sieht", "auf", "ihn", "jetzt", ",", "der", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt aufs Schwert in seiner Hand.", "tokens": ["Jetzt", "aufs", "Schwert", "in", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und mit einem Blick zum Himmel", "tokens": ["Und", "mit", "ei\u00b7nem", "Blick", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinkt er nieder auf die Kniee,", "tokens": ["Sinkt", "er", "nie\u00b7der", "auf", "die", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt das Schwert aus seinen H\u00e4nden", "tokens": ["Legt", "das", "Schwert", "aus", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die H\u00e4nde auf die Brust.", "tokens": ["Und", "die", "H\u00e4n\u00b7de", "auf", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tief gesenkt das Haupt zur Erde", "tokens": ["Tief", "ge\u00b7senkt", "das", "Haupt", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kniet er auf dem Siegesfelde,", "tokens": ["Kniet", "er", "auf", "dem", "Sie\u00b7ges\u00b7fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nah bei ihm der Graf von Habsburg,", "tokens": ["Nah", "bei", "ihm", "der", "Graf", "von", "Habs\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiter weg sein ganzes Heer.", "tokens": ["Wei\u00b7ter", "weg", "sein", "gan\u00b7zes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}