{"dta.poem.2824": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Einverstandene haben sich nichts zu sagen;", "tokens": ["Zwei", "Ein\u00b7ver\u00b7stan\u00b7de\u00b7ne", "ha\u00b7ben", "sich", "nichts", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PRF", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Antwort wissen sie zum voraus eh' sie fragen.", "tokens": ["Die", "Ant\u00b7wort", "wis\u00b7sen", "sie", "zum", "vo\u00b7raus", "eh'", "sie", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADV", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo aber zweie sich in keinem Punkt verstehn,", "tokens": ["Wo", "a\u00b7ber", "zwei\u00b7e", "sich", "in", "kei\u00b7nem", "Punkt", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird die Verst\u00e4ndigung in leeren Streit ausgehn.", "tokens": ["Wird", "die", "Ver\u00b7st\u00e4n\u00b7di\u00b7gung", "in", "lee\u00b7ren", "Streit", "aus\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was also fordert und erm\u00f6glicht Menschenwort?", "tokens": ["Was", "al\u00b7so", "for\u00b7dert", "und", "er\u00b7m\u00f6g\u00b7licht", "Men\u00b7schen\u00b7wort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halb Misverst\u00e4ndnis, halb Verst\u00e4ndnis, hier und dort.", "tokens": ["Halb", "Mis\u00b7ver\u00b7st\u00e4nd\u00b7nis", ",", "halb", "Ver\u00b7st\u00e4nd\u00b7nis", ",", "hier", "und", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "ADJD", "NN", "$,", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}