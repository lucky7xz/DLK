{"textgrid.poem.40788": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dfe Rosse, ungeduldig, stampfen vor dem Kaiserschlo\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe Rosse, ungeduldig, stampfen vor dem Kaiserschlo\u00df,", "tokens": ["Wei\u00b7\u00dfe", "Ros\u00b7se", ",", "un\u00b7ge\u00b7dul\u00b7dig", ",", "stamp\u00b7fen", "vor", "dem", "Kai\u00b7ser\u00b7schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Unten harrt die Staatskarosse und der Diener goldner Tro\u00df;", "tokens": ["Un\u00b7ten", "harrt", "die", "Staats\u00b7ka\u00b7ros\u00b7se", "und", "der", "Die\u00b7ner", "gold\u00b7ner", "Tro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Oben in der Burg Gem\u00e4chern weilt die junge Kaiserin,", "tokens": ["O\u00b7ben", "in", "der", "Burg", "Ge\u00b7m\u00e4\u00b7chern", "weilt", "die", "jun\u00b7ge", "Kai\u00b7se\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Festlich zu dem Kirchenzuge schm\u00fcckend sich mit bangem Sinn.", "tokens": ["Fest\u00b7lich", "zu", "dem", "Kir\u00b7chen\u00b7zu\u00b7ge", "schm\u00fc\u00b7ckend", "sich", "mit", "ban\u00b7gem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbm\u00e4dchen, gib mir an den Busen jenes Kreuz rubinenroth,", "tokens": ["\u00bb", "m\u00e4d\u00b7chen", ",", "gib", "mir", "an", "den", "Bu\u00b7sen", "je\u00b7nes", "Kreuz", "ru\u00b7bi\u00b7nen\u00b7roth", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df mein Auge sich gew\u00f6hne oft zu schauen Kreuz und Noth!", "tokens": ["Da\u00df", "mein", "Au\u00b7ge", "sich", "ge\u00b7w\u00f6h\u00b7ne", "oft", "zu", "schau\u00b7en", "Kreuz", "und", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Flecht' ins Haar mir jene Perlen, da\u00df sie meinen Blicken fern,", "tokens": ["Flecht'", "ins", "Haar", "mir", "je\u00b7ne", "Per\u00b7len", ",", "da\u00df", "sie", "mei\u00b7nen", "Bli\u00b7cken", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PDAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn an meines Volkes Thr\u00e4nen mahnen sie mich allzugern!", "tokens": ["Denn", "an", "mei\u00b7nes", "Vol\u00b7kes", "Thr\u00e4\u00b7nen", "mah\u00b7nen", "sie", "mich", "all\u00b7zu\u00b7gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Lege mir an Brust und Nacken Diamant und Edelstein,", "tokens": ["Le\u00b7ge", "mir", "an", "Brust", "und", "Na\u00b7cken", "Di\u00b7a\u00b7mant", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df doch etwas an dem Busen sei, nach F\u00fcrstenart, von Stein!", "tokens": ["Da\u00df", "doch", "et\u00b7was", "an", "dem", "Bu\u00b7sen", "sei", ",", "nach", "F\u00fcrs\u00b7ten\u00b7art", ",", "von", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Reiche mir den Ring der Liebe, da\u00df sein goldnes festes Band", "tokens": ["Rei\u00b7che", "mir", "den", "Ring", "der", "Lie\u00b7be", ",", "da\u00df", "sein", "gold\u00b7nes", "fes\u00b7tes", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Vor des schweren Zepters Schwielen sch\u00fctze meine zarte Hand!", "tokens": ["Vor", "des", "schwe\u00b7ren", "Zep\u00b7ters", "Schwie\u00b7len", "sch\u00fct\u00b7ze", "mei\u00b7ne", "zar\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Dr\u00fccke meiner Ahnen Krone gut mir in das weiche Haar!", "tokens": ["Dr\u00fc\u00b7cke", "mei\u00b7ner", "Ah\u00b7nen", "Kro\u00b7ne", "gut", "mir", "in", "das", "wei\u00b7che", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ach, nicht fest auf jenem Haupte ruht ihr goldner Reif, f\u00fcrwahr,", "tokens": ["Ach", ",", "nicht", "fest", "auf", "je\u00b7nem", "Haup\u00b7te", "ruht", "ihr", "gold\u00b7ner", "Reif", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo die weiche seidne Locke um den Rang mit ihr noch kriegt,", "tokens": ["Wo", "die", "wei\u00b7che", "seid\u00b7ne", "Lo\u00b7cke", "um", "den", "Rang", "mit", "ihr", "noch", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und vielleicht in solchem Kampfe wunderbar der Kron' obsiegt!", "tokens": ["Und", "viel\u00b7leicht", "in", "sol\u00b7chem", "Kamp\u00b7fe", "wun\u00b7der\u00b7bar", "der", "Kron'", "ob\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Hefte fest den Purpurmantel! Wie ertr\u00e4gt das schwache Weib", "tokens": ["Hef\u00b7te", "fest", "den", "Pur\u00b7pur\u00b7man\u00b7tel", "!", "Wie", "er\u00b7tr\u00e4gt", "das", "schwa\u00b7che", "Weib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seine Last, die Heldenm\u00e4nnern niederbog den kr\u00e4ft'gen Leib?", "tokens": ["Sei\u00b7ne", "Last", ",", "die", "Hel\u00b7den\u00b7m\u00e4n\u00b7nern", "nie\u00b7der\u00b7bog", "den", "kr\u00e4ft'\u00b7gen", "Leib", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Pagen, fa\u00dft die goldne Schleppe! Wohl bedarf ich ja der Hand,", "tokens": ["Pa\u00b7gen", ",", "fa\u00dft", "die", "gold\u00b7ne", "Schlep\u00b7pe", "!", "Wohl", "be\u00b7darf", "ich", "ja", "der", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die mir liebreich tragen helfe meines Purpurs schwer Gewand.", "tokens": ["Die", "mir", "lieb\u00b7reich", "tra\u00b7gen", "hel\u00b7fe", "mei\u00b7nes", "Pur\u00b7purs", "schwer", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Reicht mir einen blanken Spiegel! \u2013 Doch im Glase aufgegl\u00fcht", "tokens": ["Reicht", "mir", "ei\u00b7nen", "blan\u00b7ken", "Spie\u00b7gel", "!", "\u2013", "Doch", "im", "Gla\u00b7se", "auf\u00b7ge\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "KON", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Winkt ein Fr\u00fchling, der voll Lilien, voll von s\u00fc\u00dfen Rosen bl\u00fcht!", "tokens": ["Winkt", "ein", "Fr\u00fch\u00b7ling", ",", "der", "voll", "Li\u00b7li\u00b7en", ",", "voll", "von", "s\u00fc\u00b7\u00dfen", "Ro\u00b7sen", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+---+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ach, der Lenz, der waffenlose, mild und l\u00e4chelnd ist zu sehn,", "tokens": ["Ach", ",", "der", "Lenz", ",", "der", "waf\u00b7fen\u00b7lo\u00b7se", ",", "mild", "und", "l\u00e4\u00b7chelnd", "ist", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wo ein Fels im Morgenrothe majest\u00e4tisch sollte stehn!", "tokens": ["Wo", "ein", "Fels", "im", "Mor\u00b7gen\u00b7ro\u00b7the", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "soll\u00b7te", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Denn ihr finstres ernstes Antlitz sch\u00fcttelt meine Zeit voll Schmerz!", "tokens": ["Denn", "ihr", "finst\u00b7res", "erns\u00b7tes", "Ant\u00b7litz", "sch\u00fct\u00b7telt", "mei\u00b7ne", "Zeit", "voll", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihren Unmuth zu besiegen frommte eine Hand von Erz!", "tokens": ["Ih\u00b7ren", "Un\u00b7muth", "zu", "be\u00b7sie\u00b7gen", "fromm\u00b7te", "ei\u00b7ne", "Hand", "von", "Erz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch ich kann die finstren Locken und des Grames Faltenspur", "tokens": ["Doch", "ich", "kann", "die", "finst\u00b7ren", "Lo\u00b7cken", "und", "des", "Gra\u00b7mes", "Fal\u00b7ten\u00b7spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ihr mit weicher Hand gelinde streicheln aus dem Antlitz nur!\u00ab", "tokens": ["Ihr", "mit", "wei\u00b7cher", "Hand", "ge\u00b7lin\u00b7de", "strei\u00b7cheln", "aus", "dem", "Ant\u00b7litz", "nur", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJA", "VVFIN", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Und es sank ihr auf den Busen eine Thr\u00e4ne hell und licht,", "tokens": ["Und", "es", "sank", "ihr", "auf", "den", "Bu\u00b7sen", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "hell", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber unter den Demanten da bemerkte man sie nicht!", "tokens": ["A\u00b7ber", "un\u00b7ter", "den", "De\u00b7man\u00b7ten", "da", "be\u00b7merk\u00b7te", "man", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sie doch sah den feuchten Demant auf dem d\u00fcrftigen Gewand", "tokens": ["Sie", "doch", "sah", "den", "feuch\u00b7ten", "De\u00b7mant", "auf", "dem", "d\u00fcrf\u00b7ti\u00b7gen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Jenes armen Manns, der bettelnd an der Kirchenpforte stand.", "tokens": ["Je\u00b7nes", "ar\u00b7men", "Manns", ",", "der", "bet\u00b7telnd", "an", "der", "Kir\u00b7ch\u00b7en\u00b7pfor\u00b7te", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Tief bewu\u00dft der eignen Ohnmacht wallt das schwache sch\u00f6ne Weib,", "tokens": ["Tief", "be\u00b7wu\u00dft", "der", "eig\u00b7nen", "Ohn\u00b7macht", "wallt", "das", "schwa\u00b7che", "sch\u00f6\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber sieh, die Kraft der M\u00e4nner beugt vor ihr den stolzen Leib!", "tokens": ["A\u00b7ber", "sieh", ",", "die", "Kraft", "der", "M\u00e4n\u00b7ner", "beugt", "vor", "ihr", "den", "stol\u00b7zen", "Leib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O wie hoch f\u00fcr solche Schw\u00e4che der Begeist'rung Banner braust,", "tokens": ["O", "wie", "hoch", "f\u00fcr", "sol\u00b7che", "Schw\u00e4\u00b7che", "der", "Be\u00b7geist'\u00b7rung", "Ban\u00b7ner", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "APPR", "PIAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doppelt scharf die Schwerter blitzen, doppelt kr\u00e4ftig jede Faust!", "tokens": ["Dop\u00b7pelt", "scharf", "die", "Schwer\u00b7ter", "blit\u00b7zen", ",", "dop\u00b7pelt", "kr\u00e4f\u00b7tig", "je\u00b7de", "Faust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "$,", "ADJD", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}