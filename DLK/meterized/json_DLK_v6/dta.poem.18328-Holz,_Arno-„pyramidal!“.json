{"dta.poem.18328": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "\u201epyramidal!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.71", "sv:0.14", "et:0.14"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Noch bl\u00fcht ja unsre ", "tokens": ["Noch", "bl\u00fcht", "ja", "uns\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Noch heilt der Zeit gewaltge Frage", "tokens": ["Noch", "heilt", "der", "Zeit", "ge\u00b7walt\u00b7ge", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Titel und ein Portemonnaie.", "tokens": ["Ein", "Ti\u00b7tel", "und", "ein", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch wachsen t\u00e4glich unsre Z\u00f6pfe,", "tokens": ["Noch", "wach\u00b7sen", "t\u00e4g\u00b7lich", "uns\u00b7re", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u201eGlaube\u201c ist des Pudels Kern,", "tokens": ["Der", "\u201e", "Glau\u00b7be", "\u201c", "ist", "des", "Pu\u00b7dels", "Kern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Milit\u00e4r putzt seine Kn\u00f6pfe", "tokens": ["Das", "Mi\u00b7li\u00b7t\u00e4r", "putzt", "sei\u00b7ne", "Kn\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Antike wird modern.", "tokens": ["Und", "das", "An\u00b7ti\u00b7ke", "wird", "mo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch scharr'n vor meinem Cap vier Pferde,", "tokens": ["Noch", "scharr'n", "vor", "mei\u00b7nem", "Cap", "vier", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Fu\u00df zu gehn ist ja gemein \u2014", "tokens": ["Zu", "Fu\u00df", "zu", "gehn", "ist", "ja", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eo wundersch\u00f6n ist Gottes Erde", "tokens": ["\u201e", "o", "wun\u00b7der\u00b7sch\u00f6n", "ist", "Got\u00b7tes", "Er\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "ADJD", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werth, darauf vergn\u00fcgt zu sein!\u201c", "tokens": ["Und", "werth", ",", "da\u00b7rauf", "ver\u00b7gn\u00fcgt", "zu", "sein", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "PAV", "VVPP", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}