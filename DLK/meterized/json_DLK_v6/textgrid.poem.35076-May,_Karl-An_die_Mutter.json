{"textgrid.poem.35076": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "An die Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab gefehlt, und du hast es getragen,", "tokens": ["Ich", "hab", "ge\u00b7fehlt", ",", "und", "du", "hast", "es", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So manches Mal und, ach, so lang, so schwer.", "tokens": ["So", "man\u00b7ches", "Mal", "und", ",", "ach", ",", "so", "lang", ",", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "$,", "ITJ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie das mich nun bedr\u00fcckt, kann ich nicht sagen;", "tokens": ["Wie", "das", "mich", "nun", "be\u00b7dr\u00fcckt", ",", "kann", "ich", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O komm noch einmal, einmal zu mir her!", "tokens": ["O", "komm", "noch", "ein\u00b7mal", ",", "ein\u00b7mal", "zu", "mir", "her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$,", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du starbst ja nicht; du bist hinaufgestiegen", "tokens": ["Du", "starbst", "ja", "nicht", ";", "du", "bist", "hin\u00b7auf\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$.", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu reinen Geistern, meiner Mutter Geist.", "tokens": ["Zu", "rei\u00b7nen", "Geis\u00b7tern", ",", "mei\u00b7ner", "Mut\u00b7ter", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df, du siehst jetzt betend mich hier liegen;", "tokens": ["Ich", "wei\u00df", ",", "du", "siehst", "jetzt", "be\u00b7tend", "mich", "hier", "lie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "VVPP", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O komm, o komm, und sag, da\u00df du verzeihst!", "tokens": ["O", "komm", ",", "o", "komm", ",", "und", "sag", ",", "da\u00df", "du", "ver\u00b7zeihst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "VVFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Komm mir im Traum; komm in der D\u00e4mmerstunde,", "tokens": ["Komm", "mir", "im", "Traum", ";", "komm", "in", "der", "D\u00e4m\u00b7mer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn, Stern um Stern, der Himmel uns umarmt.", "tokens": ["Wenn", ",", "Stern", "um", "Stern", ",", "der", "Him\u00b7mel", "uns", "um\u00b7armt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "APPR", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bring mir Verzeihung, und bring mir die Kunde,", "tokens": ["Bring", "mir", "Ver\u00b7zei\u00b7hung", ",", "und", "bring", "mir", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Da\u00df auch die Seligkeit sich mein erbarmt!", "tokens": ["Da\u00df", "auch", "die", "Se\u00b7lig\u00b7keit", "sich", "mein", "er\u00b7barmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}