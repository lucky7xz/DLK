{"textgrid.poem.28741": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meistens lobt man alle F\u00fcrsten, wie sie leben, weil sie leben;", "tokens": ["Meis\u00b7tens", "lobt", "man", "al\u00b7le", "F\u00fcrs\u00b7ten", ",", "wie", "sie", "le\u00b7ben", ",", "weil", "sie", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "$,", "PWAV", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sind es dann nicht Heucheleyen? Nein; es ist gar recht und eben,", "tokens": ["Sind", "es", "dann", "nicht", "Heu\u00b7che\u00b7le\u00b7yen", "?", "Nein", ";", "es", "ist", "gar", "recht", "und", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$.", "PTKANT", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df man ihre Laster theils nicht verhaster etwa macht,", "tokens": ["Da\u00df", "man", "ih\u00b7re", "Las\u00b7ter", "theils", "nicht", "ver\u00b7has\u00b7ter", "et\u00b7wa", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df man sie erinnert theils, wo sie sonst nicht drauff gedacht.", "tokens": ["Da\u00df", "man", "sie", "e\u00b7rin\u00b7nert", "theils", ",", "wo", "sie", "sonst", "nicht", "drauff", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Also kan man dann die Pillen, die sonst bitter wollen schmecken,", "tokens": ["Al\u00b7so", "kan", "man", "dann", "die", "Pil\u00b7len", ",", "die", "sonst", "bit\u00b7ter", "wol\u00b7len", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Scheinlich machen und vergolden und die Pflicht ins Lob verstecken.", "tokens": ["Schein\u00b7lich", "ma\u00b7chen", "und", "ver\u00b7gol\u00b7den", "und", "die", "Pflicht", "ins", "Lob", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "ADJA", "KON", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}