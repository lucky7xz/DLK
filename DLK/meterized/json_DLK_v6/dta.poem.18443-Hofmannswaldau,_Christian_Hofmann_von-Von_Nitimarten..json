{"dta.poem.18443": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von Nitimarten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Michal legte dort ein ziegenfell ins bette/", "tokens": ["Die", "Mic\u00b7hal", "leg\u00b7te", "dort", "ein", "zie\u00b7gen\u00b7fell", "ins", "bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ART", "NN", "APPRART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als David in gefahr des lebens ward gebracht.", "tokens": ["Als", "Da\u00b7vid", "in", "ge\u00b7fahr", "des", "le\u00b7bens", "ward", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ich die freyheit nur davon zu reden h\u00e4tte/", "tokens": ["Wenn", "ich", "die", "frey\u00b7heit", "nur", "da\u00b7von", "zu", "re\u00b7den", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PAV", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie manch vertrautes Weib gar andre springe macht.", "tokens": ["Wie", "manch", "ver\u00b7trau\u00b7tes", "Weib", "gar", "and\u00b7re", "sprin\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "ADV", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wolt ich nur so viel von Nitimarten sagen:", "tokens": ["So", "wolt", "ich", "nur", "so", "viel", "von", "Ni\u00b7ti\u00b7mar\u00b7ten", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie leget einen balck/ sich selbst, ins bette nein/", "tokens": ["Sie", "le\u00b7get", "ei\u00b7nen", "balck", "/", "sich", "selbst", ",", "ins", "bet\u00b7te", "nein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PRF", "ADV", "$,", "APPRART", "VVFIN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An dem die h\u00f6rner zwar nicht zu erkennen seyn/", "tokens": ["An", "dem", "die", "h\u00f6r\u00b7ner", "zwar", "nicht", "zu", "er\u00b7ken\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch f\u00fchlet sie der mann. Denn dieser mu\u00df sie tragen.", "tokens": ["Doch", "f\u00fch\u00b7let", "sie", "der", "mann", ".", "Denn", "die\u00b7ser", "mu\u00df", "sie", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "KON", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}