{"textgrid.poem.48626": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "39. Auf Herrn L. Philip Krusens, F\u00fcrstl. Holstein. Rats und Gesandtens, seiner Magnificenz Geburts-und Namenstag, den 1. Mai 1636. in der Moskaw", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolan so brich herein,", "tokens": ["Wo\u00b7lan", "so", "brich", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "du oft gew\u00fcndschter Schein,", "tokens": ["du", "oft", "ge\u00b7w\u00fcnd\u00b7schter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "brich ein und mach' uns froh!", "tokens": ["brich", "ein", "und", "mach'", "uns", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Brich an, du s\u00fc\u00dfes Licht,", "tokens": ["Brich", "an", ",", "du", "s\u00fc\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "brich an und s\u00e4ume nicht,", "tokens": ["brich", "an", "und", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "nach dem wir w\u00fcndschen so!", "tokens": ["nach", "dem", "wir", "w\u00fcnd\u00b7schen", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Brich an! Er sieht nach dir,", "tokens": ["Brich", "an", "!", "Er", "sieht", "nach", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Musen teure Zier,", "tokens": ["der", "Mu\u00b7sen", "teu\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der hochgepreiste Held,", "tokens": ["der", "hoch\u00b7ge\u00b7preis\u00b7te", "Held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "der durch sein hohes Tun", "tokens": ["der", "durch", "sein", "ho\u00b7hes", "Tun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ihm hat verbunden nun", "tokens": ["ihm", "hat", "ver\u00b7bun\u00b7den", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "den Himmel und die Welt.", "tokens": ["den", "Him\u00b7mel", "und", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der F\u00fcrst der sch\u00f6nsten Zeit,", "tokens": ["Der", "F\u00fcrst", "der", "sch\u00f6ns\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Lenz, schl\u00e4gt weit und breit", "tokens": ["der", "Lenz", ",", "schl\u00e4gt", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sein buntes Lager auf.", "tokens": ["sein", "bun\u00b7tes", "La\u00b7ger", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der weichen L\u00fcfte Schaar", "tokens": ["Der", "wei\u00b7chen", "L\u00fcf\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "treibt das verj\u00fcngte Jahr", "tokens": ["treibt", "das", "ver\u00b7j\u00fcng\u00b7te", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "an einen neuen Lauf.", "tokens": ["an", "ei\u00b7nen", "neu\u00b7en", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Jahrs Apell, der Mai,", "tokens": ["Des", "Jahrs", "A\u00b7pell", ",", "der", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "f\u00e4rbt Alles mancherlei", "tokens": ["f\u00e4rbt", "Al\u00b7les", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und schm\u00fcckt die Auen aus.", "tokens": ["und", "schm\u00fcckt", "die", "Au\u00b7en", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Floren feuchter Man", "tokens": ["Der", "Flo\u00b7ren", "feuch\u00b7ter", "Man"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "haucht mit Gesundheit an", "tokens": ["haucht", "mit", "Ge\u00b7sund\u00b7heit", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Empandas gr\u00fcnes Haus.", "tokens": ["Em\u00b7pan\u00b7das", "gr\u00fc\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Mensch, das Vieh, das Meer,", "tokens": ["Der", "Mensch", ",", "das", "Vieh", ",", "das", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Tier' und Blumen Heer", "tokens": ["der", "Tier'", "und", "Blu\u00b7men", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und Alles, wie es hei\u00dft,", "tokens": ["und", "Al\u00b7les", ",", "wie", "es", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "was die gevierte Welt", "tokens": ["was", "die", "ge\u00b7vier\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "in ihren Armen h\u00e4lt,", "tokens": ["in", "ih\u00b7ren", "Ar\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "kriegt einen neuen Geist.", "tokens": ["kriegt", "ei\u00b7nen", "neu\u00b7en", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wolan, so brich herein,", "tokens": ["Wo\u00b7lan", ",", "so", "brich", "her\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da oft gew\u00fcndschter Schein,", "tokens": ["da", "oft", "ge\u00b7w\u00fcnd\u00b7schter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "brich ein und mach' uns froh!", "tokens": ["brich", "ein", "und", "mach'", "uns", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Brich an, du s\u00fc\u00dfes Licht,", "tokens": ["Brich", "an", ",", "du", "s\u00fc\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "brich an und s\u00e4ume nicht,", "tokens": ["brich", "an", "und", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "nach dem wir w\u00fcndschen so!", "tokens": ["nach", "dem", "wir", "w\u00fcnd\u00b7schen", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}