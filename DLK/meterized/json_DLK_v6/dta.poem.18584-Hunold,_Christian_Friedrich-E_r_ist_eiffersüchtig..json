{"dta.poem.18584": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E r ist eiffers\u00fcchtig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Verfluchte Wuth! Du Hencker meiner Seelen!", "tokens": ["Ver\u00b7fluch\u00b7te", "Wuth", "!", "Du", "Hen\u00b7cker", "mei\u00b7ner", "See\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was plagst du mich verdamte Eiffersucht!", "tokens": ["Was", "plagst", "du", "mich", "ver\u00b7dam\u00b7te", "Eif\u00b7fer\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll sich mein Geist um dich zu Tode qu\u00e4len?", "tokens": ["Soll", "sich", "mein", "Geist", "um", "dich", "zu", "To\u00b7de", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "APPR", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat Lieb und Treu nur Grausamkeit zur Frucht.", "tokens": ["Hat", "Lieb", "und", "Treu", "nur", "Grau\u00b7sam\u00b7keit", "zur", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein/ mein Muth/ auf auf erwache/", "tokens": ["Nein", "/", "mein", "Muth", "/", "auf", "auf", "er\u00b7wa\u00b7che", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "$(", "APPR", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Gel\u00fcck besteht in Rache.", "tokens": ["Dein", "Ge\u00b7l\u00fcck", "be\u00b7steht", "in", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df deinen Zorn in dessen Blut zerrinnen/", "tokens": ["La\u00df", "dei\u00b7nen", "Zorn", "in", "des\u00b7sen", "Blut", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PRELAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deiner Ruh ein Stein und Ansto\u00df ist.", "tokens": ["Der", "dei\u00b7ner", "Ruh", "ein", "Stein", "und", "An\u00b7sto\u00df", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erweichst du nicht die Diamantne Sinnen/", "tokens": ["Er\u00b7weichst", "du", "nicht", "die", "Di\u00b7a\u00b7mant\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ist dein Trost/ da\u00df du gerochen bist.", "tokens": ["So", "ist", "dein", "Trost", "/", "da\u00df", "du", "ge\u00b7ro\u00b7chen", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum mein Muth/ auf/ auf erwache", "tokens": ["Drum", "mein", "Muth", "/", "auf", "/", "auf", "er\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "$(", "APPR", "$(", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts ist s\u00fcssers als die Rache.", "tokens": ["Nichts", "ist", "s\u00fcs\u00b7sers", "als", "die", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}