{"textgrid.poem.48158": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Das ist der Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Magst du mich ganz in deine Flammen h\u00fcllen", "tokens": ["Magst", "du", "mich", "ganz", "in", "dei\u00b7ne", "Flam\u00b7men", "h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und mag das Blut, das deinen Leib durchmi\u00dft,", "tokens": ["und", "mag", "das", "Blut", ",", "das", "dei\u00b7nen", "Leib", "durch\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein Herz durchpulsen, meine Adern f\u00fcllen \u2013", "tokens": ["mein", "Herz", "durch\u00b7pul\u00b7sen", ",", "mei\u00b7ne", "A\u00b7dern", "f\u00fcl\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es bleibt ein Rest, ein Rest, der du nicht bist!", "tokens": ["es", "bleibt", "ein", "Rest", ",", "ein", "Rest", ",", "der", "du", "nicht", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist der Schatten unsrer Sonnenliebe,", "tokens": ["Das", "ist", "der", "Schat\u00b7ten", "uns\u00b7rer", "Son\u00b7nen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf unsern Himmelstraum, der Erdenspott.", "tokens": ["auf", "un\u00b7sern", "Him\u00b7mel\u00b7straum", ",", "der", "Er\u00b7den\u00b7spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dieser Rest, du, dieser Rest nicht bliebe:", "tokens": ["Wenn", "die\u00b7ser", "Rest", ",", "du", ",", "die\u00b7ser", "Rest", "nicht", "blie\u00b7be", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PPER", "$,", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wir w\u00e4ren Gott. \u2013", "tokens": ["wir", "w\u00e4\u00b7ren", "Gott", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}