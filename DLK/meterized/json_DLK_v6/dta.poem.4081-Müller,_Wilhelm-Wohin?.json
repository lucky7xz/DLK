{"dta.poem.4081": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wohin?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.71", "ca:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl aus dem Felsenquell,", "tokens": ["Wohl", "aus", "dem", "Fel\u00b7sen\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hinab zum Thale rauschen", "tokens": ["Hin\u00b7ab", "zum", "Tha\u00b7le", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frisch und wunderhell.", "tokens": ["So", "frisch", "und", "wun\u00b7der\u00b7hell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, wie mir wurde,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mir", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht, wer den Rath mir gab,", "tokens": ["Nicht", ",", "wer", "den", "Rath", "mir", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00dfte auch hinunter", "tokens": ["Ich", "mu\u00df\u00b7te", "auch", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit meinem Wanderstab.", "tokens": ["Mit", "mei\u00b7nem", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hinunter und immer weiter,", "tokens": ["Hin\u00b7un\u00b7ter", "und", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und immer dem Bache nach,", "tokens": ["Und", "im\u00b7mer", "dem", "Ba\u00b7che", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und immer heller rauschte", "tokens": ["Und", "im\u00b7mer", "hel\u00b7ler", "rauschte"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und immer heller der Bach.", "tokens": ["Und", "im\u00b7mer", "hel\u00b7ler", "der", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ist das denn meine Stra\u00dfe?", "tokens": ["Ist", "das", "denn", "mei\u00b7ne", "Stra\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O B\u00e4chlein, sprich, wohin?", "tokens": ["O", "B\u00e4ch\u00b7lein", ",", "sprich", ",", "wo\u00b7hin", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mit deinem Rauschen", "tokens": ["Du", "hast", "mit", "dei\u00b7nem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir ganz berauscht den Sinn.", "tokens": ["Mir", "ganz", "be\u00b7rauscht", "den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was sag' ich denn von Rauschen?", "tokens": ["Was", "sag'", "ich", "denn", "von", "Rau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kann kein Rauschen sein:", "tokens": ["Das", "kann", "kein", "Rau\u00b7schen", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es singen wohl die Nixen", "tokens": ["Es", "sin\u00b7gen", "wohl", "die", "Ni\u00b7xen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief unten ihren Reihn.", "tokens": ["Tief", "un\u00b7ten", "ih\u00b7ren", "Reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df singen, Gesell, la\u00df rauschen,", "tokens": ["La\u00df", "sin\u00b7gen", ",", "Ge\u00b7sell", ",", "la\u00df", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "NN", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wandre fr\u00f6hlich nach!", "tokens": ["Und", "wand\u00b7re", "fr\u00f6h\u00b7lich", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gehn ja M\u00fchlenr\u00e4der", "tokens": ["Es", "gehn", "ja", "M\u00fch\u00b7len\u00b7r\u00e4\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jedem klaren Bach.", "tokens": ["In", "je\u00b7dem", "kla\u00b7ren", "Bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}