{"textgrid.poem.47669": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich wandert' in des Kerkers]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandert' in des Kerkers", "tokens": ["Ich", "wan\u00b7dert'", "in", "des", "Ker\u00b7kers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vermaledeite Hut;", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dei\u00b7te", "Hut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ward herausgerissen", "tokens": ["Ich", "ward", "her\u00b7aus\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus seiner Nacht, ich wurde", "tokens": ["Aus", "sei\u00b7ner", "Nacht", ",", "ich", "wur\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gesetzt auf einen Esel,", "tokens": ["Ge\u00b7setzt", "auf", "ei\u00b7nen", "E\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es wurde mir die Schulter,", "tokens": ["Es", "wur\u00b7de", "mir", "die", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die nackte, wund gegei\u00dfelt,", "tokens": ["Die", "nack\u00b7te", ",", "wund", "ge\u00b7gei\u00b7\u00dfelt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Erde rann mein Blut.", "tokens": ["Zur", "Er\u00b7de", "rann", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}