{"textgrid.poem.39004": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Abends K\u00fchle lockt mich herab,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Abends K\u00fchle lockt mich herab,", "tokens": ["Des", "A\u00b7bends", "K\u00fch\u00b7le", "lockt", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich durchwandle die belebten Gassen,", "tokens": ["Ich", "durch\u00b7wand\u00b7le", "die", "be\u00b7leb\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch Geschrei und Kauf und Gespr\u00e4ch,", "tokens": ["Durch", "Ge\u00b7schrei", "und", "Kauf", "und", "Ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und irre, dem Corso vor\u00fcber,", "tokens": ["Und", "ir\u00b7re", ",", "dem", "Cor\u00b7so", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "In unbekannte, d\u00e4mmernde Stra\u00dfen hinein.", "tokens": ["In", "un\u00b7be\u00b7kann\u00b7te", ",", "d\u00e4m\u00b7mern\u00b7de", "Stra\u00b7\u00dfen", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie wohl thut das Umirren", "tokens": ["Wie", "wohl", "thut", "das", "Um\u00b7ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch fremde, hochber\u00fchmte Stadt;", "tokens": ["Durch", "frem\u00b7de", ",", "hoch\u00b7be\u00b7r\u00fchm\u00b7te", "Stadt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jeder Stein wird zum Wunder,", "tokens": ["Je\u00b7der", "Stein", "wird", "zum", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jeder ohngef\u00e4hre Laut zum M\u00e4hrchen.", "tokens": ["Je\u00b7der", "ohn\u00b7ge\u00b7f\u00e4h\u00b7re", "Laut", "zum", "M\u00e4hr\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ich dr\u00e4nge mich durch den Menschenhaufen,", "tokens": ["Ich", "dr\u00e4n\u00b7ge", "mich", "durch", "den", "Men\u00b7schen\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ein neuer, enger, voller Markt,", "tokens": ["Und", "ein", "neu\u00b7er", ",", "en\u00b7ger", ",", "vol\u00b7ler", "Markt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Liegt mit finstern Buden vor mir,", "tokens": ["Liegt", "mit", "fins\u00b7tern", "Bu\u00b7den", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Gew\u00fchl des allt\u00e4glichen Lebens", "tokens": ["Das", "Ge\u00b7w\u00fchl", "des", "all\u00b7t\u00e4g\u00b7li\u00b7chen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Bet\u00e4ubt mein m\u00fcdes Ohr,", "tokens": ["Be\u00b7t\u00e4ubt", "mein", "m\u00fc\u00b7des", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und pl\u00f6tzlich erhebt sich der Blick", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "er\u00b7hebt", "sich", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Und schaut vor sich nahe und heilig", "tokens": ["Und", "schaut", "vor", "sich", "na\u00b7he", "und", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Den edelsten Tempel,", "tokens": ["Den", "e\u00b7dels\u00b7ten", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "So wohlbekannt aus Bildern,", "tokens": ["So", "wohl\u00b7be\u00b7kannt", "aus", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "So vertraut dem Herzen.", "tokens": ["So", "ver\u00b7traut", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Offen ist das Thor der S\u00e4ulenhalle,", "tokens": ["Of\u00b7fen", "ist", "das", "Thor", "der", "S\u00e4u\u00b7len\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und wenige Betende knien hier.", "tokens": ["Und", "we\u00b7ni\u00b7ge", "Be\u00b7ten\u00b7de", "kni\u00b7en", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Mich umf\u00e4ngt das harmonische Geb\u00e4u,", "tokens": ["Mich", "um\u00b7f\u00e4ngt", "das", "har\u00b7mo\u00b7ni\u00b7sche", "Ge\u00b7b\u00e4u", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Und edle Gedanken", "tokens": ["Und", "ed\u00b7le", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Wachsen mir licht im Geiste auf.", "tokens": ["Wach\u00b7sen", "mir", "licht", "im", "Geis\u00b7te", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "So ist im Leben", "tokens": ["So", "ist", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das G\u00f6ttliche oft", "tokens": ["Das", "G\u00f6tt\u00b7li\u00b7che", "oft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dicht am Gemeinen,", "tokens": ["Dicht", "am", "Ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geringen, Allt\u00e4glichen,", "tokens": ["Ge\u00b7rin\u00b7gen", ",", "All\u00b7t\u00e4g\u00b7li\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Nur sieht es nicht das bl\u00f6de Auge.", "tokens": ["Nur", "sieht", "es", "nicht", "das", "bl\u00f6\u00b7de", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Tadle dies Niemand,", "tokens": ["Tad\u00b7le", "dies", "Nie\u00b7mand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PIS", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wenn nicht immer gro\u00dfe Vorh\u00f6fe,", "tokens": ["Wenn", "nicht", "im\u00b7mer", "gro\u00b7\u00dfe", "Vor\u00b7h\u00f6\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Prachtvolle Pl\u00e4tze,", "tokens": ["Pracht\u00b7vol\u00b7le", "Pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Weite reiche Ferne", "tokens": ["Wei\u00b7te", "rei\u00b7che", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Das Ueberirdische unsern Sinnen vorbereiten.", "tokens": ["Das", "Ue\u00b7be\u00b7rir\u00b7di\u00b7sche", "un\u00b7sern", "Sin\u00b7nen", "vor\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wir lieben in vertraulicher N\u00e4he", "tokens": ["Wir", "lie\u00b7ben", "in", "ver\u00b7trau\u00b7li\u00b7cher", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Das Himmlische zu sehn und zu f\u00fchlen.", "tokens": ["Das", "Himm\u00b7li\u00b7sche", "zu", "sehn", "und", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}