{"textgrid.poem.30676": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Marcus kunte baun ein Haus", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marcus kunte baun ein Haus", "tokens": ["Mar\u00b7cus", "kun\u00b7te", "baun", "ein", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff von Grund und au\u00df und au\u00df:", "tokens": ["Auff", "von", "Grund", "und", "au\u00df", "und", "au\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kalck, der schwiert ihm au\u00df der Haut;", "tokens": ["Kalck", ",", "der", "schwiert", "ihm", "au\u00df", "der", "Haut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lenden ist der Stein vertraut;", "tokens": ["Len\u00b7den", "ist", "der", "Stein", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4gel stehn ihm f\u00fcr der Hand;", "tokens": ["N\u00e4\u00b7gel", "stehn", "ihm", "f\u00fcr", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Blase f\u00fchrt er Sand;", "tokens": ["In", "der", "Bla\u00b7se", "f\u00fchrt", "er", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil im Kopffe Schiefer steckt,", "tokens": ["Weil", "im", "Kopf\u00b7fe", "Schie\u00b7fer", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat er auch, womit er deckt;", "tokens": ["Hat", "er", "auch", ",", "wo\u00b7mit", "er", "deckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6ltzern ist sonst sein Verstand:", "tokens": ["H\u00f6lt\u00b7zern", "ist", "sonst", "sein", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat so alles bey der Hand.", "tokens": ["Hat", "so", "al\u00b7les", "bey", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}