{"textgrid.poem.60637": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Vogel Jupiters ergriff ein Herdentier.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel Jupiters ergriff ein Herdentier.", "tokens": ["Der", "Vo\u00b7gel", "Ju\u00b7pi\u00b7ters", "er\u00b7griff", "ein", "Her\u00b7den\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Rabe sah es, schw\u00e4cher zwar", "tokens": ["Ein", "Ra\u00b7be", "sah", "es", ",", "schw\u00e4\u00b7cher", "zwar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Sehnen, aber nicht an Gier,", "tokens": ["An", "Seh\u00b7nen", ",", "a\u00b7ber", "nicht", "an", "Gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollte gleich dasselbe tun. Er kreiste rund", "tokens": ["Und", "woll\u00b7te", "gleich", "das\u00b7sel\u00b7be", "tun", ".", "Er", "kreis\u00b7te", "rund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PDAT", "VVINF", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und w\u00e4hlte aus der Schafe Schar", "tokens": ["Und", "w\u00e4hl\u00b7te", "aus", "der", "Scha\u00b7fe", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sch\u00f6nste, fetteste, das auserlesen war", "tokens": ["Das", "sch\u00f6ns\u00b7te", ",", "fet\u00b7tes\u00b7te", ",", "das", "au\u00b7ser\u00b7le\u00b7sen", "war"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "PRELS", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als Opfer f\u00fcr der G\u00f6tter Mund.", "tokens": ["Als", "Op\u00b7fer", "f\u00fcr", "der", "G\u00f6t\u00b7ter", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er sprach, mit gierigen Blicken sp\u00e4hend nach dem Lamme:", "tokens": ["Er", "sprach", ",", "mit", "gie\u00b7ri\u00b7gen", "Bli\u00b7cken", "sp\u00e4\u00b7hend", "nach", "dem", "Lam\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u00bbwer auch gewesen deine Amme,", "tokens": ["\u00bb", "wer", "auch", "ge\u00b7we\u00b7sen", "dei\u00b7ne", "Am\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "VAPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dein Leib ist pr\u00e4chtig und gesund;", "tokens": ["Dein", "Leib", "ist", "pr\u00e4ch\u00b7tig", "und", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So diene du mir denn als Mahl!\u00ab", "tokens": ["So", "die\u00b7ne", "du", "mir", "denn", "als", "Mahl", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "KOUS", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So sprechend stie\u00df er schnell zu Tal", "tokens": ["So", "spre\u00b7chend", "stie\u00df", "er", "schnell", "zu", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auf jenes Schafsgesch\u00f6pf, das kl\u00e4glich bl\u00f6kte, nieder.", "tokens": ["Auf", "je\u00b7nes", "Schafs\u00b7ge\u00b7sch\u00f6pf", ",", "das", "kl\u00e4g\u00b7lich", "bl\u00f6k\u00b7te", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch fand er, da\u00df es schwerer als ein K\u00e4se wog,", "tokens": ["Doch", "fand", "er", ",", "da\u00df", "es", "schwe\u00b7rer", "als", "ein", "K\u00e4\u00b7se", "wog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df die Wolle ein gef\u00e4hrliches Gefieder,", "tokens": ["Und", "da\u00df", "die", "Wol\u00b7le", "ein", "ge\u00b7f\u00e4hr\u00b7li\u00b7ches", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wirr gleich dem Bart, der Polyphems Gesicht umzog:", "tokens": ["Wirr", "gleich", "dem", "Bart", ",", "der", "Po\u00b7ly\u00b7phems", "Ge\u00b7sicht", "um\u00b7zog", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Darin verstrickten sich des Raben Krallen fest,", "tokens": ["Da\u00b7rin", "ver\u00b7strick\u00b7ten", "sich", "des", "Ra\u00b7ben", "Kral\u00b7len", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So da\u00df der arme Schelm gefangen sa\u00df.", "tokens": ["So", "da\u00df", "der", "ar\u00b7me", "Schelm", "ge\u00b7fan\u00b7gen", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der Sch\u00e4fer kam: ein K\u00e4fig ward sein Nest,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "kam", ":", "ein", "K\u00e4\u00b7fig", "ward", "sein", "Nest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und Kinder qu\u00e4lten ihn zu ihrem Spa\u00df.", "tokens": ["Und", "Kin\u00b7der", "qu\u00e4l\u00b7ten", "ihn", "zu", "ih\u00b7rem", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus dieser Fabel zieht den Schlu\u00df,", "tokens": ["Aus", "die\u00b7ser", "Fa\u00b7bel", "zieht", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man sich richtig messen mu\u00df.", "tokens": ["Da\u00df", "man", "sich", "rich\u00b7tig", "mes\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Diebeshandwerk bleib ein kleiner Spitzbub fern!", "tokens": ["Dem", "Die\u00b7bes\u00b7hand\u00b7werk", "bleib", "ein", "klei\u00b7ner", "Spitz\u00b7bub", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schlecht erw\u00e4hltes Vorbild kann uns schwer bedr\u00e4ngen.", "tokens": ["Ein", "schlecht", "er\u00b7w\u00e4hl\u00b7tes", "Vor\u00b7bild", "kann", "uns", "schwer", "be\u00b7dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht alle Leutefresser sind auch gro\u00dfe Herrn.", "tokens": ["Nicht", "al\u00b7le", "Leu\u00b7te\u00b7fres\u00b7ser", "sind", "auch", "gro\u00b7\u00dfe", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo Wespen durchgeschl\u00fcpft, da bleibt die M\u00fccke h\u00e4ngen.", "tokens": ["Wo", "Wes\u00b7pen", "durch\u00b7ge\u00b7schl\u00fcpft", ",", "da", "bleibt", "die", "M\u00fc\u00b7cke", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}