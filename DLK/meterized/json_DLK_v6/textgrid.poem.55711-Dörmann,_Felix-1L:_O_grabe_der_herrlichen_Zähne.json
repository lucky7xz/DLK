{"textgrid.poem.55711": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: O grabe der herrlichen Z\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O grabe der herrlichen Z\u00e4hne", "tokens": ["O", "gra\u00b7be", "der", "herr\u00b7li\u00b7chen", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Blauschimmernde Perlenreihn", "tokens": ["Blau\u00b7schim\u00b7mern\u00b7de", "Per\u00b7len\u00b7reihn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In raubtierwild-rasenden K\u00fcssen", "tokens": ["In", "raub\u00b7tier\u00b7wild\u00b7ra\u00b7sen\u00b7den", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in die Schulter mir ein!", "tokens": ["Tief", "in", "die", "Schul\u00b7ter", "mir", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wir wollen noch einmal erk\u00e4mpfen", "tokens": ["Wir", "wol\u00b7len", "noch", "ein\u00b7mal", "er\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den hei\u00dfesten, h\u00f6chsten Genu\u00df,", "tokens": ["Den", "hei\u00b7\u00dfes\u00b7ten", ",", "h\u00f6chs\u00b7ten", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Eh' von dem erschlaffenden K\u00f6rper", "tokens": ["Eh'", "von", "dem", "er\u00b7schlaf\u00b7fen\u00b7den", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die Gierde weichen mu\u00df.", "tokens": ["Die", "Gier\u00b7de", "wei\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der brennenden fiebernden Wunde", "tokens": ["Der", "bren\u00b7nen\u00b7den", "fie\u00b7bern\u00b7den", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wollustdurchfolterte Qual,", "tokens": ["Wol\u00b7lust\u00b7durch\u00b7fol\u00b7ter\u00b7te", "Qual", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sie sei unsrer sterbenden Liebe", "tokens": ["Sie", "sei", "uns\u00b7rer", "ster\u00b7ben\u00b7den", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Blutiges Totenmal.", "tokens": ["Blu\u00b7ti\u00b7ges", "To\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}