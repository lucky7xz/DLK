{"dta.poem.4397": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schnee- und Crocus-Bluhme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In wenig Stunden", "tokens": ["In", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ist alles sch\u00f6ne weg, ist alle Pracht verschwun-", "tokens": ["Ist", "al\u00b7les", "sch\u00f6\u00b7ne", "weg", ",", "ist", "al\u00b7le", "Pracht", "ver\u00b7schwun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "PTKVZ", "$,", "VAFIN", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den.", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ach! fuhr ich ferner fort, ach w\u00e4ret ihr allein", "tokens": ["Ach", "!", "fuhr", "ich", "fer\u00b7ner", "fort", ",", "ach", "w\u00e4\u00b7ret", "ihr", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "XY", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So unbest\u00e4ndig und so fl\u00fcchtig!", "tokens": ["So", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "und", "so", "fl\u00fcch\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach da\u00df auch wir nicht minder nichtig,", "tokens": ["Ach", "da\u00df", "auch", "wir", "nicht", "min\u00b7der", "nich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hinf\u00e4llig und verg\u00e4nglich seyn!", "tokens": ["Hin\u00b7f\u00e4l\u00b7lig", "und", "ver\u00b7g\u00e4ng\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}