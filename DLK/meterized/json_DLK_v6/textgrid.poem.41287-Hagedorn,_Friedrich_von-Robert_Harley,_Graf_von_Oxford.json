{"textgrid.poem.41287": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Robert Harley, Graf von Oxford", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Harley, welchen Swift und Pop' und Prior loben,", "tokens": ["Der", "Har\u00b7ley", ",", "wel\u00b7chen", "Swift", "und", "Pop'", "und", "Pri\u00b7or", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward in den Grafenstand durch Annens Wahl erhoben.", "tokens": ["Ward", "in", "den", "Gra\u00b7fen\u00b7stand", "durch", "An\u00b7nens", "Wahl", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbwie? Harley? fragt erstaunt Britanniens Bathyll,", "tokens": ["\u00bb", "wie", "?", "Har\u00b7ley", "?", "fragt", "er\u00b7staunt", "Bri\u00b7tan\u00b7ni\u00b7ens", "Ba\u00b7thyll", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "NN", "$.", "VVFIN", "ADJD", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Le Sac, ein Mann voll Geist, schnellf\u00fc\u00dfig wie Achill.", "tokens": ["Le", "Sac", ",", "ein", "Mann", "voll", "Geist", ",", "schnell\u00b7f\u00fc\u00b7\u00dfig", "wie", "A\u00b7chill", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "ADJD", "NN", "$,", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja. Lobt ihn, wie ihr wollt! erhebt ihn zu den Sternen!", "tokens": ["Ja", ".", "Lobt", "ihn", ",", "wie", "ihr", "wollt", "!", "er\u00b7hebt", "ihn", "zu", "den", "Ster\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was sieht doch, ruft er aus, in ihm die K\u00f6nigin?", "tokens": ["Was", "sieht", "doch", ",", "ruft", "er", "aus", ",", "in", "ihm", "die", "K\u00f6\u00b7ni\u00b7gin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwei Jahre gingen mir mit diesem Klotze hin!", "tokens": ["Zwei", "Jah\u00b7re", "gin\u00b7gen", "mir", "mit", "die\u00b7sem", "Klot\u00b7ze", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch konnt' er nie recht tanzen lernen!\u00ab", "tokens": ["Doch", "konnt'", "er", "nie", "recht", "tan\u00b7zen", "ler\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}