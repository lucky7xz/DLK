{"textgrid.poem.59552": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und die Sonne machte den weiten Ritt", "tokens": ["Und", "die", "Son\u00b7ne", "mach\u00b7te", "den", "wei\u00b7ten", "Ritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um die Welt,", "tokens": ["Um", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und die Sternlein sprachen:\u00bbWir reisen mit", "tokens": ["Und", "die", "Stern\u00b7lein", "spra\u00b7chen", ":", "\u00bb", "Wir", "rei\u00b7sen", "mit"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um die Welt\u00ab;", "tokens": ["Um", "die", "Welt", "\u00ab", ";"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und die Sonne sie schalt sie:\u00bbIhr bleibt zu Haus,", "tokens": ["Und", "die", "Son\u00b7ne", "sie", "schalt", "sie", ":", "\u00bb", "Ihr", "bleibt", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Denn ich brenn' euch die goldnen \u00c4uglein aus", "tokens": ["Denn", "ich", "brenn'", "euch", "die", "gold\u00b7nen", "\u00c4ug\u00b7lein", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bei dem feurigen Ritt um die Welt.\u00ab", "tokens": ["Bei", "dem", "feu\u00b7ri\u00b7gen", "Ritt", "um", "die", "Welt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und die Sternlein gingen zum lieben Mond", "tokens": ["Und", "die", "Stern\u00b7lein", "gin\u00b7gen", "zum", "lie\u00b7ben", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der Nacht,", "tokens": ["In", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "Und sie sprachen:\u00bbDu, der auf Wolken thront", "tokens": ["Und", "sie", "spra\u00b7chen", ":", "\u00bb", "Du", ",", "der", "auf", "Wol\u00b7ken", "thront"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der Nacht,", "tokens": ["In", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "La\u00df uns wandeln mit dir, denn dein milder Schein,", "tokens": ["La\u00df", "uns", "wan\u00b7deln", "mit", "dir", ",", "denn", "dein", "mil\u00b7der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Er verbrennet uns nimmer die \u00c4ugelein.\u00ab", "tokens": ["Er", "ver\u00b7bren\u00b7net", "uns", "nim\u00b7mer", "die", "\u00c4u\u00b7ge\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und er nahm sie, Gesellen der Nacht.", "tokens": ["Und", "er", "nahm", "sie", ",", "Ge\u00b7sel\u00b7len", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun willkommen, Sternlein und lieber Mond,", "tokens": ["Nun", "will\u00b7kom\u00b7men", ",", "Stern\u00b7lein", "und", "lie\u00b7ber", "Mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "KON", "ADV", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In der Nacht,", "tokens": ["In", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "Ihr verstehet, was still in den Herzen wohnt", "tokens": ["Ihr", "ver\u00b7ste\u00b7het", ",", "was", "still", "in", "den", "Her\u00b7zen", "wohnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In der Nacht.", "tokens": ["In", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Kommt und z\u00fcndet die himmlischen Lichter an,", "tokens": ["Kommt", "und", "z\u00fcn\u00b7det", "die", "himm\u00b7li\u00b7schen", "Lich\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich lustig mitschw\u00e4rmen und spielen kann", "tokens": ["Da\u00df", "ich", "lus\u00b7tig", "mit\u00b7schw\u00e4r\u00b7men", "und", "spie\u00b7len", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "KON", "VVINF", "VMFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "In den freundlichen Spielen der Nacht.", "tokens": ["In", "den", "freund\u00b7li\u00b7chen", "Spie\u00b7len", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}