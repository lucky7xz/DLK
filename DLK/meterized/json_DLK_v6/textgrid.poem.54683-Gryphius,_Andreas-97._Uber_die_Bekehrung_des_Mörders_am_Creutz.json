{"textgrid.poem.54683": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "97. Uber die Bekehrung des M\u00f6rders am Creutz", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! da\u00df auff sp\u00e4tte Bu\u00df es ja kein ander wage!", "tokens": ["Ach", "!", "da\u00df", "auff", "sp\u00e4t\u00b7te", "Bu\u00df", "es", "ja", "kein", "an\u00b7der", "wa\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "APPR", "ADJA", "NN", "PPER", "ADV", "PIAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Jesus Gnad und Reich dem M\u00f6rder hier verleiht", "tokens": ["Ob", "Je\u00b7sus", "Gnad", "und", "Reich", "dem", "M\u00f6r\u00b7der", "hier", "ver\u00b7leiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "KON", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschicht es doch nur heut! Ein K\u00f6nig zwar verzeiht/", "tokens": ["Ge\u00b7schicht", "es", "doch", "nur", "heut", "!", "Ein", "K\u00f6\u00b7nig", "zwar", "ver\u00b7zeiht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "$.", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch h\u00f6chste Schuld nur an dem h\u00f6chsten Ehren Tage.", "tokens": ["Doch", "h\u00f6chs\u00b7te", "Schuld", "nur", "an", "dem", "h\u00f6chs\u00b7ten", "Eh\u00b7ren", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}