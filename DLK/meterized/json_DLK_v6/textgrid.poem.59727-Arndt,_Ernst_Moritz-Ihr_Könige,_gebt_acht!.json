{"textgrid.poem.59727": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Ihr K\u00f6nige, gebt acht!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Ehr' im Leibe hat, ruft ", "tokens": ["Was", "Ehr'", "im", "Lei\u00b7be", "hat", ",", "ruft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "NN", "APPRART", "NN", "VAFIN", "$,", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und Tilgung langer deutscher Schanden,", "tokens": ["Und", "Til\u00b7gung", "lan\u00b7ger", "deut\u00b7scher", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ruft und flucht aus allen Landen:", "tokens": ["Es", "ruft", "und", "flucht", "aus", "al\u00b7len", "Lan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutsche Gott lebt noch und wacht.", "tokens": ["Der", "deut\u00b7sche", "Gott", "lebt", "noch", "und", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lebt und wacht der Gott der Herrlichkeit und Macht,", "tokens": ["Es", "lebt", "und", "wacht", "der", "Gott", "der", "Herr\u00b7lich\u00b7keit", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein sind die Wonnen und die Schrecken,", "tokens": ["Sein", "sind", "die", "Won\u00b7nen", "und", "die", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die aus dem Schlaf die V\u00f6lker wecken.", "tokens": ["Die", "aus", "dem", "Schlaf", "die", "V\u00f6l\u00b7ker", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ist's, der Sturm und Heitre macht.", "tokens": ["Gott", "ist's", ",", "der", "Sturm", "und", "Heit\u00b7re", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erbebt! Das Wetter ist des Herrn, der blitzt und kracht,", "tokens": ["Er\u00b7bebt", "!", "Das", "Wet\u00b7ter", "ist", "des", "Herrn", ",", "der", "blitzt", "und", "kracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wird des deutschen Haders Drachen", "tokens": ["Er", "wird", "des", "deut\u00b7schen", "Ha\u00b7ders", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Staub zerblitzen und zerkrachen.", "tokens": ["Zu", "Staub", "zer\u00b7blit\u00b7zen", "und", "zer\u00b7kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Gottes Acht und Aberacht!", "tokens": ["Auf", "Got\u00b7tes", "Acht", "und", "A\u00b7be\u00b7racht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erbebt! Denn alles Volk ruft ", "tokens": ["Er\u00b7bebt", "!", "Denn", "al\u00b7les", "Volk", "ruft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es schreit den Ruf in alle Winde,", "tokens": ["Es", "schreit", "den", "Ruf", "in", "al\u00b7le", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo es den deutschen Kaiser finde.", "tokens": ["Wo", "es", "den", "deut\u00b7schen", "Kai\u00b7ser", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut, horcht, woher es blitzt und kracht.", "tokens": ["Schaut", ",", "horcht", ",", "wo\u00b7her", "es", "blitzt", "und", "kracht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erbebt! Erkennt die Zeit, die Gott der Herr gemacht!", "tokens": ["Er\u00b7bebt", "!", "Er\u00b7kennt", "die", "Zeit", ",", "die", "Gott", "der", "Herr", "ge\u00b7macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wollt l\u00e4nger ihr im Stolz erblinden,", "tokens": ["Wollt", "l\u00e4n\u00b7ger", "ihr", "im", "Stolz", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So haut euch Gott aus allen Winden \u2013", "tokens": ["So", "haut", "euch", "Gott", "aus", "al\u00b7len", "Win\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die deutsche Acht und Aberacht.", "tokens": ["Die", "deut\u00b7sche", "Acht", "und", "A\u00b7be\u00b7racht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}