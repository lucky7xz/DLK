{"textgrid.poem.58990": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denk nicht an k\u00fcnftige Gesch\u00e4fte;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk nicht an k\u00fcnftige Gesch\u00e4fte;", "tokens": ["Denk", "nicht", "an", "k\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7sch\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's da ist, gibt Gott Licht und Kr\u00e4fte!", "tokens": ["Wenn's", "da", "ist", ",", "gibt", "Gott", "Licht", "und", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "$,", "VVFIN", "NN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im Vorrat hast du's ja nicht not;", "tokens": ["Im", "Vor\u00b7rat", "hast", "du's", "ja", "nicht", "not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was willst du mehr als t\u00e4glich Brot?", "tokens": ["Was", "willst", "du", "mehr", "als", "t\u00e4g\u00b7lich", "Brot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PIAT", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}