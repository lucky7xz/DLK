{"dta.poem.7637": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hoch, wie tief du seist, will das dir nicht sich zeigen,", "tokens": ["Wie", "hoch", ",", "wie", "tief", "du", "seist", ",", "will", "das", "dir", "nicht", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VMFIN", "ART", "PPER", "PTKNEG", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch f\u00fchlst du, ob du bist im Sinken oder Steigen.", "tokens": ["Doch", "f\u00fchlst", "du", ",", "ob", "du", "bist", "im", "Sin\u00b7ken", "o\u00b7der", "Stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Sinken f\u00fchlst du Schwer', im Steigen Leichtigkeit,", "tokens": ["Im", "Sin\u00b7ken", "f\u00fchlst", "du", "Schwer'", ",", "im", "Stei\u00b7gen", "Leich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort von dir selbst gedr\u00fcckt, und hier von Druck befreit.", "tokens": ["Dort", "von", "dir", "selbst", "ge\u00b7dr\u00fcckt", ",", "und", "hier", "von", "Druck", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVPP", "$,", "KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das merk, und denk dabei: Du kannst im freien Wallen", "tokens": ["Das", "merk", ",", "und", "denk", "da\u00b7bei", ":", "Du", "kannst", "im", "frei\u00b7en", "Wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PAV", "$.", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steigen aus jeder Tief', aus jeder H\u00f6he fallen.", "tokens": ["Stei\u00b7gen", "aus", "je\u00b7der", "Tie\u00b7f'", ",", "aus", "je\u00b7der", "H\u00f6\u00b7he", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}}}}}