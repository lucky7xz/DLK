{"dta.poem.7367": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fundator.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im Westen schwimmt ein falber Strich,", "tokens": ["Im", "Wes\u00b7ten", "schwimmt", "ein", "fal\u00b7ber", "Strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abendstern entz\u00fcndet sich", "tokens": ["Der", "A\u00b7bends\u00b7tern", "ent\u00b7z\u00fcn\u00b7det", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Grad' \u00fcber'm Sankt Georg am Thore;", "tokens": ["Grad'", "\u00fc\u00b7ber'm", "Sankt", "Ge\u00b7org", "am", "Tho\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwer haucht der Dunst vom nahen Moore.", "tokens": ["Schwer", "haucht", "der", "Dunst", "vom", "na\u00b7hen", "Moo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Schlaftrunkne Schw\u00e4ne kreisen sacht", "tokens": ["Schlaf\u00b7trunk\u00b7ne", "Schw\u00e4\u00b7ne", "krei\u00b7sen", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Um's Eiland, wo die graue Wacht", "tokens": ["Um's", "Ei\u00b7land", ",", "wo", "die", "grau\u00b7e", "Wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich hebt aus Wasserbins' und Rohre.", "tokens": ["Sich", "hebt", "aus", "Was\u00b7ser\u00b7bins'", "und", "Roh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf ihrem Dach die Fledermaus,", "tokens": ["Auf", "ih\u00b7rem", "Dach", "die", "Fle\u00b7der\u00b7maus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schaukelt sich, sie breitet aus", "tokens": ["Sie", "schau\u00b7kelt", "sich", ",", "sie", "brei\u00b7tet", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Rippenschirm des Schwingenflosses,", "tokens": ["Den", "Rip\u00b7pen\u00b7schirm", "des", "Schwin\u00b7gen\u00b7flos\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, mit dem Schwirren des Geschosses,", "tokens": ["Und", ",", "mit", "dem", "Schwir\u00b7ren", "des", "Ge\u00b7schos\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Entlang den Teich, hinauf, hinab,", "tokens": ["Ent\u00b7lang", "den", "Teich", ",", "hin\u00b7auf", ",", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann klammert sie am Fensterstab,", "tokens": ["Dann", "klam\u00b7mert", "sie", "am", "Fens\u00b7ter\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und blinzt in das Gemach des Schlosses.", "tokens": ["Und", "blinzt", "in", "das", "Ge\u00b7mach", "des", "Schlos\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein weit Gela\u00df, im Sammetstaat!", "tokens": ["Ein", "weit", "Ge\u00b7la\u00df", ",", "im", "Sam\u00b7mets\u00b7taat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo einst der m\u00e4chtige Pr\u00e4lat", "tokens": ["Wo", "einst", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Pr\u00e4\u00b7lat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Hauses Chronik hat geschrieben.", "tokens": ["Des", "Hau\u00b7ses", "Chro\u00b7nik", "hat", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch ist der Baldachin geblieben,", "tokens": ["Frisch", "ist", "der", "Bal\u00b7da\u00b7ch\u00b7in", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der g\u00fcldne Tisch, an dem er sa\u00df,", "tokens": ["Der", "g\u00fcld\u00b7ne", "Tisch", ",", "an", "dem", "er", "sa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Seelenmesse las", "tokens": ["Und", "sei\u00b7ne", "See\u00b7len\u00b7mes\u00b7se", "las"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man heut in der Kapelle dr\u00fcben.", "tokens": ["Man", "heut", "in", "der", "Ka\u00b7pel\u00b7le", "dr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Heut sind es grade hundert Jahr,", "tokens": ["Heut", "sind", "es", "gra\u00b7de", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit er gelegen auf der Bahr'", "tokens": ["Seit", "er", "ge\u00b7le\u00b7gen", "auf", "der", "Bahr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Kreuz und Silberstabe.", "tokens": ["Mit", "sei\u00b7nem", "Kreuz", "und", "Sil\u00b7ber\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ewge Lamp' an seinem Grabe", "tokens": ["Die", "ew\u00b7ge", "Lamp'", "an", "sei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat heute hundert Jahr gebrannt.", "tokens": ["Hat", "heu\u00b7te", "hun\u00b7dert", "Jahr", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinem Sessel an der Wand", "tokens": ["In", "sei\u00b7nem", "Ses\u00b7sel", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sitzt heut ein schlichter alter Knabe.", "tokens": ["Sitzt", "heut", "ein", "schlich\u00b7ter", "al\u00b7ter", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}