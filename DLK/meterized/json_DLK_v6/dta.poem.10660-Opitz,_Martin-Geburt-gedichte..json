{"dta.poem.10660": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Geburt-gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Komm sch\u00f6ner Morgenstern/ ach komm vnd la\u00df es tagen/", "tokens": ["Komm", "sch\u00f6\u00b7ner", "Mor\u00b7gens\u00b7tern", "/", "ach", "komm", "vnd", "la\u00df", "es", "ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "XY", "XY", "KON", "VVIMP", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aurora spanne doch vier Hengste vor den Wagen/", "tokens": ["Au\u00b7ro\u00b7ra", "span\u00b7ne", "doch", "vier", "Hengs\u00b7te", "vor", "den", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "CARD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm eilend vnd zieh an dein sch\u00f6n rothgelbes Kleidt/", "tokens": ["Komm", "ei\u00b7lend", "vnd", "zieh", "an", "dein", "sch\u00f6n", "roth\u00b7gel\u00b7bes", "Kleidt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Wie lange wirstu dann die Sonne schlaffen lassen?", "tokens": ["Wie", "lan\u00b7ge", "wirs\u00b7tu", "dann", "die", "Son\u00b7ne", "schlaf\u00b7fen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ey sprich/ sie solle doch sich mit dem Z\u00fcgel fassen/", "tokens": ["Ey", "sprich", "/", "sie", "sol\u00b7le", "doch", "sich", "mit", "dem", "Z\u00fc\u00b7gel", "fas\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "PPER", "VMFIN", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Rosse stunden da/ es sey schon hohe Zeit.", "tokens": ["Die", "Ros\u00b7se", "stun\u00b7den", "da", "/", "es", "sey", "schon", "ho\u00b7he", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drey mahl sein jetz und gleich sechs Jahre weg verlohren/", "tokens": ["Drey", "mahl", "sein", "jetz", "und", "gleich", "sechs", "Jah\u00b7re", "weg", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "PPOSAT", "ADV", "KON", "ADV", "CARD", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df die durch G\u00fctigkeit de\u00df Himmels ward geboren/", "tokens": ["Da\u00df", "die", "durch", "G\u00fc\u00b7tig\u00b7keit", "de\u00df", "Him\u00b7mels", "ward", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In der ich alle Tag auffs new geboren werdt/", "tokens": ["In", "der", "ich", "al\u00b7le", "Tag", "auffs", "new", "ge\u00b7bo\u00b7ren", "werdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "APPRART", "ADJD", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd da\u00df in diesem Bild all jhre hohe Gaben", "tokens": ["Vnd", "da\u00df", "in", "die\u00b7sem", "Bild", "all", "jhre", "ho\u00b7he", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "NN", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die grosse G\u00f6ttinen so sehr ersch\u00f6pffet haben/", "tokens": ["Die", "gros\u00b7se", "G\u00f6t\u00b7ti\u00b7nen", "so", "sehr", "er\u00b7sch\u00f6pf\u00b7fet", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df dessen gleichen nicht gefunden wird auff Erdt.", "tokens": ["Da\u00df", "des\u00b7sen", "glei\u00b7chen", "nicht", "ge\u00b7fun\u00b7den", "wird", "auff", "Erdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PTKNEG", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Juno/ Jovis Weib vnd Schwester/ thet jhr geben", "tokens": ["Die", "Ju\u00b7no", "/", "Jo\u00b7vis", "Weib", "vnd", "Schwes\u00b7ter", "/", "thet", "jhr", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NE", "NN", "KON", "NN", "$(", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Viel Reichthumb/ Gut vnd Gelt/ die ", "tokens": ["Viel", "Reicht\u00b7humb", "/", "Gut", "vnd", "Gelt", "/", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "$(", "ADJD", "KON", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die zarten ", "tokens": ["Die", "zar\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Natura gab mich jhr zum Sclaven jederzeit.", "tokens": ["Na\u00b7tu\u00b7ra", "gab", "mich", "jhr", "zum", "Scla\u00b7ven", "je\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach komm/ ", "tokens": ["Ach", "komm", "/"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Brich durch die sp\u00e4te Nacht: wie kanstu der versagen/", "tokens": ["Brich", "durch", "die", "sp\u00e4\u00b7te", "Nacht", ":", "wie", "kans\u00b7tu", "der", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$.", "PWAV", "VMFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der alle G\u00f6ttinen so g\u00fcnstig sich erzeigt?", "tokens": ["Der", "al\u00b7le", "G\u00f6t\u00b7ti\u00b7nen", "so", "g\u00fcns\u00b7tig", "sich", "er\u00b7zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Komm doch/ wach auff/ wach auff/ sieh an die rote Wangen/", "tokens": ["Komm", "doch", "/", "wach", "auff", "/", "wach", "auff", "/", "sieh", "an", "die", "ro\u00b7te", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ADJD", "APPR", "$(", "ADJD", "APPR", "$(", "VVIMP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "In denen alle Zier vnd Au\u00dfbundt sich er\u00e4ugt.", "tokens": ["In", "de\u00b7nen", "al\u00b7le", "Zier", "vnd", "Au\u00df\u00b7bundt", "sich", "er\u00b7\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Richt\u2019 auff dein klares Haupt/ da\u00df deine Strahl\u2019 erleuchten", "tokens": ["Richt'", "auff", "dein", "kla\u00b7res", "Haupt", "/", "da\u00df", "dei\u00b7ne", "Strahl'", "er\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den angenehmen Tag/ la\u00df weiter nicht befeuchten", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Tag", "/", "la\u00df", "wei\u00b7ter", "nicht", "be\u00b7feuch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "VVIMP", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Perlen Tau das feld/ steig auff de\u00df Himmels Saal/", "tokens": ["Den", "Per\u00b7len", "Tau", "das", "feld", "/", "steig", "auff", "de\u00df", "Him\u00b7mels", "Saal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vertreib der Wolcken Dunst/ mach Anstand mit den Winden/", "tokens": ["Ver\u00b7treib", "der", "Wol\u00b7cken", "Dunst", "/", "mach", "An\u00b7stand", "mit", "den", "Win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$(", "VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Vnd halt den Regen ab/ la\u00df dieses Fest empfinden", "tokens": ["Vnd", "halt", "den", "Re\u00b7gen", "ab", "/", "la\u00df", "die\u00b7ses", "Fest", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$(", "VVIMP", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den gantzen Erdenkrey\u00df/ erquicke Berg vnd Thal/", "tokens": ["Den", "gant\u00b7zen", "Er\u00b7den\u00b7krey\u00df", "/", "er\u00b7qui\u00b7cke", "Berg", "vnd", "Thal", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Heut ist mein Augentrost/ heut ist mein Liecht geboren/", "tokens": ["Heut", "ist", "mein", "Au\u00b7gen\u00b7trost", "/", "heut", "ist", "mein", "Liecht", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In der vollkommenlich zusammen sich verschworen", "tokens": ["In", "der", "voll\u00b7kom\u00b7men\u00b7lich", "zu\u00b7sam\u00b7men", "sich", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die dinge/ so doch sonst gar selten Freunde sind:", "tokens": ["Die", "din\u00b7ge", "/", "so", "doch", "sonst", "gar", "sel\u00b7ten", "Freun\u00b7de", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ADV", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Sch\u00f6nheit/ vnd die Zucht. Ich will mein Haupt bedecken", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "vnd", "die", "Zucht", ".", "Ich", "will", "mein", "Haupt", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mit einem Lorbeerkrantz/ Ich will viel h\u00f6her strecken", "tokens"