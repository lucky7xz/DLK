{"dta.poem.14025": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Steckbrief .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sonne nicht steigen mag,", "tokens": ["Son\u00b7ne", "nicht", "stei\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sieht so verschlafen drein,", "tokens": ["Sieht", "so", "ver\u00b7schla\u00b7fen", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00fcnschet den ganzen Tag,", "tokens": ["W\u00fcn\u00b7schet", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df wieder Nacht m\u00f6cht' sein.", "tokens": ["Da\u00df", "wie\u00b7der", "Nacht", "m\u00f6cht'", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}