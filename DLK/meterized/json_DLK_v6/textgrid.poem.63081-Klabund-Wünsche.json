{"textgrid.poem.63081": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcnsche", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du des Nachts die gro\u00dfe Stadt durchstreifst,", "tokens": ["Wenn", "du", "des", "Nachts", "die", "gro\u00b7\u00dfe", "Stadt", "durch\u00b7streifst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und deine W\u00fcnsche in den Ampeln h\u00e4ngen,", "tokens": ["Und", "dei\u00b7ne", "W\u00fcn\u00b7sche", "in", "den", "Am\u00b7peln", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versuche, da\u00df den Willen du begreifst,", "tokens": ["Ver\u00b7su\u00b7che", ",", "da\u00df", "den", "Wil\u00b7len", "du", "be\u00b7greifst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus dem sie ins erhaben Dunkle dr\u00e4ngen.", "tokens": ["Aus", "dem", "sie", "ins", "er\u00b7ha\u00b7ben", "Dunk\u00b7le", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie fl\u00fcchten fr\u00fch vor ihrer Blondheit Glanz,", "tokens": ["Sie", "fl\u00fcch\u00b7ten", "fr\u00fch", "vor", "ih\u00b7rer", "Blond\u00b7heit", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus der sie gerne M\u00f6rderstricke fl\u00f6chten.", "tokens": ["Aus", "der", "sie", "ger\u00b7ne", "M\u00f6r\u00b7der\u00b7stri\u00b7cke", "fl\u00f6ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr Dasein ist auf Strahlenschuhn ein Tanz \u2013", "tokens": ["Ihr", "Da\u00b7sein", "ist", "auf", "Strah\u00b7len\u00b7schuhn", "ein", "Tanz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie bringen Leben, wo sie sterben m\u00f6chten.", "tokens": ["Sie", "brin\u00b7gen", "Le\u00b7ben", ",", "wo", "sie", "ster\u00b7ben", "m\u00f6ch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}