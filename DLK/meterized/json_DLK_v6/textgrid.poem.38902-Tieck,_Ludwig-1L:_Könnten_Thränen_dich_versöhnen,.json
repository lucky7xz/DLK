{"textgrid.poem.38902": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nnten Thr\u00e4nen dich vers\u00f6hnen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnten Thr\u00e4nen dich vers\u00f6hnen,", "tokens": ["K\u00f6nn\u00b7ten", "Thr\u00e4\u00b7nen", "dich", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chte Reue dich verm\u00f6gen:", "tokens": ["M\u00f6ch\u00b7te", "Reu\u00b7e", "dich", "ver\u00b7m\u00f6\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie zu mir niederz\u00f6gen", "tokens": ["Da\u00df", "sie", "zu", "mir", "nie\u00b7der\u00b7z\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Gl\u00fcck, die vor'gen Gaben,", "tokens": ["Al\u00b7les", "Gl\u00fcck", ",", "die", "vor'\u00b7gen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer wollt' ich sie verh\u00f6hnen.", "tokens": ["Nim\u00b7mer", "wollt'", "ich", "sie", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber nie wird K\u00fchlung laben", "tokens": ["A\u00b7ber", "nie", "wird", "K\u00fch\u00b7lung", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den, der seine B\u00e4ume f\u00e4llt;", "tokens": ["Den", ",", "der", "sei\u00b7ne", "B\u00e4u\u00b7me", "f\u00e4llt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm erstirbt das gr\u00fcne Zelt:", "tokens": ["Ihm", "er\u00b7stirbt", "das", "gr\u00fc\u00b7ne", "Zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer sein Haus sich selbst verw\u00fcstet,", "tokens": ["Wer", "sein", "Haus", "sich", "selbst", "ver\u00b7w\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie kann der sich wohl gehaben.", "tokens": ["Nie", "kann", "der", "sich", "wohl", "ge\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}