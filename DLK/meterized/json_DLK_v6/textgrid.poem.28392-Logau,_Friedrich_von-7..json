{"textgrid.poem.28392": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand hat noch iemals sein eignes Fleisch kunt hassen;", "tokens": ["Nie\u00b7mand", "hat", "noch", "ie\u00b7mals", "sein", "eig\u00b7nes", "Fleisch", "kunt", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Solt uns dann Gottes Sohn zu lieben unterlassen?", "tokens": ["Solt", "uns", "dann", "Got\u00b7tes", "Sohn", "zu", "lie\u00b7ben", "un\u00b7ter\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Fleisch ist unser Fleisch; drum wird er unser Freund,", "tokens": ["Sein", "Fleisch", "ist", "un\u00b7ser", "Fleisch", ";", "drum", "wird", "er", "un\u00b7ser", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PAV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er es so mit uns, wie mit ihm selbsten meint.", "tokens": ["Da\u00df", "er", "es", "so", "mit", "uns", ",", "wie", "mit", "ihm", "selbs\u00b7ten", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PPER", "$,", "PWAV", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}