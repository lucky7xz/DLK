{"textgrid.poem.58585": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Lehrern au\u00dfer sich mu\u00df man nicht bleiben stehen,", "tokens": ["Bei", "Leh\u00b7rern", "au\u00b7\u00dfer", "sich", "mu\u00df", "man", "nicht", "blei\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "VMFIN", "PIS", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es kommt und gehet hin, was uns erwecken kann;", "tokens": ["Es", "kommt", "und", "ge\u00b7het", "hin", ",", "was", "uns", "er\u00b7we\u00b7cken", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O selig, wer in sich kann Gott mit Augen sehen,", "tokens": ["O", "se\u00b7lig", ",", "wer", "in", "sich", "kann", "Gott", "mit", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "APPR", "PRF", "VMFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hat den Lehrer nah, auf den es nur kommt an!", "tokens": ["Der", "hat", "den", "Leh\u00b7rer", "nah", ",", "auf", "den", "es", "nur", "kommt", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er h\u00f6rt das ew'ge Wort im stillen Grunde sagen:", "tokens": ["Er", "h\u00f6rt", "das", "ew'\u00b7ge", "Wort", "im", "stil\u00b7len", "Grun\u00b7de", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbso lenke dich, mein Kind, von allem ab in mich;", "tokens": ["\u00bb", "so", "len\u00b7ke", "dich", ",", "mein", "Kind", ",", "von", "al\u00b7lem", "ab", "in", "mich", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PIS", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dies ist der Weg allein, du darfst nicht immer fragen,", "tokens": ["Dies", "ist", "der", "Weg", "al\u00b7lein", ",", "du", "darfst", "nicht", "im\u00b7mer", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geh nur gerade zu, merk, was ich lehre dich!\u00ab", "tokens": ["Geh", "nur", "ge\u00b7ra\u00b7de", "zu", ",", "merk", ",", "was", "ich", "leh\u00b7re", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}