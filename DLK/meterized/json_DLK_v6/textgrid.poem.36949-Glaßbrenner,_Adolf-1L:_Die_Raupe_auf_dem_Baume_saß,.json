{"textgrid.poem.36949": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Raupe auf dem Baume sa\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Raupe auf dem Baume sa\u00df,", "tokens": ["Die", "Rau\u00b7pe", "auf", "dem", "Bau\u00b7me", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und von der Kron' die Bl\u00e4tter fra\u00df \u2013", "tokens": ["Und", "von", "der", "Kron'", "die", "Bl\u00e4t\u00b7ter", "fra\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja ja!", "tokens": ["Ja", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sie war im bunten Kleide,", "tokens": ["Sie", "war", "im", "bun\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als wie von Sammt und Seide,", "tokens": ["Als", "wie", "von", "Sammt", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ha ha ha ha ha ha!", "tokens": ["Ha", "ha", "ha", "ha", "ha", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ein Staatsminister ging vorbei,", "tokens": ["Ein", "Staats\u00b7mi\u00b7nis\u00b7ter", "ging", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sah das Thier und sprach: Ei ei!", "tokens": ["Der", "sah", "das", "Thier", "und", "sprach", ":", "Ei", "ei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja ja!", "tokens": ["Ja", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wie konnt' es ihr gelingen?", "tokens": ["Wie", "konnt'", "es", "ihr", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "'s geht nicht mit rechten Dingen!", "tokens": ["'s", "geht", "nicht", "mit", "rech\u00b7ten", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ha ha ha ha ha ha!", "tokens": ["Ha", "ha", "ha", "ha", "ha", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Du unbeh\u00fclflich dummes Thier!", "tokens": ["Du", "un\u00b7be\u00b7h\u00fcl\u00b7flich", "dum\u00b7mes", "Thier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wundre mich, drum sage mir:", "tokens": ["Ich", "wund\u00b7re", "mich", ",", "drum", "sa\u00b7ge", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja ja!", "tokens": ["Ja", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wie hast du's unternommen,", "tokens": ["Wie", "hast", "du's", "un\u00b7ter\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und bist so hoch gekommen?", "tokens": ["Und", "bist", "so", "hoch", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ha ha ha ha ha ha!", "tokens": ["Ha", "ha", "ha", "ha", "ha", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und als die Raupe blieb nicht stumm,", "tokens": ["Und", "als", "die", "Rau\u00b7pe", "blieb", "nicht", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wurd' er roth und dreht sich um.", "tokens": ["Da", "wurd'", "er", "roth", "und", "dreht", "sich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja ja!", "tokens": ["Ja", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Die Raupe hat gesprochen:", "tokens": ["Die", "Rau\u00b7pe", "hat", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Freund, ich bin gekrochen!", "tokens": ["Mein", "Freund", ",", "ich", "bin", "ge\u00b7kro\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ha ha ha ha ha ha!", "tokens": ["Ha", "ha", "ha", "ha", "ha", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}