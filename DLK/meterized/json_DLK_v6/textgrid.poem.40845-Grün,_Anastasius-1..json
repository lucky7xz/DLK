{"textgrid.poem.40845": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Orient, wo \u2013 wie aus bl\u00fch'ndem Hage", "tokens": ["Im", "O\u00b7rient", ",", "wo", "\u2013", "wie", "aus", "bl\u00fch'n\u00b7dem", "Ha\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "$(", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein spielend Kinderpaar rothwangig gr\u00fc\u00dft \u2013", "tokens": ["Ein", "spie\u00b7lend", "Kin\u00b7der\u00b7paar", "roth\u00b7wan\u00b7gig", "gr\u00fc\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das heit're M\u00e4rchen und die sinn'ge Sage", "tokens": ["Das", "heit'\u00b7re", "M\u00e4r\u00b7chen", "und", "die", "sinn'\u00b7ge", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Rosenw\u00e4ldern zwischen Blumen sprie\u00dft,", "tokens": ["In", "Ro\u00b7sen\u00b7w\u00e4l\u00b7dern", "zwi\u00b7schen", "Blu\u00b7men", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort gibt manch rauher Hirte dir die Kunde:", "tokens": ["Dort", "gibt", "manch", "rau\u00b7her", "Hir\u00b7te", "dir", "die", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es walle Jesus Christus, ungesehn,", "tokens": ["Es", "wal\u00b7le", "Je\u00b7sus", "Chris\u00b7tus", ",", "un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Ostern j\u00e4hrlich um die Morgenstunde", "tokens": ["Zu", "Os\u00b7tern", "j\u00e4hr\u00b7lich", "um", "die", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Auferstehungskleid auf Oelbergs H\u00f6hn", "tokens": ["Im", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7kleid", "auf", "O\u00b7el\u00b7bergs", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und seh' hinab nach seines Wandelns Thale,", "tokens": ["Und", "seh'", "hin\u00b7ab", "nach", "sei\u00b7nes", "Wan\u00b7delns", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihm ein Kreuz und Leichentuch einst wies;", "tokens": ["Das", "ihm", "ein", "Kreuz", "und", "Lei\u00b7chen\u00b7tuch", "einst", "wies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Zion stolz geprangt im goldnen Strahle,", "tokens": ["Wo", "Zi\u00b7on", "stolz", "ge\u00b7prangt", "im", "gold\u00b7nen", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Granitnes Bollwerk, das sein Fluch zerblies!", "tokens": ["Gra\u00b7nit\u00b7nes", "Boll\u00b7werk", ",", "das", "sein", "Fluch", "zer\u00b7blies", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und Ostern war es einst; der Herr sah nieder", "tokens": ["Und", "Os\u00b7tern", "war", "es", "einst", ";", "der", "Herr", "sah", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur kahlen Flur, ver\u00f6det und ergraut,", "tokens": ["Zur", "kah\u00b7len", "Flur", ",", "ver\u00b7\u00f6\u00b7det", "und", "er\u00b7graut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rings Tr\u00fcmmer, Asch' und Staub und Tr\u00fcmmer wieder", "tokens": ["Rings", "Tr\u00fcm\u00b7mer", ",", "Asch'", "und", "Staub", "und", "Tr\u00fcm\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NE", "KON", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Schutt auf Schutt, soweit das Auge schaut!", "tokens": ["Und", "Schutt", "auf", "Schutt", ",", "so\u00b7weit", "das", "Au\u00b7ge", "schaut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er wei\u00df, es sind die\u00df nur die wirren Schollen", "tokens": ["Er", "wei\u00df", ",", "es", "sind", "die\u00df", "nur", "die", "wir\u00b7ren", "Schol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PDS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchw\u00fchlten, neugepfl\u00fcgten Ackerlands,", "tokens": ["Durch\u00b7w\u00fchl\u00b7ten", ",", "neu\u00b7ge\u00b7pfl\u00fcg\u00b7ten", "A\u00b7cker\u00b7lands", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo einst die Saatenwogen fluthen sollen,", "tokens": ["Wo", "einst", "die", "Saa\u00b7ten\u00b7wo\u00b7gen", "flut\u00b7hen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und winden sich der goldne Garbenkranz!", "tokens": ["Und", "win\u00b7den", "sich", "der", "gold\u00b7ne", "Gar\u00b7ben\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er sieht daraus den Baum der neuen Lehre", "tokens": ["Er", "sieht", "da\u00b7raus", "den", "Baum", "der", "neu\u00b7en", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit tiefer Wurzel, ries'gem S\u00e4ulenschaft", "tokens": ["Mit", "tie\u00b7fer", "Wur\u00b7zel", ",", "ries'\u00b7gem", "S\u00e4u\u00b7len\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich steigend w\u00f6lben \u00fcber Land und Meere", "tokens": ["Sich", "stei\u00b7gend", "w\u00f6l\u00b7ben", "\u00fc\u00b7ber", "Land", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weithin streuen Schatten, Fr\u00fcchte, Kraft!", "tokens": ["Und", "weit\u00b7hin", "streu\u00b7en", "Schat\u00b7ten", ",", "Fr\u00fcch\u00b7te", ",", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Des Tods Triumphzug ging durch diese Gr\u00fcnde,", "tokens": ["Des", "Tods", "Tri\u00b7umph\u00b7zug", "ging", "durch", "die\u00b7se", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rings keine Spur von eines Menschen Pfad,", "tokens": ["Rings", "kei\u00b7ne", "Spur", "von", "ei\u00b7nes", "Men\u00b7schen", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Vogel singt, es rauscht kein Blatt im Winde,", "tokens": ["Kein", "Vo\u00b7gel", "singt", ",", "es", "rauscht", "kein", "Blatt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es weht kein Halm, es gr\u00fcnet keine Saat.", "tokens": ["Es", "weht", "kein", "Halm", ",", "es", "gr\u00fc\u00b7net", "kei\u00b7ne", "Saat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da\u00df doppelt gro\u00df der Sieg des Todes rage,", "tokens": ["Da\u00df", "dop\u00b7pelt", "gro\u00df", "der", "Sieg", "des", "To\u00b7des", "ra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lebt sp\u00e4rlich hier noch Eines Lebens Schein:", "tokens": ["Lebt", "sp\u00e4r\u00b7lich", "hier", "noch", "Ei\u00b7nes", "Le\u00b7bens", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es seufzt, wie eines Dichters Leichenklage,", "tokens": ["Es", "seufzt", ",", "wie", "ei\u00b7nes", "Dich\u00b7ters", "Lei\u00b7chen\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Kedrons Quelle zischend durchs Gestein:", "tokens": ["Des", "Ke\u00b7drons", "Quel\u00b7le", "zi\u00b7schend", "durchs", "Ge\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbeinst streckt' ich wohlbehaglich meine Glieder", "tokens": ["\u00bb", "einst", "streckt'", "ich", "wohl\u00b7be\u00b7hag\u00b7lich", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Bl\u00fcthenpf\u00fchl, auf weichem Silberkies,", "tokens": ["Im", "Bl\u00fc\u00b7then\u00b7pf\u00fchl", ",", "auf", "wei\u00b7chem", "Sil\u00b7ber\u00b7kies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis von Moria's alter Veste nieder", "tokens": ["Bis", "von", "Mo\u00b7ria's", "al\u00b7ter", "Ves\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NE", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In meinen Schoo\u00df der Sturm die Tr\u00fcmmer stie\u00df!", "tokens": ["In", "mei\u00b7nen", "Schoo\u00df", "der", "Sturm", "die", "Tr\u00fcm\u00b7mer", "stie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nun ich den Leib von Stein an Steine trage,", "tokens": ["Nun", "ich", "den", "Leib", "von", "Stein", "an", "Stei\u00b7ne", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ich wohl \u00e4chzen laut vor Schmerz und Zorn;", "tokens": ["Mu\u00df", "ich", "wohl", "\u00e4ch\u00b7zen", "laut", "vor", "Schmerz", "und", "Zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun die Gelenk' an Tr\u00fcmmern wund ich schlage,", "tokens": ["Nun", "die", "Ge\u00b7lenk'", "an", "Tr\u00fcm\u00b7mern", "wund", "ich", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist, gleich als blut' er, jetzt so roth mein Born.", "tokens": ["Ist", ",", "gleich", "als", "blut'", "er", ",", "jetzt", "so", "roth", "mein", "Born", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "$,", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Mein Born, so klar einst, weisend noch als Spiegel", "tokens": ["Mein", "Born", ",", "so", "klar", "einst", ",", "wei\u00b7send", "noch", "als", "Spie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "ADV", "$,", "ADJD", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der K\u00f6n'ge Burg, den Tempel gottverkl\u00e4rt,", "tokens": ["Der", "K\u00f6n'\u00b7ge", "Burg", ",", "den", "Tem\u00b7pel", "gott\u00b7ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Palastbes\u00e4te, wallumkr\u00e4nzte H\u00fcgel", "tokens": ["Pa\u00b7last\u00b7be\u00b7s\u00e4\u00b7te", ",", "wal\u00b7lum\u00b7kr\u00e4nz\u00b7te", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auch ein Volk, einst solcher F\u00fclle werth!", "tokens": ["Und", "auch", "ein", "Volk", ",", "einst", "sol\u00b7cher", "F\u00fcl\u00b7le", "werth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O da\u00df sich am Gestein zu Scherben schl\u00fcge", "tokens": ["O", "da\u00df", "sich", "am", "Ge\u00b7stein", "zu", "Scher\u00b7ben", "schl\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PRF", "APPRART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Spiegel, dem einst Solches ward zu schaun,", "tokens": ["Der", "Spie\u00b7gel", ",", "dem", "einst", "Sol\u00b7ches", "ward", "zu", "schaun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df die\u00df Bild des Tods er nimmer tr\u00fcge,", "tokens": ["Auf", "da\u00df", "die\u00df", "Bild", 