{"dta.poem.20005": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Anwesenheit des Herzogs von  Braun-  \n schweig  in Braunschweig im Jahre 1809.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich, dem Adler gleich, in's Firmament mich schwingen,", "tokens": ["K\u00f6nnt'", "ich", ",", "dem", "Ad\u00b7ler", "gleich", ",", "in's", "Fir\u00b7ma\u00b7ment", "mich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "ADV", "$,", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fr\u00f6hlich und frey, ein Gott, in's blaue Weltall singen,", "tokens": ["Fr\u00f6h\u00b7lich", "und", "frey", ",", "ein", "Gott", ",", "in's", "blau\u00b7e", "Wel\u00b7tall", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Tr\u00e4t' ich, bespritzt mit Blut, ein Mann, aus Kampf und", "tokens": ["Tr\u00e4t'", "ich", ",", "be\u00b7spritzt", "mit", "Blut", ",", "ein", "Mann", ",", "aus", "Kampf", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlacht,", "tokens": ["Schlacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Dann w\u00fcrd', o Welfe! Dir ein w\u00fcrdig Lob gebracht;", "tokens": ["Dann", "w\u00fcrd'", ",", "o", "Wel\u00b7fe", "!", "Dir", "ein", "w\u00fcr\u00b7dig", "Lob", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So aber bin ich nur ein weinend Kind gleich allen,", "tokens": ["So", "a\u00b7ber", "bin", "ich", "nur", "ein", "wei\u00b7nend", "Kind", "gleich", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So Schwerdt als Harfe w\u00fcrd' der schwachen Hand entfallen;", "tokens": ["So", "Schwerdt", "als", "Har\u00b7fe", "w\u00fcrd'", "der", "schwa\u00b7chen", "Hand", "ent\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KOUS", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch denk' ich Dein und Dein! wallt auf die\u00df tr\u00e4ge Blut,", "tokens": ["Doch", "denk'", "ich", "Dein", "und", "Dein", "!", "wallt", "auf", "die\u00df", "tr\u00e4\u00b7ge", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "KON", "PPOSAT", "$.", "VVFIN", "APPR", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sieh! dem Kinde w\u00e4chst noch alter teutscher Muth.", "tokens": ["Und", "sieh", "!", "dem", "Kin\u00b7de", "w\u00e4chst", "noch", "al\u00b7ter", "teut\u00b7scher", "Muth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann sieht er Dich in Deiner V\u00e4ter Hallen", "tokens": ["Dann", "sieht", "er", "Dich", "in", "Dei\u00b7ner", "V\u00e4\u00b7ter", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fl\u00fcchtling, verbannt und arm, in stiller Trauer wallen,", "tokens": ["Fl\u00fccht\u00b7ling", ",", "ver\u00b7bannt", "und", "arm", ",", "in", "stil\u00b7ler", "Trau\u00b7er", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Doch eh' zu neuem Kampf Dich wilder Donner ruft,", "tokens": ["Doch", "eh'", "zu", "neu\u00b7em", "Kampf", "Dich", "wil\u00b7der", "Don\u00b7ner", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steigst Du, ein treuer Sohn, in ihre stille Gruft.", "tokens": ["Steigst", "Du", ",", "ein", "treu\u00b7er", "Sohn", ",", "in", "ih\u00b7re", "stil\u00b7le", "Gruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein seltsam Schweigen hat da rings erf\u00fcllt die Mauern,", "tokens": ["Ein", "selt\u00b7sam", "Schwei\u00b7gen", "hat", "da", "rings", "er\u00b7f\u00fcllt", "die", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sah nicht Teutschland, doch man sah die Steine trauern,", "tokens": ["Man", "sah", "nicht", "Teutschland", ",", "doch", "man", "sah", "die", "Stei\u00b7ne", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NN", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da sankst Du weinend hin, ein Strahl durchflog den Chor,", "tokens": ["Da", "sankst", "Du", "wei\u00b7nend", "hin", ",", "ein", "Strahl", "durch\u00b7flog", "den", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aus dem Sarge stieg Heinrich der Leu' empor.", "tokens": ["Und", "aus", "dem", "Sar\u00b7ge", "stieg", "Hein\u00b7rich", "der", "Leu'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und all' die Helden rings in heil'gen Sarkophagen,", "tokens": ["Und", "all'", "die", "Hel\u00b7den", "rings", "in", "heil'\u00b7gen", "Sar\u00b7ko\u00b7pha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00e4nner, so Leid und Tod um Teutschland einst getragen,", "tokens": ["M\u00e4n\u00b7ner", ",", "so", "Leid", "und", "Tod", "um", "Teutschland", "einst", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "KON", "NN", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die heben ernst und stumm sich aus den S\u00e4rgen wach,", "tokens": ["Die", "he\u00b7ben", "ernst", "und", "stumm", "sich", "aus", "den", "S\u00e4r\u00b7gen", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor allen aber so Heinrich der Leue sprach:", "tokens": ["Vor", "al\u00b7len", "a\u00b7ber", "so", "Hein\u00b7rich", "der", "Leu\u00b7e", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADV", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Getrost, vieltreuer Sohn! Bald heilen all' die Wunden!", "tokens": ["Ge\u00b7trost", ",", "viel\u00b7treu\u00b7er", "Sohn", "!", "Bald", "hei\u00b7len", "all'", "die", "Wun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier blick' hinab, und sieh Germania treuverbunden.", "tokens": ["Hier", "blick'", "hin\u00b7ab", ",", "und", "sieh", "Ger\u00b7ma\u00b7nia", "treu\u00b7ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "VVIMP", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Fremden Lorbeer liegt, von Blut befleckt, entlaubt;", "tokens": ["Des", "Frem\u00b7den", "Lor\u00b7beer", "liegt", ",", "von", "Blut", "be\u00b7fleckt", ",", "ent\u00b7laubt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch segenreich umstrahlt ein Stern des Enkels Haupt.", "tokens": ["Doch", "se\u00b7gen\u00b7reich", "um\u00b7strahlt", "ein", "Stern", "des", "En\u00b7kels", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "\u201edu aber, zeuch, mein Sohn, harr' still der theuern Stunde,", "tokens": ["\u201e", "du", "a\u00b7ber", ",", "zeuch", ",", "mein", "Sohn", ",", "harr'", "still", "der", "theu\u00b7ern", "Stun\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "VVIMP", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bring' den Br\u00fcdern Dein da oben diese Kunde.\u201c \u2014", "tokens": ["Und", "bring'", "den", "Br\u00fc\u00b7dern", "Dein", "da", "o\u00b7ben", "die\u00b7se", "Kun\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "ADV", "ADV", "PDAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sprach der Leue, sprach's, und in die S\u00e4rge all'", "tokens": ["So", "sprach", "der", "Leu\u00b7e", ",", "sprach's", ",", "und", "in", "die", "S\u00e4r\u00b7ge", "all'"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sanken die Helden rings mit wundersamem Schall.", "tokens": ["San\u00b7ken", "die", "Hel\u00b7den", "rings", "mit", "wun\u00b7der\u00b7sa\u00b7mem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Da stiegest Du empor, die Faust gest\u00e4rkt zum Streite,", "tokens": ["Da", "stie\u00b7gest", "Du", "em\u00b7por", ",", "die", "Faust", "ge\u00b7st\u00e4rkt", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blitz, Donner, Feindesruf durchdrang die Luft die Weite;", "tokens": ["Blitz", ",", "Don\u00b7ner", ",", "Fein\u00b7des\u00b7ruf", "durch\u00b7drang", "die", "Luft", "die", "Wei\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Du aber schlugst den Feind mit wenig Treuen Dein,", "tokens": ["Du", "a\u00b7ber", "schlugst", "den", "Feind", "mit", "we\u00b7nig", "Treu\u00b7en", "Dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und legtest ruhend nun Dein Haupt auf einen Stein. ", "tokens": ["Und", "leg\u00b7test", "ru\u00b7hend", "nun", "Dein", "Haupt", "auf", "ei\u00b7nen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}