{"dta.poem.17414": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "35.  GOtt kan mehr viel als wenig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist das GOtt nicht kan. H\u00f6r Sp\u00f6tter auf zulach", "tokens": ["Nichts", "ist", "das", "Gott", "nicht", "kan", ".", "H\u00f6r", "Sp\u00f6t\u00b7ter", "auf", "zu\u00b7lach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN", "PTKNEG", "VMFIN", "$.", "NE", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kan zwar keinen GOtt/ wol aber G\u00f6tter machen.", "tokens": ["Er", "kan", "zwar", "kei\u00b7nen", "Gott", "/", "wol", "a\u00b7ber", "G\u00f6t\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$(", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}