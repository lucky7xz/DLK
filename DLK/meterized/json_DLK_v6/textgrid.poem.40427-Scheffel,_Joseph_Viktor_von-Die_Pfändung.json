{"textgrid.poem.40427": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Pf\u00e4ndung", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder sa\u00df beim Weine", "tokens": ["Und", "wie\u00b7der", "sa\u00df", "beim", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Waldhorn ob der Bruck", "tokens": ["Im", "Wald\u00b7horn", "ob", "der", "Bruck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr vom Rodensteine", "tokens": ["Der", "Herr", "vom", "Ro\u00b7dens\u00b7tei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit schwerem Schluck und Gluck.", "tokens": ["Mit", "schwe\u00b7rem", "Schluck", "und", "Gluck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wirt sprach tief in Trauer:", "tokens": ["Der", "Wirt", "sprach", "tief", "in", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbda\u00df Gott sich mein erbarm'!", "tokens": ["\u00bb", "da\u00df", "Gott", "sich", "mein", "er\u00b7barm'", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "PRF", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sitzt wie eine Mauer", "tokens": ["Der", "sitzt", "wie", "ei\u00b7ne", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und trinkt mich n\u00e4chstens arm.", "tokens": ["Und", "trinkt", "mich", "n\u00e4chs\u00b7tens", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie soll das all noch enden?", "tokens": ["Wie", "soll", "das", "all", "noch", "en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "PIAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein' Pfenning gibt er her ...", "tokens": ["Kein'", "Pfen\u00b7ning", "gibt", "er", "her", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaub', ich la\u00df ihn pf\u00e4nden,", "tokens": ["Ich", "glaub'", ",", "ich", "la\u00df", "ihn", "pf\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst weicht er mir nicht mehr!\u00ab", "tokens": ["Sonst", "weicht", "er", "mir", "nicht", "mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Fronvogt samt dem B\u00fcttel", "tokens": ["Der", "Fron\u00b7vogt", "samt", "dem", "B\u00fct\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam handfest an im Horn:", "tokens": ["Kam", "hand\u00b7fest", "an", "im", "Horn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbheraus den Sammetkittel,", "tokens": ["\u00bb", "he\u00b7raus", "den", "Sam\u00b7met\u00b7kit\u00b7tel", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stiefel und die Sporn.", "tokens": ["Die", "Stie\u00b7fel", "und", "die", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Heraus des Mantels Zierde,", "tokens": ["He\u00b7raus", "des", "Man\u00b7tels", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Handschuh und Zobelhut!", "tokens": ["Hand\u00b7schuh", "und", "Zo\u00b7bel\u00b7hut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verfallen diesem Wirte", "tokens": ["Ver\u00b7fal\u00b7len", "die\u00b7sem", "Wir\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist all Eu'r Hab und Gut!\u00ab", "tokens": ["Ist", "all", "Eu'r", "Hab", "und", "Gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da lacht der Rodensteiner:", "tokens": ["Da", "lacht", "der", "Ro\u00b7dens\u00b7tei\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnur zu!... wie wird mir wohl!", "tokens": ["\u00bb", "nur", "zu", "!", "...", "wie", "wird", "mir", "wohl", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s trinkt leichter sich und feiner", "tokens": ["'s", "trinkt", "leich\u00b7ter", "sich", "und", "fei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Unterkamisol!", "tokens": ["Im", "Un\u00b7ter\u00b7ka\u00b7mi\u00b7sol", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und bis ihr mir die Kehlen", "tokens": ["Und", "bis", "ihr", "mir", "die", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6nnt pf\u00e4nden aus dem Hals,", "tokens": ["K\u00f6nnt", "pf\u00e4n\u00b7den", "aus", "dem", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Werd' ich noch manchen qu\u00e4len,", "tokens": ["Werd'", "ich", "noch", "man\u00b7chen", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wein schenkt in Kurpfalz!\u00ab", "tokens": ["Der", "Wein", "schenkt", "in", "Kur\u00b7pfalz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}