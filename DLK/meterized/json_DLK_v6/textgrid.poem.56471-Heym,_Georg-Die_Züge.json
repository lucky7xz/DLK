{"textgrid.poem.56471": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Z\u00fcge", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rauchwolken, rosa, wie ein Fr\u00fchlingstag,", "tokens": ["Rauch\u00b7wol\u00b7ken", ",", "ro\u00b7sa", ",", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Die schnell der Z\u00fcge schwarze Lunge st\u00f6\u00dft,", "tokens": ["Die", "schnell", "der", "Z\u00fc\u00b7ge", "schwar\u00b7ze", "Lun\u00b7ge", "st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ziehn auf dem Strom hinab, der riesig fl\u00f6\u00dft", "tokens": ["Ziehn", "auf", "dem", "Strom", "hin\u00b7ab", ",", "der", "rie\u00b7sig", "fl\u00f6\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eisschollen breit mit Sto\u00df und lautem Schlag.", "tokens": ["Eis\u00b7schol\u00b7len", "breit", "mit", "Sto\u00df", "und", "lau\u00b7tem", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Der weite Wintertag der Niederung", "tokens": ["Der", "wei\u00b7te", "Win\u00b7ter\u00b7tag", "der", "Nie\u00b7de\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gl\u00e4nzt fern wie Feuer rot und Gold-Kristall", "tokens": ["Gl\u00e4nzt", "fern", "wie", "Feu\u00b7er", "rot", "und", "Gold\u00b7Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf Schnee und Ebenen, wo der Feuerball", "tokens": ["Auf", "Schnee", "und", "E\u00b7be\u00b7nen", ",", "wo", "der", "Feu\u00b7er\u00b7ball"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Sonne sinkt auf Wald und D\u00e4mmerung.", "tokens": ["Der", "Son\u00b7ne", "sinkt", "auf", "Wald", "und", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Die Z\u00fcge donnern auf dem Meilendamme,", "tokens": ["Die", "Z\u00fc\u00b7ge", "don\u00b7nern", "auf", "dem", "Mei\u00b7len\u00b7dam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der in die W\u00e4lder rennt, des Tages Schweif.", "tokens": ["Der", "in", "die", "W\u00e4l\u00b7der", "rennt", ",", "des", "Ta\u00b7ges", "Schweif", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Rauch steigt auf wie eine Feuerflamme,", "tokens": ["Ihr", "Rauch", "steigt", "auf", "wie", "ei\u00b7ne", "Feu\u00b7er\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die hoch im Licht des Ostwinds Schnabel zaust,", "tokens": ["Die", "hoch", "im", "Licht", "des", "Ost\u00b7winds", "Schna\u00b7bel", "zaust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der, goldgefiedert, wie ein starker Greif,", "tokens": ["Der", ",", "gold\u00b7ge\u00b7fie\u00b7dert", ",", "wie", "ein", "star\u00b7ker", "Greif", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit breiter Brust hinab gen Abend braust.", "tokens": ["Mit", "brei\u00b7ter", "Brust", "hin\u00b7ab", "gen", "A\u00b7bend", "braust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}