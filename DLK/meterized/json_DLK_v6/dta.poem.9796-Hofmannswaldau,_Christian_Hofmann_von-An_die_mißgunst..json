{"dta.poem.9796": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die mi\u00dfgunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey/ mi\u00dfgunst/ ruhig mit der zeit/", "tokens": ["Sey", "/", "mi\u00df\u00b7gunst", "/", "ru\u00b7hig", "mit", "der", "zeit", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "ADV", "$(", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob Ernst Augustens neuer ehre.", "tokens": ["Ob", "Ernst", "Au\u00b7gus\u00b7tens", "neu\u00b7er", "eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was findet doch an ihm dein neid/", "tokens": ["Was", "fin\u00b7det", "doch", "an", "ihm", "dein", "neid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Das nicht des Chur-huts w\u00fcrdig w\u00e4re?", "tokens": ["Das", "nicht", "des", "Chur\u00b7huts", "w\u00fcr\u00b7dig", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verdienst und stamm/ sein stand im reich/", "tokens": ["Ver\u00b7dienst", "und", "stamm", "/", "sein", "stand", "im", "reich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$(", "VAINF", "VVFIN", "APPRART", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein k\u00f6nigs-hertz in allen sachen/", "tokens": ["Sein", "k\u00f6\u00b7nigs\u00b7hertz", "in", "al\u00b7len", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00f6nt\u2019 ihn nicht k\u00f6nigen nur gleich/", "tokens": ["K\u00f6nt'", "ihn", "nicht", "k\u00f6\u00b7ni\u00b7gen", "nur", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VMFIN", "ADV", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Besondern gar zum k\u00f6nig machen.", "tokens": ["Be\u00b7son\u00b7dern", "gar", "zum", "k\u00f6\u00b7nig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Noch schweigt man der Gemahlin thron/", "tokens": ["Noch", "schweigt", "man", "der", "Ge\u00b7mah\u00b7lin", "thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und seinen grossen Schwieger-Sohn.", "tokens": ["Und", "sei\u00b7nen", "gros\u00b7sen", "Schwie\u00b7ger\u00b7Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}