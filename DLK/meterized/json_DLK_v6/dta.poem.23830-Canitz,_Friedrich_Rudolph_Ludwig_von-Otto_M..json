{"dta.poem.23830": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Otto M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ungern wildes Volck/ die B\u00f6hmen/ Danen/ Wenden", "tokens": ["Der", "Un\u00b7gern", "wil\u00b7des", "Volck", "/", "die", "B\u00f6h\u00b7men", "/", "Da\u00b7nen", "/", "Wen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "$(", "NN", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Welschen zittern schon/ wenn sie in meinen H\u00e4nden", "tokens": ["Und", "Wel\u00b7schen", "zit\u00b7tern", "schon", "/", "wenn", "sie", "in", "mei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Schwerdt der Rache sehn; die Satzung f\u00fchr ich", "tokens": ["Das", "Schwerdt", "der", "Ra\u00b7che", "sehn", ";", "die", "Sat\u00b7zung", "f\u00fchr", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wer in Teutschland herrscht/ hinfort sol K\u00e4yser", "tokens": ["Da\u00df", "wer", "in", "Teutschland", "herrscht", "/", "hin\u00b7fort", "sol", "K\u00e4y\u00b7ser"], "token_info": ["word", "word", "word", "w