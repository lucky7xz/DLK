{"textgrid.poem.57947": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Rathausschmerzen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wollten allzu hoch hinaus", "tokens": ["Sie", "woll\u00b7ten", "all\u00b7zu", "hoch", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKA", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon bei den Au\u00dfenmauern,", "tokens": ["Schon", "bei", "den", "Au\u00b7\u00dfen\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel weiter hat es nicht gelangt,", "tokens": ["Viel", "wei\u00b7ter", "hat", "es", "nicht", "ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun stehn wir da und trauern.", "tokens": ["Nun", "stehn", "wir", "da", "und", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Bauherr ward am S\u00e4ckel krank,", "tokens": ["Der", "Bau\u00b7herr", "ward", "am", "S\u00e4\u00b7ckel", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Geld tat ihm zerrinnen,", "tokens": ["Sein", "Geld", "tat", "ihm", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von au\u00dfen sieht es protzig aus,", "tokens": ["Von", "au\u00b7\u00dfen", "sieht", "es", "prot\u00b7zig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch klaterig von innen.", "tokens": ["Doch", "kla\u00b7te\u00b7rig", "von", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von au\u00dfen ist es wundervoll,", "tokens": ["Von", "au\u00b7\u00dfen", "ist", "es", "wun\u00b7der\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von au\u00dfen ist's beglissen,", "tokens": ["Von", "au\u00b7\u00dfen", "ist's", "be\u00b7glis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von innen aber weniger,", "tokens": ["Von", "in\u00b7nen", "a\u00b7ber", "we\u00b7ni\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist es stark bel\u00e4mmert.", "tokens": ["Da", "ist", "es", "stark", "be\u00b7l\u00e4m\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}