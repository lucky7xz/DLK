{"textgrid.poem.63338": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Winterschlaf", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem man sich nunmehr zum Winter wendet,", "tokens": ["In\u00b7dem", "man", "sich", "nun\u00b7mehr", "zum", "Win\u00b7ter", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat es der Dichter schwer,", "tokens": ["Hat", "es", "der", "Dich\u00b7ter", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sommer ist geendet,", "tokens": ["Der", "Som\u00b7mer", "ist", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine Blume w\u00e4chst nicht mehr.", "tokens": ["Und", "ei\u00b7ne", "Blu\u00b7me", "w\u00e4chst", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was soll man da besingen?", "tokens": ["Was", "soll", "man", "da", "be\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meisten Requisiten sind vereist.", "tokens": ["Die", "meis\u00b7ten", "Re\u00b7qui\u00b7si\u00b7ten", "sind", "ver\u00b7eist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man mu\u00df schon in die eigene Seele dringen", "tokens": ["Man", "mu\u00df", "schon", "in", "die", "ei\u00b7ge\u00b7ne", "See\u00b7le", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u2013 Jedoch, da haperts meist.", "tokens": ["\u2013", "Je\u00b7doch", ",", "da", "ha\u00b7perts", "meist", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man sitzt besorgt auf seinen Hintern,", "tokens": ["Man", "sitzt", "be\u00b7sorgt", "auf", "sei\u00b7nen", "Hin\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sinnt und sitzt sich seine Hose durch,", "tokens": ["Man", "sinnt", "und", "sitzt", "sich", "sei\u00b7ne", "Ho\u00b7se", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Da hilft das eben nichts, da mu\u00df man eben \u00fcberwintern", "tokens": ["\u2013", "Da", "hilft", "das", "e\u00b7ben", "nichts", ",", "da", "mu\u00df", "man", "e\u00b7ben", "\u00fc\u00b7berw\u00b7in\u00b7tern"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADV", "PIS", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Wie Frosch und Lurch.", "tokens": ["Wie", "Frosch", "und", "Lurch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}