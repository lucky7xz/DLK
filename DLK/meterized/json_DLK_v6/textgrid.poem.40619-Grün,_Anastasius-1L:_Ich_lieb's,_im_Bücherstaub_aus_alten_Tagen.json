{"textgrid.poem.40619": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lieb's, im B\u00fccherstaub aus alten Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lieb's, im B\u00fccherstaub aus alten Tagen", "tokens": ["Ich", "lie\u00b7b's", ",", "im", "B\u00fc\u00b7cher\u00b7staub", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Den R\u00e4thseln j\u00fcngster Tage nachzufragen.", "tokens": ["Den", "R\u00e4th\u00b7seln", "j\u00fcngs\u00b7ter", "Ta\u00b7ge", "nach\u00b7zu\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bunt ziehn durchs Zeitgewebe Thatenf\u00e4den,", "tokens": ["Bunt", "ziehn", "durchs", "Zeit\u00b7ge\u00b7we\u00b7be", "Tha\u00b7ten\u00b7f\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wer entwirrt Beginn und End' f\u00fcr jeden?", "tokens": ["Doch", "wer", "ent\u00b7wirrt", "Be\u00b7ginn", "und", "End'", "f\u00fcr", "je\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "KON", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das B\u00e4umlein, das der Ahn mit Sorgfalt zog", "tokens": ["Das", "B\u00e4um\u00b7lein", ",", "das", "der", "Ahn", "mit", "Sorg\u00b7falt", "zog"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Streut in den Schoo\u00df erst Enkeln Bl\u00fcth' und Frucht;", "tokens": ["Streut", "in", "den", "Schoo\u00df", "erst", "En\u00b7keln", "Bl\u00fcth'", "und", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Staube lag der Kiesel, dessen Wucht", "tokens": ["Im", "Stau\u00b7be", "lag", "der", "Kie\u00b7sel", ",", "des\u00b7sen", "Wucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ans Riesenhaupt aus Hirtenschleuder flog;", "tokens": ["Ans", "Rie\u00b7sen\u00b7haupt", "aus", "Hir\u00b7ten\u00b7schleu\u00b7der", "flog", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schon h\u00e4ngt der Stern am Himmel unbeachtet,", "tokens": ["Schon", "h\u00e4ngt", "der", "Stern", "am", "Him\u00b7mel", "un\u00b7be\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Andern einst erhellt, was uns umnachtet;", "tokens": ["Der", "An\u00b7dern", "einst", "er\u00b7hellt", ",", "was", "uns", "um\u00b7nach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und schwarz vermummt durch unsre Fastnacht schreitet", "tokens": ["Und", "schwarz", "ver\u00b7mummt", "durch", "uns\u00b7re", "Fast\u00b7nacht", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein alt Verh\u00e4ngni\u00df, dem die Larv' entgleitet.", "tokens": ["Ein", "alt", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "dem", "die", "La\u00b7rv'", "ent\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Zu D\u00fcren war's. Vorm Hexenvogte stand", "tokens": ["Zu", "D\u00fc\u00b7ren", "wa\u00b7r'", "s.", "Vorm", "He\u00b7xen\u00b7vog\u00b7te", "stand"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ein M\u00e4gdlein, einst des Gottesgartens Preis;", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "einst", "des", "Got\u00b7tes\u00b7gar\u00b7tens", "Preis", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch knickt die Kette schnell solch junges Reis", "tokens": ["Doch", "knickt", "die", "Ket\u00b7te", "schnell", "solch", "jun\u00b7ges", "Reis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Bl\u00fcthen ranken schlecht an Kerkerwand.", "tokens": ["Und", "Bl\u00fc\u00b7then", "ran\u00b7ken", "schlecht", "an", "Ker\u00b7ker\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbbekenne!\u00ab mahnt aufs Neu des Vogts Gehei\u00df,", "tokens": ["\u00bb", "be\u00b7ken\u00b7ne", "!", "\u00ab", "mahnt", "aufs", "Neu", "des", "Vogts", "Ge\u00b7hei\u00df", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbes kam durch Satansbund, durch Zaubertrug,", "tokens": ["\u00bb", "es", "kam", "durch", "Sa\u00b7tans\u00b7bund", ",", "durch", "Zau\u00b7ber\u00b7trug", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df, wann Eisblumen rings an allen Scheiben,", "tokens": ["Da\u00df", ",", "wann", "Eis\u00b7blu\u00b7men", "rings", "an", "al\u00b7len", "Schei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Lebend'ge Rosen dir am Fenster treiben;", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ge", "Ro\u00b7sen", "dir", "am", "Fens\u00b7ter", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Da\u00df Hagelsturm des Nachbars Kohl zerschlug,", "tokens": ["Da\u00df", "Ha\u00b7gel\u00b7sturm", "des", "Nach\u00b7bars", "Kohl", "zer\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Inde\u00df dein G\u00e4rtlein s\u00fc\u00dfe Fr\u00fcchte trug.\u00ab", "tokens": ["In\u00b7de\u00df", "dein", "G\u00e4rt\u00b7lein", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", "trug", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Antwortet drauf die Maid in tiefem Weh:", "tokens": ["Ant\u00b7wor\u00b7tet", "drauf", "die", "Maid", "in", "tie\u00b7fem", "Weh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbwenn Unschuld Schuld bekennt, dann wird sie Schuld!", "tokens": ["\u00bb", "wenn", "Un\u00b7schuld", "Schuld", "be\u00b7kennt", ",", "dann", "wird", "sie", "Schuld", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Bann, der fr\u00fche Blumen lockt aus Schnee,", "tokens": ["Der", "Bann", ",", "der", "fr\u00fc\u00b7he", "Blu\u00b7men", "lockt", "aus", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Liebvolle Pfleg' ist's, Herzensungeduld", "tokens": ["Lieb\u00b7vol\u00b7le", "Pfleg'", "ist's", ",", "Her\u00b7zen\u00b7sun\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VAFIN", "$,", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Und Sehnsucht nach des Lenzes s\u00fc\u00dfer Huld.", "tokens": ["Und", "Sehn\u00b7sucht", "nach", "des", "Len\u00b7zes", "s\u00fc\u00b7\u00dfer", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Doch ist's der Mi\u00dfgunst Brauch, der Ohnmacht M\u00fchn,", "tokens": ["Doch", "ist's", "der", "Mi\u00df\u00b7gunst", "Brauch", ",", "der", "Ohn\u00b7macht", "M\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die eigne F\u00e4ulni\u00df sehn in fremdem Bl\u00fchn;", "tokens": ["Die", "eig\u00b7ne", "F\u00e4ul\u00b7ni\u00df", "sehn", "in", "frem\u00b7dem", "Bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das blankste Thun, das reinste Saatenkorn", "tokens": ["Das", "blanks\u00b7te", "Thun", ",", "das", "reins\u00b7te", "Saa\u00b7ten\u00b7korn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sie meint's ged\u00fcngt nur von unreinem Born;", "tokens": ["Sie", "meint's", "ge\u00b7d\u00fcngt", "nur", "von", "un\u00b7rei\u00b7nem", "Born", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Das Reine mag ihr Auge schmerzend st\u00f6ren,", "tokens": ["Das", "Rei\u00b7ne", "mag", "ihr", "Au\u00b7ge", "schmer\u00b7zend", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Drum wird's verh\u00e4ngt mit eignen tr\u00fcben Fl\u00f6ren.", "tokens": ["Drum", "wird's", "ver\u00b7h\u00e4ngt", "mit", "eig\u00b7nen", "tr\u00fc\u00b7ben", "Fl\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "VVPP", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Du aber, meinst den Herrn so schwach und tr\u00e4ge,", "tokens": ["Du", "a\u00b7ber", ",", "meinst", "den", "Herrn", "so", "schwach", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df er die Z\u00fcgel m\u00e4cht'ger Wolkenrosse,", "tokens": ["Da\u00df", "er", "die", "Z\u00fc\u00b7gel", "m\u00e4cht'\u00b7ger", "Wol\u00b7ken\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Den Donnerkeil, des Hagelsturms Geschosse", "tokens": ["Den", "Don\u00b7ner\u00b7keil", ",", "des", "Ha\u00b7gel\u00b7sturms", "Ge\u00b7schos\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "In eines M\u00e4gdleins schwache H\u00e4nde lege?", "tokens": ["In", "ei\u00b7nes", "M\u00e4gd\u00b7leins", "schwa\u00b7che", "H\u00e4n\u00b7de", "le\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Selbst l\u00e4stert, der mich will des L\u00e4sterns zeihn!\u00ab", "tokens": ["Selbst", "l\u00e4s\u00b7tert", ",", "der", "mich", "will", "des", "L\u00e4s\u00b7terns", "zeihn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da winkt der Vogt. Die Schergen treten ein;", "tokens": ["Da", "winkt", "der", "Vogt", ".", "Die", "Scher\u00b7gen", "tre\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von rohen F\u00e4usten wird das zarte Weib", "tokens": ["Von", "ro\u00b7hen", "F\u00e4us\u00b7ten", "wird", "das", "zar\u00b7te", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gepackt und hingeschleppt zur blut'gen Kammer,", "tokens": ["Ge\u00b7packt", "und", "hin\u00b7ge\u00b7schleppt", "zur", "blut'\u00b7gen", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn ums Gest\u00e4ndni\u00df wirbt beim s\u00fcnd'gen Leib", "tokens": ["Denn", "ums", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "wirbt", "beim", "s\u00fcn\u00b7d'\u00b7gen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Mit neuer Qual sinnreich der \u00bbHexenhammer\u00ab,", "tokens": ["Mit", "neu\u00b7er", "Qual", "sinn\u00b7reich", "der", "\u00bb", "He\u00b7xen\u00b7ham\u00b7mer", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ART", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Buben wild zerpfl\u00fccken Blumensterne", "tokens": ["Wie", "Bu\u00b7ben", "wild", "zer\u00b7pfl\u00fc\u00b7cken", "Blu\u00b7mens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu sp\u00e4hen tiefer nach dem innern Kerne.", "tokens": ["Zu", "sp\u00e4\u00b7hen", "tie\u00b7fer", "nach", "dem", "in\u00b7nern", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Rasseln, dann ein Schrei, der Todte weckt!", "tokens": ["Ein", "Ras\u00b7seln", ",", "dann", "ein", "Schrei", ",", "der", "Tod\u00b7te", "weckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,",