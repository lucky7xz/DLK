{"dta.poem.8526": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "O  Nacht !", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Nacht, du goldgesticktes Zelt,", "tokens": ["O", "Nacht", ",", "du", "gold\u00b7ge\u00b7stick\u00b7tes", "Zelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Mond, du Silberlampe,", "tokens": ["O", "Mond", ",", "du", "Sil\u00b7ber\u00b7lam\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das du die ganze Welt umh\u00fcllst,", "tokens": ["Das", "du", "die", "gan\u00b7ze", "Welt", "um\u00b7h\u00fcllst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die du Allen leuchtest!", "tokens": ["Und", "die", "du", "Al\u00b7len", "leuch\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo birgt in deinen Falten sich", "tokens": ["Wo", "birgt", "in", "dei\u00b7nen", "Fal\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die allerreinste Perle?", "tokens": ["Die", "al\u00b7ler\u00b7reins\u00b7te", "Per\u00b7le", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo widerstrahlt dein tr\u00e4umend Licht", "tokens": ["Wo", "wi\u00b7der\u00b7strahlt", "dein", "tr\u00e4u\u00b7mend", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im allerklarsten Spiegel?", "tokens": ["Im", "al\u00b7ler\u00b7klars\u00b7ten", "Spie\u00b7gel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O breite siebenfach um sie", "tokens": ["O", "brei\u00b7te", "sie\u00b7ben\u00b7fach", "um", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00fctzende Gewinde,", "tokens": ["Das", "sch\u00fct\u00b7zen\u00b7de", "Ge\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Da\u00df nicht der J\u00fcngling sie erschaut!", "tokens": ["Da\u00df", "nicht", "der", "J\u00fcng\u00b7ling", "sie", "er\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auflodere in Flammen,", "tokens": ["Auf\u00b7lo\u00b7de\u00b7re", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df kein verbl\u00fchend Weib sie trifft", "tokens": ["Da\u00df", "kein", "ver\u00b7bl\u00fc\u00b7hend", "Weib", "sie", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJD", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit unheilvollem Auge!", "tokens": ["Mit", "un\u00b7heil\u00b7vol\u00b7lem", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, milde Lampe, schauend tief", "tokens": ["Und", ",", "mil\u00b7de", "Lam\u00b7pe", ",", "schau\u00b7end", "tief"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ihres Spiegels Klarheit,", "tokens": ["In", "ih\u00b7res", "Spie\u00b7gels", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Erblicktest du ein Bild darin?", "tokens": ["Er\u00b7blick\u00b7test", "du", "ein", "Bild", "da\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und war es, ach, das meine?", "tokens": ["Und", "war", "es", ",", "ach", ",", "das", "mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "ITJ", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}