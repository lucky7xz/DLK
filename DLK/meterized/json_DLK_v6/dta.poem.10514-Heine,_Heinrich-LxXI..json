{"dta.poem.10514": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wie dunkle Tr\u00e4ume stehen", "tokens": ["Wie", "dunk\u00b7le", "Tr\u00e4u\u00b7me", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00e4user in langer Reih';", "tokens": ["Die", "H\u00e4u\u00b7ser", "in", "lan\u00b7ger", "Reih'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief eingeh\u00fcllt im Mantel", "tokens": ["Tief", "ein\u00b7ge\u00b7h\u00fcllt", "im", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schreite ich schweigend vorbei.", "tokens": ["Schrei\u00b7te", "ich", "schwei\u00b7gend", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Der Thurm der Cathedrale", "tokens": ["Der", "Thurm", "der", "Cat\u00b7hed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verk\u00fcndet die zw\u00f6lfte Stund';", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "die", "zw\u00f6lf\u00b7te", "Stund'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit ihren Reizen und K\u00fcssen", "tokens": ["Mit", "ih\u00b7ren", "Rei\u00b7zen", "und", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erwartet mich Liebchen jetzund.", "tokens": ["Er\u00b7war\u00b7tet", "mich", "Lieb\u00b7chen", "je\u00b7tzund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Der Mond ist mein Begleiter,", "tokens": ["Der", "Mond", "ist", "mein", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er leuchtet mir freundlich vor;", "tokens": ["Er", "leuch\u00b7tet", "mir", "freund\u00b7lich", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da bin ich an ihrem Hause,", "tokens": ["Da", "bin", "ich", "an", "ih\u00b7rem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und freudig ruf' ich empor:", "tokens": ["Und", "freu\u00b7dig", "ruf'", "ich", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ich danke dir, alter Vertrauter,", "tokens": ["Ich", "dan\u00b7ke", "dir", ",", "al\u00b7ter", "Ver\u00b7trau\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df du meinen Weg erhellt;", "tokens": ["Da\u00df", "du", "mei\u00b7nen", "Weg", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt will ich dich entlassen,", "tokens": ["Jetzt", "will", "ich", "dich", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt leuchte der \u00fcbrigen Welt!", "tokens": ["Jetzt", "leuch\u00b7te", "der", "\u00fcb\u00b7ri\u00b7gen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und findest du einen Verliebten,", "tokens": ["Und", "fin\u00b7dest", "du", "ei\u00b7nen", "Ver\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der einsam klagt sein Leid,", "tokens": ["Der", "ein\u00b7sam", "klagt", "sein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00f6st' ihn, wie du mich selber", "tokens": ["So", "tr\u00f6st'", "ihn", ",", "wie", "du", "mich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Getr\u00f6stet in alter Zeit.", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", "in", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}