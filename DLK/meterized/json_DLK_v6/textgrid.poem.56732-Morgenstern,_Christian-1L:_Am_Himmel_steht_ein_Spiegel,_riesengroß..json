{"textgrid.poem.56732": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Himmel steht ein Spiegel, riesengro\u00df.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel steht ein Spiegel, riesengro\u00df.", "tokens": ["Am", "Him\u00b7mel", "steht", "ein", "Spie\u00b7gel", ",", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wunderland, im klarsten Sonnenlichte,", "tokens": ["Ein", "Wun\u00b7der\u00b7land", ",", "im", "klars\u00b7ten", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "entw\u00e4chst ber\u00fcckend dem kristallnen Scho\u00df.", "tokens": ["ent\u00b7w\u00e4chst", "be\u00b7r\u00fc\u00b7ckend", "dem", "kris\u00b7tall\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um bunter Tempel marmorne Gedichte", "tokens": ["Um", "bun\u00b7ter", "Tem\u00b7pel", "mar\u00b7mor\u00b7ne", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "ergr\u00fcnt geheimnisvoller Haine Kranz;", "tokens": ["er\u00b7gr\u00fcnt", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Hai\u00b7ne", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der Seen Silber dunkle K\u00e4hne spalten,", "tokens": ["der", "Seen", "Sil\u00b7ber", "dunk\u00b7le", "K\u00e4h\u00b7ne", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "und wallender Gew\u00e4nder heller Glanz", "tokens": ["und", "wal\u00b7len\u00b7der", "Ge\u00b7w\u00e4n\u00b7der", "hel\u00b7ler", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "verr\u00e4t dem Auge wandelnde Gestalten.", "tokens": ["ver\u00b7r\u00e4t", "dem", "Au\u00b7ge", "wan\u00b7deln\u00b7de", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl kenn ich dich, du seliges Gefild! ...", "tokens": ["Wohl", "kenn", "ich", "dich", ",", "du", "se\u00b7li\u00b7ges", "Ge\u00b7fild", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch was in heitrer Ruh ergl\u00e4nzt dort oben,", "tokens": ["Doch", "was", "in", "hei\u00b7trer", "Ruh", "er\u00b7gl\u00e4nzt", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ist mehr als dein getreues Spiegelbild,", "tokens": ["ist", "mehr", "als", "dein", "ge\u00b7treu\u00b7es", "Spie\u00b7gel\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ist Irdisches zu G\u00f6ttlichem erhoben.", "tokens": ["ist", "Ir\u00b7di\u00b7sches", "zu", "G\u00f6tt\u00b7li\u00b7chem", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du zeigst ein friedsam wolkenloses Gl\u00fcck,", "tokens": ["Du", "zeigst", "ein", "fried\u00b7sam", "wol\u00b7ken\u00b7lo\u00b7ses", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "um das umsonst die Staubgebornen werben ...", "tokens": ["um", "das", "um\u00b7sonst", "die", "Staub\u00b7ge\u00b7bor\u00b7nen", "wer\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und doch! Auch du bist nur ein Schemenst\u00fcck!", "tokens": ["Und", "doch", "!", "Auch", "du", "bist", "nur", "ein", "Sche\u00b7men\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Hauch \u2013: Du schl\u00e4fst im Grund in tausend Scherben.", "tokens": ["Ein", "Hauch", "\u2013", ":", "Du", "schl\u00e4fst", "im", "Grund", "in", "tau\u00b7send", "Scher\u00b7ben", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "$.", "PPER", "VVFIN", "APPRART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Hauch! ... Von d\u00fcstren Wolken l\u00f6st ein Flug", "tokens": ["Ein", "Hauch", "!", "...", "Von", "d\u00fcst\u00b7ren", "Wol\u00b7ken", "l\u00f6st", "ein", "Flug"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "APPR", "VVFIN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich von der Felskluft Schautrib\u00fcnenstufen.", "tokens": ["sich", "von", "der", "Fels\u00b7kluft", "Schau\u00b7tri\u00b7b\u00fc\u00b7nen\u00b7stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um meinen Gipfel streift ihr dumpfer Zug,", "tokens": ["Um", "mei\u00b7nen", "Gip\u00b7fel", "streift", "ihr", "dum\u00b7pfer", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "als h\u00e4tte sie mein f\u00fcrchtend Herz gerufen.", "tokens": ["als", "h\u00e4t\u00b7te", "sie", "mein", "f\u00fcrch\u00b7tend", "Herz", "ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hinunter weist beschw\u00f6rend meine Hand,", "tokens": ["Hin\u00b7un\u00b7ter", "weist", "be\u00b7schw\u00f6\u00b7rend", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "indes mein Aug nach oben bittet \u00bbBleibe!\u00ab \u2013", "tokens": ["in\u00b7des", "mein", "Aug", "nach", "o\u00b7ben", "bit\u00b7tet", "\u00bb", "Blei\u00b7be", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$(", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Umsonst! Ein Sto\u00df zermalmt des Spiegels Rand,", "tokens": ["Um\u00b7sonst", "!", "Ein", "Sto\u00df", "zer\u00b7malmt", "des", "Spie\u00b7gels", "Rand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und donnernd b\u00e4umt sich die gewaltige Scheibe", "tokens": ["und", "don\u00b7nernd", "b\u00e4umt", "sich", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "und st\u00fcrzt, von tausend Spr\u00fcngen \u00fcberzackt,", "tokens": ["und", "st\u00fcrzt", ",", "von", "tau\u00b7send", "Spr\u00fcn\u00b7gen", "\u00fc\u00b7berz\u00b7ackt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit f\u00fcrchterlichem Tosen in die Tiefen.", "tokens": ["mit", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "To\u00b7sen", "in", "die", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Abgrund schreit, von wildem Graun gepackt.", "tokens": ["Der", "Ab\u00b7grund", "schreit", ",", "von", "wil\u00b7dem", "Graun", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Blut\u00fcberstr\u00f6mt die Wolken talw\u00e4rts triefen.", "tokens": ["Blut\u00b7\u00fc\u00b7ber\u00b7str\u00f6mt", "die", "Wol\u00b7ken", "tal\u00b7w\u00e4rts", "trie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fahlgr\u00fcner Splitterregen spritzt umher,", "tokens": ["Fahl\u00b7gr\u00fc\u00b7ner", "Split\u00b7ter\u00b7re\u00b7gen", "spritzt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "den Leib der Nacht zerschneidend und zerfleischend.", "tokens": ["den", "Leib", "der", "Nacht", "zer\u00b7schnei\u00b7dend", "und", "zer\u00b7flei\u00b7schend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mordbr\u00fcllend w\u00fchlt der Sturm im Nebelmeer", "tokens": ["Mord\u00b7br\u00fcl\u00b7lend", "w\u00fchlt", "der", "Sturm", "im", "Ne\u00b7bel\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und heult in jede H\u00f6hle, wollustkreischend.", "tokens": ["und", "heult", "in", "je\u00b7de", "H\u00f6h\u00b7le", ",", "wol\u00b7lust\u00b7krei\u00b7schend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Berge Adern schwellen, brechen auf", "tokens": ["Der", "Ber\u00b7ge", "A\u00b7dern", "schwel\u00b7len", ",", "bre\u00b7chen", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und sch\u00e4umen graue F\u00fclle ins Gekl\u00fcfte.", "tokens": ["und", "sch\u00e4u\u00b7men", "grau\u00b7e", "F\u00fcl\u00b7le", "ins", "Ge\u00b7kl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Flutsturz rei\u00dft verstreuter Scherben Hauf", "tokens": ["Ihr", "Fluts\u00b7turz", "rei\u00dft", "ver\u00b7streu\u00b7ter", "Scher\u00b7ben", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "unhemmbar mit in finstre Waldnachtgr\u00fcfte.", "tokens": ["un\u00b7hemm\u00b7bar", "mit", "in", "finst\u00b7re", "Wald\u00b7nacht\u00b7gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Es wog der Forsten nasses Kronenhaar,", "tokens": ["Es", "wog", "der", "Fors\u00b7ten", "nas\u00b7ses", "Kro\u00b7nen\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "durchblendet von demantnem Pfeilgewimmel ...", "tokens": ["durch\u00b7blen\u00b7det", "von", "de\u00b7mant\u00b7nem", "Pfeil\u00b7ge\u00b7wim\u00b7mel", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch um die H\u00f6hen wird es langsam klar,", "tokens": ["Doch", "um", "die", "H\u00f6\u00b7hen", "wird", "es", "lang\u00b7sam", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "durch Tr\u00e4nen l\u00e4chelt der beraubte Himmel.", "tokens": ["durch", "Tr\u00e4\u00b7nen", "l\u00e4\u00b7chelt", "der", "be\u00b7raub\u00b7te", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und bald verblitzt der letzten Scherbe Schein,", "tokens": ["Und", "bald", "ver\u00b7blitzt", "der", "letz\u00b7ten", "Scher\u00b7be", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zum Grund gefegt vom Sturm- und Wellentanze.", "tokens": ["zum", "Grund", "ge\u00b7fegt", "vom", "Stur\u00b7m", "und", "Wel\u00b7len\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPRART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur feiner Glasstaub deckt noch Baum und Stein", "tokens": ["Nur", "fei\u00b7ner", "Glas\u00b7staub", "deckt", "noch", "Baum", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADV", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und funkelt tausendfach im Sonnenglanze ...", "tokens": ["und", "fun\u00b7kelt", "tau\u00b7send\u00b7fach", "im", "Son\u00b7nen\u00b7glan\u00b7ze", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich schau, ich sinne, hab der Zeit nicht acht \u2013:", "tokens": ["Ich", "schau", ",", "ich", "sin\u00b7ne", ",", "hab", "der", "Zeit", "nicht", "acht", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "PTKNEG", "CARD", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Tag verscheuchte l\u00e4ngst der Schattenriese.", "tokens": ["Den", "Tag", "ver\u00b7scheuch\u00b7te", "l\u00e4ngst", "der", "Schat\u00b7ten\u00b7rie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und aus der Tiefe predigen durch die Nacht", "tokens": ["Und", "aus", "der", "Tie\u00b7fe", "pre\u00b7di\u00b7gen", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "die F\u00e4lle vom versunknen Paradiese.", "tokens": ["die", "F\u00e4l\u00b7le", "vom", "ver\u00b7sun\u00b7knen", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}