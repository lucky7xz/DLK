{"textgrid.poem.66298": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geist Jehovah's, o du Taube", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geist Jehovah's, o du Taube", "tokens": ["Geist", "Je\u00b7ho\u00b7vah's", ",", "o", "du", "Tau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "FM", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes, schwebe \u00fcber mir!", "tokens": ["Got\u00b7tes", ",", "schwe\u00b7be", "\u00fc\u00b7ber", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe, Hoffnung, Treu und Glaube", "tokens": ["Lie\u00b7be", ",", "Hoff\u00b7nung", ",", "Treu", "und", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist doch ein Geschenk von dir.", "tokens": ["Ist", "doch", "ein", "Ge\u00b7schenk", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strafe, drohe, tr\u00f6ste, mahne,", "tokens": ["Stra\u00b7fe", ",", "dro\u00b7he", ",", "tr\u00f6s\u00b7te", ",", "mah\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halte mich bei Jesu Fahne", "tokens": ["Hal\u00b7te", "mich", "bei", "Je\u00b7su", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Treu und auch im Tode fest,", "tokens": ["Treu", "und", "auch", "im", "To\u00b7de", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df mich Glaube nicht verl\u00e4\u00dft.", "tokens": ["Da\u00df", "mich", "Glau\u00b7be", "nicht", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sinken w\u00fcrd' ich, ganz versinken,", "tokens": ["Sin\u00b7ken", "w\u00fcrd'", "ich", ",", "ganz", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Gnade mich verlie\u00df;", "tokens": ["Wenn", "die", "Gna\u00b7de", "mich", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie des Lichtes Str\u00f6me trinken;", "tokens": ["Nie", "des", "Lich\u00b7tes", "Str\u00f6\u00b7me", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Geno\u00df der Finsterni\u00df,", "tokens": ["Ein", "Ge\u00b7no\u00df", "der", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrd' ich, ach! gedr\u00fcckt vom Fluche,", "tokens": ["W\u00fcrd'", "ich", ",", "ach", "!", "ge\u00b7dr\u00fcckt", "vom", "Flu\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ITJ", "$.", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgel\u00f6scht aus Gottes Buche;", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "aus", "Got\u00b7tes", "Bu\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd' empfinden in der Pein", "tokens": ["W\u00fcrd'", "emp\u00b7fin\u00b7den", "in", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach! ein f\u00fcrchterliches Sein!", "tokens": ["Ach", "!", "ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum versto\u00df mich nicht im Grimme,", "tokens": ["Drum", "ver\u00b7sto\u00df", "mich", "nicht", "im", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott! am ernsten Weltgericht;", "tokens": ["Gott", "!", "am", "erns\u00b7ten", "Welt\u00b7ge\u00b7richt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re, wie des Sohnes Stimme", "tokens": ["H\u00f6\u00b7re", ",", "wie", "des", "Soh\u00b7nes", "Stim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr mich, Gnade! Gnade! spricht.", "tokens": ["F\u00fcr", "mich", ",", "Gna\u00b7de", "!", "Gna\u00b7de", "!", "spricht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm mich auf zu deinem Kinde;", "tokens": ["Nimm", "mich", "auf", "zu", "dei\u00b7nem", "Kin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O dann sing ich frei von S\u00fcnde", "tokens": ["O", "dann", "sing", "ich", "frei", "von", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In des neuen Lichtes Glanz:", "tokens": ["In", "des", "neu\u00b7en", "Lich\u00b7tes", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vater! Gnade bist du ganz!", "tokens": ["Va\u00b7ter", "!", "Gna\u00b7de", "bist", "du", "ganz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}