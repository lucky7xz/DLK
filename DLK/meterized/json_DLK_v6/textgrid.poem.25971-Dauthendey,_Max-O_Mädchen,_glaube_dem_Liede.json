{"textgrid.poem.25971": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "O M\u00e4dchen, glaube dem Liede", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weine nicht, weine nicht wieder,", "tokens": ["Wei\u00b7ne", "nicht", ",", "wei\u00b7ne", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Uralter Fr\u00fchling kommt bald.", "tokens": ["Ur\u00b7al\u00b7ter", "Fr\u00fch\u00b7ling", "kommt", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann nisten in Wolken die Lieder,", "tokens": ["Dann", "nis\u00b7ten", "in", "Wol\u00b7ken", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann unter den nickenden B\u00e4umen", "tokens": ["Dann", "un\u00b7ter", "den", "ni\u00b7cken\u00b7den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "S\u00e4umen Blumen Verliebten die Wege,", "tokens": ["S\u00e4u\u00b7men", "Blu\u00b7men", "Ver\u00b7lieb\u00b7ten", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Bis in den zufriedenen Wald.", "tokens": ["Bis", "in", "den", "zu\u00b7frie\u00b7de\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Dort sitzt das Gl\u00fcck im Gehege,", "tokens": ["Dort", "sitzt", "das", "Gl\u00fcck", "im", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und das Gl\u00fcck ist blind und taub.", "tokens": ["Und", "das", "Gl\u00fcck", "ist", "blind", "und", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O M\u00e4dchen, glaube dem Liede:", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "glau\u00b7be", "dem", "Lie\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Auch die Tr\u00e4ne wird einmal zu Staub.", "tokens": ["Auch", "die", "Tr\u00e4\u00b7ne", "wird", "ein\u00b7mal", "zu", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}}}}}