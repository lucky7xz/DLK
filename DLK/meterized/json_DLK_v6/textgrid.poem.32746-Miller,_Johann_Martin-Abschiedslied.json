{"textgrid.poem.32746": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedslied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig sehen wir uns an,", "tokens": ["Trau\u00b7rig", "se\u00b7hen", "wir", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Achten nicht des Weines!", "tokens": ["Ach\u00b7ten", "nicht", "des", "Wei\u00b7nes", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder schl\u00e4gt die Augen nieder,", "tokens": ["Je\u00b7der", "schl\u00e4gt", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der hohen Freudenlieder", "tokens": ["Und", "der", "ho\u00b7hen", "Freu\u00b7den\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schallet heute keines.", "tokens": ["Schal\u00b7let", "heu\u00b7te", "kei\u00b7nes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nun so soll ein Trauerlied", "tokens": ["Nun", "so", "soll", "ein", "Trau\u00b7er\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir, o Freund, erschallen!", "tokens": ["Dir", ",", "o", "Freund", ",", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Trinket jeder ihm zur Ehre,", "tokens": ["Trin\u00b7ket", "je\u00b7der", "ihm", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und la\u00dft der Trennung Z\u00e4hre", "tokens": ["Ach", ",", "und", "la\u00dft", "der", "Tren\u00b7nung", "Z\u00e4h\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Becher fallen.", "tokens": ["In", "den", "Be\u00b7cher", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zeuch in fernes Land, und denk", "tokens": ["Zeuch", "in", "fer\u00b7nes", "Land", ",", "und", "denk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsers Bunds hienieden!", "tokens": ["Un\u00b7sers", "Bunds", "hien\u00b7ie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dort am Sternenhimmel, Bester!", "tokens": ["Dort", "am", "Ster\u00b7nen\u00b7him\u00b7mel", ",", "Bes\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kn\u00fcpft die Ewigkeit ihn fester!", "tokens": ["Kn\u00fcpft", "die", "E\u00b7wig\u00b7keit", "ihn", "fes\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leb indes in Frieden!", "tokens": ["Leb", "in\u00b7des", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Edel warest du und treu,", "tokens": ["E\u00b7del", "wa\u00b7rest", "du", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromm und deutsches Herzens!", "tokens": ["Fromm", "und", "deut\u00b7sches", "Her\u00b7zens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bleib es, Lieber! Edeln Seelen", "tokens": ["Bleib", "es", ",", "Lie\u00b7ber", "!", "E\u00b7deln", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJD", "$.", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann's an Freuden nirgends fehlen!", "tokens": ["Kann's", "an", "Freu\u00b7den", "nir\u00b7gends", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vergi\u00df des Schmerzens!", "tokens": ["Und", "ver\u00b7gi\u00df", "des", "Schmer\u00b7zens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Heilig war uns mancher Tag,", "tokens": ["Hei\u00b7lig", "war", "uns", "man\u00b7cher", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher Abend heilig!", "tokens": ["Man\u00b7cher", "A\u00b7bend", "hei\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freundschaft gab uns alles Gutes,", "tokens": ["Freund\u00b7schaft", "gab", "uns", "al\u00b7les", "Gu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundschaft macht' uns hohes Mutes!", "tokens": ["Freund\u00b7schaft", "macht'", "uns", "ho\u00b7hes", "Mu\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! und schwand so eilig!", "tokens": ["Ach", "!", "und", "schwand", "so", "ei\u00b7lig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nun noch eins zu guter Letzt,", "tokens": ["Nun", "noch", "eins", "zu", "gu\u00b7ter", "Letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unserm Freund zu Ehren!", "tokens": ["Un\u00b7serm", "Freund", "zu", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heute sind wir noch vereinet!", "tokens": ["Heu\u00b7te", "sind", "wir", "noch", "ver\u00b7ei\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen, wenn die Stund' erscheinet,", "tokens": ["Mor\u00b7gen", ",", "wenn", "die", "Stund'", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flie\u00dfen unsre Z\u00e4hren!", "tokens": ["Flie\u00b7\u00dfen", "uns\u00b7re", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}