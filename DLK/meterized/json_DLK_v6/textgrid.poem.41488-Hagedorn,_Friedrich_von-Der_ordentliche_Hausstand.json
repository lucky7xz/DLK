{"textgrid.poem.41488": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der ordentliche Hausstand", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Crispin geht stets berauscht zu Bette,", "tokens": ["Cris\u00b7pin", "geht", "stets", "be\u00b7rauscht", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00f6fters, wann der Tag schon graut.", "tokens": ["Und", "\u00f6f\u00b7ters", ",", "wann", "der", "Tag", "schon", "graut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Weib, die l\u00e4chelnde Finette,", "tokens": ["Sein", "Weib", ",", "die", "l\u00e4\u00b7cheln\u00b7de", "Fi\u00b7net\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lebt mit dem Nachbar recht vertraut.", "tokens": ["Lebt", "mit", "dem", "Nach\u00b7bar", "recht", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum rennt Crispin zum neuen Schmause", "tokens": ["Kaum", "rennt", "Cris\u00b7pin", "zum", "neu\u00b7en", "Schmau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wittert angenehmen Wein:", "tokens": ["Und", "wit\u00b7tert", "an\u00b7ge\u00b7neh\u00b7men", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schleicht sein Weibchen aus dem Hause", "tokens": ["So", "schleicht", "sein", "Weib\u00b7chen", "aus", "dem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt den Nachbar selbst hinein.", "tokens": ["Und", "f\u00fchrt", "den", "Nach\u00b7bar", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er lobet und beschreibt ihr kl\u00fcglich", "tokens": ["Er", "lo\u00b7bet", "und", "be\u00b7schreibt", "ihr", "kl\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den wohlgenoss'nen Rebensaft:", "tokens": ["Den", "wohl\u00b7ge\u00b7noss'\u00b7nen", "Re\u00b7ben\u00b7saft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie aber r\u00fchmt ihm unverz\u00fcglich", "tokens": ["Sie", "a\u00b7ber", "r\u00fchmt", "ihm", "un\u00b7ver\u00b7z\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nachbars gute Nachbarschaft.", "tokens": ["Des", "Nach\u00b7bars", "gu\u00b7te", "Nach\u00b7bar\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Nachmittags- und Abendstunden", "tokens": ["Die", "Nach\u00b7mit\u00b7tags", "und", "A\u00b7bends\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt sie mit ihrem Nachbar zu,", "tokens": ["Bringt", "sie", "mit", "ih\u00b7rem", "Nach\u00b7bar", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wann die Nacht sich eingefunden,", "tokens": ["Und", "wann", "die", "Nacht", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bef\u00f6rdert sie des Mannes Ruh.", "tokens": ["Be\u00b7f\u00f6r\u00b7dert", "sie", "des", "Man\u00b7nes", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der gute Mann wei\u00df nichts vom Neide:", "tokens": ["Der", "gu\u00b7te", "Mann", "wei\u00df", "nichts", "vom", "Nei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gute Frau darf sich erfreun.", "tokens": ["Die", "gu\u00b7te", "Frau", "darf", "sich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er g\u00f6nnt Finetten ihre Freude;", "tokens": ["Er", "g\u00f6nnt", "Fi\u00b7net\u00b7ten", "ih\u00b7re", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie g\u00f6nnt Crispinen seinen Wein.", "tokens": ["Sie", "g\u00f6nnt", "Cris\u00b7pi\u00b7nen", "sei\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Weiber, die den M\u00e4nnern fluchen,", "tokens": ["Die", "Wei\u00b7ber", ",", "die", "den", "M\u00e4n\u00b7nern", "flu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie zu oft zu Weine gehn,", "tokens": ["Wenn", "sie", "zu", "oft", "zu", "Wei\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sollten dieses Haus besuchen", "tokens": ["Die", "soll\u00b7ten", "die\u00b7ses", "Haus", "be\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Finette Beispiel sehn.", "tokens": ["Und", "der", "Fi\u00b7net\u00b7te", "Bei\u00b7spiel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ganzes Haus- und Wirthschaftswesen", "tokens": ["Ihr", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den M\u00e4nnern, die auf Weiber schm\u00e4hlen,", "tokens": ["Den", "M\u00e4n\u00b7nern", ",", "die", "auf", "Wei\u00b7ber", "schm\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie der Nachbar sittlich macht,", "tokens": ["Wenn", "sie", "der", "Nach\u00b7bar", "sitt\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O denen kann Crispin erz\u00e4hlen,", "tokens": ["O", "de\u00b7nen", "kann", "Cris\u00b7pin", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wein ertr\u00e4nke den Verdacht.", "tokens": ["Der", "Wein", "er\u00b7tr\u00e4n\u00b7ke", "den", "Ver\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein ganzes Haus- und Wirthschaftswesen", "tokens": ["Sein", "gan\u00b7zes", "Haus", "und", "Wirth\u00b7schafts\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ordentlich und auserlesen.", "tokens": ["Ist", "or\u00b7dent\u00b7lich", "und", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}