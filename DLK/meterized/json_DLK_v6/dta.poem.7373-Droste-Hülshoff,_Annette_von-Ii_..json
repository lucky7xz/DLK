{"dta.poem.7373": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Mittagsstunde, \u2014 der Sonnenpfeil", "tokens": ["Mit\u00b7tags\u00b7stun\u00b7de", ",", "der", "Son\u00b7nen\u00b7pfeil"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Prallt an des Weihen Gefieder,", "tokens": ["Prallt", "an", "des", "Wei\u00b7hen", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der vom Gesteine grau und steil", "tokens": ["Der", "vom", "Ge\u00b7stei\u00b7ne", "grau", "und", "steil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blinzt in die Pinien nieder.", "tokens": ["Blinzt", "in", "die", "Pi\u00b7ni\u00b7en", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwarz der Wald, eine Wetternacht,", "tokens": ["Schwarz", "der", "Wald", ",", "ei\u00b7ne", "Wet\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Die aus dem Aether gesunken,", "tokens": ["Die", "aus", "dem", "A\u00b7e\u00b7ther", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Dr\u00fcber der Stral in Siegespracht", "tokens": ["Dr\u00fc\u00b7ber", "der", "Stral", "in", "Sie\u00b7ge\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Tanzt auf dem Feinde wie trunken.", "tokens": ["Tanzt", "auf", "dem", "Fein\u00b7de", "wie", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Pl\u00f6tzlich zuckt, es flattert der Weih,", "tokens": ["Pl\u00f6tz\u00b7lich", "zuckt", ",", "es", "flat\u00b7tert", "der", "Weih", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und klatscht in taumelnden Ringen,", "tokens": ["Und", "klatscht", "in", "tau\u00b7meln\u00b7den", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ueber'm Riffe sein wilder Schrei,", "tokens": ["Ue\u00b7ber'm", "Rif\u00b7fe", "sein", "wil\u00b7der", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Dann steigt er, wiegend die Schwingen;", "tokens": ["Dann", "steigt", "er", ",", "wie\u00b7gend", "die", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und am Grunde es stampft und surrt,", "tokens": ["Und", "am", "Grun\u00b7de", "es", "stampft", "und", "surrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hart unter dem Felsenmaale,", "tokens": ["Hart", "un\u00b7ter", "dem", "Fel\u00b7sen\u00b7maa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Netz im Haare, Pistol im Gurt,", "tokens": ["Netz", "im", "Haa\u00b7re", ",", "Pis\u00b7tol", "im", "Gurt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zw\u00f6lf Schergen reiten zu Thale.", "tokens": ["Zw\u00f6lf", "Scher\u00b7gen", "rei\u00b7ten", "zu", "Tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo den Schatten verk\u00fcrzt das Riff", "tokens": ["Wo", "den", "Schat\u00b7ten", "ver\u00b7k\u00fcrzt", "das", "Riff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wirft \u00fcber die zitternde Aue,", "tokens": ["Wirft", "\u00fc\u00b7ber", "die", "zit\u00b7tern\u00b7de", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Starrt gefesselt der rothe Jocliffe", "tokens": ["Starrt", "ge\u00b7fes\u00b7selt", "der", "ro\u00b7the", "Jo\u00b7clif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hinauf zum Vogel in's Blaue.", "tokens": ["Hin\u00b7auf", "zum", "Vo\u00b7gel", "in's", "Blau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "D\u00fcrr seine Zunge, \u2014 kein Tropfen labt \u2014", "tokens": ["D\u00fcrr", "sei\u00b7ne", "Zun\u00b7ge", ",", "kein", "Trop\u00b7fen", "labt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "$(", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er lacht in grimmigem Hohne,", "tokens": ["Er", "lacht", "in", "grim\u00b7mi\u00b7gem", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Neben ihm der Podesta trabt", "tokens": ["Ne\u00b7ben", "ihm", "der", "Po\u00b7des\u00b7ta", "trabt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und pfeift sich eine Canzone.", "tokens": ["Und", "pfeift", "sich", "ei\u00b7ne", "Can\u00b7zo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "R\u00fcstig stampfen die Rosse fort,", "tokens": ["R\u00fcs\u00b7tig", "stamp\u00b7fen", "die", "Ros\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dann \u201ehalt!\u201c Es lagert die Bande;", "tokens": ["Dann", "\u201e", "halt", "!", "\u201c", "Es", "la\u00b7gert", "die", "Ban\u00b7de", ";"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier ein Scherge, ein anderer dort,", "tokens": ["Hier", "ein", "Scher\u00b7ge", ",", "ein", "an\u00b7de\u00b7rer", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "ADV", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Gestreckt im knisternden Sande.", "tokens": ["Ge\u00b7streckt", "im", "knis\u00b7tern\u00b7den", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Cigarre l\u00e4\u00dft an den Grund", "tokens": ["Die", "Ci\u00b7gar\u00b7re", "l\u00e4\u00dft", "an", "den", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr bl\u00e4uliches W\u00f6lkchen schwehlen,", "tokens": ["Ihr", "bl\u00e4u\u00b7li\u00b7ches", "W\u00f6lk\u00b7chen", "schweh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und der Schlauch, von Mund zu Mund,", "tokens": ["Und", "der", "Schlauch", ",", "von", "Mund", "zu", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Str\u00f6mt in die durstigen Kehlen.", "tokens": ["Str\u00f6mt", "in", "die", "durs\u00b7ti\u00b7gen", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie so lockend die Taube lacht", "tokens": ["Wie", "so", "lo\u00b7ckend", "die", "Tau\u00b7be", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Aus gr\u00fcnem duftigem Haine!", "tokens": ["Aus", "gr\u00fc\u00b7nem", "duf\u00b7ti\u00b7gem", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von den Zw\u00f6lfen heben sich acht,", "tokens": ["Von", "den", "Zw\u00f6l\u00b7fen", "he\u00b7ben", "sich", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "CARD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie schlendern entlang das Gesteine,", "tokens": ["Sie", "schlen\u00b7dern", "ent\u00b7lang", "das", "Ge\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "L\u00e4ssig, spielend, so sorgenbaar", "tokens": ["L\u00e4s\u00b7sig", ",", "spie\u00b7lend", ",", "so", "sor\u00b7gen\u00b7baar"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADV", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Wie junge Geier im Neste,", "tokens": ["Wie", "jun\u00b7ge", "Gei\u00b7er", "im", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Einer so nach dem andern schwankt", "tokens": ["Ei\u00b7ner", "so", "nach", "dem", "an\u00b7dern", "schwankt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "ART", "ADJA", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In's Gr\u00fcn' aus der sengenden Hitze,", "tokens": ["In's", "Gr\u00fcn'", "aus", "der", "sen\u00b7gen\u00b7den", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Halt! wie elektrisch Feuer rankt", "tokens": ["Halt", "!", "wie", "e\u00b7lekt\u00b7risch", "Feu\u00b7er", "rankt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PWAV", "ADJD", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Von Aug' zu Aug' ein Geblitze.", "tokens": ["Von", "Aug'", "zu", "Aug'", "ein", "Ge\u00b7blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Horch, sie fl\u00fcstern! Zwei und zwei", "tokens": ["Horch", ",", "sie", "fl\u00fcs\u00b7tern", "!", "Zwei", "und", "zwei"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVINF", "$.", "CARD", "KON", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Pinien streifen sie leise,", "tokens": ["Die", "Pi\u00b7ni\u00b7en", "strei\u00b7fen", "sie", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Wie die Hinde witternd und scheu", "tokens": ["Wie", "die", "Hin\u00b7de", "wit\u00b7ternd", "und", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Schl\u00fcpft \u00fcber befahrene Gleise.", "tokens": ["Schl\u00fcpft", "\u00fc\u00b7ber", "be\u00b7fah\u00b7re\u00b7ne", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Zwei am Hange und zwei hinab", "tokens": ["Zwei", "am", "Han\u00b7ge", "und", "zwei", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "APPRART", "NN", "KON", "CARD", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und vier zur Rechten und Linken,", "tokens": ["Und", "vier", "zur", "Rech\u00b7ten", "und", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sachte beugen den Ast sie ab,", "tokens": ["Sach\u00b7te", "beu\u00b7gen", "den", "Ast", "sie", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ihre Augen wie Vipern blinken,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "wie", "Vi\u00b7pern", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da \u2014 im Moose ein d\u00fcrrer Baum", "tokens": ["Da", "im", "Moo\u00b7se", "ein", "d\u00fcr\u00b7rer", "Baum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit wunderlich brauner Schale, \u2014", "tokens": ["Mit", "wun\u00b7der\u00b7lich", "brau\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hui! ein Pfiff auf gekr\u00fcmmtem Daum, \u2014", "tokens": ["Hui", "!", "ein", "Pfiff", "auf", "ge\u00b7kr\u00fcmm\u00b7tem", "Daum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dort \u2014 und drunten im Thale.", "tokens": ["Und", "dort", "und", "drun\u00b7ten", "im", "Tha\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KON", "ADV", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "F\u00e4hrt vom Moose Geronimo,", "tokens": ["F\u00e4hrt", "vom", "Moo\u00b7se", "Ge\u00b7ro\u00b7ni\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und eh ihn die Schergen umschlingen,