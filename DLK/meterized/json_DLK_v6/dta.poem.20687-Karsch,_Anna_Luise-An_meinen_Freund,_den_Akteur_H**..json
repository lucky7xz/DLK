{"dta.poem.20687": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n meinen Freund, den Akteur  \n H**.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein lieber H**, glaube k\u00fcnftig                 ", "tokens": ["Mein", "lie\u00b7ber", "H", "*", "*", ",", "glau\u00b7be", "k\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "ADV", "XY", "XY", "XY", "$,", "VVFIN", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Munde, dem\u2019s die Freundschaft hei\u00dft,", "tokens": ["Dem", "Mun\u00b7de", ",", "dem's", "die", "Freund\u00b7schaft", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er Dich warnen will, bescheiden und vern\u00fcnftig,", "tokens": ["Wenn", "er", "Dich", "war\u00b7nen", "will", ",", "be\u00b7schei\u00b7den", "und", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sey nicht mehr ein stolzer Geist,", "tokens": ["Und", "sey", "nicht", "mehr", "ein", "stol\u00b7zer", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey nicht ein Trozkopf, weils nicht immer", "tokens": ["Sey", "nicht", "ein", "Troz\u00b7kopf", ",", "weils", "nicht", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nach Deinem Wunsch und Sinne geht;", "tokens": ["Nach", "Dei\u00b7nem", "Wunsch", "und", "Sin\u00b7ne", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sey furchtsam bei des Gl\u00fcckes Schimmer,", "tokens": ["Sey", "furcht\u00b7sam", "bei", "des", "Gl\u00fc\u00b7ckes", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn wenn die Sonn am h\u00f6chsten steht,", "tokens": ["Denn", "wenn", "die", "Sonn", "am", "h\u00f6chs\u00b7ten", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ziehn Ungewitter sich zusammen \u2014", "tokens": ["Ziehn", "Un\u00b7ge\u00b7wit\u00b7ter", "sich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gedenke stets an meinen Rath,", "tokens": ["Ge\u00b7den\u00b7ke", "stets", "an", "mei\u00b7nen", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nie wieder eine Stadt deswegen zu verdammen,", "tokens": ["Nie", "wie\u00b7der", "ei\u00b7ne", "Stadt", "des\u00b7we\u00b7gen", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil der und jener nicht nach Deiner Hoffnung that,", "tokens": ["Weil", "der", "und", "je\u00b7ner", "nicht", "nach", "Dei\u00b7ner", "Hoff\u00b7nung", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und weil nicht tausend H\u00e4nde l\u00e4rmen", "tokens": ["Und", "weil", "nicht", "tau\u00b7send", "H\u00e4n\u00b7de", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Beim Auf- und Abtritt in dem Spiel.", "tokens": ["Beim", "Auf", "und", "Ab\u00b7tritt", "in", "dem", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wer wird sich um den Schall des Lobgeklatsches", "tokens": ["Wer", "wird", "sich", "um", "den", "Schall", "des", "Lob\u00b7ge\u00b7klat\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "h\u00e4rmen!", "tokens": ["h\u00e4r\u00b7men", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Hast Du der Kenner fein Gef\u00fchl", "tokens": ["Hast", "Du", "der", "Ken\u00b7ner", "fein", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der klugen Leute Ruhm erstrebet,", "tokens": ["Der", "klu\u00b7gen", "Leu\u00b7te", "Ruhm", "er\u00b7stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was fr\u00e4gst Du nach des P\u00f6bels Preis,", "tokens": ["Was", "fr\u00e4gst", "Du", "nach", "des", "P\u00f6\u00b7bels", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der seine Stimme laut erhebet", "tokens": ["Der", "sei\u00b7ne", "Stim\u00b7me", "laut", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und nicht den Grund zu sagen wei\u00df,", "tokens": ["Und", "nicht", "den", "Grund", "zu", "sa\u00b7gen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Warum er klatscht und bravo schreyet,", "tokens": ["Wa\u00b7rum", "er", "klatscht", "und", "bra\u00b7vo", "schre\u00b7yet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Warum er kalt ist, oder hei\u00df,", "tokens": ["Wa\u00b7rum", "er", "kalt", "ist", ",", "o\u00b7der", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Warum er sich betr\u00fcbt, kr\u00e4nkt, \u00e4rgert und erfreuet \u2014", "tokens": ["Wa\u00b7rum", "er", "sich", "be\u00b7tr\u00fcbt", ",", "kr\u00e4nkt", ",", "\u00e4r\u00b7gert", "und", "er\u00b7freu\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sey \u00fcber Viel hinweg, verdopple Deinen Flei\u00df,", "tokens": ["Sey", "\u00fc\u00b7ber", "Viel", "hin\u00b7weg", ",", "ver\u00b7dopp\u00b7le", "Dei\u00b7nen", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verfeinre Deine Kunst, und rei\u00dfe keine Possen", "tokens": ["Ver\u00b7fein\u00b7re", "Dei\u00b7ne", "Kunst", ",", "und", "rei\u00b7\u00dfe", "kei\u00b7ne", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Lachen f\u00fcr die Gallerie.", "tokens": ["Zum", "La\u00b7chen", "f\u00fcr", "die", "Gal\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leg alles ab, was mich verdrossen;", "tokens": ["Leg", "al\u00b7les", "ab", ",", "was", "mich", "ver\u00b7dros\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kostet Dich ja keine M\u00fch", "tokens": ["Es", "kos\u00b7tet", "Dich", "ja", "kei\u00b7ne", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Pfade der Natur best\u00e4ndig treu zu bleiben,", "tokens": ["Dem", "Pfa\u00b7de", "der", "Na\u00b7tur", "be\u00b7st\u00e4n\u00b7dig", "treu", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit guter Art ihr nachzugehn;", "tokens": ["Mit", "gu\u00b7ter", "Art", "ihr", "nach\u00b7zu\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wird es mich erfreun, wenn gro\u00dfe Richter", "tokens": ["Wie", "wird", "es", "mich", "er\u00b7freun", ",", "wenn", "gro\u00b7\u00dfe", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "schreiben:", "tokens": ["schrei\u00b7ben", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Der H ** spielet wahr und sch\u00f6n \u2014", "tokens": ["Der", "H", "*", "*", "spie\u00b7let", "wahr", "und", "sch\u00f6n"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "XY", "XY", "XY", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Du hasts in Deiner Macht, es wird in Deinem Willen,", "tokens": ["Du", "hasts", "in", "Dei\u00b7ner", "Macht", ",", "es", "wird", "in", "Dei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In Deinem Folgewillen stehn,", "tokens": ["In", "Dei\u00b7nem", "Fol\u00b7ge\u00b7wil\u00b7len", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die besten W\u00fcnsche zu erf\u00fcllen,", "tokens": ["Die", "bes\u00b7ten", "W\u00fcn\u00b7sche", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Deine Freunde f\u00fcr Dich thun \u2014", "tokens": ["Die", "Dei\u00b7ne", "Freun\u00b7de", "f\u00fcr", "Dich", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Vor allen Dingen ists vonn\u00f6then,", "tokens": ["Vor", "al\u00b7len", "Din\u00b7gen", "ists", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Im Gl\u00fcck und Mi\u00dfgl\u00fcck voll Gelassenheit zu ruhn,", "tokens": ["Im", "Gl\u00fcck", "und", "Mi\u00df\u00b7gl\u00fcck", "voll", "Ge\u00b7las\u00b7sen\u00b7heit", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und nimmer die Geduld zu t\u00f6dten,", "tokens": ["Und", "nim\u00b7mer", "die", "Ge\u00b7duld", "zu", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und nimmer die Bescheidenheit,", "tokens": ["Und", "nim\u00b7mer", "die", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "In guter und in b\u00f6ser Zeit. \u2014", "tokens": ["In", "gu\u00b7ter", "und", "in", "b\u00f6\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}