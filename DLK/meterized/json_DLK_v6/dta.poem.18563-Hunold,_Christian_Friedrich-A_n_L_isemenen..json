{"dta.poem.18563": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  L isemenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Schau edle Lisemene/", "tokens": ["Schau", "ed\u00b7le", "Li\u00b7se\u00b7me\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie ich entfernt noch deiner Augen-Schein", "tokens": ["Wie", "ich", "ent\u00b7fernt", "noch", "dei\u00b7ner", "Au\u00b7gen\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu meiner Lust entlehne.", "tokens": ["Zu", "mei\u00b7ner", "Lust", "ent\u00b7leh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Hertze wil daran geweidet seyn.", "tokens": ["Mein", "Hert\u00b7ze", "wil", "da\u00b7ran", "ge\u00b7wei\u00b7det", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und kan sich niemand selber hassen/", "tokens": ["Und", "kan", "sich", "nie\u00b7mand", "sel\u00b7ber", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So mu\u00df ich ihm auch die Vergn\u00fcgung lassen.", "tokens": ["So", "mu\u00df", "ich", "ihm", "auch", "die", "Ver\u00b7gn\u00fc\u00b7gung", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Himmel ist voll Sternen/", "tokens": ["Dein", "Him\u00b7mel", "ist", "voll", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die \u00fcberall mit Strahlen mich bestreun.", "tokens": ["Die", "\u00fc\u00b7be\u00b7rall", "mit", "Strah\u00b7len", "mich", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich mich gleich entfernen/", "tokens": ["Mu\u00df", "ich", "mich", "gleich", "ent\u00b7fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wilst", "tokens": ["So", "wilst"], "token_info": ["word", "word"], "pos": ["ADV", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wer deine Sonn' einmahl gesehen/", "tokens": ["Wer", "dei\u00b7ne", "Sonn'", "ein\u00b7mahl", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem wird sie in alle L\u00e4nder gehen.", "tokens": ["Mit", "dem", "wird", "sie", "in", "al\u00b7le", "L\u00e4n\u00b7der", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So k\u00fc\u00df ich in Gedancken/", "tokens": ["So", "k\u00fc\u00df", "ich", "in", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schw\u00e4ngere den holen Bauch der Lufft.", "tokens": ["Und", "schw\u00e4n\u00b7ge\u00b7re", "den", "ho\u00b7len", "Bauch", "der", "Lufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich renn' in deine Schrancken/", "tokens": ["Ich", "renn'", "in", "dei\u00b7ne", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bau' ein Schlo\u00df auff Amors s\u00fcsse Klufft.", "tokens": ["Und", "bau'", "ein", "Schlo\u00df", "auff", "A\u00b7mors", "s\u00fcs\u00b7se", "Klufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df mich in dieser Freyheit s", "tokens": ["La\u00df", "mich", "in", "die\u00b7ser", "Frey\u00b7heit", "s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Traum kr\u00e4nckt nicht/ und kan uns doch vergn\u00fcgen.", "tokens": ["Ein", "Traum", "kr\u00e4nckt", "nicht", "/", "und", "kan", "uns", "doch", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja dencke noch der Zeiten/", "tokens": ["Ja", "den\u00b7cke", "noch", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da Einsamkeit die Garten-Lust gebahr/", "tokens": ["Da", "Ein\u00b7sam\u00b7keit", "die", "Gar\u00b7ten\u00b7Lust", "ge\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als deine Lieblichkeiten", "tokens": ["Als", "dei\u00b7ne", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir so geneigt/ als ich entz\u00fcndet war.", "tokens": ["Mir", "so", "ge\u00b7neigt", "/", "als", "ich", "ent\u00b7z\u00fcn\u00b7det", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die nun so theure Rosen brechen/", "tokens": ["Die", "nun", "so", "theu\u00b7re", "Ro\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind viel zu wehrt/ da\u00df sie die Dornen stechen.", "tokens": ["Sind", "viel", "zu", "wehrt", "/", "da\u00df", "sie", "die", "Dor\u00b7nen", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKZU", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wilst du mich verpflichten/", "tokens": ["Und", "wilst", "du", "mich", "ver\u00b7pflich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schreibe nur/ da\u00df du mir g\u00fcnstig bist:", "tokens": ["So", "schrei\u00b7be", "nur", "/", "da\u00df", "du", "mir", "g\u00fcns\u00b7tig", "bist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wort kan viel verrichten/", "tokens": ["Ein", "Wort", "kan", "viel", "ver\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df uns zum Trost der sch\u00f6nste Mund gek\u00fcst.", "tokens": ["Da\u00df", "uns", "zum", "Trost", "der", "sch\u00f6ns\u00b7te", "Mund", "ge\u00b7k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darff ich dein Paradie\u00df nicht schauen/", "tokens": ["Darff", "ich", "dein", "Pa\u00b7ra\u00b7die\u00df", "nicht", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan ichs dadurch doch in Gedancken bauen.", "tokens": ["Kan", "ichs", "da\u00b7durch", "doch", "in", "Ge\u00b7dan\u00b7cken", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PAV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich bau' es in den Schatten/", "tokens": ["Ich", "bau'", "es", "in", "den", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der dich mir stets als einen Engel weist.", "tokens": ["Der", "dich", "mir", "stets", "als", "ei\u00b7nen", "En\u00b7gel", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Engeln sich zu gatten/", "tokens": ["Mit", "En\u00b7geln", "sich", "zu", "gat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist zwar die Lust/ die mehr als menschlich heist.", "tokens": ["Ist", "zwar", "die", "Lust", "/", "die", "mehr", "als", "menschlich", "heist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "ART", "PIAT", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Allein mein Himmel ist auff Erden/", "tokens": ["Al\u00b7lein", "mein", "Him\u00b7mel", "ist", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du bey mir wirst wieder menschlich werden.", "tokens": ["Wenn", "du", "bey", "mir", "wirst", "wie\u00b7der", "menschlich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}