{"dta.poem.13111": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas bin ich? Geist und Herz entflam\u0303en sich f\u00fcr Freuden,", "tokens": ["\u201e", "was", "bin", "ich", "?", "Geist", "und", "Herz", "ent\u00b7flam\u0303en", "sich", "f\u00fcr", "Freu\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "NN", "KON", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201ewann man das Auge will in diesen Zweyen weiden.", "tokens": ["\u201e", "wann", "man", "das", "Au\u00b7ge", "will", "in", "die\u00b7sen", "Zwe\u00b7yen", "wei\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "ART", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas Herz frolocket, springt und wird in sich entz\u00fcndt,", "tokens": ["\u201e", "das", "Herz", "fro\u00b7lo\u00b7cket", ",", "springt", "und", "wird", "in", "sich", "ent\u00b7z\u00fcndt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VAFIN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ebi\u00df es die Regungen zu zeigen Mittel findt;", "tokens": ["\u201e", "bi\u00df", "es", "die", "Re\u00b7gun\u00b7gen", "zu", "zei\u00b7gen", "Mit\u00b7tel", "findt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ART", "NN", "PTKZU", "VVINF", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "\u201edas K\u00f6nigliche Paar ist in ihm eingeschrieben,", "tokens": ["\u201e", "das", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Paar", "ist", "in", "ihm", "ein\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "460\u201dSo wird es zu der Treu und Ehrfurcht angetrieben.", "tokens": ["\"", "So", "wird", "es", "zu", "der", "Treu", "und", "Ehr\u00b7furcht", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}