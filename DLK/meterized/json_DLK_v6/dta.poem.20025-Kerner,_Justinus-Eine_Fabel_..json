{"dta.poem.20025": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Eine Fabel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling war's im Land geworden", "tokens": ["Fr\u00fch\u00b7ling", "wa\u00b7r's", "im", "Land", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPRART", "NN", "VAPP"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und der Winter ward vertagt,", "tokens": ["Und", "der", "Win\u00b7ter", "ward", "ver\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne da\u00df den Herrenorden", "tokens": ["Oh\u00b7ne", "da\u00df", "den", "Her\u00b7ren\u00b7or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott noch lange drum befragt.", "tokens": ["Gott", "noch", "lan\u00b7ge", "drum", "be\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jenen packt de\u00df Zorn und Trauer,", "tokens": ["Je\u00b7nen", "packt", "de\u00df", "Zorn", "und", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er ruft: ", "tokens": ["Und", "er", "ruft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u201enehm' ihn nicht, du dummer Bauer!", "tokens": ["\u201e", "nehm'", "ihn", "nicht", ",", "du", "dum\u00b7mer", "Bau\u00b7er", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eer ist klares H\u00f6llenlicht!", "tokens": ["\u201e", "er", "ist", "kla\u00b7res", "H\u00f6l\u00b7len\u00b7licht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ediese Sonne ungeladen", "tokens": ["\u201e", "die\u00b7se", "Son\u00b7ne", "un\u00b7ge\u00b7la\u00b7den"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PDAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201edring' zu mir nicht frevelnd ein!\u201c", "tokens": ["\u201e", "dring'", "zu", "mir", "nicht", "fre\u00b7velnd", "ein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft's und schlie\u00dft den Fensterladen,", "tokens": ["Ruft's", "und", "schlie\u00dft", "den", "Fens\u00b7ter\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcllt sich in die Wildschur ein.", "tokens": ["H\u00fcllt", "sich", "in", "die", "Wild\u00b7schur", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ruhig strahlt die Sonne", "tokens": ["A\u00b7ber", "ru\u00b7hig", "strahlt", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es keimt die Saat mit Lust,", "tokens": ["Und", "es", "keimt", "die", "Saat", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fcrger, Bauer, dankt in Wonne", "tokens": ["B\u00fcr\u00b7ger", ",", "Bau\u00b7er", ",", "dankt", "in", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott daf\u00fcr in tiefer Brust.", "tokens": ["Gott", "da\u00b7f\u00fcr", "in", "tie\u00b7fer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber hinterm Ofen sitzen", "tokens": ["A\u00b7ber", "hin\u00b7term", "O\u00b7fen", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt der Herr und schimpft und flucht:", "tokens": ["Bleibt", "der", "Herr", "und", "schimpft", "und", "flucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ein der Wildschur will ich schwitzen,", "tokens": ["\u201e", "in", "der", "Wild\u00b7schur", "will", "ich", "schwit\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eich hab' keinen Lenz gesucht!\u201c", "tokens": ["\u201e", "ich", "hab'", "kei\u00b7nen", "Lenz", "ge\u00b7sucht", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00fcthend mit den F\u00fc\u00dfen stampft er:", "tokens": ["W\u00fct\u00b7hend", "mit", "den", "F\u00fc\u00b7\u00dfen", "stampft", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ewer ihn lobt ist schlecht und dumm!\u201c", "tokens": ["\u201e", "wer", "ihn", "lobt", "ist", "schlecht", "und", "dumm", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "VVFIN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus seiner Pfeife dampft er", "tokens": ["Und", "aus", "sei\u00b7ner", "Pfei\u00b7fe", "dampft", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blauen Dunst um sich herum.", "tokens": ["Blau\u00b7en", "Dunst", "um", "sich", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch der Bauer schlicht und wacker", "tokens": ["Doch", "der", "Bau\u00b7er", "schlicht", "und", "wa\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ruft: \u201eo Herr! Ihr wi\u00dft es nicht!", "tokens": ["Ruft", ":", "\u201e", "o", "Herr", "!", "Ihr", "wi\u00dft", "es", "nicht", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "FM", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ewas schon l\u00e4ngst gebrach dem Acker", "tokens": ["\u201e", "was", "schon", "l\u00e4ngst", "ge\u00b7brach", "dem", "A\u00b7cker"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201edas ist eben dieses Licht!\u201c", "tokens": ["\u201e", "das", "ist", "e\u00b7ben", "die\u00b7ses", "Licht", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201ewill euch dieses Licht nicht frommen,", "tokens": ["\u201e", "will", "euch", "die\u00b7ses", "Licht", "nicht", "from\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201enun! so schlie\u00dft vor ihm das Haus,", "tokens": ["\u201e", "nun", "!", "so", "schlie\u00dft", "vor", "ihm", "das", "Haus", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eaber, Herr! wem es willkommen,", "tokens": ["\u201e", "a\u00b7ber", ",", "Herr", "!", "wem", "es", "will\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$.", "PWS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eden la\u00dft ungeschimpft hinaus!\u201c", "tokens": ["\u201e", "den", "la\u00dft", "un\u00b7ge\u00b7schimpft", "hin\u00b7aus", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}