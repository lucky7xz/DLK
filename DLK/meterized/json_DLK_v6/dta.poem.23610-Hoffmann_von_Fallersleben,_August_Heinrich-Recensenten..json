"line.2": {"text": "Ihr ahndet Mutterfreuden nicht,", "tokens": ["Ihr", "ahn\u00b7det", "Mut\u00b7ter\u00b7freu\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr habt mit Puppen nie gespielet", "tokens": ["Ihr", "habt", "mit", "Pup\u00b7pen", "nie", "ge\u00b7spie\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wisst nicht, was ein Kindlein spricht.", "tokens": ["Und", "wisst", "nicht", ",", "was", "ein", "Kin\u00b7dlein", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}