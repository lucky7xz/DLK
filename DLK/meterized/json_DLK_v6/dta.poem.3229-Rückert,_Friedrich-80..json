{"dta.poem.3229": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du ergreifen kannst des Augenblickes Stimmung,", "tokens": ["Wenn", "du", "er\u00b7grei\u00b7fen", "kannst", "des", "Au\u00b7gen\u00b7bli\u00b7ckes", "Stim\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Woge des Gef\u00fchls anhalten in der Schwimmung,", "tokens": ["Die", "Wo\u00b7ge", "des", "Ge\u00b7f\u00fchls", "an\u00b7hal\u00b7ten", "in", "der", "Schwim\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber alles was dich regt und \u00fcbermannt,", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7les", "was", "dich", "regt", "und", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zauberkr\u00e4ftige Wort aussprechen das es bannt;", "tokens": ["Das", "zau\u00b7ber\u00b7kr\u00e4f\u00b7ti\u00b7ge", "Wort", "aus\u00b7spre\u00b7chen", "das", "es", "bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann magst du frohbewu\u00dft den Augenblick entlassen,", "tokens": ["Dann", "magst", "du", "froh\u00b7be\u00b7wu\u00dft", "den", "Au\u00b7gen\u00b7blick", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begl\u00fcckt, in Schmerz und Lust dich selber zu erfassen.", "tokens": ["Be\u00b7gl\u00fcckt", ",", "in", "Schmerz", "und", "Lust", "dich", "sel\u00b7ber", "zu", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "KON", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}