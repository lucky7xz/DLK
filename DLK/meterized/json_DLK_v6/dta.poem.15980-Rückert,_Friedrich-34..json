{"dta.poem.15980": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Geistersonn' ein ausgesandter Stral,", "tokens": ["Ich", "bin", "der", "Geis\u00b7ter\u00b7sonn'", "ein", "aus\u00b7ge\u00b7sand\u00b7ter", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und solcher Stralen sind unz\u00e4hlbar eine Zahl.", "tokens": ["Und", "sol\u00b7cher", "Stra\u00b7len", "sind", "un\u00b7z\u00e4hl\u00b7bar", "ei\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Wir sind der Sonne Glanz zusammen alzumal,", "tokens": ["Wir", "sind", "der", "Son\u00b7ne", "Glanz", "zu\u00b7sam\u00b7men", "al\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist ein eigen Licht f\u00fcr sich ein jeder Stral.", "tokens": ["Doch", "ist", "ein", "ei\u00b7gen", "Licht", "f\u00fcr", "sich", "ein", "je\u00b7der", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PRF", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Wunder, Eine Sonn' ist Alles alzumal,", "tokens": ["O", "Wun\u00b7der", ",", "Ei\u00b7ne", "Sonn'", "ist", "Al\u00b7les", "al\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ganz die gro\u00dfe Sonn' in jedem kleinsten Stral.", "tokens": ["Und", "ganz", "die", "gro\u00b7\u00dfe", "Sonn'", "in", "je\u00b7dem", "kleins\u00b7ten", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}