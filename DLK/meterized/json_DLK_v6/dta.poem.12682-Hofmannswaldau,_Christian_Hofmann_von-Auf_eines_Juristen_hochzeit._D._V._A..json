{"dta.poem.12682": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines Juristen hochzeit.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der zweymal wittwer war, nimmt itzt mit gntem muth,", "tokens": ["Der", "zwey\u00b7mal", "witt\u00b7wer", "war", ",", "nimmt", "itzt", "mit", "gn\u00b7tem", "muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott lob! die dritte frau. Wie? h\u00e4lt der die bilantze,", "tokens": ["Gott", "lob", "!", "die", "drit\u00b7te", "frau", ".", "Wie", "?", "h\u00e4lt", "der", "die", "bi\u00b7lant\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "$.", "PWAV", "$.", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der zweymal geht zu grab und dreymal zu dem tantze?", "tokens": ["Der", "zwey\u00b7mal", "geht", "zu", "grab", "und", "drey\u00b7mal", "zu", "dem", "tant\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott, der dreyeinig ist, macht eins hier dreymal gut.", "tokens": ["Gott", ",", "der", "drey\u00b7ei\u00b7nig", "ist", ",", "macht", "eins", "hier", "drey\u00b7mal", "gut", "."], "token_info": ["word", "punct", "wor