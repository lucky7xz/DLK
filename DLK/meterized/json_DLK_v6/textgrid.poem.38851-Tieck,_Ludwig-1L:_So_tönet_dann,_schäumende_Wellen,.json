{"textgrid.poem.38851": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So t\u00f6net dann, sch\u00e4umende Wellen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So t\u00f6net dann, sch\u00e4umende Wellen,", "tokens": ["So", "t\u00f6\u00b7net", "dann", ",", "sch\u00e4u\u00b7men\u00b7de", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und windet euch rund um mich her!", "tokens": ["Und", "win\u00b7det", "euch", "rund", "um", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mag Ungl\u00fcck doch laut um mich bellen,", "tokens": ["Mag", "Un\u00b7gl\u00fcck", "doch", "laut", "um", "mich", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erbost seyn das grausame Meer!", "tokens": ["Er\u00b7bost", "seyn", "das", "grau\u00b7sa\u00b7me", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich lache den st\u00fcrmenden Wettern,", "tokens": ["Ich", "la\u00b7che", "den", "st\u00fcr\u00b7men\u00b7den", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verachte den Zorngrimm der Fluth;", "tokens": ["Ver\u00b7ach\u00b7te", "den", "Zorn\u00b7grimm", "der", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "O m\u00f6gen mich Felsen zerschmettern!", "tokens": ["O", "m\u00f6\u00b7gen", "mich", "Fel\u00b7sen", "zer\u00b7schmet\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Denn nimmer wird es gut.", "tokens": ["Denn", "nim\u00b7mer", "wird", "es", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht klag ich, und mag ich nun scheitern,", "tokens": ["Nicht", "klag", "ich", ",", "und", "mag", "ich", "nun", "schei\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In w\u00e4\u00dfrigen Tiefen vergehn!", "tokens": ["In", "w\u00e4\u00df\u00b7ri\u00b7gen", "Tie\u00b7fen", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Blick wird sich nie mehr erheitern,", "tokens": ["Mein", "Blick", "wird", "sich", "nie", "mehr", "er\u00b7hei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Stern meiner Liebe zu sehn.", "tokens": ["Den", "Stern", "mei\u00b7ner", "Lie\u00b7be", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "So w\u00e4lzt euch bergab mit Gewittern,", "tokens": ["So", "w\u00e4lzt", "euch", "berg\u00b7ab", "mit", "Ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und raset, ihr St\u00fcrme, mich an,", "tokens": ["Und", "ra\u00b7set", ",", "ihr", "St\u00fcr\u00b7me", ",", "mich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df Felsen an Felsen zersplittern!", "tokens": ["Da\u00df", "Fel\u00b7sen", "an", "Fel\u00b7sen", "zer\u00b7split\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich bin ein verlorener Mann.", "tokens": ["Ich", "bin", "ein", "ver\u00b7lo\u00b7re\u00b7ner", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}