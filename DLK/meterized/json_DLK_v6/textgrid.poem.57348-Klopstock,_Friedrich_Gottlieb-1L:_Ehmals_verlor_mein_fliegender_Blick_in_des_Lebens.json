{"textgrid.poem.57348": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ehmals verlor mein fliegender Blick in des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehmals verlor mein fliegender Blick in des Lebens", "tokens": ["Eh\u00b7mals", "ver\u00b7lor", "mein", "flie\u00b7gen\u00b7der", "Blick", "in", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "K\u00fcnftiges sich, und ich schuf dann, was mir Wunsch war,", "tokens": ["K\u00fcnf\u00b7ti\u00b7ges", "sich", ",", "und", "ich", "schuf", "dann", ",", "was", "mir", "Wunsch", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "$,", "KON", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Fast zu Wirklichkeit: seine Freuden", "tokens": ["Fast", "zu", "Wirk\u00b7lich\u00b7keit", ":", "sei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Hatte das sch\u00f6ne Phantom!", "tokens": ["Hat\u00b7te", "das", "sch\u00f6\u00b7ne", "Phan\u00b7tom", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Denn das Gesetz der M\u00e4ssigung wurd' ihm gegeben,", "tokens": ["Denn", "das", "Ge\u00b7setz", "der", "M\u00e4s\u00b7si\u00b7gung", "wurd'", "ihm", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wurde gethan mit der Strenge, die zu Hofnung", "tokens": ["Wur\u00b7de", "ge\u00b7than", "mit", "der", "Stren\u00b7ge", ",", "die", "zu", "Hof\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Leitet: aber der Wunsch ist dann selbst", "tokens": ["Lei\u00b7tet", ":", "a\u00b7ber", "der", "Wunsch", "ist", "dann", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Thor, wenn er Hofnung verdient.", "tokens": ["Thor", ",", "wenn", "er", "Hof\u00b7nung", "ver\u00b7dient", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Freue dich dess, das da ist! so sagt' ich mir \u00f6fter,", "tokens": ["Freu\u00b7e", "dich", "dess", ",", "das", "da", "ist", "!", "so", "sagt'", "ich", "mir", "\u00f6f\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als dem Get\u00e4usch ich es zuliess mir zu gleissen:", "tokens": ["Als", "dem", "Ge\u00b7t\u00e4usch", "ich", "es", "zu\u00b7liess", "mir", "zu", "gleis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sagt' es, thats! und erlebt' auch, was sich", "tokens": ["Sagt'", "es", ",", "thats", "!", "und", "er\u00b7lebt'", "auch", ",", "was", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$.", "KON", "VVFIN", "ADV", "$,", "PRELS", "PRF"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00dcber Gew\u00fcnschtes erhob.", "tokens": ["\u00dc\u00b7ber", "Ge\u00b7w\u00fcnschtes", "er\u00b7hob", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Jetzo verweilt der festere Blick in des Lebens", "tokens": ["Jet\u00b7zo", "ver\u00b7weilt", "der", "fes\u00b7te\u00b7re", "Blick", "in", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vorigem sich; und ich f\u00fchle, was dahinfloh,", "tokens": ["Vo\u00b7ri\u00b7gem", "sich", ";", "und", "ich", "f\u00fch\u00b7le", ",", "was", "da\u00b7hin\u00b7floh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "$.", "KON", "PPER", "VVFIN", "$,", "PRELS", "PAV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fast, als hielt' ich's noch: s\u00fcssre Freuden", "tokens": ["Fast", ",", "als", "hielt'", "ich's", "noch", ":", "s\u00fcss\u00b7re", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "PIS", "ADV", "$.", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Giebt es mir, war nicht Phantom!", "tokens": ["Giebt", "es", "mir", ",", "war", "nicht", "Phan\u00b7tom", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Freue dich dess, das da ist! so sag' ich mir dennoch", "tokens": ["Freu\u00b7e", "dich", "dess", ",", "das", "da", "ist", "!", "so", "sag'", "ich", "mir", "den\u00b7noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Jetzt auch. Obwohl sich der Scheitel mit des Alters", "tokens": ["Jetzt", "auch", ".", "Ob\u00b7wohl", "sich", "der", "Schei\u00b7tel", "mit", "des", "Al\u00b7ters"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "KOUS", "PRF", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bl\u00fctheuhaare mir deckt; ich wandle", "tokens": ["Bl\u00fc\u00b7theu\u00b7haa\u00b7re", "mir", "deckt", ";", "ich", "wand\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Froh um das n\u00e4here Grab.", "tokens": ["Froh", "um", "das", "n\u00e4\u00b7he\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Aber ich werd' auch Leiden gewahr im Vergangnen,", "tokens": ["A\u00b7ber", "ich", "werd'", "auch", "Lei\u00b7den", "ge\u00b7wahr", "im", "Ver\u00b7gang\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wehmuth! es geht mit den Leichen der Geliebten", "tokens": ["Weh\u00b7muth", "!", "es", "geht", "mit", "den", "Lei\u00b7chen", "der", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir vorbey: wie verm\u00f6cht' ich dann mich", "tokens": ["Mir", "vor\u00b7bey", ":", "wie", "ver\u00b7m\u00f6cht'", "ich", "dann", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PPER"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dessen, das da ist, zu freun!", "tokens": ["Des\u00b7sen", ",", "das", "da", "ist", ",", "zu", "freun", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}