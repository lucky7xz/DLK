{"textgrid.poem.63697": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Noch kaum ein Fischlein springen will.", "tokens": ["Noch", "kaum", "ein", "Fisc\u00b7hlein", "sprin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am B\u00e4nkchen schon, in Rohr und Ried,", "tokens": ["Am", "B\u00e4nk\u00b7chen", "schon", ",", "in", "Rohr", "und", "Ried", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein W\u00e4scherm\u00e4gdlein emsig kniet.", "tokens": ["Ein", "W\u00e4\u00b7scher\u00b7m\u00e4gd\u00b7lein", "em\u00b7sig", "kniet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Jugendblut, kaum funfzehn Jahr,", "tokens": ["O", "Ju\u00b7gend\u00b7blut", ",", "kaum", "funf\u00b7zehn", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlafen noch ihr Augenpaar,", "tokens": ["Ver\u00b7schla\u00b7fen", "noch", "ihr", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das R\u00f6ckchen d\u00fcrftig, hochgesch\u00fcrzt,", "tokens": ["Das", "R\u00f6ck\u00b7chen", "d\u00fcrf\u00b7tig", ",", "hoch\u00b7ge\u00b7sch\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Singen sie die Zeit sich k\u00fcrzt.", "tokens": ["Mit", "Sin\u00b7gen", "sie", "die", "Zeit", "sich", "k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbam j\u00fcngsten Tag ich aufersteh'", "tokens": ["\u00bb", "am", "j\u00fcng\u00b7sten", "Tag", "ich", "auf\u00b7er\u00b7steh'"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gleich nach meinem Liebsten seh',", "tokens": ["Und", "gleich", "nach", "mei\u00b7nem", "Liebs\u00b7ten", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich ihn nicht finden kann,", "tokens": ["Und", "wenn", "ich", "ihn", "nicht", "fin\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leg' wieder mich zum Schlafen dann.", "tokens": ["Leg'", "wie\u00b7der", "mich", "zum", "Schla\u00b7fen", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Herzeleid, du Ewigkeit!", "tokens": ["O", "Her\u00b7ze\u00b7leid", ",", "du", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbander nur ist Seligkeit.", "tokens": ["Sel\u00b7ban\u00b7der", "nur", "ist", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kommt mein Liebster nicht hinein,", "tokens": ["Und", "kommt", "mein", "Liebs\u00b7ter", "nicht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag nicht im Paradiese sein!\u00ab", "tokens": ["Mag", "nicht", "im", "Pa\u00b7ra\u00b7die\u00b7se", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}