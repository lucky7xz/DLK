{"textgrid.poem.58675": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heb mich auf, du Geisteswind, \u00fcber Welt, Natur und Sinnen", "tokens": ["Heb", "mich", "auf", ",", "du", "Geis\u00b7tes\u00b7wind", ",", "\u00fc\u00b7ber", "Welt", ",", "Na\u00b7tur", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bringe mich ins Innre ein, denn ich komm' sonst nimmer drinnen!", "tokens": ["Brin\u00b7ge", "mich", "ins", "Inn\u00b7re", "ein", ",", "denn", "ich", "komm'", "sonst", "nim\u00b7mer", "drin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Tief im Grund, da ist es sch\u00f6n,", "tokens": ["Tief", "im", "Grund", ",", "da", "ist", "es", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da kann man mit Augen sehn,", "tokens": ["Da", "kann", "man", "mit", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie des Herren Glorie-Glanz", "tokens": ["Wie", "des", "Her\u00b7ren", "Glo\u00b7rie\u00b7Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Haus erf\u00fcllet ganz.", "tokens": ["Die\u00b7ses", "Haus", "er\u00b7f\u00fcl\u00b7let", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Innig, au\u00dfer Ort und Zeit ist der Ort zu deinem Throne,", "tokens": ["In\u00b7nig", ",", "au\u00b7\u00dfer", "Ort", "und", "Zeit", "ist", "der", "Ort", "zu", "dei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Mach den Grund, mein K\u00f6nig, rein, komm und ewig drinnen wohne!", "tokens": ["Mach", "den", "Grund", ",", "mein", "K\u00f6\u00b7nig", ",", "rein", ",", "komm", "und", "e\u00b7wig", "drin\u00b7nen", "woh\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Geist und Seele, Herz und Sinn", "tokens": ["Geist", "und", "See\u00b7le", ",", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sinkt zu deinen F\u00fc\u00dfen hin;", "tokens": ["Sinkt", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Herrsch in mir, treib alles aus,", "tokens": ["Herrsch", "in", "mir", ",", "treib", "al\u00b7les", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fclle mit dir selbst dies Haus!", "tokens": ["F\u00fcl\u00b7le", "mit", "dir", "selbst", "dies", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}