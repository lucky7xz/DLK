{"dta.poem.9877": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Beantwortung eines empfangenen hoch-  \n zeit-brieffes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djr w\u00fcnsch ich mehr gel\u00fcck als Ceres garben zeiget/", "tokens": ["Djr", "w\u00fcnsch", "ich", "mehr", "ge\u00b7l\u00fcck", "als", "Ce\u00b7res", "gar\u00b7ben", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Bacchus trauben liest/ als Ph\u00f6bus tage macht.", "tokens": ["Als", "Bac\u00b7chus", "trau\u00b7ben", "liest", "/", "als", "Ph\u00f6\u00b7bus", "ta\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVINF", "VVFIN", "$(", "KOUS", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil mein schlechter geist sich h\u00f6her nicht versteiget/", "tokens": ["Und", "weil", "mein", "schlech\u00b7ter", "geist", "sich", "h\u00f6\u00b7her", "nicht", "ver\u00b7stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr als die Wiener post mir schreiben hat gebracht.", "tokens": ["Mehr", "als", "die", "Wie\u00b7ner", "post", "mir", "schrei\u00b7ben", "hat", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "PPER", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schwere/ da\u00df ich nichts von deiner hand erbrochen/", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "ich", "nichts", "von", "dei\u00b7ner", "hand", "er\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das nicht/ wie mich bedeucht/ nach rosen hat geschmeckt;", "tokens": ["Das", "nicht", "/", "wie", "mich", "be\u00b7deucht", "/", "nach", "ro\u00b7sen", "hat", "ge\u00b7schmeckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$(", "PWAV", "PPER", "VVFIN", "$(", "APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Besonders dieser brieff/ den du vor wenig wochen", "tokens": ["Be\u00b7son\u00b7ders", "die\u00b7ser", "brieff", "/", "den", "du", "vor", "we\u00b7nig", "wo\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$(", "ART", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir zugeschickt/ war gantz mit ambra angesteckt:", "tokens": ["Mir", "zu\u00b7ge\u00b7schickt", "/", "war", "gantz", "mit", "am\u00b7bra", "an\u00b7ge\u00b7steckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$(", "VAFIN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn unter andern fand ich dieses auch geschrieben/", "tokens": ["Denn", "un\u00b7ter", "an\u00b7dern", "fand", "ich", "die\u00b7ses", "auch", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPER", "PDS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das ich/ wie billig war/ mit freuden angeschaut.", "tokens": ["Das", "ich", "/", "wie", "bil\u00b7lig", "war", "/", "mit", "freu\u00b7den", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$(", "KOKOM", "ADJD", "VAFIN", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es war ein kurtzer satz: nach g\u00f6ttlichem belieben/", "tokens": ["Es", "war", "ein", "kurt\u00b7zer", "satz", ":", "nach", "g\u00f6tt\u00b7li\u00b7chem", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist mein geliebtes kind und tochter eine braut.", "tokens": ["Ist", "mein", "ge\u00b7lieb\u00b7tes", "kind", "und", "toch\u00b7ter", "ei\u00b7ne", "braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "KON", "NN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein bisam-wort f\u00fcr mich! ietzt kenn ich meine pflichten/", "tokens": ["Ein", "bi\u00b7sam\u00b7wort", "f\u00fcr", "mich", "!", "ietzt", "kenn", "ich", "mei\u00b7ne", "pflich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und was ich/ hoher freund/ dir mehr als schuldig bin.", "tokens": ["Und", "was", "ich", "/", "ho\u00b7her", "freund", "/", "dir", "mehr", "als", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "$(", "ADJA", "NN", "$(", "PPER", "PIAT", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du forderst dieses auch/ ich soll ein braut-lied tichten/", "tokens": ["Du", "for\u00b7derst", "die\u00b7ses", "auch", "/", "ich", "soll", "ein", "braut\u00b7lied", "tich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "$(", "PPER", "VMFIN", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "F\u00fcrwar die beste krafft der poesie ist hin.", "tokens": ["F\u00fcr\u00b7war", "die", "bes\u00b7te", "krafft", "der", "po\u00b7e\u00b7sie", "ist", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der stern/ so mir zuvor die geister hat erwecket/", "tokens": ["Der", "stern", "/", "so", "mir", "zu\u00b7vor", "die", "geis\u00b7ter", "hat", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$(", "ADV", "PPER", "ADV", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Scheint ietzund weit von mir/ mein feuer geht mir aus/", "tokens": ["Scheint", "ie\u00b7tzund", "weit", "von", "mir", "/", "mein", "feu\u00b7er", "geht", "mir", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es hat die alte glut ein kalter schnee bedecket/", "tokens": ["Es", "hat", "die", "al\u00b7te", "glut", "ein", "kal\u00b7ter", "schnee", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein gantz Parnassus wird zu staub und ziegel-grau\u00df.", "tokens": ["Mein", "gantz", "Par\u00b7nas\u00b7sus", "wird", "zu", "staub", "und", "zie\u00b7gel\u00b7grau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NE", "VAFIN", "PTKA", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus armuth mu\u00df ich nur bey diesem brieffe bleiben/", "tokens": ["Aus", "ar\u00b7muth", "mu\u00df", "ich", "nur", "bey", "die\u00b7sem", "brief\u00b7fe", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der schlechter art und auch so niedrig ist wie ich.", "tokens": ["Der", "schlech\u00b7ter", "art", "und", "auch", "so", "nied\u00b7rig", "ist", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "ADV", "ADJD", "VAFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich traue mir nicht mehr ein geistig lied zu schreiben/", "tokens": ["Ich", "trau\u00b7e", "mir", "nicht", "mehr", "ein", "geis\u00b7tig", "lied", "zu", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den sternen zuzugehn ist nun nicht mehr f\u00fcr mich.", "tokens": ["Den", "ster\u00b7nen", "zu\u00b7zu\u00b7gehn", "ist", "nun", "nicht", "mehr", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVIZU", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die edle Donau hegt ietzt tausend haupt-poeten/", "tokens": ["Die", "ed\u00b7le", "Do\u00b7nau", "hegt", "ietzt", "tau\u00b7send", "haup\u00b7tpo\u00b7e\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mein geringer kram scheut dieser sonnen licht.", "tokens": ["Und", "mein", "ge\u00b7rin\u00b7ger", "kram", "scheut", "die\u00b7ser", "son\u00b7nen", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die feld-trompete lacht der engen kinder fl\u00f6then/", "tokens": ["Die", "feld\u00b7trom\u00b7pe\u00b7te", "lacht", "der", "en\u00b7gen", "kin\u00b7der", "fl\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ascherfarbe schickt sich zu dem purpur nicht.", "tokens": ["Und", "asc\u00b7her\u00b7far\u00b7be", "schickt", "sich", "zu", "dem", "pur\u00b7pur", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "APPR", "ART", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch wahre freundschafft wiegt alleine das gem\u00fcthe/", "tokens": ["Doch", "wah\u00b7re", "freund\u00b7schafft", "wiegt", "al\u00b7lei\u00b7ne", "das", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Schaut gute meinung mehr als gute sylben an.", "tokens": ["Schaut", "gu\u00b7te", "mei\u00b7nung", "mehr", "als", "gu\u00b7te", "syl\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PIAT", "KOKOM", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich kenne/ werther freund/ zu deutlich deine g\u00fcte/", "tokens": ["Ich", "ken\u00b7ne", "/", "wert\u00b7her", "freund", "/", "zu", "deut\u00b7lich", "dei\u00b7ne", "g\u00fc\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "PTKA", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die nur durch redlichkeit vergn\u00fcget werden kan.", "tokens": ["Die", "nur", "durch", "red\u00b7lich\u00b7keit", "ver\u00b7gn\u00fc\u00b7get", "wer\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "In dieser zuversicht entsch\u00fctt ich geist und hertze/", "tokens": ["In", "die\u00b7ser", "zu\u00b7ver\u00b7sicht", "ent\u00b7sch\u00fctt", "ich", "geist", "und", "hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und lasse meine lust mit vollen str\u00f6men aus.", "tokens": ["Und", "las\u00b7se", "mei\u00b7ne", "lust", "mit", "vol\u00b7len", "str\u00f6\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich sehe/ wie nunmehr des himmels freuden-kertze", "tokens": ["Ich", "se\u00b7he", "/", "wie", "nun\u00b7mehr", "des", "him\u00b7mels", "freu\u00b7den\u00b7kert\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mit strahlen seiner gunst bestrahlt dein liebes hau\u00df.", "tokens": ["Mit", "strah\u00b7len", "sei\u00b7ner", "gunst", "be\u00b7strahlt", "dein", "lie\u00b7bes", "hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was kan dir lieber seyn/ als diesen tag zu schauen/", "tokens": ["Was", "kan", "dir", "lie\u00b7ber", "seyn", "/", "als", "die\u00b7sen", "tag", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VAINF", "$(", "KOUS", "PDAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da du mit vaters hand der eh\u2019 geliebte frucht", "tokens": ["Da", "du", "mit", "va\u00b7ters", "hand", "der", "eh'", "ge\u00b7lieb\u00b7te", "frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Desselben redlichkeit wirst sollen anvertrauen/", "tokens": ["Des\u00b7sel\u00b7ben", "red\u00b7lich\u00b7keit", "wirst", "sol\u00b7len", "an\u00b7ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der sie durch reinen trieb der tugend hat gesucht?", "tokens": ["Der", "sie", "durch", "rei\u00b7nen", "trieb", "der", "tu\u00b7gend", "hat", "ge\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "VVFIN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da du der tochter hand in dessen ha