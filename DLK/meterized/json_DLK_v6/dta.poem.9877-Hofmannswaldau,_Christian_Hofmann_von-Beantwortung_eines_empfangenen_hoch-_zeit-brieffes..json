 "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "ADV", "ADV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und l\u00e4st die frucht allein die last der \u00e4ste seyn?", "tokens": ["Und", "l\u00e4st", "die", "frucht", "al\u00b7lein", "die", "last", "der", "\u00e4s\u00b7te", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "VVFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer klug ist/ wird mit witz wohl zu er\u00f6ffnen wissen/", "tokens": ["Wer", "klug", "ist", "/", "wird", "mit", "witz", "wohl", "zu", "er\u00b7\u00f6ff\u00b7nen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$(", "VAFIN", "APPR", "VVIMP", "ADV", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Er stellt die s\u00fcsse frucht getreuen h\u00e4nden ein.", "tokens": ["Er", "stellt", "die", "s\u00fcs\u00b7se", "frucht", "ge\u00b7treu\u00b7en", "h\u00e4n\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wer seine waare stets im krame will verwahren/", "tokens": ["Wer", "sei\u00b7ne", "waa\u00b7re", "stets", "im", "kra\u00b7me", "will", "ver\u00b7wah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "ADV", "APPRART", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und nie