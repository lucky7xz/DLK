{"textgrid.poem.43254": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie fragen mich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie fragen mich nicht", "tokens": ["Sie", "fra\u00b7gen", "mich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die mitleidigen Sterne,", "tokens": ["Die", "mit\u00b7lei\u00b7di\u00b7gen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Was mich bewegt ...", "tokens": ["Was", "mich", "be\u00b7wegt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Aus endloser Ferne", "tokens": ["Aus", "end\u00b7lo\u00b7ser", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Leuchtet ihr Licht", "tokens": ["Leuch\u00b7tet", "ihr", "Licht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mir in die Brust ...", "tokens": ["Mir", "in", "die", "Brust", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$("], "meter": "++-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie fragen mich nicht", "tokens": ["Sie", "fra\u00b7gen", "mich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die geduldigen Sterne,", "tokens": ["Die", "ge\u00b7dul\u00b7di\u00b7gen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie lauschen lautlos,", "tokens": ["Sie", "lau\u00b7schen", "laut\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Als h\u00f6rten sie gerne", "tokens": ["Als", "h\u00f6r\u00b7ten", "sie", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was Sehnsucht spricht.", "tokens": ["Was", "Sehn\u00b7sucht", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auch sie gehorchen", "tokens": ["Auch", "sie", "ge\u00b7hor\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Willenlos schweigsam", "tokens": ["Wil\u00b7len\u00b7los", "schweig\u00b7sam"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der lenkenden Kraft", "tokens": ["Der", "len\u00b7ken\u00b7den", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die herrschet unbeugsam.", "tokens": ["Die", "herr\u00b7schet", "un\u00b7beug\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Was mich bewegt", "tokens": ["Was", "mich", "be\u00b7wegt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"tex