{"textgrid.poem.45742": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[germania, das gute Weib]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Germania, das gute Weib,", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "das", "gu\u00b7te", "Weib", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schwoll an in ihrem Unterleib.", "tokens": ["Schwoll", "an", "in", "ih\u00b7rem", "Un\u00b7ter\u00b7leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sagten die Weisen um sie her,", "tokens": ["Da", "sag\u00b7ten", "die", "Wei\u00b7sen", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df sie eben schwanger w\u00e4r", "tokens": ["Da\u00df", "sie", "e\u00b7ben", "schwan\u00b7ger", "w\u00e4r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und w\u00fcrde ein Wunderkind geb\u00e4ren,", "tokens": ["Und", "w\u00fcr\u00b7de", "ein", "Wun\u00b7der\u00b7kind", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das sie br\u00e4chte zu h\u00f6chsten Ehren.", "tokens": ["Das", "sie", "br\u00e4ch\u00b7te", "zu", "h\u00f6chs\u00b7ten", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Doch kam die Stunde der Geburt,", "tokens": ["Doch", "kam", "die", "Stun\u00b7de", "der", "Ge\u00b7burt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da gingen eben Winde furt.", "tokens": ["Da", "gin\u00b7gen", "e\u00b7ben", "Win\u00b7de", "furt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht schwanger war sie, nur geschwollen,", "tokens": ["Nicht", "schwan\u00b7ger", "war", "sie", ",", "nur", "ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Teufel soll die Windmacher holen!", "tokens": ["Der", "Teu\u00b7fel", "soll", "die", "Wind\u00b7ma\u00b7cher", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}