{"textgrid.poem.65809": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "83", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie sah ich Farbe dich bed\u00fcrfen; nie", "tokens": ["Nie", "sah", "ich", "Far\u00b7be", "dich", "be\u00b7d\u00fcr\u00b7fen", ";", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PPER", "VVINF", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4rb' ich dein Sch\u00f6nes drum mit Malertinten.", "tokens": ["F\u00e4rb'", "ich", "dein", "Sch\u00f6\u00b7nes", "drum", "mit", "Ma\u00b7ler\u00b7tin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr \u00f6des Flitterlob der Poesie", "tokens": ["F\u00fcr", "\u00f6\u00b7des", "Flit\u00b7ter\u00b7lob", "der", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fand \u2013 oder glaubt' ich dich zu gro\u00df zu finden.", "tokens": ["Fand", "\u2013", "o\u00b7der", "glaubt'", "ich", "dich", "zu", "gro\u00df", "zu", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "VVFIN", "PPER", "PRF", "PTKA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und darum schildr' ich dich beredsam nicht,", "tokens": ["Und", "da\u00b7rum", "schildr'", "ich", "dich", "be\u00b7red\u00b7sam", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Damit an dir, dem Lebenden, sich zeigt.", "tokens": ["Da\u00b7mit", "an", "dir", ",", "dem", "Le\u00b7ben\u00b7den", ",", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "$,", "ART", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie weit ein heut'ger Dichter unterliegt,", "tokens": ["Wie", "weit", "ein", "heut'\u00b7ger", "Dich\u00b7ter", "un\u00b7ter\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn er Verdienst malt, das dem deinen gleicht.", "tokens": ["Wenn", "er", "Ver\u00b7dienst", "malt", ",", "das", "dem", "dei\u00b7nen", "gleicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "PRELS", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dies Schweigen machtest du zur S\u00fcnde mir,", "tokens": ["Dies", "Schwei\u00b7gen", "mach\u00b7test", "du", "zur", "S\u00fcn\u00b7de", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und doch mein Stolz ist's eben; denn verkleinert", "tokens": ["Und", "doch", "mein", "Stolz", "ist's", "e\u00b7ben", ";", "denn", "ver\u00b7klei\u00b7nert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ADV", "$.", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wird vom Verstummenden kein Reiz an dir,", "tokens": ["Wird", "vom", "Ver\u00b7stum\u00b7men\u00b7den", "kein", "Reiz", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn mancher, der beleben will, versteinert.", "tokens": ["Wenn", "man\u00b7cher", ",", "der", "be\u00b7le\u00b7ben", "will", ",", "ver\u00b7stei\u00b7nert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In einem deiner sch\u00f6nen Augen brennt", "tokens": ["In", "ei\u00b7nem", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Au\u00b7gen", "brennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mehr Leben als dein Dichterpaar erf\u00e4nd.", "tokens": ["Mehr", "Le\u00b7ben", "als", "dein", "Dich\u00b7ter\u00b7paar", "er\u00b7f\u00e4nd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}