{"textgrid.poem.58783": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "2. Weinen und Lieben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus h\u00f6ret all dein Sehnen,", "tokens": ["Je\u00b7sus", "h\u00f6\u00b7ret", "all", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesus schauet deine Tr\u00e4nen;", "tokens": ["Je\u00b7sus", "schau\u00b7et", "dei\u00b7ne", "Tr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weine frei, doch liebe mit,", "tokens": ["Wei\u00b7ne", "frei", ",", "doch", "lie\u00b7be", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So erh\u00f6rt er deine Bitt'!", "tokens": ["So", "er\u00b7h\u00f6rt", "er", "dei\u00b7ne", "Bitt'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}