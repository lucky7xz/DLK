{"textgrid.poem.64827": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ein R\u00e4thsel dir mein Schmerz,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ein R\u00e4thsel dir mein Schmerz,", "tokens": ["Ob", "ein", "R\u00e4th\u00b7sel", "dir", "mein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den du nicht vermagst zu heilen,", "tokens": ["Den", "du", "nicht", "ver\u00b7magst", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du ihn doch mit mir theilen,", "tokens": ["Willst", "du", "ihn", "doch", "mit", "mir", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du so vielgetreues Herz!", "tokens": ["Du", "so", "viel\u00b7ge\u00b7treu\u00b7es", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht gefragt hat mich dein Mund,", "tokens": ["Nicht", "ge\u00b7fragt", "hat", "mich", "dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Antwort hab' ich nicht gegeben;", "tokens": ["Ant\u00b7wort", "hab'", "ich", "nicht", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch aus meiner Lippen Leben", "tokens": ["Doch", "aus", "mei\u00b7ner", "Lip\u00b7pen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward dir, da\u00df ich leide, kund.", "tokens": ["Ward", "dir", ",", "da\u00df", "ich", "lei\u00b7de", ",", "kund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und das d\u00fcnkte dir genug!", "tokens": ["Und", "das", "d\u00fcnk\u00b7te", "dir", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie hat mich ein Weh getroffen,", "tokens": ["Nie", "hat", "mich", "ein", "Weh", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Das dir nicht zerst\u00f6rt ein Hoffen,", "tokens": ["Das", "dir", "nicht", "zer\u00b7st\u00f6rt", "ein", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir nicht eine Wunde schlug.", "tokens": ["Dir", "nicht", "ei\u00b7ne", "Wun\u00b7de", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du so mild, die Welt so arg!", "tokens": ["Du", "so", "mild", ",", "die", "Welt", "so", "arg", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich in die Zukunft sp\u00e4hen,", "tokens": ["Will", "ich", "in", "die", "Zu\u00b7kunft", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich dich allein nur stehen", "tokens": ["Seh", "ich", "dich", "al\u00b7lein", "nur", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen mir und meinem Sarg.", "tokens": ["Zwi\u00b7schen", "mir", "und", "mei\u00b7nem", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun die Hoffnung mir entglitt,", "tokens": ["Nun", "die", "Hoff\u00b7nung", "mir", "ent\u00b7glitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele gramesbitter,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "gra\u00b7mes\u00b7bit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahst du wie ein Johanniter,", "tokens": ["Nahst", "du", "wie", "ein", "Jo\u00b7han\u00b7ni\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der an's Krankenlager tritt!", "tokens": ["Der", "an's", "Kran\u00b7ken\u00b7la\u00b7ger", "tritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der, von h\u00f6h'rem Licht umtagt,", "tokens": ["Der", ",", "von", "h\u00f6h'\u00b7rem", "Licht", "um\u00b7tagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ein wundes Herz zu pflegen,", "tokens": ["Um", "ein", "wun\u00b7des", "Herz", "zu", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allem ird'schen Freudesegen", "tokens": ["Al\u00b7lem", "ird'\u00b7schen", "Freu\u00b7de\u00b7se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernst und milde hat entsagt.", "tokens": ["Ernst", "und", "mil\u00b7de", "hat", "ent\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist der Himmel nicht von Erz,", "tokens": ["Ist", "der", "Him\u00b7mel", "nicht", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O dann mu\u00df der Herr der Welten", "tokens": ["O", "dann", "mu\u00df", "der", "Herr", "der", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Gro\u00dfmuth dir vergelten,", "tokens": ["Dei\u00b7ne", "Gro\u00df\u00b7muth", "dir", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Du so vielgetreues Herz!", "tokens": ["Du", "so", "viel\u00b7ge\u00b7treu\u00b7es", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}