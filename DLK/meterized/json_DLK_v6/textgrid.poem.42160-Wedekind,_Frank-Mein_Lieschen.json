{"textgrid.poem.42160": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Mein Lieschen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieschen tr\u00e4gt keine Hosen", "tokens": ["Mein", "Lie\u00b7schen", "tr\u00e4gt", "kei\u00b7ne", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon seit dem ersten April,", "tokens": ["Schon", "seit", "dem", "ers\u00b7ten", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weil sie von der grenzenlosen", "tokens": ["Weil", "sie", "von", "der", "gren\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hitze nicht leiden will.", "tokens": ["Hit\u00b7ze", "nicht", "lei\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das gibt mir manches zu denken,", "tokens": ["Das", "gibt", "mir", "man\u00b7ches", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So dacht ich auch schon daran,", "tokens": ["So", "dacht", "ich", "auch", "schon", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr ein Paar Hosen zu schenken", "tokens": ["Ihr", "ein", "Paar", "Ho\u00b7sen", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus duftigstem Tarlatan.", "tokens": ["Aus", "duf\u00b7tigs\u00b7tem", "Tar\u00b7la\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie leicht kann sie sich beim Hupfen", "tokens": ["Wie", "leicht", "kann", "sie", "sich", "beim", "Hup\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erk\u00e4lten, eh sie's gedacht;", "tokens": ["Er\u00b7k\u00e4l\u00b7ten", ",", "eh", "sie's", "ge\u00b7dacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und bleibt ihr auch nichts als ein Schnupfen,", "tokens": ["Und", "bleibt", "ihr", "auch", "nichts", "als", "ein", "Schnup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man nimmt sich doch lieber in acht.", "tokens": ["Man", "nimmt", "sich", "doch", "lie\u00b7ber", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "APPR", "CARD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}