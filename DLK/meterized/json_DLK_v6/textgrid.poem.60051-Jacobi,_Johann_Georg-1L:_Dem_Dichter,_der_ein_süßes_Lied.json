{"textgrid.poem.60051": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Dichter, der ein s\u00fc\u00dfes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Dichter, der ein s\u00fc\u00dfes Lied", "tokens": ["Dem", "Dich\u00b7ter", ",", "der", "ein", "s\u00fc\u00b7\u00dfes", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll hoher Weisheit mir gesungen:", "tokens": ["Voll", "ho\u00b7her", "Weis\u00b7heit", "mir", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwie sch\u00f6n das zarte Veilchen bl\u00fcht;", "tokens": ["\u00bb", "wie", "sch\u00f6n", "das", "zar\u00b7te", "Veil\u00b7chen", "bl\u00fcht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bald es welkt, und wie, verschlungen", "tokens": ["Wie", "bald", "es", "welkt", ",", "und", "wie", ",", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "KON", "PWAV", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Boden, welcher es gezeugt,", "tokens": ["Vom", "Bo\u00b7den", ",", "wel\u00b7cher", "es", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es keine Sonne gr\u00fc\u00dft, sich keinem Zephyr neigt;", "tokens": ["Es", "kei\u00b7ne", "Son\u00b7ne", "gr\u00fc\u00dft", ",", "sich", "kei\u00b7nem", "Ze\u00b7phyr", "neigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie dann zu lichtern D\u00e4mmerungen", "tokens": ["Wie", "dann", "zu", "lich\u00b7tern", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist der Blume durchgedrungen,", "tokens": ["Der", "Geist", "der", "Blu\u00b7me", "durch\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Im Sch\u00f6pfers-Angesichte schwebt,", "tokens": ["Im", "Sch\u00f6p\u00b7fer\u00b7sAn\u00b7ge\u00b7sich\u00b7te", "schwebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich h\u00f6her, immer h\u00f6her hebt,", "tokens": ["Sich", "h\u00f6\u00b7her", ",", "im\u00b7mer", "h\u00f6\u00b7her", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und zwischen Engeln einst im Paradiese lebt\u00ab;", "tokens": ["Und", "zwi\u00b7schen", "En\u00b7geln", "einst", "im", "Pa\u00b7ra\u00b7die\u00b7se", "lebt", "\u00ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "APPRART", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Dichter, dessen holde Klage,", "tokens": ["Dem", "Dich\u00b7ter", ",", "des\u00b7sen", "hol\u00b7de", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Hoffnung gro\u00dfer Seligkeit,", "tokens": ["Durch", "Hoff\u00b7nung", "gro\u00b7\u00dfer", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich so zum bangen Sterbetage", "tokens": ["Mich", "so", "zum", "ban\u00b7gen", "Ster\u00b7be\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der besten Mutter eingeweiht;", "tokens": ["Der", "bes\u00b7ten", "Mut\u00b7ter", "ein\u00b7ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem will ich, voll von z\u00e4rtlichem Entz\u00fccken,", "tokens": ["Dem", "will", "ich", ",", "voll", "von", "z\u00e4rt\u00b7li\u00b7chem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Erstlinge des Fr\u00fchlings pfl\u00fccken,", "tokens": ["Die", "Erst\u00b7lin\u00b7ge", "des", "Fr\u00fch\u00b7lings", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und singend ihm sein Fest mit jungen Veilchen schm\u00fccken.", "tokens": ["Und", "sin\u00b7gend", "ihm", "sein", "Fest", "mit", "jun\u00b7gen", "Veil\u00b7chen", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}