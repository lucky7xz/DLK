{"textgrid.poem.55613": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pfau schreit h\u00e4\u00dflich, aber sein Geschrei", "tokens": ["Der", "Pfau", "schreit", "h\u00e4\u00df\u00b7lich", ",", "a\u00b7ber", "sein", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erinnert mich ans himmlische Gefieder;", "tokens": ["E\u00b7rin\u00b7nert", "mich", "ans", "himm\u00b7li\u00b7sche", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ist mir auch sein Schreien nicht zuwider.", "tokens": ["So", "ist", "mir", "auch", "sein", "Schrei\u00b7en", "nicht", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit indischen G\u00e4nsen ist's nicht gleicherlei,", "tokens": ["Mit", "in\u00b7di\u00b7schen", "G\u00e4n\u00b7sen", "ist's", "nicht", "glei\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Sie zu erdulden, ist unm\u00f6glich:", "tokens": ["Sie", "zu", "er\u00b7dul\u00b7den", ",", "ist", "un\u00b7m\u00f6g\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die h\u00e4\u00dflichen, sie schreien unertr\u00e4glich.", "tokens": ["Die", "h\u00e4\u00df\u00b7li\u00b7chen", ",", "sie", "schrei\u00b7en", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}