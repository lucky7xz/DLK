{"textgrid.poem.25513": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nnt' ich mich zum Raben machen:", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich mich zum Raben machen:", "tokens": ["K\u00f6nnt'", "ich", "mich", "zum", "Ra\u00b7ben", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Fl\u00fcsse, Berg' und Thal", "tokens": ["Ue\u00b7ber", "Fl\u00fcs\u00b7se", ",", "Ber\u00b7g'", "und", "Thal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fl\u00f6g' ich t\u00e4glich zwanzigmal,", "tokens": ["Fl\u00f6g'", "ich", "t\u00e4g\u00b7lich", "zwan\u00b7zig\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief' an deinem Fenster leise:", "tokens": ["Rie\u00b7f'", "an", "dei\u00b7nem", "Fens\u00b7ter", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin da! mein Amarant!", "tokens": ["Ich", "bin", "da", "!", "mein", "A\u00b7ma\u00b7rant", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und von meiner schnellen Reise", "tokens": ["Und", "von", "mei\u00b7ner", "schnel\u00b7len", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruht' ich aus in deiner Hand.", "tokens": ["Ruht'", "ich", "aus", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00f6nnt' ich mich zum Rehe machen:", "tokens": ["K\u00f6nnt'", "ich", "mich", "zum", "Re\u00b7he", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch die Saaten, durch den Wald", "tokens": ["Durch", "die", "Saa\u00b7ten", ",", "durch", "den", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Lief' ich t\u00e4glich, ach! wie bald!", "tokens": ["Lie\u00b7f'", "ich", "t\u00e4g\u00b7lich", ",", "ach", "!", "wie", "bald", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ITJ", "$.", "PWAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ueber deine Gartenhecken", "tokens": ["Ue\u00b7ber", "dei\u00b7ne", "Gar\u00b7ten\u00b7he\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Spr\u00e4ng' ich, hops! mit einem Sprung',", "tokens": ["Spr\u00e4ng'", "ich", ",", "hops", "!", "mit", "ei\u00b7nem", "Sprung'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ITJ", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und wie wollt' ich dann dich necken", "tokens": ["Und", "wie", "wollt'", "ich", "dann", "dich", "ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-++--+--", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Unter der Verwandelung!", "tokens": ["Un\u00b7ter", "der", "Ver\u00b7wan\u00b7de\u00b7lung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "K\u00f6nnt' ich mich zum Karpfen machen:", "tokens": ["K\u00f6nnt'", "ich", "mich", "zum", "Karp\u00b7fen", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mit der ", "tokens": ["Mit", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "T\u00e4glich hin zu dir, o Mann!", "tokens": ["T\u00e4g\u00b7lich", "hin", "zu", "dir", ",", "o", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Aus dem Wasser, spr\u00e4ng' am Ende", "tokens": ["Aus", "dem", "Was\u00b7ser", ",", "spr\u00e4ng'", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Meiner Fahrt ich hoch herauf,", "tokens": ["Mei\u00b7ner", "Fahrt", "ich", "hoch", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Und mich fischten deine H\u00e4nde", "tokens": ["Und", "mich", "fischten", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "An dem Ufer gl\u00fccklich auf.", "tokens": ["An", "dem", "U\u00b7fer", "gl\u00fcck\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Aber, w\u00fcnsch' ich armes M\u00e4dchen", "tokens": ["A\u00b7ber", ",", "w\u00fcn\u00b7sch'", "ich", "ar\u00b7mes", "M\u00e4d\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Noch so viel mich hin zu dir:", "tokens": ["Noch", "so", "viel", "mich", "hin", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Dennoch bleib' ich immer hier.", "tokens": ["Den\u00b7noch", "bleib'", "ich", "im\u00b7mer", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Nicht drei Schritte kann ich gehen,", "tokens": ["Nicht", "drei", "Schrit\u00b7te", "kann", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "CARD", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Da\u00df nicht jeder fragt: Wohin?", "tokens": ["Da\u00df", "nicht", "je\u00b7der", "fragt", ":", "Wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIS", "VVFIN", "$.", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Wohl nur, da\u00df man nicht kann sehen,", "tokens": ["Wohl", "nur", ",", "da\u00df", "man", "nicht", "kann", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wo ich mit dem Geiste bin.", "tokens": ["Wo", "ich", "mit", "dem", "Geis\u00b7te", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}