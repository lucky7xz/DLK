{"textgrid.poem.25489": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit vielen Jahren kannt' ich dich.", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit vielen Jahren kannt' ich dich.", "tokens": ["Seit", "vie\u00b7len", "Jah\u00b7ren", "kannt'", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie hatt' ich dein Gesicht gesehen:", "tokens": ["Nie", "hatt'", "ich", "dein", "Ge\u00b7sicht", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur deines Dichterruhms Troph\u00e4en", "tokens": ["Nur", "dei\u00b7nes", "Dich\u00b7ter\u00b7ruhms", "Trop\u00b7h\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erg\u00f6tzten und belehrten mich.", "tokens": ["Er\u00b7g\u00f6tz\u00b7ten", "und", "be\u00b7lehr\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kannte dich, als h\u00e4tt' ich lange", "tokens": ["Ich", "kann\u00b7te", "dich", ",", "als", "h\u00e4tt'", "ich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens Ball mit dir gespielt;", "tokens": ["Des", "Le\u00b7bens", "Ball", "mit", "dir", "ge\u00b7spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als h\u00e4tt' ich oft die Jugendwange", "tokens": ["Als", "h\u00e4tt'", "ich", "oft", "die", "Ju\u00b7gend\u00b7wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dir erhitzt, an dir gek\u00fchlt.", "tokens": ["Mit", "dir", "er\u00b7hitzt", ",", "an", "dir", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In manchem trefflichen Gesange", "tokens": ["In", "man\u00b7chem", "treff\u00b7li\u00b7chen", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gabst du dich einst geschildert hin.", "tokens": ["Gabst", "du", "dich", "einst", "ge\u00b7schil\u00b7dert", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Entsagen lehrtest du dem Range;", "tokens": ["Ent\u00b7sa\u00b7gen", "lehr\u00b7test", "du", "dem", "Ran\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Musen waren dein Gewinn.", "tokens": ["Die", "Mu\u00b7sen", "wa\u00b7ren", "dein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das pflanzte sich in meinen Sinn.", "tokens": ["Das", "pflanz\u00b7te", "sich", "in", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Entsagung ward auf meinem Gange", "tokens": ["Ent\u00b7sa\u00b7gung", "ward", "auf", "mei\u00b7nem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Durchs Leben die Begleiterin.", "tokens": ["Durchs", "Le\u00b7ben", "die", "Be\u00b7glei\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Es will gelernt seyn, das Entbehren,", "tokens": ["Es", "will", "ge\u00b7lernt", "seyn", ",", "das", "Ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und nicht zu sp\u00e4t. Was im Gehirn'", "tokens": ["Und", "nicht", "zu", "sp\u00e4t", ".", "Was", "im", "Ge\u00b7hirn'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PTKA", "ADJD", "$.", "PWS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des J\u00fcnglings f\u00fcr Entw\u00fcrfe g\u00e4hren,", "tokens": ["Des", "J\u00fcng\u00b7lings", "f\u00fcr", "Ent\u00b7w\u00fcr\u00b7fe", "g\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die einst sein Gl\u00fcck als Mann verzehren,", "tokens": ["Die", "einst", "sein", "Gl\u00fcck", "als", "Mann", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zeigt oft die fr\u00fch gefurchte Stirn.", "tokens": ["Zeigt", "oft", "die", "fr\u00fch", "ge\u00b7furch\u00b7te", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es will ge\u00fcbt seyn, das Entbehren!", "tokens": ["Es", "will", "ge\u00b7\u00fcbt", "seyn", ",", "das", "Ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Den reinen Himmel mir gew\u00e4hren,", "tokens": ["Den", "rei\u00b7nen", "Him\u00b7mel", "mir", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wozu die Unabh\u00e4ngigkeit", "tokens": ["Wo\u00b7zu", "die", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mir ihren freien Pinsel leiht,", "tokens": ["Mir", "ih\u00b7ren", "frei\u00b7en", "Pin\u00b7sel", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Das kann kein Staat mit seinen Ehren.", "tokens": ["Das", "kann", "kein", "Staat", "mit", "sei\u00b7nen", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Was sind sie anders, als ein Streit", "tokens": ["Was", "sind", "sie", "an\u00b7ders", ",", "als", "ein", "Streit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der W\u00fcnsche mit der Wirklichkeit?", "tokens": ["Der", "W\u00fcn\u00b7sche", "mit", "der", "Wirk\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der steifen Praxis mit Chim\u00e4ren?", "tokens": ["Der", "stei\u00b7fen", "Pra\u00b7xis", "mit", "Chi\u00b7m\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Staat gibt dir sein S\u00f6ldnerkleid.", "tokens": ["Der", "Staat", "gibt", "dir", "sein", "S\u00f6ld\u00b7ner\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ward es f\u00fcr dich denn zugeschnitten?", "tokens": ["Ward", "es", "f\u00fcr", "dich", "denn", "zu\u00b7ge\u00b7schnit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Gehst du darin mit sichern Tritten?", "tokens": ["Gehst", "du", "da\u00b7rin", "mit", "si\u00b7chern", "Trit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.32": {"text": "Ist's nicht zu enge, nicht zu weit?", "tokens": ["Ist's", "nicht", "zu", "en\u00b7ge", ",", "nicht", "zu", "weit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ADJA", "$,", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Staat, (ja g\u00e4b' es be\u00dfre Staaten!)", "tokens": ["Der", "Staat", ",", "(", "ja", "g\u00e4b'", "es", "be\u00df\u00b7re", "Staa\u00b7ten", "!", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Bezahlt dir deine Kraft und Zeit", "tokens": ["Be\u00b7zahlt", "dir", "dei\u00b7ne", "Kraft", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "F\u00fcr anbefohlne Th\u00e4tigkeit,", "tokens": ["F\u00fcr", "an\u00b7be\u00b7fohl\u00b7ne", "Th\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wozu du selber nie gerathen.", "tokens": ["Wo\u00b7zu", "du", "sel\u00b7ber", "nie", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Er winkt: du folgst. Er will: du mu\u00dft.", "tokens": ["Er", "winkt", ":", "du", "folgst", ".", "Er", "will", ":", "du", "mu\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und w\u00e4r's mit eingeklemmter Brust,", "tokens": ["Und", "w\u00e4r's", "mit", "ein\u00b7ge\u00b7klemm\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "W\u00e4r's mit zerri\u00dfnem, wunden Herzen:", "tokens": ["W\u00e4r's", "mit", "zer\u00b7ri\u00df\u00b7nem", ",", "wun\u00b7den", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Das Gold, worin man deine Pflicht", "tokens": ["Das", "Gold", ",", "wo\u00b7rin", "man", "dei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Gem\u00fcnzt, ist taub zu deinen Schmerzen.", "tokens": ["Ge\u00b7m\u00fcnzt", ",", "ist", "taub", "zu", "dei\u00b7nen", "Schmer\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Du bist bezahlt; mehr giltst du nicht.", "tokens": ["Du", "bist", "be\u00b7zahlt", ";", "mehr", "giltst", "du", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "An H\u00f6fen ist das Recht nur Gnade:", "tokens": ["An", "H\u00f6\u00b7fen", "ist", "das", "Recht", "nur", "Gna\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Die Wahrheit h\u00e4lt die Maske vor;", "tokens": ["Die", "Wahr\u00b7heit", "h\u00e4lt", "die", "Mas\u00b7ke", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Die Wissenschaft dient zur Parade;", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "dient", "zur", "Pa\u00b7ra\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.46": {"text": "Vergittert ist des Herrschers Ohr.", "tokens": ["Ver\u00b7git\u00b7tert", "ist", "des", "Herr\u00b7schers", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Der Unterthan liegt an den Stufen", "tokens": ["Der", "Un\u00b7ter\u00b7than", "liegt", "an", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Des Throns, und mag sich heiser rufen.", "tokens": ["Des", "Throns", ",", "und", "mag", "sich", "hei\u00b7ser", "ru\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Der Schmeicheleien laut'res Chor,", "tokens": ["Der", "Schmei\u00b7che\u00b7lei\u00b7en", "laut'\u00b7res", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wie trunken von des F\u00fcrsten Preise,", "tokens": ["Wie", "trun\u00b7ken", "von", "des", "F\u00fcrs\u00b7ten", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Dringt \u00fcber das Ge\u00e4chz empor.", "tokens": ["Dringt", "\u00fc\u00b7ber", "das", "Ge\u00b7\u00e4chz", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Nicht selten sang in dieser Weise", "tokens": ["Nicht", "sel\u00b7ten", "sang", "in", "die\u00b7ser", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Uns weiland ", "tokens": ["Uns", "wei\u00b7land"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Ihr reiner Nachhall t\u00f6nte leise", "tokens": ["Ihr", "rei\u00b7ner", "Nach\u00b7hall", "t\u00f6n\u00b7te", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Durch meinen Sinn, als ich ins Thor", "tokens": ["Durch", "mei\u00b7nen", "Sinn", 