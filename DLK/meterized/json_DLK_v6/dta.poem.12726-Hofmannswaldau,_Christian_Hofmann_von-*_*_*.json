{"dta.poem.12726": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solimene muste lachen,", "tokens": ["So\u00b7li\u00b7me\u00b7ne", "mus\u00b7te", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Armando zu ihr kam,", "tokens": ["Als", "Ar\u00b7man\u00b7do", "zu", "ihr", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sich in verliebten sachen", "tokens": ["Und", "sich", "in", "ver\u00b7lieb\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine gro\u00dfe freyheit nahm:", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "frey\u00b7heit", "nahm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, sprach sie, mu\u00df hunde f\u00fchren,", "tokens": ["Der", ",", "sprach", "sie", ",", "mu\u00df", "hun\u00b7de", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "$,", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der im beutel geld vermist,", "tokens": ["Der", "im", "beu\u00b7tel", "geld", "ver\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keine hand soll mich ber\u00fchren,", "tokens": ["Kei\u00b7ne", "hand", "soll", "mich", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welche nicht versilbert ist.", "tokens": ["Wel\u00b7che", "nicht", "ver\u00b7sil\u00b7bert", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}