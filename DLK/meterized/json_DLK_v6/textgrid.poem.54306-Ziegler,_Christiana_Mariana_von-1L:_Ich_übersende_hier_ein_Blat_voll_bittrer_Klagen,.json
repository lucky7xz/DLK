{"textgrid.poem.54306": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich \u00fcbersende hier ein Blat voll bittrer Klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich \u00fcbersende hier ein Blat voll bittrer Klagen,", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7sen\u00b7de", "hier", "ein", "Blat", "voll", "bit\u00b7trer", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dich der Todes-Fall von Buffeln so betr\u00fcbt.", "tokens": ["Da", "dich", "der", "To\u00b7des\u00b7Fall", "von", "Buf\u00b7feln", "so", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wirst verhoffentlich nicht nach dem Ursprung fragen,", "tokens": ["Du", "wirst", "ver\u00b7hof\u00b7fent\u00b7lich", "nicht", "nach", "dem", "Ur\u00b7sprung", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Man wei\u00df, wie sehr du ihn, Hochwerther Freund, geliebt.", "tokens": ["Man", "wei\u00df", ",", "wie", "sehr", "du", "ihn", ",", "Hoch\u00b7wert\u00b7her", "Freund", ",", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ADV", "PPER", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dein Kummer ist gerecht, ja s\u00e4h ich dich gleich weinen,", "tokens": ["Dein", "Kum\u00b7mer", "ist", "ge\u00b7recht", ",", "ja", "s\u00e4h", "ich", "dich", "gleich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So g\u00e4b ich dich darum gar nicht vor weibisch aus,", "tokens": ["So", "g\u00e4b", "ich", "dich", "da\u00b7rum", "gar", "nicht", "vor", "wei\u00b7bisch", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "ADV", "PTKNEG", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn jeder w\u00fcrde doch von deinen Thr\u00e4nen meynen,", "tokens": ["Denn", "je\u00b7der", "w\u00fcr\u00b7de", "doch", "von", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "mey\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die deutsche Redlichkeit pre\u00dft sie zum Augen raus.", "tokens": ["Die", "deut\u00b7sche", "Red\u00b7lich\u00b7keit", "pre\u00dft", "sie", "zum", "Au\u00b7gen", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Kan dich ein blosses Vieh zu solcher Wehmuth bringen,", "tokens": ["Kan", "dich", "ein", "blos\u00b7ses", "Vieh", "zu", "sol\u00b7cher", "Weh\u00b7muth", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schlie\u00df ich, da\u00df ein Freund noch mehrers hoffen kan.", "tokens": ["So", "schlie\u00df", "ich", ",", "da\u00df", "ein", "Freund", "noch", "meh\u00b7rers", "hof\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was Wunder, wann sich recht die Menschen um dich dringen,", "tokens": ["Was", "Wun\u00b7der", ",", "wann", "sich", "recht", "die", "Men\u00b7schen", "um", "dich", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWAV", "PRF", "ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn solche Redlichkeit trifft man gar selten an.", "tokens": ["Denn", "sol\u00b7che", "Red\u00b7lich\u00b7keit", "trifft", "man", "gar", "sel\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Viel schmeicheln mit dem Mund, und lachen unsrer Schmertzen,", "tokens": ["Viel", "schmei\u00b7cheln", "mit", "dem", "Mund", ",", "und", "la\u00b7chen", "uns\u00b7rer", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dieses heist wohl noch polit und ertz-galant;", "tokens": ["Und", "die\u00b7ses", "heist", "wohl", "noch", "po\u00b7lit", "und", "ertz\u00b7ga\u00b7lant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie geben uns das Wort: Sie meynten es von Hertzen;", "tokens": ["Sie", "ge\u00b7ben", "uns", "das", "Wort", ":", "Sie", "meyn\u00b7ten", "es", "von", "Hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da doch die Heucheley mehr als zu wohl bekant.", "tokens": ["Da", "doch", "die", "Heu\u00b7che\u00b7ley", "mehr", "als", "zu", "wohl", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PIAT", "KOKOM", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jedoch was untersteht sich meine schlechte Feder", "tokens": ["Je\u00b7doch", "was", "un\u00b7ter\u00b7steht", "sich", "mei\u00b7ne", "schlech\u00b7te", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dir hier bey meinen Schertz ein Lob-Lied mit zu streun.", "tokens": ["Dir", "hier", "bey", "mei\u00b7nen", "Schertz", "ein", "Lob\u00b7Lied", "mit", "zu", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein Kiel ist, wie du weist, von schlechten G\u00e4nse-Leder,", "tokens": ["Mein", "Kiel", "ist", ",", "wie", "du", "weist", ",", "von", "schlech\u00b7ten", "G\u00e4n\u00b7se\u00b7Le\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum kan er leider! nicht den Schwahnen \u00e4hnlich seyn.", "tokens": ["Drum", "kan", "er", "lei\u00b7der", "!", "nicht", "den", "Schwah\u00b7nen", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "$.", "PTKNEG", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du weist schon, da\u00df ich nicht mit auf dem H\u00fcgel sitze,", "tokens": ["Du", "weist", "schon", ",", "da\u00df", "ich", "nicht", "mit", "auf", "dem", "H\u00fc\u00b7gel", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Musen mustern mich, dieweil es billig, aus,", "tokens": ["Die", "Mu\u00b7sen", "mus\u00b7tern", "mich", ",", "die\u00b7weil", "es", "bil\u00b7lig", ",", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wenn ich gleich vor Angst bey Reim und Sylben schwitze,", "tokens": ["Und", "wenn", "ich", "gleich", "vor", "Angst", "bey", "Reim", "und", "Syl\u00b7ben", "schwit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So k\u00f6mmt zuletzt doch nur ein sch\u00f6nes Nichts heraus.", "tokens": ["So", "k\u00f6mmt", "zu\u00b7letzt", "doch", "nur", "ein", "sch\u00f6\u00b7nes", "Nichts", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich kan die Leyer nicht, wie sich geh\u00f6ret, stimmen,", "tokens": ["Ich", "kan", "die", "Le\u00b7yer", "nicht", ",", "wie", "sich", "ge\u00b7h\u00f6\u00b7ret", ",", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "$,", "PWAV", "PRF", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Musen-Gott hat sich mit mir noch nicht verm\u00e4hlt,", "tokens": ["Der", "Mu\u00b7sen\u00b7Gott", "hat", "sich", "mit", "mir", "noch", "nicht", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wenn ich noch so schnell will neben andern schwimmen,", "tokens": ["Und", "wenn", "ich", "noch", "so", "schnell", "will", "ne\u00b7ben", "an\u00b7dern", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VMFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So seh ich, da\u00df es mir an Flo\u00df und Federn fehlt.", "tokens": ["So", "seh", "ich", ",", "da\u00df", "es", "mir", "an", "Flo\u00df", "und", "Fe\u00b7dern", "fehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch hoff ich in der That Vergebung zu erlangen,", "tokens": ["Doch", "hoff", "ich", "in", "der", "That", "Ver\u00b7ge\u00b7bung", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil meine Feder dich zu tr\u00f6sten, sich bem\u00fcht.", "tokens": ["Weil", "mei\u00b7ne", "Fe\u00b7der", "dich", "zu", "tr\u00f6s\u00b7ten", ",", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Beyleid trocknet dir die Z\u00e4hren von den Wangen,", "tokens": ["Mein", "Bey\u00b7leid", "trock\u00b7net", "dir", "die", "Z\u00e4h\u00b7ren", "von", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So schlecht bey solchen Dienst auch meine Muse sieht.", "tokens": ["So", "schlecht", "bey", "sol\u00b7chen", "Dienst", "auch", "mei\u00b7ne", "Mu\u00b7se", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was aber soll ich wohl zu deinem Troste schreiben?", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "wohl", "zu", "dei\u00b7nem", "Tros\u00b7te", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn der Verlust, der dich betrifft, ist ungemein,", "tokens": ["Denn", "der", "Ver\u00b7lust", ",", "der", "dich", "be\u00b7tr\u00b7ifft", ",", "ist", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.35": {"text": "Und deinem Buffel mu\u00df der Nachruhm sicher bleiben", "tokens": ["Und", "dei\u00b7nem", "Buf\u00b7fel", "mu\u00df", "der", "Nach\u00b7ruhm", "si\u00b7cher", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df seines gleichen nicht leicht wird zu finden seyn.", "tokens": ["Da\u00df", "sei\u00b7nes", "glei\u00b7chen", "nicht", "leicht", "wird", "zu", "fin\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PTKNEG", "ADJD", "VAFIN", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Man sahe Kunst und Witz ihm stets zur Seiten stehen,", "tokens": ["Man", "sa\u00b7he", "Kunst", "und", "Witz", "ihm", "stets", "zur", "Sei\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.38": {"text": "Er nieste, wann sein Herr es ihm befehlen lie\u00df,", "tokens": ["Er", "nies\u00b7te", ",", "wann", "sein", "Herr", "es", "ihm", "be\u00b7feh\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und sprach man: Lincks und rechts, so wust er sich zu drehen,", "tokens": ["Und", "sprach", "man", ":", "Lincks", "und", "rechts", ",", "so", "wust", "er", "sich", "zu", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$.", "NE", "KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als wann er in der That der gr\u00f6ste Springer hie\u00df.", "tokens": ["Als", "wann", "er", "in", "der", "That", "der", "gr\u00f6s\u00b7te", "Sprin\u00b7ger", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "O! da\u00df kein hoher Geist ein Lob-Lied ihm geschrieben,", "tokens": ["O", "!", "da\u00df", "kein", "ho\u00b7her", "Geist", "ein", "Lob\u00b7Lied", "ihm", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "---+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Der seiner Tugend Lauff uns besser schildern kan.", "tokens": ["Der", "sei\u00b7ner", "Tu\u00b7gend", "Lauff", "uns", "bes\u00b7ser", "schil\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein solch Gesch\u00f6pffe mu\u00df man gantz besonders lieben.", "tokens": ["Ein", "solch", "Ge\u00b7sch\u00f6pf\u00b7fe", "mu\u00df", "man", "gantz", "be\u00b7son\u00b7ders", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.44": {"text": "Man traff bey Buffels Thun mehr, als was h\u00fcndsches, an.", "tokens": ["Man", "traff", "bey", "Buf\u00b7fels", "Thun", "mehr", ",", "als", "was", "h\u00fcnd\u00b7sches", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "NN", "ADV", "$,", "KOUS", "PIS", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Drum m\u00fcssen wir mit Recht auf sein Begr\u00e4bni\u00df dencken,", "tokens": ["Drum", "m\u00fcs\u00b7sen", "wir", "mit", "Recht", "auf", "sein", "Be\u00b7gr\u00e4b\u00b7ni\u00df", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "O! setz ihm nach Verdienst ein Epitaphium,", "tokens": ["O", "!", "setz", "ihm", "nach", "Ver\u00b7dienst", "ein", "E\u00b7pi\u00b7ta\u00b7phi\u00b7um", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich will die Schrifft darauf von Hertzen gerne schencken,", "tokens": ["Ich", "will", "die", "Schrifft", "da\u00b7rauf", "von", "Hert\u00b7zen", "ger\u00b7ne", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PAV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Was g\u00e4b ich nicht, mein Freund, um dich zu tr\u00f6sten, drum?", "tokens": ["Was", "g\u00e4b", "ich", "nicht", ",", "mein", "Freund", ",", "um", "dich", "zu", "tr\u00f6s\u00b7ten", ",", "drum", "?"], 