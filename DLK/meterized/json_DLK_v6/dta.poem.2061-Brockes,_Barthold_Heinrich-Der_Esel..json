{"dta.poem.2061": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Esel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch der Esel, ob man ihn sonder Ursach oft verach-", "tokens": ["Auch", "der", "E\u00b7sel", ",", "ob", "man", "ihn", "son\u00b7der", "Ur\u00b7sach", "oft", "ver\u00b7ach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "ADJA", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "tet,", "tokens": ["tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ist, wenn man sein ganzes Wesen ohne Vorurtheil be-", "tokens": ["Ist", ",", "wenn", "man", "sein", "gan\u00b7zes", "We\u00b7sen", "oh\u00b7ne", "Vor\u00b7urt\u00b7heil", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PIS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "trachtet,", "tokens": ["trach\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ein besonders n\u00fctzlichs Thier. Ist sein Geist gleich dumm", "tokens": ["Ein", "be\u00b7son\u00b7ders", "n\u00fctz\u00b7lichs", "Thier", ".", "Ist", "sein", "Geist", "gleich", "dumm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "und tr\u00e4ge,", "tokens": ["und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "L\u00e4\u00dft er sich gleich zu der Arbeit selten leiten, als durch", "tokens": ["L\u00e4\u00dft", "er", "sich", "gleich", "zu", "der", "Ar\u00b7beit", "sel\u00b7ten", "lei\u00b7ten", ",", "als", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "ADJD", "VVINF", "$,", "KOUS", "APPR"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schl\u00e4ge;", "tokens": ["Schl\u00e4\u00b7ge", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ist er dennoch sehr geduldig, dauerhaft, und stark da-", "tokens": ["Ist", "er", "den\u00b7noch", "sehr", "ge\u00b7dul\u00b7dig", ",", "dau\u00b7er\u00b7haft", ",", "und", "stark", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADJD", "$,", "KON", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "bey,", "tokens": ["bey", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Gro\u00dfe Lasten wegzutragen: da\u00df auch in der Arzeney", "tokens": ["Gro\u00b7\u00dfe", "Las\u00b7ten", "weg\u00b7zu\u00b7tra\u00b7gen", ":", "da\u00df", "auch", "in", "der", "Ar\u00b7ze\u00b7ney"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$.", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Dieses Thieres Milch besonders von dem st\u00e4rksten Nutzen", "tokens": ["Die\u00b7ses", "Thie\u00b7res", "Milch", "be\u00b7son\u00b7ders", "von", "dem", "st\u00e4rks\u00b7ten", "Nut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "sey;", "tokens": ["sey", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Zeiget die Erfahrung t\u00e4glich. Womit man ihn h\u00e4lt und", "tokens": ["Zei\u00b7get", "die", "Er\u00b7fah\u00b7rung", "t\u00e4g\u00b7lich", ".", "Wo\u00b7mit", "man", "ihn", "h\u00e4lt", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$.", "PWAV", "PIS", "PPER", "VVFIN", "KON"], "meter": "+-+-+-+-----+-", "measure": "unknown.measure.penta"}, "line.15": {"text": "n\u00e4hrt,", "tokens": ["n\u00e4hrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Ist von so geringen Kosten, da\u00df sein Futter fast nichts", "tokens": ["Ist", "von", "so", "ge\u00b7rin\u00b7gen", "Kos\u00b7ten", ",", "da\u00df", "sein", "Fut\u00b7ter", "fast", "nichts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PIS"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "werth:", "tokens": ["werth", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Dorn und Diesteln, Stroh und Spreu, was so Pferd", "tokens": ["Dorn", "und", "Dies\u00b7teln", ",", "Stroh", "und", "Spreu", ",", "was", "so", "Pferd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "ADV", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "als Rindvieh liegen,", "tokens": ["als", "Rind\u00b7vieh", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Fallen lassen und zertreten, fri\u00dft der Esel mit Vergn\u00fc-", "tokens": ["Fal\u00b7len", "las\u00b7sen", "und", "zer\u00b7tre\u00b7ten", ",", "fri\u00dft", "der", "E\u00b7sel", "mit", "Ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Au\u00dfer da\u00df er Lasten tr\u00e4gt, brauchet man ihn auch zum", "tokens": ["Au\u00b7\u00dfer", "da\u00df", "er", "Las\u00b7ten", "tr\u00e4gt", ",", "brau\u00b7chet", "man", "ihn", "auch", "zum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PIS", "PPER", "ADV", "APPRART"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.23": {"text": "Pfl\u00fcgen,", "tokens": ["Pfl\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Karrenziehen und zum Egen. Seine Milch dient nicht", "tokens": ["Kar\u00b7ren\u00b7zie\u00b7hen", "und", "zum", "E\u00b7gen", ".", "Sei\u00b7ne", "Milch", "dient", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPRART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "In der Schwindsucht, auch zur Gicht und zum Poda-", "tokens": ["In", "der", "Schwind\u00b7sucht", ",", "auch", "zur", "Gicht", "und", "zum", "Po\u00b7da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "gra; sie heilet", "tokens": ["gra", ";", "sie", "hei\u00b7let"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Innere Entz\u00fcndungen, sie soll gar vortrefflich seyn,", "tokens": ["In\u00b7ne\u00b7re", "Ent\u00b7z\u00fcn\u00b7dun\u00b7gen", ",", "sie", "soll", "gar", "vor\u00b7treff\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.29": {"text": "Eine sch\u00f6ne Haut zu machen, sie erg\u00e4nzet und ertheilet", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Haut", "zu", "ma\u00b7chen", ",", "sie", "er\u00b7g\u00e4n\u00b7zet", "und", "er\u00b7thei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}