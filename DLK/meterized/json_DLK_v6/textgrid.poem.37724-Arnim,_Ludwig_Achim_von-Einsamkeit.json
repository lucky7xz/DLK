{"textgrid.poem.37724": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird mir von Trauerlarven", "tokens": ["Wird", "mir", "von", "Trau\u00b7er\u00b7lar\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stra\u00dfe gar bedr\u00e4ngt,", "tokens": ["Die", "Stra\u00b7\u00dfe", "gar", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fchl' ich Schmerz den scharfen,", "tokens": ["Und", "f\u00fchl'", "ich", "Schmerz", "den", "schar\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie er den Hals beengt,", "tokens": ["Wie", "er", "den", "Hals", "be\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann leg' ich den Kopf in den Rachen", "tokens": ["Dann", "leg'", "ich", "den", "Kopf", "in", "den", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Von meinem zahmen Leu,", "tokens": ["Von", "mei\u00b7nem", "zah\u00b7men", "Leu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und lieg' da wie im Nachen", "tokens": ["Und", "lieg'", "da", "wie", "im", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und la\u00df mich treiben frei.", "tokens": ["Und", "la\u00df", "mich", "trei\u00b7ben", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So ruh' ich in den Schranken", "tokens": ["So", "ruh'", "ich", "in", "den", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der reichen Einsamkeit,", "tokens": ["Der", "rei\u00b7chen", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehe in Gedanken", "tokens": ["Und", "se\u00b7he", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch \u00fcber meine Zeit;", "tokens": ["Noch", "\u00fc\u00b7ber", "mei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kann ich der anderen lachen", "tokens": ["Da", "kann", "ich", "der", "an\u00b7de\u00b7ren", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und schrecklich lustig sein,", "tokens": ["Und", "schreck\u00b7lich", "lus\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In meinen Schreckensnachen", "tokens": ["In", "mei\u00b7nen", "Schre\u00b7cken\u00b7sna\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dringt nie das Schrecken ein.", "tokens": ["Dringt", "nie", "das", "Schre\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da will ich ruhig bleiben", "tokens": ["Da", "will", "ich", "ru\u00b7hig", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schreiet auch die Welt,", "tokens": ["Und", "schrei\u00b7et", "auch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Leu will dich entleiben", "tokens": ["Der", "Leu", "will", "dich", "ent\u00b7lei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich nur ruhig stellt;", "tokens": ["Und", "sich", "nur", "ru\u00b7hig", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wird mich doch nimmer erdr\u00fccken,", "tokens": ["Er", "wird", "mich", "doch", "nim\u00b7mer", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Doch l\u00e4\u00dft er mich nicht los,", "tokens": ["Doch", "l\u00e4\u00dft", "er", "mich", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis er mir mit Entz\u00fccken", "tokens": ["Bis", "er", "mir", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Zeigt meine Hoffnung gro\u00df.", "tokens": ["Zeigt", "mei\u00b7ne", "Hoff\u00b7nung", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mag auch sein Auge funkeln,", "tokens": ["Mag", "auch", "sein", "Au\u00b7ge", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schrecket euch zur\u00fcck,", "tokens": ["Er", "schre\u00b7cket", "euch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ruhe hier im Dunkeln", "tokens": ["Ich", "ru\u00b7he", "hier", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und finde hier mein Gl\u00fcck;", "tokens": ["Und", "fin\u00b7de", "hier", "mein", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entsteig' dann geblendet dem Rachen,", "tokens": ["Ent\u00b7steig'", "dann", "ge\u00b7blen\u00b7det", "dem", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So helle war mir's nie,", "tokens": ["So", "hel\u00b7le", "war", "mir's", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die alten Freunde lachen,", "tokens": ["Die", "al\u00b7ten", "Freun\u00b7de", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil ich den Mund nicht verzieh.", "tokens": ["Weil", "ich", "den", "Mund", "nicht", "ver\u00b7zieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Wo bin ich hingetrieben,", "tokens": ["Wo", "bin", "ich", "hin\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dies ist der alte Baum,", "tokens": ["Dies", "ist", "der", "al\u00b7te", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist noch gr\u00fcn geblieben,", "tokens": ["Er", "ist", "noch", "gr\u00fcn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gr\u00f6\u00dfer ist er kaum;", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "ist", "er", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Freunden erz\u00e4hl' ich mit Freuden", "tokens": ["Den", "Freun\u00b7den", "er\u00b7z\u00e4hl'", "ich", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nun manchen lust'gen Streich,", "tokens": ["Nun", "man\u00b7chen", "lust'\u00b7gen", "Streich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf das bittre Leiden", "tokens": ["Und", "auf", "das", "bitt\u00b7re", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Leg' ich die gr\u00fcnenden Zweig'.", "tokens": ["Leg'", "ich", "die", "gr\u00fc\u00b7nen\u00b7den", "Zweig'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Wo einer ist erschlagen,", "tokens": ["Wo", "ei\u00b7ner", "ist", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Legt jeder gr\u00fcne Zweig'.", "tokens": ["Legt", "je\u00b7der", "gr\u00fc\u00b7ne", "Zweig'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wo ein Volk geschlagen,", "tokens": ["Doch", "wo", "ein", "Volk", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da lacht ein jeder gleich;", "tokens": ["Da", "lacht", "ein", "je\u00b7der", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht schmerzliches Lachen zu sehen", "tokens": ["Nicht", "schmerz\u00b7li\u00b7ches", "La\u00b7chen", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Bedeckt der Schmerz mein Aug',", "tokens": ["Be\u00b7deckt", "der", "Schmerz", "mein", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn eisende Winde klar wehen,", "tokens": ["Wenn", "ei\u00b7sen\u00b7de", "Win\u00b7de", "klar", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Da tr\u00fcbet sie mein Hauch.", "tokens": ["Da", "tr\u00fc\u00b7bet", "sie", "mein", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sollen alle wecken,", "tokens": ["So", "sol\u00b7len", "al\u00b7le", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfindung, die verlacht,", "tokens": ["Emp\u00b7fin\u00b7dung", ",", "die", "ver\u00b7lacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die Empfindung necken,", "tokens": ["Und", "die", "Emp\u00b7fin\u00b7dung", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sie sich \u00fcberwacht:", "tokens": ["Wo", "sie", "sich", "\u00fc\u00b7ber\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann kehrte das sichere Wetter", "tokens": ["Dann", "kehr\u00b7te", "das", "si\u00b7che\u00b7re", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zu unsrer Erd' zur\u00fcck,", "tokens": ["Zu", "uns\u00b7rer", "Erd'", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und alle w\u00e4ren Retter", "tokens": ["Und", "al\u00b7le", "w\u00e4\u00b7ren", "Ret\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von unserm alten Gl\u00fcck.", "tokens": ["Von", "un\u00b7serm", "al\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}