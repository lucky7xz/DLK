{"textgrid.poem.57819": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verspruch", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind einander zugesellt", "tokens": ["Wir", "sind", "ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7sellt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alle Ewigkeit,", "tokens": ["F\u00fcr", "al\u00b7le", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns scheidet nicht die ganze Welt", "tokens": ["Uns", "schei\u00b7det", "nicht", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrer Schlechtigkeit.", "tokens": ["Mit", "ih\u00b7rer", "Schlech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der wei\u00df ja nicht, was Lieben ist,", "tokens": ["Der", "wei\u00df", "ja", "nicht", ",", "was", "Lie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der an ein Scheiden denkt,", "tokens": ["Der", "an", "ein", "Schei\u00b7den", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn zweie sich so recht gek\u00fc\u00dft,", "tokens": ["Wenn", "zwei\u00b7e", "sich", "so", "recht", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Leib und Seel' geschenkt.", "tokens": ["Sich", "Leib", "und", "Seel'", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lieben wir uns immerdar,", "tokens": ["So", "lie\u00b7ben", "wir", "uns", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Sommerszeit,", "tokens": ["Die", "gan\u00b7ze", "Som\u00b7mers\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lieben uns das ganze Jahr,", "tokens": ["Und", "lie\u00b7ben", "uns", "das", "gan\u00b7ze", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df es friert und schneit.", "tokens": ["Bis", "da\u00df", "es", "friert", "und", "schneit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und kommt der Tag, der kein Tag ist,", "tokens": ["Und", "kommt", "der", "Tag", ",", "der", "kein", "Tag", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und mu\u00df ich fort von dir,", "tokens": ["Und", "mu\u00df", "ich", "fort", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Herz doch meiner nicht vergi\u00dft,", "tokens": ["Dein", "Herz", "doch", "mei\u00b7ner", "nicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es findet sich zu mir.", "tokens": ["Es", "fin\u00b7det", "sich", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn ob der Schnee zur Erde f\u00e4llt,", "tokens": ["Denn", "ob", "der", "Schnee", "zur", "Er\u00b7de", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bl\u00fchn die Rosen rot,", "tokens": ["Und", "bl\u00fchn", "die", "Ro\u00b7sen", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind einander zugesellt", "tokens": ["Wir", "sind", "ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7sellt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Leben und im Tod.", "tokens": ["Im", "Le\u00b7ben", "und", "im", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}