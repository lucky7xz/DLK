{"textgrid.poem.26994": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Helf Gott!", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das verw\u00fcnschte Gebettel! Es haben die vorderen Kutschen", "tokens": ["Das", "ver\u00b7w\u00fcnschte", "Ge\u00b7bet\u00b7tel", "!", "Es", "ha\u00b7ben", "die", "vor\u00b7de\u00b7ren", "Kut\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Reichlich f\u00fcr uns mit bezahlt. Geben nichts. Kutscher, fahr zu!", "tokens": ["Reich\u00b7lich", "f\u00fcr", "uns", "mit", "be\u00b7zahlt", ".", "Ge\u00b7ben", "nichts", ".", "Kut\u00b7scher", ",", "fahr", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "VVPP", "$.", "NN", "PIS", "$.", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}