{"dta.poem.18794": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nimmer will ich dich verlieren!", "tokens": ["Nim\u00b7mer", "will", "ich", "dich", "ver\u00b7lie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe giebt der Liebe Kraft.", "tokens": ["Lie\u00b7be", "giebt", "der", "Lie\u00b7be", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Magst du meine Jugend zieren", "tokens": ["Magst", "du", "mei\u00b7ne", "Ju\u00b7gend", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gewaltiger Leidenschaft.", "tokens": ["Mit", "ge\u00b7wal\u00b7ti\u00b7ger", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ach! wie schmeichelt\u2019s meinem Triebe,", "tokens": ["Ach", "!", "wie", "schmei\u00b7chelt's", "mei\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn man meinen Dichter preist:", "tokens": ["Wenn", "man", "mei\u00b7nen", "Dich\u00b7ter", "preist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn das Leben ist die Liebe,", "tokens": ["Denn", "das", "Le\u00b7ben", "ist", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des Lebens Leben Geist.", "tokens": ["Und", "des", "Le\u00b7bens", "Le\u00b7ben", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}