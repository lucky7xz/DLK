, "uns", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feind nicht l\u00e4nger stehen kann,", "tokens": ["Der", "Feind", "nicht", "l\u00e4n\u00b7ger", "ste\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weicht ab auf allen Seiten:", "tokens": ["Weicht", "ab", "auf", "al\u00b7len", "Sei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Br\u00fcder, setzt nur muthig drein,", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "setzt", "nur", "mut\u00b7hig", "drein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Feinde thun verzaget seyn,", "tokens": ["Die", "Fein\u00b7de", "thun", "ver\u00b7za\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sieg und Preis sey unser,", "tokens": ["Der", "Sieg", "und", "Preis", "sey", "un\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drom, Drari, Drom,", "tokens": ["Drom", ",", "Dra\u00b7ri", ",", "Drom", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+