{"dta.poem.12363": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Frommer Soldaten seligster Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Krieg hat sich in dieser Welt               ", "tokens": ["Viel", "Krieg", "hat", "sich", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mancher Ursach erhoben;", "tokens": ["Man\u00b7cher", "Ur\u00b7sach", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Demselben hat Gott zugesellt,", "tokens": ["Dem\u00b7sel\u00b7ben", "hat", "Gott", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Musik, ihn zu loben.", "tokens": ["Die", "Mu\u00b7sik", ",", "ihn", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr erst Erfinder war Jubal,", "tokens": ["Ihr", "erst", "Er\u00b7fin\u00b7der", "war", "Ju\u00b7bal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lamechs Sohn mit Namen,", "tokens": ["Des", "La\u00b7mechs", "Sohn", "mit", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erfand Drometen- und Pfeifenschall,", "tokens": ["Er\u00b7fand", "Dro\u00b7me\u00b7ten", "und", "Pfei\u00b7fen\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Konnt sie stimmen zusammen.", "tokens": ["Konnt", "sie", "stim\u00b7men", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Die Musik gut,", "tokens": ["Die", "Mu\u00b7sik", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Erweckt den Muth,", "tokens": ["Er\u00b7weckt", "den", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Frisch unverzagt,", "tokens": ["Frisch", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Die Feind verjagt,", "tokens": ["Die", "Feind", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ruft stark, dran, dran,", "tokens": ["Ruft", "stark", ",", "dran", ",", "dran", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PAV", "$,", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "An Feind hinan,", "tokens": ["An", "Feind", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Brecht m\u00e4chtig durch,", "tokens": ["Brecht", "m\u00e4ch\u00b7tig", "durch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Schlagt Gasse und Furch,", "tokens": ["Schlagt", "Gas\u00b7se", "und", "Furch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Schie\u00dft, stecht und haut alles nieder,", "tokens": ["Schie\u00dft", ",", "stecht", "und", "haut", "al\u00b7les", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Da\u00df keiner aufsteht wieder.", "tokens": ["Da\u00df", "kei\u00b7ner", "auf\u00b7steht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als dort Elisa weissagen sollt,", "tokens": ["Als", "dort", "E\u00b7li\u00b7sa", "weis\u00b7sa\u00b7gen", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VVINF", "VMFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da Israel Durst litte,", "tokens": ["Da", "Is\u00b7rael", "Durst", "lit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sprach er: Mir bald ein Spielmann holt,", "tokens": ["Sprach", "er", ":", "Mir", "bald", "ein", "Spiel\u00b7mann", "holt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der spielt nach Davids Sitte.", "tokens": ["Der", "spielt", "nach", "Da\u00b7vids", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch spielt vor ihm des Herren Hand,", "tokens": ["Auch", "spielt", "vor", "ihm", "des", "Her\u00b7ren", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er th\u00e4te Trost weissagen:", "tokens": ["Er", "th\u00e4\u00b7te", "Trost", "weis\u00b7sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ohn Regen, flo\u00df gro\u00df Wasser durchs Land,", "tokens": ["Ohn", "Re\u00b7gen", ",", "flo\u00df", "gro\u00df", "Was\u00b7ser", "durchs", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der Feind wurd auch geschlagen.", "tokens": ["Der", "Feind", "wurd", "auch", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drom, drari, drom,", "tokens": ["Drom", ",", "dra\u00b7ri", ",", "drom", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Pom, pom, pom, pom,", "tokens": ["Pom", ",", "pom", ",", "pom", ",", "pom", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "FM.la", "$,", "FM.la", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Droml und Pfeifen gut", "tokens": ["Droml", "und", "Pfei\u00b7fen", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Macht Helden Muth,", "tokens": ["Macht", "Hel\u00b7den", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Erweckt Propheten,", "tokens": ["Er\u00b7weckt", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Reizt die Poeten;", "tokens": ["Reizt", "die", "Po\u00b7et\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "In Fried und Streit,", "tokens": ["In", "Fried", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "H\u00f6rt mans allezeit,", "tokens": ["H\u00f6rt", "mans", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Musikam soll man ehren,", "tokens": ["Mu\u00b7si\u00b7kam", "soll", "man", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Man kann ihr nicht entbehren.", "tokens": ["Man", "kann", "ihr", "nicht", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man schreibt, da\u00df wenn Timotheus,", "tokens": ["Man", "schreibt", ",", "da\u00df", "wenn", "Ti\u00b7mo\u00b7theus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "KOUS", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach der Dorier Weise th\u00e4t singen,", "tokens": ["Nach", "der", "Do\u00b7rier", "Wei\u00b7se", "th\u00e4t", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Als ein ber\u00fchmter Musikus,", "tokens": ["Als", "ein", "be\u00b7r\u00fchm\u00b7ter", "Mu\u00b7si\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Konnt' er in Harnisch bringen,", "tokens": ["Konnt'", "er", "in", "Har\u00b7nisch", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Alexandrum Magnum den Held,", "tokens": ["A\u00b7le\u00b7xan\u00b7drum", "Mag\u00b7num", "den", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Streit satt konnt er nicht werden,", "tokens": ["Streit", "satt", "konnt", "er", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Bis er zwang fast die ganze Welt,", "tokens": ["Bis", "er", "zwang", "fast", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Bekriegt den Kreis der Erden.", "tokens": ["Be\u00b7kriegt", "den", "Kreis", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Timotheus", "tokens": ["Ti\u00b7mo\u00b7theus"], "token_info": ["word"], "pos": ["NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Milesius", "tokens": ["Mi\u00b7le\u00b7si\u00b7us"], "token_info": ["word"], "pos": ["NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Konnt' gewaltig sing'n,", "tokens": ["Konnt'", "ge\u00b7wal\u00b7tig", "sing'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "That mit aufbring'n", "tokens": ["That", "mit", "auf\u00b7bring'n"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Alexandrum,", "tokens": ["A\u00b7le\u00b7xan\u00b7drum", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.14": {"text": "Regem Magnum,", "tokens": ["Re\u00b7gem", "Mag\u00b7num", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Da\u00df er in Wuth,", "tokens": ["Da\u00df", "er", "in", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Und Heldenmuth", "tokens": ["Und", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Fa\u00dfst Schild, Schwerdt und Kriegs-Waffen,", "tokens": ["Fa\u00df\u00b7st", "Schild", ",", "Schwerdt", "und", "Kriegs\u00b7Waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Im Grimm die Feind zu strafen.", "tokens": ["Im", "Grimm", "die", "Feind", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob theils gleich wollten weichen ab,", "tokens": ["Ob", "theils", "gleich", "woll\u00b7ten", "wei\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie oftmals ist geschehen:", "tokens": ["Wie", "oft\u00b7mals", "ist", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jedoch ein L\u00f6wenmuth ich hab'", "tokens": ["Je\u00b7doch", "ein", "L\u00f6\u00b7wen\u00b7muth", "ich", "hab'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und vorn sollt ihr mich sehen:", "tokens": ["Und", "vorn", "sollt", "ihr", "mich", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Kern springt vor, die Spreu bleibt hint'n,", "tokens": ["Der", "Kern", "springt", "vor", ",", "die", "Spreu", "bleibt", "hint'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft herzhaft hier drein schlagen,", "tokens": ["La\u00dft", "herz\u00b7haft", "hier", "drein", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie werden sich wohl wiederum wenden,", "tokens": ["Sie", "wer\u00b7den", "sich", "wohl", "wie\u00b7de\u00b7rum", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ihr Br\u00fcder thut nicht verzagen.", "tokens": ["Ihr", "Br\u00fc\u00b7der", "thut", "nicht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Kierieleison,", "tokens": ["Kie\u00b7rie\u00b7lei\u00b7son", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Pidi, pom, pom, pom,", "tokens": ["Pi\u00b7di", ",", "pom", ",", "pom", ",", "pom", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "FM.la", "$,", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Lerm,