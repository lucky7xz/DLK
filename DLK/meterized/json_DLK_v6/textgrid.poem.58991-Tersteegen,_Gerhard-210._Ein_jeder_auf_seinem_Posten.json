{"textgrid.poem.58991": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "210. Ein jeder auf seinem Posten", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vater sorgt und h\u00e4lt die Wacht,", "tokens": ["Der", "Va\u00b7ter", "sorgt", "und", "h\u00e4lt", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sollst ihn lieben und ansehen;", "tokens": ["Du", "sollst", "ihn", "lie\u00b7ben", "und", "an\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm du nur deinen Post'n in acht,", "tokens": ["Nimm", "du", "nur", "dei\u00b7nen", "Post'n", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott wird den seinen wohl versehen!", "tokens": ["Gott", "wird", "den", "sei\u00b7nen", "wohl", "ver\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}