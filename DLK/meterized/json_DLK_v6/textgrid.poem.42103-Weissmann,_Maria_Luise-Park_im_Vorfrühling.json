{"textgrid.poem.42103": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Park im Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O halte dich, aus dir nicht zu entgleiten", "tokens": ["O", "hal\u00b7te", "dich", ",", "aus", "dir", "nicht", "zu", "ent\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "APPR", "PPER", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ins Grenzenlose! Folg der Hunde Spiel", "tokens": ["Ins", "Gren\u00b7zen\u00b7lo\u00b7se", "!", "Folg", "der", "Hun\u00b7de", "Spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mit dem Blick, wies wei\u00df in dunkeln Weiten", "tokens": ["Nicht", "mit", "dem", "Blick", ",", "wies", "wei\u00df", "in", "dun\u00b7keln", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "VVFIN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der B\u00fcsche schwindet. Lausch nicht auf ein Ziel", "tokens": ["Der", "B\u00fc\u00b7sche", "schwin\u00b7det", ".", "Lausch", "nicht", "auf", "ein", "Ziel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im unruhvollen Trab der schnellen Pferde,", "tokens": ["Im", "un\u00b7ruh\u00b7vol\u00b7len", "Trab", "der", "schnel\u00b7len", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von fern her klopfend. Achte, da\u00df die Sohle", "tokens": ["Von", "fern", "her", "klop\u00b7fend", ".", "Ach\u00b7te", ",", "da\u00df", "die", "Soh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADV", "VVPP", "$.", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der F\u00fc\u00dfe dir nicht, schwindelnde Geberde,", "tokens": ["Der", "F\u00fc\u00b7\u00dfe", "dir", "nicht", ",", "schwin\u00b7deln\u00b7de", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-----", "measure": "unknown.measure.tri"}, "line.8": {"text": "Der H\u00fcgel Fall und Anstieg wiederhole,", "tokens": ["Der", "H\u00fc\u00b7gel", "Fall", "und", "An\u00b7stieg", "wie\u00b7der\u00b7ho\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schmeck nicht zu tief den bittern Knospenduft!..", "tokens": ["Schmeck", "nicht", "zu", "tief", "den", "bit\u00b7tern", "Knos\u00b7pen\u00b7duft", "!", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "PTKA", "ADJD", "ART", "ADJA", "NN", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und doch zuletzt, ein Trunkener und schwer", "tokens": ["Und", "doch", "zu\u00b7letzt", ",", "ein", "Trun\u00b7ke\u00b7ner", "und", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ART", "NN", "KON", "ADJD"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.11": {"text": "Brichst taumelnd du durch tr\u00fcgerische Luft", "tokens": ["Brichst", "tau\u00b7melnd", "du", "durch", "tr\u00fc\u00b7ge\u00b7ri\u00b7sche", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Tief in des Himmels schwarz besch\u00e4umtes Meer.", "tokens": ["Tief", "in", "des", "Him\u00b7mels", "schwarz", "be\u00b7sch\u00e4um\u00b7tes", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}}}}