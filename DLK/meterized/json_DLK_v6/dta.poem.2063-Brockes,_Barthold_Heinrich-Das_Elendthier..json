{"dta.poem.2063": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Elendthier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein recht sonderbar Gesch\u00f6pf, halb ein Hirsch und", "tokens": ["Ein", "recht", "son\u00b7der\u00b7bar", "Ge\u00b7sch\u00f6pf", ",", "halb", "ein", "Hirsch", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,", "ADJD", "ART", "NN", "KON"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "halb ein Pferd,", "tokens": ["halb", "ein", "Pferd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ist das Elend, das nicht minder unserer Betrachtung", "tokens": ["Ist", "das", "E\u00b7lend", ",", "das", "nicht", "min\u00b7der", "un\u00b7se\u00b7rer", "Be\u00b7trach\u00b7tung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "werth:", "tokens": ["werth", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sein Geweih ist ganz besonders, und als sonst kein Thier", "tokens": ["Sein", "Ge\u00b7weih", "ist", "ganz", "be\u00b7son\u00b7ders", ",", "und", "als", "sonst", "kein", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,", "KON", "KOUS", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "es tr\u00e4get,", "tokens": ["es", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Eines Adlers Schwingen gleich, wenn er sie herunter", "tokens": ["Ei\u00b7nes", "Ad\u00b7lers", "Schwin\u00b7gen", "gleich", ",", "wenn", "er", "sie", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKVZ"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "schl\u00e4get.", "tokens": ["schl\u00e4\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Von der Klauen saget man, da\u00df sie gro\u00dfe Kr\u00e4fte heget,", "tokens": ["Von", "der", "Klau\u00b7en", "sa\u00b7get", "man", ",", "da\u00df", "sie", "gro\u00b7\u00dfe", "Kr\u00e4f\u00b7te", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und im Krampf und Nervenschmerzen Linderung und", "tokens": ["Und", "im", "Krampf", "und", "Ner\u00b7ven\u00b7schmer\u00b7zen", "Lin\u00b7de\u00b7rung", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KON", "NN", "NN", "KON"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "H\u00fclfe bringet.", "tokens": ["H\u00fcl\u00b7fe", "brin\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Da die Dicke seiner Haut weder Hieb noch Stich durch-", "tokens": ["Da", "die", "Di\u00b7cke", "sei\u00b7ner", "Haut", "we\u00b7der", "Hieb", "noch", "Stich", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "ADV", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "dringet,", "tokens": ["drin\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Wird mit ihr, an Panzers Statt, im Gefecht, die Brust", "tokens": ["Wird", "mit", "ihr", ",", "an", "Pan\u00b7zers", "Statt", ",", "im", "Ge\u00b7fecht", ",", "die", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "$,", "APPR", "NN", "NN", "$,", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "umringet.", "tokens": ["um\u00b7rin\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Seines K\u00f6rpers Schwere gleicht einer ziemlich starken", "tokens": ["Sei\u00b7nes", "K\u00f6r\u00b7pers", "Schwe\u00b7re", "gleicht", "ei\u00b7ner", "ziem\u00b7lich", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "ADV", "VVINF"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Kuh,", "tokens": ["Kuh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Vorn am Halse ist es zotticht, aber glatt nach hinten zu:", "tokens": ["Vorn", "am", "Hal\u00b7se", "ist", "es", "zot\u00b7ticht", ",", "a\u00b7ber", "glatt", "nach", "hin\u00b7ten", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Um sein langes Obermaul soll man es nicht vorwerts", "tokens": ["Um", "sein", "lan\u00b7ges", "O\u00b7ber\u00b7maul", "soll", "man", "es", "nicht", "vor\u00b7werts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "gehen,", "tokens": ["ge\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "(da\u00df es nicht im Grafen hindre) sondern r\u00fcckwerts wei-", "tokens": ["(", "da\u00df", "es", "nicht", "im", "Gra\u00b7fen", "hind\u00b7re", ")", "son\u00b7dern", "r\u00fcck\u00b7werts", "wei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$(", "KON", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "den sehen.", "tokens": ["den", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Langen Durst und schwere Arbeit ist es t\u00fcchtig zu er-", "tokens": ["Lan\u00b7gen", "Durst", "und", "schwe\u00b7re", "Ar\u00b7beit", "ist", "es", "t\u00fcch\u00b7tig", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "tragen.", "tokens": ["tra\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Kann man also auch mit Recht von dem Elendthiere sagen,", "tokens": ["Kann", "man", "al\u00b7so", "auch", "mit", "Recht", "von", "dem", "E\u00b7lend\u00b7thie\u00b7re", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Da\u00df es einen weisen Sch\u00f6pfer uns erweis\u2019 und noch", "tokens": ["Da\u00df", "es", "ei\u00b7nen", "wei\u00b7sen", "Sch\u00f6p\u00b7fer", "uns", "er\u00b7weis'", "und", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "KON", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.27": {"text": "dabey,", "tokens": ["da\u00b7bey", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Da\u00df es blo\u00df von ihm aus Liebe uns zum Nutz erschaffen", "tokens": ["Da\u00df", "es", "blo\u00df", "von", "ihm", "aus", "Lie\u00b7be", "uns", "zum", "Nutz", "er\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "sey.", "tokens": ["sey", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}}}}}