{"textgrid.poem.35233": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der graue Winter hat bereit", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der graue Winter hat bereit", "tokens": ["Der", "grau\u00b7e", "Win\u00b7ter", "hat", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rauhem Frost und Traurigkeit", "tokens": ["Mit", "rau\u00b7hem", "Frost", "und", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Felder \u00fcberdecket,", "tokens": ["Die", "Fel\u00b7der", "\u00fc\u00b7berd\u00b7e\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So die begr\u00fcnte Fr\u00fchlingszeit", "tokens": ["So", "die", "be\u00b7gr\u00fcn\u00b7te", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erfreulich auferwecket.", "tokens": ["Er\u00b7freu\u00b7lich", "au\u00b7fer\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fluthen sind nun eisenhart,", "tokens": ["Die", "Flut\u00b7hen", "sind", "nun", "ei\u00b7sen\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wasser ist fast harnischart,", "tokens": ["Das", "Was\u00b7ser", "ist", "fast", "har\u00b7ni\u00b7schart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Wollenschnee erweichet,", "tokens": ["Mit", "Wol\u00b7len\u00b7schnee", "er\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde mit der Ruhe bahrt,", "tokens": ["Die", "Er\u00b7de", "mit", "der", "Ru\u00b7he", "bahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis sich die Sonn' erzeiget.", "tokens": ["Bis", "sich", "die", "Sonn'", "er\u00b7zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann unsre Herzen sind erstarrt", "tokens": ["Wann", "uns\u00b7re", "Her\u00b7zen", "sind", "er\u00b7starrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und von der S\u00fcnde marmorhart,", "tokens": ["Und", "von", "der", "S\u00fcn\u00b7de", "mar\u00b7mor\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann sie das Kreuz erweichen.", "tokens": ["Kann", "sie", "das", "Kreuz", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des H\u00f6chsten Gnad' ist sonnenart,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Gnad'", "ist", "son\u00b7nen\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn wir sie nur erreichen.", "tokens": ["Wenn", "wir", "sie", "nur", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der kurze Tag, die lange Nacht", "tokens": ["Der", "kur\u00b7ze", "Tag", ",", "die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Manchen viel Verdru\u00df gebracht", "tokens": ["Hat", "Man\u00b7chen", "viel", "Ver\u00b7dru\u00df", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In S\u00fcnd und Lasterleben.", "tokens": ["In", "S\u00fcnd", "und", "Las\u00b7ter\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer hat an seine Seel' gedacht,", "tokens": ["Wer", "hat", "an", "sei\u00b7ne", "Seel'", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mu\u00df in N\u00f6then schweben?", "tokens": ["Die", "mu\u00df", "in", "N\u00f6\u00b7then", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gerechter Gott in Ewigkeit,", "tokens": ["Ge\u00b7rech\u00b7ter", "Gott", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Du verwandelst Jahr und Zeit,", "tokens": ["Der", "Du", "ver\u00b7wan\u00b7delst", "Jahr", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib' nun bei uns in Gnaden.", "tokens": ["Bleib'", "nun", "bei", "uns", "in", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Sonne der Gerechtigkeit,", "tokens": ["Du", "Son\u00b7ne", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00fctz' uns vor allem Schaden!", "tokens": ["Sch\u00fctz'", "uns", "vor", "al\u00b7lem", "Scha\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}