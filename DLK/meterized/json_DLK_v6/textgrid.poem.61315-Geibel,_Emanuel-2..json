{"textgrid.poem.61315": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngst genug im weiten Raume", "tokens": ["L\u00e4ngst", "ge\u00b7nug", "im", "wei\u00b7ten", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweift' ich um mit d\u00fcrrem Gaume,", "tokens": ["Schweift'", "ich", "um", "mit", "d\u00fcr\u00b7rem", "Gau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rastlos nach dem Gl\u00fccke sucht' ich,", "tokens": ["Rast\u00b7los", "nach", "dem", "Gl\u00fc\u00b7cke", "sucht'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ergriff ich's nicht am Saume.", "tokens": ["Doch", "er\u00b7griff", "ich's", "nicht", "am", "Sau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum halt' ich ruhig l\u00e4chelnd", "tokens": ["Da\u00b7rum", "halt'", "ich", "ru\u00b7hig", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Sehnsucht jetzt im Zaume,", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", "jetzt", "im", "Zau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gelagert, wo der Eppich", "tokens": ["Und", "ge\u00b7la\u00b7gert", ",", "wo", "der", "Ep\u00b7pich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Rankt empor am Rosenbaume,", "tokens": ["Rankt", "em\u00b7por", "am", "Ro\u00b7sen\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sing' ich holder Torheit Weise", "tokens": ["Sing'", "ich", "hol\u00b7der", "Tor\u00b7heit", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bei des Weines Perlenschaume:", "tokens": ["Bei", "des", "Wei\u00b7nes", "Per\u00b7len\u00b7schau\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sucht und forscht nicht, ihr entkleidet", "tokens": ["Sucht", "und", "forscht", "nicht", ",", "ihr", "ent\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nur die Frucht vom duft'gen Flaume;", "tokens": ["Nur", "die", "Frucht", "vom", "duft'\u00b7gen", "Flau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Unerbeten von den G\u00f6ttern", "tokens": ["Un\u00b7er\u00b7be\u00b7ten", "von", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Kommt das H\u00f6chste wie im Traume.", "tokens": ["Kommt", "das", "H\u00f6chs\u00b7te", "wie", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}