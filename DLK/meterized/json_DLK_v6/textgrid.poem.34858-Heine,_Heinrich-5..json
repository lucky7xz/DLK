{"textgrid.poem.34858": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00e4lte kann wahrlich brennen", "tokens": ["Die", "K\u00e4l\u00b7te", "kann", "wahr\u00b7lich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie Feuer. Die Menschenkinder", "tokens": ["Wie", "Feu\u00b7er", ".", "Die", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$.", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Schneegest\u00f6ber rennen", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "ren\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und laufen immer geschwinder.", "tokens": ["Und", "lau\u00b7fen", "im\u00b7mer", "ge\u00b7schwin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oh, bittre Winterh\u00e4rte!", "tokens": ["Oh", ",", "bitt\u00b7re", "Win\u00b7ter\u00b7h\u00e4r\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nasen sind erfroren,", "tokens": ["Die", "Na\u00b7sen", "sind", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die Klavierkonzerte", "tokens": ["Und", "die", "Kla\u00b7vier\u00b7kon\u00b7zer\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zerrei\u00dfen uns die Ohren.", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "uns", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weit besser ist es im Summer,", "tokens": ["Weit", "bes\u00b7ser", "ist", "es", "im", "Sum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da kann ich im Walde spazieren,", "tokens": ["Da", "kann", "ich", "im", "Wal\u00b7de", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Allein mit meinem Kummer,", "tokens": ["Al\u00b7lein", "mit", "mei\u00b7nem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Liebeslieder skandieren.", "tokens": ["Und", "Lie\u00b7bes\u00b7lie\u00b7der", "skan\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}