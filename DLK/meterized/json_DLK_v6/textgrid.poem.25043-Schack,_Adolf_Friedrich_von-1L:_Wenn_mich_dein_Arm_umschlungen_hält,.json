{"textgrid.poem.25043": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mich dein Arm umschlungen h\u00e4lt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mich dein Arm umschlungen h\u00e4lt,", "tokens": ["Wenn", "mich", "dein", "Arm", "um\u00b7schlun\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deinen meine Lippen h\u00e4ngen,", "tokens": ["An", "dei\u00b7nen", "mei\u00b7ne", "Lip\u00b7pen", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dringt fernher nur der L\u00e4rm der Welt", "tokens": ["Dringt", "fern\u00b7her", "nur", "der", "L\u00e4rm", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Noch an mein Ohr mit matten Kl\u00e4ngen.", "tokens": ["Noch", "an", "mein", "Ohr", "mit", "mat\u00b7ten", "Kl\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herab aus deinen Augen taut", "tokens": ["Her\u00b7ab", "aus", "dei\u00b7nen", "Au\u00b7gen", "taut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Glanz, den meine kaum ertragen,", "tokens": ["Ein", "Glanz", ",", "den", "mei\u00b7ne", "kaum", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tiefklar, wie wenn der Himmel blaut", "tokens": ["Tief\u00b7klar", ",", "wie", "wenn", "der", "Him\u00b7mel", "blaut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An wolkenlosen Junitagen.", "tokens": ["An", "wol\u00b7ken\u00b7lo\u00b7sen", "Ju\u00b7ni\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wimpern senk' ich vor dem Licht;", "tokens": ["Die", "Wim\u00b7pern", "senk'", "ich", "vor", "dem", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erst nach und nach in ganzer F\u00fclle,", "tokens": ["Erst", "nach", "und", "nach", "in", "gan\u00b7zer", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie es kein Erdenschatten bricht,", "tokens": ["Wie", "es", "kein", "Er\u00b7den\u00b7schat\u00b7ten", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann ich es schauen, ohne H\u00fclle.", "tokens": ["Kann", "ich", "es", "schau\u00b7en", ",", "oh\u00b7ne", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch zweifelnd frag' ich: mu\u00df mein Blick", "tokens": ["Doch", "zwei\u00b7felnd", "frag'", "ich", ":", "mu\u00df", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht f\u00fcr die niedre Welt erblinden?", "tokens": ["Nicht", "f\u00fcr", "die", "nied\u00b7re", "Welt", "er\u00b7blin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O werd' ich noch den Pfad zur\u00fcck", "tokens": ["O", "werd'", "ich", "noch", "den", "Pfad", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In das verlass'ne Leben finden?", "tokens": ["In", "das", "ver\u00b7lass'\u00b7ne", "Le\u00b7ben", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}