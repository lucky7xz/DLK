{"textgrid.poem.26816": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Eichwald brauset,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eichwald brauset,", "tokens": ["Der", "Eich\u00b7wald", "brau\u00b7set", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Wolken ziehn,", "tokens": ["Die", "Wol\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das M\u00e4gdlein sitzet", "tokens": ["Das", "M\u00e4gd\u00b7lein", "sit\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "An Ufers Gr\u00fcn,", "tokens": ["An", "U\u00b7fers", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es bricht sich die Welle mit Macht, mit Macht,", "tokens": ["Es", "bricht", "sich", "die", "Wel\u00b7le", "mit", "Macht", ",", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und sie seufzt hinaus in die finstre Nacht,", "tokens": ["Und", "sie", "seufzt", "hin\u00b7aus", "in", "die", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das Auge vom Weinen getr\u00fcbet.", "tokens": ["Das", "Au\u00b7ge", "vom", "Wei\u00b7nen", "ge\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbdas Herz ist gestorben,", "tokens": ["\u00bb", "das", "Herz", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Welt ist leer,", "tokens": ["Die", "Welt", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und weiter gibt sie", "tokens": ["Und", "wei\u00b7ter", "gibt", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Wunsche nichts mehr.", "tokens": ["Dem", "Wun\u00b7sche", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du Heilige, rufe dein Kind zur\u00fcck,", "tokens": ["Du", "Hei\u00b7li\u00b7ge", ",", "ru\u00b7fe", "dein", "Kind", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich habe genossen das irdische Gl\u00fcck,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7nos\u00b7sen", "das", "ir\u00b7di\u00b7sche", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich habe gelebt und geliebet!\u00ab", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7lebt", "und", "ge\u00b7lie\u00b7bet", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Es rinnet der Tr\u00e4nen", "tokens": ["Es", "rin\u00b7net", "der", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vergeblicher Lauf,", "tokens": ["Ver\u00b7geb\u00b7li\u00b7cher", "Lauf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Klage, sie wecket", "tokens": ["Die", "Kla\u00b7ge", ",", "sie", "we\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Toten nicht auf,", "tokens": ["Die", "To\u00b7ten", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Doch nenne, was tr\u00f6stet und heilet die Brust", "tokens": ["Doch", "nen\u00b7ne", ",", "was", "tr\u00f6s\u00b7tet", "und", "hei\u00b7let", "die", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nach der s\u00fc\u00dfen Liebe verschwundener Lust,", "tokens": ["Nach", "der", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be", "ver\u00b7schwun\u00b7de\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ich, die himmlische, wills nicht versagen.", "tokens": ["Ich", ",", "die", "himm\u00b7li\u00b7sche", ",", "wills", "nicht", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "$,", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbla\u00df rinnen der Tr\u00e4nen", "tokens": ["\u00bb", "la\u00df", "rin\u00b7nen", "der", "Tr\u00e4\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vergeblichen Lauf,", "tokens": ["Ver\u00b7geb\u00b7li\u00b7chen", "Lauf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es wecke die Klage", "tokens": ["Es", "we\u00b7cke", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Toten nicht auf,", "tokens": ["Den", "To\u00b7ten", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das s\u00fc\u00dfeste Gl\u00fcck f\u00fcr die traurende Brust,", "tokens": ["Das", "s\u00fc\u00b7\u00dfes\u00b7te", "Gl\u00fcck", "f\u00fcr", "die", "trau\u00b7ren\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nach der sch\u00f6nen Liebe verschwundener Lust,", "tokens": ["Nach", "der", "sch\u00f6\u00b7nen", "Lie\u00b7be", "ver\u00b7schwun\u00b7de\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sind der Liebe Schmerzen und Klagen.\u00ab", "tokens": ["Sind", "der", "Lie\u00b7be", "Schmer\u00b7zen", "und", "Kla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}