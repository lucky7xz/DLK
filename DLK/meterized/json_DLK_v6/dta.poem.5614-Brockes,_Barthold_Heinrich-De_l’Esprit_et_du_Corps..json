{"dta.poem.5614": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De l\u2019Esprit et du Corps.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein C\u00f6rper mu\u00df den andern ja bewegen", "tokens": ["Ein", "C\u00f6r\u00b7per", "mu\u00df", "den", "an\u00b7dern", "ja", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ihm entgegen kommt, durch den mu\u00df der sich regen,", "tokens": ["Der", "ihm", "ent\u00b7ge\u00b7gen", "kommt", ",", "durch", "den", "mu\u00df", "der", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "VVFIN", "$,", "APPR", "ART", "VMFIN", "ART", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und also immerfort. Vernunfft wird solches zeigen,", "tokens": ["Und", "al\u00b7so", "im\u00b7mer\u00b7fort", ".", "Ver\u00b7nunfft", "wird", "sol\u00b7ches", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df die Bewegung nicht fey einem C\u00f6rper eigen.", "tokens": ["Da\u00df", "die", "Be\u00b7we\u00b7gung", "nicht", "fey", "ei\u00b7nem", "C\u00f6r\u00b7per", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn die Bewegung nun entstehen kan,", "tokens": ["Wenn", "die", "Be\u00b7we\u00b7gung", "nun", "ent\u00b7ste\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So zeigt sie einen Geist unwidersprechlich an,", "tokens": ["So", "zeigt", "sie", "ei\u00b7nen", "Geist", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der alle Dinge reg\u2019t,", "tokens": ["Der", "al\u00b7le", "Din\u00b7ge", "reg't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und sie zuerst beweg\u2019t,", "tokens": ["Und", "sie", "zu\u00b7erst", "be\u00b7weg't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Man kann zu ihn hinauf durch diese Stuffen steigen,", "tokens": ["Man", "kann", "zu", "ihn", "hin\u00b7auf", "durch", "die\u00b7se", "Stuf\u00b7fen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Sinnen werden ihn, auch die Vernunfft, uns zeigen.", "tokens": ["Die", "Sin\u00b7nen", "wer\u00b7den", "ihn", ",", "auch", "die", "Ver\u00b7nunfft", ",", "uns", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}