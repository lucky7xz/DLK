{"textgrid.poem.46752": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[trost f\u00fcr Winterfrost]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trost f\u00fcr Winterfrost,", "tokens": ["Trost", "f\u00fcr", "Win\u00b7ter\u00b7frost", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mai, bringst du herbei;", "tokens": ["Mai", ",", "bringst", "du", "her\u00b7bei", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Roth l\u00e4\u00dft dein Gebot", "tokens": ["Roth", "l\u00e4\u00dft", "dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bl\u00fchn die Blum aus Gr\u00fcn;", "tokens": ["Bl\u00fchn", "die", "Blum", "aus", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und durch Vogelmund", "tokens": ["Und", "durch", "Vo\u00b7gel\u00b7mund"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "Thust du jeder Brust", "tokens": ["Thust", "du", "je\u00b7der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Lust und Freude kund.", "tokens": ["Lust", "und", "Freu\u00b7de", "kund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch mein Sinn steht noch", "tokens": ["Doch", "mein", "Sinn", "steht", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht auf Freudenlicht,", "tokens": ["Nicht", "auf", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ganz ist Blumenglanz", "tokens": ["Ganz", "ist", "Blu\u00b7men\u00b7glanz"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hier zuwider mir;", "tokens": ["Hier", "zu\u00b7wi\u00b7der", "mir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Pein auch mir allein", "tokens": ["Pein", "auch", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Drang durch Vogelsang", "tokens": ["Drang", "durch", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bang ins Herz mir ein.", "tokens": ["Bang", "ins", "Herz", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wann vom Winterbann", "tokens": ["Wann", "vom", "Win\u00b7ter\u00b7bann"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Frei mein Herz, o Mai,", "tokens": ["Frei", "mein", "Herz", ",", "o", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Soll und freudenvoll", "tokens": ["Soll", "und", "freu\u00b7den\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sein bei deinem Schein;", "tokens": ["Sein", "bei", "dei\u00b7nem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Richt' hieher dein Licht,", "tokens": ["Richt'", "hie\u00b7her", "dein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mach, was schl\u00e4ft, hier wach!", "tokens": ["Mach", ",", "was", "schl\u00e4ft", ",", "hier", "wach", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ach, das kannst du nicht.", "tokens": ["Ach", ",", "das", "kannst", "du", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Geh' nur hin und weh'", "tokens": ["Geh'", "nur", "hin", "und", "weh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Um dies Heiligthum,", "tokens": ["Um", "dies", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Luft, um diese Gruft!", "tokens": ["Luft", ",", "um", "die\u00b7se", "Gruft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hall', o Vogelschall,", "tokens": ["Hall'", ",", "o", "Vo\u00b7gel\u00b7schall", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Lind im Fr\u00fchlingswind!", "tokens": ["Lind", "im", "Fr\u00fch\u00b7lings\u00b7wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Rief ich doch, und schlief", "tokens": ["Rief", "ich", "doch", ",", "und", "schlief"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Tief und fest mein Kind.", "tokens": ["Tief", "und", "fest", "mein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Rief ich und es schlief", "tokens": ["Rief", "ich", "und", "es", "schlief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Taub vor mir im Staub,", "tokens": ["Taub", "vor", "mir", "im", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wind, so weckt geschwind", "tokens": ["Wind", ",", "so", "weckt", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auch es nicht dein Hauch.", "tokens": ["Auch", "es", "nicht", "dein", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Still und leise schwill',", "tokens": ["Still", "und", "lei\u00b7se", "schwill'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nah' nicht st\u00fcrmisch ja,", "tokens": ["Nah'", "nicht", "st\u00fcr\u00b7misch", "ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da es schlafen will!", "tokens": ["Da", "es", "schla\u00b7fen", "will", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}