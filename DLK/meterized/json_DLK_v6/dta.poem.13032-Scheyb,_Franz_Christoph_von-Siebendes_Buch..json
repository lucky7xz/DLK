{"dta.poem.13032": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inzwischen nahte sich ein Ehren-werther Mann,", "tokens": ["I\u00b7nzwi\u00b7schen", "nah\u00b7te", "sich", "ein", "Eh\u00b7ren\u00b7wer\u00b7ther", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "210Der Zweifel, der den Saal verlie\u00df, war sein Gespan.", "tokens": ["Zwei\u00b7fel", ",", "der", "den", "Saal", "ver\u00b7lie\u00df", ",", "war", "sein", "Ge\u00b7span", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Er kam mit Langsamkeit und mit bedachtem Schritte;", "tokens": ["Er", "kam", "mit", "Lang\u00b7sam\u00b7keit", "und", "mit", "be\u00b7dach\u00b7tem", "Schrit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kaum war er bey dem Krei\u00df, auch fast in dessen Mitte,", "tokens": ["Kaum", "war", "er", "bey", "dem", "Krei\u00df", ",", "auch", "fast", "in", "des\u00b7sen", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ward des ganzen Saals Verwunderung erweckt,", "tokens": ["So", "ward", "des", "gan\u00b7zen", "Saals", "Ver\u00b7wun\u00b7de\u00b7rung", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann keine Tugend wu\u00dft was in der Ankunft steckt.", "tokens": ["Dann", "kei\u00b7ne", "Tu\u00b7gend", "wu\u00dft", "was", "in", "der", "An\u00b7kunft", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "215Er neigte sich und gieng zum Thron, blieb an den Treppen;", "tokens": ["neig\u00b7te", "sich", "und", "gieng", "zum", "Thron", ",", "blieb", "an", "den", "Trep\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Jm Gehn schwung er das Kleid, um es nicht nachzuschleppen,", "tokens": ["Jm", "Gehn", "schwung", "er", "das", "Kleid", ",", "um", "es", "nicht", "nach\u00b7zu\u00b7schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "KOUI", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bequemlich um den Leib. Es hieng vor seiner Brust", "tokens": ["Be\u00b7quem\u00b7lich", "um", "den", "Leib", ".", "Es", "hieng", "vor", "sei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein schimmernd goldnes Herz; so war uns fast bewu\u00dft,", "tokens": ["Ein", "schim\u00b7mernd", "gold\u00b7nes", "Herz", ";", "so", "war", "uns", "fast", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer dieser Alte sey. Er wies gelassne G\u00fcte;", "tokens": ["Wer", "die\u00b7ser", "Al\u00b7te", "sey", ".", "Er", "wies", "ge\u00b7lass\u00b7ne", "G\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "220Der sittsame Betrag entdeckte sein Gem\u00fcthe.", "tokens": ["sitt\u00b7sa\u00b7me", "Be\u00b7trag", "ent\u00b7deck\u00b7te", "sein", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}