{"dta.poem.21492": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der Seele Tod.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein seltsam Weben und Athmen durch die Nacht,", "tokens": ["Es", "geht", "ein", "selt\u00b7sam", "We\u00b7ben", "und", "Ath\u00b7men", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Seufzer der Sehnsucht beben in deinem Ohre sacht.", "tokens": ["Seuf\u00b7zer", "der", "Sehn\u00b7sucht", "be\u00b7ben", "in", "dei\u00b7nem", "Oh\u00b7re", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+++-+-+-+-+", "measure": "unknown.measure.octa.plus"}}, "stanza.2": {"line.1": {"text": "Die Winde gleiten k\u00fchler hinab den dunklen Weg,", "tokens": ["Die", "Win\u00b7de", "glei\u00b7ten", "k\u00fch\u00b7ler", "hin\u00b7ab", "den", "dunk\u00b7len", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und leise Stimmen fl\u00fcstern am bl\u00fchenden Geheg.", "tokens": ["Und", "lei\u00b7se", "Stim\u00b7men", "fl\u00fcs\u00b7tern", "am", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7heg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und in den fernen Wolken im Osten blitzt es auf,", "tokens": ["Und", "in", "den", "fer\u00b7nen", "Wol\u00b7ken", "im", "Os\u00b7ten", "blitzt", "es", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und von der Erde hebt sich ein sanfter Glanz hinauf.", "tokens": ["Und", "von", "der", "Er\u00b7de", "hebt", "sich", "ein", "sanf\u00b7ter", "Glanz", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es quillt wie Licht und Leben aus dunklem Schoo\u00df hervor,", "tokens": ["Es", "quillt", "wie", "Licht", "und", "Le\u00b7ben", "aus", "dunk\u00b7lem", "Schoo\u00df", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es ringen sich Gestalten aus Nacht und Tod empor.", "tokens": ["Es", "rin\u00b7gen", "sich", "Ge\u00b7stal\u00b7ten", "aus", "Nacht", "und", "Tod", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}