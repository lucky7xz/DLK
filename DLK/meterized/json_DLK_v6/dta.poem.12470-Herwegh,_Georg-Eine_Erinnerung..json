{"dta.poem.12470": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eine Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Polens letzte Schlacht verloren,", "tokens": ["Als", "Po\u00b7lens", "letz\u00b7te", "Schlacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ging's hinunter an den Rhein,", "tokens": ["Da", "ging's", "hin\u00b7un\u00b7ter", "an", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf den Bergen ward geschworen:", "tokens": ["Und", "auf", "den", "Ber\u00b7gen", "ward", "ge\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewir wollen freie M\u00e4nner sein!\u201c", "tokens": ["\u201e", "wir", "wol\u00b7len", "frei\u00b7e", "M\u00e4n\u00b7ner", "sein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und tief im Thal h\u00f6rt man's gewittern,", "tokens": ["Und", "tief", "im", "Thal", "h\u00f6rt", "man's", "ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und durch die Lande fliegt ein Wort,", "tokens": ["Und", "durch", "die", "Lan\u00b7de", "fliegt", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df freudig alle Herzen zittern \u2014", "tokens": ["Da\u00df", "freu\u00b7dig", "al\u00b7le", "Her\u00b7zen", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein b\u00f6ser Traum! und jenen Rittern", "tokens": ["Ein", "b\u00f6\u00b7ser", "Traum", "!", "und", "je\u00b7nen", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist hinter sieben Eisengittern", "tokens": ["Ist", "hin\u00b7ter", "sie\u00b7ben", "Ei\u00b7sen\u00b7git\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Jugend Bl\u00fcte schnell verdorrt.", "tokens": ["Der", "Ju\u00b7gend", "Bl\u00fc\u00b7te", "schnell", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl viel hat uns der Tod genommen,", "tokens": ["Wohl", "viel", "hat", "uns", "der", "Tod", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr noch das Leben uns geraubt;", "tokens": ["Mehr", "noch", "das", "Le\u00b7ben", "uns", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch drum, ihr Br\u00fcder, unbeklommen,", "tokens": ["Doch", "drum", ",", "ihr", "Br\u00fc\u00b7der", ",", "un\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "$,", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch tr\u00e4gt die Freiheit stolz ihr Haubt!", "tokens": ["Noch", "tr\u00e4gt", "die", "Frei\u00b7heit", "stolz", "ihr", "Haubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns blieb ihr Bild \u2014 was liegt am Rahmen?", "tokens": ["Uns", "blieb", "ihr", "Bild", "was", "liegt", "am", "Rah\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wen wird das schlechte Holz gereu'n?", "tokens": ["Wen", "wird", "das", "schlech\u00b7te", "Holz", "ge\u00b7reu'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft sie vergehn, die gro\u00dfen Namen!", "tokens": ["La\u00dft", "sie", "ver\u00b7gehn", ",", "die", "gro\u00b7\u00dfen", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie werden kommen, wie sie kamen,", "tokens": ["Sie", "wer\u00b7den", "kom\u00b7men", ",", "wie", "sie", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und neue Helden, neuen Samen", "tokens": ["Und", "neu\u00b7e", "Hel\u00b7den", ",", "neu\u00b7en", "Sa\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In unsrer Todten Asche streu'n.", "tokens": ["In", "uns\u00b7rer", "Tod\u00b7ten", "A\u00b7sche", "streu'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch giebt's ja Prediger vom Berge,", "tokens": ["Noch", "giebt's", "ja", "Pre\u00b7di\u00b7ger", "vom", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr die man schon die Dornen flicht,", "tokens": ["F\u00fcr", "die", "man", "schon", "die", "Dor\u00b7nen", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch freilich! die\u00df Geschlecht der Zwerge", "tokens": ["Doch", "frei\u00b7lich", "!", "die\u00df", "Ge\u00b7schlecht", "der", "Zwer\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PDS", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verstehet ihre Spr\u00fcche nicht;", "tokens": ["Ver\u00b7ste\u00b7het", "ih\u00b7re", "Spr\u00fc\u00b7che", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die tief im Witz begraben liegen,", "tokens": ["Die", "tief", "im", "Witz", "be\u00b7gra\u00b7ben", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die hohen Herrn verstummen hier \u2014", "tokens": ["Die", "ho\u00b7hen", "Herrn", "ver\u00b7stum\u00b7men", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein B\u00fccken gilts mehr und kein Biegen,", "tokens": ["Kein", "B\u00fc\u00b7cken", "gilts", "mehr", "und", "kein", "Bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freiheit ruft schon an den Wiegen:", "tokens": ["Die", "Frei\u00b7heit", "ruft", "schon", "an", "den", "Wie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u201ein meinem Zeichen m\u00fc\u00dft ihr siegen!\u201c", "tokens": ["\u201e", "in", "mei\u00b7nem", "Zei\u00b7chen", "m\u00fc\u00dft", "ihr", "sie\u00b7gen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In ihrem Zeichen siegen wir.", "tokens": ["In", "ih\u00b7rem", "Zei\u00b7chen", "sie\u00b7gen", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie Zeus durch den Olympus schreitet", "tokens": ["Wie", "Zeus", "durch", "den", "O\u00b7lym\u00b7pus", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Donnern, naht der gro\u00dfe Tag:", "tokens": ["Mit", "Don\u00b7nern", ",", "naht", "der", "gro\u00b7\u00dfe", "Tag", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob aller Welt wird er verbreitet,", "tokens": ["Ob", "al\u00b7ler", "Welt", "wird", "er", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alle Welt sich freuen mag.", "tokens": ["Da\u00df", "al\u00b7le", "Welt", "sich", "freu\u00b7en", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PIAT", "NN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Sehnen ward das Wort verliehen,", "tokens": ["Dem", "Seh\u00b7nen", "ward", "das", "Wort", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Stern der Zeit fand seine Bahn;", "tokens": ["Der", "Stern", "der", "Zeit", "fand", "sei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Sturm geweihter Melodien", "tokens": ["Dem", "Sturm", "ge\u00b7weih\u00b7ter", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird auch der letzte Feind entfliehen,", "tokens": ["Wird", "auch", "der", "letz\u00b7te", "Feind", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und, der Verhei\u00dfung Schwalben, ziehen", "tokens": ["Und", ",", "der", "Ver\u00b7hei\u00b7\u00dfung", "Schwal\u00b7ben", ",", "zie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ART", "NN", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem V\u00f6lkerfr\u00fchling wir voran.", "tokens": ["Dem", "V\u00f6l\u00b7ker\u00b7fr\u00fch\u00b7ling", "wir", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Knechtschaft Baal wird zu Schanden,", "tokens": ["Der", "Knecht\u00b7schaft", "Baal", "wird", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Blinde wei\u00df nicht was er thut:", "tokens": ["Der", "Blin\u00b7de", "wei\u00df", "nicht", "was", "er", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schl\u00e4gt den s\u00fc\u00dfen Wein in Banden", "tokens": ["Er", "schl\u00e4gt", "den", "s\u00fc\u00b7\u00dfen", "Wein", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mehrt nur seines Feuers Glut.", "tokens": ["Und", "mehrt", "nur", "sei\u00b7nes", "Feu\u00b7ers", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seht ihn, der heut der Haft entsprungen,", "tokens": ["Seht", "ihn", ",", "der", "heut", "der", "Haft", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wirft er seiner Perlen Schaar!", "tokens": ["Wie", "wirft", "er", "sei\u00b7ner", "Per\u00b7len", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hurrah, ihr frischen, freien Zungen!", "tokens": ["Hur\u00b7rah", ",", "ihr", "fri\u00b7schen", ",", "frei\u00b7en", "Zun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hurrah, du Volk der Nibelungen,", "tokens": ["Hur\u00b7rah", ",", "du", "Volk", "der", "Ni\u00b7be\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bring' diesen alten Geist dem jungen,", "tokens": ["Bring'", "die\u00b7sen", "al\u00b7ten", "Geist", "dem", "jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem guten Geist zum Opfer dar!", "tokens": ["Dem", "gu\u00b7ten", "Geist", "zum", "Op\u00b7fer", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}