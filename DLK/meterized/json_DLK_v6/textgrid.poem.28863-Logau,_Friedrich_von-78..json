{"textgrid.poem.28863": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was weg ist, l\u00e4sset Reu;", "tokens": ["Was", "weg", "ist", ",", "l\u00e4s\u00b7set", "Reu", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was kummen soll, macht Scheu.", "tokens": ["Was", "kum\u00b7men", "soll", ",", "macht", "Scheu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jugend die zerran;", "tokens": ["Die", "Ju\u00b7gend", "die", "zer\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Alter dringt heran.", "tokens": ["Das", "Al\u00b7ter", "dringt", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum dencke man dahin,", "tokens": ["Drum", "den\u00b7cke", "man", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wo Jugend stets bleibt gr\u00fcn,", "tokens": ["Wo", "Ju\u00b7gend", "stets", "bleibt", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Alter immer steht,", "tokens": ["Wo", "Al\u00b7ter", "im\u00b7mer", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wo Leben nie vergeht.", "tokens": ["Wo", "Le\u00b7ben", "nie", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}