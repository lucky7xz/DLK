{"textgrid.poem.49465": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[des schwarzen Augenpaars Gewalt]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des schwarzen Augenpaars Gewalt,", "tokens": ["Des", "schwar\u00b7zen", "Au\u00b7gen\u00b7paars", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichg\u00fcltig-kalt,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig\u00b7kalt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis zu der H\u00fcften weissem Scheine,", "tokens": ["Bis", "zu", "der", "H\u00fcf\u00b7ten", "weis\u00b7sem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der stolzen Reine", "tokens": ["Der", "stol\u00b7zen", "Rei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des Busens, all der Sch\u00f6nheit Pracht,", "tokens": ["Des", "Bu\u00b7sens", ",", "all", "der", "Sch\u00f6n\u00b7heit", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die s\u00fcss mir lacht \u2013", "tokens": ["Die", "s\u00fcss", "mir", "lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach, alles flieht, ach, alles schied, was ich gedacht.", "tokens": ["Ach", ",", "al\u00b7les", "flieht", ",", "ach", ",", "al\u00b7les", "schied", ",", "was", "ich", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "$,", "ITJ", "$,", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du tiefe Falschheit so im Bunde", "tokens": ["Du", "tie\u00b7fe", "Falschheit", "so", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit bl\u00fch'ndem Munde,", "tokens": ["Mit", "bl\u00fch'n\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie du mich zu umgarnen denkst,", "tokens": ["Wie", "du", "mich", "zu", "um\u00b7gar\u00b7nen", "denkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkannt ich l\u00e4ngst,", "tokens": ["Er\u00b7kannt", "ich", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ach, alles, was wir reizend w\u00e4hnen,", "tokens": ["Ach", ",", "al\u00b7les", ",", "was", "wir", "rei\u00b7zend", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Wunsch und Sehnen,", "tokens": ["In", "Wunsch", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie es mir winkt, wie es mich zwingt zu k\u00fcnft'gen Tr\u00e4nen.", "tokens": ["Wie", "es", "mir", "winkt", ",", "wie", "es", "mich", "zwingt", "zu", "k\u00fcnft'\u00b7gen", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O sprich doch, Liebste, Fl\u00f6tenklang", "tokens": ["O", "sprich", "doch", ",", "Liebs\u00b7te", ",", "Fl\u00f6\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinem Sang,", "tokens": ["Zu", "mei\u00b7nem", "Sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie Schrein der Hirsche soll mein Singen", "tokens": ["Wie", "Schrein", "der", "Hir\u00b7sche", "soll", "mein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir sich ringen.", "tokens": ["Zu", "dir", "sich", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sprich, Liebste, dass die Fl\u00f6te bei", "tokens": ["Sprich", ",", "Liebs\u00b7te", ",", "dass", "die", "Fl\u00f6\u00b7te", "bei"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "KOUS", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Liede sei,", "tokens": ["Dem", "Lie\u00b7de", "sei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und wenn ich schon dein Esel bin, so gib mir Heu!", "tokens": ["Und", "wenn", "ich", "schon", "dein", "E\u00b7sel", "bin", ",", "so", "gib", "mir", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}