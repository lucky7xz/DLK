{"textgrid.poem.48640": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf die Itali\u00e4nische Weise: O fronte serena", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O liebliche Wangen,", "tokens": ["O", "lieb\u00b7li\u00b7che", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ihr macht mir Verlangen,", "tokens": ["ihr", "macht", "mir", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "di\u00df Rote, di\u00df Wei\u00dfe", "tokens": ["di\u00df", "Ro\u00b7te", ",", "di\u00df", "Wei\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "PDS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "zu schauen mit Flei\u00dfe!", "tokens": ["zu", "schau\u00b7en", "mit", "Flei\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und di\u00df nur alleine", "tokens": ["Und", "di\u00df", "nur", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "ists nicht, das ich meine", "tokens": ["ists", "nicht", ",", "das", "ich", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "zu schauen, zu gr\u00fc\u00dfen,", "tokens": ["zu", "schau\u00b7en", ",", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "zu r\u00fchren, zu k\u00fcssen,", "tokens": ["zu", "r\u00fch\u00b7ren", ",", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ihr macht mir Verlangen,", "tokens": ["Ihr", "macht", "mir", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "o liebliche Wangen!", "tokens": ["o", "lieb\u00b7li\u00b7che", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "O Sonne der Wonne,", "tokens": ["O", "Son\u00b7ne", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "o Wonne der Sonne!", "tokens": ["o", "Won\u00b7ne", "der", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O Augen, sie saugen", "tokens": ["O", "Au\u00b7gen", ",", "sie", "sau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "das Licht meiner Augen!", "tokens": ["das", "Licht", "mei\u00b7ner", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O englische Sinnen,", "tokens": ["O", "eng\u00b7li\u00b7sche", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "o himmlisch Beginnen,", "tokens": ["o", "himm\u00b7lisch", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "o Himmel auf Erden,", "tokens": ["o", "Him\u00b7mel", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "magst du mir nicht werden?", "tokens": ["magst", "du", "mir", "nicht", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "O Wonne der Sonne,", "tokens": ["O", "Won\u00b7ne", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "o Sonne der Wonne!", "tokens": ["o", "Son\u00b7ne", "der", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "O Sch\u00f6nste der Sch\u00f6nen,", "tokens": ["O", "Sch\u00f6ns\u00b7te", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "benimm mir di\u00df Sehnen!", "tokens": ["be\u00b7nimm", "mir", "di\u00df", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Komm, eile, komm, komme,", "tokens": ["Komm", ",", "ei\u00b7le", ",", "komm", ",", "kom\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "du S\u00fc\u00dfe, du Fromme!", "tokens": ["du", "S\u00fc\u00b7\u00dfe", ",", "du", "From\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ach, Schwester, ich sterbe,", "tokens": ["Ach", ",", "Schwes\u00b7ter", ",", "ich", "ster\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "ich sterb', ich verderbe.", "tokens": ["ich", "sterb'", ",", "ich", "ver\u00b7der\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Komm, komme, komm, eile,", "tokens": ["Komm", ",", "kom\u00b7me", ",", "komm", ",", "ei\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "komm, tr\u00f6ste, komm, heile!", "tokens": ["komm", ",", "tr\u00f6s\u00b7te", ",", "komm", ",", "hei\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Benimm mir di\u00df Sehnen,", "tokens": ["Be\u00b7nimm", "mir", "di\u00df", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "o Sch\u00f6nste der Sch\u00f6nen!", "tokens": ["o", "Sch\u00f6ns\u00b7te", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}