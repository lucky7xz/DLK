{"textgrid.poem.45350": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ihr habt den Lord Palmerston gest\u00fcrzt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt den Lord Palmerston gest\u00fcrzt,", "tokens": ["Ihr", "habt", "den", "Lord", "Pal\u00b7mer\u00b7ston", "ge\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch darum nicht an Einflu\u00df verk\u00fcrzt,", "tokens": ["Doch", "da\u00b7rum", "nicht", "an", "Ein\u00b7flu\u00df", "ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Stolz kann er wieder sein Haupt erheben,", "tokens": ["Stolz", "kann", "er", "wie\u00b7der", "sein", "Haupt", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da ihr ihm schlie\u00dflich recht gegeben.", "tokens": ["Da", "ihr", "ihm", "schlie\u00df\u00b7lich", "recht", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}