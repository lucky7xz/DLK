{"textgrid.poem.49195": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwartzen Augen ihr, und du auch schwartzes Haar,", "tokens": ["Ihr", "schwart\u00b7zen", "Au\u00b7gen", "ihr", ",", "und", "du", "auch", "schwart\u00b7zes", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frischen Flavien, die vor mein Hertze war,", "tokens": ["Der", "fri\u00b7schen", "Fla\u00b7vi\u00b7en", ",", "die", "vor", "mein", "Hert\u00b7ze", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff die ich pflag zu richten,", "tokens": ["Auff", "die", "ich", "pflag", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als ein Weiser soll,", "tokens": ["Mehr", "als", "ein", "Wei\u00b7ser", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Schreiben, Thun und Tichten,", "tokens": ["Mein", "Schrei\u00b7ben", ",", "Thun", "und", "Tich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gehabt euch jetzund wol.", "tokens": ["Ge\u00b7habt", "euch", "je\u00b7tzund", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht gerne sprech' ich so, ruff' auch zu Zeugen an", "tokens": ["Nicht", "ger\u00b7ne", "sprech'", "ich", "so", ",", "ruff'", "auch", "zu", "Zeu\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich, Venus, und dein Kind, da\u00df ich gewi\u00df hieran", "tokens": ["Dich", ",", "Ve\u00b7nus", ",", "und", "dein", "Kind", ",", "da\u00df", "ich", "ge\u00b7wi\u00df", "hie\u00b7ran"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die minste Schuldt nicht trage,", "tokens": ["Die", "mins\u00b7te", "Schuldt", "nicht", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja alles Kummers voll", "tokens": ["Ja", "al\u00b7les", "Kum\u00b7mers", "voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich st\u00fcndlich kr\u00e4nck' und plage,", "tokens": ["Mich", "st\u00fcnd\u00b7lich", "kr\u00e4nck", "und", "pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich sie lassen soll.", "tokens": ["Da\u00df", "ich", "sie", "las\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Parcen, die Ihr uns das Thun de\u00df Lebens spinnt,", "tokens": ["Ihr", "Par\u00b7cen", ",", "die", "Ihr", "uns", "das", "Thun", "de\u00df", "Le\u00b7bens", "spinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebt mir und ihr das, was ich ihr und sie mir g\u00f6nnt,", "tokens": ["Gebt", "mir", "und", "ihr", "das", ",", "was", "ich", "ihr", "und", "sie", "mir", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "PPER", "PDS", "$,", "PWS", "PPER", "PPER", "KON", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich's ja soll erf\u00fcllen,", "tokens": ["Weil", "ich's", "ja", "soll", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll z\u00e4hmen meinen Fu\u00df,", "tokens": ["Soll", "z\u00e4h\u00b7men", "mei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wieder Lust und Willen", "tokens": ["Und", "wie\u00b7der", "Lust", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch nachmals sagen mu\u00df:", "tokens": ["Auch", "nach\u00b7mals", "sa\u00b7gen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr schwartzen Augen ihr, und du auch schwartzes Haar,", "tokens": ["Ihr", "schwart\u00b7zen", "Au\u00b7gen", "ihr", ",", "und", "du", "auch", "schwart\u00b7zes", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frischen Flavien, die vor mein Hertze war,", "tokens": ["Der", "fri\u00b7schen", "Fla\u00b7vi\u00b7en", ",", "die", "vor", "mein", "Hert\u00b7ze", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff die ich pflag zu richten,", "tokens": ["Auff", "die", "ich", "pflag", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als ein Weiser soll,", "tokens": ["Mehr", "als", "ein", "Wei\u00b7ser", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5":