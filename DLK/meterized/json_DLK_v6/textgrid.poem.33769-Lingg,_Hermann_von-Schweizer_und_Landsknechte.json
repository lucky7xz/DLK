{"textgrid.poem.33769": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Schweizer und Landsknechte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Rheintal gegen die Schweizer hielt der Landsknechte Schar,", "tokens": ["Im", "Rhein\u00b7tal", "ge\u00b7gen", "die", "Schwei\u00b7zer", "hielt", "der", "Lands\u00b7knech\u00b7te", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Feindes fester Stellung nahmen sie ungern wahr;", "tokens": ["Des", "Fein\u00b7des", "fes\u00b7ter", "Stel\u00b7lung", "nah\u00b7men", "sie", "un\u00b7gern", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie sahn aus ihrem Lager mit Grimm die H\u00f6hn hinauf,", "tokens": ["Sie", "sahn", "aus", "ih\u00b7rem", "La\u00b7ger", "mit", "Grimm", "die", "H\u00f6hn", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stolzierend in roten W\u00e4msern, gestemmt die Faust am Knauf.", "tokens": ["Stol\u00b7zie\u00b7rend", "in", "ro\u00b7ten", "W\u00e4m\u00b7sern", ",", "ge\u00b7stemmt", "die", "Faust", "am", "Knauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie sangen, tanzten und trieben der argen Kurzweil viel,", "tokens": ["Sie", "san\u00b7gen", ",", "tanz\u00b7ten", "und", "trie\u00b7ben", "der", "ar\u00b7gen", "Kurz\u00b7weil", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der Krug lag bei der Trommel und drauf das W\u00fcrfelspiel.", "tokens": ["Der", "Krug", "lag", "bei", "der", "Trom\u00b7mel", "und", "drauf", "das", "W\u00fcr\u00b7fel\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "PAV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Sie kneiften in die Wangen und nahmen den Fingerring", "tokens": ["Sie", "kneif\u00b7ten", "in", "die", "Wan\u00b7gen", "und", "nah\u00b7men", "den", "Fin\u00b7ger\u00b7ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der jungen Dirne, die eilig vorbei am Lager ging.", "tokens": ["Der", "jun\u00b7gen", "Dir\u00b7ne", ",", "die", "ei\u00b7lig", "vor\u00b7bei", "am", "La\u00b7ger", "ging", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Vor Altmoos hielten ihrer Zweitausend an der Zahl,", "tokens": ["Vor", "Alt\u00b7moos", "hiel\u00b7ten", "ih\u00b7rer", "Zweit\u00b7au\u00b7send", "an", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die Eidgenossen sahen vom Berg herab ins Tal.", "tokens": ["Die", "Eid\u00b7ge\u00b7nos\u00b7sen", "sa\u00b7hen", "vom", "Berg", "her\u00b7ab", "ins", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Gescholten und vergolten ward mancher Schimpf und Hohn", "tokens": ["Ge\u00b7schol\u00b7ten", "und", "ver\u00b7gol\u00b7ten", "ward", "man\u00b7cher", "Schimpf", "und", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVPP", "VAFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und manchem kecken Trotzwort ein Schu\u00df daf\u00fcr zum Lohn.", "tokens": ["Und", "man\u00b7chem", "ke\u00b7cken", "Trotz\u00b7wort", "ein", "Schu\u00df", "da\u00b7f\u00fcr", "zum", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Da fiel es eines Tages den guten Gesellen ein,", "tokens": ["Da", "fiel", "es", "ei\u00b7nes", "Ta\u00b7ges", "den", "gu\u00b7ten", "Ge\u00b7sel\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Sie wollten zur Hochzeit laden die Hirten auf dem Stein,", "tokens": ["Sie", "woll\u00b7ten", "zur", "Hoch\u00b7zeit", "la\u00b7den", "die", "Hir\u00b7ten", "auf", "dem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sie hingen Brautgewande mit Flei\u00df um eine Kuh,", "tokens": ["Sie", "hin\u00b7gen", "Braut\u00b7ge\u00b7wan\u00b7de", "mit", "Flei\u00df", "um", "ei\u00b7ne", "Kuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sie banden ihr um die Klauen auch nette Schnabelschuh',", "tokens": ["Sie", "ban\u00b7den", "ihr", "um", "die", "Klau\u00b7en", "auch", "net\u00b7te", "Schna\u00b7bel\u00b7schuh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Sie wanden um die H\u00f6rner ihr einen dicken Kranz", "tokens": ["Sie", "wan\u00b7den", "um", "die", "H\u00f6r\u00b7ner", "ihr", "ei\u00b7nen", "di\u00b7cken", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und f\u00fchrten sie mit B\u00e4ndern hervor als wie zum Tanz.", "tokens": ["Und", "f\u00fchr\u00b7ten", "sie", "mit", "B\u00e4n\u00b7dern", "her\u00b7vor", "als", "wie", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "KOKOM", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Es stund die Kuh und br\u00fcllte zur\u00fcck nach ihrem Haus,", "tokens": ["Es", "stund", "die", "Kuh", "und", "br\u00fcll\u00b7te", "zu\u00b7r\u00fcck", "nach", "ih\u00b7rem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Da lachten sie und sagten, sie st\u00f6\u00dft schon Seufzer aus.", "tokens": ["Da", "lach\u00b7ten", "sie", "und", "sag\u00b7ten", ",", "sie", "st\u00f6\u00dft", "schon", "Seuf\u00b7zer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Nun wird wohl einer kommen und freien um die Maid,", "tokens": ["Nun", "wird", "wohl", "ei\u00b7ner", "kom\u00b7men", "und", "frei\u00b7en", "um", "die", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Hei, wie sie pr\u00e4chtig schreitet in ihrem Schweizerkleid!", "tokens": ["Hei", ",", "wie", "sie", "pr\u00e4ch\u00b7tig", "schrei\u00b7tet", "in", "ih\u00b7rem", "Schwei\u00b7zer\u00b7kleid", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Sie riefen zu den Hirten nach Altmoos in den Ort:", "tokens": ["Sie", "rie\u00b7fen", "zu", "den", "Hir\u00b7ten", "nach", "Alt\u00b7moos", "in", "den", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Kommt doch herab zur Hochzeit, da sagten die: Aufs Wort!", "tokens": ["Kommt", "doch", "her\u00b7ab", "zur", "Hoch\u00b7zeit", ",", "da", "sag\u00b7ten", "die", ":", "Aufs", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Sie packten ihre Kolben, der Spa\u00df verdro\u00df sie fast,", "tokens": ["Sie", "pack\u00b7ten", "ih\u00b7re", "Kol\u00b7ben", ",", "der", "Spa\u00df", "ver\u00b7dro\u00df", "sie", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und als es dunkel wurde, da kamen sie zu Gast.", "tokens": ["Und", "als", "es", "dun\u00b7kel", "wur\u00b7de", ",", "da", "ka\u00b7men", "sie", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Sie brachen wie ein Waldstrom herab mit Sturmgetos,", "tokens": ["Sie", "bra\u00b7chen", "wie", "ein", "Wald\u00b7strom", "her\u00b7ab", "mit", "Sturm\u00b7ge\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Sie st\u00fcrzten wie die Stiere auf ihre Feinde los.", "tokens": ["Sie", "st\u00fcrz\u00b7ten", "wie", "die", "Stie\u00b7re", "auf", "ih\u00b7re", "Fein\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Sie rannten an und nahmen den Lagerwall im Lauf,", "tokens": ["Sie", "rann\u00b7ten", "an", "und", "nah\u00b7men", "den", "La\u00b7ger\u00b7wall", "im", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Noch eh' in ihren Zelten die Landsknecht' sprangen auf.", "tokens": ["Noch", "eh'", "in", "ih\u00b7ren", "Zel\u00b7ten", "die", "Lands\u00b7knecht'", "spran\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Die schlugen wie die B\u00e4ren um sich in ihrer Not,", "tokens": ["Die", "schlu\u00b7gen", "wie", "die", "B\u00e4\u00b7ren", "um", "sich", "in", "ih\u00b7rer", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Doch war von hellem Brande das Lager schon entloht.", "tokens": ["Doch", "war", "von", "hel\u00b7lem", "Bran\u00b7de", "das", "La\u00b7ger", "schon", "ent\u00b7loht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Da sank manch wackrer Landsknecht hin in den roten Klee,", "tokens": ["Da", "sank", "manch", "wack\u00b7rer", "Lands\u00b7knecht", "hin", "in", "den", "ro\u00b7ten", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Und die geflohn, ertranken im Rhein und Bodensee.", "tokens": ["Und", "die", "ge\u00b7flohn", ",", "er\u00b7tran\u00b7ken", "im", "Rhein", "und", "Bo\u00b7den\u00b7see", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "APPRART", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Die Schweizer aber trieben das K\u00fchlein mit nach Haus", "tokens": ["Die", "Schwei\u00b7zer", "a\u00b7ber", "trie\u00b7ben", "das", "K\u00fch\u00b7lein", "mit", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Und sagten zu einander: Es war ein feiner Strau\u00df.", "tokens": ["Und", "sag\u00b7ten", "zu", "ein\u00b7an\u00b7der", ":", "Es", "war", "ein", "fei\u00b7ner", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$.", "PPER", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}