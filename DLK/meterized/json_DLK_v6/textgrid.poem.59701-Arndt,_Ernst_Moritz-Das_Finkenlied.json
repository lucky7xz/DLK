{"textgrid.poem.59701": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Finkenlied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen ein trauriges Finkenlied:", "tokens": ["Wir", "sin\u00b7gen", "ein", "trau\u00b7ri\u00b7ges", "Fin\u00b7ken\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der edle, freie Fink ist tot,", "tokens": ["Der", "ed\u00b7le", ",", "frei\u00b7e", "Fink", "ist", "tot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn weckt zu frohem Lustgesang", "tokens": ["Ihn", "weckt", "zu", "fro\u00b7hem", "Lust\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie mehr ein irdisch Morgenrot,", "tokens": ["Nie", "mehr", "ein", "ir\u00b7disch", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hat ein be\u00dfres Land erflogen,", "tokens": ["Er", "hat", "ein", "be\u00df\u00b7res", "Land", "er\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er schwimmt auf hellern Himmelswogen \u2013", "tokens": ["Er", "schwimmt", "auf", "hel\u00b7lern", "Him\u00b7mels\u00b7wo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ach, f\u00fcr uns der Fink ist tot.", "tokens": ["Doch", "ach", ",", "f\u00fcr", "uns", "der", "Fink", "ist", "tot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "PPER", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir singen ein fr\u00f6hliches Finkenlied,", "tokens": ["Wir", "sin\u00b7gen", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Fin\u00b7ken\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Lied aus voller, deutscher Brust,", "tokens": ["Ein", "Lied", "aus", "vol\u00b7ler", ",", "deut\u00b7scher", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn wir auch in Trauern gehn,", "tokens": ["Und", "wenn", "wir", "auch", "in", "Trau\u00b7ern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solche Trauer hat in Tr\u00e4nen Lust:", "tokens": ["Sol\u00b7che", "Trau\u00b7er", "hat", "in", "Tr\u00e4\u00b7nen", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Um Tapfre sind so s\u00fc\u00df die Schmerzen,", "tokens": ["Um", "Tapf\u00b7re", "sind", "so", "s\u00fc\u00df", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie heben himmelan die Herzen,", "tokens": ["Sie", "he\u00b7ben", "him\u00b7me\u00b7lan", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Himmelfluges sich bewu\u00dft.", "tokens": ["Des", "Him\u00b7mel\u00b7flu\u00b7ges", "sich", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir singen ein fr\u00f6hliches Finkenlied \u2013", "tokens": ["Wir", "sin\u00b7gen", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Fin\u00b7ken\u00b7lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie fr\u00f6hlich war des Finken Sang,", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "war", "des", "Fin\u00b7ken", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er den Dreiklang hellsten Tons", "tokens": ["Wenn", "er", "den", "Drei\u00b7klang", "hells\u00b7ten", "Tons"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlag in guten und b\u00f6sen Tagen,", "tokens": ["Den", "Schlag", "in", "gu\u00b7ten", "und", "b\u00f6\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den mu\u00dft er immer mutig schlagen,", "tokens": ["Den", "mu\u00dft", "er", "im\u00b7mer", "mu\u00b7tig", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der war des deutschen Finken Klang.", "tokens": ["Der", "war", "des", "deut\u00b7schen", "Fin\u00b7ken", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den klang er, als vom welschen Gei'r", "tokens": ["Den", "klang", "er", ",", "als", "vom", "wel\u00b7schen", "Gei'r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der deutsche Hain war stumm gemacht,", "tokens": ["Der", "deut\u00b7sche", "Hain", "war", "stumm", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den klang er frisch durch Berg und Tal;", "tokens": ["Den", "klang", "er", "frisch", "durch", "Berg", "und", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drob hieb der Gei'r ihm Bann und Acht", "tokens": ["Drob", "hieb", "der", "Gei'r", "ihm", "Bann", "und", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "KON", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rief: Wir wolln den deutschen Schn\u00e4beln", "tokens": ["Und", "rief", ":", "Wir", "wolln", "den", "deut\u00b7schen", "Schn\u00e4\u00b7beln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die unversch\u00e4mten Kehlen knebeln,", "tokens": ["Die", "un\u00b7ver\u00b7sch\u00e4m\u00b7ten", "Keh\u00b7len", "kne\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schweigt, Freche! Bebet unsrer Macht!", "tokens": ["Schweigt", ",", "Fre\u00b7che", "!", "Be\u00b7bet", "uns\u00b7rer", "Macht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So meint' und dr\u00e4ute welsche Wut,", "tokens": ["So", "meint'", "und", "dr\u00e4u\u00b7te", "wel\u00b7sche", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Gott im Himmel meint' es nicht,", "tokens": ["Doch", "Gott", "im", "Him\u00b7mel", "meint'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schlug mit sch\u00e4rfsten Blitzen drein,", "tokens": ["Er", "schlug", "mit", "sch\u00e4rfs\u00b7ten", "Blit\u00b7zen", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ward's in deutschen Hainen licht,", "tokens": ["Da", "ward's", "in", "deut\u00b7schen", "Hai\u00b7nen", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da bl\u00fchte deutscher Fr\u00fchling wieder,", "tokens": ["Da", "bl\u00fch\u00b7te", "deut\u00b7scher", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da klangen wieder deutsche Lieder,", "tokens": ["Da", "klan\u00b7gen", "wie\u00b7der", "deut\u00b7sche", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und fremde Schn\u00e4bel kr\u00e4chzten nicht.", "tokens": ["Und", "frem\u00b7de", "Schn\u00e4\u00b7bel", "kr\u00e4chz\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und o, der Adler an der Spree,", "tokens": ["Und", "o", ",", "der", "Ad\u00b7ler", "an", "der", "Spree", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$,", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, wo er thront in h\u00f6chster Horst,", "tokens": ["Da", ",", "wo", "er", "thront", "in", "h\u00f6chs\u00b7ter", "Horst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vernahm des tapfern Finken Schlag", "tokens": ["Ver\u00b7nahm", "des", "tap\u00b7fern", "Fin\u00b7ken", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: \u00bbDer h\u00fcte mir die Forst!", "tokens": ["Und", "sprach", ":", "\u00bb", "Der", "h\u00fc\u00b7te", "mir", "die", "Forst", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der tut mit unverzagtem Singen", "tokens": ["Der", "tut", "mit", "un\u00b7ver\u00b7zag\u00b7tem", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den wundersch\u00f6nen Dreiklang klingen,", "tokens": ["Den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Drei\u00b7klang", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der h\u00fcte mir die Westenforst!\u00ab", "tokens": ["Der", "h\u00fc\u00b7te", "mir", "die", "Wes\u00b7ten\u00b7forst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und siehe, auf des Aars Gebot", "tokens": ["Und", "sie\u00b7he", ",", "auf", "des", "Aars", "Ge\u00b7bot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Froh fliegt der treue Finke hin,", "tokens": ["Froh", "fliegt", "der", "treu\u00b7e", "Fin\u00b7ke", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Morgenrot die Brust gef\u00fcllt,", "tokens": ["Mit", "Mor\u00b7gen\u00b7rot", "die", "Brust", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesanges, Sieges freudig hin,", "tokens": ["Ge\u00b7san\u00b7ges", ",", "Sie\u00b7ges", "freu\u00b7dig", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit das Land der Roten Erde", "tokens": ["Da\u00b7mit", "das", "Land", "der", "Ro\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der jungen Wonne selig werde,", "tokens": ["Der", "jun\u00b7gen", "Won\u00b7ne", "se\u00b7lig", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Westenforst, da fliegt er hin.", "tokens": ["Zur", "Wes\u00b7ten\u00b7forst", ",", "da", "fliegt", "er", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort hat sein Dreiklang frisch und frei", "tokens": ["Dort", "hat", "sein", "Drei\u00b7klang", "frisch", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geklungen mehr als drei\u00dfig Jahr", "tokens": ["Ge\u00b7klun\u00b7gen", "mehr", "als", "drei\u00b7\u00dfig", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Feld und Berg und Tal voran \u2013", "tokens": ["In", "Feld", "und", "Berg", "und", "Tal", "vo\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wollt's der k\u00f6nigliche Aar.", "tokens": ["So", "wollt's", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wollten Uhu, Kauz und Eulen", "tokens": ["Und", "woll\u00b7ten", "U\u00b7hu", ",", "Kauz", "und", "Eu\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lied der Finsternis sich heulen,", "tokens": ["Das", "Lied", "der", "Fins\u00b7ter\u00b7nis", "sich", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er hielt den Ton der Kehlen klar.", "tokens": ["Er", "hielt", "den", "Ton", "der", "Keh\u00b7len", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So klang sein freies Lied voran", "tokens": ["So", "klang", "sein", "frei\u00b7es", "Lied", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit vollem, hellem, deutschem Klang,", "tokens": ["Mit", "vol\u00b7lem", ",", "hel\u00b7lem", ",", "deut\u00b7schem", "Klang", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df es die d\u00fcstre Vogelschar", "tokens": ["Da\u00df", "es", "die", "d\u00fcst\u00b7re", "Vo\u00b7gel\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Fliehen oder Schweigen zwang.", "tokens": ["Zum", "Flie\u00b7hen", "o\u00b7der", "Schwei\u00b7gen", "zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Amseln, Lerchen, Nachtigallen,", "tokens": ["Doch", "Am\u00b7seln", ",", "Ler\u00b7chen", ",", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die h\u00f6rt man doppelt lustig schallen,", "tokens": ["Die", "h\u00f6rt", "man", "dop\u00b7pelt", "lus\u00b7tig", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wann allen vor der Finke sang.", "tokens": ["Wann", "al\u00b7len", "vor", "der", "Fin\u00b7ke", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Drum singen wir fr\u00f6hlich das Finkenlied \u2013", "tokens": ["Drum", "sin\u00b7gen", "wir", "fr\u00f6h\u00b7lich", "das", "Fin\u00b7ken\u00b7lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "O gebe Gott dem deutschen Wald", "tokens": ["O", "ge\u00b7be", "Gott", "dem", "deut\u00b7schen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Stets solches Dreiklangs Freudenschall!", "tokens": ["Stets", "sol\u00b7ches", "Drei\u00b7klangs", "Freu\u00b7den\u00b7schall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bleibt das Gl\u00fcck uns wohlgestalt.", "tokens": ["So", "bleibt", "das", "Gl\u00fcck", "uns", "wohl\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Recht, Vaterland und Freiheit klingen", "tokens": ["Recht", ",", "Va\u00b7ter\u00b7land", "und", "Frei\u00b7heit", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bleibt bestes Ding von guten Dingen,", "tokens": ["Bleibt", "bes\u00b7tes", "Ding", "von", "gu\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wann's m\u00e4chtig durch die Seelen schallt.", "tokens": ["Wann's", "m\u00e4ch\u00b7tig", "durch", "die", "See\u00b7len", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}