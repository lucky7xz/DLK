{"textgrid.poem.38091": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcndenlast", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sterben zwei Br\u00fcder in einem Tag,", "tokens": ["Es", "ster\u00b7ben", "zwei", "Br\u00fc\u00b7der", "in", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein armer und ein reicher,", "tokens": ["Ein", "ar\u00b7mer", "und", "ein", "rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der reiche, der wird in die H\u00f6lle begraben,", "tokens": ["Der", "rei\u00b7che", ",", "der", "wird", "in", "die", "H\u00f6l\u00b7le", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der arme in den Himmel.", "tokens": ["Der", "ar\u00b7me", "in", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und da der Reiche begraben ward,", "tokens": ["Und", "da", "der", "Rei\u00b7che", "be\u00b7gra\u00b7ben", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sa\u00df er in gro\u00dfer Hitze,", "tokens": ["Sa\u00df", "er", "in", "gro\u00b7\u00dfer", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sah er seinen herzgeliebten Bruder,", "tokens": ["Sah", "er", "sei\u00b7nen", "herz\u00b7ge\u00b7lieb\u00b7ten", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der ewigen Freude sitzen.", "tokens": ["In", "der", "e\u00b7wi\u00b7gen", "Freu\u00b7de", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach Bruder, herzliebster Bruder mein,", "tokens": ["Ach", "Bru\u00b7der", ",", "herz\u00b7liebs\u00b7ter", "Bru\u00b7der", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Reich mir ein Tr\u00f6pflein Wasser,", "tokens": ["Reich", "mir", "ein", "Tr\u00f6pf\u00b7lein", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wohl auf meine Zunge, wohl auf meinen Mund,", "tokens": ["Wohl", "auf", "mei\u00b7ne", "Zun\u00b7ge", ",", "wohl", "auf", "mei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Das mich erquicken m\u00f6ge.", "tokens": ["Das", "mich", "er\u00b7qui\u00b7cken", "m\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Bruder, herzliebster Bruder mein,", "tokens": ["Ach", "Bru\u00b7der", ",", "herz\u00b7liebs\u00b7ter", "Bru\u00b7der", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Tr\u00f6pflein soll dir werden,", "tokens": ["Kein", "Tr\u00f6pf\u00b7lein", "soll", "dir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast den Armen das Brod versagt,", "tokens": ["Du", "hast", "den", "Ar\u00b7men", "das", "Brod", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hasts Hunden und Schweinen gegeben.", "tokens": ["Hasts", "Hun\u00b7den", "und", "Schwei\u00b7nen", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Hab ich den Armen das Brod versagt,", "tokens": ["Hab", "ich", "den", "Ar\u00b7men", "das", "Brod", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Habs Hunden und Schweinen gegeben,", "tokens": ["Habs", "Hun\u00b7den", "und", "Schwei\u00b7nen", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mein gro\u00dfes Gut trieb Uebermuth,", "tokens": ["Mein", "gro\u00b7\u00dfes", "Gut", "trieb", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann es nicht mit mir nehmen.", "tokens": ["Kann", "es", "nicht", "mit", "mir", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Wenn Berg und Thal aufeinander st\u00e4nd,", "tokens": ["Wenn", "Berg", "und", "Thal", "auf\u00b7ein\u00b7an\u00b7der", "st\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel lieber wollt ich sie tragen,", "tokens": ["Viel", "lie\u00b7ber", "wollt", "ich", "sie", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als da\u00df ich soll stehn vor dem j\u00fcngsten Gericht,", "tokens": ["Als", "da\u00df", "ich", "soll", "stehn", "vor", "dem", "j\u00fcng\u00b7sten", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Soll alle meine S\u00fcnden beklagen.", "tokens": ["Soll", "al\u00b7le", "mei\u00b7ne", "S\u00fcn\u00b7den", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und k\u00e4m alle Jahr ein V\u00f6gelein,", "tokens": ["Und", "k\u00e4m", "al\u00b7le", "Jahr", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und n\u00e4hm nur ein Schn\u00e4blein voll Erden,", "tokens": ["Und", "n\u00e4hm", "nur", "ein", "Schn\u00e4b\u00b7lein", "voll", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So wollt ich doch die Hoffnung haben,", "tokens": ["So", "wollt", "ich", "doch", "die", "Hoff\u00b7nung", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich k\u00f6nnt seelig werden.", "tokens": ["Da\u00df", "ich", "k\u00f6nnt", "see\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Amen, Amen, steht auch dabei,", "tokens": ["A\u00b7men", ",", "A\u00b7men", ",", "steht", "auch", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ADV", "PAV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gott helf uns allen zusammen,", "tokens": ["Gott", "helf", "uns", "al\u00b7len", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wohl hier und dort aus aller Noth,", "tokens": ["Wohl", "hier", "und", "dort", "aus", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Jesum Christum Amen.", "tokens": ["Durch", "Je\u00b7sum", "Chris\u00b7tum", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}