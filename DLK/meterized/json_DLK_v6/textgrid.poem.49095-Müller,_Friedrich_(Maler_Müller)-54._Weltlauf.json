{"textgrid.poem.49095": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "54. Weltlauf", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Billigkeit heischt die Moral; da\u00df du als Bruder behandelst", "tokens": ["Bil\u00b7lig\u00b7keit", "heischt", "die", "Mo\u00b7ral", ";", "da\u00df", "du", "als", "Bru\u00b7der", "be\u00b7han\u00b7delst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "KOUS", "NN", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Deinen N\u00e4chsten, das will unsere Religion;", "tokens": ["Dei\u00b7nen", "N\u00e4chs\u00b7ten", ",", "das", "will", "un\u00b7se\u00b7re", "Re\u00b7li\u00b7gi\u00b7on", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dennoch herrschet im ganzen der Welt stets Egoismus,", "tokens": ["Den\u00b7noch", "herr\u00b7schet", "im", "gan\u00b7zen", "der", "Welt", "stets", "E\u00b7gois\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "ART", "NN", "ADV", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Selbsterhaltung deckt b\u00fcndig jeden Betrug.", "tokens": ["Selbs\u00b7ter\u00b7hal\u00b7tung", "deckt", "b\u00fcn\u00b7dig", "je\u00b7den", "Be\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}