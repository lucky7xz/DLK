{"textgrid.poem.26186": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosen \u00f6ffnen ihre runden Schalen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rosen \u00f6ffnen ihre runden Schalen", "tokens": ["Die", "Ro\u00b7sen", "\u00f6ff\u00b7nen", "ih\u00b7re", "run\u00b7den", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leuchten weithin mit den roten Strahlen,", "tokens": ["Und", "leuch\u00b7ten", "weit\u00b7hin", "mit", "den", "ro\u00b7ten", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind wie gew\u00f6lbte Muscheln in dem Gartenmeer,", "tokens": ["Sind", "wie", "ge\u00b7w\u00f6lb\u00b7te", "Mu\u00b7scheln", "in", "dem", "Gar\u00b7ten\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stehn wie die Urnen aufgegl\u00fchter Stunden unterm Laub umher.", "tokens": ["Stehn", "wie", "die", "Ur\u00b7nen", "auf\u00b7ge\u00b7gl\u00fch\u00b7ter", "Stun\u00b7den", "un\u00b7term", "Laub", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Die Dornen, die sich eng an den Rosen halten,", "tokens": ["Die", "Dor\u00b7nen", ",", "die", "sich", "eng", "an", "den", "Ro\u00b7sen", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind wie die H\u00e4nde, die sich um das Liebste falten,", "tokens": ["Sind", "wie", "die", "H\u00e4n\u00b7de", ",", "die", "sich", "um", "das", "Liebs\u00b7te", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wachen eifers\u00fcchtig und entschlossen", "tokens": ["Und", "wa\u00b7chen", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "und", "ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und haben Zudringliche fortgesto\u00dfen.", "tokens": ["Und", "ha\u00b7ben", "Zu\u00b7dring\u00b7li\u00b7che", "fort\u00b7ge\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Manch Tropfen Blut ist um die r\u00f6teste geflossen.", "tokens": ["Manch", "Trop\u00b7fen", "Blut", "ist", "um", "die", "r\u00f6\u00b7tes\u00b7te", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VAFIN", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}