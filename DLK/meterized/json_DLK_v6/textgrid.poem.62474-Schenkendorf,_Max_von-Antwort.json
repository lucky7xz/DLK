{"textgrid.poem.62474": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein falsches W\u00e4hnen,", "tokens": ["Es", "ist", "kein", "fal\u00b7sches", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein loses Bilderspiel,", "tokens": ["Kein", "lo\u00b7ses", "Bil\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein tief gewurzelt Sehnen", "tokens": ["Ein", "tief", "ge\u00b7wur\u00b7zelt", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treibt mich nach meinem Ziel.", "tokens": ["Treibt", "mich", "nach", "mei\u00b7nem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "M\u00f6gt ihr mich Tr\u00e4umer schelten,", "tokens": ["M\u00f6gt", "ihr", "mich", "Tr\u00e4u\u00b7mer", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beweinen mu\u00df ich euch,", "tokens": ["Be\u00b7wei\u00b7nen", "mu\u00df", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will nicht mit Hohn vergelten,", "tokens": ["Will", "nicht", "mit", "Hohn", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und lad' euch in mein Reich.", "tokens": ["Und", "lad'", "euch", "in", "mein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Geist von oben dringet", "tokens": ["Ein", "Geist", "von", "o\u00b7ben", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch alle V\u00f6lker hin,", "tokens": ["Durch", "al\u00b7le", "V\u00f6l\u00b7ker", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch jeden Stamm bezwinget", "tokens": ["Doch", "je\u00b7den", "Stamm", "be\u00b7zwin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein eigner tiefer Sinn.", "tokens": ["Sein", "eig\u00b7ner", "tie\u00b7fer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie viel auch sind der Stufen", "tokens": ["Wie", "viel", "auch", "sind", "der", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Thron der Ewigkeit,", "tokens": ["Am", "Thron", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor allen weit und breit.", "tokens": ["Vor", "al\u00b7len", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist das Volk im Herzen", "tokens": ["Das", "ist", "das", "Volk", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der heil'gen Christenwelt,", "tokens": ["Der", "heil'\u00b7gen", "Chris\u00b7ten\u00b7welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das fester alle Schmerzen", "tokens": ["Das", "fes\u00b7ter", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle Freuden h\u00e4lt.", "tokens": ["Und", "al\u00b7le", "Freu\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist ein Volk der Treue,", "tokens": ["Das", "ist", "ein", "Volk", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Demuth und der Kraft,", "tokens": ["Der", "De\u00b7muth", "und", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Das ist die Gottesweihe,", "tokens": ["Das", "ist", "die", "Got\u00b7tes\u00b7wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Deutschlands W\u00fcrde schafft.", "tokens": ["Die", "Deutschlands", "W\u00fcr\u00b7de", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Es kann das Herz nur eines,", "tokens": ["Es", "kann", "das", "Herz", "nur", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein einiges nur sein,", "tokens": ["Ein", "ei\u00b7ni\u00b7ges", "nur", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum soll sich des Vereines", "tokens": ["Drum", "soll", "sich", "des", "Ver\u00b7ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch jeder Deutsche freun.", "tokens": ["Auch", "je\u00b7der", "Deut\u00b7sche", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn wieder sich gestalten", "tokens": ["Wenn", "wie\u00b7der", "sich", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das alte Deutschland soll,", "tokens": ["Das", "al\u00b7te", "Deutschland", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So sei es nicht zerspalten,", "tokens": ["So", "sei", "es", "nicht", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht schmach- und wundenvoll.", "tokens": ["Nicht", "schmach", "und", "wun\u00b7den\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df, an wen ich glaube,", "tokens": ["Ich", "wei\u00df", ",", "an", "wen", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kenn' ein holdes Bild;", "tokens": ["Ich", "kenn'", "ein", "hol\u00b7des", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Teufel nicht zum Raube", "tokens": ["Dem", "Teu\u00b7fel", "nicht", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird, was mein Herz erf\u00fcllt:", "tokens": ["Wird", ",", "was", "mein", "Herz", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von einem deutschen Throne,", "tokens": ["Von", "ei\u00b7nem", "deut\u00b7schen", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von einem Eichenbaum,", "tokens": ["Von", "ei\u00b7nem", "Ei\u00b7chen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der schirmend flicht die Krone \u2013", "tokens": ["Der", "schir\u00b7mend", "flicht", "die", "Kro\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist kein Dichtertraum.", "tokens": ["Das", "ist", "kein", "Dich\u00b7ter\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}