{"textgrid.poem.64820": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, Jammer, nie geling es dir,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, Jammer, nie geling es dir,", "tokens": ["Nein", ",", "Jam\u00b7mer", ",", "nie", "ge\u00b7ling", "es", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdumpfung mir zu bringen;", "tokens": ["Ver\u00b7dump\u00b7fung", "mir", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn es donnert \u00fcber mir,", "tokens": ["Und", "wenn", "es", "don\u00b7nert", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich lauter singen!", "tokens": ["So", "will", "ich", "lau\u00b7ter", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So laut, da\u00df ich jedweden Hohn,", "tokens": ["So", "laut", ",", "da\u00df", "ich", "jed\u00b7we\u00b7den", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeglich Schreckgedr\u00f6hne", "tokens": ["Und", "jeg\u00b7lich", "Schreck\u00b7ge\u00b7dr\u00f6h\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit meiner Stimme Liederton", "tokens": ["Mit", "mei\u00b7ner", "Stim\u00b7me", "Lie\u00b7der\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bew\u00e4lt'gend \u00fcbert\u00f6ne.", "tokens": ["Be\u00b7w\u00e4lt'\u00b7gend", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn mich Blitze rings umgl\u00fch'n,", "tokens": ["Und", "wenn", "mich", "Blit\u00b7ze", "rings", "um\u00b7gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedrohend schier mein Leben,", "tokens": ["Be\u00b7dro\u00b7hend", "schier", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will mein Haupt ich stolz und k\u00fchn", "tokens": ["So", "will", "mein", "Haupt", "ich", "stolz", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur h\u00f6her noch erheben.", "tokens": ["Nur", "h\u00f6\u00b7her", "noch", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und meiner Stirne Dichtermaal", "tokens": ["Und", "mei\u00b7ner", "Stir\u00b7ne", "Dich\u00b7ter\u00b7maal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mich vor ihnen sch\u00fctzen,", "tokens": ["Wird", "mich", "vor", "ih\u00b7nen", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbleichen soll ihr Flammenstrahl", "tokens": ["Er\u00b7blei\u00b7chen", "soll", "ihr", "Flam\u00b7men\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meines Auges Blitzen.", "tokens": ["Vor", "mei\u00b7nes", "Au\u00b7ges", "Blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn der Sturm emp\u00f6rt die Fluth,", "tokens": ["Und", "wenn", "der", "Sturm", "em\u00b7p\u00f6rt", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf hinwogt meine Barke,", "tokens": ["Drauf", "hin\u00b7wogt", "mei\u00b7ne", "Bar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will ich ben\u00fctzen seine Wuth,", "tokens": ["Will", "ich", "be\u00b7n\u00fct\u00b7zen", "sei\u00b7ne", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich daran erstarke.", "tokens": ["Da\u00df", "ich", "da\u00b7ran", "er\u00b7star\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gegr\u00fc\u00dft! du Sturm auf w\u00fcster See,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "!", "du", "Sturm", "auf", "w\u00fcs\u00b7ter", "See", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df adlerm\u00e4cht'ge Schwingen", "tokens": ["Da\u00df", "ad\u00b7ler\u00b7m\u00e4cht'\u00b7ge", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch schneller, als es sonst gesch\u00e4h'", "tokens": ["Noch", "schnel\u00b7ler", ",", "als", "es", "sonst", "ge\u00b7sch\u00e4\u00b7h'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich in den Hafen bringen.", "tokens": ["Mich", "in", "den", "Ha\u00b7fen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}