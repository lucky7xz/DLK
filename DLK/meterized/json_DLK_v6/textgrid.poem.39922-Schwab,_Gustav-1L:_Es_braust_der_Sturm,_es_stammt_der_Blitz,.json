{"textgrid.poem.39922": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es braust der Sturm, es stammt der Blitz,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es braust der Sturm, es stammt der Blitz,", "tokens": ["Es", "braust", "der", "Sturm", ",", "es", "stammt", "der", "Blitz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mutter fehlt ihr Kind,", "tokens": ["Der", "Mut\u00b7ter", "fehlt", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da geht sie aus in finstrer Nacht,", "tokens": ["Da", "geht", "sie", "aus", "in", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Regen und im Wind.", "tokens": ["Im", "Re\u00b7gen", "und", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie pocht umsonst bei'm Nachbar an,", "tokens": ["Sie", "pocht", "um\u00b7sonst", "bei'm", "Nach\u00b7bar", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie geht von Haus zu Haus:", "tokens": ["Sie", "geht", "von", "Haus", "zu", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdein Kindlein ging im Sonnenschein", "tokens": ["\u00bb", "dein", "Kin\u00b7dlein", "ging", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In's gr\u00fcne Thal hinaus!\u00ab", "tokens": ["In's", "gr\u00fc\u00b7ne", "Thal", "hin\u00b7aus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie fragt den Hirten auf dem Feld,", "tokens": ["Sie", "fragt", "den", "Hir\u00b7ten", "auf", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob er sich nicht besinnt?", "tokens": ["Ob", "er", "sich", "nicht", "be\u00b7sinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja nach dem Berge wandelt' es,", "tokens": ["\u00bb", "ja", "nach", "dem", "Ber\u00b7ge", "wan\u00b7delt'", "es", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nicht kam zur\u00fcck dein Kind!\u00ab", "tokens": ["Nicht", "kam", "zu\u00b7r\u00fcck", "dein", "Kind", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie geht hinaus in's dunkle Feld,", "tokens": ["Sie", "geht", "hin\u00b7aus", "in's", "dunk\u00b7le", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Donner schreckt sie nicht,", "tokens": ["Der", "Don\u00b7ner", "schreckt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie freut sich auf der Blitze Stral,", "tokens": ["Sie", "freut", "sich", "auf", "der", "Blit\u00b7ze", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hat kein andres Licht.", "tokens": ["Sie", "hat", "kein", "and\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbo zeiget mir den finstern Berg,", "tokens": ["\u00bb", "o", "zei\u00b7get", "mir", "den", "fins\u00b7tern", "Berg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lenkt mich in meiner Not,", "tokens": ["Lenkt", "mich", "in", "mei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und scheinet mir mein Kindlein an,", "tokens": ["Und", "schei\u00b7net", "mir", "mein", "Kin\u00b7dlein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lebendig oder tot!\u00ab", "tokens": ["Le\u00b7ben\u00b7dig", "o\u00b7der", "tot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Der Berg steht in dem Blitzesschein", "tokens": ["Der", "Berg", "steht", "in", "dem", "Blit\u00b7zes\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Starr, da\u00df es ist ein Graus;", "tokens": ["Starr", ",", "da\u00df", "es", "ist", "ein", "Graus", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Vater, der sein Kind verlor,", "tokens": ["Ein", "Va\u00b7ter", ",", "der", "sein", "Kind", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht nicht betr\u00fcbter aus.", "tokens": ["Sieht", "nicht", "be\u00b7tr\u00fcb\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wieder h\u00fcllt ihn Dunkel ein,", "tokens": ["Und", "wie\u00b7der", "h\u00fcllt", "ihn", "Dun\u00b7kel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder wird es hell;", "tokens": ["Und", "wie\u00b7der", "wird", "es", "hell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu seinen F\u00fc\u00dfen ruhet grau", "tokens": ["Zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "ru\u00b7het", "grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heidnische Kapell.", "tokens": ["Die", "heid\u00b7ni\u00b7sche", "Ka\u00b7pell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie stehet fest und hebt ihr Haupt", "tokens": ["Sie", "ste\u00b7het", "fest", "und", "hebt", "ihr", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie gebaut erst heut,", "tokens": ["Als", "wie", "ge\u00b7baut", "erst", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr mi\u00dfgestaltes G\u00f6tzenbild,", "tokens": ["Ihr", "mi\u00df\u00b7ge\u00b7stal\u00b7tes", "G\u00f6t\u00b7zen\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es grinset ungescheut.", "tokens": ["Es", "grin\u00b7set", "un\u00b7ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbo weh, mein Kind, mein armes Kind,", "tokens": ["\u00bb", "o", "weh", ",", "mein", "Kind", ",", "mein", "ar\u00b7mes", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du dich b\u00e4rgest dort!", "tokens": ["Wenn", "du", "dich", "b\u00e4r\u00b7gest", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn dich gepeitscht die Schreckennacht", "tokens": ["Wenn", "dich", "ge\u00b7peitscht", "die", "Schre\u00b7cken\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den verfluchten Ort!", "tokens": ["In", "den", "ver\u00b7fluch\u00b7ten", "Ort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mein Kind mu\u00df opfern am Altar,", "tokens": ["Mein", "Kind", "mu\u00df", "op\u00b7fern", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es dient dem b\u00f6sen Geist!", "tokens": ["Es", "dient", "dem", "b\u00f6\u00b7sen", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fall' \u00fcber mich, du bleicher Berg,", "tokens": ["Fall'", "\u00fc\u00b7ber", "mich", ",", "du", "blei\u00b7cher", "Berg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde Fugen, rei\u00dft!\u00ab", "tokens": ["Der", "Er\u00b7de", "Fu\u00b7gen", ",", "rei\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Mutter kommt zur runden Th\u00fcr,", "tokens": ["Die", "Mut\u00b7ter", "kommt", "zur", "run\u00b7den", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die stehet offen stets,", "tokens": ["Die", "ste\u00b7het", "of\u00b7fen", "stets", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch tritt zu ihr kein Wandrer ein", "tokens": ["Doch", "tritt", "zu", "ihr", "kein", "Wand\u00b7rer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "PIAT", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pfleget des Gebets.", "tokens": ["Und", "pfle\u00b7get", "des", "Ge\u00b7bets", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Wolken sind geflohen fort,", "tokens": ["Die", "Wol\u00b7ken", "sind", "ge\u00b7flo\u00b7hen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Donner hallen aus,", "tokens": ["Die", "Don\u00b7ner", "hal\u00b7len", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sternen und des Mondes Schein,", "tokens": ["Der", "Ster\u00b7nen", "und", "des", "Mon\u00b7des", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wandelt keck voraus.", "tokens": ["Der", "wan\u00b7delt", "keck", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da fa\u00dft die Mutter sich ein Herz,", "tokens": ["Da", "fa\u00dft", "die", "Mut\u00b7ter", "sich", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie geht zum Tempel ein \u2013", "tokens": ["Sie", "geht", "zum", "Tem\u00b7pel", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr s\u00fc\u00dfes Kind ruht am Altar", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfes", "Kind", "ruht", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getrost im Mondenschein.", "tokens": ["Ge\u00b7trost", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es l\u00e4chelt mit den Lippen bleich,", "tokens": ["Es", "l\u00e4\u00b7chelt", "mit", "den", "Lip\u00b7pen", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man im Traume thut,", "tokens": ["Wie", "man", "im", "Trau\u00b7me", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blinkend in halboffner Hand", "tokens": ["Und", "blin\u00b7kend", "in", "hal\u00b7boff\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein silbern Gr\u00f6schlein ruht.", "tokens": ["Ein", "sil\u00b7bern", "Gr\u00f6schlein", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Kennt ihr der Engel Groschen nicht?", "tokens": ["Kennt", "ihr", "der", "En\u00b7gel", "Gro\u00b7schen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie geben ihn zu Pfand,", "tokens": ["Sie", "ge\u00b7ben", "ihn", "zu", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn f\u00fchren wollen sie ein Kind", "tokens": ["Wenn", "f\u00fch\u00b7ren", "wol\u00b7len", "sie", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVINF", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sich in's Vaterland.", "tokens": ["Mit", "sich", "in's", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und mit dem Silber spielt das Kind", "tokens": ["Und", "mit", "dem", "Sil\u00b7ber", "spielt", "das", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis Schlaf sein Auge deckt,", "tokens": ["Bis", "Schlaf", "sein", "Au\u00b7ge", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bis der Sterne Silberstrom", "tokens": ["Und", "bis", "der", "Ster\u00b7ne", "Sil\u00b7ber\u00b7strom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das zugeschlossne weckt.", "tokens": ["Das", "zu\u00b7ge\u00b7schloss\u00b7ne", "weckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Die Mutter wirft sich auf die Knie,", "tokens": ["Die", "Mut\u00b7ter", "wirft", "sich", "auf", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie weinet still und lauscht,", "tokens": ["Sie", "wei\u00b7net", "still", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie durch das alte Heidenhaus", "tokens": ["Wie", "durch", "das", "al\u00b7te", "Hei\u00b7den\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Engels Fl\u00fcgel rauscht.", "tokens": ["Des", "En\u00b7gels", "Fl\u00fc\u00b7gel", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Sie k\u00fc\u00dft ihr Kind, es athmet nicht,", "tokens": ["Sie", "k\u00fc\u00dft", "ihr", "Kind", ",", "es", "ath\u00b7met", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schl\u00e4ft ja schon so tief,", "tokens": ["Es", "schl\u00e4ft", "ja", "schon", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei seinem Hirten ist zu Haus", "tokens": ["Bei", "sei\u00b7nem", "Hir\u00b7ten", "ist", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lamm, das irre lief.", "tokens": ["Das", "Lamm", ",", "das", "ir\u00b7re", "lief", "."], "token_info": ["word", "word", "