{"dta.poem.9387": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! ich m\u00f6chte gerne wissen", "tokens": ["Ach", "!", "ich", "m\u00f6ch\u00b7te", "ger\u00b7ne", "wis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was nur sch\u00f6nes an mier sey?", "tokens": ["was", "nur", "sch\u00f6\u00b7nes", "an", "mier", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was auf H\u00f6ffligkeit geflissen", "tokens": ["Was", "auf", "H\u00f6ff\u00b7lig\u00b7keit", "ge\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Tugend falle bey?", "tokens": ["und", "der", "Tu\u00b7gend", "fal\u00b7le", "bey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df Jhm hatt gel\u00fcst zu loben", "tokens": ["da\u00df", "Jhm", "hatt", "ge\u00b7l\u00fcst", "zu", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und mich also sehr erhoben?", "tokens": ["und", "mich", "al\u00b7so", "sehr", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "