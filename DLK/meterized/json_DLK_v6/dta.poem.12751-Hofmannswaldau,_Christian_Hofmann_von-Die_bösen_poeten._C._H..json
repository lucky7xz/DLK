{"dta.poem.12751": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die b\u00f6sen poeten.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An dichtern fehlt es nicht bey diesen b\u00f6sen zeiten;", "tokens": ["An", "dich\u00b7tern", "fehlt", "es", "nicht", "bey", "die\u00b7sen", "b\u00f6\u00b7sen", "zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es fehlt an denen nur, die vor die wahrheit streiten.", "tokens": ["Es", "fehlt", "an", "de\u00b7nen", "nur", ",", "die", "vor", "die", "wahr\u00b7heit", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELS", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}