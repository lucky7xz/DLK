{"dta.poem.11939": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Stufen sind es die der Mensch empor mu\u00df streben,", "tokens": ["Drei", "Stu\u00b7fen", "sind", "es", "die", "der", "Mensch", "em\u00b7por", "mu\u00df", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ART", "ART", "NN", "PTKVZ", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um sich vom dunklen Ich zum lichten zu erheben.", "tokens": ["Um", "sich", "vom", "dunk\u00b7len", "Ich", "zum", "lich\u00b7ten", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPRART", "ADJA", "PPER", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zuerst trit aus dir selbst ins Leben rings um dich,", "tokens": ["Zu\u00b7erst", "trit", "aus", "dir", "selbst", "ins", "Le\u00b7ben", "rings", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freue dich daran, wie alles freuet sich.", "tokens": ["Und", "freu\u00b7e", "dich", "da\u00b7ran", ",", "wie", "al\u00b7les", "freu\u00b7et", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$,", "PWAV", "PIS", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann gib den Kummer auf, da\u00df Alles rings verf\u00e4llt,", "tokens": ["Dann", "gib", "den", "Kum\u00b7mer", "auf", ",", "da\u00df", "Al\u00b7les", "rings", "ver\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PTKVZ", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freu dich, da\u00df sich jung die Welt im Ganzen h\u00e4lt.", "tokens": ["Und", "freu", "dich", ",", "da\u00df", "sich", "jung", "die", "Welt", "im", "Gan\u00b7zen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PRF", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann la\u00df dies Ganze selbst zur\u00fcck ins Ew'ge schwinden,", "tokens": ["Dann", "la\u00df", "dies", "Gan\u00b7ze", "selbst", "zu\u00b7r\u00fcck", "ins", "Ew'\u00b7ge", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "NN", "ADV", "PTKVZ", "APPRART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort erst wirst du dich ganz im gro\u00dfen Ich empfinden.", "tokens": ["Dort", "erst", "wirst", "du", "dich", "ganz", "im", "gro\u00b7\u00dfen", "Ich", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "ADV", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}