{"dta.poem.19333": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesetze der Natur willst du, o Mensch, entdecken;", "tokens": ["Ge\u00b7set\u00b7ze", "der", "Na\u00b7tur", "willst", "du", ",", "o", "Mensch", ",", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "PPER", "$,", "FM", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du solltest dir das Ziel etwas bestimmter stecken.", "tokens": ["Du", "soll\u00b7test", "dir", "das", "Ziel", "et\u00b7was", "be\u00b7stimm\u00b7ter", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht das, wie sich verh\u00e4lt an und f\u00fcr sich die Welt,", "tokens": ["Nicht", "das", ",", "wie", "sich", "ver\u00b7h\u00e4lt", "an", "und", "f\u00fcr", "sich", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "$,", "PWAV", "PRF", "VVFIN", "PTKVZ", "KON", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entdecken sollst du, wie sie sich zu dir verh\u00e4lt.", "tokens": ["Ent\u00b7de\u00b7cken", "sollst", "du", ",", "wie", "sie", "sich", "zu", "dir", "ver\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wozu Gott immer sonst die Dinge m\u00f6gen dienen,", "tokens": ["Wo\u00b7zu", "Gott", "im\u00b7mer", "sonst", "die", "Din\u00b7ge", "m\u00f6\u00b7gen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wei\u00dft nicht was sie sind, nur was du hast an ihnen.", "tokens": ["Du", "wei\u00dft", "nicht", "was", "sie", "sind", ",", "nur", "was", "du", "hast", "an", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "$,", "ADV", "PWS", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}