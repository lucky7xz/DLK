{"dta.poem.23330": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Polizei, Geld und Wetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freier Mensch mit g\u00f6ttlichen Entw\u00fcrfen,", "tokens": ["Frei\u00b7er", "Mensch", "mit", "g\u00f6tt\u00b7li\u00b7chen", "Ent\u00b7w\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Voll von hohen himmlischen Ideen,", "tokens": ["Voll", "von", "ho\u00b7hen", "himm\u00b7li\u00b7schen", "I\u00b7deen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus dem Born der Sch\u00f6pfung kannst du schl\u00fcrfen,", "tokens": ["Aus", "dem", "Born", "der", "Sch\u00f6p\u00b7fung", "kannst", "du", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ins Angesicht der Gottheit sehn.", "tokens": ["Und", "ins", "An\u00b7ge\u00b7sicht", "der", "Got\u00b7theit", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber ohne Pa\u00df, da bist du kaum ein Thier,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "Pa\u00df", ",", "da", "bist", "du", "kaum", "ein", "Thier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Freier Mensch, es tauscht kein Hund mit dir.", "tokens": ["Frei\u00b7er", "Mensch", ",", "es", "tauscht", "kein", "Hund", "mit", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Freier Mensch voll hoher Gottesgaben,", "tokens": ["Frei\u00b7er", "Mensch", "voll", "ho\u00b7her", "Got\u00b7tes\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kannst du alles h\u00f6ren, alles sehn,", "tokens": ["Kannst", "du", "al\u00b7les", "h\u00f6\u00b7ren", ",", "al\u00b7les", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kannst genie\u00dfen alles, alles haben,", "tokens": ["Kannst", "ge\u00b7nie\u00b7\u00dfen", "al\u00b7les", ",", "al\u00b7les", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PIS", "$,", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Darfst nicht unbefriedigt weiter gehn.", "tokens": ["Darfst", "nicht", "un\u00b7be\u00b7frie\u00b7digt", "wei\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber ohne Geld, du giebst es selber zu,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "Geld", ",", "du", "giebst", "es", "sel\u00b7ber", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Freier Mensch, ein Schaf ist mehr als du.", "tokens": ["Frei\u00b7er", "Mensch", ",", "ein", "Schaf", "ist", "mehr", "als", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "KOUS", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Freier Mensch, setz auf die Pudelm\u00fctze,", "tokens": ["Frei\u00b7er", "Mensch", ",", "setz", "auf", "die", "Pu\u00b7del\u00b7m\u00fct\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df dich Kopfweh nicht und Zahnschmerz plagt;", "tokens": ["Da\u00df", "dich", "Kopf\u00b7weh", "nicht", "und", "Zahn\u00b7schmerz", "plagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nimm den Schirm, er ist dir heute n\u00fctze,", "tokens": ["Nimm", "den", "Schirm", ",", "er", "ist", "dir", "heu\u00b7te", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil das Wetter doch nach dir nicht fragt;", "tokens": ["Weil", "das", "Wet\u00b7ter", "doch", "nach", "dir", "nicht", "fragt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn das Wetter ist wie Geld und Polizei,", "tokens": ["Denn", "das", "Wet\u00b7ter", "ist", "wie", "Geld", "und", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Freier Mensch, leb wohl! und werd' erst \u2014 frei!", "tokens": ["Frei\u00b7er", "Mensch", ",", "leb", "wohl", "!", "und", "werd'", "erst", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$.", "KON", "VAFIN", "ADV", "$(", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}