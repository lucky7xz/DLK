{"textgrid.poem.66796": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die Dampfwalze", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die schwere Walze dampfen,", "tokens": ["Sieh", "die", "schwe\u00b7re", "Wal\u00b7ze", "damp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Milliarden Steine stampfen,", "tokens": ["Mil\u00b7li\u00b7ar\u00b7den", "Stei\u00b7ne", "stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vergewaltigt St\u00fcck um St\u00fcck:", "tokens": ["Ver\u00b7ge\u00b7wal\u00b7tigt", "St\u00fcck", "um", "St\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Menschheit, dein Geschick!", "tokens": ["Ar\u00b7me", "Menschheit", ",", "dein", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Bist wie Kiesel auf dem Pfade,", "tokens": ["Bist", "wie", "Kie\u00b7sel", "auf", "dem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst zermalmet ohne Gnade;", "tokens": ["Wirst", "zer\u00b7mal\u00b7met", "oh\u00b7ne", "Gna\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcge, Tr\u00e4gheit, Unrecht, Raub", "tokens": ["L\u00fc\u00b7ge", ",", "Tr\u00e4g\u00b7heit", ",", "Un\u00b7recht", ",", "Raub"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dampfen, stampfen dich zu Staub.", "tokens": ["Damp\u00b7fen", ",", "stamp\u00b7fen", "dich", "zu", "Staub", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}