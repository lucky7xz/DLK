{"textgrid.poem.52153": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die gel\u00f6schten Kerzen", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gewalt'ger Herd mit gl\u00fchnden Kohlen", "tokens": ["Ein", "ge\u00b7walt'\u00b7ger", "Herd", "mit", "gl\u00fchn\u00b7den", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und zwei hellen Kerzen auf dem Simse,", "tokens": ["Und", "zwei", "hel\u00b7len", "Ker\u00b7zen", "auf", "dem", "Sim\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dran ein plaudernd Paar: ein narb'ger Feldherr", "tokens": ["Dran", "ein", "plau\u00b7dernd", "Paar", ":", "ein", "na\u00b7rb'\u00b7ger", "Feld\u00b7herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "ADJD", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In der Majest\u00e4t des Greisenalters", "tokens": ["In", "der", "Ma\u00b7jes\u00b7t\u00e4t", "des", "Grei\u00b7sen\u00b7al\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und ein unbefangnes Kind der Neuzeit,", "tokens": ["Und", "ein", "un\u00b7be\u00b7fang\u00b7nes", "Kind", "der", "Neu\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ein geliebter und verzogner Neffe.", "tokens": ["Ein", "ge\u00b7lieb\u00b7ter", "und", "ver\u00b7zog\u00b7ner", "Nef\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "W\u00fcrdevoll erz\u00e4hlt der Greis von weiland,", "tokens": ["W\u00fcr\u00b7de\u00b7voll", "er\u00b7z\u00e4hlt", "der", "Greis", "von", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Von Verschollnem oder halb Verschollnem.", "tokens": ["Von", "Ver\u00b7scholl\u00b7nem", "o\u00b7der", "halb", "Ver\u00b7scholl\u00b7nem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "\u00bbdamals warst du noch ein Ungeborner,", "tokens": ["\u00bb", "da\u00b7mals", "warst", "du", "noch", "ein", "Un\u00b7ge\u00b7bor\u00b7ner", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Neffe\u00ab, sagt er, \u00bboder in den Windeln\u00ab...", "tokens": ["Nef\u00b7fe", "\u00ab", ",", "sagt", "er", ",", "\u00bb", "o\u00b7der", "in", "den", "Win\u00b7deln", "\u00ab", "..."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "KON", "APPR", "ART", "NN", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Auf dem Herde zuckt ein blaues Fl\u00e4mmchen,", "tokens": ["Auf", "dem", "Her\u00b7de", "zuckt", "ein", "blau\u00b7es", "Fl\u00e4mm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ein verge\u00dfnes Fl\u00e4mmchen aus der Asche,", "tokens": ["Ein", "ver\u00b7ge\u00df\u00b7nes", "Fl\u00e4mm\u00b7chen", "aus", "der", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und die beiden sehn den Irrwisch tanzen,", "tokens": ["Und", "die", "bei\u00b7den", "sehn", "den", "Irr\u00b7wisch", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und der Irrwisch unversehens springt er", "tokens": ["Und", "der", "Irr\u00b7wisch", "un\u00b7ver\u00b7se\u00b7hens", "springt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Auf des J\u00fcnglings bl\u00fchend kecke Lippen:", "tokens": ["Auf", "des", "J\u00fcng\u00b7lings", "bl\u00fc\u00b7hend", "ke\u00b7cke", "Lip\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "\u2013 \u00bbOhm, wie war es denn mit der Camargo?\u00ab", "tokens": ["\u2013", "\u00bb", "Ohm", ",", "wie", "war", "es", "denn", "mit", "der", "Ca\u00b7mar\u00b7go", "?", "\u00ab"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Der Benarbte l\u00e4chelt. \u00bbWissen willst du", "tokens": ["Der", "Be\u00b7narb\u00b7te", "l\u00e4\u00b7chelt", ".", "\u00bb", "Wis\u00b7sen", "willst", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Das mit der Camargo?\u00ab \u2013 Eine Kerze", "tokens": ["Das", "mit", "der", "Ca\u00b7mar\u00b7go", "?", "\u00ab", "\u2013", "Ei\u00b7ne", "Ker\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "$.", "$(", "$(", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Haucht er aus und auch die andre Kerze.", "tokens": ["Haucht", "er", "aus", "und", "auch", "die", "and\u00b7re", "Ker\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u00bbdu erlaubst? Nur da\u00df ich nicht err\u00f6te!", "tokens": ["\u00bb", "du", "er\u00b7laubst", "?", "Nur", "da\u00df", "ich", "nicht", "er\u00b7r\u00f6\u00b7te", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "ADV", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Also...\u00ab Durch das Dunkel gl\u00fchn die Kohlen.", "tokens": ["Al\u00b7so", "...", "\u00ab", "Durch", "das", "Dun\u00b7kel", "gl\u00fchn", "die", "Koh\u00b7len", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "$(", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Und der J\u00fcngling streicht ein Holz, die eine", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", "streicht", "ein", "Holz", ",", "die", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Kerze flammt er an und dann die andre:", "tokens": ["Ker\u00b7ze", "flammt", "er", "an", "und", "dann", "die", "and\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "\u00bbohm, wie war's denn mit dem Sturm auf D\u00fcppel?\u00ab", "tokens": ["\u00bb", "ohm", ",", "wie", "wa\u00b7r's", "denn", "mit", "dem", "Sturm", "auf", "D\u00fcp\u00b7pel", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PWAV", "VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}}}}