{"textgrid.poem.26747": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem Tal bei armen Hirten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Tal bei armen Hirten", "tokens": ["In", "ei\u00b7nem", "Tal", "bei", "ar\u00b7men", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschien mit jedem jungen Jahr,", "tokens": ["Er\u00b7schien", "mit", "je\u00b7dem", "jun\u00b7gen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald die ersten Lerchen schwirrten,", "tokens": ["So\u00b7bald", "die", "ers\u00b7ten", "Ler\u00b7chen", "schwirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4dchen, sch\u00f6n und wunderbar.", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie war nicht in dem Tal geboren,", "tokens": ["Sie", "war", "nicht", "in", "dem", "Tal", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man wu\u00dfte nicht, woher sie kam,", "tokens": ["Man", "wu\u00df\u00b7te", "nicht", ",", "wo\u00b7her", "sie", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schnell war ihre Spur verloren,", "tokens": ["Und", "schnell", "war", "ih\u00b7re", "Spur", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald das M\u00e4dchen Abschied nahm.", "tokens": ["So\u00b7bald", "das", "M\u00e4d\u00b7chen", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Beseligend war ihre N\u00e4he,", "tokens": ["Be\u00b7se\u00b7li\u00b7gend", "war", "ih\u00b7re", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Und alle Herzen wurden weit,", "tokens": ["Und", "al\u00b7le", "Her\u00b7zen", "wur\u00b7den", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch eine W\u00fcrde, eine H\u00f6he", "tokens": ["Doch", "ei\u00b7ne", "W\u00fcr\u00b7de", ",", "ei\u00b7ne", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entfernte die Vertraulichkeit.", "tokens": ["Ent\u00b7fern\u00b7te", "die", "Ver\u00b7trau\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie brachte Blumen mit und Fr\u00fcchte,", "tokens": ["Sie", "brach\u00b7te", "Blu\u00b7men", "mit", "und", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gereift auf einer andern Flur,", "tokens": ["Ge\u00b7reift", "auf", "ei\u00b7ner", "an\u00b7dern", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem andern Sonnenlichte,", "tokens": ["In", "ei\u00b7nem", "an\u00b7dern", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer gl\u00fccklichern Natur.", "tokens": ["In", "ei\u00b7ner", "gl\u00fcck\u00b7li\u00b7chern", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und teilte jedem eine Gabe,", "tokens": ["Und", "teil\u00b7te", "je\u00b7dem", "ei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Fr\u00fcchte, jenem Blumen aus,", "tokens": ["Dem", "Fr\u00fcch\u00b7te", ",", "je\u00b7nem", "Blu\u00b7men", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling und der Greis am Stabe,", "tokens": ["Der", "J\u00fcng\u00b7ling", "und", "der", "Greis", "am", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder ging beschenkt nach Haus.", "tokens": ["Ein", "je\u00b7der", "ging", "be\u00b7schenkt", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Willkommen waren alle G\u00e4ste,", "tokens": ["Will\u00b7kom\u00b7men", "wa\u00b7ren", "al\u00b7le", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nahte sich ein liebend Paar,", "tokens": ["Doch", "nah\u00b7te", "sich", "ein", "lie\u00b7bend", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem reichte sie der Gaben beste,", "tokens": ["Dem", "reich\u00b7te", "sie", "der", "Ga\u00b7ben", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blumen allersch\u00f6nste dar.", "tokens": ["Der", "Blu\u00b7men", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}