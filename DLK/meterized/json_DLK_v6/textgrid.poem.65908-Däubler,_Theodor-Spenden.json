{"textgrid.poem.65908": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Spenden", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den holden Wohlstand gab, mit guten H\u00e4nden", "tokens": ["Den", "hol\u00b7den", "Wohl\u00b7stand", "gab", ",", "mit", "gu\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Mutter Demeter, wo sie geweilt;", "tokens": ["Die", "Mut\u00b7ter", "De\u00b7me\u00b7ter", ",", "wo", "sie", "ge\u00b7weilt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer bei ihr blieb, wo sie vorbeigeeilt,", "tokens": ["Wer", "bei", "ihr", "blieb", ",", "wo", "sie", "vor\u00b7bei\u00b7ge\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erreichte Segnung zwischen kargen W\u00e4nden.", "tokens": ["Er\u00b7reich\u00b7te", "Seg\u00b7nung", "zwi\u00b7schen", "kar\u00b7gen", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Spruch gebar das Korn auf Steingel\u00e4nden,", "tokens": ["Ihr", "Spruch", "ge\u00b7bar", "das", "Korn", "auf", "Stein\u00b7ge\u00b7l\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zypressen hat sie Blitzen zugesteilt,", "tokens": ["Zyp\u00b7res\u00b7sen", "hat", "sie", "Blit\u00b7zen", "zu\u00b7ge\u00b7steilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Durch Kr\u00e4uter das gekr\u00e4nkte Tier geheilt,", "tokens": ["Durch", "Kr\u00e4u\u00b7ter", "das", "ge\u00b7kr\u00e4nk\u00b7te", "Tier", "ge\u00b7heilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns stark gew\u00fcnscht, da\u00df Winters Sorgen schw\u00e4nden!", "tokens": ["Uns", "stark", "ge\u00b7w\u00fcnscht", ",", "da\u00df", "Win\u00b7ters", "Sor\u00b7gen", "schw\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo meine Schritte vor Eleusis stauben,", "tokens": ["Wo", "mei\u00b7ne", "Schrit\u00b7te", "vor", "E\u00b7leu\u00b7sis", "stau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil laue Luft den Wandernden bem\u00fcht,", "tokens": ["Weil", "lau\u00b7e", "Luft", "den", "Wan\u00b7dern\u00b7den", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erwarten Frohgesinnte uns in Lauben.", "tokens": ["Er\u00b7war\u00b7ten", "Froh\u00b7ge\u00b7sinn\u00b7te", "uns", "in", "Lau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Demeters Verhei\u00dfungen begl\u00fcht,", "tokens": ["Von", "De\u00b7me\u00b7ters", "Ver\u00b7hei\u00b7\u00dfun\u00b7gen", "be\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Stehn M\u00e4nner fest gefa\u00dft bei ihren Dauben,", "tokens": ["Stehn", "M\u00e4n\u00b7ner", "fest", "ge\u00b7fa\u00dft", "bei", "ih\u00b7ren", "Dau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da bald im Fa\u00df die Sonne Wonne br\u00fcht.", "tokens": ["Da", "bald", "im", "Fa\u00df", "die", "Son\u00b7ne", "Won\u00b7ne", "br\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}