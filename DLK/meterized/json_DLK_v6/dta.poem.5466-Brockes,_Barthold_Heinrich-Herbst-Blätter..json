{"dta.poem.5466": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Bl\u00e4tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint so gar der Wind anjetzt sich zu bem\u00fchn,", "tokens": ["Es", "scheint", "so", "gar", "der", "Wind", "an\u00b7jetzt", "sich", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, zu dem Endzweck blos, die Bl\u00e4tter zu bewegen,", "tokens": ["Und", ",", "zu", "dem", "End\u00b7zweck", "blos", ",", "die", "Bl\u00e4t\u00b7ter", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um, durch derselben \u00f6fters regen,", "tokens": ["Um", ",", "durch", "der\u00b7sel\u00b7ben", "\u00f6f\u00b7ters", "re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "APPR", "PDAT", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blick nun desto mehr auf sie zu ziehn.", "tokens": ["Den", "Blick", "nun", "des\u00b7to", "mehr", "auf", "sie", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Man siehet, sieht man recht, der Farben bunte Klarheit,", "tokens": ["Man", "sie\u00b7het", ",", "sieht", "man", "recht", ",", "der", "Far\u00b7ben", "bun\u00b7te", "Klar\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "PIS", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In einer Harmonie, so angenehm sich mischen;", "tokens": ["In", "ei\u00b7ner", "Har\u00b7mo\u00b7nie", ",", "so", "an\u00b7ge\u00b7nehm", "sich", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man h\u00f6ret, h\u00f6rt man recht, in ihrem sanften Zischen", "tokens": ["Man", "h\u00f6\u00b7ret", ",", "h\u00f6rt", "man", "recht", ",", "in", "ih\u00b7rem", "sanf\u00b7ten", "Zi\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "PIS", "ADJD", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und lispelndem Geth\u00f6s\u2019 und Wispern, diese Wahrheit:", "tokens": ["Und", "lis\u00b7peln\u00b7dem", "Get\u00b7h\u00f6s'", "und", "Wis\u00b7pern", ",", "die\u00b7se", "Wahr\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u201ejhr sehet uns vielleicht zum letzten mahl,", "tokens": ["\u201e", "jhr", "se\u00b7het", "uns", "viel\u00b7leicht", "zum", "letz\u00b7ten", "mahl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "ADV", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.10": {"text": "\u201ebeseht uns heute noch, denn da wir schon gereift,", "tokens": ["\u201e", "be\u00b7seht", "uns", "heu\u00b7te", "noch", ",", "denn", "da", "wir", "schon", "ge\u00b7reift", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201esind Morgen schon vielleicht der B\u00e4ume Wipfel kahl,", "tokens": ["\u201e", "sind", "Mor\u00b7gen", "schon", "viel\u00b7leicht", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "kahl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "ADV", "ADV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.12": {"text": "\u201eund wir vermuthlich abgestreift.", "tokens": ["\u201e", "und", "wir", "ver\u00b7muth\u00b7lich", "ab\u00b7ge\u00b7streift", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201enoch k\u00f6nnt ihr euren Blick an uns vergn\u00fcgen,", "tokens": ["\u201e", "noch", "k\u00f6nnt", "ihr", "eu\u00b7ren", "Blick", "an", "uns", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "\u201enoch k\u00f6nnt ihr dessen Ehr\u2019,", "tokens": ["\u201e", "noch", "k\u00f6nnt", "ihr", "des\u00b7sen", "Ehr'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "\u201eder euch und uns gemacht, zu eurer Freude f\u00fcgen,", "tokens": ["\u201e", "der", "euch", "und", "uns", "ge\u00b7macht", ",", "zu", "eu\u00b7rer", "Freu\u00b7de", "f\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "KON", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eund opfern eure Lust dem Wesen mehr und mehr,", "tokens": ["\u201e", "und", "op\u00b7fern", "eu\u00b7re", "Lust", "dem", "We\u00b7sen", "mehr", "und", "mehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "PPOSAT", "NN", "ART", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edas euch zu gut, indem wir sterben,", "tokens": ["\u201e", "das", "euch", "zu", "gut", ",", "in\u00b7dem", "wir", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "PTKA", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "\u201euns ehe noch, als wir verderben,", "tokens": ["\u201e", "uns", "e\u00b7he", "noch", ",", "als", "wir", "ver\u00b7der\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "KOUS", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "\u201ezu eurer Lust so sch\u00f6n, so lieblich wollen f\u00e4rben.", "tokens": ["\u201e", "zu", "eu\u00b7rer", "Lust", "so", "sch\u00f6n", ",", "so", "lieb\u00b7lich", "wol\u00b7len", "f\u00e4r\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}