{"textgrid.poem.66277": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Vertrauen auf Gottes Schutz", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tief, so innig f\u00fchlt' ich's nie,", "tokens": ["So", "tief", ",", "so", "in\u00b7nig", "f\u00fchlt'", "ich's", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verzeih, wenn Thr\u00e4nen niederthauen,", "tokens": ["Ver\u00b7zeih", ",", "wenn", "Thr\u00e4\u00b7nen", "nie\u00b7dert\u00b7hau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn deine Hand entlockte sie.", "tokens": ["Denn", "dei\u00b7ne", "Hand", "ent\u00b7lock\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da steh' ich wie ein Fels im Meere,", "tokens": ["Da", "steh'", "ich", "wie", "ein", "Fels", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Best\u00fcrmt und doch versenkt in Ruh',", "tokens": ["Be\u00b7st\u00fcrmt", "und", "doch", "ver\u00b7senkt", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So voll Vertrauen, Gott! als w\u00e4re", "tokens": ["So", "voll", "Ver\u00b7trau\u00b7en", ",", "Gott", "!", "als", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$,", "NN", "$.", "KOKOM", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts auf der Welt, als ich und du.", "tokens": ["Nichts", "auf", "der", "Welt", ",", "als", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchl's, wenn mich das Elend w\u00fcrgt,", "tokens": ["Ich", "f\u00fchl's", ",", "wenn", "mich", "das", "E\u00b7lend", "w\u00fcrgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn hinter schwarzer Wolken Grauen", "tokens": ["Wenn", "hin\u00b7ter", "schwar\u00b7zer", "Wol\u00b7ken", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich jeder Stern vor mir verbirgt;", "tokens": ["Sich", "je\u00b7der", "Stern", "vor", "mir", "ver\u00b7birgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn St\u00fcrm' um meinen Kerker br\u00fcllen,", "tokens": ["Wenn", "St\u00fcrm'", "um", "mei\u00b7nen", "Ker\u00b7ker", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn um mich z\u00fcckt des Blitzes Pfeil;", "tokens": ["Wenn", "um", "mich", "z\u00fcckt", "des", "Blit\u00b7zes", "Pfeil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann leg' ich mich in deinen Willen", "tokens": ["Dann", "leg'", "ich", "mich", "in", "dei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u00fcberh\u00f6r' das Sturmgeheul.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7h\u00f6r'", "das", "Sturm\u00b7ge\u00b7heul", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mit dem Mondschild uns bedeckt,", "tokens": ["Der", "mit", "dem", "Mond\u00b7schild", "uns", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Felsenw\u00e4nde uns verbauen,", "tokens": ["Wenn", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "uns", "ver\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Gitter, Schlo\u00df und Riegel schreckt;", "tokens": ["Wenn", "Git\u00b7ter", ",", "Schlo\u00df", "und", "Rie\u00b7gel", "schreckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Einsamkeit mit stummer Lippe", "tokens": ["Wenn", "Ein\u00b7sam\u00b7keit", "mit", "stum\u00b7mer", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwindelnd auf uns niederschaut,", "tokens": ["Und", "schwin\u00b7delnd", "auf", "uns", "nie\u00b7der\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn vor dem scheu\u00dflichen Gerippe", "tokens": ["Wenn", "vor", "dem", "scheu\u00df\u00b7li\u00b7chen", "Ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Todes unsre Seele graut.", "tokens": ["Des", "To\u00b7des", "uns\u00b7re", "See\u00b7le", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist's, wenn Menschenh\u00fclf' uns flieht?", "tokens": ["Was", "ist's", ",", "wenn", "Men\u00b7schen\u00b7h\u00fcl\u00b7f'", "uns", "flieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und, wie von Fluch getroffnen Auen", "tokens": ["Und", ",", "wie", "von", "Fluch", "ge\u00b7troff\u00b7nen", "Au\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wild, bei uns vor\u00fcberzieht?", "tokens": ["Das", "Wild", ",", "bei", "uns", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist's, wenn Peiniger uns hassen,", "tokens": ["Was", "ist's", ",", "wenn", "Pei\u00b7ni\u00b7ger", "uns", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn um uns zischt die Schlange Spott?", "tokens": ["Wenn", "um", "uns", "zischt", "die", "Schlan\u00b7ge", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Br\u00fcder selber uns verlassen?", "tokens": ["Wenn", "Br\u00fc\u00b7der", "sel\u00b7ber", "uns", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir sind doch stark, wir haben Gott!", "tokens": ["Wir", "sind", "doch", "stark", ",", "wir", "ha\u00b7ben", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was that dein Sohn? Er traute dir,", "tokens": ["Was", "that", "dein", "Sohn", "?", "Er", "trau\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lie\u00df die Skorpionen hauen,", "tokens": ["Er", "lie\u00df", "die", "Skor\u00b7pi\u00b7o\u00b7nen", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: Der Vater ist bei mir!", "tokens": ["Und", "sprach", ":", "Der", "Va\u00b7ter", "ist", "bei", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er trug die Schmach der Dornenkrone,", "tokens": ["Er", "trug", "die", "Schmach", "der", "Dor\u00b7nen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Gei\u00dfel Zug, des Kreuzes Pein,", "tokens": ["Der", "Gei\u00b7\u00dfel", "Zug", ",", "des", "Kreu\u00b7zes", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00fcllte sich beim lauten Hohne", "tokens": ["Und", "h\u00fcll\u00b7te", "sich", "beim", "lau\u00b7ten", "Hoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der H\u00f6ll' in sein Vertrauen ein.", "tokens": ["Der", "H\u00f6ll'", "in", "sein", "Ver\u00b7trau\u00b7en", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn die Gewaltthat nach uns greift,", "tokens": ["Wenn", "die", "Ge\u00b7walt\u00b7that", "nach", "uns", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns h\u00e4lt in ihren Tigerklauen,", "tokens": ["Uns", "h\u00e4lt", "in", "ih\u00b7ren", "Ti\u00b7ger\u00b7klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Blut tyrannisch s\u00e4uft.", "tokens": ["Und", "un\u00b7ser", "Blut", "ty\u00b7ran\u00b7nisch", "s\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie w\u00fcrgt ja nur des Staubleibs Glieder,", "tokens": ["Sie", "w\u00fcrgt", "ja", "nur", "des", "Staub\u00b7leibs", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Christenseelen w\u00fcrgt sie nicht,", "tokens": ["Doch", "Chris\u00b7ten\u00b7see\u00b7len", "w\u00fcrgt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch diesen Leib erweckst du wieder", "tokens": ["Auch", "die\u00b7sen", "Leib", "er\u00b7weckst", "du", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schreckst Tyrannen mit Gericht.", "tokens": ["Und", "schreckst", "Ty\u00b7ran\u00b7nen", "mit", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Gott! wie gut ist's, dir vertrauen!", "tokens": ["O", "Gott", "!", "wie", "gut", "ist's", ",", "dir", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf F\u00fcrsten, den erh\u00f6hten Staub,", "tokens": ["Auf", "F\u00fcrs\u00b7ten", ",", "den", "er\u00b7h\u00f6h\u00b7ten", "Staub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt' ich den Thurm der Hoffnung bauen?", "tokens": ["Sollt'", "ich", "den", "Thurm", "der", "Hoff\u00b7nung", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Fleisch, des Wurms gewissen Raub?", "tokens": ["Auf", "Fleisch", ",", "des", "Wurms", "ge\u00b7wis\u00b7sen", "Raub", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "