{"textgrid.poem.26309": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Schrei der Abendstunde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist mein Abendfriede?", "tokens": ["Wo", "ist", "mein", "A\u00b7bend\u00b7frie\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vernichtend naht die Nacht.", "tokens": ["Ver\u00b7nich\u00b7tend", "naht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich suche nach einem Liede.", "tokens": ["Ich", "su\u00b7che", "nach", "ei\u00b7nem", "Lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich suche nach deinen H\u00e4nden,", "tokens": ["Ich", "su\u00b7che", "nach", "dei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach Gedanken, die du gedacht,", "tokens": ["Nach", "Ge\u00b7dan\u00b7ken", ",", "die", "du", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Stille stockt an den W\u00e4nden.", "tokens": ["Die", "Stil\u00b7le", "stockt", "an", "den", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Schrei liegt mir im Munde,", "tokens": ["Ein", "Schrei", "liegt", "mir", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe ihn lange bewacht,", "tokens": ["Ich", "ha\u00b7be", "ihn", "lan\u00b7ge", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Den Schrei der Abendstunde.", "tokens": ["Den", "Schrei", "der", "A\u00b7bends\u00b7tun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}