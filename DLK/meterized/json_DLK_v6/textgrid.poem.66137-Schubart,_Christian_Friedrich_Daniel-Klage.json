{"textgrid.poem.66137": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Cedernbaum von Libanon,", "tokens": ["Kein", "Ce\u00b7dern\u00b7baum", "von", "Li\u00b7ba\u00b7non", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Lorbeer aus Apollos Haine,", "tokens": ["Kein", "Lor\u00b7beer", "aus", "A\u00b7pol\u00b7los", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Pindus und kein Helikon", "tokens": ["Kein", "Pin\u00b7dus", "und", "kein", "He\u00b7li\u00b7kon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum G\u00f6ttersitze f\u00fcr die Neune \u2013", "tokens": ["Zum", "G\u00f6t\u00b7ter\u00b7sit\u00b7ze", "f\u00fcr", "die", "Neu\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Tempe bl\u00fchet um mich her,", "tokens": ["Kein", "Tem\u00b7pe", "bl\u00fc\u00b7het", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Sch\u00e4ferland, der Zeiten Wunder;", "tokens": ["Kein", "Sch\u00e4\u00b7fer\u00b7land", ",", "der", "Zei\u00b7ten", "Wun\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Himmel h\u00e4ngt von Donnern schwer,", "tokens": ["Mein", "Him\u00b7mel", "h\u00e4ngt", "von", "Don\u00b7nern", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwarz wie die Nacht, auf mich herunter.", "tokens": ["Schwarz", "wie", "die", "Nacht", ",", "auf", "mich", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Aus keinem dunklen Busche wallt", "tokens": ["Aus", "kei\u00b7nem", "dunk\u00b7len", "Bu\u00b7sche", "wallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr mich ein Lied der Philomele,", "tokens": ["F\u00fcr", "mich", "ein", "Lied", "der", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Tr\u00fcmmern alter Schl\u00f6sser schallt", "tokens": ["Aus", "Tr\u00fcm\u00b7mern", "al\u00b7ter", "Schl\u00f6s\u00b7ser", "schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Uhus Gram aus rauher Kehle.", "tokens": ["Des", "U\u00b7hus", "Gram", "aus", "rau\u00b7her", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nie seh' ich, falscher Bacchus, dich", "tokens": ["Nie", "seh'", "ich", ",", "fal\u00b7scher", "Bac\u00b7chus", ",", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NE", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der krystallnen Flasche winken,", "tokens": ["Aus", "der", "krys\u00b7tall\u00b7nen", "Fla\u00b7sche", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Fischen mu\u00df ich k\u00fcmmerlich", "tokens": ["Mit", "Fi\u00b7schen", "mu\u00df", "ich", "k\u00fcm\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wasser meinen Durst vertrinken.", "tokens": ["Im", "Was\u00b7ser", "mei\u00b7nen", "Durst", "ver\u00b7trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Statt weicher Rasen sitze ich", "tokens": ["Statt", "wei\u00b7cher", "Ra\u00b7sen", "sit\u00b7ze", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf zugespitzten harten Felsen,", "tokens": ["Auf", "zu\u00b7ge\u00b7spitz\u00b7ten", "har\u00b7ten", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und seh' es, wie sich f\u00fcrchterlich", "tokens": ["Und", "seh'", "es", ",", "wie", "sich", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Str\u00f6me in die Th\u00e4ler w\u00e4lzen.", "tokens": ["Die", "Str\u00f6\u00b7me", "in", "die", "Th\u00e4\u00b7ler", "w\u00e4l\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn giftgeschwollne Hydern mir", "tokens": ["Wenn", "gift\u00b7ge\u00b7schwoll\u00b7ne", "Hy\u00b7dern", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dreigespitzte Zunge weisen;", "tokens": ["Die", "drei\u00b7ge\u00b7spitz\u00b7te", "Zun\u00b7ge", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So flieh' ich, da\u00df die Dornen mir", "tokens": ["So", "flieh'", "ich", ",", "da\u00df", "die", "Dor\u00b7nen", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fleisch von meinen Fersen rei\u00dfen.", "tokens": ["Das", "Fleisch", "von", "mei\u00b7nen", "Fer\u00b7sen", "rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich trage \u2013 Schwei\u00df im Angesicht!", "tokens": ["Ich", "tra\u00b7ge", "\u2013", "Schwei\u00df", "im", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Last herkulischer Gesch\u00e4fte;", "tokens": ["Die", "Last", "her\u00b7ku\u00b7li\u00b7scher", "Ge\u00b7sch\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Herkuls Keule hab' ich nicht,", "tokens": ["Nur", "Her\u00b7kuls", "Keu\u00b7le", "hab'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seiner Schultern Riesenkr\u00e4fte.", "tokens": ["Und", "sei\u00b7ner", "Schul\u00b7tern", "Rie\u00b7sen\u00b7kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch mitten in dem Kummer braust", "tokens": ["Doch", "mit\u00b7ten", "in", "dem", "Kum\u00b7mer", "braust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meiner Brust olympisch Feuer,", "tokens": ["In", "mei\u00b7ner", "Brust", "o\u00b7lym\u00b7pisch", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcrmend schl\u00e4gt die k\u00fchne Faust", "tokens": ["Und", "st\u00fcr\u00b7mend", "schl\u00e4gt", "die", "k\u00fch\u00b7ne", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Silbersaiten meiner Leier.", "tokens": ["Die", "Sil\u00b7ber\u00b7sai\u00b7ten", "mei\u00b7ner", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Oft t\u00f6nt sie wie ein Wetter laut,", "tokens": ["Oft", "t\u00f6nt", "sie", "wie", "ein", "Wet\u00b7ter", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um meine Furcht zu \u00fcberschreien,", "tokens": ["Um", "mei\u00b7ne", "Furcht", "zu", "\u00fc\u00b7bersc\u00b7hrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn der erschrocknen Seele graut", "tokens": ["Wenn", "der", "er\u00b7schrock\u00b7nen", "See\u00b7le", "graut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor St\u00fcrmen, die ihr z\u00fcrnend dr\u00e4uen.", "tokens": ["Vor", "St\u00fcr\u00b7men", ",", "die", "ihr", "z\u00fcr\u00b7nend", "dr\u00e4u\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn zahlreich, wie ein Bienenschwarm,", "tokens": ["Wenn", "zahl\u00b7reich", ",", "wie", "ein", "Bie\u00b7nen\u00b7schwarm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Narren sich um mich verbreiten;", "tokens": ["Die", "Nar\u00b7ren", "sich", "um", "mich", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So zupft ein Satyr mich beim Arm", "tokens": ["So", "zupft", "ein", "Sa\u00b7tyr", "mich", "beim", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deutet komisch auf die Saiten.", "tokens": ["Und", "deu\u00b7tet", "ko\u00b7misch", "auf", "die", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dann brummt der Ba\u00df zu ihrer Schmach", "tokens": ["Dann", "brummt", "der", "Ba\u00df", "zu", "ih\u00b7rer", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Juvenalischen Ges\u00e4ngen:", "tokens": ["In", "Ju\u00b7ve\u00b7na\u00b7li\u00b7schen", "Ge\u00b7s\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Satyr gei\u00dfelt hinten nach,", "tokens": ["Der", "Sa\u00b7tyr", "gei\u00b7\u00dfelt", "hin\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lehrt die fetten Thoren springen.", "tokens": ["Und", "lehrt", "die", "fet\u00b7ten", "Tho\u00b7ren", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch schleicht die Elegie mir nach", "tokens": ["Doch", "schleicht", "die", "E\u00b7le\u00b7gie", "mir", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rothem Aug' und starren F\u00fc\u00dfen;", "tokens": ["Mit", "ro\u00b7them", "Aug'", "und", "star\u00b7ren", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So lass' ich einen Thr\u00e4nenbach,", "tokens": ["So", "lass'", "ich", "ei\u00b7nen", "Thr\u00e4\u00b7nen\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Blut aus Abels Wunde, flie\u00dfen.", "tokens": ["Wie", "Blut", "aus", "A\u00b7bels", "Wun\u00b7de", ",", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "F\u00e4llt einst mein Trost, die Leier, mir", "tokens": ["F\u00e4llt", "einst", "mein", "Trost", ",", "die", "Lei\u00b7er", ",", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus zitternden entnervten H\u00e4nden;", "tokens": ["Aus", "zit\u00b7tern\u00b7den", "ent\u00b7nerv\u00b7ten", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mag Apoll an Gr\u00e4bern hier", "tokens": ["So", "mag", "A\u00b7poll", "an", "Gr\u00e4\u00b7bern", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NE", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein durchgeseufztes Leben enden.", "tokens": ["Mein", "durch\u00b7ge\u00b7seufz\u00b7tes", "Le\u00b7ben", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}