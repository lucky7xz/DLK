{"textgrid.poem.57622": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Entschuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mensch und bin ein Mann \u2013", "tokens": ["Ich", "bin", "ein", "Mensch", "und", "bin", "ein", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dagegen ich wenig machen kann.", "tokens": ["Da\u00b7ge\u00b7gen", "ich", "we\u00b7nig", "ma\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Folgen davon sind schlimm genug \u2013", "tokens": ["Die", "Fol\u00b7gen", "da\u00b7von", "sind", "schlimm", "ge\u00b7nug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "ADJD", "ADV", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer liest eines Mannes und Menschen Buch?", "tokens": ["Wer", "liest", "ei\u00b7nes", "Man\u00b7nes", "und", "Men\u00b7schen", "Buch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nur Engel und Weiber, die liebt man heut,", "tokens": ["Nur", "En\u00b7gel", "und", "Wei\u00b7ber", ",", "die", "liebt", "man", "heut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "PRELS", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch nicht den Mann und die Menschlichkeit.", "tokens": ["Doch", "nicht", "den", "Mann", "und", "die", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich sah das ein und habe mit Macht", "tokens": ["Ich", "sah", "das", "ein", "und", "ha\u00b7be", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PTKVZ", "KON", "VAFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die modernsten Gef\u00fchlchen in Reimchen gebracht.", "tokens": ["Die", "mo\u00b7derns\u00b7ten", "Ge\u00b7f\u00fchl\u00b7chen", "in", "Reim\u00b7chen", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch ach, das wurde ein kl\u00e4gliches Zeug,", "tokens": ["Doch", "ach", ",", "das", "wur\u00b7de", "ein", "kl\u00e4g\u00b7li\u00b7ches", "Zeug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Wasser so d\u00fcnn, wie Semmel so weich.", "tokens": ["Wie", "Was\u00b7ser", "so", "d\u00fcnn", ",", "wie", "Sem\u00b7mel", "so", "weich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "PWAV", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich hege daf\u00fcr keine Schw\u00e4rmerein,", "tokens": ["Ich", "he\u00b7ge", "da\u00b7f\u00fcr", "kei\u00b7ne", "Schw\u00e4r\u00b7me\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich e\u00df lieber Schwarzbrot und trink' lieber Wein.", "tokens": ["Ich", "e\u00df", "lie\u00b7ber", "Schwarz\u00b7brot", "und", "trink'", "lie\u00b7ber", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Nach kurzer Zeit bin ich abgezehrt", "tokens": ["Nach", "kur\u00b7zer", "Zeit", "bin", "ich", "ab\u00b7ge\u00b7zehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur gewohnten Kost zur\u00fcckgekehrt.", "tokens": ["Zur", "ge\u00b7wohn\u00b7ten", "Kost", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Denn stell' ich es noch so k\u00fcnstlich an \u2013", "tokens": ["Denn", "stell'", "ich", "es", "noch", "so", "k\u00fcnst\u00b7lich", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bleib' ein Mensch, ich bleib' ein Mann.", "tokens": ["Ich", "bleib'", "ein", "Mensch", ",", "ich", "bleib'", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}