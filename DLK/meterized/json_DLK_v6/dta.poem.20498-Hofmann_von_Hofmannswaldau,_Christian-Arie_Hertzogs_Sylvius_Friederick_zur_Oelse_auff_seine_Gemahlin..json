{"dta.poem.20498": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Arie  \n Hertzogs Sylvius Friederick zur  \n Oelse auff seine Gemahlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach z\u00fcrne nicht/ erlauchte Sylvia/", "tokens": ["Ach", "z\u00fcr\u00b7ne", "nicht", "/", "er\u00b7lauch\u00b7te", "Syl\u00b7via", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$(", "VVFIN", "NE", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df dich ein mund/ der irrdisch ist/ will k\u00fcssen/", "tokens": ["Da\u00df", "dich", "ein", "mund", "/", "der", "irr\u00b7disch", "ist", "/", "will", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein eigner trieb selbst trieb mich nicht so nah/", "tokens": ["Mein", "eig\u00b7ner", "trieb", "selbst", "trieb", "mich", "nicht", "so", "nah", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des himmels schlu\u00df legt mich zu deinen f\u00fcssen/", "tokens": ["Des", "him\u00b7mels", "schlu\u00df", "legt", "mich", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der mich verblend aus dem gewichte reist/", "tokens": ["Der", "mich", "ver\u00b7blend", "aus", "dem", "ge\u00b7wich\u00b7te", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und lieben heist.", "tokens": ["Und", "lie\u00b7ben", "heist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich habe gnug dir diese noth verh\u00f6lt/", "tokens": ["Ich", "ha\u00b7be", "gnug", "dir", "die\u00b7se", "noth", "ver\u00b7h\u00f6lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie aber kan das hertze l\u00e4nger schweigen?", "tokens": ["Wie", "a\u00b7ber", "kan", "das", "hert\u00b7ze", "l\u00e4n\u00b7ger", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was das gesicht in der gestalt erzehlt/", "tokens": ["Was", "das", "ge\u00b7sicht", "in", "der", "ge\u00b7stalt", "er\u00b7zehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df auch der mund mit blassen lippen zeigen/", "tokens": ["Mu\u00df", "auch", "der", "mund", "mit", "blas\u00b7sen", "lip\u00b7pen", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach Sylvia! la\u00df dir das m\u00fcde flehn", "tokens": ["Ach", "Syl\u00b7via", "!", "la\u00df", "dir", "das", "m\u00fc\u00b7de", "flehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "VVIMP", "PPER", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zu hertzen gehn.", "tokens": ["Zu", "hert\u00b7zen", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Kan ich davor/ da\u00df ich entz\u00fcndet bin?", "tokens": ["Kan", "ich", "da\u00b7vor", "/", "da\u00df", "ich", "ent\u00b7z\u00fcn\u00b7det", "bin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein heisser blick hat diesen brand erwecket;", "tokens": ["Dein", "heis\u00b7ser", "blick", "hat", "die\u00b7sen", "brand", "er\u00b7we\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem blick war auch mein hertze hin/", "tokens": ["Mit", "ei\u00b7nem", "blick", "war", "auch", "mein", "hert\u00b7ze", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer wei\u00df/ das feur in schnee und marmor stecket?", "tokens": ["Wer", "wei\u00df", "/", "das", "feur", "in", "schnee", "und", "mar\u00b7mor", "ste\u00b7cket", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ART", "NN", "APPR", "ADJA", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich hatte kaum ein aug auff dich gewand/", "tokens": ["Ich", "hat\u00b7te", "kaum", "ein", "aug", "auff", "dich", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und war entbrandt.", "tokens": ["Und", "war", "ent\u00b7brandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ach Sylvia! erwege was es sey/", "tokens": ["Ach", "Syl\u00b7via", "!", "er\u00b7we\u00b7ge", "was", "es", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PWS", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu seiner lust unschuld\u2019ge seelen brennen/", "tokens": ["Zu", "sei\u00b7ner", "lust", "un\u00b7schuld'\u00b7ge", "see\u00b7len", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich war vorhin von diesen z\u00fcgen frey/", "tokens": ["Ich", "war", "vor\u00b7hin", "von", "die\u00b7sen", "z\u00fc\u00b7gen", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir hab ich sie erstlich lernen kennen/", "tokens": ["Mit", "dir", "hab", "ich", "sie", "erst\u00b7lich", "ler\u00b7nen", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPER", "ADJD", "VVINF", "VVINF", "$("], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So hasse denn nicht/ was du hast gemacht/", "tokens": ["So", "has\u00b7se", "denn", "nicht", "/", "was", "du", "hast", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "$(", "PWS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und auffgebracht.", "tokens": ["Und", "auff\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der himmel wei\u00df von keiner h\u00e4rtligkeit/", "tokens": ["Der", "him\u00b7mel", "wei\u00df", "von", "kei\u00b7ner", "h\u00e4rt\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er schl\u00e4gt zwar ein/ doch l\u00f6scht er auch mit regen/", "tokens": ["Er", "schl\u00e4gt", "zwar", "ein", "/", "doch", "l\u00f6scht", "er", "auch", "mit", "re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist ihm gleich/ du ausbund dieser zeit/", "tokens": ["Du", "bist", "ihm", "gleich", "/", "du", "aus\u00b7bund", "die\u00b7ser", "zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df dich doch auch zu gleicher lust bewegen/", "tokens": ["La\u00df", "dich", "doch", "auch", "zu", "glei\u00b7cher", "lust", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die h\u00f6lle lehrt was w\u00fctet und zerbricht/", "tokens": ["Die", "h\u00f6l\u00b7le", "lehrt", "was", "w\u00fc\u00b7tet", "und", "zer\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der himmel nicht.", "tokens": ["Der", "him\u00b7mel", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die sonne selbst mischt sich den th\u00e4lern ein/", "tokens": ["Die", "son\u00b7ne", "selbst", "mischt", "sich", "den", "th\u00e4\u00b7lern", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wesen wird ber\u00fchmter auff der erden/", "tokens": ["Ihr", "we\u00b7sen", "wird", "be\u00b7r\u00fchm\u00b7ter", "auff", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kohle giebt dem spiegel glantz und schein/", "tokens": ["Die", "koh\u00b7le", "giebt", "dem", "spie\u00b7gel", "glantz", "und", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch mich kan nicht den werth gemindert werden/", "tokens": ["Durch", "mich", "kan", "nicht", "den", "werth", "ge\u00b7min\u00b7dert", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PTKNEG", "ART", "ADJD", "VVPP", "VAINF", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die seele/ die den irrdschen c\u00f6rper speist/", "tokens": ["Die", "see\u00b7le", "/", "die", "den", "irrd\u00b7schen", "c\u00f6r\u00b7per", "speist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bleibt doch ein geist.", "tokens": ["Bleibt", "doch", "ein", "geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Zwar ists zu viel ein blosser mensch zu seyn/", "tokens": ["Zwar", "ists", "zu", "viel", "ein", "blos\u00b7ser", "mensch", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKA", "PIS", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sich an dem/ was g\u00f6ttlich ist/ verlieben.", "tokens": ["Und", "sich", "an", "dem", "/", "was", "g\u00f6tt\u00b7lich", "ist", "/", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "$(", "PWS", "ADJD", "VAFIN", "$(", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach aber ach! GOtt giebt das lieben ein/", "tokens": ["Ach", "a\u00b7ber", "ach", "!", "Gott", "giebt", "das", "lie\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$.", "NN", "VVFIN", "PDS", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer widersteht den \u00fcberirrdschen trieben/", "tokens": ["Wer", "wi\u00b7der\u00b7steht", "den", "\u00fc\u00b7be\u00b7rirrd\u00b7schen", "trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein armer mensch hat nur von fleisch ein hertz/", "tokens": ["Ein", "ar\u00b7mer", "mensch", "hat", "nur", "von", "fleisch", "ein", "hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und nicht von ertz.", "tokens": ["Und", "nicht", "von", "ertz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die liebe sieht auch keine gleichheit an/", "tokens": ["Die", "lie\u00b7be", "sieht", "auch", "kei\u00b7ne", "gleich\u00b7heit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie mischet grei\u00df und perlen-schmuck zusammen/", "tokens": ["Sie", "mi\u00b7schet", "grei\u00df", "und", "per\u00b7len\u00b7schmuck", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NE", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Emma k\u00fcst hier ihren unterthan/", "tokens": ["Die", "Em\u00b7ma", "k\u00fcst", "hier", "ih\u00b7ren", "un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein sclave brennt in k\u00f6niglichen flammen;", "tokens": ["Ein", "scla\u00b7ve", "brennt", "in", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Veneri ist Adon nur ein knecht", "tokens": ["Und", "Ve\u00b7ne\u00b7ri", "ist", "A\u00b7don", "nur", "ein", "knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch nicht zu schlecht.", "tokens": ["Doch", "nicht", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und weist du nicht/ wem du gewalt gethan?", "tokens": ["Und", "weist", "du", "nicht", "/", "wem", "du", "ge\u00b7walt", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$(", "PWS