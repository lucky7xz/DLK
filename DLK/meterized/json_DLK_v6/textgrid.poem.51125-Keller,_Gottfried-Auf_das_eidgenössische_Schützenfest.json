{"textgrid.poem.51125": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf das eidgen\u00f6ssische Sch\u00fctzenfest", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Laube weht der Sommerwind", "tokens": ["Im", "Lau\u00b7be", "weht", "der", "Som\u00b7mer\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber das Halmenmeer,", "tokens": ["Und", "\u00fc\u00b7ber", "das", "Hal\u00b7men\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da naht mit ihrem Festgesind", "tokens": ["Da", "naht", "mit", "ih\u00b7rem", "Fest\u00b7ge\u00b7sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fahne Freudenschwer;", "tokens": ["Die", "Fah\u00b7ne", "Freu\u00b7den\u00b7schwer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da wallt das V\u00f6lklein Wohlgetan,", "tokens": ["Da", "wallt", "das", "V\u00f6l\u00b7klein", "Wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schalk zieht mit dem Biedermann", "tokens": ["Der", "Schalk", "zieht", "mit", "dem", "Bie\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Froh hinter ihr einher.", "tokens": ["Froh", "hin\u00b7ter", "ihr", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Halt! Steckt das Banner auf den Turm:", "tokens": ["Halt", "!", "Steckt", "das", "Ban\u00b7ner", "auf", "den", "Turm", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hie Schweizerland zehn Tag,", "tokens": ["Hie", "Schwei\u00b7zer\u00b7land", "zehn", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zehn Tage lang Gem\u00fctersturm", "tokens": ["Zehn", "Ta\u00b7ge", "lang", "Ge\u00b7m\u00fc\u00b7ter\u00b7sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vaterlandsgelag!", "tokens": ["Und", "Va\u00b7ter\u00b7lands\u00b7ge\u00b7lag", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch in der Brandung lautem Spiel", "tokens": ["Doch", "in", "der", "Bran\u00b7dung", "lau\u00b7tem", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sucht still der Sch\u00fctz sein altes Ziel,", "tokens": ["Sucht", "still", "der", "Sch\u00fctz", "sein", "al\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sch\u00fctz vom alten Schlag.", "tokens": ["Der", "Sch\u00fctz", "vom", "al\u00b7ten", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr andern aber heuchelt nicht", "tokens": ["Ihr", "an\u00b7dern", "a\u00b7ber", "heu\u00b7chelt", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADV", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gebt euch, wie ihr seid,", "tokens": ["Und", "gebt", "euch", ",", "wie", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eh das Herz vor Schweigen bricht,", "tokens": ["Und", "eh", "das", "Herz", "vor", "Schwei\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verk\u00fcndet euer Leid!", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "eu\u00b7er", "Leid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Weise spreche warm erregt,", "tokens": ["Der", "Wei\u00b7se", "spre\u00b7che", "warm", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schw\u00e4tzer schwatze tief bewegt", "tokens": ["Der", "Schw\u00e4t\u00b7zer", "schwat\u00b7ze", "tief", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seinem Narrenkleid!.", "tokens": ["In", "sei\u00b7nem", "Nar\u00b7ren\u00b7kleid", "!", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und z\u00fcrnt ihr, sei die Hand geballt", "tokens": ["Und", "z\u00fcrnt", "ihr", ",", "sei", "die", "Hand", "ge\u00b7ballt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von echtem Freundeszorn:", "tokens": ["Von", "ech\u00b7tem", "Freun\u00b7des\u00b7zorn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie \u00f6ffnet sich, sobald erschallt", "tokens": ["Sie", "\u00f6ff\u00b7net", "sich", ",", "so\u00b7bald", "er\u00b7schallt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das alte Wunderhorn!", "tokens": ["Das", "al\u00b7te", "Wun\u00b7der\u00b7horn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir d\u00fcrsten all nach ", "tokens": ["Wir", "d\u00fcrs\u00b7ten", "all", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und baden alle, wenn wir krank,", "tokens": ["Und", "ba\u00b7den", "al\u00b7le", ",", "wenn", "wir", "krank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Die Freiheit gibt sich nicht in Pacht,", "tokens": ["Die", "Frei\u00b7heit", "gibt", "sich", "nicht", "in", "Pacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie folgt nicht ", "tokens": ["Sie", "folgt", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG"], "meter": "++-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und hat noch immer den verlacht,", "tokens": ["Und", "hat", "noch", "im\u00b7mer", "den", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der sie zu fangen sann,", "tokens": ["Der", "sie", "zu", "fan\u00b7gen", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das einz'ge Weib, dem gut es steht,", "tokens": ["Das", "einz'\u00b7ge", "Weib", ",", "dem", "gut", "es", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn es mit tausend M\u00e4nnern geht,", "tokens": ["Wenn", "es", "mit", "tau\u00b7send", "M\u00e4n\u00b7nern", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vertraut in Ring und Bann.", "tokens": ["Ver\u00b7traut", "in", "Ring", "und", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die wilden Rosen auf dem Hut,", "tokens": ["Die", "wil\u00b7den", "Ro\u00b7sen", "auf", "dem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sie die Augen gehn;", "tokens": ["L\u00e4\u00dft", "sie", "die", "Au\u00b7gen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann braust verwirrt der M\u00e4nner Blut,", "tokens": ["Dann", "braust", "ver\u00b7wirrt", "der", "M\u00e4n\u00b7ner", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie sie doppelt sehn.", "tokens": ["Da\u00df", "sie", "sie", "dop\u00b7pelt", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie das Volk im Streite ringt,", "tokens": ["Und", "wie", "das", "Volk", "im", "Strei\u00b7te", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ordnend ihre Fahne schwingt", "tokens": ["Sie", "ord\u00b7nend", "ih\u00b7re", "Fah\u00b7ne", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4\u00dft's im Reihn sich drehn.", "tokens": ["Und", "l\u00e4\u00dft's", "im", "Reihn", "sich", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun seid gegr\u00fc\u00dfet Mann f\u00fcr Mann,", "tokens": ["Nun", "seid", "ge\u00b7gr\u00fc\u00b7\u00dfet", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Festfanfare schallt!", "tokens": ["Die", "Fest\u00b7fan\u00b7fa\u00b7re", "schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun treib es jeder, wie er's kann,", "tokens": ["Nun", "treib", "es", "je\u00b7der", ",", "wie", "er's", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PWAV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Rufer in den Wald!", "tokens": ["Ein", "Ru\u00b7fer", "in", "den", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Getrost verge\u00dft des Tages Not,", "tokens": ["Ge\u00b7trost", "ver\u00b7ge\u00dft", "des", "Ta\u00b7ges", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df im zehnten Abendrot", "tokens": ["Bis", "da\u00df", "im", "zehn\u00b7ten", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der letzte Schu\u00df verhallt!", "tokens": ["Der", "letz\u00b7te", "Schu\u00df", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}