{"textgrid.poem.41309": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit ist dein Wunsch! Kaum trau'ich dem Entschlu\u00df.", "tokens": ["Die", "Frei\u00b7heit", "ist", "dein", "Wunsch", "!", "Kaum", "trau'\u00b7ich", "dem", "Ent\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lern' und vernimm von mir, wie man sie suchen mu\u00df.", "tokens": ["Lern'", "und", "ver\u00b7nimm", "von", "mir", ",", "wie", "man", "sie", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PPER", "$,", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Lachst du, wann Jourdains Stolz und Cadenas sich weisen,", "tokens": ["Lachst", "du", ",", "wann", "Jour\u00b7dains", "Stolz", "und", "Ca\u00b7de\u00b7nas", "sich", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NE", "NN", "KON", "NE", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sein erhabnes Mahl? Kannst du zu Hause speisen,", "tokens": ["Und", "sein", "er\u00b7hab\u00b7nes", "Mahl", "?", "Kannst", "du", "zu", "Hau\u00b7se", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und niemals andrer Gast und Tischgefangner sein?", "tokens": ["Und", "nie\u00b7mals", "an\u00b7drer", "Gast", "und", "Tischge\u00b7fang\u00b7ner", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Befriedigt deinen Durst ein kleiner Frankenwein?", "tokens": ["Be\u00b7frie\u00b7digt", "dei\u00b7nen", "Durst", "ein", "klei\u00b7ner", "Fran\u00b7ken\u00b7wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll dir ein sittsam Tuch, wie mir, zur Kleidung dienen?", "tokens": ["Soll", "dir", "ein", "sitt\u00b7sam", "Tuch", ",", "wie", "mir", ",", "zur", "Klei\u00b7dung", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "NN", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergn\u00fcgen deinen Ku\u00df die billigen Nerinen?", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "dei\u00b7nen", "Ku\u00df", "die", "bil\u00b7li\u00b7gen", "Ne\u00b7ri\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Stellt dein beredtes Gold nie den Statiren nach?", "tokens": ["Stellt", "dein", "be\u00b7red\u00b7tes", "Gold", "nie", "den", "Sta\u00b7ti\u00b7ren", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Beherbergt, ohne Neid, auch dich ein niedres Dach?", "tokens": ["Be\u00b7her\u00b7bergt", ",", "oh\u00b7ne", "Neid", ",", "auch", "dich", "ein", "nie\u00b7dres", "Dach", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "NN", "$,", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Freund, ist dein Muth so stark, ist dir nur Freiheit theuer;", "tokens": ["Freund", ",", "ist", "dein", "Muth", "so", "stark", ",", "ist", "dir", "nur", "Frei\u00b7heit", "theu\u00b7er", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "NN", "ADJD", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "So lebst du sonder Zwang, und kein Monarch lebt freier.", "tokens": ["So", "lebst", "du", "son\u00b7der", "Zwang", ",", "und", "kein", "Mon\u00b7arch", "lebt", "frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}