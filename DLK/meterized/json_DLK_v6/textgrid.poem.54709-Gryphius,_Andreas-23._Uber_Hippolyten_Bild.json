{"textgrid.poem.54709": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "23. Uber Hippolyten Bild", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schin ", "tokens": ["So", "schin"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Tugend Ebenbild/ die sch\u00f6nste Freundligkeit/", "tokens": ["Der", "Tu\u00b7gend", "E\u00b7ben\u00b7bild", "/", "die", "sch\u00f6ns\u00b7te", "Freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schin sie kurtz zuvor/ eh sie die Welt gesegnet/", "tokens": ["So", "schin", "sie", "kurtz", "zu\u00b7vor", "/", "eh", "sie", "die", "Welt", "ge\u00b7seg\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrem Br\u00e4utigam/ der vor sie starb/ begegnet.", "tokens": ["Und", "ih\u00b7rem", "Br\u00e4u\u00b7ti\u00b7gam", "/", "der", "vor", "sie", "starb", "/", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "APPR", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ieder siht di\u00df Bild mit Lust und Wunder an/", "tokens": ["Ein", "ie\u00b7der", "siht", "di\u00df", "Bild", "mit", "Lust", "und", "Wun\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PDS", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht ich der ich di\u00df Bild nur itzt noch sehen kan.", "tokens": ["Nicht", "ich", "der", "ich", "di\u00df", "Bild", "nur", "itzt", "noch", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "PPER", "PDS", "NN", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}