{"dta.poem.157": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als   Aurora   \u00fcber ihren verstorbenen Hund  \n weinete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste G\u00f6ttin meiner Seelen haltet doch mit Weinen ein/", "tokens": ["Sch\u00f6ns\u00b7te", "G\u00f6t\u00b7tin", "mei\u00b7ner", "See\u00b7len", "hal\u00b7tet", "doch", "mit", "Wei\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "ART", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ach! stopfft zu der Augen Brunnen/ und la\u00dft doch eur", "tokens": ["Ach", "!", "stopfft", "zu", "der", "Au\u00b7gen", "Brun\u00b7nen", "/", "und", "la\u00dft", "doch", "eur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "ART", "NN", "NN", "$(", "KON", "VVIMP", "ADV", "PPOSAT"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Klagen seyn.", "tokens": ["Kla\u00b7gen", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Alsdenn habt ihr einen Schatz/ der fast unsch\u00e4tzbahr/ verheeret.", "tokens": ["Als\u00b7denn", "habt", "ihr", "ei\u00b7nen", "Schatz", "/", "der", "fast", "un\u00b7sch\u00e4tz\u00b7bahr", "/", "ver\u00b7hee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$(", "ART", "ADV", "ADJD", "$(", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Doch es machten mich die Thr\u00e4nen mehr de\u00f1 \u00fcberaus begl\u00fcckt/", "tokens": ["Doch", "es", "mach\u00b7ten", "mich", "die", "Thr\u00e4\u00b7nen", "mehr", "de\u00f1", "\u00fc\u00b7be\u00b7raus", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wenn ihr sie um meinent wegen in die weite Welt geschickt:", "tokens": ["Wenn", "ihr", "sie", "um", "mei\u00b7nent", "we\u00b7gen", "in", "die", "wei\u00b7te", "Welt", "ge\u00b7schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Aber ihr vergiesset sie nur um eines Hundes wegen/", "tokens": ["A\u00b7ber", "ihr", "ver\u00b7gies\u00b7set", "sie", "nur", "um", "ei\u00b7nes", "Hun\u00b7des", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Da sie doch an Kostbarkeiten K\u00f6nigreichen \u00fcberlegen.", "tokens": ["Da", "sie", "doch", "an", "Kost\u00b7bar\u00b7kei\u00b7ten", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und dieselbe/ der ihr gleichet/ nur da\u00df ihr weit sch\u00f6ner glei\u00dft/", "tokens": ["Und", "die\u00b7sel\u00b7be", "/", "der", "ihr", "glei\u00b7chet", "/", "nur", "da\u00df", "ihr", "weit", "sch\u00f6\u00b7ner", "glei\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "$(", "PRELS", "PPER", "VVFIN", "$(", "ADV", "KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Deren Nahme so wie eurer/ ihr wie sie ", "tokens": ["De\u00b7ren", "Nah\u00b7me", "so", "wie", "eu\u00b7rer", "/", "ihr", "wie", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "ADV", "KOKOM", "PPOSAT", "$(", "PPOSAT", "PWAV", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "L\u00e4\u00dft aus ihrem G\u00f6tter-Mund viele heisse Seuffzers gehen/", "tokens": ["L\u00e4\u00dft", "aus", "ih\u00b7rem", "G\u00f6t\u00b7ter\u00b7Mund", "vie\u00b7le", "heis\u00b7se", "Seuff\u00b7zers", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Ja sie weinet wenn sie fr\u00fch von dem Bette auf wil stehen.", "tokens": ["Ja", "sie", "wei\u00b7net", "wenn", "sie", "fr\u00fch", "von", "dem", "Bet\u00b7te", "auf", "wil", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "APPR", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Aber nur des Liebsten halber/ nicht wie ihr um einen Hund/", "tokens": ["A\u00b7ber", "nur", "des", "Liebs\u00b7ten", "hal\u00b7ber", "/", "nicht", "wie", "ihr", "um", "ei\u00b7nen", "Hund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPO", "$(", "PTKNEG", "PWAV", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Jhre Thr\u00e4nen thun was Grosses/ eure was geringes/ kund.", "tokens": ["Ih\u00b7re", "Thr\u00e4\u00b7nen", "thun", "was", "Gros\u00b7ses", "/", "eu\u00b7re", "was", "ge\u00b7rin\u00b7ges", "/", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PWS", "NN", "$(", "PPOSAT", "PWS", "ADJA", "$(", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wenn nun eure Augen auch/ so wie jene/ sollen weinen/", "tokens": ["Wenn", "nun", "eu\u00b7re", "Au\u00b7gen", "auch", "/", "so", "wie", "je\u00b7ne", "/", "sol\u00b7len", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "$(", "ADV", "KOKOM", "PDS", "$(", "VMFIN", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Alsdenn mu\u00df eur Hertz sich nicht gegen mich verhelffenbeinen/", "tokens": ["Als\u00b7denn", "mu\u00df", "eur", "Hertz", "sich", "nicht", "ge\u00b7gen", "mich", "ver\u00b7hel\u00b7ffen\u00b7bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "APPR", "PPER", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Eure Freundschafft mu\u00df dem helffen/ der sonst sein Verderben", "tokens": ["Eu\u00b7re", "Freund\u00b7schafft", "mu\u00df", "dem", "helf\u00b7fen", "/", "der", "sonst", "sein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "VVINF", "$(", "ART", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "findt/", "tokens": ["findt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Und um solche m\u00fc\u00dft ihr weinen/ die der Thr\u00e4nen w\u00fcrdig sind/", "tokens": ["Und", "um", "sol\u00b7che", "m\u00fc\u00dft", "ihr", "wei\u00b7nen", "/", "die", "der", "Thr\u00e4\u00b7nen", "w\u00fcr\u00b7dig", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "PPER", "VVINF", "$(", "PRELS", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Nicht um einen der zur Lust macht/ da\u00df ihr der Possen lachet/", "tokens": ["Nicht", "um", "ei\u00b7nen", "der", "zur", "Lust", "macht", "/", "da\u00df", "ihr", "der", "Pos\u00b7sen", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ART", "APPRART", "NN", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Weinet doch nicht um den Hund die ihr Menschen sterben", "tokens": ["Wei\u00b7net", "doch", "nicht", "um", "den", "Hund", "die", "ihr", "Men\u00b7schen", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "ART", "PPOSAT", "NN", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.23": {"text": "machet.", "tokens": ["ma\u00b7chet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}