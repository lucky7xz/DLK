{"textgrid.poem.31533": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Mutter / die mein Schmerz / wie euch der eure / kr\u00e4nket!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Mutter / die mein Schmerz / wie euch der eure / kr\u00e4nket!", "tokens": ["Ach", "Mut\u00b7ter", "/", "die", "mein", "Schmerz", "/", "wie", "euch", "der", "eu\u00b7re", "/", "kr\u00e4n\u00b7ket", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "NN", "$(", "ART", "PPOSAT", "NN", "$(", "PWAV", "PPER", "ART", "PPOSAT", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verzeiht mirs / da\u00df ich mehr eur Heil als Freude such.", "tokens": ["ver\u00b7zeiht", "mirs", "/", "da\u00df", "ich", "mehr", "eur", "Heil", "als", "Freu\u00b7de", "such", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mu\u00df es thun / es steht also von mir im Buch.", "tokens": ["Ich", "mu\u00df", "es", "thun", "/", "es", "steht", "al\u00b7so", "von", "mir", "im", "Buch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$(", "PPER", "VVFIN", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Gnad' und eure S\u00fcnd / mich in di\u00df Elend senket.", "tokens": ["Mein", "Gnad'", "und", "eu\u00b7re", "S\u00fcnd", "/", "mich", "in", "di\u00df", "E\u00b7lend", "sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$(", "PPER", "APPR", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit ihr aber nicht euch gar verlassen denket /", "tokens": ["Da\u00b7mit", "ihr", "a\u00b7ber", "nicht", "euch", "gar", "ver\u00b7las\u00b7sen", "den\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPER", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so seht / da\u00df Sterbend euch versorgt mein Schaffungs-Spruch /", "tokens": ["so", "seht", "/", "da\u00df", "Ster\u00b7bend", "euch", "ver\u00b7sorgt", "mein", "Schaf\u00b7fungs\u00b7Spruch", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "so hab' ich / ob ich schon jetzt bin am Creutz ein Fluch /", "tokens": ["so", "hab'", "ich", "/", "ob", "ich", "schon", "jetzt", "bin", "am", "Creutz", "ein", "Fluch", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Johannes Herz zu euch / und eurs zu ihm / gelenket.", "tokens": ["Jo\u00b7han\u00b7nes", "Herz", "zu", "euch", "/", "und", "eurs", "zu", "ihm", "/", "ge\u00b7len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "$(", "KON", "NE", "APPR", "PPER", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd du / mein liebster Freund / wollst meiner Mutter pflegen /", "tokens": ["Vnd", "du", "/", "mein", "liebs\u00b7ter", "Freund", "/", "wollst", "mei\u00b7ner", "Mut\u00b7ter", "pfle\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "als der / in deren Leib ich diesen an mich nahm /", "tokens": ["als", "der", "/", "in", "de\u00b7ren", "Leib", "ich", "die\u00b7sen", "an", "mich", "nahm", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "APPR", "PRELAT", "NN", "PPER", "PDAT", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "in dem ich f\u00e4hig ward vor euer Heil zu sterben.", "tokens": ["in", "dem", "ich", "f\u00e4\u00b7hig", "ward", "vor", "eu\u00b7er", "Heil", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es kan / der Schmerzen Krafft / die Liebe nicht erlegen.", "tokens": ["Es", "kan", "/", "der", "Schmer\u00b7zen", "Krafft", "/", "die", "Lie\u00b7be", "nicht", "er\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ART", "NN", "NN", "$(", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Leid / ist auch ein Ast an diesem Creutzes-Stamm /", "tokens": ["Ihr", "Leid", "/", "ist", "auch", "ein", "Ast", "an", "die\u00b7sem", "Creut\u00b7zes\u00b7Stamm", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "an dem ich's Leben will / durch Sterben / euch erwerben.", "tokens": ["an", "dem", "ich's", "Le\u00b7ben", "will", "/", "durch", "Ster\u00b7ben", "/", "euch", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "$(", "APPR", "NN", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}