{"dta.poem.7531": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ring an, den Himmel mit der Erde auszugleichen!", "tokens": ["Ring", "an", ",", "den", "Him\u00b7mel", "mit", "der", "Er\u00b7de", "aus\u00b7zu\u00b7glei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wer das errungen hat, der tr\u00e4gt das Siegeszeichen.", "tokens": ["Wer", "das", "er\u00b7run\u00b7gen", "hat", ",", "der", "tr\u00e4gt", "das", "Sie\u00b7ges\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "'s ist keine Kunst, die Welt roh unter'n Fu\u00df zu treten;", "tokens": ["'s", "ist", "kei\u00b7ne", "Kunst", ",", "die", "Welt", "roh", "un\u00b7ter'n", "Fu\u00df", "zu", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "So zarte Blumen bl\u00fchn auf diesen Gartenbeeten.", "tokens": ["So", "zar\u00b7te", "Blu\u00b7men", "bl\u00fchn", "auf", "die\u00b7sen", "Gar\u00b7ten\u00b7bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist auch keine Kunst den Himmel f\u00fcr die Schwachen", "tokens": ["Es", "ist", "auch", "kei\u00b7ne", "Kunst", "den", "Him\u00b7mel", "f\u00fcr", "die", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einladend, und dem Trotz die H\u00f6lle hei\u00df zu machen.", "tokens": ["Ein\u00b7la\u00b7dend", ",", "und", "dem", "Trotz", "die", "H\u00f6l\u00b7le", "hei\u00df", "zu", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Himmel zieh herab, die Erd' empor mit Brunst!", "tokens": ["Den", "Him\u00b7mel", "zieh", "her\u00b7ab", ",", "die", "Erd'", "em\u00b7por", "mit", "Brunst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ART", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur dis, der Rede werth, ist Erdenhimmelskunst.", "tokens": ["Nur", "dis", ",", "der", "Re\u00b7de", "werth", ",", "ist", "Er\u00b7den\u00b7him\u00b7mels\u00b7kunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "ART", "NN", "ADJD", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}