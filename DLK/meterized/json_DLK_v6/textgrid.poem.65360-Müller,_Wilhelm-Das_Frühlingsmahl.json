{"textgrid.poem.65360": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Fr\u00fchlingsmahl", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat die wei\u00dfen T\u00fccher", "tokens": ["Wer", "hat", "die", "wei\u00b7\u00dfen", "T\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebreitet \u00fcber das Land?", "tokens": ["Ge\u00b7brei\u00b7tet", "\u00fc\u00b7ber", "das", "Land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die wei\u00dfen duftenden T\u00fccher", "tokens": ["Die", "wei\u00b7\u00dfen", "duf\u00b7ten\u00b7den", "T\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit ihrem gr\u00fcnen Rand?", "tokens": ["Mit", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Rand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und hat dar\u00fcber gezogen", "tokens": ["Und", "hat", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das hohe blaue Zelt?", "tokens": ["Das", "ho\u00b7he", "blau\u00b7e", "Zelt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darunter den bunten Teppich", "tokens": ["Da\u00b7run\u00b7ter", "den", "bun\u00b7ten", "Tep\u00b7pich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gelagert \u00fcber das Feld?", "tokens": ["Ge\u00b7la\u00b7gert", "\u00fc\u00b7ber", "das", "Feld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Er ist es selbst gewesen,", "tokens": ["Er", "ist", "es", "selbst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gute reiche Wirth", "tokens": ["Der", "gu\u00b7te", "rei\u00b7che", "Wirth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels und der Erden,", "tokens": ["Des", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der nimmer \u00e4rmer wird.", "tokens": ["Der", "nim\u00b7mer", "\u00e4r\u00b7mer", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er hat gedeckt die Tische", "tokens": ["Er", "hat", "ge\u00b7deckt", "die", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinem weiten Saal,", "tokens": ["In", "sei\u00b7nem", "wei\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruft was lebet und webet,", "tokens": ["Und", "ruft", "was", "le\u00b7bet", "und", "we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum gro\u00dfen Fr\u00fchlingsmahl.", "tokens": ["Zum", "gro\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie str\u00f6mt's aus allen Bl\u00fcthen", "tokens": ["Wie", "str\u00f6mt's", "aus", "al\u00b7len", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herab von Strauch und Baum!", "tokens": ["Her\u00b7ab", "von", "Strauch", "und", "Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Bl\u00fcth' ein Becher", "tokens": ["Und", "je\u00b7de", "Bl\u00fcth'", "ein", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll s\u00fc\u00dfer D\u00fcfte Schaum.", "tokens": ["Voll", "s\u00fc\u00b7\u00dfer", "D\u00fcf\u00b7te", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "H\u00f6rt ihr des Wirthes Stimme?", "tokens": ["H\u00f6rt", "ihr", "des", "Wirt\u00b7hes", "Stim\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heran, was kriecht und fliegt,", "tokens": ["He\u00b7ran", ",", "was", "kriecht", "und", "fliegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was geht und steht auf Erden,", "tokens": ["Was", "geht", "und", "steht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was unter den Wogen sich wiegt!", "tokens": ["Was", "un\u00b7ter", "den", "Wo\u00b7gen", "sich", "wiegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Und du, mein Himmelspilger,", "tokens": ["Und", "du", ",", "mein", "Him\u00b7mel\u00b7spil\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier trinke trunken dich,", "tokens": ["Hier", "trin\u00b7ke", "trun\u00b7ken", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sinke selig nieder", "tokens": ["Und", "sin\u00b7ke", "se\u00b7lig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf's Knie und denk' an mich!", "tokens": ["Auf's", "Knie", "und", "denk'", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}