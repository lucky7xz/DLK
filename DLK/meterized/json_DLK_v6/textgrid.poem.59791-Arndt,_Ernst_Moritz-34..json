{"textgrid.poem.59791": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du suchst der Dinge Grund \u2013 st\u00fcrz' in den Abgrund dich.", "tokens": ["Du", "suchst", "der", "Din\u00b7ge", "Grund", "\u2013", "st\u00fcrz'", "in", "den", "Ab\u00b7grund", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird da dein Fu\u00df nicht fest, ist nirgends Grund f\u00fcr dich;", "tokens": ["Wird", "da", "dein", "Fu\u00df", "nicht", "fest", ",", "ist", "nir\u00b7gends", "Grund", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,", "VAFIN", "ADV", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wagst du nicht ritterlich Verzweiflung und Verzagen,", "tokens": ["Wagst", "du", "nicht", "rit\u00b7ter\u00b7lich", "Ver\u00b7zwei\u00b7flung", "und", "Ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df doch lieber ab, nach Gott und Welt zu fragen:", "tokens": ["So", "la\u00df", "doch", "lie\u00b7ber", "ab", ",", "nach", "Gott", "und", "Welt", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Wissens Morgenrot wird nie dem Feigen tagen.", "tokens": ["Des", "Wis\u00b7sens", "Mor\u00b7gen\u00b7rot", "wird", "nie", "dem", "Fei\u00b7gen", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}