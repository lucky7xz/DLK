{"textgrid.poem.59228": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schreckenberger", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufs Wohlsein meiner Dame,", "tokens": ["Aufs", "Wohl\u00b7sein", "mei\u00b7ner", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eine Windfahn ist ihr Panier", "tokens": ["Ei\u00b7ne", "Wind\u00b7fahn", "ist", "ihr", "Pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fortuna ist ihr Name,", "tokens": ["For\u00b7tu\u00b7na", "ist", "ihr", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Lager ihr Quartier!", "tokens": ["Das", "La\u00b7ger", "ihr", "Quar\u00b7tier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wendet sie sich weiter,", "tokens": ["Und", "wen\u00b7det", "sie", "sich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich k\u00fcmmre mich nicht drum,", "tokens": ["Ich", "k\u00fcmm\u00b7re", "mich", "nicht", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da drau\u00dfen ohne Reiter,", "tokens": ["Da", "drau\u00b7\u00dfen", "oh\u00b7ne", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da geht die Welt so dumm.", "tokens": ["Da", "geht", "die", "Welt", "so", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Statt Pulverblitz und Knattern", "tokens": ["Statt", "Pul\u00b7ver\u00b7blitz", "und", "Knat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus jedem w\u00fcsten Haus", "tokens": ["Aus", "je\u00b7dem", "w\u00fcs\u00b7ten", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gevattern sehn und schnattern", "tokens": ["Ge\u00b7vat\u00b7tern", "sehn", "und", "schnat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alle Lust zum Land hinaus.", "tokens": ["Al\u00b7le", "Lust", "zum", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fortuna weint vor \u00c4rger,", "tokens": ["For\u00b7tu\u00b7na", "weint", "vor", "\u00c4r\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es rinnet Perl auf Perl.", "tokens": ["Es", "rin\u00b7net", "Perl", "auf", "Perl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwo ist der Schreckenberger?", "tokens": ["\u00bb", "wo", "ist", "der", "Schre\u00b7cken\u00b7ber\u00b7ger", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das war ein andrer Kerl.\u00ab", "tokens": ["Das", "war", "ein", "an\u00b7drer", "Kerl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie tut den Arm mir reichen,", "tokens": ["Sie", "tut", "den", "Arm", "mir", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fama bl\u00e4st das Geleit,", "tokens": ["Fa\u00b7ma", "bl\u00e4st", "das", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So zu dem Tempel steigen", "tokens": ["So", "zu", "dem", "Tem\u00b7pel", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wir der Unsterblichkeit.", "tokens": ["Wir", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}