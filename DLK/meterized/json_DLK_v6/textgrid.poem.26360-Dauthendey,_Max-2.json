{"textgrid.poem.26360": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Purpurschwarz klafft ein Moor. In die Heidefl\u00e4che funkelnd gestochen, g\u00e4hnt dunkel das Wasser.", "tokens": ["Pur\u00b7pur\u00b7schwarz", "klafft", "ein", "Moor", ".", "In", "die", "Hei\u00b7de\u00b7fl\u00e4\u00b7che", "fun\u00b7kelnd", "ge\u00b7sto\u00b7chen", ",", "g\u00e4hnt", "dun\u00b7kel", "das", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN", "ADJD", "VVPP", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+--+-+-+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Finster gebrochen br\u00fcten am Rand schwarze Erdschollen, schwarze verbrannte Erdschlacken,", "tokens": ["Fins\u00b7ter", "ge\u00b7bro\u00b7chen", "br\u00fc\u00b7ten", "am", "Rand", "schwar\u00b7ze", "Erd\u00b7schol\u00b7len", ",", "schwar\u00b7ze", "ver\u00b7brann\u00b7te", "Erd\u00b7schla\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "APPRART", "NN", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+--+-+--", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "Verwesungsfrost gellt \u00fcber das todschwarze Land.", "tokens": ["Ver\u00b7we\u00b7sungs\u00b7frost", "gellt", "\u00fc\u00b7ber", "das", "tod\u00b7schwar\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die M\u00e4gde, die Knaben schleichen gierig heran.", "tokens": ["Die", "M\u00e4g\u00b7de", ",", "die", "Kna\u00b7ben", "schlei\u00b7chen", "gie\u00b7rig", "he\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die Frauen zischeln und zaudern.", "tokens": ["Die", "Frau\u00b7en", "zi\u00b7scheln", "und", "zau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Verwirrt weichen die M\u00e4nner.", "tokens": ["Ver\u00b7wirrt", "wei\u00b7chen", "die", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Grinsender Grabhauch irrt durch die Abendluft,", "tokens": ["Grin\u00b7sen\u00b7der", "Grab\u00b7hauch", "irrt", "durch", "die", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-++-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Die Greise, niedergesunken, lauschen trunken, saugen verz\u00fcckt den eisigen Duft.", "tokens": ["Die", "Grei\u00b7se", ",", "nie\u00b7der\u00b7ge\u00b7sun\u00b7ken", ",", "lau\u00b7schen", "trun\u00b7ken", ",", "sau\u00b7gen", "ver\u00b7z\u00fcckt", "den", "ei\u00b7si\u00b7gen", "Duft", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVIZU", "$,", "VVFIN", "ADJD", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus den schwarzen Erdschollen, vom nachtbraunen Wasser", "tokens": ["Aus", "den", "schwar\u00b7zen", "Erd\u00b7schol\u00b7len", ",", "vom", "nacht\u00b7brau\u00b7nen", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "--+-+--+++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Rollen w\u00fchlende Stimmen, schw\u00fcle Ges\u00e4nge, und legen sich k\u00fchl", "tokens": ["Rol\u00b7len", "w\u00fch\u00b7len\u00b7de", "Stim\u00b7men", ",", "schw\u00fc\u00b7le", "Ge\u00b7s\u00e4n\u00b7ge", ",", "und", "le\u00b7gen", "sich", "k\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD"], "meter": "+-+--+-+--+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Um die nackten Leiber der M\u00e4gde, um die nackten Glieder der Knaben.", "tokens": ["Um", "die", "nack\u00b7ten", "Lei\u00b7ber", "der", "M\u00e4g\u00b7de", ",", "um", "die", "nack\u00b7ten", "Glie\u00b7der", "der", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "$,", "KOUI", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Mit wei\u00dfen Knien liegen sie nieder, die Knaben und M\u00e4gde,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Kni\u00b7en", "lie\u00b7gen", "sie", "nie\u00b7der", ",", "die", "Kna\u00b7ben", "und", "M\u00e4g\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dr\u00e4ngen begehrlich den keimblassen Leib in die k\u00fchlschwarze Erde,", "tokens": ["Dr\u00e4n\u00b7gen", "be\u00b7gehr\u00b7lich", "den", "keim\u00b7blas\u00b7sen", "Leib", "in", "die", "k\u00fchl\u00b7schwar\u00b7ze", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Pressen l\u00fcstern den Nacken, tauchen die Br\u00fcste, die wundhei\u00dfen Br\u00fcste", "tokens": ["Pres\u00b7sen", "l\u00fcs\u00b7tern", "den", "Na\u00b7cken", ",", "tau\u00b7chen", "die", "Br\u00fcs\u00b7te", ",", "die", "wund\u00b7hei\u00b7\u00dfen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "In das schwarzglei\u00dfende Wasser, zerrei\u00dfen die Erde,", "tokens": ["In", "das", "schwarz\u00b7glei\u00b7\u00dfen\u00b7de", "Was\u00b7ser", ",", "zer\u00b7rei\u00b7\u00dfen", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-++--+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Pressen die schwarzen Wunden in die schwarzen Erdschlacken.", "tokens": ["Pres\u00b7sen", "die", "schwar\u00b7zen", "Wun\u00b7den", "in", "die", "schwar\u00b7zen", "Erd\u00b7schla\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+--", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Die Frauen abseits. Graue Scheine umschleichen die wei\u00dfen wogenden K\u00f6rper,", "tokens": ["Die", "Frau\u00b7en", "ab\u00b7seits", ".", "Grau\u00b7e", "Schei\u00b7ne", "um\u00b7schlei\u00b7chen", "die", "wei\u00b7\u00dfen", "wo\u00b7gen\u00b7den", "K\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADJA", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Stumm trauern die \u00fcppigen Augen.", "tokens": ["Stumm", "trau\u00b7ern", "die", "\u00fcp\u00b7pi\u00b7gen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Frauen kauern nieder, sch\u00fctteln das wuchtige Haar", "tokens": ["Die", "Frau\u00b7en", "kau\u00b7ern", "nie\u00b7der", ",", "sch\u00fct\u00b7teln", "das", "wuch\u00b7ti\u00b7ge", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00dcber die grauen zitternden Glieder, \u00fcber den grauen frierenden Scho\u00df,", "tokens": ["\u00dc\u00b7ber", "die", "grau\u00b7en", "zit\u00b7tern\u00b7den", "Glie\u00b7der", ",", "\u00fc\u00b7ber", "den", "grau\u00b7en", "frie\u00b7ren\u00b7den", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-+--+", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Die rotwilden M\u00e4nner, mit grimmen Augen, mit witternden N\u00fcstern,", "tokens": ["Die", "rot\u00b7wil\u00b7den", "M\u00e4n\u00b7ner", ",", "mit", "grim\u00b7men", "Au\u00b7gen", ",", "mit", "wit\u00b7tern\u00b7den", "N\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Schalleer seine Worte fallen taub durch die L\u00fcfte,", "tokens": ["Schal\u00b7leer", "sei\u00b7ne", "Wor\u00b7te", "fal\u00b7len", "taub", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Nur die rasenden F\u00e4uste prallen gegen die dunkeln Erdgr\u00fcfte.", "tokens": ["Nur", "die", "ra\u00b7sen\u00b7den", "F\u00e4us\u00b7te", "pral\u00b7len", "ge\u00b7gen", "die", "dun\u00b7keln", "Erd\u00b7gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "T\u00f6dlich w\u00e4chst Schweigen. Des Mannes Augen steigen stier aus den H\u00f6hlen.", "tokens": ["T\u00f6d\u00b7lich", "w\u00e4chst", "Schwei\u00b7gen", ".", "Des", "Man\u00b7nes", "Au\u00b7gen", "stei\u00b7gen", "stier", "aus", "den", "H\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$.", "ART", "NN", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Die straffen F\u00e4uste sinken gel\u00e4hmt,", "tokens": ["Die", "straf\u00b7fen", "F\u00e4us\u00b7te", "sin\u00b7ken", "ge\u00b7l\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Die braunen Muskeln erschlaffen,", "tokens": ["Die", "brau\u00b7nen", "Mus\u00b7keln", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Grau nagt K\u00e4lte an seinem Leib.", "tokens": ["Grau", "nagt", "K\u00e4l\u00b7te", "an", "sei\u00b7nem", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Blauliche Schatten umspinnen Schenkel, H\u00fcften,", "tokens": ["Blau\u00b7li\u00b7che", "Schat\u00b7ten", "um\u00b7spin\u00b7nen", "Schen\u00b7kel", ",", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Schatten durchrinnen schwarz seine Adern,", "tokens": ["Schat\u00b7ten", "durch\u00b7rin\u00b7nen", "schwarz", "sei\u00b7ne", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Der R\u00fccken geknickt ..., die Haare gestr\u00e4ubt ...,", "tokens": ["Der", "R\u00fc\u00b7cken", "ge\u00b7knickt", "...", ",", "die", "Haa\u00b7re", "ge\u00b7str\u00e4ubt", "...", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "$,", "ART", "NN", "VVPP", "$(", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Erstickt bricht er nieder.", "tokens": ["Er\u00b7stickt", "bricht", "er", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Die Glieder fleischlos,", "tokens": ["Die", "Glie\u00b7der", "fleisc\u00b7hlos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Schwarz, ein Skelett ...", "tokens": ["Schwarz", ",", "ein", "Ske\u00b7lett", "..."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Er zerst\u00e4ubt zu Erde.", "tokens": ["Er", "zer\u00b7st\u00e4ubt", "zu", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Die M\u00e4nner schauern.", "tokens": ["Die", "M\u00e4n\u00b7ner", "schau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Frauen, reglos gekauert, fr\u00f6steln unter den H\u00fcllen ihres hei\u00dfen str\u00f6menden Haares.", "tokens": ["Die", "Frau\u00b7en", ",", "reg\u00b7los", "ge\u00b7kau\u00b7ert", ",", "fr\u00f6s\u00b7teln", "un\u00b7ter", "den", "H\u00fcl\u00b7len", "ih\u00b7res", "hei\u00b7\u00dfen", "str\u00f6\u00b7men\u00b7den", "Haa\u00b7res", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$