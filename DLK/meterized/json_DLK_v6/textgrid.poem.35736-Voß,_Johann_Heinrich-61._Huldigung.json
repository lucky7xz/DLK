{"textgrid.poem.35736": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "61. Huldigung", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oben gl\u00e4nzt des Himmels Bl\u00e4ue,", "tokens": ["O\u00b7ben", "gl\u00e4nzt", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit umher die sch\u00f6ne Flur.", "tokens": ["Weit", "um\u00b7her", "die", "sch\u00f6\u00b7ne", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des gro\u00dfen Tempels Freie", "tokens": ["In", "des", "gro\u00b7\u00dfen", "Tem\u00b7pels", "Frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00f6r ich Treue,", "tokens": ["Schw\u00f6r", "ich", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gottes Abglanz, dir, Natur!", "tokens": ["Got\u00b7tes", "Ab\u00b7glanz", ",", "dir", ",", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Brich, o Geist, des Wahnes Schranken,", "tokens": ["Brich", ",", "o", "Geist", ",", "des", "Wah\u00b7nes", "Schran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo dich Ort geengt und Zeit!", "tokens": ["Wo", "dich", "Ort", "ge\u00b7engt", "und", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf zu Gott entfleuch mit franken", "tokens": ["Auf", "zu", "Gott", "ent\u00b7fleuch", "mit", "fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NN", "APPO", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lichtgedanken,", "tokens": ["Licht\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Endlos durch Unendlichkeit!", "tokens": ["End\u00b7los", "durch", "Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau, wie hehr und wunderpr\u00e4chtig", "tokens": ["Schau", ",", "wie", "hehr", "und", "wun\u00b7der\u00b7pr\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles strahlt, so hoch du drangst!", "tokens": ["Al\u00b7les", "strahlt", ",", "so", "hoch", "du", "drangst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vater, gut und weis' und m\u00e4chtig,", "tokens": ["Va\u00b7ter", ",", "gut", "und", "weis'", "und", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie d\u00e4cht' ich", "tokens": ["O", "wie", "d\u00e4cht'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein mit Schwermut, dein mit Angst?", "tokens": ["Dein", "mit", "Schwer\u00b7mut", ",", "dein", "mit", "Angst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "$,", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aller Wesen Stimm' erhebet:", "tokens": ["Al\u00b7ler", "We\u00b7sen", "Stimm'", "er\u00b7he\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott ist Gott! in hellem Chor.", "tokens": ["Gott", "ist", "Gott", "!", "in", "hel\u00b7lem", "Chor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Staub sich regt und lebet,", "tokens": ["Wo", "ein", "Staub", "sich", "regt", "und", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles strebet", "tokens": ["Al\u00b7les", "stre\u00b7bet"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zu der Geister Wonn' empor.", "tokens": ["Zu", "der", "Geis\u00b7ter", "Wonn'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Allem Volk ins Herz geschrieben", "tokens": ["Al\u00b7lem", "Volk", "ins", "Herz", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward sein ewiges Gebot:", "tokens": ["Ward", "sein", "e\u00b7wi\u00b7ges", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reine Menschlichkeit zu \u00fcben;", "tokens": ["Rei\u00b7ne", "Menschlich\u00b7keit", "zu", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ach zu lieben", "tokens": ["Ach", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gott in uns, im Bruder Gott!", "tokens": ["Gott", "in", "uns", ",", "im", "Bru\u00b7der", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00dcberall ert\u00f6nt von allen", "tokens": ["\u00dc\u00b7be\u00b7rall", "er\u00b7t\u00f6nt", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromme Sehnsucht, frommer Dank,", "tokens": ["From\u00b7me", "Sehn\u00b7sucht", ",", "from\u00b7mer", "Dank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott vernimmt mit Wohlgefallen", "tokens": ["Gott", "ver\u00b7nimmt", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort das Lallen,", "tokens": ["Dort", "das", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dort gereiften Lobgesang.", "tokens": ["Dort", "ge\u00b7reif\u00b7ten", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wunderbar durch Glanz und Tr\u00fcbe", "tokens": ["Wun\u00b7der\u00b7bar", "durch", "Glanz", "und", "Tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Geist uns angefacht.", "tokens": ["Wird", "der", "Geist", "uns", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob der Staub um uns zerstiebe;", "tokens": ["Ob", "der", "Staub", "um", "uns", "zer\u00b7stie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Liebe", "tokens": ["Got\u00b7tes", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "L\u00e4utert auch durch Todesnacht!", "tokens": ["L\u00e4u\u00b7tert", "auch", "durch", "To\u00b7des\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}