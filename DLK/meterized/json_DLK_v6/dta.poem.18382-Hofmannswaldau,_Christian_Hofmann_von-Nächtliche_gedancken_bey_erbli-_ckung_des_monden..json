{"dta.poem.18382": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche gedancken/ bey erbli-  \n ckung des monden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Halt ein mit deinem lauf/ du k\u00f6nigin der sternen/", "tokens": ["Halt", "ein", "mit", "dei\u00b7nem", "lauf", "/", "du", "k\u00f6\u00b7ni\u00b7gin", "der", "ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "APPR", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du silber-wei\u00dfer mond mit deinem blassen schein/", "tokens": ["Du", "sil\u00b7ber\u00b7wei\u00b7\u00dfer", "mond", "mit", "dei\u00b7nem", "blas\u00b7sen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kanst aus deiner burg am besten ja von fernen/", "tokens": ["Du", "kanst", "aus", "dei\u00b7ner", "burg", "am", "bes\u00b7ten", "ja", "von", "fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein zeuge meiner noth/ und meiner wehmuth seyn.", "tokens": ["Ein", "zeu\u00b7ge", "mei\u00b7ner", "noth", "/", "und", "mei\u00b7ner", "weh\u00b7muth", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sag an/ ich schwere dir/ hastu mich auch erblicket/", "tokens": ["Sag", "an", "/", "ich", "schwe\u00b7re", "dir", "/", "has\u00b7tu", "mich", "auch", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seyt dem Melinde sich von hinnen hat verf\u00fcgt/", "tokens": ["Seyt", "dem", "Me\u00b7lin\u00b7de", "sich", "von", "hin\u00b7nen", "hat", "ver\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "APPR", "ADV", "VAFIN", "VVPP", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Da\u00df mein verwachtes aug vom schlaffe sey erquicket/", "tokens": ["Da\u00df", "mein", "ver\u00b7wach\u00b7tes", "aug", "vom", "schlaf\u00b7fe", "sey", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "VVFIN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob schon die gantze welt in sichrer ruhe liegt?", "tokens": ["Ob", "schon", "die", "gant\u00b7ze", "welt", "in", "sich\u00b7rer", "ru\u00b7he", "liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du weist/ wann mensch und vieh die m\u00fcden augen schliessen/", "tokens": ["Du", "weist", "/", "wann", "mensch", "und", "vieh", "die", "m\u00fc\u00b7den", "au\u00b7gen", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "ADJD", "KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und die gestirnte nacht den stillen welt-creys deckt/", "tokens": ["Und", "die", "ge\u00b7stirn\u00b7te", "nacht", "den", "stil\u00b7len", "welt\u00b7creys", "deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wach ich zwischen furcht/ und \u00e4ngstlichem verdriessen/", "tokens": ["So", "wach", "ich", "zwi\u00b7schen", "furcht", "/", "und", "\u00e4ngst\u00b7li\u00b7chem", "ver\u00b7dries\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$(", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und bleicher k\u00fcmmern\u00fcs die geist und blut erschreckt/", "tokens": ["Und", "blei\u00b7cher", "k\u00fcm\u00b7mer\u00b7n\u00fcs", "die", "geist", "und", "blut", "er\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald klag ich meine noth mit jammernden geberden/", "tokens": ["Bald", "klag", "ich", "mei\u00b7ne", "noth", "mit", "jam\u00b7mern\u00b7den", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bald ruf ich himmel/ erd und das verh\u00e4ngn\u00fcs an;", "tokens": ["Bald", "ruf", "ich", "him\u00b7mel", "/", "erd", "und", "das", "ver\u00b7h\u00e4ng\u00b7n\u00fcs", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald red ich gantz verwirrt von tausend angst-beschwerden/", "tokens": ["Bald", "red", "ich", "gantz", "ver\u00b7wirrt", "von", "tau\u00b7send", "angst\u00b7be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Womit so k\u00fcmmerlich mein hertz ist angethan.", "tokens": ["Wo\u00b7mit", "so", "k\u00fcm\u00b7mer\u00b7lich", "mein", "hertz", "ist", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald spiel ich wieder auch mit fl\u00fcchtigen gedancken/", "tokens": ["Bald", "spiel", "ich", "wie\u00b7der", "auch", "mit", "fl\u00fcch\u00b7ti\u00b7gen", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch h\u00e4uf ich solcher art nur immer meine pein/", "tokens": ["Doch", "h\u00e4uf", "ich", "sol\u00b7cher", "art", "nur", "im\u00b7mer", "mei\u00b7ne", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bis letzt die geister selbst aus ihren zirckeln wancken/", "tokens": ["Bis", "letzt", "die", "geis\u00b7ter", "selbst", "aus", "ih\u00b7ren", "zir\u00b7ckeln", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und drauf mein gantzer leib wil wie erstorben seyn.", "tokens": ["Und", "drauf", "mein", "gant\u00b7zer", "leib", "wil", "wie", "ers\u00b7tor\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "ADJA", "NN", "VMFIN", "KOKOM", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So pfleg ich tag und nacht den jammer zu beklagen/", "tokens": ["So", "pfleg", "ich", "tag", "und", "nacht", "den", "jam\u00b7mer", "zu", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der meine brust best\u00fcrmt mit ungemeiner noth/", "tokens": ["Der", "mei\u00b7ne", "brust", "be\u00b7st\u00fcrmt", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So w\u00fcnsch ich mehrentheils bey hundert tausend plagen/", "tokens": ["So", "w\u00fcnsch", "ich", "meh\u00b7ren\u00b7theils", "bey", "hun\u00b7dert", "tau\u00b7send", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "CARD", "CARD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mir nichts als meinen sarg/ mein grab und meinen todt.", "tokens": ["Mir", "nichts", "als", "mei\u00b7nen", "sarg", "/", "mein", "grab", "und", "mei\u00b7nen", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "KOKOM", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja mond! du weist genug mein innerlich betr\u00fcben/", "tokens": ["Ja", "mond", "!", "du", "weist", "ge\u00b7nug", "mein", "in\u00b7ner\u00b7lich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du kennest gar zu wohl den ursprung meiner pein.", "tokens": ["Du", "ken\u00b7nest", "gar", "zu", "wohl", "den", "ur\u00b7sprung", "mei\u00b7ner", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "La\u00df dann Melinden auch ein theil von meinem lieben/", "tokens": ["La\u00df", "dann", "Me\u00b7lin\u00b7den", "auch", "ein", "theil", "von", "mei\u00b7nem", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und meiner traurigkeit nicht unverholen seyn/", "tokens": ["Und", "mei\u00b7ner", "trau\u00b7rig\u00b7keit", "nicht", "un\u00b7ver\u00b7ho\u00b7len", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bring vor ihr zartes ohr mein jammer-volles klagen/", "tokens": ["Bring", "vor", "ihr", "zar\u00b7tes", "ohr", "mein", "jam\u00b7mer\u00b7vol\u00b7les", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zeig ihr in deinem licht und deiner bleichen pracht", "tokens": ["Zeig", "ihr", "in", "dei\u00b7nem", "licht", "und", "dei\u00b7ner", "blei\u00b7chen", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den abris meiner angst und meiner heissen plagen/", "tokens": ["Den", "ab\u00b7ris", "mei\u00b7ner", "angst", "und", "mei\u00b7ner", "heis\u00b7sen", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die meinen leib erstarrt/ mein autlitz blas gemacht.", "tokens": ["Die", "mei\u00b7nen", "leib", "er\u00b7starrt", "/", "mein", "aut\u00b7litz", "blas", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$(", "PPOSAT", "FM", "FM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sag ihr/ wie meine brust mit bleicher furcht \u00fcmbfangen/", "tokens": ["Sag", "ihr", "/", "wie", "mei\u00b7ne", "brust", "mit", "blei\u00b7cher", "furcht", "\u00fcmb\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$(", "KOKOM", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Worinn die trauer-sucht ihr schwartzes wonhaus baut/", "tokens": ["Wo\u00b7rinn", "die", "trau\u00b7e\u00b7rsucht", "ihr", "schwart\u00b7zes", "won\u00b7haus", "baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie mein gew\u00f6hntes aug mit seufzendem verlangen", "tokens": ["Wie", "mein", "ge\u00b7w\u00f6hn\u00b7tes", "aug", "mit", "seuf\u00b7zen\u00b7dem", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Viel tausend tausendmahl nach ihrer wohnung schaut/", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send\u00b7mahl", "nach", "ih\u00b7rer", "woh\u00b7nung", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie mein best\u00fcrtzter geist mit \u00e4ngstlichem beginnen/", "tokens": ["Wie", "mein", "be\u00b7st\u00fcrtz\u00b7ter", "geist", "mit", "\u00e4ngst\u00b7li\u00b7chem", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Auff seinen untergang und sein verderben l\u00e4uft/", "tokens": ["Auff", "sei\u00b7nen", "un\u00b7ter\u00b7gang", "und", "sein", "ver\u00b7der\u00b7ben", "l\u00e4uft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie unm\u00fcth und verdrus/ die foltern meiner sinnen/", "tokens": ["Wie", "un\u00b7m\u00fcth", "und", "ver\u00b7drus", "/", "die", "fol\u00b7tern", "mei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "NE", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Nur seele geist und blut mit drangsal \u00fcberh\u00e4uft.", "tokens": ["Nur", "see\u00b7le", "geist", "und", "blut", "mit", "dran\u00b7gsal", "\u00fc\u00b7berh\u00b7\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Stell ihr auch endlich vor mein k\u00fcmmerndes bezeigen/", "tokens": ["Stell", "ihr", "auch", "end\u00b7lich", "vor", "mein", "k\u00