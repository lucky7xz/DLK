{"textgrid.poem.62866": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der L\u00f6we sa\u00df auf seinem Thron von Knochen", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we sa\u00df auf seinem Thron von Knochen", "tokens": ["Der", "L\u00f6\u00b7we", "sa\u00df", "auf", "sei\u00b7nem", "Thron", "von", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sann auf Sklaverey und Tod.", "tokens": ["Und", "sann", "auf", "Skla\u00b7ve\u00b7rey", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Igel kam ihm in den Weg gekrochen;", "tokens": ["Ein", "I\u00b7gel", "kam", "ihm", "in", "den", "Weg", "ge\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ha! Wurm! so br\u00fcllte der Despot,", "tokens": ["Ha", "!", "Wurm", "!", "so", "br\u00fcll\u00b7te", "der", "Des\u00b7pot", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hielt ihn zwischen seinen Klauen,", "tokens": ["Und", "hielt", "ihn", "zwi\u00b7schen", "sei\u00b7nen", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem Schluck verschling ich dich!", "tokens": ["Mit", "ei\u00b7nem", "Schluck", "ver\u00b7schling", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Igel sprach: verschlingen kannst du mich:", "tokens": ["Der", "I\u00b7gel", "sprach", ":", "ver\u00b7schlin\u00b7gen", "kannst", "du", "mich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVINF", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Allein du kannst mich nicht verdauen.", "tokens": ["Al\u00b7lein", "du", "kannst", "mich", "nicht", "ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}