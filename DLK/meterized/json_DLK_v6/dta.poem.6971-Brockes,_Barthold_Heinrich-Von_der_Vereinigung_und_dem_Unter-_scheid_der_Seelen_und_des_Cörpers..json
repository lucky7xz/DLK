{"dta.poem.6971": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn mitten in dem Lauf das Sonnen-Licht,", "tokens": ["Wenn", "mit\u00b7ten", "in", "dem", "Lauf", "das", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus dessen fruchtbarn Quell, so Licht als W\u00e4rme bricht,", "tokens": ["Aus", "des\u00b7sen", "frucht\u00b7barn", "Quell", ",", "so", "Licht", "als", "W\u00e4r\u00b7me", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom unbest\u00e4nd\u2019gen Mond, der Schimmer Glantz und", "tokens": ["Vom", "un\u00b7be\u00b7st\u00e4n\u00b7d'\u00b7gen", "Mond", ",", "der", "Schim\u00b7mer", "Glantz", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "NN", "KON"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Prangen,", "tokens": ["Pran\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Von ihm allein empfangen,", "tokens": ["Von", "ihm", "al\u00b7lein", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein eigen Licht sieht aufgefangen;", "tokens": ["Sein", "ei\u00b7gen", "Licht", "sieht", "auf\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Spricht jemand darum wol: Da\u00df seine reine Gluht,", "tokens": ["Spricht", "je\u00b7mand", "da\u00b7rum", "wol", ":", "Da\u00df", "sei\u00b7ne", "rei\u00b7ne", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "ADV", "$.", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die aus ihr selber stammt und nimmer ruht,", "tokens": ["Die", "aus", "ihr", "sel\u00b7ber", "stammt", "und", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Weil sie ein dicker C\u00f6rper decket,", "tokens": ["Weil", "sie", "ein", "di\u00b7cker", "C\u00f6r\u00b7per", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verl\u00f6scht und stirbt, indem sie sich verstecket?", "tokens": ["Ver\u00b7l\u00f6scht", "und", "stirbt", ",", "in\u00b7dem", "sie", "sich", "ver\u00b7ste\u00b7cket", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}