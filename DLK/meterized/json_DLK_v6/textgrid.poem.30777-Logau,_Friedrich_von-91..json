{"textgrid.poem.30777": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrsten, die euch die Geschencke, nicht die Treu pflegt zu verbinden,", "tokens": ["F\u00fcrs\u00b7ten", ",", "die", "euch", "die", "Ge\u00b7schen\u00b7cke", ",", "nicht", "die", "Treu", "pflegt", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Diese habt ihr nur so lange, weil sie sich beschenckt befinden.", "tokens": ["Die\u00b7se", "habt", "ihr", "nur", "so", "lan\u00b7ge", ",", "weil", "sie", "sich", "be\u00b7schenckt", "be\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}