{"dta.poem.6481": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns kan ein naher Ton verdrie\u00dflich seyn und plagen,", "tokens": ["Uns", "kan", "ein", "na\u00b7her", "Ton", "ver\u00b7drie\u00df\u00b7lich", "seyn", "und", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADJD", "VAINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den man von weiten kan ertragen.", "tokens": ["Den", "man", "von", "wei\u00b7ten", "kan", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wie er uns bewegt verschiedentlich,", "tokens": ["So", "wie", "er", "uns", "be\u00b7wegt", "ver\u00b7schie\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Also empf\u00e4ngt man ihn auch auf verschiedne Weise.", "tokens": ["Al\u00b7so", "emp\u00b7f\u00e4ngt", "man", "ihn", "auch", "auf", "ver\u00b7schied\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ein jeder hat den Ton in sich.", "tokens": ["Ein", "je\u00b7der", "hat", "den", "Ton", "in", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es h\u00f6ren zween ihn nicht gleich starck und auch gleich leise.", "tokens": ["Es", "h\u00f6\u00b7ren", "zween", "ihn", "nicht", "gleich", "starck", "und", "auch", "gleich", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Krancker, der vom Schmertzen abgezehret,", "tokens": ["Ein", "Kran\u00b7cker", ",", "der", "vom", "Schmert\u00b7zen", "ab\u00b7ge\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fchlt Unmuth, wenn er auch den kleinsten Schall nur h\u00f6ret.", "tokens": ["F\u00fchlt", "Un\u00b7muth", ",", "wenn", "er", "auch", "den", "kleins\u00b7ten", "Schall", "nur", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja wenn sie gleich nicht kranck; So lassen zween", "tokens": ["Ja", "wenn", "sie", "gleich", "nicht", "kranck", ";", "So", "las\u00b7sen", "zween"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "$.", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Uns von demselben Ton verschiedne W\u00fcrckung sehn.", "tokens": ["Uns", "von", "dem\u00b7sel\u00b7ben", "Ton", "ver\u00b7schied\u00b7ne", "W\u00fcr\u00b7ckung", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem einen schwindelt schon", "tokens": ["Dem", "ei\u00b7nen", "schwin\u00b7delt", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Durch einen nicht gar starcken Ton:", "tokens": ["Durch", "ei\u00b7nen", "nicht", "gar", "star\u00b7cken", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da jener, wenn er gleich ein starckes Lermen sp\u00fcret,", "tokens": ["Da", "je\u00b7ner", ",", "wenn", "er", "gleich", "ein", "star\u00b7ckes", "Ler\u00b7men", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dennoch nicht wird ger\u00fchret.", "tokens": ["Den\u00b7noch", "nicht", "wird", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}