{"textgrid.poem.49220": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde trinkt f\u00fcr sich, die B\u00e4ume trinken Erden;", "tokens": ["Die", "Er\u00b7de", "trinkt", "f\u00fcr", "sich", ",", "die", "B\u00e4u\u00b7me", "trin\u00b7ken", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$,", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Meere pflegt die Luft auch zu getrunken werden;", "tokens": ["Vom", "Mee\u00b7re", "pflegt", "die", "Luft", "auch", "zu", "ge\u00b7trun\u00b7ken", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonne trinkt das Meer, der Monde trinkt die Sonnen,", "tokens": ["Die", "Son\u00b7ne", "trinkt", "das", "Meer", ",", "der", "Mon\u00b7de", "trinkt", "die", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wolt dann ihr Freunde mir das Trinken nicht vergonnen?", "tokens": ["Wolt", "dann", "ihr", "Freun\u00b7de", "mir", "das", "Trin\u00b7ken", "nicht", "ver\u00b7gon\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}