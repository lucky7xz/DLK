{"dta.poem.11472": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem Hallmann-Frantzischen  \n hochzeit-feste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt ich mit Schlesien nicht die verliebten lieder", "tokens": ["H\u00e4tt", "ich", "mit", "Schle\u00b7si\u00b7en", "nicht", "die", "ver\u00b7lieb\u00b7ten", "lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleichsam allen zug zum tichten hingelegt;", "tokens": ["Und", "gleich\u00b7sam", "al\u00b7len", "zug", "zum", "tich\u00b7ten", "hin\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So legt ich nicht so offt die stumpffe feder nieder,", "tokens": ["So", "legt", "ich", "nicht", "so", "offt", "die", "stumpf\u00b7fe", "fe\u00b7der", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die itzund, wie du siehst, kein rechter trieb bewegt.", "tokens": ["Die", "it\u00b7zund", ",", "wie", "du", "siehst", ",", "kein", "rech\u00b7ter", "trieb", "be\u00b7wegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "PIAT", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schm\u00fcckte meinen vers mit den beliebten rosen,", "tokens": ["Ich", "schm\u00fcck\u00b7te", "mei\u00b7nen", "vers", "mit", "den", "be\u00b7lieb\u00b7ten", "ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die, o belobte Braut! in deinem nahmen stehn:", "tokens": ["Die", ",", "o", "be\u00b7lob\u00b7te", "Braut", "!", "in", "dei\u00b7nem", "nah\u00b7men", "stehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "FM", "ADJA", "NN", "$.", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und unterliesse nicht den lippen liebzukosen,", "tokens": ["Und", "un\u00b7ter\u00b7lies\u00b7se", "nicht", "den", "lip\u00b7pen", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die deinem br\u00e4utigam so wohl entgegen gehn.", "tokens": ["Die", "dei\u00b7nem", "br\u00e4u\u00b7ti\u00b7gam", "so", "wohl", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "ADV", "ADV", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den lippen, welche noch mit frischen blumen prangen:", "tokens": ["Den", "lip\u00b7pen", ",", "wel\u00b7che", "noch", "mit", "fri\u00b7schen", "blu\u00b7men", "pran\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die keine falsche bien\u2019 jemahlen anger\u00fchrt:", "tokens": ["Die", "kei\u00b7ne", "fal\u00b7sche", "bi\u00b7en'", "je\u00b7mah\u00b7len", "an\u00b7ge\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dergleichen sind gewi\u00df itzt seltsam zu erlangen,", "tokens": ["Derg\u00b7lei\u00b7chen", "sind", "ge\u00b7wi\u00df", "itzt", "selt\u00b7sam", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da man die wespen fast in allen g\u00e4rten sp\u00fcrt.", "tokens": ["Da", "man", "die", "wes\u00b7pen", "fast", "in", "al\u00b7len", "g\u00e4r\u00b7ten", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch m\u00fcst ich sie allein mit solchen worten loben,", "tokens": ["Doch", "m\u00fcst", "ich", "sie", "al\u00b7lein", "mit", "sol\u00b7chen", "wor\u00b7ten", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die, auserlesne Braut! so nett als deine seyn.", "tokens": ["Die", ",", "au\u00b7ser\u00b7les\u00b7ne", "Braut", "!", "so", "nett", "als", "dei\u00b7ne", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "$.", "ADV", "ADJD", "KOKOM", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wiewohl! ich h\u00e4tte dich doch nicht genung erhoben;", "tokens": ["Wie\u00b7wohl", "!", "ich", "h\u00e4t\u00b7te", "dich", "doch", "nicht", "ge\u00b7nung", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn deine tugend nimmt weit gr\u00f6ssre schrancken ein.", "tokens": ["Denn", "dei\u00b7ne", "tu\u00b7gend", "nimmt", "weit", "gr\u00f6ss\u00b7re", "schran\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum zeigt\u2019 ich: Da\u00df dein schatz dich recht zu sch\u00e4tzen wissen,", "tokens": ["Drum", "zeigt'", "ich", ":", "Da\u00df", "dein", "schatz", "dich", "recht", "zu", "sch\u00e4t\u00b7zen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "KOUS", "PPOSAT", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der sich am hefftigsten in deinen geist verliebt,", "tokens": ["Der", "sich", "am", "heff\u00b7tigs\u00b7ten", "in", "dei\u00b7nen", "geist", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "In deinen edlen geist, den welt und himmel k\u00fcssen,", "tokens": ["In", "dei\u00b7nen", "ed\u00b7len", "geist", ",", "den", "welt", "und", "him\u00b7mel", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und dem die mi\u00dfgunst selbst ein gutes zeugni\u00df giebt,", "tokens": ["Und", "dem", "die", "mi\u00df\u00b7gunst", "selbst", "ein", "gu\u00b7tes", "zeug\u00b7ni\u00df", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zwar siehet er den leib nicht mit des Zeno augen,", "tokens": ["Zwar", "sie\u00b7het", "er", "den", "leib", "nicht", "mit", "des", "Ze\u00b7no", "au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dessen niedrigkeit, als unvollkommen, an:", "tokens": ["Und", "des\u00b7sen", "nied\u00b7rig\u00b7keit", ",", "als", "un\u00b7voll\u00b7kom\u00b7men", ",", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "PTKNEG", "$,", "KOUS", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er wei\u00df, da\u00df perlen mehr als kiesel-steine taugen,", "tokens": ["Er", "wei\u00df", ",", "da\u00df", "per\u00b7len", "mehr", "als", "kie\u00b7sel\u00b7stei\u00b7ne", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "PIAT", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ob man die letzten gleich weit gr\u00f6sser haben kan.", "tokens": ["Ob", "man", "die", "letz\u00b7ten", "gleich", "weit", "gr\u00f6s\u00b7ser", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "ADV", "ADJD", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedennoch wei\u00df er auch: Da\u00df sonne lufft und stunde", "tokens": ["Je\u00b7den\u00b7noch", "wei\u00df", "er", "auch", ":", "Da\u00df", "son\u00b7ne", "lufft", "und", "stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "KOUS", "ADJA", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der zarten farb und haut nicht wenig abbruch thun:", "tokens": ["Der", "zar\u00b7ten", "farb", "und", "haut", "nicht", "we\u00b7nig", "ab\u00b7bruch", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drum stehet seine lieb auf einem festern grunde,", "tokens": ["Drum", "ste\u00b7het", "sei\u00b7ne", "lieb", "auf", "ei\u00b7nem", "fes\u00b7tern", "grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und will im hertzen mehr, als in den armen, ruhn.", "tokens": ["Und", "will", "im", "hert\u00b7zen", "mehr", ",", "als", "in", "den", "ar\u00b7men", ",", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Solt\u2019 ich noch weiter gehn; so geben deine minen,", "tokens": ["Solt'", "ich", "noch", "wei\u00b7ter", "gehn", ";", "so", "ge\u00b7ben", "dei\u00b7ne", "mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und deines br\u00e4utigams durchdringender verstand,", "tokens": ["Und", "dei\u00b7nes", "br\u00e4u\u00b7ti\u00b7gams", "durch\u00b7drin\u00b7gen\u00b7der", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den Meditrine kennt, dich ferner zu bedienen,", "tokens": ["Den", "Me\u00b7di\u00b7tri\u00b7ne", "kennt", ",", "dich", "fer\u00b7ner", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mir endlich zeug genung zum tichten an die hand.", "tokens": ["Mir", "end\u00b7lich", "zeug", "ge\u00b7nung", "zum", "tich\u00b7ten", "an", "die", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPRART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch weil die Musen sich vor meinen feind erkl\u00e4ret,", "tokens": ["Doch", "weil", "die", "Mu\u00b7sen", "sich", "vor", "mei\u00b7nen", "feind", "er\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und eure demuth selbst mich gleichsam schweigen heist:", "tokens": ["Und", "eu\u00b7re", "de\u00b7muth", "selbst", "mich", "gleich\u00b7sam", "schwei\u00b7gen", "heist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPER", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.35": {"text": "So kan ich auch sonst nichts, als diesen wunsch, gew\u00e4hren,", "tokens": ["So", "kan", "ich", "auch", "sonst", "nichts", ",", "als", "die\u00b7sen", "wunsch", ",", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PIS", "$,", "KOUS", "PDAT", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der aus dem hertzen mehr, als aus der feder, fleust.", "tokens": ["Der", "aus", "dem", "hert\u00b7zen", "mehr", ",", "als", "aus", "der", "fe\u00b7der", ",", "fleust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.37": {"text": "Der himmel segne dich, und lasse seine fl\u00fcgel", "tokens": ["Der", "him\u00b7mel", "seg\u00b7ne", "dich", ",", "und", "las\u00b7se", "sei\u00b7ne", "fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durchaus-vergn\u00fcgtes paar! stets deine decke seyn!", "tokens": ["Durch\u00b7aus\u00b7ver\u00b7gn\u00fcg\u00b7tes", "paar", "!", "stets", "dei\u00b7ne", "de\u00b7cke", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gl\u00fcck und zufriedenheit sey deines bundes siegel,", "tokens": ["Gl\u00fcck", "und", "zu\u00b7frie\u00b7den\u00b7heit", "sey", "dei\u00b7nes", "bun\u00b7des", "sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df beyder hertzen sich in wahrer lust erfreun!", "tokens": ["Da\u00df", "bey\u00b7der", "hert\u00b7zen", "sich", "in", "wah\u00b7rer", "lust", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Gott sey dein schutz und schirm, so steckt kein kaltes fieber", "tokens": ["Gott", "sey", "dein", "schutz", "und", "schirm", ",", "so", "steckt", "kein", "kal\u00b7tes", "fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "$,", "ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und keine zwietrachts-pest dich und dein lager an!", "tokens": ["Und", "kei\u00b7ne", "zwie\u00b7trachts\u00b7pest", "dich", "und", "dein", "la\u00b7ger", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er pflantz in haus und hertz je l\u00e4nger und je lieber,", "tokens": ["Er", "pflantz", "in", "haus", "und", "hertz", "je", "l\u00e4n\u00b7ger", "und", "je", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "NN", "KON", "NN", "ADV", "ADJD", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und liefre, was ihr hofft, und ich nicht schreiben kan!", "tokens": ["Und", "lief\u00b7re", ",", "was", "ihr", "hofft", ",", "und", "ich", "nicht", "schrei\u00b7ben", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ein mehrers wei\u00df ich nicht auf das papier zu bringen,", "tokens": ["Ein", "meh\u00b7rers", "wei\u00df", "ich", "nicht", "auf", "das", "pa\u00b7pier", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Weil, wie ich schon gesagt, mir trieb und kunst gebricht:", "tokens": ["Weil", ",", "wie", "ich", "schon", "ge\u00b7sagt", ",", "mir", "trieb", "und", "kunst", "ge\u00b7bricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wiewohl! wo nachtigalln und edle schwanen singen,", "tokens": ["Wie\u00b7wohl", "!", "wo", "nach\u00b7ti\u00b7galln", "und", "ed\u00b7le", "schwa\u00b7nen", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PWAV", "VVIZU", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da h\u00f6rt man ohnedem die leichten fincken nicht.", "tokens": ["Da", "h\u00f6rt", "man", "oh\u00b7ne\u00b7dem", "die", "leich\u00b7ten", "fin\u00b7cken", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "ART", "ADJA", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Drum \u00fcberla\u00df ich das vor ausge\u00fcbtre sinnen,", "tokens": ["Drum", "\u00fc\u00b7berl\u00b7a\u00df", "ich", "das", "vor", "aus\u00b7ge\u00b7\u00fcbt\u00b7re", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Was meine poesie nicht auszudr\u00fccken wei\u00df:", "tokens": ["Was", "mei\u00b7ne", "po\u00b7e\u00b7sie", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Mich deucht, ich sehe schon ein solches lied beginnen,", "tokens": ["Mich", "deucht", ",", "ich", "se\u00b7he", "schon", "ein", "sol\u00b7ches", "lied", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Das voller feuer ist, wie meines voller ei\u00df.", "tokens": ["Das", "vol\u00b7ler", "feu\u00b7er", "ist", ",", "wie", "mei\u00b7nes", "vol\u00b7ler", "ei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Indessen, ob wir auch schon alle verse tr\u00e4umen,", "tokens": ["In\u00b7des\u00b7sen", ",", "ob", "wir", "auch", "schon", "al\u00b7le", "ver\u00b7se", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und niemand, edles Paar! der Musen trieb vers\u00e4umt;", "tokens": ["Und", "nie\u00b7mand", ",", "ed\u00b7les", "Paar", "!", "der", "Mu\u00b7sen", "trieb", "ver\u00b7s\u00e4umt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So bin ich doch gewi\u00df: Da\u00df unter allen reimen", "tokens": ["So", "bin", "ich", "doch", "ge\u00b7wi\u00df", ":", "Da\u00df", "un\u00b7ter", "al\u00b7len", "rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$.", "KOUS", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Sich braut und br\u00e4utigam am allerbesten reimt.", "tokens": ["Sich", "braut", "und", "br\u00e4u\u00b7ti\u00b7gam", "am", "al\u00b7ler\u00b7bes\u00b7ten", "reimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "NE", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}