{"textgrid.poem.63637": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser Welt voll banger Widerspr\u00fcche,", "tokens": ["In", "die\u00b7ser", "Welt", "voll", "ban\u00b7ger", "Wi\u00b7der\u00b7spr\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie f\u00fchlst du Zweifel deine Brust beklemmen,", "tokens": ["Wie", "f\u00fchlst", "du", "Zwei\u00b7fel", "dei\u00b7ne", "Brust", "be\u00b7klem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die eklen D\u00fcnste dieser Hexenk\u00fcche", "tokens": ["Die", "ek\u00b7len", "D\u00fcns\u00b7te", "die\u00b7ser", "He\u00b7xen\u00b7k\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Sinn verwirren und den Atem hemmen!", "tokens": ["Den", "Sinn", "ver\u00b7wir\u00b7ren", "und", "den", "A\u00b7tem", "hem\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich trug einmal ein Bl\u00fcmchen in der Hand,", "tokens": ["Ich", "trug", "ein\u00b7mal", "ein", "Bl\u00fcm\u00b7chen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor dessen Hauch ein jeder Mi\u00dfduft schwand.", "tokens": ["Vor", "des\u00b7sen", "Hauch", "ein", "je\u00b7der", "Mi\u00df\u00b7duft", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es schien mit seines Kelches zartem Neigen", "tokens": ["Es", "schien", "mit", "sei\u00b7nes", "Kel\u00b7ches", "zar\u00b7tem", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich zu ermuntern, mir den Weg zu zeigen.", "tokens": ["Mich", "zu", "er\u00b7mun\u00b7tern", ",", "mir", "den", "Weg", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seit mir das Bl\u00fcmchen in den Staub gefallen,", "tokens": ["Seit", "mir", "das", "Bl\u00fcm\u00b7chen", "in", "den", "Staub", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann ich den Weg nur tastend weiterwallen.", "tokens": ["Kann", "ich", "den", "Weg", "nur", "tas\u00b7tend", "wei\u00b7ter\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}