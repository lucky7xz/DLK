{"textgrid.poem.54807": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "21. Uber eine Bibliothec", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist was nach uns lebt/ wordurch wir selber leben/", "tokens": ["Di\u00df", "ist", "was", "nach", "uns", "lebt", "/", "wor\u00b7durch", "wir", "sel\u00b7ber", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "APPR", "PPER", "VVFIN", "$(", "PWAV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wir den schwachen Leib der Grufft zu Pfande geben.", "tokens": ["Wann", "wir", "den", "schwa\u00b7chen", "Leib", "der", "Grufft", "zu", "Pfan\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die B\u00fccher weisen zwar di\u00df alles was wir ehren;", "tokens": ["Die", "B\u00fc\u00b7cher", "wei\u00b7sen", "zwar", "di\u00df", "al\u00b7les", "was", "wir", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PDS", "PIS", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch lehren B\u00fccher auch dich ohne B\u00fccher lehren.", "tokens": ["Doch", "leh\u00b7ren", "B\u00fc\u00b7cher", "auch", "dich", "oh\u00b7ne", "B\u00fc\u00b7cher", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}