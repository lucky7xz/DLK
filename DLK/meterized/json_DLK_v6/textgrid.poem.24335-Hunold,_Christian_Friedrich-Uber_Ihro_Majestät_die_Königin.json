{"textgrid.poem.24335": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber Ihro Majest\u00e4t die K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend sprach ohnl\u00e4ngst: Du Zweig der Obetritten/", "tokens": ["Die", "Tu\u00b7gend", "sprach", "ohn\u00b7l\u00e4ngst", ":", "Du", "Zweig", "der", "O\u00b7be\u00b7trit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PPER", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich ruhe nicht/ bi\u00df dir der alte Thron erstritten.", "tokens": ["Ich", "ru\u00b7he", "nicht", "/", "bi\u00df", "dir", "der", "al\u00b7te", "Thron", "er\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Helden wehlte sie Dir Preussens Majest\u00e4t", "tokens": ["Zum", "Hel\u00b7den", "wehl\u00b7te", "sie", "Dir", "Preus\u00b7sens", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deiner Sonnen Glantz dem Adler zum Magnet.", "tokens": ["Und", "dei\u00b7ner", "Son\u00b7nen", "Glantz", "dem", "Ad\u00b7ler", "zum", "Mag\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--