{"textgrid.poem.37473": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zu Neujahr", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will das Gl\u00fcck nach seinem Sinn", "tokens": ["Will", "das", "Gl\u00fcck", "nach", "sei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir was Gutes schenken,", "tokens": ["Dir", "was", "Gu\u00b7tes", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sage Dank und nimm es hin", "tokens": ["Sa\u00b7ge", "Dank", "und", "nimm", "es", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "VVIMP", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne viel Bedenken.", "tokens": ["Oh\u00b7ne", "viel", "Be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jede Gabe sei begr\u00fc\u00dft,", "tokens": ["Je\u00b7de", "Ga\u00b7be", "sei", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch vor allen Dingen:", "tokens": ["Doch", "vor", "al\u00b7len", "Din\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das, warum du dich bem\u00fchst,", "tokens": ["Das", ",", "wa\u00b7rum", "du", "dich", "be\u00b7m\u00fchst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6ge dir gelingen.", "tokens": ["M\u00f6\u00b7ge", "dir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}