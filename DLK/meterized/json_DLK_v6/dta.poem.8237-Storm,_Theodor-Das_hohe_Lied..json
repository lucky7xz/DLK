{"dta.poem.8237": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das hohe Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Im Winde weht der bunte Tr\u00f6delkram;", "tokens": ["Im", "Win\u00b7de", "weht", "der", "bun\u00b7te", "Tr\u00f6\u00b7del\u00b7kram", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und drinnen sitzt im Wirbelstaub der Gassen", "tokens": ["Und", "drin\u00b7nen", "sitzt", "im", "Wir\u00b7bel\u00b7staub", "der", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das schlanke Kind des Juden Abraham.", "tokens": ["Das", "schlan\u00b7ke", "Kind", "des", "Ju\u00b7den", "Ab\u00b7ra\u00b7ham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie st\u00fctzt das Haupt in ihre wei\u00dfe Hand,", "tokens": ["Sie", "st\u00fctzt", "das", "Haupt", "in", "ih\u00b7re", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Sturm des Busens bebt die leichte H\u00fclle;", "tokens": ["Im", "Sturm", "des", "Bu\u00b7sens", "bebt", "die", "leich\u00b7te", "H\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man sieht's, an dieser Augen Sonnenbrand", "tokens": ["Man", "sieht's", ",", "an", "die\u00b7ser", "Au\u00b7gen", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gedieh der Mund zu seiner Purpurf\u00fclle.", "tokens": ["Ge\u00b7dieh", "der", "Mund", "zu", "sei\u00b7ner", "Pur\u00b7pur\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Lippe schweigt, die schwarzen Locken ranken", "tokens": ["Die", "Lip\u00b7pe", "schweigt", ",", "die", "schwar\u00b7zen", "Lo\u00b7cken", "ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sich um die Stirn wie schmachtende Gedanken.", "tokens": ["Sich", "um", "die", "Stirn", "wie", "schmach\u00b7ten\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie liest vertieft in einem alten Buch", "tokens": ["Sie", "liest", "ver\u00b7tieft", "in", "ei\u00b7nem", "al\u00b7ten", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von einem K\u00f6nig, der die Harfe schlug,", "tokens": ["Von", "ei\u00b7nem", "K\u00f6\u00b7nig", ",", "der", "die", "Har\u00b7fe", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und liebefordernd in den goldnen Klang", "tokens": ["Und", "lie\u00b7be\u00b7for\u00b7dernd", "in", "den", "gold\u00b7nen", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Manch z\u00e4rtlich Lied an Zions M\u00e4dchen sang.", "tokens": ["Manch", "z\u00e4rt\u00b7lich", "Lied", "an", "Zi\u00b7ons", "M\u00e4d\u00b7chen", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}