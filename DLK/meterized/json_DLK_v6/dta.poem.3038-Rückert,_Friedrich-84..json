{"dta.poem.3038": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Liebe giebts auf Erden ohne Leid,", "tokens": ["Nur", "ei\u00b7ne", "Lie\u00b7be", "giebts", "auf", "Er\u00b7den", "oh\u00b7ne", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ohne Eifersucht, weil ohne Groll und Neid,", "tokens": ["Weil", "oh\u00b7ne", "Ei\u00b7fer\u00b7sucht", ",", "weil", "oh\u00b7ne", "Groll", "und", "Neid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ohne Eigennutz; weil, wer sie liebt auf Erden,", "tokens": ["Und", "oh\u00b7ne", "Ei\u00b7gen\u00b7nutz", ";", "weil", ",", "wer", "sie", "liebt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$.", "KOUS", "$,", "PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr seine Liebe nicht geliebt will wieder werden.", "tokens": ["F\u00fcr", "sei\u00b7ne", "Lie\u00b7be", "nicht", "ge\u00b7liebt", "will", "wie\u00b7der", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Welch eine Lieb' ist das? zu welchem Liebesgut?", "tokens": ["Welch", "ei\u00b7ne", "Lieb'", "ist", "das", "?", "zu", "wel\u00b7chem", "Lie\u00b7bes\u00b7gut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PDS", "$.", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem, das der Geiz nicht nehmen kann in Hut.", "tokens": ["Zu", "ei\u00b7nem", ",", "das", "der", "Geiz", "nicht", "neh\u00b7men", "kann", "in", "Hut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zu einem, das nicht wird durch kleinste Theilung kleiner,", "tokens": ["Zu", "ei\u00b7nem", ",", "das", "nicht", "wird", "durch", "kleins\u00b7te", "Thei\u00b7lung", "klei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PTKNEG", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tausend in Besitz ganz haben, ganz wie einer.", "tokens": ["Das", "tau\u00b7send", "in", "Be\u00b7sitz", "ganz", "ha\u00b7ben", ",", "ganz", "wie", "ei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "APPR", "NN", "ADV", "VAFIN", "$,", "ADV", "KOKOM", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Lieb' ist es zu Gott, die keinen aus will schlie\u00dfen,", "tokens": ["Die", "Lieb'", "ist", "es", "zu", "Gott", ",", "die", "kei\u00b7nen", "aus", "will", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PIAT", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr sich vielfach in Mitliebenden genie\u00dfen.", "tokens": ["Viel\u00b7mehr", "sich", "viel\u00b7fach", "in", "Mit\u00b7lie\u00b7ben\u00b7den", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das ist die Liebe, die noch nicht das Volk gewann,", "tokens": ["Das", "ist", "die", "Lie\u00b7be", ",", "die", "noch", "nicht", "das", "Volk", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das einen eignen Gott zu seinem Hort ersann.", "tokens": ["Das", "ei\u00b7nen", "eig\u00b7nen", "Gott", "zu", "sei\u00b7nem", "Hort", "er\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die hat auch nicht der Mann, der den zum allgemeinen", "tokens": ["Die", "hat", "auch", "nicht", "der", "Mann", ",", "der", "den", "zum", "all\u00b7ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "ART", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewordnen Hort der Welt neu machen will zum seinen.", "tokens": ["Ge\u00b7word\u00b7nen", "Hort", "der", "Welt", "neu", "ma\u00b7chen", "will", "zum", "sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "VMFIN", "APPRART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Liebe hat nur, wer mit Liebesandacht sieht", "tokens": ["Die", "Lie\u00b7be", "hat", "nur", ",", "wer", "mit", "Lie\u00b7be\u00b7san\u00b7dacht", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedweden Liebenden, der vorm Geliebten kniet.", "tokens": ["Jed\u00b7we\u00b7den", "Lie\u00b7ben\u00b7den", ",", "der", "vorm", "Ge\u00b7lieb\u00b7ten", "kniet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Auf welcher auch er kniet der tausend Tempelstufen;", "tokens": ["Auf", "wel\u00b7cher", "auch", "er", "kniet", "der", "tau\u00b7send", "Tem\u00b7pel\u00b7stu\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPER", "VVFIN", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Allerheiligste wird er mit Lieb' ihn rufen.", "tokens": ["Ins", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "wird", "er", "mit", "Lieb'", "ihn", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Nur lieblos wird er nicht ihn n\u00f6th'gen einzutreten,", "tokens": ["Nur", "lieb\u00b7los", "wird", "er", "nicht", "ihn", "n\u00f6\u00b7th'\u00b7gen", "ein\u00b7zu\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Noch minder wehren ihm auch drau\u00dfen anzubeten.", "tokens": ["Noch", "min\u00b7der", "weh\u00b7ren", "ihm", "auch", "drau\u00b7\u00dfen", "an\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}