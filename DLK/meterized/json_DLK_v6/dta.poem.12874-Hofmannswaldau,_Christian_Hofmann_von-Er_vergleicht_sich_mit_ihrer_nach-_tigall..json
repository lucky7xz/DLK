{"dta.poem.12874": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er vergleicht sich mit ihrer nach-  \n tigall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beliebte nachtigall! es schlie\u00dft dich zwar Lisette", "tokens": ["Be\u00b7lieb\u00b7te", "nach\u00b7ti\u00b7gall", "!", "es", "schlie\u00dft", "dich", "zwar", "Li\u00b7set\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wol als mich, in einen kercker ein.", "tokens": ["So", "wol", "als", "mich", ",", "in", "ei\u00b7nen", "ker\u00b7cker", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir liegen beyd\u2019 an einer kette,", "tokens": ["Wir", "lie\u00b7gen", "beyd'", "an", "ei\u00b7ner", "ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein dein zustand will doch nicht der meine seyn.", "tokens": ["Al\u00b7lein", "dein", "zu\u00b7stand", "will", "doch", "nicht", "der", "mei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ADV", "PTKNEG", "ART", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn was erwirbt dir nicht die anmuth deiner weisen?", "tokens": ["Denn", "was", "er\u00b7wirbt", "dir", "nicht", "die", "an\u00b7muth", "dei\u00b7ner", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es pflegt dich ihre gunst mit eigner hand zu speisen.", "tokens": ["Es", "pflegt", "dich", "ih\u00b7re", "gunst", "mit", "eig\u00b7ner", "hand", "zu", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir aber, hin ich gleich zum singen stets bereit,", "tokens": ["Mir", "a\u00b7ber", ",", "hin", "ich", "gleich", "zum", "sin\u00b7gen", "stets", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "PPER", "ADV", "APPRART", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will ihres hertzens grausamkeit", "tokens": ["Will", "ih\u00b7res", "hert\u00b7zens", "grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch keinen andern lohn gew\u00e4hren,", "tokens": ["Noch", "kei\u00b7nen", "an\u00b7dern", "lohn", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als d\u00fcrres brodt der angst, und wasser bittrer z\u00e4hren.", "tokens": ["Als", "d\u00fcr\u00b7res", "brodt", "der", "angst", ",", "und", "was\u00b7ser", "bit\u00b7trer", "z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "$,", "KON", "PWS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}