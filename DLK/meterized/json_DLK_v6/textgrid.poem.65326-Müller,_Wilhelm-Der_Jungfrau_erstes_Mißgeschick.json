{"textgrid.poem.65326": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Jungfrau erstes Mi\u00dfgeschick", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sang und sprang ich m\u00fc\u00dfig durch die Stube,", "tokens": ["J\u00fcngst", "sang", "und", "sprang", "ich", "m\u00fc\u00b7\u00dfig", "durch", "die", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sprach die Mutter: T\u00f6chterchen, bei Leibe,", "tokens": ["Da", "sprach", "die", "Mut\u00b7ter", ":", "T\u00f6ch\u00b7ter\u00b7chen", ",", "bei", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Thu' das nicht mehr! Bist aus den Kinderschuhen.", "tokens": ["Thu'", "das", "nicht", "mehr", "!", "Bist", "aus", "den", "Kin\u00b7der\u00b7schu\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "ADV", "$.", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Freier gucken schon durch unsre Th\u00fcren,", "tokens": ["Die", "Frei\u00b7er", "gu\u00b7cken", "schon", "durch", "uns\u00b7re", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da mu\u00df das kluge M\u00e4gdlein h\u00fcbsch in Ehren", "tokens": ["Da", "mu\u00df", "das", "klu\u00b7ge", "M\u00e4gd\u00b7lein", "h\u00fcbsch", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Herde stehn und ihre Spindel r\u00fchren.", "tokens": ["Am", "Her\u00b7de", "stehn", "und", "ih\u00b7re", "Spin\u00b7del", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, und da k\u00f6mmt mein Liebster just gegangen!", "tokens": ["Ach", ",", "und", "da", "k\u00f6mmt", "mein", "Liebs\u00b7ter", "just", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will nach meiner Spindel hastig springen,", "tokens": ["Ich", "will", "nach", "mei\u00b7ner", "Spin\u00b7del", "has\u00b7tig", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lasse sie gerad' in's Feuer fallen.", "tokens": ["Und", "las\u00b7se", "sie", "ge\u00b7rad'", "in's", "Feu\u00b7er", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}