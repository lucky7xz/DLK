nes Endes Ziel. La\u00df deiner Knechte Kinder", "tokens": ["blo\u00df", "dei\u00b7nes", "En\u00b7des", "Ziel", ".", "La\u00df", "dei\u00b7ner", "Knech\u00b7te", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$.", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "auch bleiben stets vor dir! Ihr Same sei nichts minder,", "tokens": ["auch", "blei\u00b7ben", "stets", "vor", "dir", "!", "Ihr", "Sa\u00b7me", "sei", "nichts", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "als unsrer V\u00e4ter war, von dir gebenedeit,", "tokens": ["als", "uns\u00b7rer", "V\u00e4\u00b7ter", "war", ",", "von", "dir", "ge\u00b7be\u00b7ne\u00b7deit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$,", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.88": {"text": "und breche, wie vor sie, durch alle b\u00f6se Zeit.", "tokens": ["und", "bre\u00b7che", ",", "wie", "vor", "sie", ",", "durch", "al\u00b7le", "b\u00f6\u00b7se", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "PWAV", "APPR", "PPER", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}