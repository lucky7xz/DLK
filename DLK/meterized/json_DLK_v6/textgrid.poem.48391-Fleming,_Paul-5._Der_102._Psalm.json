xa"}, "line.49": {"text": "Der Unterdr\u00fcckten Wundsch, das auserpre\u00dfte Flehen", "tokens": ["Der", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Wund\u00b7sch", ",", "das", "au\u00b7ser\u00b7pre\u00df\u00b7te", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "h\u00f6rt er, l\u00e4\u00dft keinen Man nicht h\u00fclflos von ihm gehen,", "tokens": ["h\u00f6rt", "er", ",", "l\u00e4\u00dft", "kei\u00b7nen", "Man", "nicht", "h\u00fcl\u00b7flos", "von", "ihm", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PIAT", "PIS", "PTKNEG", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.51": {"text": "der ihm nur trauen kan. Er wendet sich zu dir,", "tokens": ["der", "ihm", "nur", "trau\u00b7en", "kan", ".", "Er", "wen\u00b7det", "sich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "verschm\u00e4ht nicht, was du ihm in deiner Not tr\u00e4gst f\u00fcr.", "tokens": ["ver\u00b7schm\u00e4ht", "nicht", ",", "was", "du", "ihm", "in", "dei\u00b7ner", "Not", "tr\u00e4gst", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Das werd' in ewige Demanten eingegraben,", "tokens": ["Das", "werd'", "in