{"textgrid.poem.48391": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Der 102. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, h\u00f6re mein Gebet, und la\u00df mein sehnlichs Schreien", "tokens": ["Herr", ",", "h\u00f6\u00b7re", "mein", "Ge\u00b7bet", ",", "und", "la\u00df", "mein", "sehn\u00b7lichs", "Schrei\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu dir und vor dich ein! Verbirge nicht vom Neuen", "tokens": ["zu", "dir", "und", "vor", "dich", "ein", "!", "Ver\u00b7bir\u00b7ge", "nicht", "vom", "Neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "PPER", "PTKVZ", "$.", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dein Antlitz erst f\u00fcr mir! Neig', Herr, dein leises Ohr,", "tokens": ["dein", "Ant\u00b7litz", "erst", "f\u00fcr", "mir", "!", "Neig'", ",", "Herr", ",", "dein", "lei\u00b7ses", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPER", "$.", "NN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "vernimb, was in der Not ich dir ietzt bringe vor!", "tokens": ["ver\u00b7nimb", ",", "was", "in", "der", "Not", "ich", "dir", "ietzt", "brin\u00b7ge", "vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn meine Tage sind als wie ein Rauch vergangen,", "tokens": ["Denn", "mei\u00b7ne", "Ta\u00b7ge", "sind", "als", "wie", "ein", "Rauch", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der eh zerfleucht, als k\u00f6mpt. Die d\u00fcrren Beine hangen", "tokens": ["der", "eh", "zer\u00b7fleucht", ",", "als", "k\u00f6mpt", ".", "Die", "d\u00fcr\u00b7ren", "Bei\u00b7ne", "han\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "VVPP", "$,", "KOUS", "ADJD", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und sind ganz ausgebrant. Mein Herz ist wund und matt", "tokens": ["und", "sind", "ganz", "aus\u00b7ge\u00b7brant", ".", "Mein", "Herz", "ist", "wund", "und", "matt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wie ein verschmachter Halm, der nicht mehr Nahrung hat.", "tokens": ["wie", "ein", "ver\u00b7schmach\u00b7ter", "Halm", ",", "der", "nicht", "mehr", "Nah\u00b7rung", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin verduttet ganz, da\u00df ich auch kan vergessen", "tokens": ["Ich", "bin", "ver\u00b7dut\u00b7tet", "ganz", ",", "da\u00df", "ich", "auch", "kan", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das grauerliche Brot und ekle Kost zu essen.", "tokens": ["das", "grau\u00b7er\u00b7li\u00b7che", "Brot", "und", "ek\u00b7le", "Kost", "zu", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Backen trucknen aus, die Schl\u00e4fe fallen ein,", "tokens": ["Die", "Ba\u00b7cken", "truck\u00b7nen", "aus", ",", "die", "Schl\u00e4\u00b7fe", "fal\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ich bin durch steten Harm nur worden Haut und Bein.", "tokens": ["ich", "bin", "durch", "ste\u00b7ten", "Harm", "nur", "wor\u00b7den", "Haut", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "VAPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gleich als der Pelikan im w\u00fcsten Rohre schreiet", "tokens": ["Gleich", "als", "der", "Pe\u00b7li\u00b7kan", "im", "w\u00fcs\u00b7ten", "Roh\u00b7re", "schrei\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und wie ein wilder Kauz, der sich zu machen scheuet", "tokens": ["und", "wie", "ein", "wil\u00b7der", "Kauz", ",", "der", "sich", "zu", "ma\u00b7chen", "scheu\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "aus seiner \u00f6den Statt, gleich wie ein Vogel girrt,", "tokens": ["aus", "sei\u00b7ner", "\u00f6\u00b7den", "Statt", ",", "gleich", "wie", "ein", "Vo\u00b7gel", "girrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "KOKOM", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "wenn ihm sein Ehgemahl vom Garn' erhaschet wird,", "tokens": ["wenn", "ihm", "sein", "Eh\u00b7ge\u00b7mahl", "vom", "Garn'", "er\u00b7ha\u00b7schet", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "der stets sein Einsamsein ruft aus auf allen B\u00e4uen:", "tokens": ["der", "stets", "sein", "Ein\u00b7sam\u00b7sein", "ruft", "aus", "auf", "al\u00b7len", "B\u00e4u\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "so bin anietzo ich. Man schm\u00e4ht mich stets vom Neuen.", "tokens": ["so", "bin", "an\u00b7iet\u00b7zo", "ich", ".", "Man", "schm\u00e4ht", "mich", "stets", "vom", "Neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$.", "PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So oft es taget nur, so tritt mein Feind vor mich,", "tokens": ["So", "oft", "es", "ta\u00b7get", "nur", ",", "so", "tritt", "mein", "Feind", "vor", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "k\u00fchlt seinen Mut an mir und l\u00e4stert trotziglich.", "tokens": ["k\u00fchlt", "sei\u00b7nen", "Mut", "an", "mir", "und", "l\u00e4s\u00b7tert", "trot\u00b7zig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich bin sein Spott und Schwur. Wo ist mein erstes Tischen?", "tokens": ["Ich", "bin", "sein", "Spott", "und", "Schwur", ".", "Wo", "ist", "mein", "ers\u00b7tes", "Ti\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN", "$.", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Asch' e\u00df ich ietzt f\u00fcr Brot. Mit Thr\u00e4nen mu\u00df ich mischen", "tokens": ["Asch'", "e\u00df", "ich", "ietzt", "f\u00fcr", "Brot", ".", "Mit", "Thr\u00e4\u00b7nen", "mu\u00df", "ich", "mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "$.", "APPR", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "den ungeschmacken Trank, weil du so zornig bist", "tokens": ["den", "un\u00b7ge\u00b7schma\u00b7cken", "Trank", ",", "weil", "du", "so", "zor\u00b7nig", "bist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und deine Dr\u00e4uung mir das Mark und Seele fri\u00dft.", "tokens": ["und", "dei\u00b7ne", "Dr\u00e4u\u00b7ung", "mir", "das", "Mark", "und", "See\u00b7le", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du hubest mich empor hoch \u00fcber alle Gro\u00dfen.", "tokens": ["Du", "hu\u00b7best", "mich", "em\u00b7por", "hoch", "\u00fc\u00b7ber", "al\u00b7le", "Gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie hastu mich denn ietzt zu Boden so gesto\u00dfen?", "tokens": ["Wie", "has\u00b7tu", "mich", "denn", "ietzt", "zu", "Bo\u00b7den", "so", "ge\u00b7sto\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein ganzer Lebenslauf gleicht einem Schatten nur,", "tokens": ["Mein", "gan\u00b7zer", "Le\u00b7bens\u00b7lauf", "gleicht", "ei\u00b7nem", "Schat\u00b7ten", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "der, wenn der K\u00f6rper weicht, verl\u00e4sset keine Spur.", "tokens": ["der", ",", "wenn", "der", "K\u00f6r\u00b7per", "weicht", ",", "ver\u00b7l\u00e4s\u00b7set", "kei\u00b7ne", "Spur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bei Zusehn schwind' ich ab, der Lenden Mark verrinnet,", "tokens": ["Bei", "Zu\u00b7sehn", "schwind'", "ich", "ab", ",", "der", "Len\u00b7den", "Mark", "ver\u00b7rin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und ich dorr aus, wie Gras, das man am Warmen s\u00f6nnet.", "tokens": ["und", "ich", "dorr", "aus", ",", "wie", "Gras", ",", "das", "man", "am", "War\u00b7men", "s\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Was bin ich gegen dir, du starker Zebaoth?", "tokens": ["Was", "bin", "ich", "ge\u00b7gen", "dir", ",", "du", "star\u00b7ker", "Ze\u00b7bao\u00b7th", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du bleibest ewig Herr und ohne Wandel Gott,", "tokens": ["Du", "blei\u00b7best", "e\u00b7wig", "Herr", "und", "oh\u00b7ne", "Wan\u00b7del", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "dich \u00e4ndert keine Zeit, du Herrscher aller Zeiten.", "tokens": ["dich", "\u00e4n\u00b7dert", "kei\u00b7ne", "Zeit", ",", "du", "Herr\u00b7scher", "al\u00b7ler", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein ist die Ewigkeit, du Prinz der Ewigkeiten.", "tokens": ["Dein", "ist", "die", "E\u00b7wig\u00b7keit", ",", "du", "Prinz", "der", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn dieses Ganze denn die Glut wird \u00e4schern ein,", "tokens": ["Wenn", "die\u00b7ses", "Gan\u00b7ze", "denn", "die", "Glut", "wird", "\u00e4sc\u00b7hern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "so wird doch f\u00fcr und f\u00fcr noch dein Ged\u00e4chtn\u00fc\u00df sein.", "tokens": ["so", "wird", "doch", "f\u00fcr", "und", "f\u00fcr", "noch", "dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "KON", "APPR", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach! mache dich doch auf und hilf mir \u00e4rmsten Armen,", "tokens": ["Ach", "!", "ma\u00b7che", "dich", "doch", "auf", "und", "hilf", "mir", "\u00e4rms\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "wenn deines Zions Drangs du dich noch kanst erbarmen,", "tokens": ["wenn", "dei\u00b7nes", "Zi\u00b7ons", "Drangs", "du", "dich", "noch", "kanst", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "so mache dich doch auf! Ietzt ist es hohe Zeit,", "tokens": ["so", "ma\u00b7che", "dich", "doch", "auf", "!", "Ietzt", "ist", "es", "ho\u00b7he", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measu