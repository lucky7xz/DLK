er": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "und sich vor dessen Macht und Ehre willig schmiegen,", "tokens": ["und", "sich", "vor", "des\u00b7sen", "Macht", "und", "Eh\u00b7re", "wil\u00b7lig", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PRELAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "der von der heilgen H\u00f6h' auf dieses Tiefe schaut,", "tokens": ["der", "von", "der", "heil\u00b7gen", "H\u00f6h'", "auf", "die\u00b7ses", "Tie\u00b7fe", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "da\u00df er das arme Volk, das seiner Gnade traut,", "tokens": ["da\u00df", "er", "das", "ar\u00b7me", "Volk", ",", "das", "sei\u00b7ner", "Gna\u00b7de", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "und hart umbfesselt ist, aus seinen Ketten rei\u00dfe,", "tokens": ["und", "hart", "umb\u00b7fes\u00b7selt", "ist", ",", "aus", "sei\u00b7nen", "Ket\u00b7ten", "rei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "und den geschwornen Tod der Seufzenden zerschmei\u00dfe;", "tokens": ["und", "den", "ge\u00b7schwor\u00b7nen", "Tod", "der", "Seuf\u