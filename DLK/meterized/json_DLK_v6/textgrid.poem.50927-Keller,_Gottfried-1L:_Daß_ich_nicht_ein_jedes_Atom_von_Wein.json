{"textgrid.poem.50927": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df ich nicht ein jedes Atom von Wein", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich nicht ein jedes Atom von Wein", "tokens": ["Da\u00df", "ich", "nicht", "ein", "je\u00b7des", "A\u00b7tom", "von", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In einer Flut von Bl\u00f6digkeiten b\u00fc\u00dfe,", "tokens": ["In", "ei\u00b7ner", "Flut", "von", "Bl\u00f6\u00b7dig\u00b7kei\u00b7ten", "b\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schenke mir das perlende Gold vom Rhein", "tokens": ["Schen\u00b7ke", "mir", "das", "per\u00b7len\u00b7de", "Gold", "vom", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Unvermischt in seiner starken S\u00fc\u00dfe!", "tokens": ["Un\u00b7ver\u00b7mischt", "in", "sei\u00b7ner", "star\u00b7ken", "S\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Deine Augen la\u00df frei von Tr\u00e4nen sein,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "la\u00df", "frei", "von", "Tr\u00e4\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "VAINF", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df die lieblichen Strahlen nicht versiegen!", "tokens": ["Da\u00df", "die", "lieb\u00b7li\u00b7chen", "Strah\u00b7len", "nicht", "ver\u00b7sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Weich genug droht schon der bl\u00e4uliche Schein", "tokens": ["Weich", "ge\u00b7nug", "droht", "schon", "der", "bl\u00e4u\u00b7li\u00b7che", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie ein zartes Traumbild zu verfliegen.", "tokens": ["Wie", "ein", "zar\u00b7tes", "Traum\u00b7bild", "zu", "ver\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchlingstage, Stunden der Seligkeit,", "tokens": ["Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", ",", "Stun\u00b7den", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie sie linde in unsre Seelen rinnen! \u2013", "tokens": ["Wie", "sie", "lin\u00b7de", "in", "uns\u00b7re", "See\u00b7len", "rin\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADJA", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und wir sollten die k\u00f6stliche Neige Zeit", "tokens": ["Und", "wir", "soll\u00b7ten", "die", "k\u00f6st\u00b7li\u00b7che", "Nei\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mit dem Gedanken der Ewigkeit verd\u00fcnnen?", "tokens": ["Mit", "dem", "Ge\u00b7dan\u00b7ken", "der", "E\u00b7wig\u00b7keit", "ver\u00b7d\u00fcn\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}}}}