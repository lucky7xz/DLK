{"textgrid.poem.43396": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir singen von dem freien Rhein,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen von dem freien Rhein,", "tokens": ["Wir", "sin\u00b7gen", "von", "dem", "frei\u00b7en", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja freilich sollt' er frei wol sein!", "tokens": ["Ja", "frei\u00b7lich", "sollt'", "er", "frei", "wol", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch k\u00f6nnen wir's nicht weiter bringen,", "tokens": ["Doch", "k\u00f6n\u00b7nen", "wir's", "nicht", "wei\u00b7ter", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur vom freien Rhein zu singen.", "tokens": ["Als", "nur", "vom", "frei\u00b7en", "Rhein", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Freiheit lebt im Lied allein,", "tokens": ["Die", "Frei\u00b7heit", "lebt", "im", "Lied", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum singen wir vom freien Rhein.", "tokens": ["Drum", "sin\u00b7gen", "wir", "vom", "frei\u00b7en", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}