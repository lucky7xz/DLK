{"dta.poem.5434": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte P.. S.. j\u00fcngst sich vorgenommen, seinen Hund,", "tokens": ["Es", "hat\u00b7te", "P.", ".", "S.", ".", "j\u00fcngst", "sich", "vor\u00b7ge\u00b7nom\u00b7men", ",", "sei\u00b7nen", "Hund", ","], "token_info": ["word", "word", "abbreviation", "punct", "abbreviation", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$.", "NN", "$.", "ADV", "PRF", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den treu-und muthigen Alart, recht v\u00f6llig einmahl", "tokens": ["Den", "treu\u00b7\u00b7und", "mut\u00b7hi\u00b7gen", "A\u00b7lart", ",", "recht", "v\u00f6l\u00b7lig", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "satt zu machen;", "tokens": ["satt", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Er warf, des Endes, manchen Bissen von dem, was auf der", "tokens": ["Er", "warf", ",", "des", "En\u00b7des", ",", "man\u00b7chen", "Bis\u00b7sen", "von", "dem", ",", "was", "auf", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "PIAT", "NN", "APPR", "ART", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Tafel stund,", "tokens": ["Ta\u00b7fel", "stund", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nebst wei\u00df-und schwartzem Brodt ihm zu. Alart ri\u00df den", "tokens": ["Nebst", "wei\u00df\u00b7\u00b7und", "schwart\u00b7zem", "Brodt", "ihm", "zu", ".", "A\u00b7lart", "ri\u00df", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "PPER", "PTKVZ", "$.", "NE", "VVFIN", "ART"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "geschlossnen Rachen", "tokens": ["ge\u00b7schloss\u00b7nen", "Ra\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Bey jedem Wurf schnell von einander, schlang den erhasch-", "tokens": ["Bey", "je\u00b7dem", "Wurf", "schnell", "von", "ein\u00b7an\u00b7der", ",", "schlang", "den", "er\u00b7hasch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "APPR", "ADV", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "ten Bissen nieder,", "tokens": ["ten", "Bis\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ohn ihn zu kauen und zu schmecken, und schlo\u00df den heis-", "tokens": ["Ohn", "ihn", "zu", "kau\u00b7en", "und", "zu", "schme\u00b7cken", ",", "und", "schlo\u00df", "den", "heis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "sen Rachen wieder,", "tokens": ["sen", "Ra\u00b7chen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Mit starrem Blick nach mehr sich sehnend. Ich sah\u2019 Alarts", "tokens": ["Mit", "star\u00b7rem", "Blick", "nach", "mehr", "sich", "seh\u00b7nend", ".", "Ich", "sah'", "Al\u00b7arts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "PRF", "VVPP", "$.", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Betragen an,", "tokens": ["Be\u00b7tra\u00b7gen", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Da\u00df er von aller Niedlichkeit der ihm geg\u00f6nnten guten", "tokens": ["Da\u00df", "er", "von", "al\u00b7ler", "Nied\u00b7lich\u00b7keit", "der", "ihm", "ge\u00b7g\u00f6nn\u00b7ten", "gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ART", "PPER", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Bissen,", "tokens": ["Bis\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Weil er sie ungek\u00e4ut verschlang, nicht das geringste muste", "tokens": ["Weil", "er", "sie", "un\u00b7ge\u00b7k\u00e4ut", "ver\u00b7schlang", ",", "nicht", "das", "ge\u00b7rings\u00b7te", "mus\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "wissen.", "tokens": ["wis\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ach, dacht ich bey mir mit Betr\u00fcbnis, ach leider! da\u00df fast", "tokens": ["Ach", ",", "dacht", "ich", "bey", "mir", "mit", "Be\u00b7tr\u00fcb\u00b7nis", ",", "ach", "lei\u00b7der", "!", "da\u00df", "fast"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "$,", "ITJ", "ADV", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "jedermann", "tokens": ["je\u00b7der\u00b7mann"], "token_info": ["word"], "pos": ["PIS"], "meter": "+--", "measure": "dactylic.init"}, "line.20": {"text": "Mit dem uns zugeworffnen Guten, so uns der Sch\u00f6pfer", "tokens": ["Mit", "dem", "uns", "zu\u00b7ge\u00b7worff\u00b7nen", "Gu\u00b7ten", ",", "so", "uns", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$,", "ADV", "PPER", "ART", "NN"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "hier beschehrt,", "tokens": ["hier", "be\u00b7schehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Und oft in reichem Maa\u00df uns g\u00f6nnet, recht eben wie", "tokens": ["Und", "oft", "in", "rei\u00b7chem", "Maa\u00df", "uns", "g\u00f6n\u00b7net", ",", "recht", "e\u00b7ben", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,", "ADV", "ADV", "KOKOM"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Alart verf\u00e4hrt!", "tokens": ["A\u00b7lart", "ver\u00b7f\u00e4hrt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "An statt uns an Gesundheit, Klugheit, Geld, Ehr\u2019,", "tokens": ["An", "statt", "uns", "an", "Ge\u00b7sund\u00b7heit", ",", "Klug\u00b7heit", ",", "Geld", ",", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "und noch viel andren Gaben,", "tokens": ["und", "noch", "viel", "an\u00b7dren", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Die GOtt uns oft so reichlich schenckt, in fr\u00f6lichem Ge-", "tokens": ["Die", "Gott", "uns", "oft", "so", "reich\u00b7lich", "schenckt", ",", "in", "fr\u00f6\u00b7li\u00b7chem", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "nu\u00df zu laben,", "tokens": ["nu\u00df", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}