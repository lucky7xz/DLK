{"textgrid.poem.60872": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Horaz und Klopstock", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klopstock suchte, beschr\u00e4nkt wie Horaz auf Hymnus und Ode,", "tokens": ["Klops\u00b7tock", "such\u00b7te", ",", "be\u00b7schr\u00e4nkt", "wie", "Ho\u00b7raz", "auf", "Hym\u00b7nus", "und", "O\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "KOKOM", "NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Immer erhaben zu sein; aber es fehlte der Stoff.", "tokens": ["Im\u00b7mer", "er\u00b7ha\u00b7ben", "zu", "sein", ";", "a\u00b7ber", "es", "fehl\u00b7te", "der", "Stoff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn nicht lebte Horaz als deutscher Magister in Hamburg,", "tokens": ["Denn", "nicht", "leb\u00b7te", "Ho\u00b7raz", "als", "deut\u00b7scher", "Ma\u00b7gis\u00b7ter", "in", "Ham\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "NE", "KOUS", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aber in C\u00e4sars Rom, als es der Erde gebot.", "tokens": ["A\u00b7ber", "in", "C\u00e4\u00b7sars", "Rom", ",", "als", "es", "der", "Er\u00b7de", "ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Such, o moderner Poet, durch Geist zu erg\u00e4nzen des Stoffs Fehl,", "tokens": ["Such", ",", "o", "mo\u00b7der\u00b7ner", "Po\u00b7et", ",", "durch", "Geist", "zu", "er\u00b7g\u00e4n\u00b7zen", "des", "Stoffs", "Fehl", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "ART", "NN", "NN", "$,"], "meter": "+-+---+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Durch vielseitigen Stil decke die M\u00e4ngel der Zeit.", "tokens": ["Durch", "viel\u00b7sei\u00b7ti\u00b7gen", "Stil", "de\u00b7cke", "die", "M\u00e4n\u00b7gel", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}