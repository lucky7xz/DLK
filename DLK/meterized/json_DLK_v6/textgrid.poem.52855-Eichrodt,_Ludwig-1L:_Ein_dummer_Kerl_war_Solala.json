{"textgrid.poem.52855": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein dummer Kerl war Solala", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein dummer Kerl war Solala", "tokens": ["Ein", "dum\u00b7mer", "Kerl", "war", "So\u00b7la\u00b7la"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am liebsten voll des Weins,", "tokens": ["Am", "liebs\u00b7ten", "voll", "des", "Weins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was drau\u00dfen in der Welt geschah,", "tokens": ["Was", "drau\u00b7\u00dfen", "in", "der", "Welt", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war ihm furchtbar eins.", "tokens": ["Das", "war", "ihm", "furcht\u00b7bar", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er lebte in den Tag hinein", "tokens": ["Er", "leb\u00b7te", "in", "den", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Solala und pries den Wein.", "tokens": ["Der", "So\u00b7la\u00b7la", "und", "pries", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst sa\u00df er in dem Schwanen gut,", "tokens": ["Einst", "sa\u00df", "er", "in", "dem", "Schwa\u00b7nen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der edle Solala,", "tokens": ["Der", "ed\u00b7le", "So\u00b7la\u00b7la", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und trank sich frischen Lebensmuth,", "tokens": ["Und", "trank", "sich", "fri\u00b7schen", "Le\u00b7bens\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar lange sa\u00df er da.", "tokens": ["Gar", "lan\u00b7ge", "sa\u00df", "er", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Langeweile plagt' ihn sehr,", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", "plagt'", "ihn", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da kam der Veitel Stern daher.", "tokens": ["Da", "kam", "der", "Vei\u00b7tel", "Stern", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie geht's, wie steht's, Herr Solala?", "tokens": ["Wie", "geht's", ",", "wie", "steht's", ",", "Herr", "So\u00b7la\u00b7la", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beginnt der Veitel Stern,", "tokens": ["Be\u00b7ginnt", "der", "Vei\u00b7tel", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gibt's was zu handeln? - \"Handeln? Ja!", "tokens": ["Gibt's", "was", "zu", "han\u00b7deln", "?", "\"", "Han\u00b7deln", "?", "Ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PIS", "PTKZU", "VVINF", "$.", "$(", "$(", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was m\u00f6chtest, Veitel, gern?", "tokens": ["Was", "m\u00f6ch\u00b7test", ",", "Vei\u00b7tel", ",", "gern", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "NE", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Hauptsach' ist, da\u00df man was thut,", "tokens": ["Die", "Haupt\u00b7sach'", "ist", ",", "da\u00df", "man", "was", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist f\u00fcr Langeweile gut.\"", "tokens": ["Das", "ist", "f\u00fcr", "Lan\u00b7ge\u00b7wei\u00b7le", "gut", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr habt da sch\u00f6ne Kleider an,", "tokens": ["Ihr", "habt", "da", "sch\u00f6\u00b7ne", "Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wollt Ihr f\u00fcr den Rock?", "tokens": ["Was", "wollt", "Ihr", "f\u00fcr", "den", "Rock", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\"den Rock - ich nicht entbehren kann,", "tokens": ["\"", "den", "Rock", "ich", "nicht", "ent\u00b7beh\u00b7ren", "kann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nein, eher noch den Stock.", "tokens": ["Nein", ",", "e\u00b7her", "noch", "den", "Stock", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie sollt' ich mich denn ausziehn, he?", "tokens": ["Wie", "sollt'", "ich", "mich", "denn", "aus\u00b7ziehn", ",", "he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,", "FM", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wenn ich heut Abend ", "tokens": ["Wenn", "ich", "heut", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nun ja, den Stock! die lange Weil'", "tokens": ["Nun", "ja", ",", "den", "Stock", "!", "die", "lan\u00b7ge", "Weil'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertreibt er Euch doch nicht,", "tokens": ["Ver\u00b7treibt", "er", "Euch", "doch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\"ach nein, der Stock ist mir nicht feil.", "tokens": ["\"", "ach", "nein", ",", "der", "Stock", "ist", "mir", "nicht", "feil", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da geht mir auf ein Licht:", "tokens": ["Da", "geht", "mir", "auf", "ein", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Stock, den stellt man an die Wand,", "tokens": ["Den", "Stock", ",", "den", "stellt", "man", "an", "die", "Wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was thu ich dann mit meiner ", "tokens": ["Was", "thu", "ich", "dann", "mit", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Versteht sich. Aber Euer Hut?", "tokens": ["Ver\u00b7steht", "sich", ".", "A\u00b7ber", "Eu\u00b7er", "Hut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den la\u00dft mir ohne Streit.", "tokens": ["Den", "la\u00dft", "mir", "oh\u00b7ne", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\"mein Hut, mein Hut? Das geht nicht gut.", "tokens": ["\"", "mein", "Hut", ",", "mein", "Hut", "?", "Das", "geht", "nicht", "gut", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6r' an und sei gescheit:", "tokens": ["H\u00f6r'", "an", "und", "sei", "ge\u00b7scheit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn der Herr Pfarrer mich erkennt,", "tokens": ["Wenn", "der", "Herr", "Pfar\u00b7rer", "mich", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie mach' ich ihm mein ", "tokens": ["Wie", "mach'", "ich", "ihm", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ei, habt ihr da nicht eine Uhr?", "tokens": ["Ei", ",", "habt", "ihr", "da", "nicht", "ei\u00b7ne", "Uhr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\"da mu\u00df ich bitten schon,", "tokens": ["\"", "da", "mu\u00df", "ich", "bit\u00b7ten", "schon", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Uhr bekam ich, denk' Dir nur,", "tokens": ["Die", "Uhr", "be\u00b7kam", "ich", ",", "denk'", "Dir", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Confirmation.", "tokens": ["Zur", "Con\u00b7fir\u00b7ma\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da m\u00fc\u00dft' ich ja, nicht wahr, Herr Wirth?", "tokens": ["Da", "m\u00fc\u00dft'", "ich", "ja", ",", "nicht", "wahr", ",", "Herr", "Wirth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PTKNEG", "ADJD", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch einmal werden ", "tokens": ["Noch", "ein\u00b7mal", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Das Messer? \"Wenn's mich nur nicht gar", "tokens": ["Das", "Mes\u00b7ser", "?", "\"", "Wenn's", "mich", "nur", "nicht", "gar"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "PWS", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Vatern \u00fcberwirft.", "tokens": ["Mit", "Va\u00b7tern", "\u00fc\u00b7berw\u00b7irft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sagt, da\u00df alle Vierteljahr", "tokens": ["Er", "sagt", ",", "da\u00df", "al\u00b7le", "Vier\u00b7tel\u00b7jahr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich ein's verlieren d\u00fcrft'.", "tokens": ["Ich", "ein's", "ver\u00b7lie\u00b7ren", "d\u00fcrft'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie kann ich es verlieren dann,", "tokens": ["Wie", "kann", "ich", "es", "ver\u00b7lie\u00b7ren", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich's nicht mehr ", "tokens": ["Wenn", "ich's", "nicht", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Jetzt wei\u00df ich was! ruft Veitel Stern,", "tokens": ["Jetzt", "wei\u00df", "ich", "was", "!", "ruft", "Vei\u00b7tel", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$.", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sind wir doch so dumm!", "tokens": ["Was", "sind", "wir", "doch", "so", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geld hat von uns ein Jeder gern,", "tokens": ["Geld", "hat", "von", "uns", "ein", "Je\u00b7der", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So handeln wir darum!", "tokens": ["So", "han\u00b7deln", "wir", "da\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin reell, Ihr seid nicht knapp,", "tokens": ["Ich", "bin", "re\u00b7ell", ",", "Ihr", "seid", "nicht", "knapp", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kaufe Euer ", "tokens": ["Ich", "kau\u00b7fe", "Eu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "\"halloh, so schreit der Solala,", "tokens": ["\"", "hal\u00b7loh", ",", "so", "schreit", "der", "So\u00b7la\u00b7la", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein Handel fein,", "tokens": ["Das", "ist", "ein", "Han\u00b7del", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob man schon so was Neues sah?", "tokens": ["Ob", "man", "schon", "so", "was", "Neu\u00b7es", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PWS", "NN", "VVFIN", "$."