{"textgrid.poem.60169": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie flohen nicht, sie st\u00fcrzten und erreichten", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie flohen nicht, sie st\u00fcrzten und erreichten", "tokens": ["Sie", "flo\u00b7hen", "nicht", ",", "sie", "st\u00fcrz\u00b7ten", "und", "er\u00b7reich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deckbare W\u00e4lder, athemlos,", "tokens": ["Deck\u00b7ba\u00b7re", "W\u00e4l\u00b7der", ",", "at\u00b7hem\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "O Sieger! deine wundgeschlagne Feinde keichten:", "tokens": ["O", "Sie\u00b7ger", "!", "dei\u00b7ne", "wund\u00b7ge\u00b7schlag\u00b7ne", "Fein\u00b7de", "keich\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Held Heinrich ist gro\u00df.", "tokens": ["Held", "Hein\u00b7rich", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gleich einem wilden Thiere, festgeschlossen", "tokens": ["Gleich", "ei\u00b7nem", "wil\u00b7den", "Thie\u00b7re", ",", "fest\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sahst du, die Phalanx Oesterreichs,", "tokens": ["Sahst", "du", ",", "die", "Pha\u00b7lanx", "O\u00b7es\u00b7ter\u00b7reichs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NE", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr Stolz versah sich nicht, als deine Blitze schossen,", "tokens": ["Ihr", "Stolz", "ver\u00b7sah", "sich", "nicht", ",", "als", "dei\u00b7ne", "Blit\u00b7ze", "schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des t\u00f6dtlichen Streichs.", "tokens": ["Des", "t\u00f6dt\u00b7li\u00b7chen", "Streichs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Mit deinem lange Zeit gesch\u00e4rften Degen,", "tokens": ["Mit", "dei\u00b7nem", "lan\u00b7ge", "Zeit", "ge\u00b7sch\u00e4rf\u00b7ten", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zertrenntest du das Schuppenschild", "tokens": ["Zer\u00b7trenn\u00b7test", "du", "das", "Schup\u00b7pen\u00b7schild"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Thieres, das mit Grim, getreten dir entgegen;", "tokens": ["Des", "Thie\u00b7res", ",", "das", "mit", "Grim", ",", "ge\u00b7tre\u00b7ten", "dir", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fcrchterlich wild;", "tokens": ["So", "f\u00fcrch\u00b7ter\u00b7lich", "wild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Da\u00df von dem Rasseln seiner starken Glieder", "tokens": ["Da\u00df", "von", "dem", "Ras\u00b7seln", "sei\u00b7ner", "star\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Feld erth\u00f6nte, wie zur Zeit,", "tokens": ["Das", "Feld", "er\u00b7th\u00f6n\u00b7te", ",", "wie", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Claudius Marcell, den Gallier warf nieder,", "tokens": ["Als", "Clau\u00b7di\u00b7us", "Mar\u00b7cell", ",", "den", "Gal\u00b7lier", "warf", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im schr\u00f6cklichen Streit.", "tokens": ["Im", "schr\u00f6ck\u00b7li\u00b7chen", "Streit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und seine Waffen, gl\u00e4nzend gleich dem Blitze,", "tokens": ["Und", "sei\u00b7ne", "Waf\u00b7fen", ",", "gl\u00e4n\u00b7zend", "gleich", "dem", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geweyht dem Tempel Jupiters,", "tokens": ["Ge\u00b7weyht", "dem", "Tem\u00b7pel", "Ju\u00b7pi\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und frisch, mit Blut bespritzt, zur\u00fcckflog an die Spitze", "tokens": ["Und", "frisch", ",", "mit", "Blut", "be\u00b7spritzt", ",", "zu\u00b7r\u00fcck\u00b7flog", "an", "die", "Spit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des muthigen Heers.", "tokens": ["Des", "mut\u00b7hi\u00b7gen", "Heers", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}