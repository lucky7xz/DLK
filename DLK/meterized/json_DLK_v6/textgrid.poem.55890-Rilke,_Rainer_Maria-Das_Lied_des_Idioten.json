{"textgrid.poem.55890": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied des Idioten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hindern mich nicht. Sie lassen mich gehn.", "tokens": ["Sie", "hin\u00b7dern", "mich", "nicht", ".", "Sie", "las\u00b7sen", "mich", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sagen es k\u00f6nne nichts geschehn.", "tokens": ["Sie", "sa\u00b7gen", "es", "k\u00f6n\u00b7ne", "nichts", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "PIS", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie gut.", "tokens": ["Wie", "gut", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Es kann nichts geschehn. Alles kommt und kreist", "tokens": ["Es", "kann", "nichts", "ge\u00b7schehn", ".", "Al\u00b7les", "kommt", "und", "kreist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVPP", "$.", "PIS", "VVFIN", "KON", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "immerfort um den heiligen Geist,", "tokens": ["im\u00b7mer\u00b7fort", "um", "den", "hei\u00b7li\u00b7gen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "um den gewissen Geist (du wei\u00dft) \u2013,", "tokens": ["um", "den", "ge\u00b7wis\u00b7sen", "Geist", "(", "du", "wei\u00dft", ")", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "$(", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wie gut.", "tokens": ["wie", "gut", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Nein man mu\u00df wirklich nicht meinen es sei", "tokens": ["Nein", "man", "mu\u00df", "wirk\u00b7lich", "nicht", "mei\u00b7nen", "es", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIS", "VMFIN", "ADJD", "PTKNEG", "VVFIN", "PPER", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "irgend eine Gefahr dabei.", "tokens": ["ir\u00b7gend", "ei\u00b7ne", "Ge\u00b7fahr", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PAV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da ist freilich das Blut.", "tokens": ["Da", "ist", "frei\u00b7lich", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das Blut ist das Schwerste. Das Blut ist schwer.", "tokens": ["Das", "Blut", "ist", "das", "Schwers\u00b7te", ".", "Das", "Blut", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Manchmal glaub ich, ich kann nicht mehr \u2013.", "tokens": ["Manch\u00b7mal", "glaub", "ich", ",", "ich", "kann", "nicht", "mehr", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "$(", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "(wie gut.)", "tokens": ["(", "wie", "gut", ".", ")"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Ah was ist das f\u00fcr ein sch\u00f6ner Ball;", "tokens": ["Ah", "was", "ist", "das", "f\u00fcr", "ein", "sch\u00f6\u00b7ner", "Ball", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VAFIN", "PDS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "rot und rund wie ein \u00dcberall.", "tokens": ["rot", "und", "rund", "wie", "ein", "\u00dc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Gut, da\u00df ihr ihn erschuft.", "tokens": ["Gut", ",", "da\u00df", "ihr", "ihn", "er\u00b7schuft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ob der wohl kommt wenn man ruft?", "tokens": ["Ob", "der", "wohl", "kommt", "wenn", "man", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "VVFIN", "KOUS", "PIS", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wie sich das alles seltsam benimmt,", "tokens": ["Wie", "sich", "das", "al\u00b7les", "selt\u00b7sam", "be\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ineinandertreibt, auseinanderschwimmt:", "tokens": ["in\u00b7ein\u00b7an\u00b7der\u00b7treibt", ",", "aus\u00b7ein\u00b7an\u00b7der\u00b7schwimmt", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "freundlich, ein wenig unbestimmt.", "tokens": ["freund\u00b7lich", ",", "ein", "we\u00b7nig", "un\u00b7be\u00b7stimmt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "PIS", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie gut.", "tokens": ["Wie", "gut", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}