{"dta.poem.5299": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt seh ich, mit ger\u00fchrten Blicken,", "tokens": ["Jetzt", "seh", "ich", ",", "mit", "ge\u00b7r\u00fchr\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein frisch und fast lebendig Gr\u00fcn,", "tokens": ["Ein", "frisch", "und", "fast", "le\u00b7ben\u00b7dig", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erde-Fl\u00e4chen \u00fcberziehen.", "tokens": ["Der", "Er\u00b7de\u00b7Fl\u00e4\u00b7chen", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6re gleichsam mit Entz\u00fccken,", "tokens": ["Ich", "h\u00f6\u00b7re", "gleich\u00b7sam", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der neuen V\u00f6gel Harmonien,", "tokens": ["Der", "neu\u00b7en", "V\u00f6\u00b7gel", "Har\u00b7mo\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bem\u00fcht, die Menschen zu erqvicken.", "tokens": ["Be\u00b7m\u00fcht", ",", "die", "Men\u00b7schen", "zu", "er\u00b7qvi\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man siehet, wo die Blumen bl\u00fchn,", "tokens": ["Man", "sie\u00b7het", ",", "wo", "die", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fast in buntem Feuer gl\u00fchn,", "tokens": ["Und", "fast", "in", "bun\u00b7tem", "Feu\u00b7er", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Beeten recht in g\u00fcldnen St\u00fccken,", "tokens": ["Die", "Bee\u00b7ten", "recht", "in", "g\u00fcld\u00b7nen", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit Ranckenwerck, worin Carmin,", "tokens": ["Mit", "Ran\u00b7cken\u00b7werck", ",", "wo\u00b7rin", "Car\u00b7min", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sammt Purpur und Ultramarin,", "tokens": ["Sammt", "Pur\u00b7pur", "und", "Ult\u00b7ra\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Den holden Schmuck der Kr\u00e4uter schm\u00fccken,", "tokens": ["Den", "hol\u00b7den", "Schmuck", "der", "Kr\u00e4u\u00b7ter", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ja, scheint nicht \u00f6fters ein Rubin,", "tokens": ["Ja", ",", "scheint", "nicht", "\u00f6f\u00b7ters", "ein", "Ru\u00b7bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG", "ADV", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Durch das Smaragden-gleiche Gr\u00fcn,", "tokens": ["Durch", "das", "Sma\u00b7rag\u00b7den\u00b7glei\u00b7che", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Uns gleichsam Strahlen zuzuschicken?", "tokens": ["Uns", "gleich\u00b7sam", "Strah\u00b7len", "zu\u00b7zu\u00b7schi\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ach da\u00df mein Geist, wenn oft in ihn", "tokens": ["Ach", "da\u00df", "mein", "Geist", ",", "wenn", "oft", "in", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich die Betrachtungen bem\u00fchn", "tokens": ["Sich", "die", "Be\u00b7trach\u00b7tun\u00b7gen", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVINF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Des Fr\u00fchlings Sch\u00f6nheit abzudr\u00fccken,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Sch\u00f6n\u00b7heit", "ab\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Oft von des Sch\u00f6pfers Werck, ein reiner Spiegel schien!", "tokens": ["Oft", "von", "des", "Sch\u00f6p\u00b7fers", "Werck", ",", "ein", "rei\u00b7ner", "Spie\u00b7gel", "schien", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}