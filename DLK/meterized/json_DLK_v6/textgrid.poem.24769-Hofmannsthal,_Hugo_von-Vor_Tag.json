{"textgrid.poem.24769": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Vor Tag", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun liegt und zuckt am fahlen Himmelsrand", "tokens": ["Nun", "liegt", "und", "zuckt", "am", "fah\u00b7len", "Him\u00b7mels\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In sich zusammengesunken das Gewitter.", "tokens": ["In", "sich", "zu\u00b7sam\u00b7men\u00b7ge\u00b7sun\u00b7ken", "das", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nun denkt der Kranke: \u00bbTag! jetzt werd ich schlafen!\u00ab", "tokens": ["Nun", "denkt", "der", "Kran\u00b7ke", ":", "\u00bb", "Tag", "!", "jetzt", "werd", "ich", "schla\u00b7fen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcckt die hei\u00dfen Lider zu. Nun streckt", "tokens": ["Und", "dr\u00fcckt", "die", "hei\u00b7\u00dfen", "Li\u00b7der", "zu", ".", "Nun", "streckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die junge Kuh im Stall die starken N\u00fcstern", "tokens": ["Die", "jun\u00b7ge", "Kuh", "im", "Stall", "die", "star\u00b7ken", "N\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach k\u00fchlem Fr\u00fchduft. Nun im stummen Wald", "tokens": ["Nach", "k\u00fch\u00b7lem", "Fr\u00fch\u00b7duft", ".", "Nun", "im", "stum\u00b7men", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hebt der Landstreicher ungewaschen sich", "tokens": ["Hebt", "der", "Land\u00b7strei\u00b7cher", "un\u00b7ge\u00b7wa\u00b7schen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "PRF"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Aus weichem Bett vorj\u00e4hrigen Laubes auf", "tokens": ["Aus", "wei\u00b7chem", "Bett", "vor\u00b7j\u00e4h\u00b7ri\u00b7gen", "Lau\u00b7bes", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und wirft mit frecher Hand den n\u00e4chsten Stein", "tokens": ["Und", "wirft", "mit", "fre\u00b7cher", "Hand", "den", "n\u00e4chs\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nach einer Taube, die schlaftrunken fliegt,", "tokens": ["Nach", "ei\u00b7ner", "Tau\u00b7be", ",", "die", "schlaf\u00b7trun\u00b7ken", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und graust sich selber, wie der Stein so dumpf", "tokens": ["Und", "graust", "sich", "sel\u00b7ber", ",", "wie", "der", "Stein", "so", "dumpf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,", "PWAV", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und schwer zur Erde f\u00e4llt. Nun rennt das Wasser,", "tokens": ["Und", "schwer", "zur", "Er\u00b7de", "f\u00e4llt", ".", "Nun", "rennt", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Als wollte es der Nacht, der fortgeschlichnen, nach", "tokens": ["Als", "woll\u00b7te", "es", "der", "Nacht", ",", "der", "fort\u00b7ge\u00b7schlich\u00b7nen", ",", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ins Dunkel st\u00fcrzen, unteilnehmend, wild", "tokens": ["Ins", "Dun\u00b7kel", "st\u00fcr\u00b7zen", ",", "un\u00b7teil\u00b7neh\u00b7mend", ",", "wild"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und kalten Hauches hin, indessen droben", "tokens": ["Und", "kal\u00b7ten", "Hau\u00b7ches", "hin", ",", "in\u00b7des\u00b7sen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Heiland und die Mutter leise, leise", "tokens": ["Der", "Hei\u00b7land", "und", "die", "Mut\u00b7ter", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sich unterreden auf dem Br\u00fccklein: leise,", "tokens": ["Sich", "un\u00b7ter\u00b7re\u00b7den", "auf", "dem", "Br\u00fcck\u00b7lein", ":", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "$.", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und doch ist ihre kleine Rede ewig", "tokens": ["Und", "doch", "ist", "ih\u00b7re", "klei\u00b7ne", "Re\u00b7de", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und unzerst\u00f6rbar wie die Sterne droben.", "tokens": ["Und", "un\u00b7zer\u00b7st\u00f6r\u00b7bar", "wie", "die", "Ster\u00b7ne", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Er tr\u00e4gt sein Kreuz und sagt nur: \u00bbMeine Mutter!\u00ab", "tokens": ["Er", "tr\u00e4gt", "sein", "Kreuz", "und", "sagt", "nur", ":", "\u00bb", "Mei\u00b7ne", "Mut\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "$.", "$(", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und sieht sie an, und: \u00bbAch, mein lieber Sohn!\u00ab", "tokens": ["Und", "sieht", "sie", "an", ",", "und", ":", "\u00bb", "Ach", ",", "mein", "lie\u00b7ber", "Sohn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "$.", "$(", "ITJ", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Sagt sie. \u2013 Nun hat der Himmel mit der Erde", "tokens": ["Sagt", "sie", ".", "\u2013", "Nun", "hat", "der", "Him\u00b7mel", "mit", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein stumm beklemmend Zwiegespr\u00e4ch. Dann geht", "tokens": ["Ein", "stumm", "be\u00b7klem\u00b7mend", "Zwie\u00b7ge\u00b7spr\u00e4ch", ".", "Dann", "geht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ein Schauer durch den schweren, alten Leib:", "tokens": ["Ein", "Schau\u00b7er", "durch", "den", "schwe\u00b7ren", ",", "al\u00b7ten", "Leib", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sie r\u00fcstet sich, den neuen Tag zu leben.", "tokens": ["Sie", "r\u00fcs\u00b7tet", "sich", ",", "den", "neu\u00b7en", "Tag", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Nun steigt das geisterhafte Fr\u00fchlicht. Nun", "tokens": ["Nun", "steigt", "das", "geis\u00b7ter\u00b7haf\u00b7te", "Fr\u00fch\u00b7licht", ".", "Nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Schleicht einer ohne Schuh von einem Frauenbett,", "tokens": ["Schleicht", "ei\u00b7ner", "oh\u00b7ne", "Schuh", "von", "ei\u00b7nem", "Frau\u00b7en\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "L\u00e4uft wie ein Schatten, klettert wie ein Dieb", "tokens": ["L\u00e4uft", "wie", "ein", "Schat\u00b7ten", ",", "klet\u00b7tert", "wie", "ein", "Dieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Durchs Fenster in sein eigenes Zimmer, sieht", "tokens": ["Durchs", "Fens\u00b7ter", "in", "sein", "ei\u00b7ge\u00b7nes", "Zim\u00b7mer", ",", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Sich im Wandspiegel und hat pl\u00f6tzlich Angst", "tokens": ["Sich", "im", "Wand\u00b7spie\u00b7gel", "und", "hat", "pl\u00f6tz\u00b7lich", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "KON", "VAFIN", "ADJD", "NN"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.31": {"text": "Vor diesem blassen, \u00fcbern\u00e4chtigen Fremden,", "tokens": ["Vor", "die\u00b7sem", "blas\u00b7sen", ",", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7gen", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Als h\u00e4tte dieser selbe heute nacht", "tokens": ["Als", "h\u00e4t\u00b7te", "die\u00b7ser", "sel\u00b7be", "heu\u00b7te", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Den guten Knaben, der er war, ermordet", "tokens": ["Den", "gu\u00b7ten", "Kna\u00b7ben", ",", "der", "er", "war", ",", "er\u00b7mor\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und k\u00e4me jetzt, die H\u00e4nde sich zu waschen", "tokens": ["Und", "k\u00e4\u00b7me", "jetzt", ",", "die", "H\u00e4n\u00b7de", "sich", "zu", "wa\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Im Kr\u00fcglein seines Opfers wie zum Hohn,", "tokens": ["Im", "Kr\u00fcg\u00b7lein", "sei\u00b7nes", "Op\u00b7fers", "wie", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und darum sei der Himmel so beklommen", "tokens": ["Und", "da\u00b7rum", "sei", "der", "Him\u00b7mel", "so", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und alles in der Luft so sonderbar.", "tokens": ["Und", "al\u00b7les", "in", "der", "Luft", "so", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Nun geht die Stallt\u00fcr. Und nun ist auch Tag.", "tokens": ["Nun", "geht", "die", "Stall\u00b7t\u00fcr", ".", "Und", "nun", "ist", "auch", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "KON", "ADV", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}