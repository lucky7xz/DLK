{"dta.poem.9413": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stam\u0304buch/ auf den Ged\u00e4cht-  \n n\u00fcs-spruch:  \n  Christen Seyn Jmmer Streiter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Christen Seyn Jmmer Streiter hier auf Erden/", "tokens": ["Chris\u00b7ten", "Seyn", "Jm\u00b7mer", "Strei\u00b7ter", "hier", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Bi\u00df zu den Frommen Sie versetzet werden/", "tokens": ["Bi\u00df", "zu", "den", "From\u00b7men", "Sie", "ver\u00b7set\u00b7zet", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wird mit Wollust/ Frieden und mit Freuden", "tokens": ["Da", "wird", "mit", "Wol\u00b7lust", "/", "Frie\u00b7den", "und", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "$(", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jesus Sie weiden.", "tokens": ["Je\u00b7sus", "Sie", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Christen Seyn Jmmer Seelig und in frieden/", "tokens": ["Chris\u00b7ten", "Seyn", "Jm\u00b7mer", "See\u00b7lig", "und", "in", "frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "APPR", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wenn Sie aus diesem Elend abgeschieden/", "tokens": ["Wenn", "Sie", "aus", "die\u00b7sem", "E\u00b7lend", "ab\u00b7ge\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn Sie gegleubet: Jhnen wird gegeben", "tokens": ["Wenn", "Sie", "ge\u00b7gleu\u00b7bet", ":", "Jh\u00b7nen", "wird", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ewiges Leben.", "tokens": ["E\u00b7wi\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}