{"textgrid.poem.47470": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zur W\u00fcste grimmig ausgebrannt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur W\u00fcste grimmig ausgebrannt", "tokens": ["Zur", "W\u00fcs\u00b7te", "grim\u00b7mig", "aus\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von hei\u00dfer Bu\u00dfe Sonnenstich,", "tokens": ["Von", "hei\u00b7\u00dfer", "Bu\u00b7\u00dfe", "Son\u00b7nen\u00b7stich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War meines Seins verlorne Flur;", "tokens": ["War", "mei\u00b7nes", "Seins", "ver\u00b7lor\u00b7ne", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da st\u00e4ubet' ich, da wirbelt' ich,", "tokens": ["Da", "st\u00e4u\u00b7bet'", "ich", ",", "da", "wir\u00b7belt'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein aufgew\u00fchlter W\u00fcstensand,", "tokens": ["Ein", "auf\u00b7ge\u00b7w\u00fchl\u00b7ter", "W\u00fcs\u00b7ten\u00b7sand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch in die Luft getragen", "tokens": ["Hoch", "in", "die", "Luft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vom Winde, zum Azur.", "tokens": ["Vom", "Win\u00b7de", ",", "zum", "A\u00b7zur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gott sei gelobt! Er hat mir", "tokens": ["Gott", "sei", "ge\u00b7lobt", "!", "Er", "hat", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die m\u00f6rderische Gluth ged\u00e4mpft,", "tokens": ["Die", "m\u00f6r\u00b7de\u00b7ri\u00b7sche", "Gluth", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mir einen Regen herabgesandt,", "tokens": ["Mir", "ei\u00b7nen", "Re\u00b7gen", "her\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mich mild zur\u00fcckgeschlagen", "tokens": ["Mich", "mild", "zu\u00b7r\u00fcck\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Zu meinem alten Ruhestand,", "tokens": ["Zu", "mei\u00b7nem", "al\u00b7ten", "Ru\u00b7he\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hat mich gemacht zu fr\u00f6hlichem,", "tokens": ["Hat", "mich", "ge\u00b7macht", "zu", "fr\u00f6h\u00b7li\u00b7chem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PTKZU", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.14": {"text": "Frisch aufgebl\u00fchtem Land.", "tokens": ["Frisch", "auf\u00b7ge\u00b7bl\u00fch\u00b7tem", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}