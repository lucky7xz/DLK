{"dta.poem.226": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er wegen der Liebe zu der  Esperancen  \n das Leben verliehren solte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Menschen m\u00fcssen sterben?", "tokens": ["Wenn", "die", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche einen Engel sehn:", "tokens": ["Wel\u00b7che", "ei\u00b7nen", "En\u00b7gel", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird meine Brust verderben", "tokens": ["So", "wird", "mei\u00b7ne", "Brust", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Und es ist mit mir geschehn.", "tokens": ["Und", "es", "ist", "mit", "mir", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn den Anblick solcher Strahlen", "tokens": ["Denn", "den", "An\u00b7blick", "sol\u00b7cher", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu bezahlen/", "tokens": ["Zu", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mu\u00df ich bald zu Grunde gehn.", "tokens": ["Mu\u00df", "ich", "bald", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil ich mich zu hoch verstiegen", "tokens": ["Weil", "ich", "mich", "zu", "hoch", "ver\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKA", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Engel-Bild geliebt/", "tokens": ["Und", "ein", "En\u00b7gel\u00b7Bild", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht mirs/ als mit ", "tokens": ["Geht", "mirs", "/", "als", "mit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "$(", "KOKOM", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und mein Stoltz den Tod mir giebt.", "tokens": ["Und", "mein", "Stoltz", "den", "Tod", "mir", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum will ich den Entschlu\u00df fassen/", "tokens": ["Drum", "will", "ich", "den", "Ent\u00b7schlu\u00df", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erblassen/", "tokens": ["Und", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mehr vergn\u00fcget als betr\u00fcbt.", "tokens": ["Mehr", "ver\u00b7gn\u00fc\u00b7get", "als", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles was die Welt uns schencket/", "tokens": ["Al\u00b7les", "was", "die", "Welt", "uns", "schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRELS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reucht doch nur nach Unbestand", "tokens": ["Reucht", "doch", "nur", "nach", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man sich zu freuen dencket/", "tokens": ["Wenn", "man", "sich", "zu", "freu\u00b7en", "den\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Schmertz schon bey der Hand.", "tokens": ["Ist", "der", "Schmertz", "schon", "bey", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil nun nichts best\u00e4ndig heisset/", "tokens": ["Weil", "nun", "nichts", "be\u00b7st\u00e4n\u00b7dig", "heis\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So zerreisset", "tokens": ["So", "zer\u00b7reis\u00b7set"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Auch nur meines Lebens-Band.", "tokens": ["Auch", "nur", "mei\u00b7nes", "Le\u00b7bens\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lebt wohl meine ", "tokens": ["Lebt", "wohl", "mei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gute Nacht du Vater Hertz", "tokens": ["Gu\u00b7te", "Nacht", "du", "Va\u00b7ter", "Hertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Lebens-Fackel Glantze/", "tokens": ["Mit", "der", "Le\u00b7bens\u00b7Fa\u00b7ckel", "Glant\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lencket sichs schon Abend-werts", "tokens": ["Len\u00b7cket", "sichs", "schon", "A\u00b7ben\u00b7dwerts"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat man nun mein Blut vergossen", "tokens": ["Hat", "man", "nun", "mein", "Blut", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie beschlossen/", "tokens": ["Wie", "be\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ach so h\u00f6ret auf mein Schmertz.", "tokens": ["Ach", "so", "h\u00f6\u00b7ret", "auf", "mein", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}