{"textgrid.poem.55082": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Fischer", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wasser rauscht', das Wasser schwoll,", "tokens": ["Das", "Was\u00b7ser", "rauscht'", ",", "das", "Was\u00b7ser", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fischer sa\u00df daran,", "tokens": ["Ein", "Fi\u00b7scher", "sa\u00df", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah nach dem Angel ruhevoll,", "tokens": ["Sah", "nach", "dem", "An\u00b7gel", "ru\u00b7he\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "K\u00fchl bis ans Herz hinan.", "tokens": ["K\u00fchl", "bis", "ans", "Herz", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie er sitzt und wie er lauscht,", "tokens": ["Und", "wie", "er", "sitzt", "und", "wie", "er", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Teilt sich die Flut empor;", "tokens": ["Teilt", "sich", "die", "Flut", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus dem bewegten Wasser rauscht", "tokens": ["Aus", "dem", "be\u00b7weg\u00b7ten", "Was\u00b7ser", "rauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein feuchtes Weib hervor.", "tokens": ["Ein", "feuch\u00b7tes", "Weib", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sang zu ihm, sie sprach zu ihm:", "tokens": ["Sie", "sang", "zu", "ihm", ",", "sie", "sprach", "zu", "ihm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwas lockst du meine Brut", "tokens": ["\u00bb", "was", "lockst", "du", "mei\u00b7ne", "Brut"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Menschenwitz und Menschenlist", "tokens": ["Mit", "Men\u00b7schen\u00b7witz", "und", "Men\u00b7schen\u00b7list"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinauf in Todesglut?", "tokens": ["Hin\u00b7auf", "in", "To\u00b7des\u00b7glut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach w\u00fc\u00dftest du, wie's Fischlein ist", "tokens": ["Ach", "w\u00fc\u00df\u00b7test", "du", ",", "wie's", "Fisc\u00b7hlein", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wohlig auf dem Grund,", "tokens": ["So", "woh\u00b7lig", "auf", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du stiegst herunter, wie du bist,", "tokens": ["Du", "stiegst", "her\u00b7un\u00b7ter", ",", "wie", "du", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und w\u00fcrdest erst gesund.", "tokens": ["Und", "w\u00fcr\u00b7dest", "erst", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Labt sich die liebe Sonne nicht,", "tokens": ["Labt", "sich", "die", "lie\u00b7be", "Son\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond sich nicht im Meer?", "tokens": ["Der", "Mond", "sich", "nicht", "im", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kehrt wellenatmend ihr Gesicht", "tokens": ["Kehrt", "wel\u00b7le\u00b7nat\u00b7mend", "ihr", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht doppelt sch\u00f6ner her?", "tokens": ["Nicht", "dop\u00b7pelt", "sch\u00f6\u00b7ner", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lockt dich der tiefe Himmel nicht,", "tokens": ["Lockt", "dich", "der", "tie\u00b7fe", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das feuchtverkl\u00e4rte Blau?", "tokens": ["Das", "feucht\u00b7ver\u00b7kl\u00e4r\u00b7te", "Blau", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lockt dich dein eigen Angesicht", "tokens": ["Lockt", "dich", "dein", "ei\u00b7gen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht her in ew'gen Tau?\u00ab", "tokens": ["Nicht", "her", "in", "ew'\u00b7gen", "Tau", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Wasser rauscht', das Wasser schwoll,", "tokens": ["Das", "Was\u00b7ser", "rauscht'", ",", "das", "Was\u00b7ser", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Netzt' ihm den nackten Fu\u00df;", "tokens": ["Netzt'", "ihm", "den", "nack\u00b7ten", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sein Herz wuchs ihm so sehnsuchtsvoll,", "tokens": ["Sein", "Herz", "wuchs", "ihm", "so", "sehn\u00b7suchts\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bei der Liebsten Gru\u00df.", "tokens": ["Wie", "bei", "der", "Liebs\u00b7ten", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sprach zu ihm, sie sang zu ihm;", "tokens": ["Sie", "sprach", "zu", "ihm", ",", "sie", "sang", "zu", "ihm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da war's um ihn geschehn:", "tokens": ["Da", "wa\u00b7r's", "um", "ihn", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Halb zog sie ihn, halb sank er hin,", "tokens": ["Halb", "zog", "sie", "ihn", ",", "halb", "sank", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ward nicht mehr gesehn.", "tokens": ["Und", "ward", "nicht", "mehr", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}