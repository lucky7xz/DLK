{"dta.poem.15947": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mannhafte Poesie ist was ich hier, o Sohn,", "tokens": ["Mann\u00b7haf\u00b7te", "Poe\u00b7sie", "ist", "was", "ich", "hier", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PWS", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir bringe, denn du hast die knabenhafte schon.", "tokens": ["Dir", "brin\u00b7ge", ",", "denn", "du", "hast", "die", "kna\u00b7ben\u00b7haf\u00b7te", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mannhafte Poesie, die Grundsatz und Gedanken", "tokens": ["Mann\u00b7haf\u00b7te", "Poe\u00b7sie", ",", "die", "Grund\u00b7satz", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "F\u00fchrt gegen Fantasie und Traumwerk in die Schranken:", "tokens": ["F\u00fchrt", "ge\u00b7gen", "Fan\u00b7ta\u00b7sie", "und", "Traum\u00b7werk", "in", "die", "Schran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Das Kinderm\u00e4rchen aus der Vorzeit Ammenstuben,", "tokens": ["Das", "Kin\u00b7der\u00b7m\u00e4r\u00b7chen", "aus", "der", "Vor\u00b7zeit", "Am\u00b7men\u00b7stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von S\u00e4ngern, K\u00f6nigen, Rittern und Reutersbuben;", "tokens": ["Von", "S\u00e4n\u00b7gern", ",", "K\u00f6\u00b7ni\u00b7gen", ",", "Rit\u00b7tern", "und", "Reu\u00b7ters\u00b7bu\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Vor\u00fcber tanzte dir der bunte Spuk, wor\u00fcber", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "tanz\u00b7te", "dir", "der", "bun\u00b7te", "Spuk", ",", "wo\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du einst dich freutest, freu dich nun, das ist vor\u00fcber.", "tokens": ["Du", "einst", "dich", "freu\u00b7test", ",", "freu", "dich", "nun", ",", "das", "ist", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht stehen bleiben sollst du mir beim Knabenhaften;", "tokens": ["Nicht", "ste\u00b7hen", "blei\u00b7ben", "sollst", "du", "mir", "beim", "Kna\u00b7ben\u00b7haf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVINF", "VMFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer werden will ein Mann, darf nicht am Knaben haften.", "tokens": ["Wer", "wer\u00b7den", "will", "ein", "Mann", ",", "darf", "nicht", "am", "Kna\u00b7ben", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAINF", "VMFIN", "ART", "NN", "$,", "VMFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}