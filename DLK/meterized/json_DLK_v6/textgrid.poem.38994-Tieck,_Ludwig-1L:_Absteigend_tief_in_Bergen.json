{"textgrid.poem.38994": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Absteigend tief in Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Absteigend tief in Bergen", "tokens": ["Ab\u00b7stei\u00b7gend", "tief", "in", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dem Ruheplatz mich n\u00e4hernd,", "tokens": ["Dem", "Ru\u00b7he\u00b7platz", "mich", "n\u00e4\u00b7hernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vernehm' ich deutsches Wort,", "tokens": ["Ver\u00b7nehm'", "ich", "deut\u00b7sches", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus unbekanntem Munde.", "tokens": ["Aus", "un\u00b7be\u00b7kann\u00b7tem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von neuem geht das Herz mir auf,", "tokens": ["Von", "neu\u00b7em", "geht", "das", "Herz", "mir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Thr\u00e4nen ergie\u00dfen sich", "tokens": ["Und", "Thr\u00e4\u00b7nen", "er\u00b7gie\u00b7\u00dfen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei der bescheidenen Bitte", "tokens": ["Bei", "der", "be\u00b7schei\u00b7de\u00b7nen", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Des wandernden Manns und der Frau.", "tokens": ["Des", "wan\u00b7dern\u00b7den", "Manns", "und", "der", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Sie wandern von Rom,", "tokens": ["Sie", "wan\u00b7dern", "von", "Rom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wo sie alle Heiligth\u00fcmer gegr\u00fc\u00dft,", "tokens": ["Wo", "sie", "al\u00b7le", "Hei\u00b7ligt\u00b7h\u00fc\u00b7mer", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "In Staub und Hitze", "tokens": ["In", "Staub", "und", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Zum fernen Schwarzwald, der Heimath zur\u00fcck.", "tokens": ["Zum", "fer\u00b7nen", "Schwarz\u00b7wald", ",", "der", "Hei\u00b7math", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "In N\u00f6then gebetet", "tokens": ["In", "N\u00f6\u00b7then", "ge\u00b7be\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Haben sie heut", "tokens": ["Ha\u00b7ben", "sie", "heut"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Und der Himmel erh\u00f6rt sie,", "tokens": ["Und", "der", "Him\u00b7mel", "er\u00b7h\u00f6rt", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "So jubeln sie laut,", "tokens": ["So", "ju\u00b7beln", "sie", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Er sendet ihnen im einsamen Gebirge", "tokens": ["Er", "sen\u00b7det", "ih\u00b7nen", "im", "ein\u00b7sa\u00b7men", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Deutsche Landsleute zu.", "tokens": ["Deut\u00b7sche", "Lands\u00b7leu\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Mit Trost reicht' ich ihnen die Gabe", "tokens": ["Mit", "Trost", "reicht'", "ich", "ih\u00b7nen", "die", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und war noch lange bewegt;", "tokens": ["Und", "war", "noch", "lan\u00b7ge", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da dacht' ich der Worte", "tokens": ["Da", "dacht'", "ich", "der", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Unsers gro\u00dfen Freundes:", "tokens": ["Un\u00b7sers", "gro\u00b7\u00dfen", "Freun\u00b7des", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seh' ich den Pilgrim kann ich mich nie der Thr\u00e4nen enthalten.", "tokens": ["Seh'", "ich", "den", "Pil\u00b7grim", "kann", "ich", "mich", "nie", "der", "Thr\u00e4\u00b7nen", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VMFIN", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "O, wie beseelet uns Menschen ein falscher Begriff!", "tokens": ["O", ",", "wie", "be\u00b7see\u00b7let", "uns", "Men\u00b7schen", "ein", "fal\u00b7scher", "Be\u00b7griff", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch die Armen haben f\u00fcr ihr Leben", "tokens": ["Doch", "die", "Ar\u00b7men", "ha\u00b7ben", "f\u00fcr", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "N\u00e4chst des Herzens und Glaubens Befriedigung", "tokens": ["N\u00e4chst", "des", "Her\u00b7zens", "und", "Glau\u00b7bens", "Be\u00b7frie\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "NN", "NN"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Tausendfaches sch\u00f6nes Erinnern,", "tokens": ["Tau\u00b7send\u00b7fa\u00b7ches", "sch\u00f6\u00b7nes", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Von Roms Herrlichkeit,", "tokens": ["Von", "Roms", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den hohen Gebirgen", "tokens": ["Den", "ho\u00b7hen", "Ge\u00b7bir\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und Florenz Pracht.", "tokens": ["Und", "Flo\u00b7renz", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wir alle wallen", "tokens": ["Wir", "al\u00b7le", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In gl\u00e4ubigem Gef\u00fchl \u2013", "tokens": ["In", "gl\u00e4u\u00b7bi\u00b7gem", "Ge\u00b7f\u00fchl", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und kannst du denn immer", "tokens": ["Und", "kannst", "du", "denn", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So scharf es sondern,", "tokens": ["So", "scharf", "es", "son\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ob nicht im Glauben,", "tokens": ["Ob", "nicht", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Im bewegten Herzen,", "tokens": ["Im", "be\u00b7weg\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "In der Entz\u00fcckung,", "tokens": ["In", "der", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Dich, wenn auch nur wenig,", "tokens": ["Dich", ",", "wenn", "auch", "nur", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ADV", "ADV", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Aberglauben beschleicht?", "tokens": ["A\u00b7berg\u00b7lau\u00b7ben", "be\u00b7schleicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}