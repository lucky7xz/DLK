{"textgrid.poem.63837": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Eine Weihnachtsepistel", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du neidest mich mit deinem g\u00f6nnenden", "tokens": ["Du", "nei\u00b7dest", "mich", "mit", "dei\u00b7nem", "g\u00f6n\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Selbstlosen Neide, Freund, um all den Zauber", "tokens": ["Selbst\u00b7lo\u00b7sen", "Nei\u00b7de", ",", "Freund", ",", "um", "all", "den", "Zau\u00b7ber"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "KOUI", "PIAT", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "An Farb' und Licht und immergr\u00fcnem Flor", "tokens": ["An", "Fa\u00b7rb'", "und", "Licht", "und", "im\u00b7mer\u00b7gr\u00fc\u00b7nem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Winters hier im S\u00fcden. Einzig nur,", "tokens": ["Des", "Win\u00b7ters", "hier", "im", "S\u00fc\u00b7den", ".", "Ein\u00b7zig", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$.", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df es um Weihnacht uns an Schnee und Eis", "tokens": ["Da\u00df", "es", "um", "Weih\u00b7nacht", "uns", "an", "Schnee", "und", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Schlittenbahn gebricht, \u00bbwas doch durchaus", "tokens": ["Und", "Schlit\u00b7ten\u00b7bahn", "ge\u00b7bricht", ",", "\u00bb", "was", "doch", "durc\u00b7haus"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "$(", "PWS", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Geh\u00f6rt zu einem ", "tokens": ["Ge\u00b7h\u00f6rt", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "M\u00fcss' ich wohl auch beklagen.", "tokens": ["M\u00fcss'", "ich", "wohl", "auch", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Freilich war's", "tokens": ["Frei\u00b7lich", "wa\u00b7r's"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Mir selbst verwunderlich, als fr\u00fche schon", "tokens": ["Mir", "selbst", "ver\u00b7wun\u00b7der\u00b7lich", ",", "als", "fr\u00fc\u00b7he", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$,", "KOUS", "ADJA", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die heil'ge Nacht vom klaren Firmament", "tokens": ["Die", "heil'\u00b7ge", "Nacht", "vom", "kla\u00b7ren", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Herabsank und ich hoch am Bergeshang", "tokens": ["Her\u00b7ab\u00b7sank", "und", "ich", "hoch", "am", "Ber\u00b7ges\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hinschlendernd auf den See herniedersah, \u2013", "tokens": ["Hin\u00b7schlen\u00b7dernd", "auf", "den", "See", "her\u00b7nie\u00b7der\u00b7sah", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Weitum der Ufer reingeschwungner Ring,", "tokens": ["Wei\u00b7tum", "der", "U\u00b7fer", "rein\u00b7ge\u00b7schwung\u00b7ner", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Der einer edlen Silberschale gleich", "tokens": ["Der", "ei\u00b7ner", "ed\u00b7len", "Sil\u00b7ber\u00b7scha\u00b7le", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die dunkle Flut umfa\u00dfte, \u2013 da\u00df mich noch", "tokens": ["Die", "dunk\u00b7le", "Flut", "um\u00b7fa\u00df\u00b7te", ",", "\u2013", "da\u00df", "mich", "noch"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So lind die Luft umspielte, wie bei euch", "tokens": ["So", "lind", "die", "Luft", "um\u00b7spiel\u00b7te", ",", "wie", "bei", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "PWAV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Im Mai, und dachte: Heut ist Heiligabend;", "tokens": ["Im", "Mai", ",", "und", "dach\u00b7te", ":", "Heut", "ist", "Hei\u00b7li\u00b7ga\u00b7bend", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Heut flockt vielleicht der Schnee in dichtem Schwarm", "tokens": ["Heut", "flockt", "viel\u00b7leicht", "der", "Schnee", "in", "dich\u00b7tem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Auf M\u00fcnchens Gassen, oder schneit es nicht,", "tokens": ["Auf", "M\u00fcn\u00b7chens", "Gas\u00b7sen", ",", "o\u00b7der", "schneit", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "So heult ein rauher Winterwind mit Macht", "tokens": ["So", "heult", "ein", "rau\u00b7her", "Win\u00b7ter\u00b7wind", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Weit vom Gebirg daher, da\u00df, die versp\u00e4tet", "tokens": ["Weit", "vom", "Ge\u00b7birg", "da\u00b7her", ",", "da\u00df", ",", "die", "ver\u00b7sp\u00e4\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PAV", "$,", "KOUS", "$,", "PRELS", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "Noch unterwegs sind, ihre roten Nasen", "tokens": ["Noch", "un\u00b7ter\u00b7wegs", "sind", ",", "ih\u00b7re", "ro\u00b7ten", "Na\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Tief in den Kragen stecken und trotzdem", "tokens": ["Tief", "in", "den", "Kra\u00b7gen", "ste\u00b7cken", "und", "trotz\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "KON", "PAV"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Den trefflichsten Katarrh nach Hause bringen. \u2013", "tokens": ["Den", "treff\u00b7lichs\u00b7ten", "Ka\u00b7tarrh", "nach", "Hau\u00b7se", "brin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Nun, l\u00e4ndlich sittlich. Auch ein Schnupfen wohl", "tokens": ["Nun", ",", "l\u00e4nd\u00b7lich", "sitt\u00b7lich", ".", "Auch", "ein", "Schnup\u00b7fen", "wohl"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "ADJD", "$.", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Geh\u00f6rt zu einem \u00bbrichtigen\u00ab Weihnachtsfest,", "tokens": ["Ge\u00b7h\u00f6rt", "zu", "ei\u00b7nem", "\u00bb", "rich\u00b7ti\u00b7gen", "\u00ab", "Weih\u00b7nachts\u00b7fest", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "$(", "VVINF", "$(", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Und mit Sylvesterpunsch kuriert man ihn.", "tokens": ["Und", "mit", "Syl\u00b7ves\u00b7ter\u00b7punsch", "ku\u00b7riert", "man", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich aber d\u00fcnkt, die ", "tokens": ["Mich", "a\u00b7ber", "d\u00fcnkt", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Historische, hat von Katarrhen nichts", "tokens": ["His\u00b7to\u00b7ri\u00b7sche", ",", "hat", "von", "Ka\u00b7tar\u00b7rhen", "nichts"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "APPR", "NE", "PIS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und Sturm und Schnee gewu\u00dft. Lag doch, geh\u00fcllt", "tokens": ["Und", "Sturm", "und", "Schnee", "ge\u00b7wu\u00dft", ".", "Lag", "doch", ",", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$.", "NN", "ADV", "$,", "VVPP"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "In leichte Windeln nur, im offnen Stall", "tokens": ["In", "leich\u00b7te", "Win\u00b7deln", "nur", ",", "im", "off\u00b7nen", "Stall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das liebe Christkind. Und die Hirten, die", "tokens": ["Das", "lie\u00b7be", "Christ\u00b7kind", ".", "Und", "die", "Hir\u00b7ten", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Engels Botschaft h\u00f6rten, ihre Herden", "tokens": ["Des", "En\u00b7gels", "Bot\u00b7schaft", "h\u00f6r\u00b7ten", ",", "ih\u00b7re", "Her\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf freiem Felde h\u00fctend bei der Nacht,", "tokens": ["Auf", "frei\u00b7em", "Fel\u00b7de", "h\u00fc\u00b7tend", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie krochen frierend nicht in dumpfe H\u00fctten,", "tokens": ["Sie", "kro\u00b7chen", "frie\u00b7rend", "nicht", "in", "dump\u00b7fe", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn lau und lieblich war die Luft. Auch ragt'", "tokens": ["Denn", "lau", "und", "lieb\u00b7lich", "war", "die", "Luft", ".", "Auch", "ragt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein Lorbeer wohl hoch an des Stalles Mauer", "tokens": ["Ein", "Lor\u00b7beer", "wohl", "hoch", "an", "des", "Stal\u00b7les", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und str\u00f6mte seinen Duft aufs Kripplein nieder,", "tokens": ["Und", "str\u00f6m\u00b7te", "sei\u00b7nen", "Duft", 