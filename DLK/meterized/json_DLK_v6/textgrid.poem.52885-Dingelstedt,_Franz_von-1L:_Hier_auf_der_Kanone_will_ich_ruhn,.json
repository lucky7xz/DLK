{"textgrid.poem.52885": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier auf der Kanone will ich ruhn,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier auf der Kanone will ich ruhn,", "tokens": ["Hier", "auf", "der", "Ka\u00b7no\u00b7ne", "will", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf den eisenbeschlagenen R\u00e4dern;", "tokens": ["Auf", "den", "ei\u00b7sen\u00b7be\u00b7schla\u00b7ge\u00b7nen", "R\u00e4\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ist freilich kein Lager von Eiderdun',", "tokens": ["Ist", "frei\u00b7lich", "kein", "La\u00b7ger", "von", "Ei\u00b7der\u00b7dun'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Matratzen und st\u00e4hlernen Federn.", "tokens": ["Mit", "Ma\u00b7trat\u00b7zen", "und", "st\u00e4h\u00b7ler\u00b7nen", "Fe\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch schlief vielleicht schon mancher Held", "tokens": ["Doch", "schlief", "viel\u00b7leicht", "schon", "man\u00b7cher", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor der Schlacht in der n\u00e4mlichen Weisen", "tokens": ["Vor", "der", "Schlacht", "in", "der", "n\u00e4m\u00b7li\u00b7chen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sp\u00e4ter noch tiefer \u2013 im blutigen Feld,", "tokens": ["Und", "sp\u00e4\u00b7ter", "noch", "tie\u00b7fer", "\u2013", "im", "blu\u00b7ti\u00b7gen", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJD", "$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auf dem Leib, statt drunter dein Eisen.", "tokens": ["Auf", "dem", "Leib", ",", "statt", "drun\u00b7ter", "dein", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "PAV", "PPOSAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Erz\u00e4hle mir nun, du eherner Mund,", "tokens": ["Er\u00b7z\u00e4h\u00b7le", "mir", "nun", ",", "du", "e\u00b7her\u00b7ner", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von deinen glorreichen Tagen,", "tokens": ["Von", "dei\u00b7nen", "glor\u00b7rei\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie du einst zu schwerer Schlachtenstund'", "tokens": ["Wie", "du", "einst", "zu", "schwe\u00b7rer", "Schlach\u00b7ten\u00b7stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die Reveille munter geschlagen.", "tokens": ["Die", "Re\u00b7veil\u00b7le", "mun\u00b7ter", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Bei Jena oder bei Austerlitz,", "tokens": ["Bei", "Je\u00b7na", "o\u00b7der", "bei", "Aus\u00b7ter\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gen Moskau oder gen Kassel,", "tokens": ["Gen", "Mos\u00b7kau", "o\u00b7der", "gen", "Kas\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo flammte zuletzt dein t\u00f6tlicher Blitz,", "tokens": ["Wo", "flamm\u00b7te", "zu\u00b7letzt", "dein", "t\u00f6t\u00b7li\u00b7cher", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo rollte dein letztes Gerassel?", "tokens": ["Wo", "roll\u00b7te", "dein", "letz\u00b7tes", "Ge\u00b7ras\u00b7sel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Oder bist du gar dem alten Fritz", "tokens": ["O\u00b7der", "bist", "du", "gar", "dem", "al\u00b7ten", "Fritz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schon gefolgt zu r\u00fchmlicher Frone?", "tokens": ["Schon", "ge\u00b7folgt", "zu", "r\u00fchm\u00b7li\u00b7cher", "Fro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nein, hier am Z\u00fcndloch, wo ich sitz',", "tokens": ["Nein", ",", "hier", "am", "Z\u00fcnd\u00b7loch", ",", "wo", "ich", "sitz'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht ein N. mit Lorbeer und Krone.", "tokens": ["Steht", "ein", "N.", "mit", "Lor\u00b7beer", "und", "Kro\u00b7ne", "."], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Den Namen, den Lorbeer kenn ich wohl,", "tokens": ["Den", "Na\u00b7men", ",", "den", "Lor\u00b7beer", "kenn", "ich", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Zeugen deiner Bl\u00fcte;", "tokens": ["Die", "Zeu\u00b7gen", "dei\u00b7ner", "Bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht wahr, da brummtest und summtest du hohl,", "tokens": ["Nicht", "wahr", ",", "da", "brumm\u00b7test", "und", "summ\u00b7test", "du", "hohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da gl\u00fchte dein Leib und spr\u00fchte?", "tokens": ["Da", "gl\u00fch\u00b7te", "dein", "Leib", "und", "spr\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Es flog das Rad auf bezwungener Erd'", "tokens": ["Es", "flog", "das", "Rad", "auf", "be\u00b7zwun\u00b7ge\u00b7ner", "Erd'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber Lebende und \u00fcber Leichen,", "tokens": ["\u00dc\u00b7ber", "Le\u00b7ben\u00b7de", "und", "\u00fc\u00b7ber", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zusammen st\u00fcrzte die bange Herd'", "tokens": ["Zu\u00b7sam\u00b7men", "st\u00fcrz\u00b7te", "die", "ban\u00b7ge", "Herd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unter deinen gewaltigen Streichen.", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "ge\u00b7wal\u00b7ti\u00b7gen", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Du gabst den Takt zu dem Waffentanz,", "tokens": ["Du", "gabst", "den", "Takt", "zu", "dem", "Waf\u00b7fen\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hoch h\u00fcpfte dein Herz, das beherzte,", "tokens": ["Hoch", "h\u00fcpf\u00b7te", "dein", "Herz", ",", "das", "be\u00b7herz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und sch\u00f6n zu der Panzer, der Schwerter Glanz", "tokens": ["Und", "sch\u00f6n", "zu", "der", "Pan\u00b7zer", ",", "der", "Schwer\u00b7ter", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Stund dein Antlitz, das pulvergeschw\u00e4rzte.", "tokens": ["Stund", "dein", "Ant\u00b7litz", ",", "das", "pul\u00b7ver\u00b7ge\u00b7schw\u00e4rz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Jetzt bist du blank, jetzt bist du zahm,", "tokens": ["Jetzt", "bist", "du", "blank", ",", "jetzt", "bist", "du", "zahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lahm ist deine Lafette,", "tokens": ["Und", "lahm", "ist", "dei\u00b7ne", "La\u00b7fet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dein Kupfergesicht hochrot vor Scham", "tokens": ["Dein", "Kup\u00b7fer\u00b7ge\u00b7sicht", "hoch\u00b7rot", "vor", "Scham"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und feist, als ob's gealtert h\u00e4tte.", "tokens": ["Und", "feist", ",", "als", "ob's", "ge\u00b7al\u00b7tert", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun, sch\u00e4me dich nicht, du elektrischer Aal,", "tokens": ["Nun", ",", "sch\u00e4\u00b7me", "dich", "nicht", ",", "du", "e\u00b7lekt\u00b7ri\u00b7scher", "Aal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hast ja noch einen wackeren Posten,", "tokens": ["Hast", "ja", "noch", "ei\u00b7nen", "wa\u00b7cke\u00b7ren", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn auch da dr\u00fcben im Arsenal,", "tokens": ["Wenn", "auch", "da", "dr\u00fc\u00b7ben", "im", "Ar\u00b7se\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Futter, die Kugeln rosten.", "tokens": ["Dein", "Fut\u00b7ter", ",", "die", "Ku\u00b7geln", "ros\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ert\u00f6nst du nicht vom Walle herab", "tokens": ["Er\u00b7t\u00f6nst", "du", "nicht", "vom", "Wal\u00b7le", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In die bebenden Niederungen,", "tokens": ["In", "die", "be\u00b7ben\u00b7den", "Nie\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wenn ein armer Sklave aus seinem Grab,", "tokens": ["Wenn", "ein", "ar\u00b7mer", "Skla\u00b7ve", "aus", "sei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus seinen Ketten entsprungen?", "tokens": ["Aus", "sei\u00b7nen", "Ket\u00b7ten", "ent\u00b7sprun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Wenn ein Kr\u00e4merhaus in Flammen ger\u00e4t,", "tokens": ["Wenn", "ein", "Kr\u00e4\u00b7mer\u00b7haus", "in", "Flam\u00b7men", "ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Zur Friedensrevue vor den Toren,", "tokens": ["Zur", "Frie\u00b7dens\u00b7re\u00b7vue", "vor", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zum Namenstag Seiner Majest\u00e4t,", "tokens": ["Zum", "Na\u00b7mens\u00b7tag", "Sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und so oft ein Prinze\u00dfchen geboren?", "tokens": ["Und", "so", "oft", "ein", "Prin\u00b7ze\u00df\u00b7chen", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+-+---+-", "measure": "anapaest.init"}}, "stanza.13": {"line.1": {"text": "Geduld! Vielleicht kannst du wiederum, \u2013", "tokens": ["Ge\u00b7duld", "!", "Viel\u00b7leicht", "kannst", "du", "wie\u00b7de\u00b7rum", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bald! \u2013 in die Feinde hageln;", "tokens": ["Und", "bald", "!", "\u2013", "in", "die", "Fein\u00b7de", "ha\u00b7geln", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis dahin, mein Veteran, sei stumm,", "tokens": ["Bis", "da\u00b7hin", ",", "mein", "Ve\u00b7te\u00b7ran", ",", "sei", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sie dir das Maul nicht vernageln!", "tokens": ["Da\u00df", "sie", "dir", "das", "Maul", "nicht", "ver\u00b7na\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}