{"dta.poem.90": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er ihre Br\u00fcste k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Blondine deiner Br\u00fcste Ku\u00df/", "tokens": ["Blon\u00b7di\u00b7ne", "dei\u00b7ner", "Br\u00fcs\u00b7te", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hegt mehr von s\u00fcssen Uberflu\u00df", "tokens": ["Hegt", "mehr", "von", "s\u00fcs\u00b7sen", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als tausend Zucker-Fladen", "tokens": ["Als", "tau\u00b7send", "Zu\u00b7cke\u00b7rFla\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und theure ", "tokens": ["Und", "theu\u00b7re"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Mehr S\u00fcssigkeit quilt aus dem Schnee", "tokens": ["Mehr", "S\u00fcs\u00b7sig\u00b7keit", "quilt", "aus", "dem", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Der Br\u00fcste/ als aus ", "tokens": ["Der", "Br\u00fcs\u00b7te", "/", "als", "aus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "KOKOM", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Feige wird zur Schleen", "tokens": ["Die", "Fei\u00b7ge", "wird", "zur", "Schleen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kein Honig kan bestehen/", "tokens": ["Kein", "Ho\u00b7nig", "kan", "be\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df nicht zu Gall und Wermuth wird", "tokens": ["Da\u00df", "nicht", "zu", "Gall", "und", "Wer\u00b7muth", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "NE", "KON", "NN", "VAFIN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Wenn es der Brust wird beygef\u00fchrt.", "tokens": ["Wenn", "es", "der", "Brust", "wird", "bey\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Wein wird schlechte Pf\u00fctze", "tokens": ["Der", "Wein", "wird", "schlech\u00b7te", "Pf\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Benimmt dein Busen alle Krafft/", "tokens": ["Be\u00b7nimmt", "dein", "Bu\u00b7sen", "al\u00b7le", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dein unbefleckte Br\u00fcste", "tokens": ["Dein", "un\u00b7be\u00b7fleck\u00b7te", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die Zinsen Himmels-L\u00fcste.", "tokens": ["Die", "Zin\u00b7sen", "Him\u00b7mels\u00b7L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}