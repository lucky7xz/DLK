{"dta.poem.14190": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Venus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Mutter gr\u00fc\u00dfen tausend Lieder,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mut\u00b7ter", "gr\u00fc\u00b7\u00dfen", "tau\u00b7send", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, wieder jung, im Brautkranz s\u00fc\u00df zu sehen;", "tokens": ["Die", ",", "wie\u00b7der", "jung", ",", "im", "Braut\u00b7kranz", "s\u00fc\u00df", "zu", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "$,", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wald will sprechen, rauschend Str\u00f6me gehen,", "tokens": ["Der", "Wald", "will", "spre\u00b7chen", ",", "rau\u00b7schend", "Str\u00f6\u00b7me", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Najaden tauchen singend auf und nieder.", "tokens": ["Na\u00b7ja\u00b7den", "tau\u00b7chen", "sin\u00b7gend", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}