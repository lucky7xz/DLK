{"textgrid.poem.25083": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch ein Schimmern rings und Leuchten!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Schimmern rings und Leuchten!", "tokens": ["Welch", "ein", "Schim\u00b7mern", "rings", "und", "Leuch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Funkelnd in des Morgens Strahl", "tokens": ["Fun\u00b7kelnd", "in", "des", "Mor\u00b7gens", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00fchn die Tropfen von den feuchten", "tokens": ["Spr\u00fchn", "die", "Trop\u00b7fen", "von", "den", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweigen nieder in das Thal.", "tokens": ["Zwei\u00b7gen", "nie\u00b7der", "in", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Licht auf den beeisten Spitzen,", "tokens": ["Licht", "auf", "den", "be\u00b7eis\u00b7ten", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Licht selbst tief im Abgrundschacht!", "tokens": ["Licht", "selbst", "tief", "im", "Ab\u00b7grund\u00b7schacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! durch all das Strahlen, Blitzen", "tokens": ["Ach", "!", "durch", "all", "das", "Strah\u00b7len", ",", "Blit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "APPR", "PIAT", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trag' ich einsam meine Nacht.", "tokens": ["Trag'", "ich", "ein\u00b7sam", "mei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}