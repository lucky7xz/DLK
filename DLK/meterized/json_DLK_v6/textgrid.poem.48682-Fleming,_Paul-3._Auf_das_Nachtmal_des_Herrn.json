{"textgrid.poem.48682": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf das Nachtmal des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hohe Wundermal, da selbst der Wirt wird gessen,", "tokens": ["Das", "ho\u00b7he", "Wun\u00b7der\u00b7mal", ",", "da", "selbst", "der", "Wirt", "wird", "ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "di\u00df Brot, der Wein; nicht so: der Leib, di\u00df Blut,", "tokens": ["di\u00df", "Brot", ",", "der", "Wein", ";", "nicht", "so", ":", "der", "Leib", ",", "di\u00df", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ART", "NN", "$.", "PTKNEG", "ADV", "$.", "ART", "NN", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das so viel an gesunden Kranken tut;", "tokens": ["das", "so", "viel", "an", "ge\u00b7sun\u00b7den", "Kran\u00b7ken", "tut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das tote Lebende f\u00fcr Tod zum Leben essen,", "tokens": ["das", "to\u00b7te", "Le\u00b7ben\u00b7de", "f\u00fcr", "Tod", "zum", "Le\u00b7ben", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "das neue Testament, der letzte Wille dessen,", "tokens": ["das", "neu\u00b7e", "Tes\u00b7ta\u00b7ment", ",", "der", "letz\u00b7te", "Wil\u00b7le", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der menschlich starb, nun g\u00f6ttlich lebt und Hut", "tokens": ["der", "menschlich", "starb", ",", "nun", "g\u00f6tt\u00b7lich", "lebt", "und", "Hut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "f\u00fcr diese h\u00e4lt, so hei\u00dfen Gottes Gut,", "tokens": ["f\u00fcr", "die\u00b7se", "h\u00e4lt", ",", "so", "hei\u00b7\u00dfen", "Got\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und was? Wie kan ein Mensch die G\u00f6ttlichkeit ermessen?", "tokens": ["und", "was", "?", "Wie", "kan", "ein", "Mensch", "die", "G\u00f6tt\u00b7lich\u00b7keit", "er\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$.", "PWAV", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hinweg, Vernunft, du kluge T\u00f6rin du.", "tokens": ["Hin\u00b7weg", ",", "Ver\u00b7nunft", ",", "du", "klu\u00b7ge", "T\u00f6\u00b7rin", "du", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weg, weiser Wahn, halt Ohr und Augen zu.", "tokens": ["Weg", ",", "wei\u00b7ser", "Wahn", ",", "halt", "Ohr", "und", "Au\u00b7gen", "zu", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ungelehrten sind hier die gelehrten K\u00f6pfe.", "tokens": ["Die", "un\u00b7ge\u00b7lehr\u00b7ten", "sind", "hier", "die", "ge\u00b7lehr\u00b7ten", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Pfand meines Heils, ich komme mit Begier", "tokens": ["Pfand", "mei\u00b7nes", "Heils", ",", "ich", "kom\u00b7me", "mit", "Be\u00b7gier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zu deiner Kost und nehme sie zu mir,", "tokens": ["zu", "dei\u00b7ner", "Kost", "und", "neh\u00b7me", "sie", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df mein Tod in dir sterb' und ich dein Leben sch\u00f6pfe.", "tokens": ["da\u00df", "mein", "Tod", "in", "dir", "sterb'", "und", "ich", "dein", "Le\u00b7ben", "sch\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "KON", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}