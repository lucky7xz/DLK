{"textgrid.poem.24015": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Trennung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt in mir wie eine Wolke", "tokens": ["Es", "liegt", "in", "mir", "wie", "ei\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der d\u00fcstre Abend, der uns schied.", "tokens": ["Der", "d\u00fcst\u00b7re", "A\u00b7bend", ",", "der", "uns", "schied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es stand kein Stern am grauen Himmel", "tokens": ["Es", "stand", "kein", "Stern", "am", "grau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von den Zweigen klang kein Lied.", "tokens": ["Und", "von", "den", "Zwei\u00b7gen", "klang", "kein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verdrossene Menschen gingen eilig", "tokens": ["Ver\u00b7dros\u00b7se\u00b7ne", "Men\u00b7schen", "gin\u00b7gen", "ei\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im feuchten Dunkel uns vorbei.", "tokens": ["Im", "feuch\u00b7ten", "Dun\u00b7kel", "uns", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf nasser Bank verschlungen sa\u00dfen", "tokens": ["Auf", "nas\u00b7ser", "Bank", "ver\u00b7schlun\u00b7gen", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wortlos und herzensbang wir zwei.", "tokens": ["Wort\u00b7los", "und", "her\u00b7zens\u00b7bang", "wir", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PPER", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es sah der Mond durch d\u00fcrre Aeste.", "tokens": ["Es", "sah", "der", "Mond", "durch", "d\u00fcr\u00b7re", "A\u00b7es\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf deinem Antlitz lag sein Schein", "tokens": ["Auf", "dei\u00b7nem", "Ant\u00b7litz", "lag", "sein", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So d\u00fcster-tot, \u2013 mein heimgegangnes", "tokens": ["So", "d\u00fcs\u00b7ter\u00b7tot", ",", "\u2013", "mein", "heim\u00b7ge\u00b7gang\u00b7nes"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "$(", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcck h\u00fcllte er in Strahlen ein.", "tokens": ["Gl\u00fcck", "h\u00fcll\u00b7te", "er", "in", "Strah\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn dein Blick, dein seelenvoller,", "tokens": ["Und", "wenn", "dein", "Blick", ",", "dein", "see\u00b7len\u00b7vol\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich zu mir hob, in Schmerzen mild,", "tokens": ["Sich", "zu", "mir", "hob", ",", "in", "Schmer\u00b7zen", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus bleichem Mondenstrahlenglanze,", "tokens": ["Aus", "blei\u00b7chem", "Mon\u00b7dens\u00b7trah\u00b7len\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da sah ich meines Schicksals Bild:", "tokens": ["Da", "sah", "ich", "mei\u00b7nes", "Schick\u00b7sals", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Sch\u00f6ne, das ich still erdichtet", "tokens": ["Das", "Sch\u00f6\u00b7ne", ",", "das", "ich", "still", "er\u00b7dich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rein im Herzen aufgestellt,", "tokens": ["Und", "rein", "im", "Her\u00b7zen", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie es vor meinem hei\u00dfen W\u00fcnschen", "tokens": ["Wie", "es", "vor", "mei\u00b7nem", "hei\u00b7\u00dfen", "W\u00fcn\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fliehend in Schmerz zusammenf\u00e4llt.", "tokens": ["Flie\u00b7hend", "in", "Schmerz", "zu\u00b7sam\u00b7men\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}