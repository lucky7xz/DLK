{"textgrid.poem.26317": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es sind nicht leere L\u00fcfte", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt r\u00fchrt der Morgenwind die B\u00e4ume an.", "tokens": ["Jetzt", "r\u00fchrt", "der", "Mor\u00b7gen\u00b7wind", "die", "B\u00e4u\u00b7me", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie wiegen sich. Sie fl\u00fcstern, winken dann,", "tokens": ["Sie", "wie\u00b7gen", "sich", ".", "Sie", "fl\u00fcs\u00b7tern", ",", "win\u00b7ken", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leichthin jeder Baum dort l\u00e4cheln kann.", "tokens": ["Und", "leich\u00b7thin", "je\u00b7der", "Baum", "dort", "l\u00e4\u00b7cheln", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie deuten auf den Himmel, wo der Geist", "tokens": ["Sie", "deu\u00b7ten", "auf", "den", "Him\u00b7mel", ",", "wo", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der G\u00fcte mit der gro\u00dfen Sonne kreist", "tokens": ["Der", "G\u00fc\u00b7te", "mit", "der", "gro\u00b7\u00dfen", "Son\u00b7ne", "kreist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes Blatt das helle Leben preist.", "tokens": ["Und", "je\u00b7des", "Blatt", "das", "hel\u00b7le", "Le\u00b7ben", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Zweige wiegen sich so flink und leicht.", "tokens": ["Die", "Zwei\u00b7ge", "wie\u00b7gen", "sich", "so", "flink", "und", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Baum dem Himmel H\u00e4nde reicht.", "tokens": ["Ein", "je\u00b7der", "Baum", "dem", "Him\u00b7mel", "H\u00e4n\u00b7de", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Baumes Seele der des Menschen gleicht.", "tokens": ["Des", "Bau\u00b7mes", "See\u00b7le", "der", "des", "Men\u00b7schen", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Seele ist die Summe unsrer Kraft,", "tokens": ["Die", "See\u00b7le", "ist", "die", "Sum\u00b7me", "uns\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sich im Augenblick zusammenrafft", "tokens": ["Die", "sich", "im", "Au\u00b7gen\u00b7blick", "zu\u00b7sam\u00b7men\u00b7rafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und neue Ewigkeiten in uns schafft.", "tokens": ["Und", "neu\u00b7e", "E\u00b7wig\u00b7kei\u00b7ten", "in", "uns", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Geist der Ewigkeiten baut im Raum,", "tokens": ["Der", "Geist", "der", "E\u00b7wig\u00b7kei\u00b7ten", "baut", "im", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Geist wirkt auch im Menschen und im Baum.", "tokens": ["Der", "Geist", "wirkt", "auch", "im", "Men\u00b7schen", "und", "im", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein K\u00f6rper wird so leicht, du sp\u00fcrst ihn kaum.", "tokens": ["Dein", "K\u00f6r\u00b7per", "wird", "so", "leicht", ",", "du", "sp\u00fcrst", "ihn", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es sind nicht leere L\u00fcfte die dort wehn.", "tokens": ["Es", "sind", "nicht", "lee\u00b7re", "L\u00fcf\u00b7te", "die", "dort", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es sind nicht tote Zweige, die sich drehn.", "tokens": ["Es", "sind", "nicht", "to\u00b7te", "Zwei\u00b7ge", ",", "die", "sich", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du kannst die Weltallseele wachsen sehn.", "tokens": ["Du", "kannst", "die", "Welt\u00b7all\u00b7see\u00b7le", "wach\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}