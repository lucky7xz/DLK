{"textgrid.poem.24041": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gott zeigt Adam das Paradies", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchrt der g\u00fctestille Herr der Welten,", "tokens": ["F\u00fchrt", "der", "g\u00fc\u00b7tes\u00b7til\u00b7le", "Herr", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ewig jung in seinem blonden Barte,", "tokens": ["E\u00b7wig", "jung", "in", "sei\u00b7nem", "blon\u00b7den", "Bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor das Bl\u00fcheland der jungen Erde", "tokens": ["Vor", "das", "Bl\u00fc\u00b7he\u00b7land", "der", "jun\u00b7gen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Adam hin, den nackten braunen Knaben.", "tokens": ["A\u00b7dam", "hin", ",", "den", "nack\u00b7ten", "brau\u00b7nen", "Kna\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Zeigt ihm all die moosbl\u00fchbunten Steine,", "tokens": ["Zeigt", "ihm", "all", "die", "moos\u00b7bl\u00fch\u00b7bun\u00b7ten", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All die sch\u00f6nen V\u00f6gel, stillen Tiere,", "tokens": ["All", "die", "sch\u00f6\u00b7nen", "V\u00f6\u00b7gel", ",", "stil\u00b7len", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "All die weiten saftiggr\u00fcnen Wiesen,", "tokens": ["All", "die", "wei\u00b7ten", "saf\u00b7tig\u00b7gr\u00fc\u00b7nen", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Berg und Thal und Busch und Baum und Wasser.", "tokens": ["Berg", "und", "Thal", "und", "Busch", "und", "Baum", "und", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alles liegt in frischer, keuscher Reine", "tokens": ["Al\u00b7les", "liegt", "in", "fri\u00b7scher", ",", "keu\u00b7scher", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Unterm silbergrauen hohen Himmel.", "tokens": ["Un\u00b7term", "sil\u00b7ber\u00b7grau\u00b7en", "ho\u00b7hen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und er spricht mit leisen Deuteworten,", "tokens": ["Und", "er", "spricht", "mit", "lei\u00b7sen", "Deu\u00b7te\u00b7wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der Vater spricht zum kleinen Kinde,", "tokens": ["Wie", "der", "Va\u00b7ter", "spricht", "zum", "klei\u00b7nen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und er legt den Vaterarm um Adam.", "tokens": ["Und", "er", "legt", "den", "Va\u00b7te\u00b7rarm", "um", "A\u00b7dam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aengstlich vor dem Reichtum steht der Knabe,", "tokens": ["A\u00b7engst\u00b7lich", "vor", "dem", "Reich\u00b7tum", "steht", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Halbgebeugt vor dieser sch\u00f6nen Erde.", "tokens": ["Halb\u00b7ge\u00b7beugt", "vor", "die\u00b7ser", "sch\u00f6\u00b7nen", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hielt ihn nicht der Gottesarm, der linde,", "tokens": ["Hielt", "ihn", "nicht", "der", "Got\u00b7tes\u00b7arm", ",", "der", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sank er nieder auf den Schoo\u00df der Keime.", "tokens": ["Sank", "er", "nie\u00b7der", "auf", "den", "Schoo\u00df", "der", "Kei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ahnung senkte ihm ins Herz der Vater.", "tokens": ["Ah\u00b7nung", "senk\u00b7te", "ihm", "ins", "Herz", "der", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}