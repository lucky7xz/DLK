{"textgrid.poem.63209": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du deutsche Stadt, du deutscheste der St\u00e4dte,", "tokens": ["Du", "deut\u00b7sche", "Stadt", ",", "du", "deut\u00b7sches\u00b7te", "der", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich Wankenden besch\u00fctzen deine Mauern.", "tokens": ["Mich", "Wan\u00b7ken\u00b7den", "be\u00b7sch\u00fct\u00b7zen", "dei\u00b7ne", "Mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zart bist du zu dem Zarten, rauh zum Rauhern.", "tokens": ["Zart", "bist", "du", "zu", "dem", "Zar\u00b7ten", ",", "rauh", "zum", "Rau\u00b7hern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bete deine steinernen Gebete.", "tokens": ["Ich", "be\u00b7te", "dei\u00b7ne", "stei\u00b7ner\u00b7nen", "Ge\u00b7be\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Zeit, da gut und fromm selbst das Ger\u00e4te!", "tokens": ["O", "Zeit", ",", "da", "gut", "und", "fromm", "selbst", "das", "Ge\u00b7r\u00e4\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchle mich bewegt von edlen Schauern.", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "be\u00b7wegt", "von", "ed\u00b7len", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gott, welcher Bild und Giebel ward, wird dauern,", "tokens": ["Gott", ",", "wel\u00b7cher", "Bild", "und", "Gie\u00b7bel", "ward", ",", "wird", "dau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "KON", "NN", "VAFIN", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn wir l\u00e4ngst D\u00fcnger nur f\u00fcr Friedhofbeete.", "tokens": ["Wenn", "wir", "l\u00e4ngst", "D\u00fcn\u00b7ger", "nur", "f\u00fcr", "Fried\u00b7hof\u00b7bee\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sind diese Gr\u00e4ben f\u00fcr den Krieg geschaffen?", "tokens": ["Sind", "die\u00b7se", "Gr\u00e4\u00b7ben", "f\u00fcr", "den", "Krieg", "ge\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Scharten bl\u00fcht der Ginster und der Flieder.", "tokens": ["Um", "Schar\u00b7ten", "bl\u00fcht", "der", "Gins\u00b7ter", "und", "der", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Goldschmied, nahm er Gold, um zu erraffen?", "tokens": ["Der", "Gold\u00b7schmied", ",", "nahm", "er", "Gold", ",", "um", "zu", "er\u00b7raf\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Zeit war ewig. Lerchen ihre Lieder.", "tokens": ["Die", "Zeit", "war", "e\u00b7wig", ".", "Ler\u00b7chen", "ih\u00b7re", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lass unsere Seelen sich zur Einfalt straffen", "tokens": ["Lass", "un\u00b7se\u00b7re", "See\u00b7len", "sich", "zur", "Ein\u00b7falt", "straf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gib uns D\u00fcrer, gib Hans Sachs uns wieder!", "tokens": ["Und", "gib", "uns", "D\u00fc\u00b7rer", ",", "gib", "Hans", "Sachs", "uns", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NE", "$,", "VVIMP", "NE", "NE", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}