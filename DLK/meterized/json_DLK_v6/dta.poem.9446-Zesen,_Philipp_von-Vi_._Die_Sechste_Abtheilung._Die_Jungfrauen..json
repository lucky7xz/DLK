{"dta.poem.9446": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n Die Sechste Abtheilung.  \n Die Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer blitzet mit solchen verg\u00fcldeten strahlen/", "tokens": ["Wer", "blit\u00b7zet", "mit", "sol\u00b7chen", "ver\u00b7g\u00fcl\u00b7de\u00b7ten", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie fr\u00fche des morgens das r\u00f6thliche Licht?", "tokens": ["Wie", "fr\u00fc\u00b7he", "des", "mor\u00b7gens", "das", "r\u00f6th\u00b7li\u00b7che", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie sonsten der Silber-mond pfleget zu praalen/", "tokens": ["Wie", "sons\u00b7ten", "der", "Sil\u00b7ber\u00b7mond", "pfle\u00b7get", "zu", "praa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie t\u00e4glich das gl\u00e4ntzen der Sonnen anbricht?", "tokens": ["Wie", "t\u00e4g\u00b7lich", "das", "gl\u00e4nt\u00b7zen", "der", "Son\u00b7nen", "an\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie flammet und hitzet", "tokens": ["Sie", "flam\u00b7met", "und", "hit\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und schrecklicher blitzet/", "tokens": ["und", "schreck\u00b7li\u00b7cher", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Als jrgend die Schlacht;", "tokens": ["Als", "jr\u00b7gend", "die", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wann alles erblitzet", "tokens": ["Wann", "al\u00b7les", "er\u00b7blit\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Vom pulver erhitzet/", "tokens": ["Vom", "pul\u00b7ver", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wann alles vom schreyen und schie\u00dfen erkracht.", "tokens": ["Wann", "al\u00b7les", "vom", "schre\u00b7yen", "und", "schie\u00b7\u00dfen", "er\u00b7kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "ADJA", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}