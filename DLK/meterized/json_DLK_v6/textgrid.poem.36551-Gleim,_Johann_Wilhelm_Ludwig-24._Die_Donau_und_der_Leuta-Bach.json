{"textgrid.poem.36551": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "24. Die Donau und der Leuta-Bach", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stolze Donau ging, mit ihrem stolzen Gange,", "tokens": ["Die", "stol\u00b7ze", "Do\u00b7nau", "ging", ",", "mit", "ih\u00b7rem", "stol\u00b7zen", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stolze Wien vorbei.", "tokens": ["Das", "stol\u00b7ze", "Wi\u00b7en", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der kleine Leuta-Bach", "tokens": ["Der", "klei\u00b7ne", "Leuta\u00b7Bach"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ging ihrem stolzen Gange nach.", "tokens": ["Ging", "ih\u00b7rem", "stol\u00b7zen", "Gan\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die stolze Donau sprach:", "tokens": ["Die", "stol\u00b7ze", "Do\u00b7nau", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist dein Geschick, du kleiner Sch\u00e4ker, nicht", "tokens": ["Ist", "dein", "Ge\u00b7schick", ",", "du", "klei\u00b7ner", "Sch\u00e4\u00b7ker", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein herrliches Geschick?", "tokens": ["Ein", "herr\u00b7li\u00b7ches", "Ge\u00b7schick", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In der Gesellschaft meiner, welch ein Gl\u00fcck!", "tokens": ["In", "der", "Ge\u00b7sell\u00b7schaft", "mei\u00b7ner", ",", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die kleine Leuta spricht:", "tokens": ["Die", "klei\u00b7ne", "Leu\u00b7ta", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch das Gefilde, welches mich", "tokens": ["Durch", "das", "Ge\u00b7fil\u00b7de", ",", "wel\u00b7ches", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den kleinen Silberbach einst nannte,", "tokens": ["Den", "klei\u00b7nen", "Sil\u00b7ber\u00b7bach", "einst", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flo\u00df ich so gl\u00fccklich zwischen Blumen, ich,", "tokens": ["Flo\u00df", "ich", "so", "gl\u00fcck\u00b7lich", "zwi\u00b7schen", "Blu\u00b7men", ",", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Eh' ich dich kannte!", "tokens": ["Eh'", "ich", "dich", "kann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Kaum aber kenn' ich dich, so werd' ich fortgerissen,", "tokens": ["Kaum", "a\u00b7ber", "kenn'", "ich", "dich", ",", "so", "werd'", "ich", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mu\u00df, was alle Sklaven m\u00fcssen,", "tokens": ["Und", "mu\u00df", ",", "was", "al\u00b7le", "Skla\u00b7ven", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PRELS", "PIAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In deinem Strudel fort, nicht meiner m\u00e4chtig, ach!", "tokens": ["In", "dei\u00b7nem", "Stru\u00b7del", "fort", ",", "nicht", "mei\u00b7ner", "m\u00e4ch\u00b7tig", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PTKNEG", "PPOSAT", "ADJD", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man l\u00e4uft den gro\u00dfen Herrn an ihre H\u00f6fe nach,", "tokens": ["Man", "l\u00e4uft", "den", "gro\u00b7\u00dfen", "Herrn", "an", "ih\u00b7re", "H\u00f6\u00b7fe", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seufzt dann oft, wie du, o kleiner Leuta-Bach!", "tokens": ["Und", "seufzt", "dann", "oft", ",", "wie", "du", ",", "o", "klei\u00b7ner", "Leuta\u00b7Bach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}