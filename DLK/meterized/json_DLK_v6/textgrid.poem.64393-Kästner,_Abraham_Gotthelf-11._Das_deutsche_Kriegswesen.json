{"textgrid.poem.64393": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "11. Das deutsche Kriegswesen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Riesengleicher Kraft, doch ohne Kunst regiert,", "tokens": ["Mit", "Rie\u00b7sen\u00b7glei\u00b7cher", "Kraft", ",", "doch", "oh\u00b7ne", "Kunst", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat einst des Deutschen Arm das breite Schwert gef\u00fchrt;", "tokens": ["Hat", "einst", "des", "Deut\u00b7schen", "Arm", "das", "brei\u00b7te", "Schwert", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Muskeln nicht so stark, doch mit ge\u00fcbterm Witze", "tokens": ["An", "Mus\u00b7keln", "nicht", "so", "stark", ",", "doch", "mit", "ge\u00b7\u00fcb\u00b7term", "Wit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lenkt jetzt des Franzen Hand des leichten Degens Spitze;", "tokens": ["Lenkt", "jetzt", "des", "Fran\u00b7zen", "Hand", "des", "leich\u00b7ten", "De\u00b7gens", "Spit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wir f\u00fchlen, da\u00df uns l\u00e4ngst der Ahnen Kraft gebricht,", "tokens": ["Wir", "f\u00fch\u00b7len", ",", "da\u00df", "uns", "l\u00e4ngst", "der", "Ah\u00b7nen", "Kraft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und um der Nachbarn Kunst bem\u00fchn wir uns noch nicht.", "tokens": ["Und", "um", "der", "Nach\u00b7barn", "Kunst", "be\u00b7m\u00fchn", "wir", "uns", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mars schrie, wie tausend M\u00e4nner schreyn", "tokens": ["Mars", "schrie", ",", "wie", "tau\u00b7send", "M\u00e4n\u00b7ner", "schreyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fochte schlechter noch, als einer sollte streiten:", "tokens": ["Und", "foch\u00b7te", "schlech\u00b7ter", "noch", ",", "als", "ei\u00b7ner", "soll\u00b7te", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,", "KOUS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Reicht Dieses zu ein Mars zu seyn,", "tokens": ["Reicht", "Die\u00b7ses", "zu", "ein", "Mars", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "APPR", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie manchen Mars sehn unsre Zeiten.", "tokens": ["Wie", "man\u00b7chen", "Mars", "sehn", "uns\u00b7re", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}