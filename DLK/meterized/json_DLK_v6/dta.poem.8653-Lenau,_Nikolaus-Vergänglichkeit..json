{"dta.poem.8653": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Berge schaut hinaus ins tiefe Schweigen", "tokens": ["Vom", "Ber\u00b7ge", "schaut", "hin\u00b7aus", "ins", "tie\u00b7fe", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mondbeseelten, sch\u00f6nen Sommernacht", "tokens": ["Der", "mond\u00b7be\u00b7seel\u00b7ten", ",", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Burgruine; und in Tannenzweigen", "tokens": ["Die", "Bur\u00b7gru\u00b7i\u00b7ne", ";", "und", "in", "Tan\u00b7nen\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verseufzt ein L\u00fcftchen, das allein bewacht", "tokens": ["Ver\u00b7seufzt", "ein", "L\u00fcft\u00b7chen", ",", "das", "al\u00b7lein", "be\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die tr\u00fcmmervolle Einsamkeit,", "tokens": ["Die", "tr\u00fcm\u00b7mer\u00b7vol\u00b7le", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den bangen Laut: \u201eVerg\u00e4nglichkeit!\u201c", "tokens": ["Den", "ban\u00b7gen", "Laut", ":", "\u201e", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201everg\u00e4nglichkeit!\u201c mahnt mich im stillen Thale", "tokens": ["\u201e", "ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "\u201c", "mahnt", "mich", "im", "stil\u00b7len", "Tha\u00b7le"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ernste Schaar bekreuzter H\u00fcgel dort,", "tokens": ["Die", "erns\u00b7te", "Schaar", "be\u00b7kreuz\u00b7ter", "H\u00fc\u00b7gel", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo dauernder der Schmerz in Todtenmahle,", "tokens": ["Wo", "dau\u00b7ern\u00b7der", "der", "Schmerz", "in", "Tod\u00b7ten\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als in verlassne Herzen sich gebohrt;", "tokens": ["Als", "in", "ver\u00b7lass\u00b7ne", "Her\u00b7zen", "sich", "ge\u00b7bohrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bei Sterbetages Wiederkehr", "tokens": ["Bei", "Ster\u00b7be\u00b7ta\u00b7ges", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Befeuchtet sich kein Auge mehr.", "tokens": ["Be\u00b7feuch\u00b7tet", "sich", "kein", "Au\u00b7ge", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der wechselnden Gef\u00fchle Traumgestalten", "tokens": ["Der", "wech\u00b7seln\u00b7den", "Ge\u00b7f\u00fch\u00b7le", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchrauschen \u00e4ffend unser Herz, es sucht", "tokens": ["Durc\u00b7hrau\u00b7schen", "\u00e4f\u00b7fend", "un\u00b7ser", "Herz", ",", "es", "sucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergebens seinen Himmel festzuhalten,", "tokens": ["Ver\u00b7ge\u00b7bens", "sei\u00b7nen", "Him\u00b7mel", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fortgerissen in die rasche Flucht", "tokens": ["Und", "fort\u00b7ge\u00b7ris\u00b7sen", "in", "die", "ra\u00b7sche", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird selbst der Jammer, und der Hauch", "tokens": ["Wird", "selbst", "der", "Jam\u00b7mer", ",", "und", "der", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sanften Wehmuth schwindet auch.", "tokens": ["Der", "sanf\u00b7ten", "Weh\u00b7muth", "schwin\u00b7det", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Horch' ich hinab in meines Busens Tiefen,", "tokens": ["Horch'", "ich", "hin\u00b7ab", "in", "mei\u00b7nes", "Bu\u00b7sens", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201everg\u00e4nglichkeit!\u201c klagt's hier auch meinem Ohr,", "tokens": ["\u201e", "ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "\u201c", "klagt's", "hier", "auch", "mei\u00b7nem", "Ohr", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo l\u00e4ngst der Kindheit Freudenkl\u00e4ng' entschliefen,", "tokens": ["Wo", "l\u00e4ngst", "der", "Kind\u00b7heit", "Freu\u00b7den\u00b7kl\u00e4ng'", "ent\u00b7schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe Zauberlied sich still verlor;", "tokens": ["Der", "Lie\u00b7be", "Zau\u00b7berl\u00b7ied", "sich", "still", "ver\u00b7lor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo bald in jenen Seufzer bang", "tokens": ["Wo", "bald", "in", "je\u00b7nen", "Seuf\u00b7zer", "bang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinstirbt der lezte frohe Klang.", "tokens": ["Hins\u00b7tirbt", "der", "lez\u00b7te", "fro\u00b7he", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Verg\u00e4nglichkeit! dein Hauch, als Sturmesw\u00fcthen,", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "dein", "Hauch", ",", "als", "Stur\u00b7mes\u00b7w\u00fct\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirft hingeschmettert Eichen in den Staub;", "tokens": ["Wirft", "hin\u00b7ge\u00b7schmet\u00b7tert", "Ei\u00b7chen", "in", "den", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Hauch, als linder West, entf\u00fchrt die Bl\u00fcthen", "tokens": ["Dein", "Hauch", ",", "als", "lin\u00b7der", "West", ",", "ent\u00b7f\u00fchrt", "die", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Rosenstrauch in schmeichlerischem Raub.", "tokens": ["Dem", "Ro\u00b7sen\u00b7strauch", "in", "schmeich\u00b7le\u00b7ri\u00b7schem", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Bl\u00fcthen hier, so f\u00e4chelt dort", "tokens": ["Wie", "Bl\u00fc\u00b7then", "hier", ",", "so", "f\u00e4\u00b7chelt", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Hauch die welken Sterne fort!", "tokens": ["Dein", "Hauch", "die", "wel\u00b7ken", "Ster\u00b7ne", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}