{"textgrid.poem.62229": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ich ritt auf einem Pferde]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ritt auf einem Pferde", "tokens": ["Ich", "ritt", "auf", "ei\u00b7nem", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den gr\u00fcnen Wald entlang,", "tokens": ["Den", "gr\u00fc\u00b7nen", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Bl\u00fcten war die Erde,", "tokens": ["Voll", "Bl\u00fc\u00b7ten", "war", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich rasch hinunter sprang.", "tokens": ["Ich", "rasch", "hin\u00b7un\u00b7ter", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor mir auf gr\u00fcner Aue,", "tokens": ["Vor", "mir", "auf", "gr\u00fc\u00b7ner", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel hundert V\u00f6gelein,", "tokens": ["Viel", "hun\u00b7dert", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So h\u00fcbsche, kleine graue,", "tokens": ["So", "h\u00fcb\u00b7sche", ",", "klei\u00b7ne", "grau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit schwarzen Aeugelein.", "tokens": ["Mit", "schwar\u00b7zen", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr singet goldne Lieder,", "tokens": ["Ihr", "sin\u00b7get", "gold\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Mozart'scher Manier,", "tokens": ["Nach", "Mo\u00b7zart'\u00b7scher", "Ma\u00b7nier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seid Ihr denn alle Br\u00fcder,", "tokens": ["Seid", "Ihr", "denn", "al\u00b7le", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Ihr versammelt hier?", "tokens": ["Da\u00df", "Ihr", "ver\u00b7sam\u00b7melt", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Antwort lautet leise:", "tokens": ["Die", "Ant\u00b7wort", "lau\u00b7tet", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbeinf\u00e4ltig Menschenkind,", "tokens": ["\u00bb", "ein\u00b7f\u00e4l\u00b7tig", "Men\u00b7schen\u00b7kind", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind auf weiter Reise,", "tokens": ["Wir", "sind", "auf", "wei\u00b7ter", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle gleichgesinnt.\u00ab", "tokens": ["Und", "al\u00b7le", "gleich\u00b7ge\u00b7sinnt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sieh', es gr\u00fcnt an allen Hecken", "tokens": ["Sieh'", ",", "es", "gr\u00fcnt", "an", "al\u00b7len", "He\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf allen L\u00e4nderein,", "tokens": ["Und", "auf", "al\u00b7len", "L\u00e4n\u00b7der\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es tummeln sich die Gecken", "tokens": ["Und", "es", "tum\u00b7meln", "sich", "die", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Fr\u00fchlings Sonnenschein!", "tokens": ["In", "des", "Fr\u00fch\u00b7lings", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nachtigallen singen, fl\u00f6ten,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ",", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerchen steigen jubelnd auf,", "tokens": ["Ler\u00b7chen", "stei\u00b7gen", "ju\u00b7belnd", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Fr\u00f6sche und die Kr\u00f6ten", "tokens": ["Doch", "die", "Fr\u00f6\u00b7sche", "und", "die", "Kr\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hemmen der Begeist'rung Lauf!", "tokens": ["Hem\u00b7men", "der", "Be\u00b7geist'\u00b7rung", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}