{"textgrid.poem.36847": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut, Mi\u00dfg\u00fcnst'ge, vernehmt's, bestieg ich den wolkigen Eryx.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut, Mi\u00dfg\u00fcnst'ge, vernehmt's, bestieg ich den wolkigen Eryx.", "tokens": ["Heut", ",", "Mi\u00df\u00b7g\u00fcnst'\u00b7ge", ",", "ver\u00b7nehmt's", ",", "be\u00b7stieg", "ich", "den", "wol\u00b7ki\u00b7gen", "E\u00b7ryx", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Aber fragt ihr warum? geb' ich die Antwort euch gern.", "tokens": ["A\u00b7ber", "fragt", "ihr", "wa\u00b7rum", "?", "geb'", "ich", "die", "Ant\u00b7wort", "euch", "gern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "$.", "VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sch\u00f6n zwar ist's vom felsigen Haupt, dem taubenbewohnten,", "tokens": ["Sch\u00f6n", "zwar", "ist's", "vom", "fel\u00b7si\u00b7gen", "Haupt", ",", "dem", "tau\u00b7ben\u00b7be\u00b7wohn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nieder zu blicken auf Thal, Ufer und Insel und Meer.", "tokens": ["Nie\u00b7der", "zu", "bli\u00b7cken", "auf", "Thal", ",", "U\u00b7fer", "und", "In\u00b7sel", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+-+-+--++-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Jene Klippen, von Wellen umsch\u00e4umt, bezaubert die Sage;", "tokens": ["Je\u00b7ne", "Klip\u00b7pen", ",", "von", "Wel\u00b7len", "um\u00b7sch\u00e4umt", ",", "be\u00b7zau\u00b7bert", "die", "Sa\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Denn der wilde Cyklop warf sie hinaus in das Meer.", "tokens": ["Denn", "der", "wil\u00b7de", "Cyk\u00b7lop", "warf", "sie", "hin\u00b7aus", "in", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APZR", "APPR", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Seinem Vater feierte hier Aeneas das Kampfspiel,", "tokens": ["Sei\u00b7nem", "Va\u00b7ter", "fei\u00b7er\u00b7te", "hier", "A\u00b7e\u00b7neas", "das", "Kampf\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Wo der rauschenden See Trepanons Sichel entsteigt.", "tokens": ["Wo", "der", "rau\u00b7schen\u00b7den", "See", "Tre\u00b7pa\u00b7nons", "Si\u00b7chel", "ent\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NE", "NE", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Dort um die Inseln schlug der R\u00f6mer blutige Seeschlacht", "tokens": ["Dort", "um", "die", "In\u00b7seln", "schlug", "der", "R\u00f6\u00b7mer", "blu\u00b7ti\u00b7ge", "See\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und zu Frieden und Bund bot der Karthager die Hand.", "tokens": ["Und", "zu", "Frie\u00b7den", "und", "Bund", "bot", "der", "Kar\u00b7tha\u00b7ger", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+---+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Ueber die Ebene blick' ich hinweg, die rebenbegr\u00fcnte,", "tokens": ["Ue\u00b7ber", "die", "E\u00b7be\u00b7ne", "blick'", "ich", "hin\u00b7weg", ",", "die", "re\u00b7ben\u00b7be\u00b7gr\u00fcn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Lilybaeon ergl\u00e4nzt sonnig am \u00e4u\u00dfersten Strand,", "tokens": ["Li\u00b7ly\u00b7ba\u00b7e\u00b7on", "er\u00b7gl\u00e4nzt", "son\u00b7nig", "am", "\u00e4u\u00b7\u00dfers\u00b7ten", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Dem mit Aeolus Gunst das Schiff am Abend entwandert,", "tokens": ["Dem", "mit", "A\u00b7e\u00b7o\u00b7lus", "Gunst", "das", "Schiff", "am", "A\u00b7bend", "ent\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Um mit dem Fr\u00fchroth schon gl\u00fccklich in Tunis zu sein.", "tokens": ["Um", "mit", "dem", "Fr\u00fch\u00b7roth", "schon", "gl\u00fcck\u00b7lich", "in", "Tu\u00b7nis", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADV", "ADJD", "APPR", "NE", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Sch\u00f6n zu schauen ist das; doch wi\u00dft, den erm\u00fcdenden Bergpfad", "tokens": ["Sch\u00f6n", "zu", "schau\u00b7en", "ist", "das", ";", "doch", "wi\u00dft", ",", "den", "er\u00b7m\u00fc\u00b7den\u00b7den", "Berg\u00b7pfad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKZU", "VVINF", "VAFIN", "PDS", "$.", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Stieg der Wandrer darum nicht, der verha\u00dfte, hinan.", "tokens": ["Stieg", "der", "Wand\u00b7rer", "da\u00b7rum", "nicht", ",", "der", "ver\u00b7ha\u00df\u00b7te", ",", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "PTKNEG", "$,", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "+-+-+-+--+---", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Eine Wallfahrt gebot ihm das Herz; zum Tempel der Venus", "tokens": ["Ei\u00b7ne", "Wall\u00b7fahrt", "ge\u00b7bot", "ihm", "das", "Herz", ";", "zum", "Tem\u00b7pel", "der", "Ve\u00b7nus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Trieb ihn die Andacht, es trieb ernstlicher Dank ihn empor.", "tokens": ["Trieb", "ihn", "die", "An\u00b7dacht", ",", "es", "trieb", "ernst\u00b7li\u00b7cher", "Dank", "ihn", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Fromm ist jeder nach eigener Art, mir verg\u00f6nnet die meine,", "tokens": ["Fromm", "ist", "je\u00b7der", "nach", "ei\u00b7ge\u00b7ner", "Art", ",", "mir", "ver\u00b7g\u00f6n\u00b7net", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Nur dem eigenen Drang bin ich ja immer gefolgt.", "tokens": ["Nur", "dem", "ei\u00b7ge\u00b7nen", "Drang", "bin", "ich", "ja", "im\u00b7mer", "ge\u00b7folgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Heuchelt, wie's euch bed\u00fcnkt; ich bekenne fr\u00f6hlich, der G\u00f6ttin", "tokens": ["Heu\u00b7chelt", ",", "wie's", "euch", "be\u00b7d\u00fcnkt", ";", "ich", "be\u00b7ken\u00b7ne", "fr\u00f6h\u00b7lich", ",", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Hab' ich Jugend und Kraft gerne zu Dienste geweiht.", "tokens": ["Hab'", "ich", "Ju\u00b7gend", "und", "Kraft", "ger\u00b7ne", "zu", "Diens\u00b7te", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Nicht mit Asche bestreut' ich mein Haupt, doch kr\u00e4nzt' ich's mit Rosen,", "tokens": ["Nicht", "mit", "A\u00b7sche", "be\u00b7streut'", "ich", "mein", "Haupt", ",", "doch", "kr\u00e4nzt'", "ich's", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Wenn ein M\u00e4dchen mich oft feurigen Armes umschlang.", "tokens": ["Wenn", "ein", "M\u00e4d\u00b7chen", "mich", "oft", "feu\u00b7ri\u00b7gen", "Ar\u00b7mes", "um\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Drum erh\u00f6re mein br\u00fcnstig Gebet, o Himmlische, wende", "tokens": ["Drum", "er\u00b7h\u00f6\u00b7re", "mein", "br\u00fcns\u00b7tig", "Ge\u00b7bet", ",", "o", "Himm\u00b7li\u00b7sche", ",", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Deine Gunst von dem Schwarm, der dich verl\u00e4ugnet, hinweg.", "tokens": ["Dei\u00b7ne", "Gunst", "von", "dem", "Schwarm", ",", "der", "dich", "ver\u00b7l\u00e4ug\u00b7net", ",", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Dir zu opfern gebietet Natur allm\u00e4chtigen Dranges,", "tokens": ["Dir", "zu", "op\u00b7fern", "ge\u00b7bie\u00b7tet", "Na\u00b7tur", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Dran\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Und zu l\u00e4ugnen versucht's Fr\u00f6mmler und Heuchler umsonst.", "tokens": ["Und", "zu", "l\u00e4ug\u00b7nen", "ver\u00b7sucht's", "Fr\u00f6mm\u00b7ler", "und", "Heuch\u00b7ler", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "KON", "NN", "ADV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Nimm mein Opfer und gieb mir ins Leben Sch\u00f6nheit in F\u00fclle,", "tokens": ["Nimm", "mein", "Op\u00b7fer", "und", "gieb", "mir", "ins", "Le\u00b7ben", "Sch\u00f6n\u00b7heit", "in", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "APPRART", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Gieb der Grazie Huld, aber die edelste, mir!", "tokens": ["Gieb", "der", "Gra\u00b7zie", "Huld", ",", "a\u00b7ber", "die", "e\u00b7dels\u00b7te", ",", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "$,", "ADV", "ART", "ADJA", "$,", "PPER", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}}}}}