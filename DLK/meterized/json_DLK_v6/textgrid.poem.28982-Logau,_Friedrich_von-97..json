{"textgrid.poem.28982": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zu N\u00fcrnberg wird gehandelt,", "tokens": ["Was", "zu", "N\u00fcrn\u00b7berg", "wird", "ge\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird gewi\u00df was gutes seyn;", "tokens": ["Wird", "ge\u00b7wi\u00df", "was", "gu\u00b7tes", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann gut Ding darff gute Weile.", "tokens": ["Dann", "gut", "Ding", "darff", "gu\u00b7te", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VMFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo es sich zum \u00e4rgsten wandelt", "tokens": ["Wo", "es", "sich", "zum", "\u00e4rgs\u00b7ten", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Hoffnung nicht trifft ein,", "tokens": ["Und", "mit", "Hoff\u00b7nung", "nicht", "trifft", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebe niemand Schuld der Eile.", "tokens": ["Ge\u00b7be", "nie\u00b7mand", "Schuld", "der", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}