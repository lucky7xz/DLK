{"dta.poem.5724": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Reflexions Generales sur la M a-  \n tiere et sur le Mouvement.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verlangt man Regeln wol von mehrer Wahrheit,", "tokens": ["Ver\u00b7langt", "man", "Re\u00b7geln", "wol", "von", "meh\u00b7rer", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Nachricht, die von gr\u00f6\u00df\u2019rer Klarheit?", "tokens": ["Und", "Nach\u00b7richt", ",", "die", "von", "gr\u00f6\u00df'\u00b7rer", "Klar\u00b7heit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn aus Bewegung blos die Theilungen entstehen;", "tokens": ["Wenn", "aus", "Be\u00b7we\u00b7gung", "blos", "die", "Thei\u00b7lun\u00b7gen", "ent\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch die Theilungen Figuren sind gemacht;", "tokens": ["Und", "durch", "die", "Thei\u00b7lun\u00b7gen", "Fi\u00b7gu\u00b7ren", "sind", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird, da sie durch einander gehen,", "tokens": ["Wird", ",", "da", "sie", "durch", "ein\u00b7an\u00b7der", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was uns vor Augen dargestellt,", "tokens": ["Was", "uns", "vor", "Au\u00b7gen", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erhalten und hervorgebracht.", "tokens": ["Er\u00b7hal\u00b7ten", "und", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bewegung und der Stoff formirt die Welr.", "tokens": ["Be\u00b7we\u00b7gung", "und", "der", "Stoff", "for\u00b7mirt", "die", "Welr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}