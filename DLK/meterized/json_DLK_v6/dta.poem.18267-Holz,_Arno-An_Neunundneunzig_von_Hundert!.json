{"dta.poem.18267": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An Neunundneunzig von Hundert!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Von alter und von neuer Kunst,", "tokens": ["Von", "al\u00b7ter", "und", "von", "neu\u00b7er", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Fleischgenu\u00df und Sinnenbrunst,", "tokens": ["Von", "Fleischge\u00b7nu\u00df", "und", "Sin\u00b7nen\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und gerbt nur Leder, altes Leder!", "tokens": ["Und", "gerbt", "nur", "Le\u00b7der", ",", "al\u00b7tes", "Le\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr la\u00dft um jede Attit\u00fcde", "tokens": ["Ihr", "la\u00dft", "um", "je\u00b7de", "At\u00b7ti\u00b7t\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wei\u00dfgewaschnes Hemdchen wehn,", "tokens": ["Ein", "wei\u00df\u00b7ge\u00b7waschnes", "Hemd\u00b7chen", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn um die Sch\u00f6nheit nackt zu sehn,", "tokens": ["Denn", "um", "die", "Sch\u00f6n\u00b7heit", "nackt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind eure Seelen viel zu pr\u00fcde!", "tokens": ["Sind", "eu\u00b7re", "See\u00b7len", "viel", "zu", "pr\u00fc\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}