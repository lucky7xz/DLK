{"dta.poem.3218": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du noch nicht erschwangst, das kannst du noch erschwingen;", "tokens": ["Was", "du", "noch", "nicht", "er\u00b7schwangst", ",", "das", "kannst", "du", "noch", "er\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was du schon errangst, la\u00df dir nie mehr entringen.", "tokens": ["Und", "was", "du", "schon", "er\u00b7rangst", ",", "la\u00df", "dir", "nie", "mehr", "ent\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von solchem Ehrgeiz wo sich l\u00e4\u00dft ein Sch\u00fcler treiben,", "tokens": ["Von", "sol\u00b7chem", "Ehr\u00b7geiz", "wo", "sich", "l\u00e4\u00dft", "ein", "Sch\u00fc\u00b7ler", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PWAV", "PRF", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wird der erste bald geworden seyn und bleiben.", "tokens": ["Der", "wird", "der", "ers\u00b7te", "bald", "ge\u00b7wor\u00b7den", "seyn", "und", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word",