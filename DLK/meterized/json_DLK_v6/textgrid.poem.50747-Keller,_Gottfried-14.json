{"textgrid.poem.50747": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr nennt uns Tr\u00e4umer, Schwindler, ", "tokens": ["Ihr", "nennt", "uns", "Tr\u00e4u\u00b7mer", ",", "Schwind\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ehrlich wir nach Licht und Wahrheit streben:", "tokens": ["Wenn", "ehr\u00b7lich", "wir", "nach", "Licht", "und", "Wahr\u00b7heit", "stre\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So merket auf mit hochgehobnen Ohren!", "tokens": ["So", "mer\u00b7ket", "auf", "mit", "hoch\u00b7ge\u00b7hob\u00b7nen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Volk zu lichten nur dies ", "tokens": ["Dem", "Volk", "zu", "lich\u00b7ten", "nur", "dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "ADV", "PDS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wer sind die Schwindler nun? \u2013 Ihr, ", "tokens": ["Wer", "sind", "die", "Schwind\u00b7ler", "nun", "?", "\u2013", "Ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$.", "$(", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und wenn die Sterne uns geheim erz\u00e4hlen", "tokens": ["Und", "wenn", "die", "Ster\u00b7ne", "uns", "ge\u00b7heim", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ew'gem Fr\u00fchling, von Unsterblichkeit:", "tokens": ["Von", "ew'\u00b7gem", "Fr\u00fch\u00b7ling", ",", "von", "U\u00b7nsterb\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was geht das euch denn an in ", "tokens": ["Was", "geht", "das", "euch", "denn", "an", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "PPER", "ADV", "APPR", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir lassen uns das Sonnenlicht nicht stehlen", "tokens": ["Wir", "las\u00b7sen", "uns", "das", "Son\u00b7nen\u00b7licht", "nicht", "steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch unsre Lampe, die die Nacht erhellt:", "tokens": ["Noch", "uns\u00b7re", "Lam\u00b7pe", ",", "die", "die", "Nacht", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn uns geh\u00f6rt die ", "tokens": ["Denn", "uns", "ge\u00b7h\u00f6rt", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}