{"textgrid.poem.34521": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Duft der Gr\u00e4ser zieht zur Stadt hinein,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Duft der Gr\u00e4ser zieht zur Stadt hinein,", "tokens": ["Der", "Duft", "der", "Gr\u00e4\u00b7ser", "zieht", "zur", "Stadt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und alles Leben s\u00e4ttigt Sonnenschein.", "tokens": ["und", "al\u00b7les", "Le\u00b7ben", "s\u00e4t\u00b7tigt", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Selig und tr\u00e4g, in wohligem Ermatten", "tokens": ["Se\u00b7lig", "und", "tr\u00e4g", ",", "in", "woh\u00b7li\u00b7gem", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "lieg ich zur\u00fcckgelehnt in luftigem Schatten.", "tokens": ["lieg", "ich", "zu\u00b7r\u00fcck\u00b7ge\u00b7lehnt", "in", "luf\u00b7ti\u00b7gem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Still l\u00e4chelnd, wie ein dummvergn\u00fcgtes Kind,", "tokens": ["Still", "l\u00e4\u00b7chelnd", ",", "wie", "ein", "dumm\u00b7ver\u00b7gn\u00fcg\u00b7tes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "blinzl ich zum Fenster, wo der warme Wind", "tokens": ["blinzl", "ich", "zum", "Fens\u00b7ter", ",", "wo", "der", "war\u00b7me", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "mit rotgestreiften Jalousieen spielt,", "tokens": ["mit", "rot\u00b7ge\u00b7streif\u00b7ten", "Ja\u00b7lou\u00b7si\u00b7e\u00b7en", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wo dann und wann das Licht ins Zimmer schielt.", "tokens": ["wo", "dann", "und", "wann", "das", "Licht", "ins", "Zim\u00b7mer", "schielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O tiefes Gl\u00fcck, befreit von Wunsch und Denken,", "tokens": ["O", "tie\u00b7fes", "Gl\u00fcck", ",", "be\u00b7freit", "von", "Wunsch", "und", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich ganz in heitres Spielen zu versenken,", "tokens": ["sich", "ganz", "in", "heit\u00b7res", "Spie\u00b7len", "zu", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "ob alles Werdens Angst zu triumphieren \u2013", "tokens": ["ob", "al\u00b7les", "Wer\u00b7dens", "Angst", "zu", "tri\u00b7um\u00b7phie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich in des Sommers Ruhe zu verlieren.", "tokens": ["sich", "in", "des", "Som\u00b7mers", "Ru\u00b7he", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}