{"dta.poem.23582": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Censorenmissverst\u00e4ndniss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Kaiserkronen sind erfroren,", "tokens": ["\u201e", "die", "Kai\u00b7ser\u00b7kro\u00b7nen", "sind", "er\u00b7fro\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und heuer sieht das Volk sie nicht.\u201c", "tokens": ["Und", "heu\u00b7er", "sieht", "das", "Volk", "sie", "nicht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fasst den Nachtfrost bei den Ohren,", "tokens": ["So", "fasst", "den", "Nacht\u00b7frost", "bei", "den", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201edie K\u00f6nigskerzen sind erfroren,", "tokens": ["\u201e", "die", "K\u00f6\u00b7nigs\u00b7ker\u00b7zen", "sind", "er\u00b7fro\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und heuer gl\u00e4nzt nicht mehr ihr Licht.\u201c", "tokens": ["Und", "heu\u00b7er", "gl\u00e4nzt", "nicht", "mehr", "ihr", "Licht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herbstwind that's, o ihr Censoren,", "tokens": ["Der", "Herbst\u00b7wind", "that's", ",", "o", "ihr", "Cen\u00b7so\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht strafet mich, nicht straft den Dichter!", "tokens": ["Nicht", "stra\u00b7fet", "mich", ",", "nicht", "straft", "den", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Wahrheit sprach und spricht sein Mund:", "tokens": ["Nur", "Wahr\u00b7heit", "sprach", "und", "spricht", "sein", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dichter ist nur ein Berichter,", "tokens": ["Der", "Dich\u00b7ter", "ist", "nur", "ein", "Be\u00b7rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er thut nur das Erlebni\u00df kund.", "tokens": ["Er", "thut", "nur", "das", "Er\u00b7leb\u00b7ni\u00df", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}