{"textgrid.poem.41231": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Geist der Lieder! der du der Herrscher Lob", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Geist der Lieder! der du der Herrscher Lob", "tokens": ["O", "Geist", "der", "Lie\u00b7der", "!", "der", "du", "der", "Herr\u00b7scher", "Lob"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Herrscher, welche Bilder der Gottheit sind,", "tokens": ["Der", "Herr\u00b7scher", ",", "wel\u00b7che", "Bil\u00b7der", "der", "Got\u00b7theit", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf hellem Mondgew\u00f6lke schwebend", "tokens": ["Auf", "hel\u00b7lem", "Mond\u00b7ge\u00b7w\u00f6l\u00b7ke", "schwe\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit in die Zukunft hin\u00fcber singest,", "tokens": ["Weit", "in", "die", "Zu\u00b7kunft", "hin\u00b7\u00fc\u00b7ber", "sin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Fahr' auf zur Harfe! bereite den hohen Schwung", "tokens": ["Fahr'", "auf", "zur", "Har\u00b7fe", "!", "be\u00b7rei\u00b7te", "den", "ho\u00b7hen", "Schwung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPRART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den unerreichten \u00fcber die Sch\u00f6pfung hin:", "tokens": ["Den", "un\u00b7er\u00b7reich\u00b7ten", "\u00fc\u00b7ber", "die", "Sch\u00f6p\u00b7fung", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er ist gekr\u00f6net! sing zur Veste,", "tokens": ["Er", "ist", "ge\u00b7kr\u00f6\u00b7net", "!", "sing", "zur", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sing zu den Inseln: Er ist gekr\u00f6net!", "tokens": ["Sing", "zu", "den", "In\u00b7seln", ":", "Er", "ist", "ge\u00b7kr\u00f6\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Wie wenn der Adler, den ein gelinder Lenz", "tokens": ["Wie", "wenn", "der", "Ad\u00b7ler", ",", "den", "ein", "ge\u00b7lin\u00b7der", "Lenz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am h\u00f6chsten Felsen k\u00f6niglich auferzog,", "tokens": ["Am", "h\u00f6chs\u00b7ten", "Fel\u00b7sen", "k\u00f6\u00b7nig\u00b7lich", "auf\u00b7er\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jetzt um das Nest die Fl\u00fcgel \u00fcbet,", "tokens": ["Jetzt", "um", "das", "Nest", "die", "Fl\u00fc\u00b7gel", "\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonnenan jetzo sich herrlich aufhebt \u2013", "tokens": ["Son\u00b7ne\u00b7nan", "jet\u00b7zo", "sich", "herr\u00b7lich", "auf\u00b7hebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der L\u00fcfte Barden str\u00f6men ihm Gr\u00fc\u00dfe nach,", "tokens": ["Der", "L\u00fcf\u00b7te", "Bar\u00b7den", "str\u00f6\u00b7men", "ihm", "Gr\u00fc\u00b7\u00dfe", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Berg und H\u00fcgel hallet Bewunderung.", "tokens": ["Und", "Berg", "und", "H\u00fc\u00b7gel", "hal\u00b7let", "Be\u00b7wun\u00b7de\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein \u00e4chter Sohn des gro\u00dfen Vaters", "tokens": ["Ein", "\u00e4ch\u00b7ter", "Sohn", "des", "gro\u00b7\u00dfen", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00dft er in folgenden Lobges\u00e4ngen,", "tokens": ["Hei\u00dft", "er", "in", "fol\u00b7gen\u00b7den", "Lob\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Nach ihm der Herrschaft \u00fcber die m\u00e4chtigen", "tokens": ["Nach", "ihm", "der", "Herr\u00b7schaft", "\u00fc\u00b7ber", "die", "m\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bewohnerreichen Himmelsgebiete werth.", "tokens": ["Be\u00b7woh\u00b7ner\u00b7rei\u00b7chen", "Him\u00b7mels\u00b7ge\u00b7bie\u00b7te", "werth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Vater h\u00f6rt es, und die Mutter;", "tokens": ["Der", "Va\u00b7ter", "h\u00f6rt", "es", ",", "und", "die", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beide durchschleicht ein geheim' Vergn\u00fcgen:", "tokens": ["Bei\u00b7de", "durch\u00b7schleicht", "ein", "ge\u00b7heim'", "Ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "So rief dir Deutschland, bl\u00fchender Joseph! zu,", "tokens": ["So", "rief", "dir", "Deutschland", ",", "bl\u00fc\u00b7hen\u00b7der", "Jo\u00b7se\u00b7ph", "!", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "ADJA", "NE", "$.", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als im Gewimmel fr\u00f6hlicher Tausende", "tokens": ["Als", "im", "Ge\u00b7wim\u00b7mel", "fr\u00f6h\u00b7li\u00b7cher", "Tau\u00b7sen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein Haupt sich hin zur Krone neigte,", "tokens": ["Dein", "Haupt", "sich", "hin", "zur", "Kro\u00b7ne", "neig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df vom gedrungenen, vollen Jauchzen", "tokens": ["Da\u00df", "vom", "ge\u00b7drun\u00b7ge\u00b7nen", ",", "vol\u00b7len", "Jauch\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Th\u00fcrme Frankfurts freudig erbebeten,", "tokens": ["Die", "Th\u00fcr\u00b7me", "Frank\u00b7furts", "freu\u00b7dig", "er\u00b7be\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der blaue Main es, Ufer hinab, dem Rhein,", "tokens": ["Der", "blau\u00b7e", "Main", "es", ",", "U\u00b7fer", "hin\u00b7ab", ",", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,", "NN", "PTKVZ", "$,", "PRELS", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem schilfbekr\u00e4nzten Bruder, zutrug,", "tokens": ["Dem", "schilf\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Bru\u00b7der", ",", "zu\u00b7trug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dieser den horchenden Wasserwelten.", "tokens": ["Die\u00b7ser", "den", "hor\u00b7chen\u00b7den", "Was\u00b7ser\u00b7wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Es sah mit Augen, welch ein Geschenk er sey", "tokens": ["Es", "sah", "mit", "Au\u00b7gen", ",", "welch", "ein", "Ge\u00b7schenk", "er", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAT", "ART", "NN", "PPER", "VAFIN"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der Sohn, der einst, vom Himmel herabgefleht,", "tokens": ["Der", "Sohn", ",", "der", "einst", ",", "vom", "Him\u00b7mel", "her\u00b7ab\u00b7ge\u00b7fleht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Franzen's und Theresen's Augen", "tokens": ["In", "Fran\u00b7zen's", "und", "The\u00b7re\u00b7sen's", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Winkenden Kronen entgegenreifet,", "tokens": ["Win\u00b7ken\u00b7den", "Kro\u00b7nen", "ent\u00b7ge\u00b7gen\u00b7rei\u00b7fet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Voll seiner Ahnen, seiner Bestimmung voll,", "tokens": ["Voll", "sei\u00b7ner", "Ah\u00b7nen", ",", "sei\u00b7ner", "Be\u00b7stim\u00b7mung", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Getreu den Mustern, eilend, wie sie, zu seyn", "tokens": ["Ge\u00b7treu", "den", "Mus\u00b7tern", ",", "ei\u00b7lend", ",", "wie", "sie", ",", "zu", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "VVPP", "$,", "PWAV", "PPER", "$,", "PTKZU", "VAINF"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der Erde Lust, der Gottheit Spiegel,", "tokens": ["Der", "Er\u00b7de", "Lust", ",", "der", "Got\u00b7theit", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6pfer des Gl\u00fcckes von Teut's Geschlechtern;", "tokens": ["Sch\u00f6p\u00b7fer", "des", "Gl\u00fc\u00b7ckes", "von", "Teut's", "Ge\u00b7schlech\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Der untergeb'nen Vater, des Lasters Feind,", "tokens": ["Der", "un\u00b7ter\u00b7ge\u00b7b'\u00b7nen", "Va\u00b7ter", ",", "des", "Las\u00b7ters", "Feind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch, wenn's in Th\u00fcrmen wohnet, aus Golde trinkt,", "tokens": ["Auch", ",", "wenn's", "in", "Th\u00fcr\u00b7men", "woh\u00b7net", ",", "aus", "Gol\u00b7de", "trinkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Tugend Freund, auch, wenn sie pfl\u00fcget,", "tokens": ["Der", "Tu\u00b7gend", "Freund", ",", "auch", ",", "wenn", "sie", "pfl\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00fctzer und Lied der gesch\u00fctzten Barden.", "tokens": ["Sch\u00fct\u00b7zer", "und", "Lied", "der", "ge\u00b7sch\u00fctz\u00b7ten", "Bar\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "So gleicht die Frucht dem Stamme. So str\u00f6mt die Kraft", "tokens": ["So", "gleicht", "die", "Frucht", "dem", "Stam\u00b7me", ".", "So", "str\u00f6mt", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erhab'ner Zeuger in den Erzeugten aus.", "tokens": ["Er\u00b7hab'\u00b7ner", "Zeu\u00b7ger", "in", "den", "Er\u00b7zeug\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Eichen sprossen eitel Eichen,", "tokens": ["Von", "Ei\u00b7chen", "spros\u00b7sen", "ei\u00b7tel", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tauben entfliegen nicht Adlernesten.", "tokens": ["Tau\u00b7ben", "ent\u00b7flie\u00b7gen", "nicht", "Ad\u00b7ler\u00b7nes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Wie vielen K\u00e4mpfen Oesterreichs Heldenhaus", "tokens": ["Wie", "vie\u00b7len", "K\u00e4mp\u00b7fen", "O\u00b7es\u00b7ter\u00b7reichs", "Hel\u00b7den\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dich sich ausbot, Vaterland! wei\u00df der Belt,", "tokens": ["F\u00fcr", "dich", "sich", "aus\u00b7bot", ",", "Va\u00b7ter\u00b7land", "!", "wei\u00df", "der", "Belt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$,", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erz\u00e4hlt der Rhein noch, r\u00fchmt die Donau,", "tokens": ["Er\u00b7z\u00e4hlt", "der", "Rhein", "noch", ",", "r\u00fchmt", "die", "Do\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADV", "$,", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo sie die F\u00fclle vereinter Wasser", "tokens": ["Wo", "sie", "die", "F\u00fcl\u00b7le", "ver\u00b7ein\u00b7ter", "Was\u00b7ser"], "t