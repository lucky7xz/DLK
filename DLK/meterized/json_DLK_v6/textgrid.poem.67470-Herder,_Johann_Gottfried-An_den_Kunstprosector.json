{"textgrid.poem.67470": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den Kunstprosector", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df die Rose mir bl\u00fchn! Warum zerknicket Dein Finger", "tokens": ["La\u00df", "die", "Ro\u00b7se", "mir", "bl\u00fchn", "!", "Wa\u00b7rum", "zer\u00b7kni\u00b7cket", "Dein", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$.", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit Apothekerkunst dieses ambrosische Kind?", "tokens": ["Mit", "A\u00b7pot\u00b7he\u00b7ker\u00b7kunst", "die\u00b7ses", "am\u00b7bro\u00b7si\u00b7sche", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-----+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "La\u00df die Lilie bl\u00fchn! In ihrem Strahlengewande", "tokens": ["La\u00df", "die", "Li\u00b7lie", "bl\u00fchn", "!", "In", "ih\u00b7rem", "Strah\u00b7len\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Webet Ger\u00fcche sie mir, athmend in himmlischem Thau.", "tokens": ["We\u00b7bet", "Ge\u00b7r\u00fc\u00b7che", "sie", "mir", ",", "ath\u00b7mend", "in", "himm\u00b7li\u00b7schem", "Thau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Lebend wirken die Holden mit Sonn' und dem Aether ihr Werk aus;", "tokens": ["Le\u00b7bend", "wir\u00b7ken", "die", "Hol\u00b7den", "mit", "Sonn'", "und", "dem", "A\u00b7e\u00b7ther", "ihr", "Werk", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Todt von Dir gedr\u00fcckt, geben sie Zwiebelgeruch.", "tokens": ["Todt", "von", "Dir", "ge\u00b7dr\u00fcckt", ",", "ge\u00b7ben", "sie", "Zwie\u00b7bel\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}