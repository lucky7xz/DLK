{"textgrid.poem.48987": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um Mitternacht, wer weint herab?", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht, wer weint herab?", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "wer", "weint", "her\u00b7ab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "PWS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hemmt des Stromes Rauschen?", "tokens": ["Wer", "hemmt", "des", "Stro\u00b7mes", "Rau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Emma, du beim Sternentanz", "tokens": ["Em\u00b7ma", ",", "du", "beim", "Ster\u00b7nen\u00b7tanz"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und deiner S\u00f6hne Br\u00e4ute?", "tokens": ["Und", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Br\u00e4u\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie lagern hin, das Haupt auf dem Stein \u2013", "tokens": ["Sie", "la\u00b7gern", "hin", ",", "das", "Haupt", "auf", "dem", "Stein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fluch den brennenden Sternen!", "tokens": ["Fluch", "den", "bren\u00b7nen\u00b7den", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Es zittern bleich die Kinder der Nacht", "tokens": ["Es", "zit\u00b7tern", "bleich", "die", "Kin\u00b7der", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In der Flut, es horchen die Klippen", "tokens": ["In", "der", "Flut", ",", "es", "hor\u00b7chen", "die", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im schwarzen, schwarzen Tale,", "tokens": ["Im", "schwar\u00b7zen", ",", "schwar\u00b7zen", "Ta\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Klippe sich hebt aus dunkler Flut,", "tokens": ["Wo", "Klip\u00b7pe", "sich", "hebt", "aus", "dunk\u00b7ler", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wo ihre Wunden triefen!", "tokens": ["Wo", "ih\u00b7re", "Wun\u00b7den", "trie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach meine S\u00f6hne im blutigen Grund,", "tokens": ["Ach", "mei\u00b7ne", "S\u00f6h\u00b7ne", "im", "blu\u00b7ti\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wer hat Euch alle erschlagen?", "tokens": ["Wer", "hat", "Euch", "al\u00b7le", "er\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, meine Blumen am schattigen Bach,", "tokens": ["Ach", ",", "mei\u00b7ne", "Blu\u00b7men", "am", "schat\u00b7ti\u00b7gen", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wer hat Euch alle gem\u00e4het?", "tokens": ["Wer", "hat", "Euch", "al\u00b7le", "ge\u00b7m\u00e4\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sag, Mildeburg! Die \u00c4uglein na\u00df,", "tokens": ["Sag", ",", "Mil\u00b7de\u00b7burg", "!", "Die", "\u00c4ug\u00b7lein", "na\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Blut soll Violen f\u00e4rben.", "tokens": ["Dein", "Blut", "soll", "Vi\u00b7o\u00b7len", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sag, Heldengast! Dein M\u00fcndlein bla\u00df,", "tokens": ["Sag", ",", "Hel\u00b7den\u00b7gast", "!", "Dein", "M\u00fcnd\u00b7lein", "bla\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$.", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Haar zerzaust im Sterben.", "tokens": ["Dein", "Haar", "zer\u00b7zaust", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Heimchen der Nacht rief uns ins Land,", "tokens": ["Kein", "Heim\u00b7chen", "der", "Nacht", "rief", "uns", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist doch alles stille.", "tokens": ["Es", "ist", "doch", "al\u00b7les", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Rosenbett war uns bestellt \u2013", "tokens": ["Ein", "Ro\u00b7sen\u00b7bett", "war", "uns", "be\u00b7stellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schlummern auf Speer und Schwertern.", "tokens": ["Wir", "schlum\u00b7mern", "auf", "Speer", "und", "Schwer\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gestillt ward unser Heldendurst", "tokens": ["Ge\u00b7stillt", "ward", "un\u00b7ser", "Hel\u00b7den\u00b7durst"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im schwarzen, schwarzen Tale,", "tokens": ["Im", "schwar\u00b7zen", ",", "schwar\u00b7zen", "Ta\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geendigt unser Todeslauf", "tokens": ["Ge\u00b7en\u00b7digt", "un\u00b7ser", "To\u00b7des\u00b7lauf"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im schwarzen, schwarzen Tale.", "tokens": ["Im", "schwar\u00b7zen", ",", "schwar\u00b7zen", "Ta\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich Thurmann lieg auf einem Ro\u00df;", "tokens": ["Ich", "Thur\u00b7mann", "lieg", "auf", "ei\u00b7nem", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich Wethal an der Klippe \u2013", "tokens": ["Ich", "Wet\u00b7hal", "an", "der", "Klip\u00b7pe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drei Speere halt ich in der Brust,", "tokens": ["Drei", "Spee\u00b7re", "halt", "ich", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Schwert in meiner Rippe.", "tokens": ["Ein", "Schwert", "in", "mei\u00b7ner", "Rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir schlummern vom Dunkel gedeckt gar schwer \u2013", "tokens": ["Wir", "schlum\u00b7mern", "vom", "Dun\u00b7kel", "ge\u00b7deckt", "gar", "schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "ADV", "ADJD", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wir schlummern Heldenleben!", "tokens": ["Wir", "schlum\u00b7mern", "Hel\u00b7den\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir machten weinen der M\u00fctter mehr", "tokens": ["Wir", "mach\u00b7ten", "wei\u00b7nen", "der", "M\u00fct\u00b7ter", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Bl\u00e4tter im Sturme beben!", "tokens": ["Als", "Bl\u00e4t\u00b7ter", "im", "Stur\u00b7me", "be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Macht unserm Leib kein Bad zurecht,", "tokens": ["Macht", "un\u00b7serm", "Leib", "kein", "Bad", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PIAT", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00f6rt die goldne Kammer,", "tokens": ["Zer\u00b7st\u00f6rt", "die", "gold\u00b7ne", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir alle schlummern im schwarzen Grund", "tokens": ["Wir", "al\u00b7le", "schlum\u00b7mern", "im", "schwar\u00b7zen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umw\u00f6lbt vom blutigen Schilde.", "tokens": ["Um\u00b7w\u00f6lbt", "vom", "blu\u00b7ti\u00b7gen", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}