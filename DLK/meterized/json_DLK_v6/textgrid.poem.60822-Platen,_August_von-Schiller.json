{"textgrid.poem.60822": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Etwas weniger, Freund, Liebschaften! So w\u00e4rst du beliebt zwar", "tokens": ["Et\u00b7was", "we\u00b7ni\u00b7ger", ",", "Freund", ",", "Lieb\u00b7schaf\u00b7ten", "!", "So", "w\u00e4rst", "du", "be\u00b7liebt", "zwar"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "NN", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "ADV"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Weniger, weil ja so sehr Thekla gefallen und Max:", "tokens": ["We\u00b7ni\u00b7ger", ",", "weil", "ja", "so", "sehr", "The\u00b7kla", "ge\u00b7fal\u00b7len", "und", "Max", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADV", "ADV", "ADV", "NE", "VVPP", "KON", "NE", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Eins doch find ich zu stark, da\u00df selbst die begeisterte Jungfrau", "tokens": ["Eins", "doch", "find", "ich", "zu", "stark", ",", "da\u00df", "selbst", "die", "be\u00b7geis\u00b7ter\u00b7te", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Noch sich verliebt, furchtbar schnell, in den britischen Lord.", "tokens": ["Noch", "sich", "ver\u00b7liebt", ",", "furcht\u00b7bar", "schnell", ",", "in", "den", "bri\u00b7ti\u00b7schen", "Lord", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVPP", "$,", "ADJD", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}