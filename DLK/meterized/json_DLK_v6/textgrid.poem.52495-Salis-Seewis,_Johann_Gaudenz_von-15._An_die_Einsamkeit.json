{"textgrid.poem.52495": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "15. An die Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsamkeit, sanfter Trauer Labsal! Amme", "tokens": ["Ein\u00b7sam\u00b7keit", ",", "sanf\u00b7ter", "Trau\u00b7er", "Lab\u00b7sal", "!", "Am\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "NN", "$.", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Jedes tiefen Gef\u00fchls! In deinem Scho\u00dfe", "tokens": ["Je\u00b7des", "tie\u00b7fen", "Ge\u00b7f\u00fchls", "!", "In", "dei\u00b7nem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ruh' ich, wie in buschiger Bucht der stille", "tokens": ["Ruh'", "ich", ",", "wie", "in", "bu\u00b7schi\u00b7ger", "Bucht", "der", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Friedliche Nachen.", "tokens": ["Fried\u00b7li\u00b7che", "Na\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier, wo der schaumumw\u00f6lkte Bach vom Felsen", "tokens": ["Hier", ",", "wo", "der", "schau\u00b7mum\u00b7w\u00f6lk\u00b7te", "Bach", "vom", "Fel\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schmilzt, dann perlend zerspritzt und K\u00fchlung dampfet,", "tokens": ["Schmilzt", ",", "dann", "per\u00b7lend", "zer\u00b7spritzt", "und", "K\u00fch\u00b7lung", "damp\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "VVPP", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da\u00df die Weidenstauden umher im Taustaub", "tokens": ["Da\u00df", "die", "Wei\u00b7dens\u00b7tau\u00b7den", "um\u00b7her", "im", "Tau\u00b7staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schwankend erschauern,", "tokens": ["Schwan\u00b7kend", "er\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier will ich weilen! Hier ist's traut und heimlich;", "tokens": ["Hier", "will", "ich", "wei\u00b7len", "!", "Hier", "ist's", "traut", "und", "heim\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Friede s\u00e4nftigt des vollen Busens Wallung;", "tokens": ["Frie\u00b7de", "s\u00e4nf\u00b7tigt", "des", "vol\u00b7len", "Bu\u00b7sens", "Wal\u00b7lung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Jede Sorg' entschlummert zu s\u00fc\u00dfen Tr\u00e4umen;", "tokens": ["Je\u00b7de", "Sor\u00b7g'", "ent\u00b7schlum\u00b7mert", "zu", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Hier will ich weilen!", "tokens": ["Hier", "will", "ich", "wei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Aber was schimmert durch die regen Bl\u00e4tter?", "tokens": ["A\u00b7ber", "was", "schim\u00b7mert", "durch", "die", "re\u00b7gen", "Bl\u00e4t\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ist's ein Schleier? \u2013 Sie ist's! Ich folg', ich folge!", "tokens": ["Ist's", "ein", "Schlei\u00b7er", "?", "\u2013", "Sie", "ist's", "!", "Ich", "fol\u00b7g'", ",", "ich", "fol\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Z\u00fcrne mir nicht, Einsamkeit, du Vertraute!", "tokens": ["Z\u00fcr\u00b7ne", "mir", "nicht", ",", "Ein\u00b7sam\u00b7keit", ",", "du", "Ver\u00b7trau\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Kannst uns begleiten!", "tokens": ["Kannst", "uns", "be\u00b7glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}