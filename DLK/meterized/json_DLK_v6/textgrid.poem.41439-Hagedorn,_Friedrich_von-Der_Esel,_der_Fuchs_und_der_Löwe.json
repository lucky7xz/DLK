{"textgrid.poem.41439": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Esel, der Fuchs und der L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Esel kam der Fuchs auf seine Distelweide,", "tokens": ["Zum", "E\u00b7sel", "kam", "der", "Fuchs", "auf", "sei\u00b7ne", "Dis\u00b7tel\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: Freund, meinen Gru\u00df zuvor,", "tokens": ["Und", "sprach", ":", "Freund", ",", "mei\u00b7nen", "Gru\u00df", "zu\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du scheinst noch immer jung in deinem alten Kleide.", "tokens": ["Du", "scheinst", "noch", "im\u00b7mer", "jung", "in", "dei\u00b7nem", "al\u00b7ten", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie lustig spielt noch jetzt dein hochansehnlich Ohr!", "tokens": ["Wie", "lus\u00b7tig", "spielt", "noch", "jetzt", "dein", "hoch\u00b7an\u00b7sehn\u00b7lich", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du bist und bleibst ein Freund der Freude.", "tokens": ["Du", "bist", "und", "bleibst", "ein", "Freund", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh auf! der Morgen wird recht sch\u00f6n.", "tokens": ["Sieh", "auf", "!", "der", "Mor\u00b7gen", "wird", "recht", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was fangen wir nun an? Nicht wahr, wir wollen beide", "tokens": ["Was", "fan\u00b7gen", "wir", "nun", "an", "?", "Nicht", "wahr", ",", "wir", "wol\u00b7len", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PTKNEG", "ADJD", "$,", "PPER", "VMFIN", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In jenem Wald spazieren gehn?", "tokens": ["In", "je\u00b7nem", "Wald", "spa\u00b7zie\u00b7ren", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ei ja, versetzt der Freund: was ist denn dort zu sehen?", "tokens": ["Ei", "ja", ",", "ver\u00b7setzt", "der", "Freund", ":", "was", "ist", "denn", "dort", "zu", "se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "VVFIN", "ART", "NN", "$.", "PWS", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Muster, sagt der Schalk, vollkommner Eselinnen.", "tokens": ["Ein", "Mus\u00b7ter", ",", "sagt", "der", "Schalk", ",", "voll\u00b7komm\u00b7ner", "E\u00b7se\u00b7lin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es wiehert mancher Hengst, die Spr\u00f6de zu gewinnen;", "tokens": ["Es", "wie\u00b7hert", "man\u00b7cher", "Hengst", ",", "die", "Spr\u00f6\u00b7de", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Doch sie wird dir nicht widerstehn.", "tokens": ["Doch", "sie", "wird", "dir", "nicht", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sieh auf! ... Ei ja ... und sieh der Sonne rothes Licht!", "tokens": ["Sieh", "auf", "!", "...", "Ei", "ja", "...", "und", "sieh", "der", "Son\u00b7ne", "ro\u00b7thes", "Licht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "NN", "ADV", "$(", "KON", "VVIMP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "(so wortreich ist der Fuchs: er schwatzt, wie Redner pflegen,", "tokens": ["(", "so", "wort\u00b7reich", "ist", "der", "Fuchs", ":", "er", "schwatzt", ",", "wie", "Red\u00b7ner", "pfle\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ART", "NE", "$.", "PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die mehr bet\u00e4uben, als bewegen;", "tokens": ["Die", "mehr", "be\u00b7t\u00e4u\u00b7ben", ",", "als", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Doch merke man sich auch, da\u00df er zum Esel spricht.)", "tokens": ["Doch", "mer\u00b7ke", "man", "sich", "auch", ",", "da\u00df", "er", "zum", "E\u00b7sel", "spricht", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIS", "PRF", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie wandeln plaudernd fort. Bald aber zeiget sich", "tokens": ["Sie", "wan\u00b7deln", "plau\u00b7dernd", "fort", ".", "Bald", "a\u00b7ber", "zei\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "ADV", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der K\u00f6nig selbst, der L\u00f6w' in seinem h\u00f6chsten Grimme.", "tokens": ["Der", "K\u00f6\u00b7nig", "selbst", ",", "der", "L\u00f6\u00b7w'", "in", "sei\u00b7nem", "h\u00f6chs\u00b7ten", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der Anblick nimmt sogleich dem Esel Muth und Stimme.", "tokens": ["Der", "An\u00b7blick", "nimmt", "sog\u00b7leich", "dem", "E\u00b7sel", "Muth", "und", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er zittert, l\u00e4uft, und f\u00e4llt. Ein L\u00f6w' ist f\u00fcrchterlich.", "tokens": ["Er", "zit\u00b7tert", ",", "l\u00e4uft", ",", "und", "f\u00e4llt", ".", "Ein", "L\u00f6\u00b7w'", "ist", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Der Fuchs h\u00e4lt gleichwol Stand, und sagt: Begl\u00fcckt bin ich,", "tokens": ["Der", "Fuchs", "h\u00e4lt", "gleich\u00b7wol", "Stand", ",", "und", "sagt", ":", "Be\u00b7gl\u00fcckt", "bin", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "NN", "$,", "KON", "VVFIN", "$.", "VVPP", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Herr! heute dich nicht zu verfehlen.", "tokens": ["Herr", "!", "heu\u00b7te", "dich", "nicht", "zu", "ver\u00b7feh\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ich eilte dich zu sehn. Zum Fr\u00fchst\u00fcck bring' ich dir", "tokens": ["Ich", "eil\u00b7te", "dich", "zu", "sehn", ".", "Zum", "Fr\u00fch\u00b7st\u00fcck", "bring'", "ich", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "APPRART", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den Kern des Eselstamm's, dort jenes feiste Thier.", "tokens": ["Den", "Kern", "des", "E\u00b7sel\u00b7stam\u00b7m's", ",", "dort", "je\u00b7nes", "feis\u00b7te", "Thier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Der ernste L\u00f6we spricht: Zur Mahlzeit dien' es mir;", "tokens": ["Der", "erns\u00b7te", "L\u00f6\u00b7we", "spricht", ":", "Zur", "Mahl\u00b7zeit", "dien'", "es", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dich selbst will ich zum Fr\u00fchst\u00fcck w\u00e4hlen.", "tokens": ["Dich", "selbst", "will", "ich", "zum", "Fr\u00fch\u00b7st\u00fcck", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Schnell wird der Fuchs zerst\u00fcckt. Was lehrt des L\u00f6wen That?", "tokens": ["Schnell", "wird", "der", "Fuchs", "zer\u00b7st\u00fcckt", ".", "Was", "lehrt", "des", "L\u00f6\u00b7wen", "That", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NE", "VVPP", "$.", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verr\u00e4ther hasset man, und nutzet den Verrath.", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "has\u00b7set", "man", ",", "und", "nut\u00b7zet", "den", "Ver\u00b7rath", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}