{"textgrid.poem.67831": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3. Lied des jungen Reuters", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit fr\u00fchem Morgen", "tokens": ["Mit", "fr\u00fc\u00b7hem", "Mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sey schon mein Pferd gef\u00fcttert.", "tokens": ["Sey", "schon", "mein", "Pferd", "ge\u00b7f\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bald's nur taget,", "tokens": ["So", "bald's", "nur", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit Sonnenaufgang", "tokens": ["Mit", "Son\u00b7nen\u00b7auf\u00b7gang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mu\u00df ich von hinnen reiten.", "tokens": ["Mu\u00df", "ich", "von", "hin\u00b7nen", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da steht mein Vater,", "tokens": ["Da", "steht", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da mir zur Seite steht er", "tokens": ["Da", "mir", "zur", "Sei\u00b7te", "steht", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der alte Vater,", "tokens": ["Der", "al\u00b7te", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dr\u00e4ngt sich an meine Seite.", "tokens": ["Dr\u00e4ngt", "sich", "an", "mei\u00b7ne", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Er steht mit mir zu sprechen;", "tokens": ["Er", "steht", "mit", "mir", "zu", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er spricht, mich zu ermahnen,", "tokens": ["Er", "spricht", ",", "mich", "zu", "er\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mich ermahnend weint er.", "tokens": ["Und", "mich", "er\u00b7mah\u00b7nend", "weint", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Still, weine nicht, mein Vater!", "tokens": ["Still", ",", "wei\u00b7ne", "nicht", ",", "mein", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still, weine nicht, mein Alter!", "tokens": ["Still", ",", "wei\u00b7ne", "nicht", ",", "mein", "Al\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frisch ich weggetrabet,", "tokens": ["So", "frisch", "ich", "weg\u00b7ge\u00b7tra\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So frisch trab' ich zur\u00fccke,", "tokens": ["So", "frisch", "trab'", "ich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um dich nur nicht zu kr\u00e4nken.", "tokens": ["Um", "dich", "nur", "nicht", "zu", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ei, mein Hengstchen,", "tokens": ["Ei", ",", "mein", "Hengst\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ei, mein Brauner,", "tokens": ["Ei", ",", "mein", "Brau\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wohin streichst du?", "tokens": ["Wo\u00b7hin", "streichst", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wohin schnaubst du?", "tokens": ["Wo\u00b7hin", "schnaubst", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wohin wirst mich tragen?", "tokens": ["Wo\u00b7hin", "wirst", "mich", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ei in Krieg hin!", "tokens": ["Ei", "in", "Krieg", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hin in fremde Lande!", "tokens": ["Hin", "in", "frem\u00b7de", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dahin streichst du,", "tokens": ["Da\u00b7hin", "streichst", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "(dahin schnaubst du,)", "tokens": ["(", "da\u00b7hin", "schnaubst", "du", ",", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dahin wirst mich tragen.", "tokens": ["Da\u00b7hin", "wirst", "mich", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wird dir zu sauer", "tokens": ["Wird", "dir", "zu", "sau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die weite Strasse?", "tokens": ["Die", "wei\u00b7te", "Stras\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wird zu schwer dir", "tokens": ["Wird", "zu", "schwer", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKA", "ADJD", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Dieser Sack mit Haber?", "tokens": ["Die\u00b7ser", "Sack", "mit", "Ha\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Oder dieser junge Reuter", "tokens": ["O\u00b7der", "die\u00b7ser", "jun\u00b7ge", "Reu\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dieser Reuters-Livrei,", "tokens": ["In", "die\u00b7ser", "Reu\u00b7ter\u00b7sLi\u00b7vrei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mit dem blanken S\u00e4bel?", "tokens": ["Mit", "dem", "blan\u00b7ken", "S\u00e4\u00b7bel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ja zu sauer", "tokens": ["Ja", "zu", "sau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PTKA", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wird der lange Weg mir,", "tokens": ["Wird", "der", "lan\u00b7ge", "Weg", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und diese Nacht, stockfinster,", "tokens": ["Und", "die\u00b7se", "Nacht", ",", "stock\u00b7fins\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und diese gr\u00fcne Heide,", "tokens": ["Und", "die\u00b7se", "gr\u00fc\u00b7ne", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dieser schwarze Morast \u2013 \u2013 \u2013", "tokens": ["Und", "die\u00b7ser", "schwar\u00b7ze", "Mo\u00b7rast", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}