{"textgrid.poem.62260": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[der Krater der Berge Feuer spr\u00fcht]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krater der Berge Feuer spr\u00fcht,", "tokens": ["Der", "Kra\u00b7ter", "der", "Ber\u00b7ge", "Feu\u00b7er", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das V\u00f6glein verstummt, das Bl\u00fcmlein verbl\u00fcht,", "tokens": ["Das", "V\u00f6\u00b7glein", "ver\u00b7stummt", ",", "das", "Bl\u00fcm\u00b7lein", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dies Bild gleicht der Habsucht gef\u00e4hrlichem Spiel,", "tokens": ["Dies", "Bild", "gleicht", "der", "Hab\u00b7sucht", "ge\u00b7f\u00e4hr\u00b7li\u00b7chem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Verheeren, verw\u00fcsten, vernichten ihr Ziel. \u2013", "tokens": ["Ver\u00b7hee\u00b7ren", ",", "ver\u00b7w\u00fcs\u00b7ten", ",", "ver\u00b7nich\u00b7ten", "ihr", "Ziel", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}