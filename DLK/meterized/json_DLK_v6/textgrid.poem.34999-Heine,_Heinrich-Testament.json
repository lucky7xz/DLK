{"textgrid.poem.34999": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Testament", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mache jetzt mein Testament,", "tokens": ["Ich", "ma\u00b7che", "jetzt", "mein", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht nun bald mit mir zu End'.", "tokens": ["Es", "geht", "nun", "bald", "mit", "mir", "zu", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur wundre ich mich, da\u00df nicht schon l\u00e4ngstens", "tokens": ["Nur", "wund\u00b7re", "ich", "mich", ",", "da\u00df", "nicht", "schon", "l\u00e4ngs\u00b7tens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PTKNEG", "ADV", "ADV"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Herz gebrochen vor Gram und \u00c4ngsten.", "tokens": ["Mein", "Herz", "ge\u00b7bro\u00b7chen", "vor", "Gram", "und", "\u00c4ngs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du aller Frauen Huld und Zier,", "tokens": ["Du", "al\u00b7ler", "Frau\u00b7en", "Huld", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Luise! ich vermache dir", "tokens": ["Lu\u00b7i\u00b7se", "!", "ich", "ver\u00b7ma\u00b7che", "dir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zw\u00f6lf alte Hemde und hundert Fl\u00f6he", "tokens": ["Zw\u00f6lf", "al\u00b7te", "Hem\u00b7de", "und", "hun\u00b7dert", "Fl\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "KON", "CARD", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dreimalhunderttausend Fl\u00fcche.", "tokens": ["Und", "drei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Fl\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem guten Freund, der mit gutem Rat", "tokens": ["Dem", "gu\u00b7ten", "Freund", ",", "der", "mit", "gu\u00b7tem", "Rat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir immer riet und nie was tat,", "tokens": ["Mir", "im\u00b7mer", "riet", "und", "nie", "was", "tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "ADV", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt, als Verm\u00e4chtnis, rat ich ihm selber:", "tokens": ["Jetzt", ",", "als", "Ver\u00b7m\u00e4cht\u00b7nis", ",", "rat", "ich", "ihm", "sel\u00b7ber", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nimm eine Kuh und zeuge K\u00e4lber.", "tokens": ["Nimm", "ei\u00b7ne", "Kuh", "und", "zeu\u00b7ge", "K\u00e4l\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wem geb ich meine Religion,", "tokens": ["Wem", "geb", "ich", "mei\u00b7ne", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Glauben an Vater, Geist und Sohn?", "tokens": ["Den", "Glau\u00b7ben", "an", "Va\u00b7ter", ",", "Geist", "und", "Sohn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Kaiser von China, der Rabbi von Posen,", "tokens": ["Der", "Kai\u00b7ser", "von", "Chi\u00b7na", ",", "der", "Rab\u00b7bi", "von", "Po\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie sollen beide darum losen.", "tokens": ["Sie", "sol\u00b7len", "bei\u00b7de", "da\u00b7rum", "lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den deutschen Freiheits- und Gleichheitstraum,", "tokens": ["Den", "deut\u00b7schen", "Frei\u00b7heits", "und", "Gleich\u00b7heits\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Seifenblasen vom besten Schaum,", "tokens": ["Die", "Sei\u00b7fen\u00b7bla\u00b7sen", "vom", "bes\u00b7ten", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vermach ich dem Zensor der Stadt Kr\u00e4hwinkel;", "tokens": ["Ver\u00b7mach", "ich", "dem", "Zen\u00b7sor", "der", "Stadt", "Kr\u00e4h\u00b7win\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nahrhafter freilich ist Pumpernickel.", "tokens": ["Nahr\u00b7haf\u00b7ter", "frei\u00b7lich", "ist", "Pum\u00b7per\u00b7ni\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Taten, die ich noch nicht getan,", "tokens": ["Die", "Ta\u00b7ten", ",", "die", "ich", "noch", "nicht", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den ganzen Vaterlandsrettungsplan,", "tokens": ["Den", "gan\u00b7zen", "Va\u00b7ter\u00b7lands\u00b7ret\u00b7tungs\u00b7plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nebst einem Rezept gegen Katzenjammer,", "tokens": ["Nebst", "ei\u00b7nem", "Re\u00b7zept", "ge\u00b7gen", "Kat\u00b7zen\u00b7jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermach ich den Helden der badischen Kammer.", "tokens": ["Ver\u00b7mach", "ich", "den", "Hel\u00b7den", "der", "ba\u00b7di\u00b7schen", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und eine Schlafm\u00fctz', wei\u00df wie Kreid',", "tokens": ["Und", "ei\u00b7ne", "Schlaf\u00b7m\u00fctz'", ",", "wei\u00df", "wie", "Kreid'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermach ich dem Vetter, der zur Zeit", "tokens": ["Ver\u00b7mach", "ich", "dem", "Vet\u00b7ter", ",", "der", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcr die Heidschnuckenrechte so k\u00fchn geredet;", "tokens": ["F\u00fcr", "die", "Heidsc\u00b7hnu\u00b7cken\u00b7rech\u00b7te", "so", "k\u00fchn", "ge\u00b7re\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt schweigt er wie ein echter R\u00f6mer.", "tokens": ["Jetzt", "schweigt", "er", "wie", "ein", "ech\u00b7ter", "R\u00f6\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ich vermache dem Sittenwart", "tokens": ["Und", "ich", "ver\u00b7ma\u00b7che", "dem", "Sit\u00b7ten\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Glaubensvogt zu Stuttegard", "tokens": ["Und", "Glau\u00b7bens\u00b7vogt", "zu", "Stut\u00b7te\u00b7gard"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Paar Pistolen (doch nicht geladen),", "tokens": ["Ein", "Paar", "Pis\u00b7to\u00b7len", "(", "doch", "nicht", "ge\u00b7la\u00b7den", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$(", "ADV", "PTKNEG", "VVPP", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann seiner Frau damit Furcht einjagen.", "tokens": ["Kann", "sei\u00b7ner", "Frau", "da\u00b7mit", "Furcht", "ein\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PAV", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein treues Abbild von meinem Stei\u00df,", "tokens": ["Ein", "treu\u00b7es", "Ab\u00b7bild", "von", "mei\u00b7nem", "Stei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vermach ich der schw\u00e4bischen Schule; ich wei\u00df,", "tokens": ["Ver\u00b7mach", "ich", "der", "schw\u00e4\u00b7bi\u00b7schen", "Schu\u00b7le", ";", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr wolltet mein Gesicht nicht haben,", "tokens": ["Ihr", "woll\u00b7tet", "mein", "Ge\u00b7sicht", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun k\u00f6nnt ihr am Gegenteil euch laben.", "tokens": ["Nun", "k\u00f6nnt", "ihr", "am", "Ge\u00b7gen\u00b7teil", "euch", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Zw\u00f6lf Kr\u00fcge Seidlitzer Wasser vermach", "tokens": ["Zw\u00f6lf", "Kr\u00fc\u00b7ge", "Seid\u00b7lit\u00b7zer", "Was\u00b7ser", "ver\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich dem edlen Dichtergem\u00fct, das, ach!", "tokens": ["Ich", "dem", "ed\u00b7len", "Dich\u00b7ter\u00b7ge\u00b7m\u00fct", ",", "das", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "PDS", "$,", "ITJ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seit Jahren leidet an Sangesverstopfung;", "tokens": ["Seit", "Jah\u00b7ren", "lei\u00b7det", "an", "San\u00b7ges\u00b7ver\u00b7stop\u00b7fung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihn tr\u00f6stete Liebe, Glaube und Hoffnung.", "tokens": ["Ihn", "tr\u00f6s\u00b7te\u00b7te", "Lie\u00b7be", ",", "Glau\u00b7be", "und", "Hoff\u00b7nung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und dieses ist ein Kodizill:", "tokens": ["Und", "die\u00b7ses", "ist", "ein", "Ko\u00b7di\u00b7zill", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr den Fall, da\u00df keiner annehmen will", "tokens": ["F\u00fcr", "den", "Fall", ",", "da\u00df", "kei\u00b7ner", "an\u00b7neh\u00b7men", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die erw\u00e4hnten Legate, so sollen sie alle", "tokens": ["Die", "er\u00b7w\u00e4hn\u00b7ten", "Le\u00b7ga\u00b7te", ",", "so", "sol\u00b7len", "sie", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "PIAT"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Der r\u00f6misch-katholischen Kirche verfallen.", "tokens": ["Der", "r\u00f6\u00b7mischka\u00b7tho\u00b7li\u00b7schen", "Kir\u00b7che", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}