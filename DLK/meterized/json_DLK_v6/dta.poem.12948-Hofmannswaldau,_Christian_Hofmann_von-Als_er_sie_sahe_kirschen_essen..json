{"dta.poem.12948": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie sahe kirschen essen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es satzte Flavia sich in das gr\u00fcne gras,", "tokens": ["Es", "satz\u00b7te", "Fla\u00b7via", "sich", "in", "das", "gr\u00fc\u00b7ne", "gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ihre hand viel kirschen abgepfl\u00fccket:", "tokens": ["Als", "ih\u00b7re", "hand", "viel", "kir\u00b7schen", "ab\u00b7ge\u00b7pfl\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indem sie nun dieselben as,", "tokens": ["In\u00b7dem", "sie", "nun", "die\u00b7sel\u00b7ben", "as", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So stund Leander gantz entz\u00fccket.", "tokens": ["So", "stund", "Le\u00b7an\u00b7der", "gantz", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein auge konnte nicht die lippen unterscheiden,", "tokens": ["Sein", "au\u00b7ge", "konn\u00b7te", "nicht", "die", "lip\u00b7pen", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gleichwol war er ihr gantz nah,", "tokens": ["Und", "gleich\u00b7wol", "war", "er", "ihr", "gantz", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil er nichts als rothe kirschen in blutrothen kirschen sah.", "tokens": ["Weil", "er", "nichts", "als", "ro\u00b7the", "kir\u00b7schen", "in", "blut\u00b7ro\u00b7then", "kir\u00b7schen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}