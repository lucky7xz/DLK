{"textgrid.poem.35579": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Kaspar Hauser Lied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er wahrlich liebte die Sonne, die purpurn den H\u00fcgel hinabstieg,", "tokens": ["Er", "wahr\u00b7lich", "lieb\u00b7te", "die", "Son\u00b7ne", ",", "die", "pur\u00b7purn", "den", "H\u00fc\u00b7gel", "hin\u00b7ab\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Wege des Walds, den singenden Schwarzvogel", "tokens": ["Die", "We\u00b7ge", "des", "Walds", ",", "den", "sin\u00b7gen\u00b7den", "Schwarz\u00b7vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und die Freude des Gr\u00fcns.", "tokens": ["Und", "die", "Freu\u00b7de", "des", "Gr\u00fcns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ernsthaft war sein Wohnen im Schatten des Baums", "tokens": ["Ernst\u00b7haft", "war", "sein", "Woh\u00b7nen", "im", "Schat\u00b7ten", "des", "Baums"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und rein sein Antlitz.", "tokens": ["Und", "rein", "sein", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gott sprach eine sanfte Flamme zu seinem Herzen:", "tokens": ["Gott", "sprach", "ei\u00b7ne", "sanf\u00b7te", "Flam\u00b7me", "zu", "sei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "O Mensch!", "tokens": ["O", "Mensch", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Stille fand sein Schritt die Stadt am Abend;", "tokens": ["Stil\u00b7le", "fand", "sein", "Schritt", "die", "Stadt", "am", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die dunkle Klage seines Munds:", "tokens": ["Die", "dunk\u00b7le", "Kla\u00b7ge", "sei\u00b7nes", "Munds", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will ein Reiter werden.", "tokens": ["Ich", "will", "ein", "Rei\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihm aber folgte Busch und Tier,", "tokens": ["Ihm", "a\u00b7ber", "folg\u00b7te", "Busch", "und", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Haus und D\u00e4mmergarten wei\u00dfer Menschen", "tokens": ["Haus", "und", "D\u00e4m\u00b7mer\u00b7gar\u00b7ten", "wei\u00b7\u00dfer", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sein M\u00f6rder suchte nach ihm.", "tokens": ["Und", "sein", "M\u00f6r\u00b7der", "such\u00b7te", "nach", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchling und Sommer und sch\u00f6n der Herbst", "tokens": ["Fr\u00fch\u00b7ling", "und", "Som\u00b7mer", "und", "sch\u00f6n", "der", "Herbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJD", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Des Gerechten, sein leiser Schritt", "tokens": ["Des", "Ge\u00b7rech\u00b7ten", ",", "sein", "lei\u00b7ser", "Schritt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "An den dunklen Zimmern Tr\u00e4umender hin.", "tokens": ["An", "den", "dunk\u00b7len", "Zim\u00b7mern", "Tr\u00e4u\u00b7men\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nachts blieb er mit seinem Stern allein;", "tokens": ["Nachts", "blieb", "er", "mit", "sei\u00b7nem", "Stern", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sah, da\u00df Schnee fiel in kahles Gezweig", "tokens": ["Sah", ",", "da\u00df", "Schnee", "fiel", "in", "kah\u00b7les", "Ge\u00b7zweig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und im d\u00e4mmernden Hausflur den Schatten des M\u00f6rders.", "tokens": ["Und", "im", "d\u00e4m\u00b7mern\u00b7den", "Haus\u00b7flur", "den", "Schat\u00b7ten", "des", "M\u00f6r\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.7": {"line.1": {"text": "Silbern sank des Ungebornen Haupt hin.", "tokens": ["Sil\u00b7bern", "sank", "des", "Un\u00b7ge\u00b7bor\u00b7nen", "Haupt", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}