{"textgrid.poem.61190": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An Georg Herwegh", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es scholl dein Lied mir in das Ohr", "tokens": ["Es", "scholl", "dein", "Lied", "mir", "in", "das", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schwertesscharf, so glockent\u00f6nig,", "tokens": ["So", "schwer\u00b7tes\u00b7scharf", ",", "so", "glo\u00b7cken\u00b7t\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als w\u00e4r' aus seiner Gruft empor", "tokens": ["Als", "w\u00e4r'", "aus", "sei\u00b7ner", "Gruft", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewallt ein alter Dichterk\u00f6nig.", "tokens": ["Ge\u00b7wallt", "ein", "al\u00b7ter", "Dich\u00b7ter\u00b7k\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und doch! Ich weis' es nicht von mir,", "tokens": ["Und", "doch", "!", "Ich", "weis'", "es", "nicht", "von", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich mu\u00df dich in die Schranken laden;", "tokens": ["Ich", "mu\u00df", "dich", "in", "die", "Schran\u00b7ken", "la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Komm an in voller Harnischzier,", "tokens": ["Komm", "an", "in", "vol\u00b7ler", "Har\u00b7nischzier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf Tod und Leben Kampf mit dir,", "tokens": ["Auf", "Tod", "und", "Le\u00b7ben", "Kampf", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kampf, du Poet von Gottes Gnaden.", "tokens": ["Kampf", ",", "du", "Po\u00b7et", "von", "Got\u00b7tes", "Gna\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist du dir selber klar bewu\u00dft,", "tokens": ["Bist", "du", "dir", "sel\u00b7ber", "klar", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df deine Lieder Aufruhr l\u00e4uten;", "tokens": ["Da\u00df", "dei\u00b7ne", "Lie\u00b7der", "Auf\u00b7ruhr", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jeglicher nach seiner Brust", "tokens": ["Da\u00df", "jeg\u00b7li\u00b7cher", "nach", "sei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das \u00c4rgste mag aus ihnen deuten?", "tokens": ["Das", "\u00c4rgs\u00b7te", "mag", "aus", "ih\u00b7nen", "deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Zwerg, der matte Pfeile schnitzt,", "tokens": ["Der", "Zwerg", ",", "der", "mat\u00b7te", "Pfei\u00b7le", "schnitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl, - schie\u00df' er, ohne fest zu zielen;", "tokens": ["Wohl", ",", "schie\u00df'", "er", ",", "oh\u00b7ne", "fest", "zu", "zie\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "$(", "VVFIN", "PPER", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch wer vom Wetterlicht umblitzt", "tokens": ["Doch", "wer", "vom", "Wet\u00b7ter\u00b7licht", "um\u00b7blitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Donnerwagen grollend sitzt,", "tokens": ["Im", "Don\u00b7ner\u00b7wa\u00b7gen", "grol\u00b7lend", "sitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der soll nicht mit den Z\u00fcgeln spielen.", "tokens": ["Der", "soll", "nicht", "mit", "den", "Z\u00fc\u00b7geln", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcrwahr, ein S\u00e4mann schreitest du,", "tokens": ["F\u00fcr\u00b7wahr", ",", "ein", "S\u00e4\u00b7mann", "schrei\u00b7test", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Samen streut, doch der Zerst\u00f6rung;", "tokens": ["Der", "Sa\u00b7men", "streut", ",", "doch", "der", "Zer\u00b7st\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Gl\u00f6ckner, der aus ihrer Ruh'", "tokens": ["Ein", "Gl\u00f6ck\u00b7ner", ",", "der", "aus", "ih\u00b7rer", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die V\u00f6lker st\u00fcrmt, doch zur Emp\u00f6rung.", "tokens": ["Die", "V\u00f6l\u00b7ker", "st\u00fcrmt", ",", "doch", "zur", "Em\u00b7p\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du willst die Flamme, die so rein", "tokens": ["Du", "willst", "die", "Flam\u00b7me", ",", "die", "so", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und heilig strahlt durch alle Lande,", "tokens": ["Und", "hei\u00b7lig", "strahlt", "durch", "al\u00b7le", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du willst den warmen Gottesschein", "tokens": ["Du", "willst", "den", "war\u00b7men", "Got\u00b7tes\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Fackel Herostrats entweihn", "tokens": ["Zur", "Fa\u00b7ckel", "He\u00b7rost\u00b7rats", "ent\u00b7weihn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schwingst sie wild zum Tempelbrande.", "tokens": ["Und", "schwingst", "sie", "wild", "zum", "Tem\u00b7pel\u00b7bran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wozu sonst dieses Schwerterklirrn,", "tokens": ["Wo\u00b7zu", "sonst", "die\u00b7ses", "Schwer\u00b7ter\u00b7klirrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kriege, die dein Lied gefodert,", "tokens": ["Die", "Krie\u00b7ge", ",", "die", "dein", "Lied", "ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die hast'ge Glut, die durch dein Hirn", "tokens": ["Die", "hast'\u00b7ge", "Glut", ",", "die", "durch", "dein", "Hirn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend Funken pr\u00e4chtig lodert?", "tokens": ["In", "tau\u00b7send", "Fun\u00b7ken", "pr\u00e4ch\u00b7tig", "lo\u00b7dert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O nein! Das ist nicht deutsche Art!", "tokens": ["O", "nein", "!", "Das", "ist", "nicht", "deut\u00b7sche", "Art", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PDS", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl k\u00e4mpfen wir auch f\u00fcr das Neue;", "tokens": ["Wohl", "k\u00e4mp\u00b7fen", "wir", "auch", "f\u00fcr", "das", "Neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ums Freiheitsbanner dicht geschart,", "tokens": ["Ums", "Frei\u00b7heits\u00b7ban\u00b7ner", "dicht", "ge\u00b7schart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stehn auch wir; doch aufbewahrt", "tokens": ["So", "stehn", "auch", "wir", ";", "doch", "auf\u00b7be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Verha\u00dft auch uns ist der Baschkir,", "tokens": ["Ver\u00b7ha\u00dft", "auch", "uns", "ist", "der", "Baschkir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Unterjocher der Gedanken,", "tokens": ["Der", "Un\u00b7ter\u00b7jo\u00b7cher", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und keinen Deut begehren wir", "tokens": ["Und", "kei\u00b7nen", "Deut", "be\u00b7geh\u00b7ren", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von jenen \u00fcberm\u00fct'gen Franken.", "tokens": ["Von", "je\u00b7nen", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7gen", "Fran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen auch, da\u00df frei das Wort", "tokens": ["Wir", "wol\u00b7len", "auch", ",", "da\u00df", "frei", "das", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch alle L\u00fcfte m\u00f6ge fluten;", "tokens": ["Durch", "al\u00b7le", "L\u00fcf\u00b7te", "m\u00f6\u00b7ge", "flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Es d\u00fcnkt auch uns in S\u00fcd und Nord", "tokens": ["Es", "d\u00fcnkt", "auch", "uns", "in", "S\u00fcd", "und", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Wort der beste Freiheitshort -", "tokens": ["Das", "Wort", "der", "bes\u00b7te", "Frei\u00b7heits\u00b7hort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch soll darum dein Volk verbluten?", "tokens": ["Doch", "soll", "da\u00b7rum", "dein", "Volk", "ver\u00b7blu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nein! Glaub', der Tag ist bald erwacht,", "tokens": ["Nein", "!", "Glaub'", ",", "der", "Tag", "ist", "bald", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgen naht, wo wir's erringen,", "tokens": ["Der", "Mor\u00b7gen", "naht", ",", "wo", "wir's", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ohne Kampf, doch ohne Schlacht,", "tokens": ["Nicht", "oh\u00b7ne", "Kampf", ",", "doch", "oh\u00b7ne", "Schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist ist st\u00e4rker als die Klingen.", "tokens": ["Der", "Geist", "ist", "st\u00e4r\u00b7ker", "als", "die", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geharnischt steht er auf dem Plan,", "tokens": ["Ge\u00b7har\u00b7nischt", "steht", "er", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er, der mit Luthern einst gefochten;", "tokens": ["Er", ",", "der", "mit", "Lu\u00b7thern", "einst", "ge\u00b7foch\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch tausend Lanzen bricht er Bahn,", "tokens": ["Durch", "tau\u00b7send", "Lan\u00b7zen", "bricht", "er", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mag die H\u00f6lle dr\u00e4uend nahn:", "tokens": ["Und", "mag", "die", "H\u00f6l\u00b7le", "dr\u00e4u\u00b7end", "nahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Lorbeer bleibt ihm doch geflochten.", "tokens": ["Der", "Lor\u00b7beer", "bleibt", "ihm", "doch", "ge\u00b7floch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-