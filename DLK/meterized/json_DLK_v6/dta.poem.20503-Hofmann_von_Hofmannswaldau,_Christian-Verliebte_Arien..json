{"dta.poem.20503": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sclaven schlaffen in den banden/", "tokens": ["Scla\u00b7ven", "schlaf\u00b7fen", "in", "den", "ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ich liege sonder ruh;", "tokens": ["Nur", "ich", "lie\u00b7ge", "son\u00b7der", "ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner hoffnung schiff will stranden/", "tokens": ["Mei\u00b7ner", "hoff\u00b7nung", "schiff", "will", "stran\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und l\u00e4ufft wilden klippen zu/", "tokens": ["Und", "l\u00e4ufft", "wil\u00b7den", "klip\u00b7pen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "VVINF", "PTKZU", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn die flammen deiner liebe", "tokens": ["Denn", "die", "flam\u00b7men", "dei\u00b7ner", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "PPOSAT", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hegen in sich lauter triebe:", "tokens": ["He\u00b7gen", "in", "sich", "lau\u00b7ter", "trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil ihr weg will seyn", "tokens": ["Weil", "ihr", "weg", "will", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VAINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Nur stein/", "tokens": ["Nur", "stein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Da\u00df ich mit weh", "tokens": ["Da\u00df", "ich", "mit", "weh"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Dich nur lieb\u2019 Arcadie.", "tokens": ["Dich", "nur", "lieb'", "Ar\u00b7ca\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "K\u00f6nt ich nur dein auge k\u00fcssen/", "tokens": ["K\u00f6nt", "ich", "nur", "dein", "au\u00b7ge", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrd\u2019 ich meiner bande lo\u00df/", "tokens": ["W\u00fcrd'", "ich", "mei\u00b7ner", "ban\u00b7de", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine geister solten fliessen", "tokens": ["Mei\u00b7ne", "geis\u00b7ter", "sol\u00b7ten", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir ersterbend auff dem schoo\u00df;", "tokens": ["Dir", "ers\u00b7ter\u00b7bend", "auff", "dem", "schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn so wolt ich liljen pfl\u00fccken/", "tokens": ["Denn", "so", "wolt", "ich", "lil\u00b7jen", "pfl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auff deine br\u00fcste dr\u00fccken/", "tokens": ["Und", "auff", "dei\u00b7ne", "br\u00fcs\u00b7te", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sagen sonder leid/", "tokens": ["Sa\u00b7gen", "son\u00b7der", "leid", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "erfreut/", "tokens": ["er\u00b7freut", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Ihr weisser schnee", "tokens": ["Ihr", "weis\u00b7ser", "schnee"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Gleicht dir nicht Arcadie.", "tokens": ["Gleicht", "dir", "nicht", "Ar\u00b7ca\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df k\u00fcssen in gedancken/", "tokens": ["Ich", "mu\u00df", "k\u00fcs\u00b7sen", "in", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich mu\u00df lieben sonder that;", "tokens": ["Ich", "mu\u00df", "lie\u00b7ben", "son\u00b7der", "that", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner sinnen zirckel wancken/", "tokens": ["Mei\u00b7ner", "sin\u00b7nen", "zir\u00b7ckel", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich hoffe sonder rath/", "tokens": ["Weil", "ich", "hof\u00b7fe", "son\u00b7der", "rath", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja mein leben wird verschwinden/", "tokens": ["Ja", "mein", "le\u00b7ben", "wird", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "VVINF", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh\u2019 es die noch wird empfinden/", "tokens": ["Eh'", "es", "die", "noch", "wird", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die den todt mir bringt/", "tokens": ["Die", "den", "todt", "mir", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "PPER", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und schwingt/", "tokens": ["Und", "schwingt", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Da\u00df ich vergeh", "tokens": ["Da\u00df", "ich", "ver\u00b7geh"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Verwund/ Arcadie.", "tokens": ["Ver\u00b7wund", "/", "Ar\u00b7ca\u00b7die", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NE", "$."], "meter": "-++--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Denn die flammen ihrer augen/", "tokens": ["Denn", "die", "flam\u00b7men", "ih\u00b7rer", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein hertz in brand gebracht/", "tokens": ["Die", "mein", "hertz", "in", "brand", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen mir den geist aussaugen/", "tokens": ["K\u00f6n\u00b7nen", "mir", "den", "geist", "aus\u00b7sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich sonder lebens macht", "tokens": ["Da\u00df", "ich", "son\u00b7der", "le\u00b7bens", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df verlodern in der flammen/", "tokens": ["Mu\u00df", "ver\u00b7lo\u00b7dern", "in", "der", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sich legt um mich zusammen/", "tokens": ["Die", "sich", "legt", "um", "mich", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Indem deine gunst/", "tokens": ["In\u00b7dem", "dei\u00b7ne", "gunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Die brunst", "tokens": ["Die", "brunst"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Durch ihre see", "tokens": ["Durch", "ih\u00b7re", "see"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Nicht abk\u00fchlt/ Arcadie.", "tokens": ["Nicht", "ab\u00b7k\u00fchlt", "/", "Ar\u00b7ca\u00b7die", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Deine purpur-rothe wangen/", "tokens": ["Dei\u00b7ne", "pur\u00b7pur\u00b7ro\u00b7the", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich doch nicht anger\u00fchrt/", "tokens": ["Die", "ich", "doch", "nicht", "an\u00b7ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben meinen geist gefangen/", "tokens": ["Ha\u00b7ben", "mei\u00b7nen", "geist", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihn sclavisch zugef\u00fchrt;", "tokens": ["Die", "ihn", "scla\u00b7visch", "zu\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die liljen deiner br\u00fcste/", "tokens": ["Und", "die", "lil\u00b7jen", "dei\u00b7ner", "br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner lippen s\u00fcsse l\u00fcste/", "tokens": ["Dei\u00b7ner", "lip\u00b7pen", "s\u00fcs\u00b7se", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Machen durch den zwang", "tokens": ["Ma\u00b7chen", "durch", "den", "zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Mich kranck/", "tokens": ["Mich", "kranck", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Da\u00df ich vergeh/", "tokens": ["Da\u00df", "ich", "ver\u00b7geh", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und dir sterb\u2019/ Arcadie.", "tokens": ["Und", "dir", "sterb'", "/", "Ar\u00b7ca\u00b7die", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}