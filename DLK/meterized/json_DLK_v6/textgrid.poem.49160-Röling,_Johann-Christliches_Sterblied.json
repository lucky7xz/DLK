{"textgrid.poem.49160": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schutz und Hort betr\u00fcbter Herzen,", "tokens": ["Schutz", "und", "Hort", "be\u00b7tr\u00fcb\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Tiefe meiner Schmerzen", "tokens": ["Aus", "der", "Tie\u00b7fe", "mei\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruf' ich, o mein Gott, zu dir!", "tokens": ["Ruf'", "ich", ",", "o", "mein", "Gott", ",", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mir nicht dein Ohr absagen,", "tokens": ["La\u00df", "mir", "nicht", "dein", "Ohr", "ab\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wem soll ich mein Elend klagen,", "tokens": ["Wem", "soll", "ich", "mein", "E\u00b7lend", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmest du es, Herr, nicht f\u00fcr,", "tokens": ["Nim\u00b7mest", "du", "es", ",", "Herr", ",", "nicht", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wer ist, der mich aufricht,", "tokens": ["Und", "wer", "ist", ",", "der", "mich", "auf\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6rest du mein Flehen nicht?", "tokens": ["H\u00f6\u00b7rest", "du", "mein", "Fle\u00b7hen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht mir meine S\u00fcnd' im Wege", "tokens": ["Steht", "mir", "mei\u00b7ne", "S\u00fcnd'", "im", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verleget alle Stege", "tokens": ["Und", "ver\u00b7le\u00b7get", "al\u00b7le", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner allgemeinen Huld,", "tokens": ["Dei\u00b7ner", "all\u00b7ge\u00b7mei\u00b7nen", "Huld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach so lass' doch Gnade walten,", "tokens": ["Ach", "so", "lass'", "doch", "Gna\u00b7de", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer kann dir die Wage halten,", "tokens": ["Wer", "kann", "dir", "die", "Wa\u00b7ge", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Fragst du uns um unsre Schuld?", "tokens": ["Fragst", "du", "uns", "um", "uns\u00b7re", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch die Heil'gen werden scheu,", "tokens": ["Auch", "die", "Heil'\u00b7gen", "wer\u00b7den", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Legst du mit uns Rechnung bei.", "tokens": ["Legst", "du", "mit", "uns", "Rech\u00b7nung", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, o Bild der' Lieb' und G\u00fcte,", "tokens": ["Nein", ",", "o", "Bild", "der'", "Lieb'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, dies ist nicht dein Gem\u00fcthe;", "tokens": ["Nein", ",", "dies", "ist", "nicht", "dein", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feind bist du der S\u00fcnde sehr,", "tokens": ["Feind", "bist", "du", "der", "S\u00fcn\u00b7de", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Strafest sie in allen Wegen,", "tokens": ["Stra\u00b7fest", "sie", "in", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir sie vermeiden m\u00f6gen", "tokens": ["Da\u00df", "wir", "sie", "ver\u00b7mei\u00b7den", "m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dich f\u00fcrchten immer mehr,", "tokens": ["Und", "dich", "f\u00fcrch\u00b7ten", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und doch schenkst du sie auch leicht,", "tokens": ["Und", "doch", "schenkst", "du", "sie", "auch", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn man nur zu Kreuze kreucht.", "tokens": ["Wenn", "man", "nur", "zu", "Kreu\u00b7ze", "kreucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, zum Kreuze deines Sohnes,", "tokens": ["Ja", ",", "zum", "Kreu\u00b7ze", "dei\u00b7nes", "Soh\u00b7nes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsres milden Gnadenthrones,", "tokens": ["Uns\u00b7res", "mil\u00b7den", "Gna\u00b7dent\u00b7hro\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, zum Kreuz, da ihn erh\u00f6ht,", "tokens": ["Ja", ",", "zum", "Kreuz", ",", "da", "ihn", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dran mein' Handschrift ist geschlagen,", "tokens": ["Dran", "mein'", "Hand\u00b7schrift", "ist", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausgelescht und abgetragen", "tokens": ["Aus\u00b7ge\u00b7lescht", "und", "ab\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Tod selbst mit ert\u00f6dt;", "tokens": ["Und", "mein", "Tod", "selbst", "mit", "er\u00b7t\u00f6dt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch dies wei\u00df ich, da\u00df ich dein", "tokens": ["Durch", "dies", "wei\u00df", "ich", ",", "da\u00df", "ich", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sonst Keines k\u00f6nne sein.", "tokens": ["Und", "sonst", "Kei\u00b7nes", "k\u00f6n\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lass' mir diesen meinen Glauben,", "tokens": ["Lass'", "mir", "die\u00b7sen", "mei\u00b7nen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebster Sch\u00f6pfer Nichtes rauben;", "tokens": ["Liebs\u00b7ter", "Sch\u00f6p\u00b7fer", "Nich\u00b7tes", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich harr' und warte dein", "tokens": ["Ach", ",", "ich", "harr'", "und", "war\u00b7te", "dein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der fr\u00fchen Morgenwache,", "tokens": ["Von", "der", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ich mich zur Ruhe mache", "tokens": ["Bis", "ich", "mich", "zur", "Ru\u00b7he", "ma\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und bis wieder an den Schein;", "tokens": ["Und", "bis", "wie\u00b7der", "an", "den", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wo doch, ruf' ich immer zu,", "tokens": ["Wo", "doch", ",", "ruf'", "ich", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo, Herr, ruf' ich, bleibest du?", "tokens": ["Wo", ",", "Herr", ",", "ruf'", "ich", ",", "blei\u00b7best", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Nun, ich wei\u00df, du wirst erscheinen;", "tokens": ["Nun", ",", "ich", "wei\u00df", ",", "du", "wirst", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du z\u00fcrnest mit den Deinen,", "tokens": ["Was", "du", "z\u00fcr\u00b7nest", "mit", "den", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist doch nichts als lauter Lieb';", "tokens": ["Ist", "doch", "nichts", "als", "lau\u00b7ter", "Lieb'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "KOKOM", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, dein Herz ist viel gelinder,", "tokens": ["Ach", ",", "dein", "Herz", "ist", "viel", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6chster Freund zerknirschter S\u00fcnder,", "tokens": ["H\u00f6chs\u00b7ter", "Freund", "zer\u00b7knirschter", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Als da\u00df Solches uns betr\u00fcb'", "tokens": ["Als", "da\u00df", "Sol\u00b7ches", "uns", "be\u00b7tr\u00fcb'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Anders, als da\u00df deine Treu'", "tokens": ["An\u00b7ders", ",", "als", "da\u00df", "dei\u00b7ne", "Treu'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOKOM", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns desto bekannter sei.", "tokens": ["Uns", "des\u00b7to", "be\u00b7kann\u00b7ter", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Still denn, meine Seele, stille!", "tokens": ["Still", "denn", ",", "mei\u00b7ne", "See\u00b7le", ",", "stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thu', was deines Sch\u00f6pfers Wille,", "tokens": ["Thu'", ",", "was", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erst der Regen, dann das Licht,", "tokens": ["Erst", "der", "Re\u00b7gen", ",", "dann", "das", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als w\u00e4chst des Feldes Segen,", "tokens": ["Als", "w\u00e4chst", "des", "Fel\u00b7des", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df sich dein Herz verpflegen;", "tokens": ["So", "mu\u00df", "sich", "dein", "Herz", "ver\u00b7pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Erstlich weine minder nicht,", "tokens": ["Erst\u00b7lich", "wei\u00b7ne", "min\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann folgt Gottes Gnadenschein,", "tokens": ["Dann", "folgt", "Got\u00b7tes", "Gna\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und du wirst recht gl\u00fccklich sein.", "tokens": ["Und", "du", "wirst", "recht", "gl\u00fcck\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}