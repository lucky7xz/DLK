{"textgrid.poem.61764": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Trinkspruch", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sto\u00dft an im Saft der besten Reben!", "tokens": ["Sto\u00dft", "an", "im", "Saft", "der", "bes\u00b7ten", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sto\u00dft an: Land Mecklenburg soll leben,", "tokens": ["Sto\u00dft", "an", ":", "Land", "Meck\u00b7len\u00b7burg", "soll", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Land Mecklenburg mit Schwert und Pflug!", "tokens": ["Land", "Meck\u00b7len\u00b7burg", "mit", "Schwert", "und", "Pflug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Perle gab es uns der Frauen", "tokens": ["Die", "Per\u00b7le", "gab", "es", "uns", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jenes Paar mit greisen Brauen,", "tokens": ["Und", "je\u00b7nes", "Paar", "mit", "grei\u00b7sen", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das unsres Ruhmes Schlachten schlug.", "tokens": ["Das", "uns\u00b7res", "Ruh\u00b7mes", "Schlach\u00b7ten", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon wallt sie l\u00e4ngst im Paradiese,", "tokens": ["Schon", "wallt", "sie", "l\u00e4ngst", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohe K\u00f6nigin Luise,", "tokens": ["Die", "ho\u00b7he", "K\u00f6\u00b7ni\u00b7gin", "Lu\u00b7i\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Deutschlands starken Hort gebar,", "tokens": ["Die", "Deutschlands", "star\u00b7ken", "Hort", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Doch flammend steht's in tausend Herzen,", "tokens": ["Doch", "flam\u00b7mend", "steht's", "in", "tau\u00b7send", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie zur Zeit der Schmach und Schmerzen", "tokens": ["Wie", "sie", "zur", "Zeit", "der", "Schmach", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Engel ihres Volkes war.", "tokens": ["Der", "En\u00b7gel", "ih\u00b7res", "Vol\u00b7kes", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wollt ihr nach den Helden fragen:", "tokens": ["Und", "wollt", "ihr", "nach", "den", "Hel\u00b7den", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Marschall Vorw\u00e4rts la\u00dft euch sagen,", "tokens": ["Vom", "Mar\u00b7schall", "Vor\u00b7w\u00e4rts", "la\u00dft", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem blanksten Schwert des Vaterlands;", "tokens": ["Dem", "blanks\u00b7ten", "Schwert", "des", "Va\u00b7ter\u00b7lands", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt durchhallten seine Siege,", "tokens": ["Die", "Welt", "durch\u00b7hall\u00b7ten", "sei\u00b7ne", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nie zu Rostock seiner Wiege", "tokens": ["Doch", "nie", "zu", "Ros\u00b7tock", "sei\u00b7ner", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verga\u00df der Greis im Lorbeerkranz.", "tokens": ["Ver\u00b7ga\u00df", "der", "Greis", "im", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den andern kennt ihr auch, den Alten,", "tokens": ["Den", "an\u00b7dern", "kennt", "ihr", "auch", ",", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hoch und ernst, die Stirn in Falten,", "tokens": ["Der", "hoch", "und", "ernst", ",", "die", "Stirn", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein H\u00fcter wacht an Preu\u00dfens Thron.", "tokens": ["Ein", "H\u00fc\u00b7ter", "wacht", "an", "Preu\u00b7\u00dfens", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist des Kriegsgotts Wagenlenker,", "tokens": ["Das", "ist", "des", "Kriegs\u00b7gotts", "Wa\u00b7gen\u00b7len\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist der k\u00fchne Schlachtendenker,", "tokens": ["Das", "ist", "der", "k\u00fch\u00b7ne", "Schlach\u00b7ten\u00b7den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schweiger Moltke, Parchims Sohn.", "tokens": ["Der", "Schwei\u00b7ger", "Molt\u00b7ke", ",", "Par\u00b7chims", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum sto\u00dft im Saft der besten Reben,", "tokens": ["Drum", "sto\u00dft", "im", "Saft", "der", "bes\u00b7ten", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sto\u00dft an: Land Mecklenburg soll leben,", "tokens": ["Sto\u00dft", "an", ":", "Land", "Meck\u00b7len\u00b7burg", "soll", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Land Mecklenburg mit Schwert und Pflug!", "tokens": ["Land", "Meck\u00b7len\u00b7burg", "mit", "Schwert", "und", "Pflug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Perle gab es uns der Frauen", "tokens": ["Die", "Per\u00b7le", "gab", "es", "uns", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jenes Paar mit greisen Brauen,", "tokens": ["Und", "je\u00b7nes", "Paar", "mit", "grei\u00b7sen", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das unsres Ruhmes Schlachten schlug.", "tokens": ["Das", "uns\u00b7res", "Ruh\u00b7mes", "Schlach\u00b7ten", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}