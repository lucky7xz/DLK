{"textgrid.poem.35588": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Herbstseele", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00e4gerruf und Blutgebell;", "tokens": ["J\u00e4\u00b7ger\u00b7ruf", "und", "Blut\u00b7ge\u00b7bell", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter Kreuz und braunem H\u00fcgel", "tokens": ["Hin\u00b7ter", "Kreuz", "und", "brau\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blindet sacht der Weiherspiegel,", "tokens": ["Blin\u00b7det", "sacht", "der", "Wei\u00b7her\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreit der Habicht hart und hell.", "tokens": ["Schreit", "der", "Ha\u00b7bicht", "hart", "und", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcber Stoppelfeld und Pfad", "tokens": ["\u00dc\u00b7ber", "Stop\u00b7pel\u00b7feld", "und", "Pfad"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Banget schon ein schwarzes Schweigen;", "tokens": ["Ban\u00b7get", "schon", "ein", "schwar\u00b7zes", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiner Himmel in den Zweigen;", "tokens": ["Rei\u00b7ner", "Him\u00b7mel", "in", "den", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Bach rinnt still und stad.", "tokens": ["Nur", "der", "Bach", "rinnt", "still", "und", "stad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald entgleitet Fisch und Wild.", "tokens": ["Bald", "ent\u00b7glei\u00b7tet", "Fisch", "und", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blaue Seele, dunkles Wandern", "tokens": ["Blau\u00b7e", "See\u00b7le", ",", "dunk\u00b7les", "Wan\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schied uns bald von Lieben, Andern.", "tokens": ["Schied", "uns", "bald", "von", "Lie\u00b7ben", ",", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abend wechselt Sinn und Bild.", "tokens": ["A\u00b7bend", "wech\u00b7selt", "Sinn", "und", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rechten Lebens Brot und Wein,", "tokens": ["Rech\u00b7ten", "Le\u00b7bens", "Brot", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott in deine milden H\u00e4nde", "tokens": ["Gott", "in", "dei\u00b7ne", "mil\u00b7den", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt der Mensch das dunkle Ende,", "tokens": ["Legt", "der", "Mensch", "das", "dunk\u00b7le", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Schuld und rote Pein.", "tokens": ["Al\u00b7le", "Schuld", "und", "ro\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}