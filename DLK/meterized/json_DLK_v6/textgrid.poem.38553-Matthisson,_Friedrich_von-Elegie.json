{"textgrid.poem.38553": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist, des Erdewallens m\u00fcde,", "tokens": ["Mein", "Geist", ",", "des", "E\u00b7rde\u00b7wal\u00b7lens", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehnt sich, o Gruft! nach deiner Ruh,", "tokens": ["Sehnt", "sich", ",", "o", "Gruft", "!", "nach", "dei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn meines Herzens goldner Friede", "tokens": ["Denn", "mei\u00b7nes", "Her\u00b7zens", "gold\u00b7ner", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flog seinem Eden wieder zu.", "tokens": ["Flog", "sei\u00b7nem", "E\u00b7den", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Regenbogenschimmer schwanden", "tokens": ["Wie", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7schim\u00b7mer", "schwan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend holde Phantasien;", "tokens": ["Der", "Ju\u00b7gend", "hol\u00b7de", "Phan\u00b7ta\u00b7si\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den Kranz, so Lieb' und Freundschaft wanden,", "tokens": ["Den", "Kranz", ",", "so", "Lieb'", "und", "Freund\u00b7schaft", "wan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hies, Trennung, deine Hand verbl\u00fchn!", "tokens": ["Hies", ",", "Tren\u00b7nung", ",", "dei\u00b7ne", "Hand", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! selige Erinnerungen!", "tokens": ["O", "!", "se\u00b7li\u00b7ge", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Da ich, am lenzumbl\u00fcmten Bach,", "tokens": ["Da", "ich", ",", "am", "len\u00b7zum\u00b7bl\u00fcm\u00b7ten", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von Nachtigallen eingesungen,", "tokens": ["Von", "Nach\u00b7ti\u00b7gal\u00b7len", "ein\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als sorgenfreier Knabe lag;", "tokens": ["Als", "sor\u00b7gen\u00b7frei\u00b7er", "Kna\u00b7be", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da unbedornten Blumenwegen", "tokens": ["Da", "un\u00b7be\u00b7dorn\u00b7ten", "Blu\u00b7men\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fckken Strom auf Strom entquoll,", "tokens": ["Ent\u00b7z\u00fck\u00b7ken", "Strom", "auf", "Strom", "ent\u00b7quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir Sph\u00e4renmelodie entgegen", "tokens": ["Mir", "Sph\u00e4\u00b7ren\u00b7me\u00b7lo\u00b7die", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In jedem Fr\u00fchlingsliede scholl;", "tokens": ["In", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7lie\u00b7de", "scholl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da mir noch keine Thr\u00e4nen flossen,", "tokens": ["Da", "mir", "noch", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als die die Freude weinen hies,", "tokens": ["Als", "die", "die", "Freu\u00b7de", "wei\u00b7nen", "hies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ich, vom Mutterarm umschlossen,", "tokens": ["Da", "ich", ",", "vom", "Mut\u00b7ter\u00b7arm", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich \u00fcberschwenglich selig pries;", "tokens": ["Mich", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "se\u00b7lig", "pries", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da Ruhe Stund in Stunde webte,", "tokens": ["Da", "Ru\u00b7he", "Stund", "in", "Stun\u00b7de", "web\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wunders\u00fc\u00dfe Lieder sang,", "tokens": ["Mir", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um jeden meiner Tritte schwebte,", "tokens": ["Um", "je\u00b7den", "mei\u00b7ner", "Trit\u00b7te", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich den Kelch des Schlummers trank.", "tokens": ["Bis", "ich", "den", "Kelch", "des", "Schlum\u00b7mers", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O! steh mir immerdar zur Seite,", "tokens": ["O", "!", "steh", "mir", "im\u00b7mer\u00b7dar", "zur", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebtes Bild der Knabenzeit!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Bild", "der", "Kna\u00b7ben\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zur Vollendung, dann geleite", "tokens": ["Bis", "zur", "Vol\u00b7len\u00b7dung", ",", "dann", "ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPRART", "NN", "$,", "ADV", "ADJA"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mich im Triumph zur Ewigkeit.", "tokens": ["Mich", "im", "Tri\u00b7umph", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Ach! meines Herzens goldner Friede", "tokens": ["Ach", "!", "mei\u00b7nes", "Her\u00b7zens", "gold\u00b7ner", "Frie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flog seinem Eden wieder zu,", "tokens": ["Flog", "sei\u00b7nem", "E\u00b7den", "wie\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Geist, des Erdenwallens m\u00fcde,", "tokens": ["Mein", "Geist", ",", "des", "Er\u00b7den\u00b7wal\u00b7lens", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehnt sich, o Gruft! nach deiner Ruh!", "tokens": ["Sehnt", "sich", ",", "o", "Gruft", "!", "nach", "dei\u00b7ner", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hinauf! Hinauf! zu jenem Lande", "tokens": ["Hin\u00b7auf", "!", "Hin\u00b7auf", "!", "zu", "je\u00b7nem", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von wo du stammest, o mein Geist!", "tokens": ["Von", "wo", "du", "stam\u00b7mest", ",", "o", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PPER", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo du, im schimmernden Gewande,", "tokens": ["Wo", "du", ",", "im", "schim\u00b7mern\u00b7den", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich ewig deines Gottes freust!", "tokens": ["Dich", "e\u00b7wig", "dei\u00b7nes", "Got\u00b7tes", "freust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dort trinkst, in vollen Taumelz\u00fcgen,", "tokens": ["Dort", "trinkst", ",", "in", "vol\u00b7len", "Tau\u00b7mel\u00b7z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du s\u00fc\u00dfe, niebereute Lust!", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", ",", "nie\u00b7be\u00b7reu\u00b7te", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort wird der Z\u00e4hren Quell' versiegen,", "tokens": ["Dort", "wird", "der", "Z\u00e4h\u00b7ren", "Quell'", "ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort schwellt kein Seufzer mehr die Brust!", "tokens": ["Dort", "schwellt", "kein", "Seuf\u00b7zer", "mehr", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dort str\u00f6mt dir Paradieseswonne", "tokens": ["Dort", "str\u00f6mt", "dir", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus tausend Lebensb\u00e4chen zu,", "tokens": ["Aus", "tau\u00b7send", "Le\u00b7bens\u00b7b\u00e4\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort l\u00e4chelt eine mildre Sonne", "tokens": ["Dort", "l\u00e4\u00b7chelt", "ei\u00b7ne", "mild\u00b7re", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir unaussprechlich sanfte Ruh!", "tokens": ["Dir", "un\u00b7aus\u00b7sprech\u00b7lich", "sanf\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Bald reich, o Tod! dem Lebensm\u00fcden,", "tokens": ["Bald", "reich", ",", "o", "Tod", "!", "dem", "Le\u00b7bens\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ersch\u00f6pften Pilger deine Hand!", "tokens": ["Er\u00b7sch\u00f6pf\u00b7ten", "Pil\u00b7ger", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn alles Menschengl\u00fck hienieden", "tokens": ["Denn", "al\u00b7les", "Men\u00b7schen\u00b7gl\u00fck", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Nebeldunst und leerer Tand!", "tokens": ["Ist", "Ne\u00b7bel\u00b7dunst", "und", "lee\u00b7rer", "Tand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hinab denn, o mein Leib! zum Staube!", "tokens": ["Hin\u00b7ab", "denn", ",", "o", "mein", "Leib", "!", "zum", "Stau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "FM", "PPOSAT", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald wird dein lezter Morgen graun!", "tokens": ["Bald", "wird", "dein", "lez\u00b7ter", "Mor\u00b7gen", "graun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann werd ich dich, an den ich glaube,", "tokens": ["Dann", "werd", "ich", "dich", ",", "an", "den", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Durch alle Ewigkeiten schaun!", "tokens": ["Durch", "al\u00b7le", "E\u00b7wig\u00b7kei\u00b7ten", "schaun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}