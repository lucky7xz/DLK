{"dta.poem.7438": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jacta alea est.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wiewohl mein fromme Mutter weint,", "tokens": ["Wie\u00b7wohl", "mein", "from\u00b7me", "Mut\u00b7ter", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich die Sach' h\u00e4tt gfangen an:", "tokens": ["Da", "ich", "die", "Sach'", "h\u00e4tt", "gfan\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott woll sie tr\u00f6sten, es mu\u00df gahn,", "tokens": ["Gott", "woll", "sie", "tr\u00f6s\u00b7ten", ",", "es", "mu\u00df", "gahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sollt es brechen auch vor'm End,", "tokens": ["Und", "sollt", "es", "bre\u00b7chen", "auch", "vor'm", "End", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wills Gott, so mag's nit werden g'wend't,", "tokens": ["Wills", "Gott", ",", "so", "mag's", "nit", "wer\u00b7den", "g'\u00b7wend't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "ADV", "NE", "PTKNEG", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Darum will brauchen F\u00fc\u00df und H\u00e4nd.", "tokens": ["Da\u00b7rum", "will", "brau\u00b7chen", "F\u00fc\u00df", "und", "H\u00e4nd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich hab's gewagt.", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}