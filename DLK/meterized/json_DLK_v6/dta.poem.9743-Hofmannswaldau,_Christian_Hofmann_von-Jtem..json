{"dta.poem.9743": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du/ der du gehst vorbey/ verg\u00f6nne mir die ruh!", "tokens": ["Du", "/", "der", "du", "gehst", "vor\u00b7bey", "/", "ver\u00b7g\u00f6n\u00b7ne", "mir", "die", "ruh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "VVFIN", "PTKVZ", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich war ein guter mann/ der krum gieng und geb\u00fccket/", "tokens": ["Ich", "war", "ein", "gu\u00b7ter", "mann", "/", "der", "krum", "gieng", "und", "ge\u00b7b\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von solcher last beschwert/ die dr\u00fcckt und nicht erdr\u00fccket/", "tokens": ["Von", "sol\u00b7cher", "last", "be\u00b7schwert", "/", "die", "dr\u00fcckt", "und", "nicht", "er\u00b7dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$(", "ART", "VVFIN", "KON", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ich nahm unten ab/ so nahm ich oben zu.", "tokens": ["Als", "ich", "nahm", "un\u00b7ten", "ab", "/", "so", "nahm", "ich", "o\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}