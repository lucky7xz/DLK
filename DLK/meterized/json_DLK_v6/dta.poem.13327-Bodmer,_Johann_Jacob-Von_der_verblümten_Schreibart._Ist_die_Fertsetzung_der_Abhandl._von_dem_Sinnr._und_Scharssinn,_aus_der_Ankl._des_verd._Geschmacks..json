{"dta.poem.13327": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So entsetzlich sind die H\u00f6hen,", "tokens": ["So", "ent\u00b7setz\u00b7lich", "sind", "die", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die bald steil, bald rauch, bald glatt,", "tokens": ["Die", "bald", "steil", ",", "bald", "rauch", ",", "bald", "glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das Aug von vielem sehen,", "tokens": ["Da\u00df", "das", "Aug", "von", "vie\u00b7lem", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so ferner Reise, matt,", "tokens": ["Und", "so", "fer\u00b7ner", "Rei\u00b7se", ",", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaum zun Gipseln kan gelangen,", "tokens": ["Kaum", "zun", "Gip\u00b7seln", "kan", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die, wenn sie voll Wolken hangen,", "tokens": ["Die", ",", "wenn", "sie", "voll", "Wol\u00b7ken", "han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach dem bl\u00f6den Augenschein,", "tokens": ["Nach", "dem", "bl\u00f6\u00b7den", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst des Himmels St\u00fctzen seyn.", "tokens": ["Selbst", "des", "Him\u00b7mels", "St\u00fct\u00b7zen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}