{"textgrid.poem.32434": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "113. Eine Gesundheit auf die Gesundheiten", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg, weg mit W\u00fcnschen, Reimen, Schw\u00e4nken!", "tokens": ["Weg", ",", "weg", "mit", "W\u00fcn\u00b7schen", ",", "Rei\u00b7men", ",", "Schw\u00e4n\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trinkt flei\u00dfig, aber trinket still!", "tokens": ["Trinkt", "flei\u00b7\u00dfig", ",", "a\u00b7ber", "trin\u00b7ket", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wird an die Gesundheit denken,", "tokens": ["Wer", "wird", "an", "die", "Ge\u00b7sund\u00b7heit", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man die Gl\u00e4ser leeren will?", "tokens": ["Wenn", "man", "die", "Gl\u00e4\u00b7ser", "lee\u00b7ren", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}