{"dta.poem.18530": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den mancherley zeit-vertreib.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje wunderlich wird nicht die lange zeit vertrieben!", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "wird", "nicht", "die", "lan\u00b7ge", "zeit", "ver\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eine bringt sie zu mit schl\u00e4gen/ der mit lieben/", "tokens": ["Der", "ei\u00b7ne", "bringt", "sie", "zu", "mit", "schl\u00e4\u00b7gen", "/", "der", "mit", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "APPR", "VVINF", "$(", "ART", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort jenem macht music/ dem karten-spiel vergn\u00fcgen/", "tokens": ["Dort", "je\u00b7nem", "macht", "mu\u00b7sic", "/", "dem", "kar\u00b7ten\u00b7spiel", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "VVFIN", "NE", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sieht zum fenster nau\u00df/ ein ander f\u00e4nget fliegen.", "tokens": ["Der", "sieht", "zum", "fens\u00b7ter", "nau\u00df", "/", "ein", "an\u00b7der", "f\u00e4n\u00b7get", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PTKVZ", "$(", "ART", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der l\u00e4ufft die stub entzwey/ der schnitzelt an der wand/", "tokens": ["Der", "l\u00e4ufft", "die", "stub", "ent\u00b7zwey", "/", "der", "schnit\u00b7zelt", "an", "der", "wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "VVFIN", "PTKVZ", "$(", "ART", "VVFIN", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jener beisset sich die n\u00e4gel von der hand.", "tokens": ["Und", "je\u00b7ner", "beis\u00b7set", "sich", "die", "n\u00e4\u00b7gel", "von", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch andre suchen noch was anders auszu\u00fcben.", "tokens": ["Noch", "and\u00b7re", "su\u00b7chen", "noch", "was", "an\u00b7ders", "aus\u00b7zu\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "PWS", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wunderlich wird nicht die lange zeit vertrieben!", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "wird", "nicht", "die", "lan\u00b7ge", "zeit", "ver\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}