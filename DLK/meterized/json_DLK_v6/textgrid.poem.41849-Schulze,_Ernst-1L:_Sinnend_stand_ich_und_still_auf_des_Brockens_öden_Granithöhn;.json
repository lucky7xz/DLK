{"textgrid.poem.41849": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sinnend stand ich und still auf des Brockens \u00f6den Granith\u00f6hn;", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinnend stand ich und still auf des Brockens \u00f6den Granith\u00f6hn;", "tokens": ["Sin\u00b7nend", "stand", "ich", "und", "still", "auf", "des", "Bro\u00b7ckens", "\u00f6\u00b7den", "Gra\u00b7nit\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "K\u00fchn auf Felsen gest\u00fctzt, w\u00e4hnte sich sichrer die Kraft.", "tokens": ["K\u00fchn", "auf", "Fel\u00b7sen", "ge\u00b7st\u00fctzt", ",", "w\u00e4hn\u00b7te", "sich", "sich\u00b7rer", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nebel umzog das Gebirg, und es flo\u00df grauwogende D\u00e4mmrung", "tokens": ["Ne\u00b7bel", "um\u00b7zog", "das", "Ge\u00b7birg", ",", "und", "es", "flo\u00df", "grau\u00b7wo\u00b7gen\u00b7de", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Rings durch die Tief', und es sank dunkel die zitternde Gluth.", "tokens": ["Rings", "durch", "die", "Tie\u00b7f'", ",", "und", "es", "sank", "dun\u00b7kel", "die", "zit\u00b7tern\u00b7de", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Fruchtlos schauten die Wanderer hin in's Thal, und es klagte", "tokens": ["Frucht\u00b7los", "schau\u00b7ten", "die", "Wan\u00b7de\u00b7rer", "hin", "in's", "Thal", ",", "und", "es", "klag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Jeglicher, da\u00df kein Dank lohne den schwierigen Pfad.", "tokens": ["Jeg\u00b7li\u00b7cher", ",", "da\u00df", "kein", "Dank", "loh\u00b7ne", "den", "schwie\u00b7ri\u00b7gen", "Pfad", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Aber entz\u00fcckt hob hoch sich der Geist des sinnigen Dichters,", "tokens": ["A\u00b7ber", "ent\u00b7z\u00fcckt", "hob", "hoch", "sich", "der", "Geist", "des", "sin\u00b7ni\u00b7gen", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ADJD", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Und aus leichtem Gew\u00f6lk schuf er ein Zaubergefild.", "tokens": ["Und", "aus", "leich\u00b7tem", "Ge\u00b7w\u00f6lk", "schuf", "er", "ein", "Zau\u00b7ber\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Form und Gestalt rang schnell aus dem Nichts sich empor, und bedeutsam", "tokens": ["Form", "und", "Ge\u00b7stalt", "rang", "schnell", "aus", "dem", "Nichts", "sich", "em\u00b7por", ",", "und", "be\u00b7deut\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "PIS", "PRF", "PTKVZ", "$,", "KON", "ADJD"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Tagte das Bild, ringsum wallte lebendiger Reiz.", "tokens": ["Tag\u00b7te", "das", "Bild", ",", "ring\u00b7sum", "wall\u00b7te", "le\u00b7ben\u00b7di\u00b7ger", "Reiz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Bergh\u00f6hn th\u00fcrmten sich k\u00fchn, und auf zackigen Klippen erhob sich", "tokens": ["Berg\u00b7h\u00f6hn", "th\u00fcrm\u00b7ten", "sich", "k\u00fchn", ",", "und", "auf", "za\u00b7cki\u00b7gen", "Klip\u00b7pen", "er\u00b7hob", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,", "KON", "APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Dunkel der Wald, und es schwamm zitternd der silberne See.", "tokens": ["Dun\u00b7kel", "der", "Wald", ",", "und", "es", "schwamm", "zit\u00b7ternd", "der", "sil\u00b7ber\u00b7ne", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPER", "ADJD", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Inselchen lockten mit stillem Geb\u00fcsch; leichtschwankende Nachen", "tokens": ["In\u00b7sel\u00b7chen", "lock\u00b7ten", "mit", "stil\u00b7lem", "Ge\u00b7b\u00fcsch", ";", "leicht\u00b7schwan\u00b7ken\u00b7de", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wiegten zum heimlichen Sitz harrender Liebe sich hin.", "tokens": ["Wieg\u00b7ten", "zum", "heim\u00b7li\u00b7chen", "Sitz", "har\u00b7ren\u00b7der", "Lie\u00b7be", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Wiesen verbreiteten rings ihr Bl\u00fcthengewand um des Flusses", "tokens": ["Wie\u00b7sen", "ver\u00b7brei\u00b7te\u00b7ten", "rings", "ihr", "Bl\u00fc\u00b7then\u00b7ge\u00b7wand", "um", "des", "Flus\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Rollende Fluth, und es schwieg ruhend das schattige Thal.", "tokens": ["Rol\u00b7len\u00b7de", "Fluth", ",", "und", "es", "schwieg", "ru\u00b7hend", "das", "schat\u00b7ti\u00b7ge", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Alles erschien mir fern wie ein freundliches Land der Verkl\u00e4rung,", "tokens": ["Al\u00b7les", "er\u00b7schien", "mir", "fern", "wie", "ein", "freund\u00b7li\u00b7ches", "Land", "der", "Ver\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und nicht sterbliche Lust l\u00e4chelte dort mir herab,", "tokens": ["Und", "nicht", "sterb\u00b7li\u00b7che", "Lust", "l\u00e4\u00b7chel\u00b7te", "dort", "mir", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "NN", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Denn schon schwamm die erbleichende Gluth tief unter der Dichtung", "tokens": ["Denn", "schon", "schwamm", "die", "er\u00b7blei\u00b7chen\u00b7de", "Gluth", "tief", "un\u00b7ter", "der", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Leuchtender Welt, stets hob h\u00f6her das Bild sich empor,", "tokens": ["Leuch\u00b7ten\u00b7der", "Welt", ",", "stets", "hob", "h\u00f6\u00b7her", "das", "Bild", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Herrlicher s\u00e4umte sich stets mit flammendem Golde der Sehnsucht", "tokens": ["Herr\u00b7li\u00b7cher", "s\u00e4um\u00b7te", "sich", "stets", "mit", "flam\u00b7men\u00b7dem", "Gol\u00b7de", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Wundergebiet, stets ward dunkler das irdische Thal.", "tokens": ["Wun\u00b7der\u00b7ge\u00b7biet", ",", "stets", "ward", "dunk\u00b7ler", "das", "ir\u00b7di\u00b7sche", "Thal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Ach, da dacht' ich an dich, Holdselige, welche des Freundes", "tokens": ["Ach", ",", "da", "dacht'", "ich", "an", "dich", ",", "Hold\u00b7se\u00b7li\u00b7ge", ",", "wel\u00b7che", "des", "Freun\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "N\u00e4chtlichen Gram so oft mischte mit d\u00e4mmernder Lust.", "tokens": ["N\u00e4cht\u00b7li\u00b7chen", "Gram", "so", "oft", "mischte", "mit", "d\u00e4m\u00b7mern\u00b7der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Wehmuth l\u00e4chelte still mir im sinnenden Blick; wohl f\u00fchlt' ich", "tokens": ["Weh\u00b7muth", "l\u00e4\u00b7chel\u00b7te", "still", "mir", "im", "sin\u00b7nen\u00b7den", "Blick", ";", "wohl", "f\u00fchlt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Tieferen Schmerz; doch fern tagte mir zartere Lust.", "tokens": ["Tie\u00b7fe\u00b7ren", "Schmerz", ";", "doch", "fern", "tag\u00b7te", "mir", "zar\u00b7te\u00b7re", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.27": {"text": "Sieh, du schautest herab aus dem lichten Gew\u00f6lk in des Sieges", "tokens": ["Sieh", ",", "du", "schau\u00b7test", "her\u00b7ab", "aus", "dem", "lich\u00b7ten", "Ge\u00b7w\u00f6lk", "in", "des", "Sie\u00b7ges"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Goldenem Kranz; dein Blick l\u00e4chelte ruhig und mild.", "tokens": ["Gol\u00b7de\u00b7nem", "Kranz", ";", "dein", "Blick", "l\u00e4\u00b7chel\u00b7te", "ru\u00b7hig", "und", "mild", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Sterne blinkten empor, wie du l\u00e4cheltest, tr\u00f6stende Sterne,", "tokens": ["Ster\u00b7ne", "blink\u00b7ten", "em\u00b7por", ",", "wie", "du", "l\u00e4\u00b7chel\u00b7test", ",", "tr\u00f6s\u00b7ten\u00b7de", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Raschaufstrahlendes Licht folgte der winkenden Hand,", "tokens": ["Ra\u00b7schauf\u00b7strah\u00b7len\u00b7des", "Licht", "folg\u00b7te", "der", "win\u00b7ken\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Glanzreich w\u00f6lbte zum Thor sich des Friedens heiliger Bogen,", "tokens": ["Glanz\u00b7reich", "w\u00f6lb\u00b7te", "zum", "Thor", "sich", "des", "Frie\u00b7dens", "hei\u00b7li\u00b7ger", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Und aus Morgengew\u00f6lk ebnete hell sich die Bahn.", "tokens": ["Und", "aus", "Mor\u00b7gen\u00b7ge\u00b7w\u00f6lk", "eb\u00b7ne\u00b7te", "hell", "sich", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJD", "PRF", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "M\u00e4chtig ergriff den verlangenden Geist stillschweigende Sehnsucht,", "tokens": ["M\u00e4ch\u00b7tig", "er\u00b7griff", "den", "ver\u00b7lan\u00b7gen\u00b7den", "Geist", "still\u00b7schwei\u00b7gen\u00b7de", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Dich nur sah ich, und dich f\u00fchlt' ich im Herzen allein;", "tokens": ["Dich", "nur", "sah", "ich", ",", "und", "dich", "f\u00fchlt'", "ich", "im", "Her\u00b7zen", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "N\u00e4chtlich versank um's hohe Gebirg mir die d\u00e4mmernde Welt rings,", "tokens": ["N\u00e4cht\u00b7lich", "ver\u00b7sank", "um's", "ho\u00b7he", "Ge\u00b7birg", "mir", "die", "d\u00e4m\u00b7mern\u00b7de", "Welt", "rings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Doch hoch \u00fcber mir hob klar sich der himmlische Dom.", "tokens": ["Doch", "hoch", "\u00fc\u00b7ber", "mir", "hob", "klar", "sich", "der", "himm\u00b7li\u00b7sche", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "ADJD", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Ach, wohl bl\u00fchet nur dort mir die Rose des Gl\u00fccks, und der Hoffnung", "tokens": ["Ach", ",", "wohl", "bl\u00fc\u00b7het", "nur", "dort", "mir", "die", "Ro\u00b7se", "des", "Gl\u00fccks", ",", "und", "der", "Hoff\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "ADV", "PPER", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Leitstern d\u00e4mmert nur dort leise dem Herzen empor!", "tokens": ["Leits\u00b7tern", "d\u00e4m\u00b7mert", "nur", "dort", "lei\u00b7se", "dem", "Her\u00b7zen", "em\u00b7por", "!"], "token_info": ["word