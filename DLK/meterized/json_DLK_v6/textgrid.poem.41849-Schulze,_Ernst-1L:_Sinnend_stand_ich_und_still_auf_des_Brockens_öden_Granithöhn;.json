", "PPER", "$.", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.47": {"text": "Doch, da senkte die Sonne sich ganz; schwarz wogte die Nacht auf,", "tokens": ["Doch", ",", "da", "senk\u00b7te", "die", "Son\u00b7ne", "sich", "ganz", ";", "schwarz", "wog\u00b7te", "die", "Nacht", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "ADV", "$.", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-++-++-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Graunvoll tobte der Sturm \u00fcber die Haide daher,", "tokens": ["Graun\u00b7voll", "tob\u00b7te", "der", "Sturm", "\u00fc\u00b7ber", "die", "Hai\u00b7de", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "PAV", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Kein trostk\u00fcndender Stern durchblinkte den tr\u00fcberen Nebel,", "tokens": ["Kein", "trost\u00b7k\u00fcn\u00b7den\u00b7der", "Stern", "durch\u00b7blink\u00b7te", "den", "tr\u00fc\u00b7be\u00b7ren", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Und in finsteren Duft senkte dein Bild sich hinab.", "tokens": ["Und", "in", "fins\u00b7te\u00b7ren", "Duft", "senk\u00b7te", "dein", "Bild", "sich", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.51": {"text": "Traum nur ist und Schatten das Hei