{"textgrid.poem.31016": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "130.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mach es wie die T\u00fcrcken, wann sie zu Felde ziehen:", "tokens": ["Ich", "mach", "es", "wie", "die", "T\u00fcr\u00b7cken", ",", "wann", "sie", "zu", "Fel\u00b7de", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie schicken halb Armeen, die nennen sie Partyen.", "tokens": ["Sie", "schi\u00b7cken", "halb", "Ar\u00b7me\u00b7en", ",", "die", "nen\u00b7nen", "sie", "Par\u00b7ty\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+---+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drey tausent Sinn-Getichte, wol mehr noch, sind gegangen,", "tokens": ["Drey", "tau\u00b7sent", "Sinn\u00b7Ge\u00b7tich\u00b7te", ",", "wol", "mehr", "noch", ",", "sind", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "$,", "ADV", "ADV", "ADV", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Um hin und her zu streiffen und Nachricht wo zufangen,", "tokens": ["Um", "hin", "und", "her", "zu", "streif\u00b7fen", "und", "Nach\u00b7richt", "wo", "zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "KON", "NN", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ob achten, ob verachten bey Klugen zu erlangen.", "tokens": ["Ob", "ach\u00b7ten", ",", "ob", "ver\u00b7ach\u00b7ten", "bey", "Klu\u00b7gen", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "KOUS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}