{"textgrid.poem.58179": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "217. Der g\u00f6ttlichen Liebe F\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb' ist s\u00fc\u00df doch auch gestrenge,", "tokens": ["Die", "Lieb'", "ist", "s\u00fc\u00df", "doch", "auch", "ge\u00b7stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr F\u00fchren geht oft wunderenge,", "tokens": ["Ihr", "F\u00fch\u00b7ren", "geht", "oft", "wun\u00b7de\u00b7ren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4sset nichts der Eigenheit,", "tokens": ["Sie", "l\u00e4s\u00b7set", "nichts", "der", "Ei\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr scharfes Aug' pr\u00fcft Herz und Nieren;", "tokens": ["Ihr", "schar\u00b7fes", "Aug'", "pr\u00fcft", "Herz", "und", "Nie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df dich nur g\u00e4nzlich ihrem F\u00fchren,", "tokens": ["La\u00df", "dich", "nur", "g\u00e4nz\u00b7lich", "ih\u00b7rem", "F\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird das Enge endlich weit!", "tokens": ["So", "wird", "das", "En\u00b7ge", "end\u00b7lich", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}