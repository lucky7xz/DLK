{"dta.poem.11247": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die um geld buhlende Dorinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorindens wahl-spruch ist: Wer schmiert, der f\u00e4hret wohl;", "tokens": ["Do\u00b7rin\u00b7dens", "wahl\u00b7spruch", "ist", ":", "Wer", "schmiert", ",", "der", "f\u00e4h\u00b7ret", "wohl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$.", "PWS", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das heist, da\u00df man ihr vor das glei\u00df bezahlen soll,", "tokens": ["Das", "heist", ",", "da\u00df", "man", "ihr", "vor", "das", "glei\u00df", "be\u00b7zah\u00b7len", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "PPER", "APPR", "ART", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh da\u00df sie fahren l\u00e4\u00dft. Doch dieser f\u00e4hrt zur h\u00f6llen,", "tokens": ["Eh", "da\u00df", "sie", "fah\u00b7ren", "l\u00e4\u00dft", ".", "Doch", "die\u00b7ser", "f\u00e4hrt", "zur", "h\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVINF", "VVFIN", "$.", "KON", "PDS", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich als Kutscher will zu ihrem zeuge stellen.", "tokens": ["Der", "sich", "als", "Kut\u00b7scher", "will", "zu", "ih\u00b7rem", "zeu\u00b7ge", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOUS", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}