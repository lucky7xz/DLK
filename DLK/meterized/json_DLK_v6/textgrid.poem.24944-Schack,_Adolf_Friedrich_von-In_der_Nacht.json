{"textgrid.poem.24944": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft hat der Tag die strahlenhellen", "tokens": ["Sanft", "hat", "der", "Tag", "die", "strah\u00b7len\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehm\u00fcden Augen zugethan;", "tokens": ["Seh\u00b7m\u00fc\u00b7den", "Au\u00b7gen", "zu\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Ruhe sammelt seine Wellen", "tokens": ["Zur", "Ru\u00b7he", "sam\u00b7melt", "sei\u00b7ne", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der laute Lebensocean,", "tokens": ["Der", "lau\u00b7te", "Le\u00b7ben\u00b7so\u00b7ce\u00b7an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und von dem Wechsel der Gestalten,", "tokens": ["Und", "von", "dem", "Wech\u00b7sel", "der", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der unten ewig wogt und kreist,", "tokens": ["Der", "un\u00b7ten", "e\u00b7wig", "wogt", "und", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schaut wieder zu der Nacht, der alten", "tokens": ["Schaut", "wie\u00b7der", "zu", "der", "Nacht", ",", "der", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geliebten Freundin, auf dein Geist.", "tokens": ["Ge\u00b7lieb\u00b7ten", "Freun\u00b7din", ",", "auf", "dein", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Still blickt sie mit vertieftem Sinnen,", "tokens": ["Still", "blickt", "sie", "mit", "ver\u00b7tief\u00b7tem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter, die vor allem war,", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "vor", "al\u00b7lem", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dich herab, und Schauer rinnen", "tokens": ["Auf", "dich", "her\u00b7ab", ",", "und", "Schau\u00b7er", "rin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deine Seele wunderbar;", "tokens": ["Durch", "dei\u00b7ne", "See\u00b7le", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir ist, als ob die teuren Z\u00fcge", "tokens": ["Dir", "ist", ",", "als", "ob", "die", "teu\u00b7ren", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du s\u00e4hest, die dich angelacht,", "tokens": ["Du", "s\u00e4\u00b7hest", ",", "die", "dich", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als deine Kindheit in der Wiege", "tokens": ["Als", "dei\u00b7ne", "Kind\u00b7heit", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrem ersten Schlaf erwacht.", "tokens": ["Aus", "ih\u00b7rem", "ers\u00b7ten", "Schlaf", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein gro\u00df Geheimnis, ahnst du, trage", "tokens": ["Ein", "gro\u00df", "Ge\u00b7heim\u00b7nis", ",", "ahnst", "du", ",", "tra\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttliche in ihrem Scho\u00df;", "tokens": ["Die", "G\u00f6tt\u00b7li\u00b7che", "in", "ih\u00b7rem", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sp\u00e4hst danach in banger Frage \u2013", "tokens": ["Du", "sp\u00e4hst", "da\u00b7nach", "in", "ban\u00b7ger", "Fra\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst, sie schweigt und l\u00e4chelt blo\u00df;", "tokens": ["Um\u00b7sonst", ",", "sie", "schweigt", "und", "l\u00e4\u00b7chelt", "blo\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie ihr Blick unwiderstehlich,", "tokens": ["Doch", "wie", "ihr", "Blick", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich bannend, auf dich niederschaut,", "tokens": ["Dich", "ban\u00b7nend", ",", "auf", "dich", "nie\u00b7der\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchlst du, wie \u00fcber dich allm\u00e4hlich", "tokens": ["F\u00fchlst", "du", ",", "wie", "\u00fc\u00b7ber", "dich", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein inniges Gen\u00fcgen taut.", "tokens": ["Ein", "in\u00b7ni\u00b7ges", "Ge\u00b7n\u00fc\u00b7gen", "taut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Die Frage stirbt auf deinem Munde,", "tokens": ["Die", "Fra\u00b7ge", "stirbt", "auf", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Zweifel wird Gebet;", "tokens": ["Und", "je\u00b7der", "Zwei\u00b7fel", "wird", "Ge\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du f\u00fchlst, wie aus dem Weltabgrunde", "tokens": ["Du", "f\u00fchlst", ",", "wie", "aus", "dem", "Welt\u00b7ab\u00b7grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Odem dir entgegenweht:", "tokens": ["Ein", "O\u00b7dem", "dir", "ent\u00b7ge\u00b7gen\u00b7weht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht w\u00e4hnst du ferner dich versto\u00dfen,", "tokens": ["Nicht", "w\u00e4hnst", "du", "fer\u00b7ner", "dich", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht heimatlos, und frei von Harm", "tokens": ["Nicht", "hei\u00b7mat\u00b7los", ",", "und", "frei", "von", "Harm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kehrst zu dem Tagewerk, dem gro\u00dfen,", "tokens": ["Kehrst", "zu", "dem", "Ta\u00b7ge\u00b7werk", ",", "dem", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zur\u00fcck du aus des Schlummers Arm.", "tokens": ["Zu\u00b7r\u00fcck", "du", "aus", "des", "Schlum\u00b7mers", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}