{"textgrid.poem.48015": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Enge", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus m\u00f6cht ich ziehn in die bl\u00fchende Weite,", "tokens": ["Hin\u00b7aus", "m\u00f6cht", "ich", "ziehn", "in", "die", "bl\u00fc\u00b7hen\u00b7de", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "ein Lied auf der Lippe, den Lenz als Geleite,", "tokens": ["ein", "Lied", "auf", "der", "Lip\u00b7pe", ",", "den", "Lenz", "als", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "KOUS", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "in rauschende W\u00e4lder an sonnigen Borden,", "tokens": ["in", "rau\u00b7schen\u00b7de", "W\u00e4l\u00b7der", "an", "son\u00b7ni\u00b7gen", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "auf ragende Berge \u2013 nach S\u00fcden, nach Norden \u2013", "tokens": ["auf", "ra\u00b7gen\u00b7de", "Ber\u00b7ge", "\u2013", "nach", "S\u00fc\u00b7den", ",", "nach", "Nor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "NN", "$,", "APPR", "NN", "$("], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hinaus nur, hinaus!", "tokens": ["Hin\u00b7aus", "nur", ",", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Hinaus m\u00f6cht ich ziehn auf die l\u00e4rmenden Gassen,", "tokens": ["Hin\u00b7aus", "m\u00f6cht", "ich", "ziehn", "auf", "die", "l\u00e4r\u00b7men\u00b7den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "ein Tropfen, versinken im Meere der Massen,", "tokens": ["ein", "Trop\u00b7fen", ",", "ver\u00b7sin\u00b7ken", "im", "Mee\u00b7re", "der", "Mas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "der eigenen Pulse Anschwellen und Schwinden", "tokens": ["der", "ei\u00b7ge\u00b7nen", "Pul\u00b7se", "An\u00b7schwel\u00b7len", "und", "Schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "erschauernd als Herzschlag der Menschheit empfinden;", "tokens": ["er\u00b7schau\u00b7ernd", "als", "Herz\u00b7schlag", "der", "Menschheit", "emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hinaus nur, hinaus!", "tokens": ["Hin\u00b7aus", "nur", ",", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Hinaus m\u00f6cht ich ziehn in die schimmernde Ferne; \u2013", "tokens": ["Hin\u00b7aus", "m\u00f6cht", "ich", "ziehn", "in", "die", "schim\u00b7mern\u00b7de", "Fer\u00b7ne", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "schon gl\u00e4nzen zu H\u00e4upten mir tropische Sterne:", "tokens": ["schon", "gl\u00e4n\u00b7zen", "zu", "H\u00e4up\u00b7ten", "mir", "tro\u00b7pi\u00b7sche", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PTKZU", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "in gl\u00fchenden N\u00e4chten, in pressenden Armen", "tokens": ["in", "gl\u00fc\u00b7hen\u00b7den", "N\u00e4ch\u00b7ten", ",", "in", "pres\u00b7sen\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "m\u00f6cht ich zu lachendem Leben erwarmen . . .", "tokens": ["m\u00f6cht", "ich", "zu", "la\u00b7chen\u00b7dem", "Le\u00b7ben", "er\u00b7war\u00b7men", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$.", "$.", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Hinaus nur, hinaus!", "tokens": ["Hin\u00b7aus", "nur", ",", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}