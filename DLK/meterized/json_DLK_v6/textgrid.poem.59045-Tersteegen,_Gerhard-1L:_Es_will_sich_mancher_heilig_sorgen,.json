{"textgrid.poem.59045": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es will sich mancher heilig sorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es will sich mancher heilig sorgen,", "tokens": ["Es", "will", "sich", "man\u00b7cher", "hei\u00b7lig", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er \u00e4ngst't, er gr\u00e4mt sich heut' und morgen;", "tokens": ["Er", "\u00e4ngst't", ",", "er", "gr\u00e4mt", "sich", "heut'", "und", "mor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kind, schlie\u00df die Augen, bleib im Scho\u00df", "tokens": ["Kind", ",", "schlie\u00df", "die", "Au\u00b7gen", ",", "bleib", "im", "Scho\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und saug dich ohne Sorgen gro\u00df!", "tokens": ["Und", "saug", "dich", "oh\u00b7ne", "Sor\u00b7gen", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}