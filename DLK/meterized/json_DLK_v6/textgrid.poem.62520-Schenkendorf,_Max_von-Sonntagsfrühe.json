{"textgrid.poem.62520": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsfr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottesstille, Sonntagsfr\u00fche,", "tokens": ["Got\u00b7tes\u00b7stil\u00b7le", ",", "Sonn\u00b7tags\u00b7fr\u00fc\u00b7he", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhe, die der Herr gebot!", "tokens": ["Ru\u00b7he", ",", "die", "der", "Herr", "ge\u00b7bot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seele, wach' und gl\u00fche", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "wach'", "und", "gl\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit im hellen Morgenroth.", "tokens": ["Mit", "im", "hel\u00b7len", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich in dem Zimmer bleiben,", "tokens": ["K\u00f6nnt'", "ich", "in", "dem", "Zim\u00b7mer", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann das Volk zur Kirche wallt?", "tokens": ["Wann", "das", "Volk", "zur", "Kir\u00b7che", "wallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich Alltagswerke treiben,", "tokens": ["K\u00f6nnt'", "ich", "All\u00b7tags\u00b7wer\u00b7ke", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann der Glockenruf erschallt?", "tokens": ["Wann", "der", "Glo\u00b7cken\u00b7ruf", "er\u00b7schallt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo die holden Worte weilen,", "tokens": ["Wo", "die", "hol\u00b7den", "Wor\u00b7te", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Herr auf Erden sprach,", "tokens": ["Die", "der", "Herr", "auf", "Er\u00b7den", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasset auch das Brod mich theilen,", "tokens": ["Las\u00b7set", "auch", "das", "Brod", "mich", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das er seinen J\u00fcngern brach.", "tokens": ["Das", "er", "sei\u00b7nen", "J\u00fcn\u00b7gern", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O das nenn' ich sel'ge Stunde,", "tokens": ["O", "das", "nenn'", "ich", "sel'\u00b7ge", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo man dein, o Herr, gedenkt,", "tokens": ["Wo", "man", "dein", ",", "o", "Herr", ",", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man mit der frohen Kunde", "tokens": ["Wo", "man", "mit", "der", "fro\u00b7hen", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem ew'gen Heil uns tr\u00e4nkt.", "tokens": ["Von", "dem", "ew'\u00b7gen", "Heil", "uns", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Neues Leben, neue St\u00e4rke,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", ",", "neu\u00b7e", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiner Andacht frische Glut", "tokens": ["Rei\u00b7ner", "An\u00b7dacht", "fri\u00b7sche", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem frommen Liebeswerke", "tokens": ["Zu", "dem", "from\u00b7men", "Lie\u00b7bes\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6pf' ich aus der Gnadenflut.", "tokens": ["Sch\u00f6pf'", "ich", "aus", "der", "Gna\u00b7den\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und von g\u00f6ttlichen Gedanken", "tokens": ["Und", "von", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen reichen Bl\u00fctenstrau\u00df", "tokens": ["Ei\u00b7nen", "rei\u00b7chen", "Bl\u00fc\u00b7ten\u00b7strau\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trag' ich heimw\u00e4rts, Gott zu danken", "tokens": ["Trag'", "ich", "heim\u00b7w\u00e4rts", ",", "Gott", "zu", "dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem kleinen stillen Haus.", "tokens": ["In", "dem", "klei\u00b7nen", "stil\u00b7len", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Erde weit und ohne Grenzen!", "tokens": ["Er\u00b7de", "weit", "und", "oh\u00b7ne", "Gren\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel dr\u00fcber ausgespannt!", "tokens": ["Him\u00b7mel", "dr\u00fc\u00b7ber", "aus\u00b7ge\u00b7spannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reich an Sternen und an Kr\u00e4nzen", "tokens": ["Reich", "an", "Ster\u00b7nen", "und", "an", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheint ihr mir ein heilig Land.", "tokens": ["Scheint", "ihr", "mir", "ein", "hei\u00b7lig", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df die Flamme stets mir brennen,", "tokens": ["La\u00df", "die", "Flam\u00b7me", "stets", "mir", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O mein Heiland Jesu Christ!", "tokens": ["O", "mein", "Hei\u00b7land", "Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df es alle Welt erkennen,", "tokens": ["La\u00df", "es", "al\u00b7le", "Welt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Herz dein Altar ist.", "tokens": ["Da\u00df", "mein", "Herz", "dein", "Al\u00b7tar", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}}}}}