{"textgrid.poem.56085": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unwillk\u00fcrlich sehn sie seinem Spiel", "tokens": ["Un\u00b7will\u00b7k\u00fcr\u00b7lich", "sehn", "sie", "sei\u00b7nem", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "lange zu; zuweilen tritt das runde", "tokens": ["lan\u00b7ge", "zu", ";", "zu\u00b7wei\u00b7len", "tritt", "das", "run\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "seiende Gesicht aus dem Profil,", "tokens": ["sei\u00b7en\u00b7de", "Ge\u00b7sicht", "aus", "dem", "Pro\u00b7fil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "klar und ganz wie eine volle Stunde,", "tokens": ["klar", "und", "ganz", "wie", "ei\u00b7ne", "vol\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "welche anhebt und zu Ende schl\u00e4gt.", "tokens": ["wel\u00b7che", "an\u00b7hebt", "und", "zu", "En\u00b7de", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch die Andern z\u00e4hlen nicht die Schl\u00e4ge,", "tokens": ["Doch", "die", "An\u00b7dern", "z\u00e4h\u00b7len", "nicht", "die", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "tr\u00fcb von M\u00fchsal und vom Leben tr\u00e4ge;", "tokens": ["tr\u00fcb", "von", "M\u00fch\u00b7sal", "und", "vom", "Le\u00b7ben", "tr\u00e4\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und sie merken gar nicht, wie es tr\u00e4gt \u2013,", "tokens": ["und", "sie", "mer\u00b7ken", "gar", "nicht", ",", "wie", "es", "tr\u00e4gt", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "wie es alles tr\u00e4gt, auch dann, noch immer,", "tokens": ["wie", "es", "al\u00b7les", "tr\u00e4gt", ",", "auch", "dann", ",", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wenn es m\u00fcde in dem kleinen Kleid", "tokens": ["wenn", "es", "m\u00fc\u00b7de", "in", "dem", "klei\u00b7nen", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "neben ihnen wie im Wartezimmer", "tokens": ["ne\u00b7ben", "ih\u00b7nen", "wie", "im", "War\u00b7te\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sitzt und warten will auf seine Zeit.", "tokens": ["sitzt", "und", "war\u00b7ten", "will", "auf", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}