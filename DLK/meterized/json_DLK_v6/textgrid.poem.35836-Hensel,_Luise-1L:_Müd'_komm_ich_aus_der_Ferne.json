{"textgrid.poem.35836": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcd' komm ich aus der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcd' komm ich aus der Ferne", "tokens": ["M\u00fcd'", "komm", "ich", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit schwerem Wanderstab;", "tokens": ["Mit", "schwe\u00b7rem", "Wan\u00b7der\u00b7stab", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! gr\u00fc\u00dfen wollt' ich gerne", "tokens": ["Ach", "!", "gr\u00fc\u00b7\u00dfen", "wollt'", "ich", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVINF", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der treu'sten Freundin Grab.", "tokens": ["Der", "treu'\u00b7sten", "Freun\u00b7din", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sagen keine Worte,", "tokens": ["Es", "sa\u00b7gen", "kei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es weht aus keinem Lied,", "tokens": ["Es", "weht", "aus", "kei\u00b7nem", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich in ihr gefunden,", "tokens": ["Was", "ich", "in", "ihr", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mir mit ihr verbl\u00fcht.", "tokens": ["Was", "mir", "mit", "ihr", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das reichste Herz an G\u00fcte,", "tokens": ["Das", "reichs\u00b7te", "Herz", "an", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ich auf Erden fand,", "tokens": ["Das", "ich", "auf", "Er\u00b7den", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das bergen diese Blumen,", "tokens": ["Das", "ber\u00b7gen", "die\u00b7se", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das decket dieser Sand. \u2013", "tokens": ["Das", "de\u00b7cket", "die\u00b7ser", "Sand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich knie' an ihrem Grabe", "tokens": ["Ich", "knie'", "an", "ih\u00b7rem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So einsam und so arm.", "tokens": ["So", "ein\u00b7sam", "und", "so", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es tranken seine Blumen", "tokens": ["Es", "tran\u00b7ken", "sei\u00b7ne", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl nimmer Thau, so warm. \u2013", "tokens": ["Wohl", "nim\u00b7mer", "Thau", ",", "so", "warm", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O, dr\u00e4ngen meine Thr\u00e4nen", "tokens": ["O", ",", "dr\u00e4n\u00b7gen", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab, hinab zu ihr", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7ab", "zu", "ihr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weht' aus ihrem Munde", "tokens": ["Und", "weht'", "aus", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Hauch herauf zu mir! \u2013", "tokens": ["Ein", "Hauch", "her\u00b7auf", "zu", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch still und kalt und \u00f6de", "tokens": ["Doch", "still", "und", "kalt", "und", "\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ruht Alles weit umher \u2013", "tokens": ["Ruht", "Al\u00b7les", "weit", "um\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weckt mein lauter Jammer", "tokens": ["Es", "weckt", "mein", "lau\u00b7ter", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich, Selige! nicht mehr. \u2013", "tokens": ["Dich", ",", "Se\u00b7li\u00b7ge", "!", "nicht", "mehr", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So ruh' in Gottes Frieden", "tokens": ["So", "ruh'", "in", "Got\u00b7tes", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Deiner stillen Gruft,", "tokens": ["In", "Dei\u00b7ner", "stil\u00b7len", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis des Erweckers Stimme", "tokens": ["Bis", "des", "Er\u00b7we\u00b7ckers", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur ew'gen Wonne ruft.", "tokens": ["Zur", "ew'\u00b7gen", "Won\u00b7ne", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Schlaf' wohl, schlaf' wohl, Geliebte! \u2013", "tokens": ["Schlaf'", "wohl", ",", "schlaf'", "wohl", ",", "Ge\u00b7lieb\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich nehme welkend Laub", "tokens": ["Ich", "neh\u00b7me", "wel\u00b7kend", "Laub"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von deinem stillen H\u00fcgel", "tokens": ["Von", "dei\u00b7nem", "stil\u00b7len", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine Handvoll Staub.", "tokens": ["Und", "ei\u00b7ne", "Hand\u00b7voll", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das ist, was mir geblieben", "tokens": ["Das", "ist", ",", "was", "mir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von aller Erdenlust:", "tokens": ["Von", "al\u00b7ler", "Er\u00b7den\u00b7lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ruh' als Angedenken", "tokens": ["Es", "ruh'", "als", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf meiner kranken Brust.", "tokens": ["Auf", "mei\u00b7ner", "kran\u00b7ken", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wenn sie mich begraben", "tokens": ["Und", "wenn", "sie", "mich", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dereinst im fernen Land,", "tokens": ["De\u00b7reinst", "im", "fer\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deckt mein gebrochnes Herze", "tokens": ["Deckt", "mein", "ge\u00b7broch\u00b7nes", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Deines H\u00fcgels Sand.", "tokens": ["Noch", "Dei\u00b7nes", "H\u00fc\u00b7gels", "Sand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}