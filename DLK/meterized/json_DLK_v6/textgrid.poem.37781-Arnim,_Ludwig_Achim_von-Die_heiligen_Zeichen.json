{"textgrid.poem.37781": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die heiligen Zeichen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunder! schreit's durch alle Gassen,", "tokens": ["Wun\u00b7der", "!", "schreit's", "durch", "al\u00b7le", "Gas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Priester Wunder! schreien:", "tokens": ["Auch", "die", "Pries\u00b7ter", "Wun\u00b7der", "!", "schrei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbihr sollt neuen Glauben fassen,", "tokens": ["\u00bb", "ihr", "sollt", "neu\u00b7en", "Glau\u00b7ben", "fas\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch durch diese Zeichen weihen.", "tokens": ["Euch", "durch", "die\u00b7se", "Zei\u00b7chen", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht die Brust der kranken Nonne", "tokens": ["Seht", "die", "Brust", "der", "kran\u00b7ken", "Non\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist bezeichnet mit dem Kreutze,", "tokens": ["Ist", "be\u00b7zeich\u00b7net", "mit", "dem", "Kreut\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit des Dornenkranzes Sonne", "tokens": ["Mit", "des", "Dor\u00b7nen\u00b7kran\u00b7zes", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fcht die Stirn vom Schmerzensreitze.", "tokens": ["Gl\u00fcht", "die", "Stirn", "vom", "Schmer\u00b7zens\u00b7reit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die heilgen N\u00e4gelmahle", "tokens": ["Und", "die", "heil\u00b7gen", "N\u00e4\u00b7gel\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimmern roth an H\u00e4nd' und F\u00fc\u00dfen,", "tokens": ["Schim\u00b7mern", "roth", "an", "H\u00e4nd'", "und", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So will Gott im Erdenthale", "tokens": ["So", "will", "Gott", "im", "Er\u00b7dent\u00b7ha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Lange Leiden ihr vers\u00fc\u00dfen.", "tokens": ["Lan\u00b7ge", "Lei\u00b7den", "ihr", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie der Herr des Walds erst stellet", "tokens": ["Wie", "der", "Herr", "des", "Walds", "erst", "stel\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeichen zu den sch\u00f6nsten Eichen,", "tokens": ["Zei\u00b7chen", "zu", "den", "sch\u00f6ns\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh er sie zur Kirche f\u00e4llet", "tokens": ["Eh", "er", "sie", "zur", "Kir\u00b7che", "f\u00e4l\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Himmel soll erreichen;", "tokens": ["Die", "den", "Him\u00b7mel", "soll", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So ist Gott der Sohn gekommen", "tokens": ["So", "ist", "Gott", "der", "Sohn", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oeffnet mit den heilgen Wunden", "tokens": ["Oeff\u00b7net", "mit", "den", "heil\u00b7gen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kopf und Herz, die noch beklommen", "tokens": ["Kopf", "und", "Herz", ",", "die", "noch", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den letzten Erdenstunden.", "tokens": ["Von", "den", "letz\u00b7ten", "Er\u00b7dens\u00b7tun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seht sie sterben, seht sie scheiden", "tokens": ["Seht", "sie", "ster\u00b7ben", ",", "seht", "sie", "schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ist unser, bleibt uns eigen,", "tokens": ["Sie", "ist", "un\u00b7ser", ",", "bleibt", "uns", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solcher Tod ist zu beneiden", "tokens": ["Sol\u00b7cher", "Tod", "ist", "zu", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie wird einst f\u00fcr uns zeugen.", "tokens": ["Und", "sie", "wird", "einst", "f\u00fcr", "uns", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf dem Altar unsrer Kirche", "tokens": ["Auf", "dem", "Al\u00b7tar", "uns\u00b7rer", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wird der Leichnam bald verehret,", "tokens": ["Wird", "der", "Leich\u00b7nam", "bald", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie segnend Wunder wirke", "tokens": ["Da\u00df", "sie", "seg\u00b7nend", "Wun\u00b7der", "wir\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Glauben, den sie lehret.\u00ab", "tokens": ["In", "dem", "Glau\u00b7ben", ",", "den", "sie", "leh\u00b7ret", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tausend stehen an dem Bette,", "tokens": ["Tau\u00b7send", "ste\u00b7hen", "an", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gn\u00e4dger Gott, die Heilge rette", "tokens": ["Gn\u00e4d\u00b7ger", "Gott", ",", "die", "Heil\u00b7ge", "ret\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Statt dies Zeichen ihr zu schenken.", "tokens": ["Statt", "dies", "Zei\u00b7chen", "ihr", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df sie hier mit ihrer Lehre", "tokens": ["Da\u00df", "sie", "hier", "mit", "ih\u00b7rer", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem nahen selgen Anschaun", "tokens": ["Aus", "dem", "na\u00b7hen", "sel\u00b7gen", "An\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Unsern irdschen Wahn zerst\u00f6re", "tokens": ["Un\u00b7sern", "ird\u00b7schen", "Wahn", "zer\u00b7st\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Herzens Eis mag aufthaun.", "tokens": ["Und", "des", "Her\u00b7zens", "Eis", "mag", "auft\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Dieses Wunder mich nicht w\u00e4rmet,", "tokens": ["Die\u00b7ses", "Wun\u00b7der", "mich", "nicht", "w\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Zeichen mir nicht strahlet,", "tokens": ["Die\u00b7ses", "Zei\u00b7chen", "mir", "nicht", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Volk im Glauben schw\u00e4rmet", "tokens": ["Wo", "ein", "Volk", "im", "Glau\u00b7ben", "schw\u00e4r\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Trugbild leicht gemahlet.\u00ab", "tokens": ["Ist", "ein", "Trug\u00b7bild", "leicht", "ge\u00b7mah\u00b7let", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zornig drohet ihm die Menge,", "tokens": ["Zor\u00b7nig", "dro\u00b7het", "ihm", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Nonne winket Frieden,", "tokens": ["Doch", "die", "Non\u00b7ne", "win\u00b7ket", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder kniet nun das Gedr\u00e4nge,", "tokens": ["Wie\u00b7der", "kniet", "nun", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ruft nach Segen bey der M\u00fcden.", "tokens": ["Ruft", "nach", "Se\u00b7gen", "bey", "der", "M\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und mit ihrem letzten Athem", "tokens": ["Und", "mit", "ih\u00b7rem", "letz\u00b7ten", "A\u00b7them"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt die Fromme ihre Stimme:", "tokens": ["Hebt", "die", "From\u00b7me", "ih\u00b7re", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsegne ", "tokens": ["\u00bb", "seg\u00b7ne"], "token_info": ["punct", "word"], "pos": ["$(", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der mich f\u00fchrt, wohin ich klimme.", "tokens": ["Der", "mich", "f\u00fchrt", ",", "wo\u00b7hin", "ich", "klim\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Achtet h\u00f6her nicht die Zeichen", "tokens": ["Ach\u00b7tet", "h\u00f6\u00b7her", "nicht", "die", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als den Geist, der ist das Wesen,", "tokens": ["Als", "den", "Geist", ",", "der", "ist", "das", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Zeichen m\u00fcssen weichen", "tokens": ["Die\u00b7se", "Zei\u00b7chen", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.14": {"line.1": {"text": "In dem ausgezehrten Leibe", "tokens": ["In", "dem", "aus\u00b7ge\u00b7zehr\u00b7ten", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden frey der Seele Fl\u00fcgel,", "tokens": ["Wur\u00b7den", "frey", "der", "See\u00b7le", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im heilgen Zeitvertreibe", "tokens": ["Und", "im", "heil\u00b7gen", "Zeit\u00b7ver\u00b7trei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcckte sie mir auf das Siegel.", "tokens": ["Dr\u00fcck\u00b7te", "sie", "mir", "auf", "das", "Sie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wo ich ", "tokens": ["Wo", "ich"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wo die H\u00e4nde mich gek\u00fchlet,", "tokens": ["Wo", "die", "H\u00e4n\u00b7de", "mich", "ge\u00b7k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Geist zu Gott getragen.", "tokens": ["Wenn", "der", "Geist", "zu", "Gott", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wo die H\u00e4nd' im Schlaf gefalten,", "tokens": ["Wo", "die", "H\u00e4nd'", "im", "Schlaf"