{"dta.poem.20608": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Je \u2019s isch nit anderst, lueg mi a, wie", "tokens": ["Je", "'s", "isch", "nit", "an\u00b7derst", ",", "lu\u00b7eg", "mi", "a", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "ADJD", "PTKNEG", "ADV", "$,", "FM", "FM", "FM", "$,", "PWAV"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "d\u2019 witt,", "tokens": ["d'", "witt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "und mit der Zit verbrennt die ganzi Welt.", "tokens": ["und", "mit", "der", "Zit", "ver\u00b7brennt", "die", "gan\u00b7zi", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es goht e W\u00e4chter us um Mitternacht,", "tokens": ["Es", "goht", "e", "W\u00e4ch\u00b7ter", "us", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "e fremde Ma, me wei\u00df nit, wer er isch,", "tokens": ["e", "frem\u00b7de", "Ma", ",", "me", "wei\u00df", "nit", ",", "wer", "er", "isch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "er funklet, wie ne Stern, und r\u00fceft:", "tokens": ["er", "funk\u00b7let", ",", "wie", "ne", "Stern", ",", "und", "r\u00fceft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wacht auf, es kommt der Tag!&#8220; &#8212;               ", "tokens": ["Wacht", "auf", ",", "es", "kommt", "der", "Tag", "!", "&#8220;", "&#8212;"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "XML_entity", "XML_entity"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Drob r\u00f6thet si", "tokens": ["Drob", "r\u00f6\u00b7thet", "si"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "der Himmel, und es duudert \u00fcberal,", "tokens": ["der", "Him\u00b7mel", ",", "und", "es", "duu\u00b7dert", "\u00fc\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "z\u2019 erst heimli, alsgmach lut, wie sellemol", "tokens": ["z'", "erst", "heim\u00b7li", ",", "alsg\u00b7mach", "lut", ",", "wie", "sel\u00b7le\u00b7mol"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "NE", "$,", "KOUS", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "wo Anno Sechsen\u00fcnzgi der Franzos", "tokens": ["wo", "An\u00b7no", "Sech\u00b7sen\u00b7\u00fcnz\u00b7gi", "der", "Fran\u00b7zos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "so uding gscho\u00dfe het. Der Bode wankt,", "tokens": ["so", "u\u00b7ding", "gscho\u00b7\u00dfe", "het", ".", "Der", "Bo\u00b7de", "wankt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "VAFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "a\u00df d\u2019 Chilch-Th\u00fcrn guge; d\u2019 Glocke schla-", "tokens": ["a\u00df", "d'", "Chilch\u00b7T\u00b7h\u00fcrn", "gu\u00b7ge", ";", "d'", "Glo\u00b7cke", "schla"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "VVFIN", "$.", "NE", "NE", "TRUNC"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "gen a,", "tokens": ["gen", "a", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "und l\u00fcte selber Bet-Zit wit und breit,", "tokens": ["und", "l\u00fc\u00b7te", "sel\u00b7ber", "Bet\u00b7Zit", "wit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und alles betet. Dr\u00fcber chunnt der Tag;", "tokens": ["und", "al\u00b7les", "be\u00b7tet", ".", "Dr\u00fc\u00b7ber", "chunnt", "der", "Tag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "o, bh\u00fctis Gott, me brucht ke Sunn derzu,", "tokens": ["o", ",", "bh\u00fc\u00b7tis", "Gott", ",", "me", "brucht", "ke", "Sunn", "der\u00b7zu", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ADJA", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "der Himmel stoht im Blitz, und d\u2019 Welt", "tokens": ["der", "Him\u00b7mel", "stoht", "im", "Blitz", ",", "und", "d'", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "KON", "NE", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "im Glast.", "tokens": ["im", "Glast", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Druf gschieht no viel, i ha iez nit der Zit;", "tokens": ["Druf", "gschieht", "no", "viel", ",", "i", "ha", "iez", "nit", "der", "Zit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "ADV", "$,", "FM", "FM", "FM", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "und endli z\u00fcndets a, und brennt und brennt,", "tokens": ["und", "end\u00b7li", "z\u00fcn\u00b7dets", "a", ",", "und", "brennt", "und", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "wo Boden isch, und niemes l\u00f6scht; es", "tokens": ["wo", "Bo\u00b7den", "isch", ",", "und", "nie\u00b7mes", "l\u00f6scht", ";", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "ADJD", "$,", "KON", "PIS", "VVFIN", "$.", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "glnmst", "tokens": ["glnmst"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "zlezt selber ab. Wie meinsch, siehts us", "tokens": ["zlezt", "sel\u00b7ber", "ab", ".", "Wie", "meinsch", ",", "siehts", "us"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "PWAV", "ADJD", "$,", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "derno?", "tokens": ["der\u00b7no", "?"], "token_info": ["word", "punct"], "pos": ["PDS", "$."], "meter": "-+", "measure": "iambic.single"}}}}}