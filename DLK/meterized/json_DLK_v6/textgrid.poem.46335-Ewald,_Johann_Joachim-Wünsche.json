{"textgrid.poem.46335": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcnsche", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Max w\u00fcnscht sich Rang, Stax w\u00fcnscht sich Macht,", "tokens": ["Max", "w\u00fcnscht", "sich", "Rang", ",", "Stax", "w\u00fcnscht", "sich", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "NN", "$,", "NE", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scot w\u00fcnscht, da\u00df nur sein F\u00fcrst ihm l\u00e4chle,", "tokens": ["Scot", "w\u00fcnscht", ",", "da\u00df", "nur", "sein", "F\u00fcrst", "ihm", "l\u00e4ch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Pedrill, da\u00df ihn sein M\u00e4dchen f\u00e4chle,", "tokens": ["Ped\u00b7rill", ",", "da\u00df", "ihn", "sein", "M\u00e4d\u00b7chen", "f\u00e4ch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Thrax folgt Hunden auf die Jagd;", "tokens": ["Und", "Thrax", "folgt", "Hun\u00b7den", "auf", "die", "Jagd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Cham will ein schallendes Ger\u00fcchte", "tokens": ["Cham", "will", "ein", "schal\u00b7len\u00b7des", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und ich ... nur hundert Sinngedichte.", "tokens": ["Und", "ich", "...", "nur", "hun\u00b7dert", "Sinn\u00b7ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}