{"textgrid.poem.63955": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das Vergi\u00dfmeinnicht", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinniges Bl\u00fcmchen,", "tokens": ["Sin\u00b7ni\u00b7ges", "Bl\u00fcm\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Blaues Vergi\u00dfmeinnicht,", "tokens": ["Blau\u00b7es", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Entpfl\u00fcckt dem leise", "tokens": ["Ent\u00b7pfl\u00fcckt", "dem", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Murmelnden Bach", "tokens": ["Mur\u00b7meln\u00b7den", "Bach"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Von M\u00e4dchenhand,", "tokens": ["Von", "M\u00e4d\u00b7chen\u00b7hand", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Tr\u00e4nenbetaut", "tokens": ["Tr\u00e4\u00b7nen\u00b7be\u00b7taut"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Unterm Abschiedsku\u00df", "tokens": ["Un\u00b7term", "Ab\u00b7schieds\u00b7ku\u00df"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Dem scheidenden", "tokens": ["Dem", "schei\u00b7den\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Liebsten gegeben, \u2013", "tokens": ["Liebs\u00b7ten", "ge\u00b7ge\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "$,", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Hast eine Seele du?", "tokens": ["Hast", "ei\u00b7ne", "See\u00b7le", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ri\u00df die Holde", "tokens": ["Ri\u00df", "die", "Hol\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Grausam", "tokens": ["Grau\u00b7sam"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Dich aus bachumrieseltem", "tokens": ["Dich", "aus", "ba\u00b7chum\u00b7rie\u00b7sel\u00b7tem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Blumenleben?", "tokens": ["Blu\u00b7men\u00b7le\u00b7ben", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "F\u00fchltest du schmerzlich", "tokens": ["F\u00fchl\u00b7test", "du", "schmerz\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Die pfl\u00fcckende Hand?", "tokens": ["Die", "pfl\u00fc\u00b7cken\u00b7de", "Hand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Starbest du", "tokens": ["Star\u00b7best", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Von n\u00e4hrender Wurzel", "tokens": ["Von", "n\u00e4h\u00b7ren\u00b7der", "Wur\u00b7zel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Geknickt?", "tokens": ["Ge\u00b7knickt", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Himmelblau,", "tokens": ["Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Wie zuvor,", "tokens": ["Wie", "zu\u00b7vor", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Noch schimmert dein Aug'! \u2013 \u2013 \u2013", "tokens": ["Noch", "schim\u00b7mert", "dein", "Aug'", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "In ein Wasserglas", "tokens": ["In", "ein", "Was\u00b7ser\u00b7glas"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Stellt dich der Knabe,", "tokens": ["Stellt", "dich", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Kaum da\u00df er das R\u00e4nzel", "tokens": ["Kaum", "da\u00df", "er", "das", "R\u00e4n\u00b7zel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "An den Nagel geh\u00e4ngt:", "tokens": ["An", "den", "Na\u00b7gel", "ge\u00b7h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und frisch bleibst du,", "tokens": ["Und", "frisch", "bleibst", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bl\u00fchend", "tokens": ["Bl\u00fc\u00b7hend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als wenn noch", "tokens": ["Als", "wenn", "noch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wurzelnd du st\u00e4ndest im Bach.", "tokens": ["Wur\u00b7zelnd", "du", "st\u00e4n\u00b7dest", "im", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Oft zur Sehnsuchtsstunde", "tokens": ["Oft", "zur", "Sehn\u00b7suchts\u00b7stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der D\u00e4mmerung", "tokens": ["Der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Nimmt er dich aus dem Glase,", "tokens": ["Nimmt", "er", "dich", "aus", "dem", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Betrachtet dich innig,", "tokens": ["Be\u00b7trach\u00b7tet", "dich", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Liebesbote du,", "tokens": ["Lie\u00b7bes\u00b7bo\u00b7te", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Von ihrer Hand", "tokens": ["Von", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit Tr\u00e4nen benetzt,", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Gewandert in seine. \u2013 \u2013 \u2013 \u2013", "tokens": ["Ge\u00b7wan\u00b7dert", "in", "sei\u00b7ne", ".", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "$.", "$(", "$(", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Linke im braunen Gelock,", "tokens": ["Die", "Lin\u00b7ke", "im", "brau\u00b7nen", "Ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ans Fenster sich lehnend,", "tokens": ["Ans", "Fens\u00b7ter", "sich", "leh\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So sieht er mit sehnendem Blick", "tokens": ["So", "sieht", "er", "mit", "seh\u00b7nen\u00b7dem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Hinaus in die Gegend,", "tokens": ["Hin\u00b7aus", "in", "die", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wo weit dahinten", "tokens": ["Wo", "weit", "da\u00b7hin\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "PAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sein Liebchen weilt.", "tokens": ["Sein", "Lieb\u00b7chen", "weilt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Seine Gedanken gehen", "tokens": ["Sei\u00b7ne", "Ge\u00b7dan\u00b7ken", "ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Weit die Giebel hin\u00fcber,", "tokens": ["Weit", "die", "Gie\u00b7bel", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Die T\u00fcrme und Mauern der Stadt", "tokens": ["Die", "T\u00fcr\u00b7me", "und", "Mau\u00b7ern", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Weit, weit hinweg,", "tokens": ["Weit", ",", "weit", "hin\u00b7weg", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Bis wo in stiller Kammer", "tokens": ["Bis", "wo", "in", "stil\u00b7ler", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein M\u00e4gdlein steht am Fenster,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "steht", "am", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und Tr\u00e4nen der Wehmut", "tokens": ["Und", "Tr\u00e4\u00b7nen", "der", "Weh\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Im Auge", "tokens": ["Im", "Au\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Ins blassende Abendrot sieht ...", "tokens": ["Ins", "blas\u00b7sen\u00b7de", "A\u00b7ben\u00b7drot", "sieht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Jetzt, Vergi\u00dfmeinnicht,", "tokens": ["Jetzt", ",", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Streift dich sein Auge,", "tokens": ["Streift", "dich", "sein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Er k\u00fc\u00dft anstatt der lieben", "tokens": ["Er", "k\u00fc\u00dft", "an\u00b7statt", "der", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Geberin dich.", "tokens": ["Ge\u00b7be\u00b7rin", "dich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.20": {"text": "F\u00fchltest du seinen Ku\u00df,", "tokens": ["F\u00fchl\u00b7test", "du", "sei\u00b7nen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Blume der Treue,", "tokens": ["Blu\u00b7me", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Z\u00fcrnst du der Maid,", "tokens": ["Z\u00fcrnst", "du", "der", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Da\u00df dein Leben sie k\u00fcrzte,", "tokens": ["Da\u00df", "dein", "Le\u00b7ben", "sie", "k\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Das nun bald welkende?", "tokens": ["Das", "nun", "bald", "wel\u00b7ken\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Oder lispelst", "tokens": ["O\u00b7der", "lis\u00b7pelst"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Ihre Mahnung", "tokens": ["Ih\u00b7re", "Mah\u00b7nung"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Dem J\u00fcngling zu,", "tokens": ["Dem", "J\u00fcng\u00b7ling", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Ihr Tr\u00e4nenwort:", "tokens": ["Ihr", "Tr\u00e4\u00b7nen\u00b7wort", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "\u00bbvergi\u00df nicht mein!\u00ab", "tokens": ["\u00bb", "ver\u00b7gi\u00df", "nicht", "mein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PTKNEG", "PPOSAT", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}