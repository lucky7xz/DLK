{"textgrid.poem.63107": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fran\u00e7ois Montcorbier, genannt Villon,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fran\u00e7ois Montcorbier, genannt Villon,", "tokens": ["Fran\u00e7ois", "Mont\u00b7cor\u00b7bier", ",", "ge\u00b7nannt", "Vil\u00b7lon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVPP", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geboren Vierzehnhunderteinunddrei\u00dfig,", "tokens": ["Ge\u00b7bo\u00b7ren", "Vier\u00b7zehn\u00b7hun\u00b7der\u00b7tei\u00b7nund\u00b7drei\u00b7\u00dfig", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Sch\u00fcler faul, als Buhler strebsam flei\u00dfig,", "tokens": ["Als", "Sch\u00fc\u00b7ler", "faul", ",", "als", "Buh\u00b7ler", "streb\u00b7sam", "flei\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$,", "KOUS", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus dunkelstem Paris, und darob lichtscheu.", "tokens": ["Aus", "dun\u00b7kels\u00b7tem", "Pa\u00b7ris", ",", "und", "da\u00b7rob", "licht\u00b7scheu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "$,", "KON", "PAV", "ADJD", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit Faltern schwebend, Bl\u00fcten bl\u00fchend, pflichtscheu.", "tokens": ["Mit", "Fal\u00b7tern", "schwe\u00b7bend", ",", "Bl\u00fc\u00b7ten", "bl\u00fc\u00b7hend", ",", "pflicht\u00b7scheu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "NN", "VVPP", "$,", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Bekannt von Meung sur Loire bis Roussillon,", "tokens": ["Be\u00b7kannt", "von", "Meung", "sur", "Loi\u00b7re", "bis", "Rous\u00b7sil\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NE", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Leibpoet des Herzogs von Bourbon", "tokens": ["Der", "Leib\u00b7poet", "des", "Her\u00b7zogs", "von", "Bour\u00b7bon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und Leibpoet des letzten Stra\u00dfenweibs,", "tokens": ["Und", "Leib\u00b7poet", "des", "letz\u00b7ten", "Stra\u00b7\u00dfen\u00b7weibs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Bedacht auf sondre Art des Zeitvertreibs,", "tokens": ["Be\u00b7dacht", "auf", "sond\u00b7re", "Art", "des", "Zeit\u00b7ver\u00b7treibs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Landstreicher, Gauner, Dieb, Zechpreller \u2013 und", "tokens": ["Land\u00b7strei\u00b7cher", ",", "Gau\u00b7ner", ",", "Dieb", ",", "Zech\u00b7prel\u00b7ler", "\u2013", "und"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$(", "KON"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.11": {"text": "H\u00fcndischer oft traktiert als der geringste Hund,", "tokens": ["H\u00fcn\u00b7di\u00b7scher", "oft", "trak\u00b7tiert", "als", "der", "ge\u00b7rings\u00b7te", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Um eines Haares Breite M\u00f6rder gar,", "tokens": ["Um", "ei\u00b7nes", "Haa\u00b7res", "Brei\u00b7te", "M\u00f6r\u00b7der", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mitglied der Bruderschaft der coquillards -", "tokens": ["Mit\u00b7glied", "der", "Bru\u00b7der\u00b7schaft", "der", "co\u00b7quil\u00b7lards"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Liegt hier begraben: was er lebt' und litt,", "tokens": ["Liegt", "hier", "be\u00b7gra\u00b7ben", ":", "was", "er", "lebt'", "und", "litt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "$.", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Teilt er euch in des Meisters Werken mit.", "tokens": ["Teilt", "er", "euch", "in", "des", "Meis\u00b7ters", "Wer\u00b7ken", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Lag seine Stirn im Kot, sein armer Leib im Kofen,", "tokens": ["Lag", "sei\u00b7ne", "Stirn", "im", "Kot", ",", "sein", "ar\u00b7mer", "Leib", "im", "Ko\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Aus seinem Munde klang ein goldner Chor von Strophen.", "tokens": ["Aus", "sei\u00b7nem", "Mun\u00b7de", "klang", "ein", "gold\u00b7ner", "Chor", "von", "Stro\u00b7phen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das erdendunkle Herz entz\u00fcndet Sternenlicht.", "tokens": ["Das", "er\u00b7den\u00b7dunk\u00b7le", "Herz", "ent\u00b7z\u00fcn\u00b7det", "Ster\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als er am Himmelstore angelangt,", "tokens": ["Als", "er", "am", "Him\u00b7mels\u00b7to\u00b7re", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Hat die Madonna selbst gebetet und gebangt.", "tokens": ["Hat", "die", "Ma\u00b7don\u00b7na", "selbst", "ge\u00b7be\u00b7tet", "und", "ge\u00b7bangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gottvater lie\u00df ihn gn\u00e4dig in den Himmel ein:", "tokens": ["Gott\u00b7va\u00b7ter", "lie\u00df", "ihn", "gn\u00e4\u00b7dig", "in", "den", "Him\u00b7mel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil du mich stets gesucht, sollst du willkommen sein.", "tokens": ["Weil", "du", "mich", "stets", "ge\u00b7sucht", ",", "sollst", "du", "will\u00b7kom\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gefunden hast du mich. Du bist Poet nicht mehr.", "tokens": ["Ge\u00b7fun\u00b7den", "hast", "du", "mich", ".", "Du", "bist", "Po\u00b7et", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Tritt als ein Engel in das selige Engelheer.", "tokens": ["Tritt", "als", "ein", "En\u00b7gel", "in", "das", "se\u00b7li\u00b7ge", "En\u00b7gel\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Da l\u00e4chelt Villon ernst \u2013 und schluchzt mit einemmal:", "tokens": ["Da", "l\u00e4\u00b7chelt", "Vil\u00b7lon", "ernst", "\u2013", "und", "schluchzt", "mit", "ei\u00b7nem\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "$(", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich komme aus der allertiefsten H\u00f6lle Qual.", "tokens": ["Ich", "kom\u00b7me", "aus", "der", "al\u00b7ler\u00b7tiefs\u00b7ten", "H\u00f6l\u00b7le", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "L\u00e4\u00dft du die M\u00f6rder, Diebe, F\u00e4lscher, Ehebrecher,", "tokens": ["L\u00e4\u00dft", "du", "die", "M\u00f6r\u00b7der", ",", "Die\u00b7be", ",", "F\u00e4l\u00b7scher", ",", "E\u00b7heb\u00b7re\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Die Dirnen, R\u00e4uber, S\u00e4ufer, Gauner, Degenstecher,", "tokens": ["Die", "Dir\u00b7nen", ",", "R\u00e4u\u00b7ber", ",", "S\u00e4u\u00b7fer", ",", "Gau\u00b7ner", ",", "De\u00b7gen\u00b7ste\u00b7cher", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die meine Br\u00fcder sind, nicht in den Himmel ein,", "tokens": ["Die", "mei\u00b7ne", "Br\u00fc\u00b7der", "sind", ",", "nicht", "in", "den", "Him\u00b7mel", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.30": {"text": "So soll die Seligkeit mir nicht vorhanden sein.", "tokens": ["So", "soll", "die", "Se\u00b7lig\u00b7keit", "mir", "nicht", "vor\u00b7han\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nicht eine Stunde blieb ich selig, wenn ich w\u00fc\u00dft,", "tokens": ["Nicht", "ei\u00b7ne", "Stun\u00b7de", "blieb", "ich", "se\u00b7lig", ",", "wenn", "ich", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df in der H\u00f6ll ein armer Bruder leiden m\u00fc\u00dft.", "tokens": ["Da\u00df", "in", "der", "H\u00f6ll", "ein", "ar\u00b7mer", "Bru\u00b7der", "lei\u00b7den", "m\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gottvater, lebe wohl! Ich will kein Heuchlergl\u00fcck!", "tokens": ["Gott\u00b7va\u00b7ter", ",", "le\u00b7be", "wohl", "!", "Ich", "will", "kein", "Heuc\u00b7hler\u00b7gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Zu meinen Br\u00fcdern kehr ich in die H\u00f6ll zur\u00fcck.", "tokens": ["Zu", "mei\u00b7nen", "Br\u00fc\u00b7dern", "kehr", "ich", "in", "die", "H\u00f6ll", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und bin erst wieder hier, wenn die Posaune lehrt,", "tokens": ["Und", "bin", "erst", "wie\u00b7der", "hier", ",", "wenn", "die", "Po\u00b7sau\u00b7ne", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df Gott dem \u00c4rmsten auch das himmlisch Reich gew\u00e4hrt.", "tokens": ["Da\u00df", "Gott", "dem", "\u00c4rms\u00b7ten", "auch", "das", "himm\u00b7lisch", "Reich", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df Gott dem Letzten auch ob seiner Tat nicht grollt,", "tokens": ["Da\u00df", "Gott", "dem", "Letz\u00b7ten", "auch", "ob", "sei\u00b7ner", "Tat", "nicht", "grollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die ohne Gott nicht w\u00e4r \u2013 denn Gott hat ", "tokens": ["Die", "oh\u00b7ne", "Gott", "nicht", "w\u00e4r", "\u2013", "denn", "Gott", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PTKNEG", "VAFIN", "$(", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Schenk allen Erdenwandrern die ersehnte Ruh! \u2013", "tokens": ["Schenk", "al\u00b7len", "Er\u00b7den\u00b7wand\u00b7rern", "die", "er\u00b7sehn\u00b7te", "Ruh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und hob die Hand zu Gott. Und sank der Tiefe zu.", "tokens": ["Und", "hob", "die", "Hand", "zu", "Gott", ".", "Und", "sank", "der", "Tie\u00b7fe", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}