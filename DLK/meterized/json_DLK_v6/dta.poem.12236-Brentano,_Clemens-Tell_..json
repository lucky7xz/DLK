{"dta.poem.12236": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tell .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Ury bey den Linden               ", "tokens": ["Zu", "U\u00b7ry", "bey", "den", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Vogt steckt auf den Huth,", "tokens": ["Der", "Vogt", "steckt", "auf", "den", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach: Ich will den finden,", "tokens": ["Und", "sprach", ":", "Ich", "will", "den", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dem kein Ehr anthut.", "tokens": ["Der", "dem", "kein", "Ehr", "an\u00b7thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ich that nicht Ehr dem Huthe,", "tokens": ["Ich", "that", "nicht", "Ehr", "dem", "Hu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sah ihn k\u00fchnlich an,", "tokens": ["Ich", "sah", "ihn", "k\u00fchn\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sagt: Du traust dem Muthe,", "tokens": ["Er", "sagt", ":", "Du", "traust", "dem", "Mu\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Will sehn, ob du ein Mann! \u2014", "tokens": ["Will", "sehn", ",", "ob", "du", "ein", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Er fa\u00dft den Anschlag eitel,", "tokens": ["Er", "fa\u00dft", "den", "An\u00b7schlag", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ich nun schie\u00df geschwind", "tokens": ["Da\u00df", "ich", "nun", "schie\u00df", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Den Apfel von dem Scheitel", "tokens": ["Den", "Ap\u00b7fel", "von", "dem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Meinem allerliebsten Kind.", "tokens": ["Mei\u00b7nem", "al\u00b7ler\u00b7liebs\u00b7ten", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach Vater, was hab' ich gethan,", "tokens": ["Ach", "Va\u00b7ter", ",", "was", "hab'", "ich", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du mich also bindest an?", "tokens": ["Da\u00df", "du", "mich", "al\u00b7so", "bin\u00b7dest", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Kind schweig still, mein Herz schonst gro\u00df,", "tokens": ["Mein", "Kind", "schweig", "still", ",", "mein", "Herz", "schonst", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hoff, es soll mein Pfeilgescho\u00df", "tokens": ["Ich", "hoff", ",", "es", "soll", "mein", "Pfeil\u00b7ge\u00b7scho\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Schaden dir bereiten,", "tokens": ["Kein", "Scha\u00b7den", "dir", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du tr\u00e4gst kein Schuld und ich kein S\u00fcnd,", "tokens": ["Du", "tr\u00e4gst", "kein", "Schuld", "und", "ich", "kein", "S\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ruf nur zu Gott mit mir mein Kind,", "tokens": ["Ruf", "nur", "zu", "Gott", "mit", "mir", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott wird den Pfeil schon leiten.", "tokens": ["Gott", "wird", "den", "Pfeil", "schon", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Halt auf dein Haupt, richt dich nur auf,", "tokens": ["Halt", "auf", "dein", "Haupt", ",", "richt", "dich", "nur", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Gottes Namen schie\u00df ich drauf,", "tokens": ["In", "Got\u00b7tes", "Na\u00b7men", "schie\u00df", "ich", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der gerechte Gott soll leben!", "tokens": ["Der", "ge\u00b7rech\u00b7te", "Gott", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Vater mein, Gott mit uns h\u00e4lt,", "tokens": ["Ach", "Va\u00b7ter", "mein", ",", "Gott", "mit", "uns", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPOSAT", "$,", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der Apfel von dem Scheitel f\u00e4llt,", "tokens": ["Der", "Ap\u00b7fel", "von", "dem", "Schei\u00b7tel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott hat den Segen geben.", "tokens": ["Gott", "hat", "den", "Se\u00b7gen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}