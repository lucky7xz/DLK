{"dta.poem.9975": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alle die mich haben nicht/ meynen den noch mich zu haben/", "tokens": ["Al\u00b7le", "die", "mich", "ha\u00b7ben", "nicht", "/", "mey\u00b7nen", "den", "noch", "mich", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRELS", "PPER", "VAFIN", "PTKNEG", "$(", "VVFIN", "ART", "ADV", "PPER", "PTKZU", "VAINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Gro\u00df und kleine nennt man mich/ bin weit \u00fcber andre gaben.", "tokens": ["Gro\u00df", "und", "klei\u00b7ne", "nennt", "man", "mich", "/", "bin", "weit", "\u00fc\u00b7ber", "and\u00b7re", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "VVFIN", "PIS", "PPER", "$(", "VAFIN", "ADJD", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Hat aus ungl\u00fcck einer mich in dem leben gleich verlohren/", "tokens": ["Hat", "aus", "un\u00b7gl\u00fcck", "ei\u00b7ner", "mich", "in", "dem", "le\u00b7ben", "gleich", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "PPER", "APPR", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Werd in seinem sterben ich bey ihm als wie neugebohren.", "tokens": ["Werd", "in", "sei\u00b7nem", "ster\u00b7ben", "ich", "bey", "ihm", "als", "wie", "neu\u00b7ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "VVFIN", "PPER", "APPR", "PPER", "KOUS", "KOKOM", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}