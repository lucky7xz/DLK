{"textgrid.poem.36708": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Subiaco", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne bliebst du im Kloster, im Roseng\u00e4rtchen, das lieblich", "tokens": ["Ger\u00b7ne", "bliebst", "du", "im", "Klos\u00b7ter", ",", "im", "Ro\u00b7sen\u00b7g\u00e4rt\u00b7chen", ",", "das", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie ein M\u00e4rchen so hoch \u00fcber dem Anio h\u00e4ngt.", "tokens": ["Wie", "ein", "M\u00e4r\u00b7chen", "so", "hoch", "\u00fc\u00b7ber", "dem", "A\u00b7nio", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber ein Frauengeschlecht von vollendeter \u00fcppiger Sch\u00f6nheit", "tokens": ["A\u00b7ber", "ein", "Frau\u00b7en\u00b7ge\u00b7schlecht", "von", "voll\u00b7en\u00b7de\u00b7ter", "\u00fcp\u00b7pi\u00b7ger", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zieht aus dem Himmel, es zieht dich auf die Erde zur\u00fcck.", "tokens": ["Zieht", "aus", "dem", "Him\u00b7mel", ",", "es", "zieht", "dich", "auf", "die", "Er\u00b7de", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}