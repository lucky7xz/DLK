{"dta.poem.57": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  stolze Pfau .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Pfau den die Natur, mit sch\u00f6nen Fe-\ndern schm\u00fckt,", "tokens": ["Ein", "Pfau", "den", "die", "Na\u00b7tur", ",", "mit", "sch\u00f6\u00b7nen", "Fe", "dern", "schm\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "$,", "APPR", "ADJA", "TRUNC", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit einem langen Schweif vor andern", "tokens": ["Mit", "ei\u00b7nem", "lan\u00b7gen", "Schweif", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weis seinen Vorzug wol, er dreht die", "tokens": ["Weis", "sei\u00b7nen", "Vor\u00b7zug", "wol", ",", "er", "dreht", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wolt er seine Pracht der ganzen Welt erz\u00e4h-", "tokens": ["Als", "wolt", "er", "sei\u00b7ne", "Pracht", "der", "gan\u00b7zen", "Welt", "er\u00b7z\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er br\u00fcstet sich damit und dehnet seinen Schwanz,", "tokens": ["Er", "br\u00fcs\u00b7tet", "sich", "da\u00b7mit", "und", "deh\u00b7net", "sei\u00b7nen", "Schwanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit ein jeder seh den Strahlenreichen Glanz:", "tokens": ["Da\u00b7mit", "ein", "je\u00b7der", "seh", "den", "Strah\u00b7len\u00b7rei\u00b7chen", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und alsdenn blizt sein Schweif gleich einen Regen-", "tokens": ["Und", "als\u00b7denn", "blizt", "sein", "Schweif", "gleich", "ei\u00b7nen", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der in der Luft erscheint, mit Farben \u00fcberzogen.", "tokens": ["Der", "in", "der", "Luft", "er\u00b7scheint", ",", "mit", "Far\u00b7ben", "\u00fc\u00b7berz\u00b7o\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Finger der Natur der ihn also gemahlt,", "tokens": ["Der", "Fin\u00b7ger", "der", "Na\u00b7tur", "der", "ihn", "al\u00b7so", "ge\u00b7mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "Das bald ein feurig Gold, daraus ein Saphir", "tokens": ["Das", "bald", "ein", "feu\u00b7rig", "Gold", ",", "da\u00b7raus", "ein", "Sa\u00b7phir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJD", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bald ein schmaragdnes Gr\u00fcn, von seinen Federn", "tokens": ["Bald", "ein", "schma\u00b7ragd\u00b7nes", "Gr\u00fcn", ",", "von", "sei\u00b7nen", "Fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Hat aller Farben Schmuk hier wunderbahr verei-", "tokens": ["Hat", "al\u00b7ler", "Far\u00b7ben", "Schmuk", "hier", "wun\u00b7der\u00b7bahr", "ver\u00b7ei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nachdem vom Sonnenlicht, dies sch\u00f6n geschw\u00e4nz-", "tokens": ["Nach\u00b7dem", "vom", "Son\u00b7nen\u00b7licht", ",", "dies", "sch\u00f6n", "ge\u00b7schw\u00e4nz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "$,", "PDS", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das hin und wieder geht, der Strahlen Abglanz", "tokens": ["Das", "hin", "und", "wie\u00b7der", "geht", ",", "der", "Strah\u00b7len", "Ab\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PTKVZ", "KON", "ADV", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nachdem ver\u00e4ndern sich der Federn bunte Strah-", "tokens": ["Nach\u00b7dem", "ver\u00b7\u00e4n\u00b7dern", "sich", "der", "Fe\u00b7dern", "bun\u00b7te", "Strah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "PRF", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die sich bald gelb, meist gr\u00fcn vor unsern Augen", "tokens": ["Die", "sich", "bald", "gelb", ",", "meist", "gr\u00fcn", "vor", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wahr ist es da\u00df der Schweif am Pfau recht Wun-", "tokens": ["Wahr", "ist", "es", "da\u00df", "der", "Schweif", "am", "Pfau", "recht", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und da\u00df er Schimmerreich und lieblich anzusehn:", "tokens": ["Und", "da\u00df", "er", "Schim\u00b7mer\u00b7reich", "und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein er gl\u00e4nzet nur, von aussen an den Fl\u00fcgeln", "tokens": ["Al\u00b7lein", "er", "gl\u00e4n\u00b7zet", "nur", ",", "von", "aus\u00b7sen", "an", "den", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die um und um besezt mit lauter Blendungs-Spie-", "tokens": ["Die", "um", "und", "um", "be\u00b7sezt", "mit", "lau\u00b7ter", "Blen\u00b7dungs\u00b7Spie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "KON", "APPR", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und sieht etwa der Pfau die schwarzen F\u00fcsse an,", "tokens": ["Und", "sieht", "et\u00b7wa", "der", "Pfau", "die", "schwar\u00b7zen", "F\u00fcs\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die garstig anzusehn, die er nicht leiden kan;", "tokens": ["Die", "gars\u00b7tig", "an\u00b7zu\u00b7sehn", ",", "die", "er", "nicht", "lei\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVIZU", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So f\u00e4llt der stolze Sinn, da lernet er erkennen,", "tokens": ["So", "f\u00e4llt", "der", "stol\u00b7ze", "Sinn", ",", "da", "ler\u00b7net", "er", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was sch\u00f6nes an ihm ist, sey nur sein Kleid zu nen-", "tokens": ["Was", "sch\u00f6\u00b7nes", "an", "ihm", "ist", ",", "sey", "nur", "sein", "Kleid", "zu", "nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "APPR", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+-+--+-+++-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Gewis! ein wahres Bild von Menschen in der", "tokens": ["Ge\u00b7wis", "!", "ein", "wah\u00b7res", "Bild", "von", "Men\u00b7schen", "in", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die durch den leeren Wind des Stolzes aufge-", "tokens": ["Die", "durch", "den", "lee\u00b7ren", "Wind", "des", "Stol\u00b7zes", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die in dem Lauf der Welt mit G\u00fctern sind begl\u00fck-", "tokens": ["Die", "in", "dem", "Lauf", "der", "Welt", "mit", "G\u00fc\u00b7tern", "sind", "be\u00b7gl\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hingegen aber nicht mit Tugend ausgeschm\u00fckket.", "tokens": ["Hin\u00b7ge\u00b7gen", "a\u00b7ber", "nicht", "mit", "Tu\u00b7gend", "aus\u00b7ge\u00b7schm\u00fck\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.29": {"text": "Die bilden sich gar viel, auf ihrem blanken Schein,", "tokens": ["Die", "bil\u00b7den", "sich", "gar", "viel", ",", "auf", "ih\u00b7rem", "blan\u00b7ken", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Auf ihren reichen Schaz, auf ihren Goldklump", "tokens": ["Auf", "ih\u00b7ren", "rei\u00b7chen", "Schaz", ",", "auf", "ih\u00b7ren", "Gold\u00b7klump"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da ihnen Tugend fehlt, und Wiz, Geschiklichkeiten,", "tokens": ["Da", "ih\u00b7nen", "Tu\u00b7gend", "fehlt", ",", "und", "Wiz", ",", "Ge\u00b7schik\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So mu\u00df der Kleider Schmuk sie in der Welt", "tokens": ["So", "mu\u00df", "der", "Klei\u00b7der", "Schmuk", "sie", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Der Pfau ist auch ein Bild von deren Eitelkeit,", "tokens": ["Der", "Pfau", "ist", "auch", "ein", "Bild", "von", "de\u00b7ren", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die falscher D\u00fcnkel triegt, als wenn ein herrlich", "tokens": ["Die", "fal\u00b7scher", "D\u00fcn\u00b7kel", "triegt", ",", "als", "wenn", "ein", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Das in das Auge prahlt, sie k\u00f6nte sch\u00f6ner ma-", "tokens": ["Das", "in", "das", "Au\u00b7ge", "prahlt", ",", "sie", "k\u00f6n\u00b7te", "sch\u00f6\u00b7ner", "ma"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie br\u00fcsten sich damit, dar\u00fcber kluge lachen.", "tokens": ["Sie", "br\u00fcs\u00b7ten", "sich", "da\u00b7mit", ",", "da\u00b7r\u00fc\u00b7ber", "klu\u00b7ge", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$,", "PAV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie meinen weil ihr Leib mit sch\u00f6nen Schmuk ver-", "tokens": ["Sie", "mei\u00b7nen", "weil", "ihr", "Leib", "mit", "sch\u00f6\u00b7nen", "Schmuk", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "So w\u00fcrden sie dadurch der Sch\u00f6nheit Ebenbild;", "tokens": ["So", "w\u00fcr\u00b7den", "sie", "da\u00b7durch", "der", "Sch\u00f6n\u00b7heit", "E\u00b7ben\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie wollen Stand und Rang, nicht nach Verdienst", "tokens": ["Sie", "wol\u00b7len", "Stand", "und", "Rang", ",", "nicht", "nach", "Ver\u00b7dienst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "$,", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Nur nach verbr\u00e4mten Schmuk erborgter Sch\u00f6n-", "tokens": ["Nur", "nach", "ver\u00b7br\u00e4m\u00b7ten", "Schmuk", "er\u00b7borg\u00b7ter", "Sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Jhr Tokken! lernet doch hie euren albern Sinn,", "tokens": ["Ihr", "Tok\u00b7ken", "!", "ler\u00b7net", "doch", "hie", "eu\u00b7ren", "al\u00b7bern", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Komts auf die Kleider an, so tretet unten hin:", "tokens": ["Komts", "auf", "die", "Klei\u00b7der", "an", ",", "so", "tre\u00b7tet", "un\u00b7ten", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der Pfau das sch\u00f6ne Thier; wird mit den schwarzen", "tokens": ["Der", "Pfau", "das", "sch\u00f6\u00b7ne", "Thier", ";", "wird", "mit", "den", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "F\u00fcr euch nach diesem Schlus zuerst hertreten m\u00fcssen.", "tokens": ["F\u00fcr", "euch", "nach",