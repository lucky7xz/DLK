{"textgrid.poem.28807": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hofe-Leute, hohe Teufel; ist es nicht zu viel gesaget?", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hofe-Leute, hohe Teufel; ist es nicht zu viel gesaget?", "tokens": ["Ho\u00b7fe\u00b7Leu\u00b7te", ",", "ho\u00b7he", "Teu\u00b7fel", ";", "ist", "es", "nicht", "zu", "viel", "ge\u00b7sa\u00b7get", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "PTKA", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nein, weil mancher arme Leute sehrer als der Teuffel plaget.", "tokens": ["Nein", ",", "weil", "man\u00b7cher", "ar\u00b7me", "Leu\u00b7te", "seh\u00b7rer", "als", "der", "Teuf\u00b7fel", "pla\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIAT", "ADJA", "NN", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Falschheit und Betriegligkeiten, Hinderlist, Verleumdung, L\u00fcgen", "tokens": ["Falschheit", "und", "Be\u00b7trieg\u00b7lig\u00b7kei\u00b7ten", ",", "Hin\u00b7der\u00b7list", ",", "Ver\u00b7leum\u00b7dung", ",", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Sind de\u00df Hofes Meisterst\u00fccke, sind de\u00df Teuffels sein Vergn\u00fcgen.", "tokens": ["Sind", "de\u00df", "Ho\u00b7fes", "Meis\u00b7ter\u00b7st\u00fc\