{"dta.poem.11953": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bedenke, wenn du gehst, da\u00df nichts von dir hier bleibt,", "tokens": ["Be\u00b7den\u00b7ke", ",", "wenn", "du", "gehst", ",", "da\u00df", "nichts", "von", "dir", "hier", "bleibt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als was ein Wort, ein Werk von dir in Herzen schreibt.", "tokens": ["Als", "was", "ein", "Wort", ",", "ein", "Werk", "von", "dir", "in", "Her\u00b7zen", "schreibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bedenke, wenn du gehst, da\u00df du nichts nimmst von hier,", "tokens": ["Be\u00b7den\u00b7ke", ",", "wenn", "du", "gehst", ",", "da\u00df", "du", "nichts", "nimmst", "von", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als was von dort war und nach dort gestrebt in dir.", "tokens": ["Als", "was", "von", "dort", "war", "und", "nach", "dort", "ge\u00b7strebt", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADV", "VAFIN", "KON", "APPR", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Heil dir, wenn du gehst und beides dies empfindest,", "tokens": ["O", "Heil", "dir", ",", "wenn", "du", "gehst", "und", "bei\u00b7des", "dies", "emp\u00b7fin\u00b7dest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "PIS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du hier bleibest und dich dr\u00fcben wieder findest.", "tokens": ["Da\u00df", "du", "hier", "blei\u00b7best", "und", "dich", "dr\u00fc\u00b7ben", "wie\u00b7der", "fin\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}