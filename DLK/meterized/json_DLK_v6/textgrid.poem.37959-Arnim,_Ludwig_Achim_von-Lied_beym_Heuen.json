{"textgrid.poem.37959": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lied beym Heuen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es h\u00e4tt ein Biedermann ein Weib,", "tokens": ["Es", "h\u00e4tt", "ein", "Bie\u00b7der\u00b7mann", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr T\u00fcck wollt sie nit lan,", "tokens": ["Ihr", "T\u00fcck", "wollt", "sie", "nit", "lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das macht ihr grader stolzer Leib,", "tokens": ["Das", "macht", "ihr", "gra\u00b7der", "stol\u00b7zer", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie bat ihren Mann,", "tokens": ["Da\u00df", "sie", "bat", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und da\u00df er f\u00fchr ins Heu, ins He", "tokens": ["Und", "da\u00df", "er", "f\u00fchr", "ins", "Heu", ",", "ins", "He"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach Gromat in das Ge", "tokens": ["Nach", "Gro\u00b7mat", "in", "das", "Ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mann der wollt erf\u00fcllen,", "tokens": ["Der", "Mann", "der", "wollt", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Frauen ihren Willen,", "tokens": ["Der", "Frau\u00b7en", "ih\u00b7ren", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er stieg zu aller \u00f6berst,", "tokens": ["Er", "stieg", "zu", "al\u00b7ler", "\u00f6\u00b7berst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl auf die Dillen,", "tokens": ["Wohl", "auf", "die", "Dil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er sprach, er wollt ins Heu, ins He", "tokens": ["Er", "sprach", ",", "er", "wollt", "ins", "Heu", ",", "ins", "He"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach Gromat in das Ge", "tokens": ["Nach", "Gro\u00b7mat", "in", "das", "Ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}