{"textgrid.poem.47423": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[das Aethermeer, das n\u00e4chtige]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Aethermeer, das n\u00e4chtige,", "tokens": ["Das", "A\u00b7e\u00b7ther\u00b7meer", ",", "das", "n\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ergl\u00e4nzet in des Mondes hehrem Glanze;", "tokens": ["Er\u00b7gl\u00e4n\u00b7zet", "in", "des", "Mon\u00b7des", "heh\u00b7rem", "Glan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon blicket er, der pr\u00e4chtige,", "tokens": ["Schon", "bli\u00b7cket", "er", ",", "der", "pr\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch's dunkle Laub als goldne Pomeranze.", "tokens": ["Durch's", "dunk\u00b7le", "Laub", "als", "gold\u00b7ne", "Po\u00b7me\u00b7ran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es streuet ihm die Lilie", "tokens": ["Es", "streu\u00b7et", "ihm", "die", "Li\u00b7lie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weihrauch empor; es hangt an ihm bezaubert", "tokens": ["Weih\u00b7rauch", "em\u00b7por", ";", "es", "hangt", "an", "ihm", "be\u00b7zau\u00b7bert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "B\u00fclb\u00fcl, die tonkunstm\u00e4chtige,", "tokens": ["B\u00fcl\u00b7b\u00fcl", ",", "die", "ton\u00b7kunst\u00b7m\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dfet ihn mit ihrer s\u00fc\u00dfen Stanze.", "tokens": ["Und", "gr\u00fc\u00b7\u00dfet", "ihn", "mit", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Stan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, ach, wie stolz erhebt er sich,", "tokens": ["Doch", ",", "ach", ",", "wie", "stolz", "er\u00b7hebt", "er", "sich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit er nie zu Hoffnungen, zu sch\u00f6nen,", "tokens": ["Da\u00b7mit", "er", "nie", "zu", "Hoff\u00b7nun\u00b7gen", ",", "zu", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die S\u00e4ngerin berechtige,", "tokens": ["Die", "S\u00e4n\u00b7ge\u00b7rin", "be\u00b7rech\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nie die stille, liebevolle Pflanze! \u2013", "tokens": ["Und", "nie", "die", "stil\u00b7le", ",", "lie\u00b7be\u00b7vol\u00b7le", "Pflan\u00b7ze", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du bist der Mond, die Lilie", "tokens": ["Du", "bist", "der", "Mond", ",", "die", "Li\u00b7lie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist mein Gem\u00fcth und Nachtigall die Lippe,", "tokens": ["Ist", "mein", "Ge\u00b7m\u00fcth", "und", "Nach\u00b7ti\u00b7gall", "die", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die dir umsonst and\u00e4chtige", "tokens": ["Die", "dir", "um\u00b7sonst", "an\u00b7d\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebete weiht die Sommernacht, die ganze.", "tokens": ["Ge\u00b7be\u00b7te", "weiht", "die", "Som\u00b7mer\u00b7nacht", ",", "die", "gan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}