{"textgrid.poem.48644": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Eurus sich noch streubet,", "tokens": ["Weil", "Eu\u00b7rus", "sich", "noch", "streu\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "best\u00fcrmt die gro\u00dfe Welt,", "tokens": ["be\u00b7st\u00fcrmt", "die", "gro\u00b7\u00dfe", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so trauret Wald und Feld", "tokens": ["so", "trau\u00b7ret", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und was di\u00df Rund umleibet.", "tokens": ["und", "was", "di\u00df", "Rund", "um\u00b7lei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur ich bin au\u00dfer Kummer:", "tokens": ["Nur", "ich", "bin", "au\u00b7\u00dfer", "Kum\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wenn meine ", "tokens": ["wenn", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "mich in die Arme nimmt,", "tokens": ["mich", "in", "die", "Ar\u00b7me", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "ihr H\u00e4upt ist mir der Lenz,", "tokens": ["ihr", "H\u00e4upt", "ist", "mir", "der", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "ihr Antlitz Sommer.", "tokens": ["ihr", "Ant\u00b7litz", "Som\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}