{"textgrid.poem.64858": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O ", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Den Schmerz noch kann durchdringen,", "tokens": ["Den", "Schmerz", "noch", "kann", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der auf des Lebens wildem Meer", "tokens": ["Der", "auf", "des", "Le\u00b7bens", "wil\u00b7dem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Well' und Sturm mu\u00df ringen!", "tokens": ["Mit", "Well'", "und", "Sturm", "mu\u00df", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sucht und findet eine Bahn \u2013", "tokens": ["Er", "sucht", "und", "fin\u00b7det", "ei\u00b7ne", "Bahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fcckselig, wer noch leiden kann!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "wer", "noch", "lei\u00b7den", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Busen, den noch Weh durchbebt,", "tokens": ["Der", "Bu\u00b7sen", ",", "den", "noch", "Weh", "durch\u00b7bebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht auch der Lust noch offen,", "tokens": ["Steht", "auch", "der", "Lust", "noch", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die n\u00e4cht'gen Wolken schwebt", "tokens": ["Und", "durch", "die", "n\u00e4cht'\u00b7gen", "Wol\u00b7ken", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein fromm' Messiashoffen;", "tokens": ["Ein", "fromm'", "Mes\u00b7si\u00b7as\u00b7hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und viel besa\u00df, wer viel verlor \u2013", "tokens": ["Und", "viel", "be\u00b7sa\u00df", ",", "wer", "viel", "ver\u00b7lor", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus reicher Saat keimt's reich empor.", "tokens": ["Aus", "rei\u00b7cher", "Saat", "keimt's", "reich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer seiner W\u00fcnsche Bl\u00fcthenflor", "tokens": ["Wer", "sei\u00b7ner", "W\u00fcn\u00b7sche", "Bl\u00fc\u00b7then\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Thr\u00e4nen kann begie\u00dfen,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "kann", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem werden sie noch einst hervor", "tokens": ["Dem", "wer\u00b7den", "sie", "noch", "einst", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erf\u00fcllt als Blumen sprie\u00dfen;", "tokens": ["Er\u00b7f\u00fcllt", "als", "Blu\u00b7men", "sprie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ob die nun heut', ob morgen sein,", "tokens": ["Ob", "die", "nun", "heut'", ",", "ob", "mor\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "$,", "KOUS", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird wohl zuletzt dasselbe sein.", "tokens": ["Wird", "wohl", "zu\u00b7letzt", "das\u00b7sel\u00b7be", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PDAT", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch elend, wahrhaft elend ist,", "tokens": ["Doch", "e\u00b7lend", ",", "wahr\u00b7haft", "e\u00b7lend", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der selbst vom Schmerz versto\u00dfen,", "tokens": ["Der", "selbst", "vom", "Schmerz", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der, da die Lust ihn doch nicht gr\u00fc\u00dft,", "tokens": ["Der", ",", "da", "die", "Lust", "ihn", "doch", "nicht", "gr\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Gram selbst ausgeschlossen;", "tokens": ["Vom", "Gram", "selbst", "aus\u00b7ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "De\u00df Nacht nicht schwarz, de\u00df Tag nicht klar,", "tokens": ["De\u00df", "Nacht", "nicht", "schwarz", ",", "de\u00df", "Tag", "nicht", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$,", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O der ist elend, ist's f\u00fcrwahr!", "tokens": ["O", "der", "ist", "e\u00b7lend", ",", "ist's", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den kein Verlangen mehr bewegt,", "tokens": ["Den", "kein", "Ver\u00b7lan\u00b7gen", "mehr", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein schmerzenfreudig' Sehnen,", "tokens": ["Kein", "schmer\u00b7zen\u00b7freu\u00b7dig'", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df Busen keinen Wunsch mehr hegt,", "tokens": ["De\u00df", "Bu\u00b7sen", "kei\u00b7nen", "Wunsch", "mehr", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Auge ohne Thr\u00e4nen. \u2013", "tokens": ["De\u00df", "Au\u00b7ge", "oh\u00b7ne", "Thr\u00e4\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja elend, elend sicherlich", "tokens": ["Ja", "e\u00b7lend", ",", "e\u00b7lend", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "ADJD", "$,", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Jeder, der so ist, wie ich", "tokens": ["Ist", "Je\u00b7der", ",", "der", "so", "ist", ",", "wie", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ADV", "VAFIN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}