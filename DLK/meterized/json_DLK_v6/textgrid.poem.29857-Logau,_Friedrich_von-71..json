{"textgrid.poem.29857": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, f\u00fcr dem der Mensch so fleucht und so erschrickt,", "tokens": ["Der", "Tod", ",", "f\u00fcr", "dem", "der", "Mensch", "so", "fleucht", "und", "so", "er\u00b7schrickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wehrt an ihm selbst so lang, als lang ein Auge blickt.", "tokens": ["Wehrt", "an", "ihm", "selbst", "so", "lang", ",", "als", "lang", "ein", "Au\u00b7ge", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Todes Furcht ist tod mehr als der Tod; der Tod", "tokens": ["De\u00df", "To\u00b7des", "Furcht", "ist", "tod", "mehr", "als", "der", "Tod", ";", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "PIAT", "KOKOM", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verk\u00fcrtzt, was ihn verg\u00e4llt: der Furchte bittre Noth.", "tokens": ["Ver\u00b7k\u00fcrtzt", ",", "was", "ihn", "ver\u00b7g\u00e4llt", ":", "der", "Furch\u00b7te", "bitt\u00b7re", "Noth", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVPP", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}