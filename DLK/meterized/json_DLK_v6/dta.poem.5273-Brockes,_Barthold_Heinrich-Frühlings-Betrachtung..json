{"dta.poem.5273": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lieber Mensch, schau, wie im Lentzen", "tokens": ["Lie\u00b7ber", "Mensch", ",", "schau", ",", "wie", "im", "Lent\u00b7zen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "PTKVZ", "$,", "PWAV", "APPRART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Alle Dinge herrlich gl\u00e4ntzen!", "tokens": ["Al\u00b7le", "Din\u00b7ge", "herr\u00b7lich", "gl\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Crcaturen Pracht", "tokens": ["Von", "der", "Cr\u00b7ca\u00b7tu\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird man itzt recht angelacht.", "tokens": ["Wird", "man", "itzt", "recht", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau, wie alles sich erfreuet!", "tokens": ["Schau", ",", "wie", "al\u00b7les", "sich", "er\u00b7freu\u00b7et", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau, wie alles sich erneuet!", "tokens": ["Schau", ",", "wie", "al\u00b7les", "sich", "er\u00b7neu\u00b7et", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Wercke der Natur", "tokens": ["Al\u00b7le", "Wer\u00b7cke", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aendern Farben und Figur.", "tokens": ["A\u00b7en\u00b7dern", "Far\u00b7ben", "und", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Schau die Luft, wie rein, wie klar!", "tokens": ["Schau", "die", "Luft", ",", "wie", "rein", ",", "wie", "klar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schau, wie j\u00fcngst noch welcke Felder,", "tokens": ["Schau", ",", "wie", "j\u00fcngst", "noch", "wel\u00b7cke", "Fel\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Schau, wie j\u00fcngst noch d\u00fcrre W\u00e4lder,", "tokens": ["Schau", ",", "wie", "j\u00fcngst", "noch", "d\u00fcr\u00b7re", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00f6h\u2019n und Tiefen unsrer Erden", "tokens": ["H\u00f6h'n", "und", "Tie\u00b7fen", "uns\u00b7rer", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gr\u00fcn, und schnell belaubet werden!", "tokens": ["Gr\u00fcn", ",", "und", "schnell", "be\u00b7lau\u00b7bet", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Was noch gestern sumpfig war,", "tokens": ["Was", "noch", "ge\u00b7stern", "sump\u00b7fig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Wiesen, welche gantz verschlemmet,", "tokens": ["Wie\u00b7sen", ",", "wel\u00b7che", "gantz", "ver\u00b7schlem\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Uebersp\u00fclet, \u00fcberschwemmet,", "tokens": ["Ue\u00b7ber\u00b7sp\u00fc\u00b7let", ",", "\u00fc\u00b7bersc\u00b7hwem\u00b7met", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "(da die Wasser itzt verseigen)", "tokens": ["(", "da", "die", "Was\u00b7ser", "itzt", "ver\u00b7sei\u00b7gen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Scheinen aus der Fluth zu steigen,", "tokens": ["Schei\u00b7nen", "aus", "der", "Fluth", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und in einen gr\u00fcnen Sammt,", "tokens": ["Und", "in", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Sammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Drauf ein Gold von Blumen flammt,", "tokens": ["Drauf", "ein", "Gold", "von", "Blu\u00b7men", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Eingekleidet, sich zu zeigen.", "tokens": ["Ein\u00b7ge\u00b7klei\u00b7det", ",", "sich", "zu", "zei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Tieff\u2019 und unwegbare Wege,", "tokens": ["Tief\u00b7f'", "und", "un\u00b7weg\u00b7ba\u00b7re", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Schl\u00fcpfrich- schwartz- und weiche Stege", "tokens": ["Schl\u00fcpf\u00b7rich", "schwartz", "und", "wei\u00b7che", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["TRUNC", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sind jetzt brauchbar, wei\u00df und hart.", "tokens": ["Sind", "jetzt", "brauch\u00b7bar", ",", "wei\u00df", "und", "hart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Alles gl\u00e4ntzet, alles gl\u00fchet,", "tokens": ["Al\u00b7les", "gl\u00e4nt\u00b7zet", ",", "al\u00b7les", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Alles funckelt, alles bl\u00fchet,", "tokens": ["Al\u00b7les", "fun\u00b7ckelt", ",", "al\u00b7les", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Durch der Sonnen Gegenwart.", "tokens": ["Durch", "der", "Son\u00b7nen", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}