{"textgrid.poem.26245": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es k\u00e4mpft der deutschen Erde Geist", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es k\u00e4mpfen nicht nur Mann gen Mann", "tokens": ["Es", "k\u00e4mp\u00b7fen", "nicht", "nur", "Mann", "gen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Fu\u00df und hoch zu Pferde,", "tokens": ["Zu", "Fu\u00df", "und", "hoch", "zu", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne es dir sagen kann:", "tokens": ["Die", "Son\u00b7ne", "es", "dir", "sa\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es k\u00e4mpft der Geist der Erde.", "tokens": ["Es", "k\u00e4mpft", "der", "Geist", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df zur Ruh' bei einem Baum,", "tokens": ["Ich", "sa\u00df", "zur", "Ruh'", "bei", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hielt die Luft umschlungen,", "tokens": ["Der", "hielt", "die", "Luft", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne kam zum Bl\u00e4ttersaum", "tokens": ["Die", "Son\u00b7ne", "kam", "zum", "Bl\u00e4t\u00b7ter\u00b7saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat mir's zugesungen.", "tokens": ["Und", "hat", "mir's", "zu\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu meinen F\u00fc\u00dfen gl\u00e4nzte Gras", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "gl\u00e4nz\u00b7te", "Gras"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blank wie der Pferde M\u00e4hnen,", "tokens": ["Blank", "wie", "der", "Pfer\u00b7de", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war vom scharfen Tau noch na\u00df", "tokens": ["Es", "war", "vom", "schar\u00b7fen", "Tau", "noch", "na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein Gesicht voll Tr\u00e4nen.", "tokens": ["Wie", "ein", "Ge\u00b7sicht", "voll", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es k\u00e4mpft der deutschen Erde Geist,", "tokens": ["Es", "k\u00e4mpft", "der", "deut\u00b7schen", "Er\u00b7de", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er will die V\u00f6lker f\u00fchren,", "tokens": ["Er", "will", "die", "V\u00f6l\u00b7ker", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel Blut aus tausend Wunden schwei\u00dft,", "tokens": ["Viel", "Blut", "aus", "tau\u00b7send", "Wun\u00b7den", "schwei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Grashalm mu\u00df es sp\u00fcren.", "tokens": ["Der", "Gras\u00b7halm", "mu\u00df", "es", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sah dann im jungen Morgenblau", "tokens": ["Sah", "dann", "im", "jun\u00b7gen", "Mor\u00b7gen\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell eine Taube fliegen,", "tokens": ["Hell", "ei\u00b7ne", "Tau\u00b7be", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Lichtbild spiegelte im Tau.", "tokens": ["Ihr", "Licht\u00b7bild", "spie\u00b7gel\u00b7te", "im", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heiliger Geist, hilf siegen!", "tokens": ["Hei\u00b7li\u00b7ger", "Geist", ",", "hilf", "sie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}