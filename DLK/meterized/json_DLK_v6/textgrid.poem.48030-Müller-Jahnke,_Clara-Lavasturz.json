{"textgrid.poem.48030": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Lavasturz", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist wie die M\u00e4rchenstadt,", "tokens": ["Mein", "Herz", "ist", "wie", "die", "M\u00e4r\u00b7chen\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "drin Lachen und Lust erklungen,", "tokens": ["drin", "La\u00b7chen", "und", "Lust", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "bis donnernden Grimms die Lavaglut", "tokens": ["bis", "don\u00b7nern\u00b7den", "Grimms", "die", "La\u00b7vag\u00b7lut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ihr Totenlied gesungen.", "tokens": ["ihr", "To\u00b7ten\u00b7lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sank dem Zecher der Goldpokal,", "tokens": ["Da", "sank", "dem", "Ze\u00b7cher", "der", "Gold\u00b7po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "gef\u00fcllt bis zum Ueberlaufen,", "tokens": ["ge\u00b7f\u00fcllt", "bis", "zum", "Ue\u00b7berl\u00b7au\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "da ward der Liebe das Hochzeitsbett", "tokens": ["da", "ward", "der", "Lie\u00b7be", "das", "Hoch\u00b7zeits\u00b7bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "zum lodernden Scheiterhaufen.", "tokens": ["zum", "lo\u00b7dern\u00b7den", "Schei\u00b7ter\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ein Glutschrei war es, ein Flammenmeer", "tokens": ["Und", "ein", "Glut\u00b7schrei", "war", "es", ",", "ein", "Flam\u00b7men\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "$,", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie st\u00fcrzender Sonnen Gefunkel,", "tokens": ["wie", "st\u00fcr\u00b7zen\u00b7der", "Son\u00b7nen", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "dann legte dar\u00fcber bleiern und schwer", "tokens": ["dann", "leg\u00b7te", "da\u00b7r\u00fc\u00b7ber", "blei\u00b7ern", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "VVINF", "KON", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sich Asche und Nacht und Dunkel . . . . . .", "tokens": ["sich", "A\u00b7sche", "und", "Nacht", "und", "Dun\u00b7kel", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["PRF", "NN", "KON", "NN", "KON", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}