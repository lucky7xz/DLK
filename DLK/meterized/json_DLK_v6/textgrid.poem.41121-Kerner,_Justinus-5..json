{"textgrid.poem.41121": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bruder! bin nicht wie du!", "tokens": ["Bru\u00b7der", "!", "bin", "nicht", "wie", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PTKNEG", "PWAV", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du warst ein Herz voll Klarheit,", "tokens": ["Du", "warst", "ein", "Herz", "voll", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll Festigkeit, voll Wahrheit,", "tokens": ["Voll", "Fes\u00b7tig\u00b7keit", ",", "voll", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Liebe und voll Ruh'!", "tokens": ["Voll", "Lie\u00b7be", "und", "voll", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bruder! bin nicht wie du!", "tokens": ["Bru\u00b7der", "!", "bin", "nicht", "wie", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PTKNEG", "PWAV", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mein Herz von wilden Wogen", "tokens": ["Mein", "Herz", "von", "wil\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der lauten Welt durchzogen,", "tokens": ["Der", "lau\u00b7ten", "Welt", "durch\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Klarheit nicht, nicht Ruh'!", "tokens": ["Hat", "Klar\u00b7heit", "nicht", ",", "nicht", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bruder! aus deinem Licht", "tokens": ["Bru\u00b7der", "!", "aus", "dei\u00b7nem", "Licht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Bruder! verla\u00df mich nicht!", "tokens": ["Bru\u00b7der", "!", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}