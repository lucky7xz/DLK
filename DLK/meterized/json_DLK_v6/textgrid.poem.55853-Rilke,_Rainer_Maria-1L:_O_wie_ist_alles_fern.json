{"textgrid.poem.55853": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie ist alles fern", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ist alles fern", "tokens": ["O", "wie", "ist", "al\u00b7les", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PIS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und lange vergangen.", "tokens": ["und", "lan\u00b7ge", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich glaube, der Stern,", "tokens": ["Ich", "glau\u00b7be", ",", "der", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "von welchem ich Glanz empfange,", "tokens": ["von", "wel\u00b7chem", "ich", "Glanz", "emp\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "ist seit Jahrtausenden tot.", "tokens": ["ist", "seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Ich glaube, im Boot,", "tokens": ["Ich", "glau\u00b7be", ",", "im", "Boot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "das vor\u00fcberfuhr,", "tokens": ["das", "vor\u00b7\u00fc\u00b7ber\u00b7fuhr", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "h\u00f6rte ich etwas Banges sagen.", "tokens": ["h\u00f6r\u00b7te", "ich", "et\u00b7was", "Ban\u00b7ges", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Im Hause hat eine Uhr", "tokens": ["Im", "Hau\u00b7se", "hat", "ei\u00b7ne", "Uhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.10": {"text": "geschlagen...", "tokens": ["ge\u00b7schla\u00b7gen", "..."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "In welchem Haus?...", "tokens": ["In", "wel\u00b7chem", "Haus", "?", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PWAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ich m\u00f6chte aus meinem Herzen hinaus", "tokens": ["Ich", "m\u00f6ch\u00b7te", "aus", "mei\u00b7nem", "Her\u00b7zen", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "unter den gro\u00dfen Himmel treten.", "tokens": ["un\u00b7ter", "den", "gro\u00b7\u00dfen", "Him\u00b7mel", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Ich m\u00f6chte beten.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Und einer von allen Sternen", "tokens": ["Und", "ei\u00b7ner", "von", "al\u00b7len", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "m\u00fc\u00dfte wirklich noch sein.", "tokens": ["m\u00fc\u00df\u00b7te", "wirk\u00b7lich", "noch", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Ich glaube, ich w\u00fc\u00dfte,", "tokens": ["Ich", "glau\u00b7be", ",", "ich", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "welcher allein", "tokens": ["wel\u00b7cher", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["PRELS", "ADV"], "meter": "+---", "measure": "dactylic.init"}, "line.19": {"text": "gedauert hat, \u2013", "tokens": ["ge\u00b7dau\u00b7ert", "hat", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "welcher wie eine wei\u00dfe Stadt", "tokens": ["wel\u00b7cher", "wie", "ei\u00b7ne", "wei\u00b7\u00dfe", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "KOKOM", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "am Ende des Strahls in den Himmeln steht...", "tokens": ["am", "En\u00b7de", "des", "Strahls", "in", "den", "Him\u00b7meln", "steht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}