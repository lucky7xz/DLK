{"textgrid.poem.42811": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df im Lande Leute verstreut,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df im Lande Leute verstreut,", "tokens": ["Ich", "wei\u00df", "im", "Lan\u00b7de", "Leu\u00b7te", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die saufen sich wissend zu Tode;", "tokens": ["Die", "sau\u00b7fen", "sich", "wis\u00b7send", "zu", "To\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "(saufen sich, hungern sich, h\u00e4rmen \u2013 ganz gleich!", "tokens": ["(", "sau\u00b7fen", "sich", ",", "hun\u00b7gern", "sich", ",", "h\u00e4r\u00b7men", "\u2013", "ganz", "gleich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "$(", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Sind alle, die ich meine, nicht reich.)", "tokens": ["Sind", "al\u00b7le", ",", "die", "ich", "mei\u00b7ne", ",", "nicht", "reich", ".", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Mein Vater sagte: \u00bbDie Leute von heut,", "tokens": ["Mein", "Va\u00b7ter", "sag\u00b7te", ":", "\u00bb", "Die", "Leu\u00b7te", "von", "heut", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die haben so unsinnige Mode.\u00ab", "tokens": ["Die", "ha\u00b7ben", "so", "un\u00b7sin\u00b7ni\u00b7ge", "Mo\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich antwortete: \u00bbJa die Leute \u2013 heut \u2013 Leut \u2013 \u00ab", "tokens": ["Ich", "ant\u00b7wor\u00b7te\u00b7te", ":", "\u00bb", "Ja", "die", "Leu\u00b7te", "\u2013", "heut", "\u2013", "Leut", "\u2013", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKANT", "ART", "NN", "$(", "ADV", "$(", "NN", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbansehnlich unauff\u00e4llig gemein\u00ab,", "tokens": ["\u00bb", "an\u00b7sehn\u00b7lich", "un\u00b7auf\u00b7f\u00e4l\u00b7lig", "ge\u00b7mein", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADJD", "ADJD", "$(", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das scheint mir das Ziel der Mode zu sein.", "tokens": ["Das", "scheint", "mir", "das", "Ziel", "der", "Mo\u00b7de", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich bin von die Leute von heute", "tokens": ["Ich", "bin", "von", "die", "Leu\u00b7te", "von", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Antipode der Mode.", "tokens": ["Ein", "An\u00b7ti\u00b7po\u00b7de", "der", "Mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bin meines Vaters mi\u00dfratenes Kind.", "tokens": ["Ich", "bin", "mei\u00b7nes", "Va\u00b7ters", "mi\u00df\u00b7ra\u00b7te\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gestern starb er. Und heute", "tokens": ["Ge\u00b7stern", "starb", "er", ".", "Und", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wei\u00df ich, da\u00df viele von uns zu Tode", "tokens": ["Wei\u00df", "ich", ",", "da\u00df", "vie\u00b7le", "von", "uns", "zu", "To\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIS", "APPR", "PPER", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Sich qu\u00e4len und trotzen, die ebenso sind", "tokens": ["Sich", "qu\u00e4\u00b7len", "und", "trot\u00b7zen", ",", "die", "e\u00b7ben\u00b7so", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "KON", "VVINF", "$,", "PRELS", "ADV", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wie Vater, Urahne, Gro\u00dfmutter und Kind. \u2013", "tokens": ["Wie", "Va\u00b7ter", ",", "Ur\u00b7ah\u00b7ne", ",", "Gro\u00df\u00b7mut\u00b7ter", "und", "Kind", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Da pfeift sich was wie Seemannswind:", "tokens": ["Da", "pfeift", "sich", "was", "wie", "See\u00b7manns\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sauf zu! Hihi! Sauf zu! Hihi!", "tokens": ["Sauf", "zu", "!", "Hi\u00b7hi", "!", "Sauf", "zu", "!", "Hi\u00b7hi", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "NE", "$.", "NN", "PTKVZ", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich habe keine Sorgen;", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6chstens vielleicht die eine, die", "tokens": ["H\u00f6chs\u00b7tens", "viel\u00b7leicht", "die", "ei\u00b7ne", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "ART", "ART", "$,", "PRELS"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Um die Leute von morgen.", "tokens": ["Um", "die", "Leu\u00b7te", "von", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "ADV", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}