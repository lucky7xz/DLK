{"textgrid.poem.25289": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die Nacht mit dumpfem Rauschen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Nacht mit dumpfem Rauschen", "tokens": ["Durch", "die", "Nacht", "mit", "dum\u00b7pfem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt vorbei des Stromes Wut;", "tokens": ["Treibt", "vor\u00b7bei", "des", "Stro\u00b7mes", "Wut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und mit tr\u00e4umerischem Lauschen", "tokens": ["Und", "mit", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Starr ich auf die dunkle Flut.", "tokens": ["Starr", "ich", "auf", "die", "dunk\u00b7le", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schattenhafte K\u00e4hne wallen", "tokens": ["Schat\u00b7ten\u00b7haf\u00b7te", "K\u00e4h\u00b7ne", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir vorbei, in Nacht hinein;", "tokens": ["Mir", "vor\u00b7bei", ",", "in", "Nacht", "hin\u00b7ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe Stimmen, sie verhallen,", "tokens": ["Lie\u00b7be", "Stim\u00b7men", ",", "sie", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Str\u00f6mung t\u00f6nt allein.", "tokens": ["Und", "die", "Str\u00f6\u00b7mung", "t\u00f6nt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "\u00d6des Schweigen, banges Dunkel!", "tokens": ["\u00d6\u00b7des", "Schwei\u00b7gen", ",", "ban\u00b7ges", "Dun\u00b7kel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerzlich irrt mein Blick empor.", "tokens": ["Schmerz\u00b7lich", "irrt", "mein", "Blick", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da erbl\u00fcht mit Trostgefunkel", "tokens": ["Da", "er\u00b7bl\u00fcht", "mit", "Trost\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gestirn dem Wolkenflor.", "tokens": ["Ein", "Ge\u00b7stirn", "dem", "Wol\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbsieh, ich bleibe!\u00ab winkt sein Auge/", "tokens": ["\u00bb", "sieh", ",", "ich", "blei\u00b7be", "!", "\u00ab", "winkt", "sein", "Au\u00b7ge", "/"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PPER", "VVFIN", "$.", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die bange Seele zieht", "tokens": ["Und", "die", "ban\u00b7ge", "See\u00b7le", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf zu diesem treuen Auge,", "tokens": ["Auf", "zu", "die\u00b7sem", "treu\u00b7en", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Kind zur Mutter flieht.", "tokens": ["Wie", "ein", "Kind", "zur", "Mut\u00b7ter", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn dereinst des Todes Grauen", "tokens": ["Wenn", "de\u00b7reinst", "des", "To\u00b7des", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herz umsp\u00fclt und bricht,", "tokens": ["Die\u00b7ses", "Herz", "um\u00b7sp\u00fclt", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df noch einmal dich erschauen", "tokens": ["La\u00df", "noch", "ein\u00b7mal", "dich", "er\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Wassern, s\u00fc\u00dfes Licht!", "tokens": ["\u00dc\u00b7ber", "Was\u00b7sern", ",", "s\u00fc\u00b7\u00dfes", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis den letzten Liebesfunken,", "tokens": ["Bis", "den", "letz\u00b7ten", "Lie\u00b7be\u00b7sfun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der aus meinem Auge scheint,", "tokens": ["Der", "aus", "mei\u00b7nem", "Au\u00b7ge", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Blicke aufgetrunken", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "auf\u00b7ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dem Sternenglanz vereint.", "tokens": ["Und", "dem", "Ster\u00b7nen\u00b7glanz", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}