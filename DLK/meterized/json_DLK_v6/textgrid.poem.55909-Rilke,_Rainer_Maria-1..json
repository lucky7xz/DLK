{"textgrid.poem.55909": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lag. Und ihre Kinderarme waren", "tokens": ["Sie", "lag", ".", "Und", "ih\u00b7re", "Kin\u00b7der\u00b7ar\u00b7me", "wa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von Dienern um den Welkenden gebunden,", "tokens": ["von", "Die\u00b7nern", "um", "den", "Wel\u00b7ken\u00b7den", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "auf dem sie lag die s\u00fc\u00dfen langen Stunden,", "tokens": ["auf", "dem", "sie", "lag", "die", "s\u00fc\u00b7\u00dfen", "lan\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein wenig bang vor seinen vielen Jahren.", "tokens": ["ein", "we\u00b7nig", "bang", "vor", "sei\u00b7nen", "vie\u00b7len", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "APPR", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und manchmal wandte sie in seinem Barte", "tokens": ["Und", "manch\u00b7mal", "wand\u00b7te", "sie", "in", "sei\u00b7nem", "Bar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ihr Angesicht, wenn eine Eule schrie;", "tokens": ["ihr", "An\u00b7ge\u00b7sicht", ",", "wenn", "ei\u00b7ne", "Eu\u00b7le", "schrie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und alles, was die Nacht war, kam und scharte", "tokens": ["und", "al\u00b7les", ",", "was", "die", "Nacht", "war", ",", "kam", "und", "schar\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit Bangen und Verlangen sich um sie.", "tokens": ["mit", "Ban\u00b7gen", "und", "Ver\u00b7lan\u00b7gen", "sich", "um", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Sterne zitterten wie ihresgleichen,", "tokens": ["Die", "Ster\u00b7ne", "zit\u00b7ter\u00b7ten", "wie", "ih\u00b7res\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein Duft ging suchend durch das Schlafgemach,", "tokens": ["ein", "Duft", "ging", "su\u00b7chend", "durch", "das", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Vorhang r\u00fchrte sich und gab ein Zeichen,", "tokens": ["der", "Vor\u00b7hang", "r\u00fchr\u00b7te", "sich", "und", "gab", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und leise ging ihr Blick dem Zeichen nach \u2013.", "tokens": ["und", "lei\u00b7se", "ging", "ihr", "Blick", "dem", "Zei\u00b7chen", "nach", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aber sie hielt sich an dem dunkeln Alten", "tokens": ["A\u00b7ber", "sie", "hielt", "sich", "an", "dem", "dun\u00b7keln", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "und, von der Nacht der N\u00e4chte nicht erreicht,", "tokens": ["und", ",", "von", "der", "Nacht", "der", "N\u00e4ch\u00b7te", "nicht", "er\u00b7reicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "lag sie auf seinem f\u00fcrstlichen Erkalten", "tokens": ["lag", "sie", "auf", "sei\u00b7nem", "f\u00fcrst\u00b7li\u00b7chen", "Er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "jungfr\u00e4ulich und wie eine Seele leicht.", "tokens": ["jung\u00b7fr\u00e4u\u00b7lich", "und", "wie", "ei\u00b7ne", "See\u00b7le", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}