{"textgrid.poem.24551": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Belehrsamkeit und Tugend/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Belehrsamkeit und Tugend/", "tokens": ["Be\u00b7lehr\u00b7sam\u00b7keit", "und", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sahen sich nach einer Wohnung um.", "tokens": ["Die", "sa\u00b7hen", "sich", "nach", "ei\u00b7ner", "Woh\u00b7nung", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die muntre Lust der Jugend", "tokens": ["Die", "mun\u00b7tre", "Lust", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bek\u00fcmmerte sich ebenfalls darum.", "tokens": ["Be\u00b7k\u00fcm\u00b7mer\u00b7te", "sich", "e\u00b7ben\u00b7falls", "da\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie kamen alle drey", "tokens": ["Sie", "ka\u00b7men", "al\u00b7le", "drey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor eine Th\u00fcr/ und wolten Wohnung machen.", "tokens": ["Vor", "ei\u00b7ne", "Th\u00fcr", "/", "und", "wol\u00b7ten", "Woh\u00b7nung", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hier dr\u00e4ngte sich die Lust mit ihren Sachen/", "tokens": ["Hier", "dr\u00e4ng\u00b7te", "sich", "die", "Lust", "mit", "ih\u00b7ren", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df nicht die ersten zwey", "tokens": ["Da\u00df", "nicht", "die", "ers\u00b7ten", "zwey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die besten Zimmer n\u00e4hmen.", "tokens": ["Die", "bes\u00b7ten", "Zim\u00b7mer", "n\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Drauf sprach der Herr/ der Edel/ wie mein Freund:", "tokens": ["Drauf", "sprach", "der", "Herr", "/", "der", "E\u00b7del", "/", "wie", "mein", "Freund", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$(", "ART", "NN", "$(", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn jene nicht vor dir zu wohnen k\u00e4men/", "tokens": ["Wenn", "je\u00b7ne", "nicht", "vor", "dir", "zu", "woh\u00b7nen", "k\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So w\u00fcrden sie zu einem andern gehn:", "tokens": ["So", "w\u00fcr\u00b7den", "sie", "zu", "ei\u00b7nem", "an\u00b7dern", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das Mittel-Stockwerck ist der Tugend aufgehoben/", "tokens": ["Das", "Mit\u00b7tel\u00b7Stockwerck", "ist", "der", "Tu\u00b7gend", "auf\u00b7ge\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die Lust soll unten an/ und wie wir heute sehn/", "tokens": ["Die", "Lust", "soll", "un\u00b7ten", "an", "/", "und", "wie", "wir", "heu\u00b7te", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKVZ", "$(", "KON", "PWAV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wohnt Gelehrsamkeit/ Hochwehrter Tr \u2013 \u2013 oben.", "tokens": ["So", "wohnt", "Ge\u00b7lehr\u00b7sam\u00b7keit", "/", "Hoch\u00b7wehr\u00b7ter", "Tr", "\u2013", "\u2013", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$(", "ADJA", "NN", "$(", "$(", "ADV", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}