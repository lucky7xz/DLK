{"textgrid.poem.66445": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Zinkenistentrost", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fccklich ist der Zinkenist,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", "der", "Zin\u00b7ke\u00b7nist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herr und sein Geselle!", "tokens": ["Der", "Herr", "und", "sein", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6mmt, wenn er gestorben ist,", "tokens": ["Er", "k\u00f6mmt", ",", "wenn", "er", "ge\u00b7stor\u00b7ben", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewi\u00df nicht in die H\u00f6lle:", "tokens": ["Ge\u00b7wi\u00df", "nicht", "in", "die", "H\u00f6l\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn Gott h\u00e4lt oft ein Freudenfest", "tokens": ["Denn", "Gott", "h\u00e4lt", "oft", "ein", "Freu\u00b7den\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit auserw\u00e4hlten Christen;", "tokens": ["Mit", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Chris\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und weil man da Posaunen bl\u00e4st,", "tokens": ["Und", "weil", "man", "da", "Po\u00b7sau\u00b7nen", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So braucht man Zinkenisten.", "tokens": ["So", "braucht", "man", "Zin\u00b7ke\u00b7nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}