{"dta.poem.3705": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Weins\u00fcppchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Anne Margrittchen!               ", "tokens": ["An\u00b7ne", "Mar\u00b7gritt\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was willst du, mein Liebchen?", "tokens": ["Was", "willst", "du", ",", "mein", "Lieb\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich trinke so gerne", "tokens": ["Ich", "trin\u00b7ke", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gez\u00fcckerten Wein.", "tokens": ["Ge\u00b7z\u00fc\u00b7cker\u00b7ten", "Wein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Zwey Pfund Zuckerchen,", "tokens": ["Zwey", "Pfund", "Zu\u00b7cker\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Ein Pfund Butterchen,", "tokens": ["Ein", "Pfund", "But\u00b7ter\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Zwey Maa\u00dfe Wein,", "tokens": ["Zwey", "Maa\u00b7\u00dfe", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So mu\u00df es gut seyn.", "tokens": ["So", "mu\u00df", "es", "gut", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Sch\u00fctt' es in ein Kesselchen,", "tokens": ["Sch\u00fctt'", "es", "in", "ein", "Kes\u00b7sel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "R\u00fchr es mit dem L\u00f6ffelchen,", "tokens": ["R\u00fchr", "es", "mit", "dem", "L\u00f6f\u00b7fel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Anne Margritchen,", "tokens": ["An\u00b7ne", "Mar\u00b7grit\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Welch S\u00fcpplein ist das?", "tokens": ["Welch", "S\u00fcp\u00b7plein", "ist", "das", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PDS", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Eine Weinsupp! eine Weinsupp!", "tokens": ["Ei\u00b7ne", "Wein\u00b7supp", "!", "ei\u00b7ne", "Wein\u00b7supp", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}