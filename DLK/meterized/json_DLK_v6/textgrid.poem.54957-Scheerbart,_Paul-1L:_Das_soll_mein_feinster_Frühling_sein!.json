{"textgrid.poem.54957": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das soll mein feinster Fr\u00fchling sein!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das soll mein feinster Fr\u00fchling sein!", "tokens": ["Das", "soll", "mein", "feins\u00b7ter", "Fr\u00fch\u00b7ling", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es leuchten tausend Sonnen,", "tokens": ["Es", "leuch\u00b7ten", "tau\u00b7send", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hinter den Bergen", "tokens": ["Und", "hin\u00b7ter", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wogen die Meere des ewigen Sommers.", "tokens": ["Wo\u00b7gen", "die", "Mee\u00b7re", "des", "e\u00b7wi\u00b7gen", "Som\u00b7mers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Ich komme noch hin.", "tokens": ["Ich", "kom\u00b7me", "noch", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ich komme mit Welten", "tokens": ["Ich", "kom\u00b7me", "mit", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und lache gewaltig.", "tokens": ["Und", "la\u00b7che", "ge\u00b7wal\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Berge sind hoch,", "tokens": ["Die", "Ber\u00b7ge", "sind", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Aber r\u00fcber komm ich doch.", "tokens": ["A\u00b7ber", "r\u00fc\u00b7ber", "komm", "ich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Tausend Sonnen beleuchten", "tokens": ["Tau\u00b7send", "Son\u00b7nen", "be\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Den wilden h\u00f6ckrigen Pfad.", "tokens": ["Den", "wil\u00b7den", "h\u00f6ck\u00b7ri\u00b7gen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Das soll mein feinster Fr\u00fchling sein.", "tokens": ["Das", "soll", "mein", "feins\u00b7ter", "Fr\u00fch\u00b7ling", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das Sonnenlicht macht Alles rein.", "tokens": ["Das", "Son\u00b7nen\u00b7licht", "macht", "Al\u00b7les", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Alte, alte Wunderwelt!", "tokens": ["Al\u00b7te", ",", "al\u00b7te", "Wun\u00b7der\u00b7welt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}