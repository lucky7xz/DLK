{"textgrid.poem.41108": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Alter und Winter", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter und Winter, Herbheit der Natur!", "tokens": ["Al\u00b7ter", "und", "Win\u00b7ter", ",", "Herb\u00b7heit", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "O da\u00df man auch im Kampf der Elemente", "tokens": ["O", "da\u00df", "man", "auch", "im", "Kampf", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PIS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch duftend wie die Blume sterben k\u00f6nnte!", "tokens": ["Noch", "duf\u00b7tend", "wie", "die", "Blu\u00b7me", "ster\u00b7ben", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch ach! man stirbt nicht, man vertrocknet nur.", "tokens": ["Doch", "ach", "!", "man", "stirbt", "nicht", ",", "man", "ver\u00b7trock\u00b7net", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIS", "VVFIN", "PTKNEG", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und so vertrocknet lebt man sich zum Spott,", "tokens": ["Und", "so", "ver\u00b7trock\u00b7net", "lebt", "man", "sich", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt jahrelang an seiner Bahre zimmern,", "tokens": ["H\u00f6rt", "jah\u00b7re\u00b7lang", "an", "sei\u00b7ner", "Bah\u00b7re", "zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis endlich f\u00e4llt saftlos der Leib in Tr\u00fcmmern,", "tokens": ["Bis", "end\u00b7lich", "f\u00e4llt", "saft\u00b7los", "der", "Leib", "in", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wo die Seele hinf\u00e4hrt, wei\u00df nur Gott.", "tokens": ["Und", "wo", "die", "See\u00b7le", "hin\u00b7f\u00e4hrt", ",", "wei\u00df", "nur", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}