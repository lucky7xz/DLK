{"textgrid.poem.40094": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Wechselwirkung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fc\u00dfte nicht mich nach der Form zu richten,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "nicht", "mich", "nach", "der", "Form", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wird mir bedeutet von gestrengen Richtern.", "tokens": ["wird", "mir", "be\u00b7deu\u00b7tet", "von", "ge\u00b7stren\u00b7gen", "Rich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und freilich: leicht ereignet sich's an Dichtern,", "tokens": ["Und", "frei\u00b7lich", ":", "leicht", "er\u00b7eig\u00b7net", "sich's", "an", "Dich\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADJD", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df sie formloser leben als sie dichten.", "tokens": ["da\u00df", "sie", "form\u00b7lo\u00b7ser", "le\u00b7ben", "als", "sie", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "KOKOM", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn leider m\u00fcssen sie die Menschen sichten", "tokens": ["Denn", "lei\u00b7der", "m\u00fcs\u00b7sen", "sie", "die", "Men\u00b7schen", "sich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem Inhalt nach, der hinter den Gesichtern:", "tokens": ["dem", "In\u00b7halt", "nach", ",", "der", "hin\u00b7ter", "den", "Ge\u00b7sich\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zwar Mancher h\u00e4lt's mit formgerechten Wichtern,", "tokens": ["zwar", "Man\u00b7cher", "h\u00e4lt's", "mit", "form\u00b7ge\u00b7rech\u00b7ten", "Wich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch Mancher w\u00e4gt nach schwereren Gewichten.", "tokens": ["doch", "Man\u00b7cher", "w\u00e4gt", "nach", "schwe\u00b7re\u00b7ren", "Ge\u00b7wich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Mir ergeht es gar blos wie dem Trichter,", "tokens": ["Und", "Mir", "er\u00b7geht", "es", "gar", "blos", "wie", "dem", "Trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der von sich giebt, was man hineingetrichtert.", "tokens": ["der", "von", "sich", "giebt", ",", "was", "man", "hin\u00b7ein\u00b7ge\u00b7trich\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "VVFIN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gebt mir Gef\u00fchl \u2013 in echter Form, in schlichter:", "tokens": ["Gebt", "mir", "Ge\u00b7f\u00fchl", "\u2013", "in", "ech\u00b7ter", "Form", ",", "in", "schlich\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$(", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und formvoll wird vonselbst sich alles schlichten!", "tokens": ["und", "form\u00b7voll", "wird", "von\u00b7selbst", "sich", "al\u00b7les", "schlich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "doch wenn empfindungslos ihr splitterrichtert,", "tokens": ["doch", "wenn", "emp\u00b7fin\u00b7dungs\u00b7los", "ihr", "split\u00b7ter\u00b7rich\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "so werd' ich formlos nur nach Mir mich richten!", "tokens": ["so", "werd'", "ich", "form\u00b7los", "nur", "nach", "Mir", "mich", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}