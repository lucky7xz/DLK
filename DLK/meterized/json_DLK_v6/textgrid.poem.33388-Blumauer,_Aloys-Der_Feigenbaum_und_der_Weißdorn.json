{"textgrid.poem.33388": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Feigenbaum und der Wei\u00dfdorn", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6nbelaubter Wei\u00dfdorn trotzte", "tokens": ["Ein", "sch\u00f6n\u00b7be\u00b7laub\u00b7ter", "Wei\u00df\u00b7dorn", "trotz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Feigenbaum, der um die Fr\u00fchlingszeit,", "tokens": ["Dem", "Fei\u00b7gen\u00b7baum", ",", "der", "um", "die", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch kaum belaubt, doch schon von Fr\u00fcchten strotzte,", "tokens": ["Noch", "kaum", "be\u00b7laubt", ",", "doch", "schon", "von", "Fr\u00fcch\u00b7ten", "strotz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und that mit seinen Bl\u00fcthen breit.", "tokens": ["Und", "that", "mit", "sei\u00b7nen", "Bl\u00fc\u00b7then", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sind denn deine Bl\u00fcthen? frug", "tokens": ["Wo", "sind", "denn", "dei\u00b7ne", "Bl\u00fc\u00b7then", "?", "frug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wei\u00dfdorn ihn \u2013 Und wo \u2013 erwiedert' jener \u2013", "tokens": ["Der", "Wei\u00df\u00b7dorn", "ihn", "\u2013", "Und", "wo", "\u2013", "er\u00b7wie\u00b7dert'", "je\u00b7ner", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$(", "KON", "PWAV", "$(", "VVFIN", "PDAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist deine Frucht? \u2013 Dagegen bin ich sch\u00f6ner,", "tokens": ["Ist", "dei\u00b7ne", "Frucht", "?", "\u2013", "Da\u00b7ge\u00b7gen", "bin", "ich", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "$(", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Versetzt' der Strauch, und das ist mir genug.", "tokens": ["Ver\u00b7setzt'", "der", "Strauch", ",", "und", "das", "ist", "mir", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So lass' uns denn in Frieden leben,", "tokens": ["So", "lass'", "uns", "denn", "in", "Frie\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erwiederte der Feigenbaum:", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "der", "Fei\u00b7gen\u00b7baum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Dir hat Natur f\u00fcr's Auge Reiz gegeben,", "tokens": ["Dir", "hat", "Na\u00b7tur", "f\u00fcr's", "Au\u00b7ge", "Reiz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mir gab sie Fr\u00fcchte f\u00fcr den Gaum.", "tokens": ["Mir", "gab", "sie", "Fr\u00fcch\u00b7te", "f\u00fcr", "den", "Gaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Fabel gilt den M\u00e4nnern, deren Schriften", "tokens": ["Die", "Fa\u00b7bel", "gilt", "den", "M\u00e4n\u00b7nern", ",", "de\u00b7ren", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt bald Nutzen, bald Vergn\u00fcgen stiften:", "tokens": ["Der", "Welt", "bald", "Nut\u00b7zen", ",", "bald", "Ver\u00b7gn\u00fc\u00b7gen", "stif\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der unterh\u00e4lt, der unterrichtet sie.", "tokens": ["Der", "un\u00b7ter\u00b7h\u00e4lt", ",", "der", "un\u00b7ter\u00b7rich\u00b7tet", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die g\u00fctige Natur, in ihren Gaben allen", "tokens": ["Die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", ",", "in", "ih\u00b7ren", "Ga\u00b7ben", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gleich m\u00fctterlich, gibt dem die Gabe zu gefallen,", "tokens": ["Gleich", "m\u00fct\u00b7ter\u00b7lich", ",", "gibt", "dem", "die", "Ga\u00b7be", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und jenem die des Unterrichts, da\u00df nie", "tokens": ["Und", "je\u00b7nem", "die", "des", "Un\u00b7ter\u00b7richts", ",", "da\u00df", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "ART", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Kind von ihr das andere beneide;", "tokens": ["Ein", "Kind", "von", "ihr", "das", "an\u00b7de\u00b7re", "be\u00b7nei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur ihrem liebsten Sohn Voltairen \u2013 gab sie beide.", "tokens": ["Nur", "ih\u00b7rem", "liebs\u00b7ten", "Sohn", "Vol\u00b7tai\u00b7ren", "\u2013", "gab", "sie", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN", "$(", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}