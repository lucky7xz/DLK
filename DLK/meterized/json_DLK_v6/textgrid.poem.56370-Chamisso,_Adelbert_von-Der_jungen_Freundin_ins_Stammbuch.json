{"textgrid.poem.56370": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der jungen Freundin ins Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zehn Zentner schwer aus lauterem Dukatengold", "tokens": ["Zehn", "Zent\u00b7ner", "schwer", "aus", "lau\u00b7te\u00b7rem", "Du\u00b7ka\u00b7ten\u00b7gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verfertige der Meister Goldschmidt einen Stuhl,", "tokens": ["Ver\u00b7fer\u00b7ti\u00b7ge", "der", "Meis\u00b7ter", "Gold\u00b7schmidt", "ei\u00b7nen", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und spare Diamanten nicht, Rubinen nicht,", "tokens": ["Und", "spa\u00b7re", "Di\u00b7a\u00b7man\u00b7ten", "nicht", ",", "Ru\u00b7bi\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "$,", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht leuchtende Karfunkel, nicht der Perlen Zier", "tokens": ["Nicht", "leuch\u00b7ten\u00b7de", "Kar\u00b7fun\u00b7kel", ",", "nicht", "der", "Per\u00b7len", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An diesem Kunstwerk, welches ich, so reich es sei,", "tokens": ["An", "die\u00b7sem", "Kunst\u00b7werk", ",", "wel\u00b7ches", "ich", ",", "so", "reich", "es", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So reich und kostbar, voll und bar bezahlen will,", "tokens": ["So", "reich", "und", "kost\u00b7bar", ",", "voll", "und", "bar", "be\u00b7zah\u00b7len", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird nur der Fall, wof\u00fcr ich es bestimme, wahr;", "tokens": ["Wird", "nur", "der", "Fall", ",", "wo\u00b7f\u00fcr", "ich", "es", "be\u00b7stim\u00b7me", ",", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn dir verhei\u00df ich, teures Kind, sotanen Stuhl,", "tokens": ["Denn", "dir", "ver\u00b7hei\u00df", "ich", ",", "teu\u00b7res", "Kind", ",", "so\u00b7ta\u00b7nen", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darauf gem\u00e4chlich du in Ehren sitzen magst,", "tokens": ["Da\u00b7rauf", "ge\u00b7m\u00e4ch\u00b7lich", "du", "in", "Eh\u00b7ren", "sit\u00b7zen", "magst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Im Falle man dich \u00fcberhaupt nur sitzen l\u00e4\u00dft.", "tokens": ["Im", "Fal\u00b7le", "man", "dich", "\u00fc\u00b7ber\u00b7haupt", "nur", "sit\u00b7zen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PRF", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}