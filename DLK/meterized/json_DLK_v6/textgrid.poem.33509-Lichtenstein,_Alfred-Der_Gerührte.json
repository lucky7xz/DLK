{"textgrid.poem.33509": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Ger\u00fchrte", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe gern verlassen", "tokens": ["Ich", "ha\u00b7be", "gern", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den lauten Tod der Stadt,", "tokens": ["Den", "lau\u00b7ten", "Tod", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tausend Fratzen hat,", "tokens": ["Der", "tau\u00b7send", "Frat\u00b7zen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die gelbe Nacht der Gassen.", "tokens": ["Die", "gel\u00b7be", "Nacht", "der", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schreite in den weiten,", "tokens": ["Ich", "schrei\u00b7te", "in", "den", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Silbrigen Himmel ein;", "tokens": ["Silb\u00b7ri\u00b7gen", "Him\u00b7mel", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die frommen Glieder gleiten", "tokens": ["Die", "from\u00b7men", "Glie\u00b7der", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief in das sanfte Sein.", "tokens": ["Tief", "in", "das", "sanf\u00b7te", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin im wei\u00dfen Leuchten", "tokens": ["Ich", "bin", "im", "wei\u00b7\u00dfen", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Wolke, Wiese, Wind.", "tokens": ["Von", "Wol\u00b7ke", ",", "Wie\u00b7se", ",", "Wind", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin Baum, bin Dorf, bin Kind..", "tokens": ["Bin", "Baum", ",", "bin", "Dorf", ",", "bin", "Kind", ".."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "VAFIN", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie sich die Augen feuchten!", "tokens": ["Wie", "sich", "die", "Au\u00b7gen", "feuch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald wird am Silberende", "tokens": ["Bald", "wird", "am", "Sil\u00b7be\u00b7ren\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gr\u00fcne Abend stehn ...", "tokens": ["Der", "gr\u00fc\u00b7ne", "A\u00b7bend", "stehn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hebe selge H\u00e4nde \u2013", "tokens": ["Ich", "he\u00b7be", "sel\u00b7ge", "H\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will ihm entgegengehn \u2013", "tokens": ["Will", "ihm", "ent\u00b7ge\u00b7gen\u00b7gehn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}