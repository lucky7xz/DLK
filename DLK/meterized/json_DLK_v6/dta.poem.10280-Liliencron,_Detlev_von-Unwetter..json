{"dta.poem.10280": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Unwetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm pre\u00dft trotzig an die Fensterscheiben             ", "tokens": ["Der", "Sturm", "pre\u00dft", "trot\u00b7zig", "an", "die", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die rauhe Stirn; tiefschwarze Wolken treiben", "tokens": ["Die", "rau\u00b7he", "Stirn", ";", "tief\u00b7schwar\u00b7ze", "Wol\u00b7ken", "trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Fetzen einer Riesentrauerfahne,", "tokens": ["Wie", "Fet\u00b7zen", "ei\u00b7ner", "Rie\u00b7sen\u00b7trau\u00b7er\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schnell wie Bilder ziehn im Fieberwahne.", "tokens": ["Und", "schnell", "wie", "Bil\u00b7der", "ziehn", "im", "Fie\u00b7ber\u00b7wah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Rettung suchend, zog, von Angst befangen,", "tokens": ["Wie", "Ret\u00b7tung", "su\u00b7chend", ",", "zog", ",", "von", "Angst", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meine Arme dich ein hei\u00df Verlangen.", "tokens": ["In", "mei\u00b7ne", "Ar\u00b7me", "dich", "ein", "hei\u00df", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie hold das war: Ein Bl\u00e4ttchen, sturmgetrieben,", "tokens": ["Wie", "hold", "das", "war", ":", "Ein", "Bl\u00e4tt\u00b7chen", ",", "sturm\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PDS", "VAFIN", "$.", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Flog mir ans Herz, dort ist es auch geblieben.", "tokens": ["Flog", "mir", "ans", "Herz", ",", "dort", "ist", "es", "auch", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}