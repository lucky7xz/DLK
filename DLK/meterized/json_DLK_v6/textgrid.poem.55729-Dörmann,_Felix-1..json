{"textgrid.poem.55729": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Totenm\u00fcde,", "tokens": ["To\u00b7ten\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mit wankenden Gliedern,", "tokens": ["Mit", "wan\u00b7ken\u00b7den", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit fiebernder Stirne", "tokens": ["Mit", "fie\u00b7bern\u00b7der", "Stir\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Steh' ich auf d\u00fcrrer,", "tokens": ["Steh'", "ich", "auf", "d\u00fcr\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Trostloser Heide.", "tokens": ["Trost\u00b7lo\u00b7ser", "Hei\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist zerfressen,", "tokens": ["Mein", "Herz", "ist", "zer\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Qualenzerrissen", "tokens": ["Qua\u00b7len\u00b7zer\u00b7ris\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und halbvermodert.", "tokens": ["Und", "halb\u00b7ver\u00b7mo\u00b7dert", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mich schreckt nicht mehr", "tokens": ["Mich", "schreckt", "nicht", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der zischenden Blitze", "tokens": ["Der", "zi\u00b7schen\u00b7den", "Blit\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Todk\u00fcndendes Funkeln,", "tokens": ["Tod\u00b7k\u00fcn\u00b7den\u00b7des", "Fun\u00b7keln", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nicht der Bestien", "tokens": ["Nicht", "der", "Be\u00b7sti\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "N\u00e4her schwellendes,", "tokens": ["N\u00e4\u00b7her", "schwel\u00b7len\u00b7des", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Hungriges Heulen.", "tokens": ["Hung\u00b7ri\u00b7ges", "Heu\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und wenn am dunstigen,", "tokens": ["Und", "wenn", "am", "duns\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Bleifarbenen Himmel", "tokens": ["Blei\u00b7far\u00b7be\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Schimmernde Schemen", "tokens": ["Schim\u00b7mern\u00b7de", "Sche\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Fata Morganas", "tokens": ["Fa\u00b7ta", "Mor\u00b7ga\u00b7nas"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "In brennenden Farben", "tokens": ["In", "bren\u00b7nen\u00b7den", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Lockend ergl\u00e4nzen, \u2013", "tokens": ["Lo\u00b7ckend", "er\u00b7gl\u00e4n\u00b7zen", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "$,", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Ich bleibe kalt.", "tokens": ["Ich", "blei\u00b7be", "kalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was noch vor Monden", "tokens": ["Was", "noch", "vor", "Mon\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In meiner Seele", "tokens": ["In", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Rasende Flammen,", "tokens": ["Ra\u00b7sen\u00b7de", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zuckende Wonnen", "tokens": ["Zu\u00b7cken\u00b7de", "Won\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Emporgewirbelt,", "tokens": ["Em\u00b7por\u00b7ge\u00b7wir\u00b7belt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich seh es kaum.", "tokens": ["Ich", "seh", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein F\u00fchlen ist tot. \u2013", "tokens": ["Mein", "F\u00fch\u00b7len", "ist", "tot", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Vor\u00fcbergegangen", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist Wollusttoben,", "tokens": ["Ist", "Wol\u00b7lust\u00b7to\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Knirschender Zorn", "tokens": ["Knir\u00b7schen\u00b7der", "Zorn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und fressende Reue.", "tokens": ["Und", "fres\u00b7sen\u00b7de", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In mir ist Stille,", "tokens": ["In", "mir", "ist", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Tiefste Stille,", "tokens": ["Tiefs\u00b7te", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Stille des Todes.", "tokens": ["Stil\u00b7le", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}