{"textgrid.poem.65412": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Seiler", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor ist ein Seiler worden,", "tokens": ["A\u00b7mor", "ist", "ein", "Sei\u00b7ler", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drehet Seile, Schn\u00fcr' und Ketten", "tokens": ["Dre\u00b7het", "Sei\u00b7le", ",", "Schn\u00fcr'", "und", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den sammetweichen F\u00e4den", "tokens": ["Aus", "den", "sam\u00b7met\u00b7wei\u00b7chen", "F\u00e4\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner goldnen Lockenkrone.", "tokens": ["Dei\u00b7ner", "gold\u00b7nen", "Lo\u00b7cken\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so gro\u00df sind seine K\u00fcnste,", "tokens": ["Und", "so", "gro\u00df", "sind", "sei\u00b7ne", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er aus den kleinen, feinen,", "tokens": ["Da\u00df", "er", "aus", "den", "klei\u00b7nen", ",", "fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "D\u00fcnnen, zarten Ringelh\u00e4rchen", "tokens": ["D\u00fcn\u00b7nen", ",", "zar\u00b7ten", "Rin\u00b7gel\u00b7h\u00e4r\u00b7chen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Diamantenfeste Bande", "tokens": ["Di\u00b7a\u00b7man\u00b7ten\u00b7fes\u00b7te", "Ban\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "F\u00fcr die armen Herzen windet.", "tokens": ["F\u00fcr", "die", "ar\u00b7men", "Her\u00b7zen", "win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in hunderten zusammen", "tokens": ["Und", "in", "hun\u00b7der\u00b7ten", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Schn\u00fcrt er sie mit einem Seile,", "tokens": ["Schn\u00fcrt", "er", "sie", "mit", "ei\u00b7nem", "Sei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00e4ngt sie dann vor Schlafengehen", "tokens": ["H\u00e4ngt", "sie", "dann", "vor", "Schla\u00b7fen\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "An den Riegel deiner Kammer,", "tokens": ["An", "den", "Rie\u00b7gel", "dei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "An des Ladens Schraubenspitze,", "tokens": ["An", "des", "La\u00b7dens", "Schrau\u00b7ben\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die fr\u00f6mmsten jeden Sonntag", "tokens": ["Und", "die", "fr\u00f6mms\u00b7ten", "je\u00b7den", "Sonn\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "An das Kreuz auf deinem Busen.", "tokens": ["An", "das", "Kreuz", "auf", "dei\u00b7nem", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}