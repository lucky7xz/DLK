{"textgrid.poem.26506": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um des Menschen Werth zu messen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um des Menschen Werth zu messen,", "tokens": ["Um", "des", "Men\u00b7schen", "Werth", "zu", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fragt doch nicht, was er erreicht;", "tokens": ["Fragt", "doch", "nicht", ",", "was", "er", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er Kraft und Muth besessen,", "tokens": ["Ob", "er", "Kraft", "und", "Muth", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ergr\u00fcndet ihr vielleicht.", "tokens": ["Das", "er\u00b7gr\u00fcn\u00b7det", "ihr", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mancher hob sich, statt zu stranden,", "tokens": ["Man\u00b7cher", "hob", "sich", ",", "statt", "zu", "stran\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn des Unheils Woge schlug \u2013", "tokens": ["Wenn", "des", "Un\u00b7heils", "Wo\u00b7ge", "schlug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber dieses sei verstanden:", "tokens": ["A\u00b7ber", "die\u00b7ses", "sei", "ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob er auch das Gl\u00fcck ertrug.", "tokens": ["Ob", "er", "auch", "das", "Gl\u00fcck", "er\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob er, wenn es niederthaute,", "tokens": ["Ob", "er", ",", "wenn", "es", "nie\u00b7dert\u00b7hau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Labend und erf\u00fcllungsschwer,", "tokens": ["La\u00b7bend", "und", "er\u00b7f\u00fcl\u00b7lungs\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie besch\u00e4mt den Segen schaute,", "tokens": ["Wie", "be\u00b7sch\u00e4mt", "den", "Se\u00b7gen", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder schn\u00f6de rief: noch mehr!", "tokens": ["O\u00b7der", "schn\u00f6\u00b7de", "rief", ":", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "$.", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob er, wenn der Preis ihm blinkte,", "tokens": ["Ob", "er", ",", "wenn", "der", "Preis", "ihm", "blink\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig dankend hingekniet,", "tokens": ["Se\u00b7lig", "dan\u00b7kend", "hin\u00b7ge\u00b7kniet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder sich das Haupt umzinkte", "tokens": ["O\u00b7der", "sich", "das", "Haupt", "um\u00b7zink\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Br\u00fcder Reih'n vermied.", "tokens": ["Und", "der", "Br\u00fc\u00b7der", "Reih'n", "ver\u00b7mied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer da auch im Weihrauchqualme", "tokens": ["Wer", "da", "auch", "im", "Weih\u00b7rauch\u00b7qual\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als ein stolzer Sieger stand:", "tokens": ["Als", "ein", "stol\u00b7zer", "Sie\u00b7ger", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenem nur geb\u00fchrt die Palme,", "tokens": ["Je\u00b7nem", "nur", "ge\u00b7b\u00fchrt", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich selber \u00fcberwand!", "tokens": ["Der", "sich", "sel\u00b7ber", "\u00fc\u00b7ber\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}