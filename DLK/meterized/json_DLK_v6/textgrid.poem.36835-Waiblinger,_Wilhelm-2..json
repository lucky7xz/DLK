{"textgrid.poem.36835": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hle, G\u00f6ttin der Liebe, mit den Grazien", "tokens": ["W\u00e4h\u00b7le", ",", "G\u00f6t\u00b7tin", "der", "Lie\u00b7be", ",", "mit", "den", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heute Paphos zum Sitz und morgen Knidos,", "tokens": ["Heu\u00b7te", "Pa\u00b7phos", "zum", "Sitz", "und", "mor\u00b7gen", "Kni\u00b7dos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "KON", "ADV", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich beneide dich nicht; denn bald lockt Capri's", "tokens": ["Ich", "be\u00b7nei\u00b7de", "dich", "nicht", ";", "denn", "bald", "lockt", "Ca\u00b7pri's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "KON", "ADV", "VVFIN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Morgenl\u00e4ndischer Fels in seine Stille;", "tokens": ["Mor\u00b7gen\u00b7l\u00e4n\u00b7di\u00b7scher", "Fels", "in", "sei\u00b7ne", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Bald zu Ischia's duft'gen Bergen rudr' ich;", "tokens": ["Bald", "zu", "Is\u00b7chia's", "duft'\u00b7gen", "Ber\u00b7gen", "rudr'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Bald aus Reben- und heitern S\u00e4ulentempeln", "tokens": ["Bald", "aus", "Re\u00b7ben", "und", "hei\u00b7tern", "S\u00e4u\u00b7len\u00b7tem\u00b7peln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "In Pompeji die See und Thal und Ufer", "tokens": ["In", "Pom\u00b7pe\u00b7ji", "die", "See", "und", "Thal", "und", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und blauschattig Gebirg und Insel seh' ich,", "tokens": ["Und", "blau\u00b7schat\u00b7tig", "Ge\u00b7birg", "und", "In\u00b7sel", "seh'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Bald aus wildem Get\u00f6se des Toledo", "tokens": ["Bald", "aus", "wil\u00b7dem", "Ge\u00b7t\u00f6\u00b7se", "des", "To\u00b7le\u00b7do"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Fl\u00fccht' ich mich in Sorrent's Orangenhaine.", "tokens": ["Fl\u00fccht'", "ich", "mich", "in", "Sor\u00b7rent's", "O\u00b7ran\u00b7gen\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, geliebt ist der Berg dir wohl, der sch\u00f6ne,", "tokens": ["Ja", ",", "ge\u00b7liebt", "ist", "der", "Berg", "dir", "wohl", ",", "der", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "VAFIN", "ART", "NN", "PPER", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jener Stammberg im eb'nen Schwaben mein' ich,", "tokens": ["Je\u00b7ner", "Stamm\u00b7berg", "im", "eb'\u00b7nen", "Schwa\u00b7ben", "mein'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der dir Eigenthum fast geworden, dessen", "tokens": ["Der", "dir", "Ei\u00b7gen\u00b7thum", "fast", "ge\u00b7wor\u00b7den", ",", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "NN", "ADV", "VAPP", "$,", "PRELAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wolkenscheitel den Schmetterling dir sandte,", "tokens": ["Wol\u00b7ken\u00b7schei\u00b7tel", "den", "Schmet\u00b7ter\u00b7ling", "dir", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und der Zeuge des Becherklangs gewesen,", "tokens": ["Und", "der", "Zeu\u00b7ge", "des", "Be\u00b7cher\u00b7klangs", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Als gro\u00dfsinnige Freunde mein gedachten", "tokens": ["Als", "gro\u00df\u00b7sin\u00b7ni\u00b7ge", "Freun\u00b7de", "mein", "ge\u00b7dach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sei er beiden gelobt, der Hohenstaufe,", "tokens": ["Sei", "er", "bei\u00b7den", "ge\u00b7lobt", ",", "der", "Ho\u00b7hen\u00b7stau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "VVPP", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Paladin des gewalt'gen Schwabens sei er,", "tokens": ["Pa\u00b7la\u00b7din", "des", "ge\u00b7walt'\u00b7gen", "Schwa\u00b7bens", "sei", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Capitol uns genannt des Heldenhauses!", "tokens": ["Ca\u00b7pi\u00b7tol", "uns", "ge\u00b7nannt", "des", "Hel\u00b7den\u00b7hau\u00b7ses", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Aber sch\u00f6ner noch ist des Deutschen Erbland.", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", "noch", "ist", "des", "Deut\u00b7schen", "Er\u00b7bland", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Frage Friedrich den Kaiser, frage Manfred!", "tokens": ["Fra\u00b7ge", "Fried\u00b7rich", "den", "Kai\u00b7ser", ",", "fra\u00b7ge", "Man\u00b7fred", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "$,", "VVFIN", "NE", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Hier auf bl\u00fchenden Felsen, die der Abend", "tokens": ["Hier", "auf", "bl\u00fc\u00b7hen\u00b7den", "Fel\u00b7sen", ",", "die", "der", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Purpurn f\u00e4rbt und der frische Meerwind k\u00fchlet,", "tokens": ["Pur\u00b7purn", "f\u00e4rbt", "und", "der", "fri\u00b7sche", "Meer\u00b7wind", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hier im ewigen Schatten der Citrone,", "tokens": ["Hier", "im", "e\u00b7wi\u00b7gen", "Schat\u00b7ten", "der", "Cit\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Freund, umathmen mich L\u00fcfte rein und milde,", "tokens": ["Freund", ",", "um\u00b7ath\u00b7men", "mich", "L\u00fcf\u00b7te", "rein", "und", "mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wie die G\u00f6tter sie trinken! Klar und helle", "tokens": ["Wie", "die", "G\u00f6t\u00b7ter", "sie", "trin\u00b7ken", "!", "Klar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$.", "NN", "KON", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Lockt zum Bade das heit're Meer, es lockt die", "tokens": ["Lockt", "zum", "Ba\u00b7de", "das", "heit'\u00b7re", "Meer", ",", "es", "lockt", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Schatt'ge, hallende Grotte. Wie die Seele,", "tokens": ["Schatt'\u00b7ge", ",", "hal\u00b7len\u00b7de", "Grot\u00b7te", ".", "Wie", "die", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die in Unschuld ich liebt', durchs holde Auge,", "tokens": ["Die", "in", "Un\u00b7schuld", "ich", "liebt'", ",", "durchs", "hol\u00b7de", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Leuchtet ruhig der stille Grund der Wasser.", "tokens": ["Leuch\u00b7tet", "ru\u00b7hig", "der", "stil\u00b7le", "Grund", "der", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Selbst das Kieselchen siehst du hier, nur selten,", "tokens": ["Selbst", "das", "Kie\u00b7sel\u00b7chen", "siehst", "du", "hier", ",", "nur", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Dem bescheidenen Wunsch des Innern \u00e4hnlich,", "tokens": ["Dem", "be\u00b7schei\u00b7de\u00b7nen", "Wunsch", "des", "In\u00b7nern", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Regt ein lieblicher Schauer diese Tiefe.", "tokens": ["Regt", "ein", "lieb\u00b7li\u00b7cher", "Schau\u00b7er", "die\u00b7se", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Hier zu k\u00fchlen den Leib und hinzupl\u00e4tschern", "tokens": ["Hier", "zu", "k\u00fch\u00b7len", "den", "Leib", "und", "hin\u00b7zu\u00b7pl\u00e4t\u00b7schern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "KON", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Unterm Felsen ist Wonne, nur dem Seegott", "tokens": ["Un\u00b7term", "Fel\u00b7sen", "ist", "Won\u00b7ne", ",", "nur", "dem", "See\u00b7gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Und der l\u00fcsternen Nymphe ganz gegeben.", "tokens": ["Und", "der", "l\u00fcs\u00b7ter\u00b7nen", "Nym\u00b7phe", "ganz", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Aber steige die Nacht, die k\u00fchle, holde,", "tokens": ["A\u00b7ber", "stei\u00b7ge", "die", "Nacht", ",", "die", "k\u00fch\u00b7le", ",", "hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Steige nur den gewund'nen Pfad der Felsschlucht", "tokens": ["Stei\u00b7ge", "nur", "den", "ge\u00b7wun\u00b7d'\u00b7nen", "Pfad", "der", "Fels\u00b7schlucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Hier empor, und die Last der \u00fcpp'gen Pflanzen,", "tokens": ["Hier", "em\u00b7por", ",", "und", "die", "Last", "der", "\u00fcpp'\u00b7gen", "Pflan\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Die verschwenderisch niederh\u00e4ngt und schwellend", "tokens": ["Die", "ver\u00b7schwen\u00b7de\u00b7risch", "nie\u00b7der\u00b7h\u00e4ngt", "und", "schwel\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Gr\u00fcnt und wuchert, erblicke sie mit Staunen,", "tokens": ["Gr\u00fcnt", "und", "wu\u00b7chert", ",", "er\u00b7bli\u00b7cke", "sie", "mit", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Und schon lachen die G\u00e4rten dir entgegen,", "tokens": ["Und", "schon", "la\u00b7chen", "die", "G\u00e4r\u00b7ten", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Weinlaub rankt sich empor, in stolzem Wuchse", "tokens": ["Wein\u00b7laub", "rankt", "sich", "em\u00b7por", ",", "in", "stol\u00b7zem", "Wuch\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "Bl\u00fchet \u00fcber der Mauer die Orange,", "tokens": ["Bl\u00fc\u00b7het", "\u00fc\u00b7ber", "der", "Mau\u00b7er", "die", "O\u00b7ran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Die Granate, der Lorbeer und die Feige.", "tokens": ["Die", "Gra\u00b7na\u00b7te", ",", "der", "Lor\u00b7beer", "und", "die", "Fei\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Was im kindischen Drang' der ersten Liebe", "tokens": ["Was", "im", "kin\u00b7di\u00b7schen", "Drang'", "der", "ers\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Von Elysiums Fr\u00fcchten du getr\u00e4umet,", "tokens": ["Von", "E\u00b7ly\u00b7si\u00b7ums", "Fr\u00fcch\u00b7ten", "du", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "