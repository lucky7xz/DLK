{"dta.poem.23793": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Gnaden-Wahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht die Worte h\u00e4lt/ die im Gesetze stehen/", "tokens": ["Wer", "nicht", "die", "Wor\u00b7te", "h\u00e4lt", "/", "die", "im", "Ge\u00b7set\u00b7ze", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$(", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem deutest du den Fluch O GOtt mit Schre-", "tokens": ["Dem", "deu\u00b7test", "du", "den", "Fluch", "O", "Gott", "mit", "Schre"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "NE", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer aber wird von uns dem strengen Spruch entgehen", "tokens": ["Wer", "a\u00b7ber", "wird", "von", "uns", "dem", "stren\u00b7gen", "Spruch", "ent\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil kein einiger sich dessen r\u00fchmen kan? ", "tokens": ["Die\u00b7weil", "kein", "ei\u00b7ni\u00b7ger", "sich", "des\u00b7sen", "r\u00fch\u00b7men", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PIAT", "PRF", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja wenn ein Sterblicher gleich alles k\u00f6nt erf\u00fcllen/", "tokens": ["Ja", "wenn", "ein", "Sterb\u00b7li\u00b7cher", "gleich", "al\u00b7les", "k\u00f6nt", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "H\u00e4tt\u2019 er zu deinem Reich darum kein besser Recht.", "tokens": ["H\u00e4tt'", "er", "zu", "dei\u00b7nem", "Reich", "da\u00b7rum", "kein", "bes\u00b7ser", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PAV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Wercke gelten nicht. Er th\u00e4te deinen Willen/", "tokens": ["Die", "Wer\u00b7cke", "gel\u00b7ten", "nicht", ".", "Er", "th\u00e4\u00b7te", "dei\u00b7nen", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch w\u00e4r er immerhin ein armer S\u00fcnden-Knecht ", "tokens": ["Doch", "w\u00e4r", "er", "im\u00b7mer\u00b7hin", "ein", "ar\u00b7mer", "S\u00fcn\u00b7den\u00b7Knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df zwar da\u00df dein Sohn sein heilges Blut vergosse\u0303/", "tokens": ["Ich", "wei\u00df", "zwar", "da\u00df", "dein", "Sohn", "sein", "heil\u00b7ges", "Blut", "ver\u00b7goss\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und von der Missethat uns alle l\u00f6\u00dfgezehlt. ", "tokens": ["Und", "von", "der", "Mis\u00b7se\u00b7that", "uns", "al\u00b7le", "l\u00f6\u00df\u00b7ge\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie vielen aber bleibt der Himmel doch verschlossen!", "tokens": ["Wie", "vie\u00b7len", "a\u00b7ber", "bleibt", "der", "Him\u00b7mel", "doch", "ver\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil du die wenigsten auf Erden auserwehlt. ", "tokens": ["Weil", "du", "die", "we\u00b7nigs\u00b7ten", "auf", "Er\u00b7den", "au\u00b7ser\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.13": {"text": "Wie soll ich das verstehn/ da\u00df du hast k\u00f6nnen hassen", "tokens": ["Wie", "soll", "ich", "das", "ver\u00b7stehn", "/", "da\u00df", "du", "hast", "k\u00f6n\u00b7nen", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$(", "KOUS", "PPER", "VAFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Esau der doch nie des Tages-Licht erblickt? ", "tokens": ["Den", "E\u00b7sau", "der", "doch", "nie", "des", "Ta\u00b7ges\u00b7Licht", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie kan ich mit dem Arm des Glaubens dich umfassen/", "tokens": ["Wie", "kan", "ich", "mit", "dem", "Arm", "des", "Glau\u00b7bens", "dich", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Eh deine Liebe sich zu meiner Schwachheit b\u00fcckt? ", "tokens": ["Eh", "dei\u00b7ne", "Lie\u00b7be", "sich", "zu", "mei\u00b7ner", "Schwach\u00b7heit", "b\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du wilst zwar deinen Geist auf Bitte mir gewehren/", "tokens": ["Du", "wilst", "zwar", "dei\u00b7nen", "Geist", "auf", "Bit\u00b7te", "mir", "ge\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Tr\u00f6ster welcher uns zum Weg der Wahrheit", "tokens": ["Den", "Tr\u00f6s\u00b7ter", "wel\u00b7cher", "uns", "zum", "Weg", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRELS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wie aber kan ich HErr den Geist von dir begehren/", "tokens": ["Wie", "a\u00b7ber", "kan", "ich", "Herr", "den", "Geist", "von", "dir", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn nicht derselbe Geist schon Hertz und Lippen", "tokens": ["Wenn", "nicht", "der\u00b7sel\u00b7be", "Geist", "schon", "Hertz", "und", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PDAT", "NN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "D\u00fcrfft\u2019 ich als schlechter Thon mit meinem T\u00f6pfer streite\u0303?", "tokens": ["D\u00fcrfft'", "ich", "als", "schlech\u00b7ter", "Thon", "mit", "mei\u00b7nem", "T\u00f6p\u00b7fer", "streit\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "D\u00fcrfft\u2019 ich als ein Gesch\u00f6pf dem Sch\u00f6pffer reden ein?", "tokens": ["D\u00fcrfft'", "ich", "als", "ein", "Ge\u00b7sch\u00f6pf", "dem", "Sch\u00f6pf\u00b7fer", "re\u00b7den", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So fragt\u2019 ich: kanst du mich so leicht zur Freude leiten/", "tokens": ["So", "fragt'", "ich", ":", "kanst", "du", "mich", "so", "leicht", "zur", "Freu\u00b7de", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Warum steht mir es frey der H\u00f6llen Raub zu seyn?", "tokens": ["Wa\u00b7rum", "steht", "mir", "es", "frey", "der", "H\u00f6l\u00b7len", "Raub", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ist an des S\u00fcnders Heyl dir HErr so viel gelegen/", "tokens": ["Ist", "an", "des", "S\u00fcn\u00b7ders", "Heyl", "dir", "Herr", "so", "viel", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "PPER", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie solches mir dein Mund und Eyd-Schwur saget", "tokens": ["Wie", "sol\u00b7ches", "mir", "dein", "Mund", "und", "Ey\u00b7dSchwur", "sa\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "PPOSAT", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Warum verg\u00f6nnest du/ da\u00df sich die L\u00fcsten regen?", "tokens": ["Wa\u00b7rum", "ver\u00b7g\u00f6n\u00b7nest", "du", "/", "da\u00df", "sich", "die", "L\u00fcs\u00b7ten", "re\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "KOUS", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ist Satan/ Welt und Fleisch denn m\u00e4chtiger als du?", "tokens": ["Ist", "Sa\u00b7tan", "/", "Welt", "und", "Fleisch", "denn", "m\u00e4ch\u00b7ti\u00b7ger", "als", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "NN", "KON", "NN", "KON", "ADJA", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach GOtt! so qv\u00e4len mich zum \u00f6fftern die Gedancken;", "tokens": ["Ach", "Gott", "!", "so", "qv\u00e4\u00b7len", "mich", "zum", "\u00f6ff\u00b7tern", "die", "Ge\u00b7dan\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Noch mehr verwirret mich der Schrift gelehrte\u0303 Streit/", "tokens": ["Noch", "mehr", "ver\u00b7wir\u00b7ret", "mich", "der", "Schrift", "ge\u00b7lehr\u00b7t\u1ebd", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn sie sich nach der Kunst um deine Worte zancken;", "tokens": ["Wenn", "sie", "sich", "nach", "der", "Kunst", "um", "dei\u00b7ne", "Wor\u00b7te", "zan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn dieser Gnade bringt/ und jener Sterben dreut.", "tokens": ["Wenn", "die\u00b7ser", "Gna\u00b7de", "bringt", "/", "und", "je\u00b7ner", "Ster\u00b7ben", "dreut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$(", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es scheint als h\u00e4tten sie mit dir im Rath gesessen/", "tokens": ["Es", "scheint", "als", "h\u00e4t\u00b7ten", "sie", "mit", "dir", "im", "Rath", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und da mit dir zugleich das Urtheil abgefa\u00dft/", "tokens": ["Und", "da", "mit", "dir", "zu\u00b7gleich", "das", "Ur\u00b7theil", "ab\u00b7ge\u00b7fa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weil sie sich unterstehn nach ihrer Schnur zu messen/", "tokens": ["Weil", "sie", "sich", "un\u00b7ter\u00b7stehn", "nach", "ih\u00b7rer", "Schnur", "zu", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was du Unendlicher in dir verborgen hast.", "tokens": ["Was", "du", "Un\u00b7end\u00b7li\u00b7cher", "in", "dir", "ver\u00b7bor\u00b7gen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bald wil die blasse Furcht mich in den Abgrund st\u00fcrtzen/", "tokens": ["Bald", "wil", "die", "blas\u00b7se", "Furcht", "mich", "in", "den", "Ab\u00b7grund", "st\u00fcrt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bald gr\u00fcbelt die Vernunfft/ doch kan ihr frecher Tand", "tokens": ["Bald", "gr\u00fc\u00b7belt", "die", "Ver\u00b7nunfft", "/", "doch", "kan", "ihr", "fre\u00b7cher", "Tand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und mein Gewissen nichts als Zweifels-Knoten sch\u00fcrtze\u0303", "tokens": ["Und", "mein", "Ge\u00b7wis\u00b7sen", "nichts", "als", "Zwei\u00b7fels\u00b7Kno\u00b7ten", "sch\u00fcrtz\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So gar da\u00df Sicherheit offt nimmet \u00fcberhand.", "tokens": ["So", "gar", "da\u00df", "Si\u00b7cher\u00b7heit", "offt", "nim\u00b7met", "\u00fc\u00b7ber\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Zuletzt erhohl\u2019 ich mich und flieh\u2019 in deine Wunden/", "tokens": ["Zu\u00b7letzt", "er\u00b7hohl'", "ich", "mich", "und", "flieh'", "in", "dei\u00b7ne", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mein Heyland/ die dir nicht \u00fcmsonst geschlagen sind!", "tokens": ["Mein", "Hey\u00b7land", "/", "die", "dir", "nicht", "\u00fcm\u00b7sonst", "ge\u00b7schla\u00b7gen", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Im \u00fcbrigen sey dir dein Rath-Schlu\u00df ungebunden/", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "sey", "dir", "dein", "Ra\u00b7th\u00b7Schlu\u00df", "un\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Ich unterwerffe mich dir Vater als dein Kind", "tokens": ["Ich", "un\u00b7ter\u00b7werf\u00b7fe", "mich", "dir", "Va\u00b7ter", "als", "dein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "NN", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Hilf da\u00df ich wandeln mag/ als wenn durch from\u0303es Leben", "tokens": ["Hilf", "da\u00df", "ich", "wan\u00b7deln", "mag", "/", "als", "wenn", "durch", "from\u0303es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "VMFIN", "$(", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.46": {"text": "Ich k\u00f6nt\u2019 erwerben hier die Sch\u00e4tze jener Welt;", "tokens": ["Ich", "k\u00f6nt'", "er\u00b7wer\u00b7ben", "hier", "die", "Sch\u00e4t\u00b7ze", "je\u00b7ner", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch wollest du dabey mir solchen Glauben geben/", "tokens": ["Doch", "wol\u00b7lest", "du", "da\u00b7bey", "mir", "sol\u00b7chen", "Glau\u00b7ben", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der mein Verdienst f\u00fcr nichts/ und dich f\u00fcr alles h\u00e4lt.", "tokens": ["Der", "mein", "Ver\u00b7dienst", "f\u00fcr", "nichts", "/", "und", "dich", "f\u00fcr", "al\u00b7les", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIS", "$(", "KON", "PRF", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}