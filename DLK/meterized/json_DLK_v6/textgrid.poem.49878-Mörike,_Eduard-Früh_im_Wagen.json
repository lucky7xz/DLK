{"textgrid.poem.49878": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fch im Wagen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es graut vom Morgenreif", "tokens": ["Es", "graut", "vom", "Mor\u00b7gen\u00b7reif"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In D\u00e4mmerung das Feld,", "tokens": ["In", "D\u00e4m\u00b7me\u00b7rung", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schon ein blasser Streif", "tokens": ["Da", "schon", "ein", "blas\u00b7ser", "Streif"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den fernen Ost erhellt;", "tokens": ["Den", "fer\u00b7nen", "Ost", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man sieht im Lichte bald", "tokens": ["Man", "sieht", "im", "Lich\u00b7te", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Morgenstern vergehn,", "tokens": ["Den", "Mor\u00b7gens\u00b7tern", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Und doch am Fichtenwald", "tokens": ["Und", "doch", "am", "Fich\u00b7ten\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den vollen Mond noch stehn:", "tokens": ["Den", "vol\u00b7len", "Mond", "noch", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So ist mein scheuer Blick,", "tokens": ["So", "ist", "mein", "scheu\u00b7er", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den schon die Ferne dr\u00e4ngt,", "tokens": ["Den", "schon", "die", "Fer\u00b7ne", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch in das Schmerzensgl\u00fcck", "tokens": ["Noch", "in", "das", "Schmer\u00b7zens\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Abschiedsnacht versenkt.", "tokens": ["Der", "Ab\u00b7schieds\u00b7nacht", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein blaues Auge steht", "tokens": ["Dein", "blau\u00b7es", "Au\u00b7ge", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein dunkler See vor mir,", "tokens": ["Ein", "dunk\u00b7ler", "See", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Ku\u00df, dein Hauch umweht,", "tokens": ["Dein", "Ku\u00df", ",", "dein", "Hauch", "um\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Fl\u00fcstern mich noch hier.", "tokens": ["Dein", "Fl\u00fcs\u00b7tern", "mich", "noch", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An deinem Hals begr\u00e4bt", "tokens": ["An", "dei\u00b7nem", "Hals", "be\u00b7gr\u00e4bt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich weinend mein Gesicht,", "tokens": ["Sich", "wei\u00b7nend", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Purpurschw\u00e4rze webt", "tokens": ["Und", "Pur\u00b7pur\u00b7schw\u00e4r\u00b7ze", "webt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir vor dem Auge dicht.", "tokens": ["Mir", "vor", "dem", "Au\u00b7ge", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die Sonne kommt; \u2013 sie scheucht", "tokens": ["Die", "Son\u00b7ne", "kommt", ";", "\u2013", "sie", "scheucht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Traum hinweg im Nu,", "tokens": ["Den", "Traum", "hin\u00b7weg", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von den Bergen streicht", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "streicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schauer auf mich zu.", "tokens": ["Ein", "Schau\u00b7er", "auf", "mich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}