{"textgrid.poem.67949": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "17. Die Sorge", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde, darum sollt ich sorgen", "tokens": ["Freun\u00b7de", ",", "da\u00b7rum", "sollt", "ich", "sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PAV", "VMFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter welchem Dach ich lebe?", "tokens": ["Un\u00b7ter", "wel\u00b7chem", "Dach", "ich", "le\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich drunter nur verborgen,", "tokens": ["Wenn", "ich", "drun\u00b7ter", "nur", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh und frei und gl\u00fccklich lebe;", "tokens": ["Froh", "und", "frei", "und", "gl\u00fcck\u00b7lich", "le\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ums ungewisse Morgen", "tokens": ["Und", "ums", "un\u00b7ge\u00b7wis\u00b7se", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht in Furcht und Hoffen schwebe \u2013", "tokens": ["Nicht", "in", "Furcht", "und", "Hof\u00b7fen", "schwe\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "ChOR:", "tokens": ["ChOR", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Das sind Sch\u00e4tze! auf den Wogen", "tokens": ["Das", "sind", "Sch\u00e4t\u00b7ze", "!", "auf", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kommen sie nicht angezogen.", "tokens": ["Kom\u00b7men", "sie", "nicht", "an\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich aus dem Flusse trinke", "tokens": ["Wenn", "ich", "aus", "dem", "Flus\u00b7se", "trin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelhelles, reines Wasser,", "tokens": ["Spie\u00b7gel\u00b7hel\u00b7les", ",", "rei\u00b7nes", "Was\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dabei mich gl\u00fccklich d\u00fcnke,", "tokens": ["Und", "da\u00b7bei", "mich", "gl\u00fcck\u00b7lich", "d\u00fcn\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und wie jener reiche Prasser,", "tokens": ["Und", "wie", "je\u00b7ner", "rei\u00b7che", "Pras\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht in goldnen Ketten hinke,", "tokens": ["Nicht", "in", "gold\u00b7nen", "Ket\u00b7ten", "hin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um ein Tr\u00f6pfchen Rebenwasser \u2013", "tokens": ["Um", "ein", "Tr\u00f6pf\u00b7chen", "Re\u00b7ben\u00b7was\u00b7ser", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freunde, traut nicht leerem", "tokens": ["Freun\u00b7de", ",", "traut", "nicht", "lee\u00b7rem"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schimmer, Goldne Ketten dr\u00fccken immer.", "tokens": ["Schim\u00b7mer", ",", "Gold\u00b7ne", "Ket\u00b7ten", "dr\u00fc\u00b7cken", "im\u00b7mer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ists, hohes Herz zu f\u00fchlen,", "tokens": ["Sch\u00f6n", "ists", ",", "ho\u00b7hes", "Herz", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpfen k\u00f6nnen mit dem Gl\u00fccke,", "tokens": ["K\u00e4mp\u00b7fen", "k\u00f6n\u00b7nen", "mit", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft den Sieg ihm abzuzielen,", "tokens": ["Oft", "den", "Sieg", "ihm", "ab\u00b7zu\u00b7zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer weichen ihm zur\u00fccke,", "tokens": ["Nim\u00b7mer", "wei\u00b7chen", "ihm", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Dornen fort sich w\u00fchlen,", "tokens": ["Durch", "die", "Dor\u00b7nen", "fort", "sich", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf zum freien Sonnenblicke! \u2013", "tokens": ["Auf", "zum", "frei\u00b7en", "Son\u00b7nen\u00b7bli\u00b7cke", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "ChOR:", "tokens": ["ChOR", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Freunde, nie dem Gl\u00fcck sich beugen,", "tokens": ["Freun\u00b7de", ",", "nie", "dem", "Gl\u00fcck", "sich", "beu\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Heisset, zu den G\u00f6ttern steigen.", "tokens": ["Heis\u00b7set", ",", "zu", "den", "G\u00f6t\u00b7tern", "stei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber, auch das Gl\u00fcck besieget,", "tokens": ["A\u00b7ber", ",", "auch", "das", "Gl\u00fcck", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat noch niemand \u00fcberwunden,", "tokens": ["Hat", "noch", "nie\u00b7mand", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sich unter Amorn schmieget.", "tokens": ["Der", "sich", "un\u00b7ter", "A\u00b7morn", "schmie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn hat Ruhe je gefunden?", "tokens": ["Denn", "hat", "Ru\u00b7he", "je", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dem Thor zu F\u00fcssen lieget,", "tokens": ["Wer", "dem", "Thor", "zu", "F\u00fcs\u00b7sen", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nur lohnen kann mit Wunden.", "tokens": ["Der", "nur", "loh\u00b7nen", "kann", "mit", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "ChOR:", "tokens": ["ChOR", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Blinder Knabe, seine Blinden", "tokens": ["Blin\u00b7der", "Kna\u00b7be", ",", "sei\u00b7ne", "Blin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lohnet er mit Reu und Binden.", "tokens": ["Loh\u00b7net", "er", "mit", "Reu", "und", "Bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}