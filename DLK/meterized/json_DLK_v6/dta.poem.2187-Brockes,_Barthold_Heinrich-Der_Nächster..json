{"dta.poem.2187": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wahrheit scheint ein solches Wesen, von welchem", "tokens": ["Die", "Wahr\u00b7heit", "scheint", "ein", "sol\u00b7ches", "We\u00b7sen", ",", "von", "wel\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "NN", "$,", "APPR", "PWAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wir gestehen m\u00fcssen,", "tokens": ["wir", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Ungelehrte nichts von ihr, Gelehrte nur den Na-", "tokens": ["Da\u00df", "Un\u00b7ge\u00b7lehr\u00b7te", "nichts", "von", "ihr", ",", "Ge\u00b7lehr\u00b7te", "nur", "den", "Na"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PIS", "APPR", "PPER", "$,", "NN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "men, wissen.", "tokens": ["men", ",", "wis\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Begn\u00fcgen sich nun gro\u00dfe Geister mit ihrem Namen blo\u00df", "tokens": ["Be\u00b7gn\u00fc\u00b7gen", "sich", "nun", "gro\u00b7\u00dfe", "Geis\u00b7ter", "mit", "ih\u00b7rem", "Na\u00b7men", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "allein;", "tokens": ["al\u00b7lein", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wie herrlich und wie voller Liebreiz mu\u00df denn die Wahr-", "tokens": ["Wie", "herr\u00b7lich", "und", "wie", "vol\u00b7ler", "Lieb\u00b7reiz", "mu\u00df", "denn", "die", "Wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJA", "NN", "VMFIN", "ADV", "ART", "TRUNC"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "heit selbst nicht seyn!", "tokens": ["heit", "selbst", "nicht", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKNEG", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}