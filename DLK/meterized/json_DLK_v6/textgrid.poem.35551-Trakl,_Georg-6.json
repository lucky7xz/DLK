{"textgrid.poem.35551": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abends schweben blutige Linnen,", "tokens": ["A\u00b7bends", "schwe\u00b7ben", "blu\u00b7ti\u00b7ge", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wolken \u00fcber stummen W\u00e4ldern,", "tokens": ["Wol\u00b7ken", "\u00fc\u00b7ber", "stum\u00b7men", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die geh\u00fcllt in schwarze Linnen.", "tokens": ["Die", "ge\u00b7h\u00fcllt", "in", "schwar\u00b7ze", "Lin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spatzen l\u00e4rmen auf den Feldern.", "tokens": ["Spat\u00b7zen", "l\u00e4r\u00b7men", "auf", "den", "Fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie liegt ganz wei\u00df im Dunkel.", "tokens": ["Und", "sie", "liegt", "ganz", "wei\u00df", "im", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterm Dach verhaucht ein Girren.", "tokens": ["Un\u00b7term", "Dach", "ver\u00b7haucht", "ein", "Gir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Aas in Busch und Dunkel", "tokens": ["Wie", "ein", "Aas", "in", "Busch", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen ihren Mund umschwirren.", "tokens": ["Flie\u00b7gen", "ih\u00b7ren", "Mund", "um\u00b7schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traumhaft klingt im braunen Weiler", "tokens": ["Traum\u00b7haft", "klingt", "im", "brau\u00b7nen", "Wei\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach ein Klang von Tanz und Geigen,", "tokens": ["Nach", "ein", "Klang", "von", "Tanz", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebt ihr Antlitz durch den Weiler,", "tokens": ["Schwebt", "ihr", "Ant\u00b7litz", "durch", "den", "Wei\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weht ihr Haar in kahlen Zweigen.", "tokens": ["Weht", "ihr", "Haar", "in", "kah\u00b7len", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}