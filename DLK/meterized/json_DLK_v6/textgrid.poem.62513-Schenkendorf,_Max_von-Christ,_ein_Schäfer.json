{"textgrid.poem.62513": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Christ, ein Sch\u00e4fer", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr auf den gr\u00fcnen Fluren", "tokens": ["Seht", "ihr", "auf", "den", "gr\u00fc\u00b7nen", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenen holden Sch\u00e4fer ziehn?", "tokens": ["Je\u00b7nen", "hol\u00b7den", "Sch\u00e4\u00b7fer", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht ihr auch auf seinen Spuren", "tokens": ["Seht", "ihr", "auch", "auf", "sei\u00b7nen", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner alle Felder bl\u00fchn?", "tokens": ["Sch\u00f6\u00b7ner", "al\u00b7le", "Fel\u00b7der", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kennt ihr nicht die frohen Heerden?", "tokens": ["Kennt", "ihr", "nicht", "die", "fro\u00b7hen", "Heer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauet an den Sch\u00e4ferstab,", "tokens": ["Schau\u00b7et", "an", "den", "Sch\u00e4\u00b7fer\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den der Himmel wie der Erden", "tokens": ["Den", "der", "Him\u00b7mel", "wie", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater seinen H\u00e4nden gab.", "tokens": ["Va\u00b7ter", "sei\u00b7nen", "H\u00e4n\u00b7den", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schaut, ein Lamm hat sich verlaufen,", "tokens": ["Schaut", ",", "ein", "Lamm", "hat", "sich", "ver\u00b7lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er eilet schnellen Lauf,", "tokens": ["Und", "er", "ei\u00b7let", "schnel\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft den ganzen andern Haufen,", "tokens": ["L\u00e4\u00dft", "den", "gan\u00b7zen", "an\u00b7dern", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchet sein verlornes auf.", "tokens": ["Su\u00b7chet", "sein", "ver\u00b7lor\u00b7nes", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf den Schultern heimgetragen,", "tokens": ["Auf", "den", "Schul\u00b7tern", "heim\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt es der getreue Hirt;", "tokens": ["Bringt", "es", "der", "ge\u00b7treu\u00b7e", "Hirt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keines darf nun \u00e4ngstlich zagen,", "tokens": ["Kei\u00b7nes", "darf", "nun", "\u00e4ngst\u00b7lich", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei es noch so weit verirrt.", "tokens": ["Sei", "es", "noch", "so", "weit", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00f6chtet ihr auf dieser Erden", "tokens": ["M\u00f6ch\u00b7tet", "ihr", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlen solche treue Hut,", "tokens": ["F\u00fch\u00b7len", "sol\u00b7che", "treu\u00b7e", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fc\u00dft ihr Sch\u00e4flein Christi werden,", "tokens": ["M\u00fc\u00dft", "ihr", "Sch\u00e4f\u00b7lein", "Chris\u00b7ti", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denen gibt er selbst sein Blut.", "tokens": ["De\u00b7nen", "gibt", "er", "selbst", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, mein Gott, auf deine Weiden,", "tokens": ["Herr", ",", "mein", "Gott", ",", "auf", "dei\u00b7ne", "Wei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dein Br\u00fcnnlein leite mich,", "tokens": ["An", "dein", "Br\u00fcnn\u00b7lein", "lei\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So durch Freuden, als durch Leiden,", "tokens": ["So", "durch", "Freu\u00b7den", ",", "als", "durch", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchre du mich seliglich!", "tokens": ["F\u00fch\u00b7re", "du", "mich", "se\u00b7lig\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}