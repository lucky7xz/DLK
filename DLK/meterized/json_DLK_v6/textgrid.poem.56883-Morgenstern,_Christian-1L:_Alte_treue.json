{"textgrid.poem.56883": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alte treue", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alte treue", "tokens": ["Al\u00b7te", "treu\u00b7e"], "token_info": ["word", "word"], "pos": ["ADJA", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fahne Einsamkeit,", "tokens": ["Fah\u00b7ne", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "mit dir scheue", "tokens": ["mit", "dir", "scheu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "ich keinen Streit.", "tokens": ["ich", "kei\u00b7nen", "Streit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "H\u00fclle mich ein,", "tokens": ["H\u00fcl\u00b7le", "mich", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "mein Panier,", "tokens": ["mein", "Pa\u00b7nier", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "in dir", "tokens": ["in", "dir"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "will ich leben und begraben sein.", "tokens": ["will", "ich", "le\u00b7ben", "und", "be\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}