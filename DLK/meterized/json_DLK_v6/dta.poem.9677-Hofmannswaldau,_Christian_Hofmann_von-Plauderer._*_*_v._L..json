{"dta.poem.9677": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Plauderer.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfe plaudrer insgemein nicht viel n\u00fctzen oder t\u00fcgen;", "tokens": ["Gro\u00b7\u00dfe", "plaud\u00b7rer", "ins\u00b7ge\u00b7mein", "nicht", "viel", "n\u00fct\u00b7zen", "o\u00b7der", "t\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Denn durch viel es plaudern wird meist gebohren vieles l\u00fcgen.", "tokens": ["Denn", "durch", "viel", "es", "plau\u00b7dern", "wird", "meist", "ge\u00b7boh\u00b7ren", "vie\u00b7les", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PPER", "VVINF", "VAFIN", "ADV", "VVPP", "PIS", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}