{"textgrid.poem.40431": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Fahndung", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder sprach der Rodenstein:", "tokens": ["Und", "wie\u00b7der", "sprach", "der", "Ro\u00b7dens\u00b7tein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbpelzkappenschwerenot!", "tokens": ["\u00bb", "pelz\u00b7kap\u00b7pen\u00b7schwe\u00b7re\u00b7not", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hans Breuning, Stabstrompeter mein,", "tokens": ["Hans", "Breu\u00b7ning", ",", "Stabs\u00b7trom\u00b7pe\u00b7ter", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist untreu oder tot?", "tokens": ["Bist", "un\u00b7treu", "o\u00b7der", "tot", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Lebst noch?... Lebst noch und hebst noch?", "tokens": ["Lebst", "noch", "?", "...", "Lebst", "noch", "und", "hebst", "noch", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "$(", "VVIMP", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Man g'sp\u00fcrt dich nirgend mehr ...", "tokens": ["Man", "g'\u00b7sp\u00fcrt", "dich", "nir\u00b7gend", "mehr", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon naht die durstige Maiweinzeit,", "tokens": ["Schon", "naht", "die", "durs\u00b7ti\u00b7ge", "Mai\u00b7wein\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du mu\u00dft mir wieder her!\u00ab", "tokens": ["Du", "mu\u00dft", "mir", "wie\u00b7der", "her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ritt, bis er gen Darmstadt kam,", "tokens": ["Er", "ritt", ",", "bis", "er", "gen", "Darm\u00b7stadt", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Fahnden war gegl\u00fcckt;", "tokens": ["Kein", "Fahn\u00b7den", "war", "ge\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da lacht' er, als am schwarzen Lamm", "tokens": ["Da", "lacht'", "er", ",", "als", "am", "schwar\u00b7zen", "Lamm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Fenster er geblickt:", "tokens": ["Durchs", "Fens\u00b7ter", "er", "ge\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bber lebt noch!... Lebt noch und hebt noch!", "tokens": ["\u00bb", "er", "lebt", "noch", "!", "...", "Lebt", "noch", "und", "hebt", "noch", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$.", "$(", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch frag' mich keiner: wie?", "tokens": ["Doch", "frag'", "mich", "kei\u00b7ner", ":", "wie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIS", "$.", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie kommt mein alter Fl\u00fcgelmann", "tokens": ["Wie", "kommt", "mein", "al\u00b7ter", "Fl\u00fc\u00b7gel\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In solche Kompagnie?\u00ab", "tokens": ["In", "sol\u00b7che", "Kom\u00b7pag\u00b7nie", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In Z\u00fcchten sa\u00df der Stammgastschar", "tokens": ["In", "Z\u00fcch\u00b7ten", "sa\u00df", "der", "Stamm\u00b7gast\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Rang und W\u00fcrden dort,", "tokens": ["Nach", "Rang", "und", "W\u00fcr\u00b7den", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcnnbier ihr Vespertr\u00fcnklein war,", "tokens": ["D\u00fcnn\u00b7bier", "ihr", "Ves\u00b7per\u00b7tr\u00fcn\u00b7klein", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es klang kein lautes Wort.", "tokens": ["Es", "klang", "kein", "lau\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbsacht stets!... Sacht und bedacht stets", "tokens": ["\u00bb", "sacht", "stets", "!", "...", "Sacht", "und", "be\u00b7dacht", "stets"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$.", "$(", "NN", "KON", "VVFIN", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ist Lebens Hochgenu\u00df\u00ab,", "tokens": ["Ist", "Le\u00b7bens", "Hoch\u00b7ge\u00b7nu\u00df", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "NN", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So fl\u00fcstert ein Kanzleimann just", "tokens": ["So", "fl\u00fcs\u00b7tert", "ein", "Kanz\u00b7lei\u00b7mann", "just"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Zum Kreisamtssyndikus.", "tokens": ["Zum", "Kreis\u00b7amts\u00b7syn\u00b7di\u00b7kus", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In dieser Sch\u00f6ppleinschl\u00fcrfer Reih'", "tokens": ["In", "die\u00b7ser", "Sch\u00f6p\u00b7plein\u00b7schl\u00fcr\u00b7fer", "Reih'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df auch ein stilles Gast,", "tokens": ["Sa\u00df", "auch", "ein", "stil\u00b7les", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als es acht Uhr war vorbei,", "tokens": ["Und", "als", "es", "acht", "Uhr", "war", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "CARD", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nahm's Stock und Hut mit Hast.", "tokens": ["Nahm's", "Stock", "und", "Hut", "mit", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbacht jetzt!... acht jetzt ... gut Nacht jetzt!", "tokens": ["\u00bb", "acht", "jetzt", "!", "...", "acht", "jetzt", "...", "gut", "Nacht", "jetzt", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "ADV", "$.", "$(", "CARD", "ADV", "$(", "ADJD", "NN", "ADV", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Einst war ich nicht so brav,", "tokens": ["Einst", "war", "ich", "nicht", "so", "brav", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch ehrbar wandeln ist das best'!", "tokens": ["Doch", "ehr\u00b7bar", "wan\u00b7deln", "ist", "das", "best'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "VAFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich geh' ins Bett und schlaf'.\u00ab", "tokens": ["Ich", "geh'", "ins", "Bett", "und", "schlaf'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Rodenstein in grimmem Zorn", "tokens": ["Der", "Ro\u00b7dens\u00b7tein", "in", "grim\u00b7mem", "Zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hub graunhaft sich empor;", "tokens": ["Hub", "graun\u00b7haft", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dreimal stie\u00df er ins J\u00e4gerhorn", "tokens": ["Drei\u00b7mal", "stie\u00df", "er", "ins", "J\u00e4\u00b7ger\u00b7horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und blies mit Macht den Chor:", "tokens": ["Und", "blies", "mit", "Macht", "den", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bb'raus da! 'raus aus dem Haus da!", "tokens": ["\u00bb", "'raus", "da", "!", "'raus", "aus", "dem", "Haus", "da", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "NE", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "'raus mit dem Deserteur!", "tokens": ["'raus", "mit", "dem", "De\u00b7ser\u00b7teur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Das lahme, zahme Gast da drin", "tokens": ["Das", "lah\u00b7me", ",", "zah\u00b7me", "Gast", "da", "drin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh\u00f6rt zum wilden Heer!\u00ab", "tokens": ["Ge\u00b7h\u00f6rt", "zum", "wil\u00b7den", "Heer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da fa\u00dft das Gast ein Schreck und Graus,", "tokens": ["Da", "fa\u00dft", "das", "Gast", "ein", "Schreck", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erst sank es tief ins Knie,", "tokens": ["Erst", "sank", "es", "tief", "ins", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann st\u00fcrzt es einen Ma\u00dfkrug aus,", "tokens": ["Dann", "st\u00fcrzt", "es", "ei\u00b7nen", "Ma\u00df\u00b7krug", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlug's Fenster ein und schrie:", "tokens": ["Schlug's", "Fens\u00b7ter", "ein", "und", "schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bb'naus da! 'naus aus dem Haus da!", "tokens": ["\u00bb", "'naus", "da", "!", "'naus", "aus", "dem", "Haus", "da", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "O Horn und Sporn und Zorn!", "tokens": ["O", "Horn", "und", "Sporn", "und", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Rodenstein! O Maienwein!", "tokens": ["O", "Ro\u00b7dens\u00b7tein", "!", "O", "Mai\u00b7en\u00b7wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch bin ich nicht verlor'n.", "tokens": ["Noch", "bin", "ich", "nicht", "ver\u00b7lor'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Rumdiridi, Freijagd!", "tokens": ["Rum\u00b7di\u00b7ri\u00b7di", ",", "Frei\u00b7jagd", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Hoidirido, Freinacht!", "tokens": ["Hoi\u00b7di\u00b7ri\u00b7do", ",", "Frei\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Alter Patron,", "tokens": ["Al\u00b7ter", "Pat\u00b7ron", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Empfah' deinen Sohn!", "tokens": ["Emp\u00b7fah'", "dei\u00b7nen", "Sohn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "----+", "measure": "unknown.measure.single"}, "line.13": {"text": "Hussah, halloh!", "tokens": ["Hus\u00b7sah", ",", "hal\u00b7loh", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Jo, hihahoh!", "tokens": ["Jo", ",", "hi\u00b7ha\u00b7hoh", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "'naus, 'naus, 'naus!\u00ab", "tokens": ["'naus", ",", "'naus", ",", "'naus", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}