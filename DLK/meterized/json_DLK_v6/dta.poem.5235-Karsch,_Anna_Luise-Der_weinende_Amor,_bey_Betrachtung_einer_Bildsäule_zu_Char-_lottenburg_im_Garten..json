{"dta.poem.5235": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Geh\u2019 Bube, fern von meinen Augen eile!", "tokens": ["Geh'", "Bu\u00b7be", ",", "fern", "von", "mei\u00b7nen", "Au\u00b7gen", "ei\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Friedrich mehr, als Jupiter?", "tokens": ["Ist", "Fried\u00b7rich", "mehr", ",", "als", "Ju\u00b7pi\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ja \u2014 schluchzte Amor, gieb mir \u2014 Mutter \u2014", "tokens": ["Ja", "schluchz\u00b7te", "A\u00b7mor", ",", "gieb", "mir", "Mut\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "NE", "$,", "VVIMP", "PPER", "$(", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "meine Pfeile!", "tokens": ["mei\u00b7ne", "Pfei\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mehr als ein Gott ist er!", "tokens": ["Mehr", "als", "ein", "Gott", "ist", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "++-+--", "measure": "unknown.measure.tri"}}}}}