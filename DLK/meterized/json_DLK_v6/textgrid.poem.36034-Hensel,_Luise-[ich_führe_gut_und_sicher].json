{"textgrid.poem.36034": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[ich f\u00fchre gut und sicher]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchre gut und sicher", "tokens": ["Ich", "f\u00fch\u00b7re", "gut", "und", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich deinen Pilgerlauf", "tokens": ["Dich", "dei\u00b7nen", "Pil\u00b7ger\u00b7lauf"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schlie\u00dfe dir die Pforte", "tokens": ["Und", "schlie\u00b7\u00dfe", "dir", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ew'gen Heimat auf.", "tokens": ["Der", "ew'\u00b7gen", "Hei\u00b7mat", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch hast du mich gebrochen,", "tokens": ["Doch", "hast", "du", "mich", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So nimm in tiefem Schmerz", "tokens": ["So", "nimm", "in", "tie\u00b7fem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Kopf mir ab, und dr\u00fccke", "tokens": ["Den", "Kopf", "mir", "ab", ",", "und", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich weinend in dein Herz.", "tokens": ["Mich", "wei\u00b7nend", "in", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}