{"dta.poem.11326": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den polternden Sterops.  \n C. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Sterops schl\u00e4gt ja ohne ruh", "tokens": ["Herr", "Ste\u00b7rops", "schl\u00e4gt", "ja", "oh\u00b7ne", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die th\u00fcren gar erschrecklich zu,", "tokens": ["Die", "th\u00fc\u00b7ren", "gar", "er\u00b7schreck\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mu\u00df ihm denn dran fehlen,", "tokens": ["Was", "mu\u00df", "ihm", "denn", "dran", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er sie so kan qu\u00e4len?", "tokens": ["Da\u00df", "er", "sie", "so", "kan", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die pfosten haben ihm ja warlich nichts gethan;", "tokens": ["Die", "pfos\u00b7ten", "ha\u00b7ben", "ihm", "ja", "war\u00b7lich", "nichts", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch la\u00dft ihn donnern, weil er kan:", "tokens": ["Doch", "la\u00dft", "ihn", "don\u00b7nern", ",", "weil", "er", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er will durch dieses schlagen", "tokens": ["Er", "will", "durch", "die\u00b7ses", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die d\u00fcnst\u2019 ihm aus dem kopffe jagen.", "tokens": ["Die", "d\u00fcnst'", "ihm", "aus", "dem", "kopf\u00b7fe", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}