{"dta.poem.13060": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eja da w\u00e4r alles recht und f\u00fcglich ausgedacht,", "tokens": ["\u201e", "ja", "da", "w\u00e4r", "al\u00b7les", "recht", "und", "f\u00fcg\u00b7lich", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201evielleicht auch schon so viel, als zu dem Zweck gebracht,", "tokens": ["\u201e", "viel\u00b7leicht", "auch", "schon", "so", "viel", ",", "als", "zu", "dem", "Zweck", "ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "ADV", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eman h\u00e4tt auch dieses Streits Entscheidung schon gewonnen,", "tokens": ["\u201e", "man", "h\u00e4tt", "auch", "die\u00b7ses", "Streits", "Ent\u00b7schei\u00b7dung", "schon", "ge\u00b7won\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "ADV", "PDAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewann ihr nicht Sterne w\u00e4rt, nein: sondern helle Sonnen.", "tokens": ["\u201e", "wann", "ihr", "nicht", "Ster\u00b7ne", "w\u00e4rt", ",", "nein", ":", "son\u00b7dern", "hel\u00b7le", "Son\u00b7nen", "."], "token_info": ["punct