{"textgrid.poem.24304": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Egomet quidem", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nierenkrank; nerv\u00f6s; herzleidend;", "tokens": ["Nie\u00b7ren\u00b7krank", ";", "ner\u00b7v\u00f6s", ";", "herz\u00b7lei\u00b7dend", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Korpulent; libidinos; rhachitisch;", "tokens": ["Kor\u00b7pu\u00b7lent", ";", "li\u00b7bi\u00b7di\u00b7nos", ";", "rhac\u00b7hi\u00b7tisch", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "ADJD", "$.", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nikotinisiert; thee\u00efnvergiftet;", "tokens": ["Ni\u00b7ko\u00b7ti\u00b7ni\u00b7siert", ";", "thee\u00efn\u00b7ver\u00b7gif\u00b7tet", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohne irgendwelches Bankguthaben;", "tokens": ["Oh\u00b7ne", "ir\u00b7gend\u00b7wel\u00b7ches", "Bank\u00b7gut\u00b7ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Religionslos; unbeamtet; ohne Titel;", "tokens": ["Re\u00b7li\u00b7gi\u00b7ons\u00b7los", ";", "un\u00b7be\u00b7am\u00b7tet", ";", "oh\u00b7ne", "Ti\u00b7tel", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "$.", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Unst\u00e4t, hier bald, dort bald, nirgendwo zu Hause;", "tokens": ["Un\u00b7st\u00e4t", ",", "hier", "bald", ",", "dort", "bald", ",", "nir\u00b7gend\u00b7wo", "zu", "Hau\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Egomet quidem.", "tokens": ["E\u00b7go\u00b7met", "qui\u00b7dem", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Aber:", "tokens": ["A\u00b7ber", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Nieren, Nerven, Herz: den ganzen Kadaver,", "tokens": ["Nie\u00b7ren", ",", "Ner\u00b7ven", ",", "Herz", ":", "den", "gan\u00b7zen", "Ka\u00b7da\u00b7ver", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Alles Tr\u00fcbe, Giftige, Kummertr\u00e4chtige,", "tokens": ["Al\u00b7les", "Tr\u00fc\u00b7be", ",", "Gif\u00b7ti\u00b7ge", ",", "Kum\u00b7mer\u00b7tr\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Alles Gemeine gerne und leicht vergessend,", "tokens": ["Al\u00b7les", "Ge\u00b7mei\u00b7ne", "ger\u00b7ne", "und", "leicht", "ver\u00b7ges\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "KON", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Anger\u00fchrt vom Genius des Augenblickes:", "tokens": ["An\u00b7ge\u00b7r\u00fchrt", "vom", "Ge\u00b7nius", "des", "Au\u00b7gen\u00b7bli\u00b7ckes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Stolz dann, frei dann, Grandseigneur und heiter,", "tokens": ["Stolz", "dann", ",", "frei", "dann", ",", "Grand\u00b7seig\u00b7neur", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "ADV", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dionysisch fromm mit allen G\u00f6ttern", "tokens": ["Di\u00b7o\u00b7ny\u00b7sisch", "fromm", "mit", "al\u00b7len", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Auf vertrautestem Fu\u00dfe; allen Teufeln", "tokens": ["Auf", "ver\u00b7trau\u00b7tes\u00b7tem", "Fu\u00b7\u00dfe", ";", "al\u00b7len", "Teu\u00b7feln"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Kennerm\u00e4\u00dfig tolerant leutselig", "tokens": ["Ken\u00b7ner\u00b7m\u00e4\u00b7\u00dfig", "to\u00b7le\u00b7rant", "leut\u00b7se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Zugetan wie alten treuen Dienern", "tokens": ["Zu\u00b7ge\u00b7tan", "wie", "al\u00b7ten", "treu\u00b7en", "Die\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und verliebt in alles Menschliche:", "tokens": ["Und", "ver\u00b7liebt", "in", "al\u00b7les", "Menschli\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Egomet quidem.", "tokens": ["E\u00b7go\u00b7met", "qui\u00b7dem", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}