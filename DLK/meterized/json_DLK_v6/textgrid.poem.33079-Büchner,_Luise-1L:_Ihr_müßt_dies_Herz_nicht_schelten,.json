{"textgrid.poem.33079": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr m\u00fc\u00dft dies Herz nicht schelten,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00fc\u00dft dies Herz nicht schelten,", "tokens": ["Ihr", "m\u00fc\u00dft", "dies", "Herz", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sich so schwer ergiebt,", "tokens": ["Das", "sich", "so", "schwer", "er\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6nnt' schneller es gesunden,", "tokens": ["K\u00f6nnt'", "schnel\u00b7ler", "es", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann h\u00e4tt' es nie geliebt.", "tokens": ["Dann", "h\u00e4tt'", "es", "nie", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gliche dann sein F\u00fchlen", "tokens": ["Es", "gli\u00b7che", "dann", "sein", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja nur dem Morgenthau,", "tokens": ["Ja", "nur", "dem", "Mor\u00b7gent\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den eine Sonnenstunde", "tokens": ["Den", "ei\u00b7ne", "Son\u00b7nen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinwegk\u00fc\u00dft von der Au.", "tokens": ["Hin\u00b7weg\u00b7k\u00fc\u00dft", "von", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann w\u00e4r' es wie die Welle,", "tokens": ["Dann", "w\u00e4r'", "es", "wie", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So leicht und schnell erregt,", "tokens": ["So", "leicht", "und", "schnell", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie der Sommerfaden,", "tokens": ["Und", "wie", "der", "Som\u00b7mer\u00b7fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den jeder Hauch bewegt.", "tokens": ["Den", "je\u00b7der", "Hauch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ach! es gleicht dem Felsen,", "tokens": ["Doch", "ach", "!", "es", "gleicht", "dem", "Fel\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sich nicht beugen l\u00e4\u00dft;", "tokens": ["Der", "sich", "nicht", "beu\u00b7gen", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie er am Schoo\u00df der Erde,", "tokens": ["Wie", "er", "am", "Schoo\u00df", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt es sein F\u00fchlen fest.", "tokens": ["H\u00e4lt", "es", "sein", "F\u00fch\u00b7len", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Weil man darauf kann bauen,", "tokens": ["Weil", "man", "da\u00b7rauf", "kann", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie auf den Felsengrund,", "tokens": ["Wie", "auf", "den", "Fel\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil es ein Starkes, Festes,", "tokens": ["Weil", "es", "ein", "Star\u00b7kes", ",", "Fes\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird es so schwer gesund!", "tokens": ["Wird", "es", "so", "schwer", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}