{"dta.poem.10460": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gegr\u00fc\u00dft, du gro\u00dfe,", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Geheimni\u00dfvolle Stadt,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Stadt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die einst in ihrem Schoo\u00dfe", "tokens": ["Die", "einst", "in", "ih\u00b7rem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Liebchen umschlossen hat.", "tokens": ["Mein", "Lieb\u00b7chen", "um\u00b7schlos\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sagt an, ihr Th\u00fcrme und Thore,", "tokens": ["Sagt", "an", ",", "ihr", "Th\u00fcr\u00b7me", "und", "Tho\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo ist die Liebste mein?", "tokens": ["Wo", "ist", "die", "Liebs\u00b7te", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch hab' ich sie anvertrauet,", "tokens": ["Euch", "hab'", "ich", "sie", "an\u00b7ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr solltet mir B\u00fcrge seyn.", "tokens": ["Ihr", "soll\u00b7tet", "mir", "B\u00fcr\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Unschuldig sind die Th\u00fcrme,", "tokens": ["Un\u00b7schul\u00b7dig", "sind", "die", "Th\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie konnten nicht von der Stell',", "tokens": ["Sie", "konn\u00b7ten", "nicht", "von", "der", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als sie mit Koffern und Schachteln", "tokens": ["Als", "sie", "mit", "Kof\u00b7fern", "und", "Schach\u00b7teln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Stadt verlassen so schnell.", "tokens": ["Die", "Stadt", "ver\u00b7las\u00b7sen", "so", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Die Thore jedoch, die lie\u00dfen", "tokens": ["Die", "Tho\u00b7re", "je\u00b7doch", ",", "die", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Liebchen entwischen gar still;", "tokens": ["Mein", "Lieb\u00b7chen", "ent\u00b7wi\u00b7schen", "gar", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Thor ist immer willig,", "tokens": ["Ein", "Thor", "ist", "im\u00b7mer", "wil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn eine Th\u00f6rin will.", "tokens": ["Wenn", "ei\u00b7ne", "Th\u00f6\u00b7rin", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}