{"textgrid.poem.59845": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mirza-Schaffy! Wo mu\u00df ich dich finden!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mirza-Schaffy! Wo mu\u00df ich dich finden!", "tokens": ["Mir\u00b7za\u00b7Schaffy", "!", "Wo", "mu\u00df", "ich", "dich", "fin\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin hat sich dein Fu\u00df verloren?", "tokens": ["Wo\u00b7hin", "hat", "sich", "dein", "Fu\u00df", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wie kommt der Sehende unter die Blinden,", "tokens": ["Wie", "kommt", "der", "Se\u00b7hen\u00b7de", "un\u00b7ter", "die", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie kommt der Weise zu den Toren?", "tokens": ["Wie", "kommt", "der", "Wei\u00b7se", "zu", "den", "To\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sprach: Was soll das Wort mir frommen?", "tokens": ["Ich", "sprach", ":", "Was", "soll", "das", "Wort", "mir", "from\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VMFIN", "ART", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weise mu\u00df zu den Toren gehn,", "tokens": ["Der", "Wei\u00b7se", "mu\u00df", "zu", "den", "To\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sonst w\u00fcrde die Weisheit verlorengehn,", "tokens": ["Sonst", "w\u00fcr\u00b7de", "die", "Weis\u00b7heit", "ver\u00b7lo\u00b7ren\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da Toren nie zum Weisen kommen.", "tokens": ["Da", "To\u00b7ren", "nie", "zum", "Wei\u00b7sen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die ihr so gro\u00df und klug euch deuchtet,", "tokens": ["Die", "ihr", "so", "gro\u00df", "und", "klug", "euch", "deuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6gt ihr das eine doch bedenken:", "tokens": ["M\u00f6gt", "ihr", "das", "ei\u00b7ne", "doch", "be\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne selbst, wenn sie uns leuchtet,", "tokens": ["Die", "Son\u00b7ne", "selbst", ",", "wenn", "sie", "uns", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df ihren Strahl zur Erde senken!", "tokens": ["Mu\u00df", "ih\u00b7ren", "Strahl", "zur", "Er\u00b7de", "sen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}