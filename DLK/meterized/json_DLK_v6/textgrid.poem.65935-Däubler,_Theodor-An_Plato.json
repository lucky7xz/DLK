{"textgrid.poem.65935": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Plato", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es trafen sich die Sonnen auf der Stirne,", "tokens": ["Es", "tra\u00b7fen", "sich", "die", "Son\u00b7nen", "auf", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Plato schwer zur innern Hut geneigt;", "tokens": ["Die", "Pla\u00b7to", "schwer", "zur", "in\u00b7nern", "Hut", "ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wu\u00dfte, durch des Leibes Blutbusch zweigt", "tokens": ["Er", "wu\u00df\u00b7te", ",", "durch", "des", "Lei\u00b7bes", "Blut\u00b7bu\u00b7sch", "zweigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Urspr\u00fcnglichkeit des Herzens, bis zum Firne", "tokens": ["Ur\u00b7spr\u00fcng\u00b7lich\u00b7keit", "des", "Her\u00b7zens", ",", "bis", "zum", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Krystallhafter Gestalten \u00fcberm Hirne,", "tokens": ["Krys\u00b7tall\u00b7haf\u00b7ter", "Ge\u00b7stal\u00b7ten", "\u00fc\u00b7berm", "Hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zu dem sich der Erbrachtheit Spiegel zeigt:", "tokens": ["Zu", "dem", "sich", "der", "Er\u00b7bracht\u00b7heit", "Spie\u00b7gel", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann, unter K\u00fchle, wo die Preisung schweigt,", "tokens": ["Dann", ",", "un\u00b7ter", "K\u00fch\u00b7le", ",", "wo", "die", "Prei\u00b7sung", "schweigt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erklingt der Gang erg\u00f6ttlichter Gestirne.", "tokens": ["Er\u00b7klingt", "der", "Gang", "er\u00b7g\u00f6tt\u00b7lich\u00b7ter", "Ge\u00b7stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Himmeln wandelte unter Platanen,", "tokens": ["Ihr", "Him\u00b7meln", "wan\u00b7del\u00b7te", "un\u00b7ter", "Pla\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da Plato, du, den alten Staat entwarfst!", "tokens": ["Da", "Pla\u00b7to", ",", "du", ",", "den", "al\u00b7ten", "Staat", "ent\u00b7warfst", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Mann erschritt Vollzug so klug den Ahnen,", "tokens": ["Kein", "Mann", "er\u00b7schritt", "Voll\u00b7zug", "so", "klug", "den", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Als er, der sich besonnen hat: du darfst,", "tokens": ["Als", "er", ",", "der", "sich", "be\u00b7son\u00b7nen", "hat", ":", "du", "darfst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PRF", "VVPP", "VAFIN", "$.", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu steigen, Liebendlebende ermahnen!", "tokens": ["Zu", "stei\u00b7gen", ",", "Lie\u00b7bend\u00b7le\u00b7ben\u00b7de", "er\u00b7mah\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ergreif uns Herr, der du Gesetze harfst!", "tokens": ["Er\u00b7greif", "uns", "Herr", ",", "der", "du", "Ge\u00b7set\u00b7ze", "harfst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}