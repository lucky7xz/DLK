{"dta.poem.18489": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Atheisten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus allen glauben pflegt ich etwas mir zu klauben/", "tokens": ["Aus", "al\u00b7len", "glau\u00b7ben", "pflegt", "ich", "et\u00b7was", "mir", "zu", "klau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VVFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df gott und teuffel sey/ das kont ich nur nicht glauben/", "tokens": ["Da\u00df", "gott", "und", "teuf\u00b7fel", "sey", "/", "das", "kont", "ich", "nur", "nicht", "glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "$(", "PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gar anders bin ich jetzt/ gelehret durch den todt/", "tokens": ["Gar", "an\u00b7ders", "bin", "ich", "jetzt", "/", "ge\u00b7leh\u00b7ret", "durch", "den", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "$(", "VVPP", "APPR", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich f\u00fchle was die h\u00f6ll\u2019/ und wei\u00df nun da\u00df ein gott.", "tokens": ["Ich", "f\u00fch\u00b7le", "was", "die", "h\u00f6ll'", "/", "und", "wei\u00df", "nun", "da\u00df", "ein", "gott", "."], "token_info": ["word", "word", "word", "word", "