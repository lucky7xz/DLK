{"dta.poem.16250": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "304.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vermeiden sollen sich, die nicht zusammenpassen;", "tokens": ["Ver\u00b7mei\u00b7den", "sol\u00b7len", "sich", ",", "die", "nicht", "zu\u00b7sam\u00b7men\u00b7pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "$,", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wahl der Gesellschaft ist jedwedem freigelassen.", "tokens": ["Wahl", "der", "Ge\u00b7sell\u00b7schaft", "ist", "jed\u00b7we\u00b7dem", "frei\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Zu wen'gen passen, ist ein nicht geringes Leiden,", "tokens": ["Zu", "wen'\u00b7gen", "pas\u00b7sen", ",", "ist", "ein", "nicht", "ge\u00b7rin\u00b7ges", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$,", "VAFIN", "ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn schwer ist mit der Welt Ber\u00fchrung zu vermeiden.", "tokens": ["Denn", "schwer", "ist", "mit", "der", "Welt", "Be\u00b7r\u00fch\u00b7rung", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ganz ungl\u00fccklich ist, wer allen Umgang ha\u00dft,", "tokens": ["Doch", "ganz", "un\u00b7gl\u00fcck\u00b7lich", "ist", ",", "wer", "al\u00b7len", "Um\u00b7gang", "ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, auf sich selbst beschr\u00e4nkt, auch zu sich selbst nicht pa\u00dft.", "tokens": ["Und", ",", "auf", "sich", "selbst", "be\u00b7schr\u00e4nkt", ",", "auch", "zu", "sich", "selbst", "nicht", "pa\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "ADV", "APPR", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}