{"textgrid.poem.31732": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Erst achtzehn Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein letztes Gl\u00fchn! Da zog an brit'scher K\u00fcste", "tokens": ["Ein", "letz\u00b7tes", "Gl\u00fchn", "!", "Da", "zog", "an", "brit'\u00b7scher", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D\u00e4mmernd herauf die sch\u00f6nste Winternacht;", "tokens": ["D\u00e4m\u00b7mernd", "her\u00b7auf", "die", "sch\u00f6ns\u00b7te", "Win\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im Mondenstrahle flo\u00df die Wasserw\u00fcste,", "tokens": ["Im", "Mon\u00b7dens\u00b7trah\u00b7le", "flo\u00df", "die", "Was\u00b7ser\u00b7w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den H\u00fcgeln lag des Schnees Pracht.", "tokens": ["Und", "auf", "den", "H\u00fc\u00b7geln", "lag", "des", "Schnees", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Leer das Gestad. Es schwieg der Dampfer Sausen;", "tokens": ["Leer", "das", "Ge\u00b7stad", ".", "Es", "schwieg", "der", "Damp\u00b7fer", "Sau\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Matros und Krieger war'n des Tages matt; \u2013", "tokens": ["Mat\u00b7ros", "und", "Krie\u00b7ger", "wa\u00b7r'n", "des", "Ta\u00b7ges", "matt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Doch durch die Stille sandte dumpf ihr Brausen", "tokens": ["Doch", "durch", "die", "Stil\u00b7le", "sand\u00b7te", "dumpf", "ihr", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "London, der Themse dunkle Riesenstadt.", "tokens": ["Lon\u00b7don", ",", "der", "Them\u00b7se", "dunk\u00b7le", "Rie\u00b7sen\u00b7stadt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Ihr galt es gleich, mocht auch der Schlummer dr\u00fccken", "tokens": ["Ihr", "galt", "es", "gleich", ",", "mocht", "auch", "der", "Schlum\u00b7mer", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch m\u00fcdes Auge zu ersehnter Ruh;", "tokens": ["Manch", "m\u00fc\u00b7des", "Au\u00b7ge", "zu", "er\u00b7sehn\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es w\u00e4lzte donnernd \u00fcber Park und Br\u00fccken", "tokens": ["Es", "w\u00e4lz\u00b7te", "don\u00b7nernd", "\u00fc\u00b7ber", "Park", "und", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Derselbe L\u00e4rm sich nur dem Morgen zu.", "tokens": ["Der\u00b7sel\u00b7be", "L\u00e4rm", "sich", "nur", "dem", "Mor\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zaubrisch und still da drau\u00dfen das Gefild!", "tokens": ["Zaub\u00b7risch", "und", "still", "da", "drau\u00b7\u00dfen", "das", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Hier nur das Volk, in buntem Strome, wild", "tokens": ["Hier", "nur", "das", "Volk", ",", "in", "bun\u00b7tem", "Stro\u00b7me", ",", "wild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zusammenflutend, schaffend, ringend, suchend,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7flu\u00b7tend", ",", "schaf\u00b7fend", ",", "rin\u00b7gend", ",", "su\u00b7chend", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schwelgend und darbend, betend bald und fluchend!", "tokens": ["Schwel\u00b7gend", "und", "dar\u00b7bend", ",", "be\u00b7tend", "bald", "und", "flu\u00b7chend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "VVPP", "ADV", "KON", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und Schimmern rings, von Dach und Tor und Fenster;", "tokens": ["Und", "Schim\u00b7mern", "rings", ",", "von", "Dach", "und", "Tor", "und", "Fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort buhlt die Luft in seidenem Gewand!", "tokens": ["Dort", "buhlt", "die", "Luft", "in", "sei\u00b7de\u00b7nem", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier \u00fcberm Golde h\u00f6hnische Gespenster", "tokens": ["Hier", "\u00fc\u00b7berm", "Gol\u00b7de", "h\u00f6h\u00b7ni\u00b7sche", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dort geballt die magre Bettlerhand!", "tokens": ["Und", "dort", "ge\u00b7ballt", "die", "mag\u00b7re", "Bett\u00b7ler\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Seufzer hier, ein Ku\u00df dort! Von Terrassen", "tokens": ["Ein", "Seuf\u00b7zer", "hier", ",", "ein", "Ku\u00df", "dort", "!", "Von", "Ter\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Treppen: Jubel, Fl\u00fcstern und Gest\u00f6hn \u2013", "tokens": ["Und", "Trep\u00b7pen", ":", "Ju\u00b7bel", ",", "Fl\u00fcs\u00b7tern", "und", "Ge\u00b7st\u00f6hn", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das ist der Tanz, in dem auf Londons Gassen", "tokens": ["Das", "ist", "der", "Tanz", ",", "in", "dem", "auf", "Lon\u00b7dons", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich rastlos zwei Millionen Menschen drehn!", "tokens": ["Sich", "rast\u00b7los", "zwei", "Mil\u00b7lion\u00b7en", "Men\u00b7schen", "drehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "CARD", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er brauste fort. Da hob auch ", "tokens": ["Er", "braus\u00b7te", "fort", ".", "Da", "hob", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich sacht empor; es fiel der Sterne Licht", "tokens": ["Sich", "sacht", "em\u00b7por", ";", "es", "fiel", "der", "Ster\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf die Gestalt, so tief gebeugt, so hager,", "tokens": ["Auf", "die", "Ge\u00b7stalt", ",", "so", "tief", "ge\u00b7beugt", ",", "so", "ha\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf ihr bleiches, starres Angesicht.", "tokens": ["Und", "auf", "ihr", "blei\u00b7ches", ",", "star\u00b7res", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie sann \u2013 nur einen Augenblick; sie pre\u00dfte", "tokens": ["Sie", "sann", "\u2013", "nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ";", "sie", "pre\u00df\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das kranke Kind an ihre nackte Brust;", "tokens": ["Das", "kran\u00b7ke", "Kind", "an", "ih\u00b7re", "nack\u00b7te", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das arme Weib schritt rasch durch die Pal\u00e4ste;", "tokens": ["Das", "ar\u00b7me", "Weib", "schritt", "rasch", "durch", "die", "Pa\u00b7l\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ach, das Wohin \u2013 sie hat es nicht gewu\u00dft!", "tokens": ["Ach", ",", "das", "Wo\u00b7hin", "\u2013", "sie", "hat", "es", "nicht", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbder Mutter Brot! Und Kleider diesem Kinde!\u00ab", "tokens": ["\u00bb", "der", "Mut\u00b7ter", "Brot", "!", "Und", "Klei\u00b7der", "die\u00b7sem", "Kin\u00b7de", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "KON", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rief sie. \u00bbOh, wie toll das Herz mir schl\u00e4gt!", "tokens": ["So", "rief", "sie", ".", "\u00bb", "Oh", ",", "wie", "toll", "das", "Herz", "mir", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ITJ", "$,", "PWAV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gern tr\u00fcg ich dich, mein Sohn, so warm und linde,", "tokens": ["Gern", "tr\u00fcg", "ich", "dich", ",", "mein", "Sohn", ",", "so", "warm", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie wohl die Mutter ihre Kinder tr\u00e4gt.", "tokens": ["Wie", "wohl", "die", "Mut\u00b7ter", "ih\u00b7re", "Kin\u00b7der", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch ist es Zeit! Bist du erst gro\u00dfgezogen", "tokens": ["Noch", "ist", "es", "Zeit", "!", "Bist", "du", "erst", "gro\u00df\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und siehst am Strand der Schiffe bunte Schar:", "tokens": ["Und", "siehst", "am", "Strand", "der", "Schif\u00b7fe", "bun\u00b7te", "Schar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da eilst du treulos durch die blauen Wogen,", "tokens": ["Da", "eilst", "du", "treu\u00b7los", "durch", "die", "blau\u00b7en", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein wilder Seemann, wie dein Vater war!", "tokens": ["Ein", "wil\u00b7der", "See\u00b7mann", ",", "wie", "dein", "Va\u00b7ter", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dein Vater? Still! \u2013 Das war ein sel'ger Morgen,", "tokens": ["Dein", "Va\u00b7ter", "?", "Still", "!", "\u2013", "Das", "war", "ein", "sel'\u00b7ger", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADJD", "$.", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als weinend ich an seiner Brust erwacht!", "tokens": ["Als", "wei\u00b7nend", "ich", "an", "sei\u00b7ner", "Brust", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kam der Mai, der Juni drauf, verborgen", "tokens": ["Es", "kam", "der", "Mai", ",", "der", "Ju\u00b7ni", "drauf", ",", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hielt ich, was fr\u00fch mich schon so bleich gemacht.", "tokens": ["Hielt", "ich", ",", "was", "fr\u00fch", "mich", "schon", "so", "bleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADJD", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erst als im Herbst das gelbe Laub der B\u00e4ume", "tokens": ["Erst", "als", "im", "Herbst", "das", "gel\u00b7be", "Laub", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Leis rauschend in die gr\u00fcne Themse fiel:", "tokens": ["Leis", "r