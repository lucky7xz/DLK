rd", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Und erm\u00fcdet schlafen ein,", "tokens": ["Und", "er\u00b7m\u00fc\u00b7det", "schla\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Mu\u00df mein Herz dennoch geflissen", "tokens": ["Mu\u00df", "mein", "Herz", "den\u00b7noch", "ge\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Und auf dich gerichtet sein.", "tokens": ["Und", "auf", "dich", "ge\u00b7rich\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.69": {"text": "Meiner Seele mit Begier", "tokens": ["Mei\u00b7ner", "See\u00b7le", "mit", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.70": {"text": "Tr\u00e4ume stets, o Gott, von dir,", "tokens": ["Tr\u00e4u\u00b7me", "stets", ",", "o", "Gott", ",", "von", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Da\u00df ich fest an dir bekleibe", "tokens": ["Da\u00df", "ich", "fest", "an", "dir", "be\u00b7klei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Und auch schlafend dein verbleibe.", "tokens": ["Und", "auch", "schla\u00b7fend", "dein", "ver\u00b7blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "La\u00df mich diese Nacht empfinden", "tokens": ["La\u00df", "mich", "die\u00b7se", "Nacht", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Eine sanft und s\u00fc\u00dfe Ruh,", "tokens": ["Ei\u00b7ne", "sanft", "und", "s\u00fc\u00b7\u00dfe", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Alles Uebel la\u00df verschwinden,", "tokens": ["Al\u00b7les", "Ue\u00b7bel", "la\u00df", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIMP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Decke mich mit Segen zu;", "tokens": ["De\u00b7cke", "mich", "mit", "Se\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Leib und Seele, Mut und Blut,", "tokens": ["Leib", "und", "See\u00b7le", ",", "Mut", "und", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.78": {"text": "Weib und Kinder, Hab' und Gut,", "tokens": ["Weib", "und", "Kin\u00b7der", ",", "Hab'", "und", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", 