{"textgrid.poem.49333": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Abendgesang", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werde munter, mein Gem\u00fcte,", "tokens": ["Wer\u00b7de", "mun\u00b7ter", ",", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr Sinne geht herf\u00fcr,", "tokens": ["Und", "ihr", "Sin\u00b7ne", "geht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr preiset Gottes G\u00fcte,", "tokens": ["Da\u00df", "ihr", "prei\u00b7set", "Got\u00b7tes", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch' er hat gethan an mir,", "tokens": ["Welch'", "er", "hat", "ge\u00b7than", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als er mich den ganzen Tag", "tokens": ["Als", "er", "mich", "den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr so mancher schweren Plag", "tokens": ["F\u00fcr", "so", "man\u00b7cher", "schwe\u00b7ren", "Plag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat erhalten und besch\u00fctzet,", "tokens": ["Hat", "er\u00b7hal\u00b7ten", "und", "be\u00b7sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df mich Satan nicht beschmitzet.", "tokens": ["Da\u00df", "mich", "Sa\u00b7tan", "nicht", "be\u00b7schmit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lob und Dank sei dir gesungen,", "tokens": ["Lob", "und", "Dank", "sei", "dir", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vater der Barmherzigkeit,", "tokens": ["Va\u00b7ter", "der", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df mir ist mein Werk gelungen,", "tokens": ["Da\u00df", "mir", "ist", "mein", "Werk", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df du mich f\u00fcr allem Leid", "tokens": ["Da\u00df", "du", "mich", "f\u00fcr", "al\u00b7lem", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und f\u00fcr S\u00fcnden mancher Art", "tokens": ["Und", "f\u00fcr", "S\u00fcn\u00b7den", "man\u00b7cher", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "So getreulich hast bewahrt,", "tokens": ["So", "ge\u00b7treu\u00b7lich", "hast", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Auch die Feind hinweg getrieben,", "tokens": ["Auch", "die", "Feind", "hin\u00b7weg", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df ich unbesch\u00e4digt blieben.", "tokens": ["Da\u00df", "ich", "un\u00b7be\u00b7sch\u00e4\u00b7digt", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Keine Klugheit kan verstehen", "tokens": ["Kei\u00b7ne", "Klug\u00b7heit", "kan", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Deine G\u00fct' und Wunderthat;", "tokens": ["Dei\u00b7ne", "G\u00fct'", "und", "Wun\u00b7der\u00b7that", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ja, kein Menschenkind kan sehen,", "tokens": ["Ja", ",", "kein", "Men\u00b7schen\u00b7kind", "kan", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Was dein' Hand erwiesen hat;", "tokens": ["Was", "dein'", "Hand", "er\u00b7wie\u00b7sen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Deine Wolthat ist zu viel,", "tokens": ["Dei\u00b7ne", "Wolt\u00b7hat", "ist", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Sie hat weder Ma\u00df noch Ziel.", "tokens": ["Sie", "hat", "we\u00b7der", "Ma\u00df", "noch", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Herr, du hast mich so gef\u00fchret,", "tokens": ["Herr", ",", "du", "hast", "mich", "so", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da\u00df kein Unfall mich ber\u00fchret.", "tokens": ["Da\u00df", "kein", "Un\u00b7fall", "mich", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Dieser Tag ist nun vergangen,", "tokens": ["Die\u00b7ser", "Tag", "ist", "nun", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Die betr\u00fcbte Nacht bricht an;", "tokens": ["Die", "be\u00b7tr\u00fcb\u00b7te", "Nacht", "bricht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Es ist hin der Sonnen Prangen,", "tokens": ["Es", "ist", "hin", "der", "Son\u00b7nen", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Welch' uns all' erfreuen kan.", "tokens": ["Welch'", "uns", "all'", "er\u00b7freu\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Stehe mir, o Vater, bei,", "tokens": ["Ste\u00b7he", "mir", ",", "o", "Va\u00b7ter", ",", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Da\u00df dein Glanz stets vor mir sei", "tokens": ["Da\u00df", "dein", "Glanz", "stets", "vor", "mir", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und mein kaltes Herz erhitze,", "tokens": ["Und", "mein", "kal\u00b7tes", "Herz", "er\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wenn ich gleich im Finstern sitze.", "tokens": ["Wenn", "ich", "gleich", "im", "Fins\u00b7tern", "sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Herr, verzeihe mir aus Gnaden", "tokens": ["Herr", ",", "ver\u00b7zei\u00b7he", "mir", "aus", "Gna\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Alle S\u00fcnd' und Missethat,", "tokens": ["Al\u00b7le", "S\u00fcnd'", "und", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Die mein armes Herz beladen", "tokens": ["Die", "mein", "ar\u00b7mes", "Herz", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und so gar vergiftet hat,", "tokens": ["Und", "so", "gar", "ver\u00b7gif\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Da\u00df auch Satan b\u00f6s und stil", "tokens": ["Da\u00df", "auch", "Sa\u00b7tan", "b\u00f6s", "und", "stil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Mich zur H\u00f6llen st\u00fcrzen wil.", "tokens": ["Mich", "zur", "H\u00f6l\u00b7len", "st\u00fcr\u00b7zen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Aber, Herr, du kanst mich retten,", "tokens": ["A\u00b7ber", ",", "Herr", ",", "du", "kanst", "mich", "ret\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Strafe nicht mein \u00dcbertreten.", "tokens": ["Stra\u00b7fe", "nicht", "mein", "\u00dc\u00b7bert\u00b7re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Bin ich gleich von dir gewichen,", "tokens": ["Bin", "ich", "gleich", "von", "dir", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Stell' ich mich doch wieder ein,", "tokens": ["Stell'", "ich", "mich", "doch", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Hat uns doch dein Sohn verglichen", "tokens": ["Hat", "uns", "doch", "dein", "Sohn", "ver\u00b7gli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Durch sein' Angst und Todespein.", "tokens": ["Durch", "sein'", "Angst", "und", "To\u00b7de\u00b7spein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ich verleugne nicht die Schuld,", "tokens": ["Ich", "ver\u00b7leug\u00b7ne", "nicht", "die", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Aber deine Gnad und Huld", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Gnad", "und", "Huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ist viel gr\u00f6\u00dfer als die S\u00fcnde,", "tokens": ["Ist", "viel", "gr\u00f6\u00b7\u00dfer", "als", "die", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Welch' ich stets in mir befinde.", "tokens": ["Welch'", "ich", "stets", "in", "mir", "be\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "O du Licht der frommen Seelen,", "tokens": ["O", "du", "Licht", "der", "from\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "O du Glanz der Ewigkeit,", "tokens": ["O", "du", "Glanz", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Dir wil ich mich ganz befehlen", "tokens": ["Dir", "wil", "ich", "mich", "ganz", "be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Diese Nacht und allezeit.", "tokens": ["Die\u00b7se", "Nacht", "und", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Bleibe doch, mein Gott, bei mir,", "tokens": ["Blei\u00b7be", "doch", ",", "mein", "Gott", ",", "bei", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Weil es nunmehr dunkel schier", "tokens": ["Weil", "es", "nun\u00b7mehr", "dun\u00b7kel", "schier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Und ich mich sehr drob betr\u00fcbe;", "tokens": ["Und", "ich", "mich", "sehr", "drob", "be\u00b7tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Tr\u00f6ste mich mit deiner Liebe.", "tokens": ["Tr\u00f6s\u00b7te", "mich", "mit", "dei\u00b7ner", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Sch\u00fctze mich f\u00fcrs Teufels Netzen,", "tokens": ["Sch\u00fct\u00b7ze", "mich", "f\u00fcrs", "Teu\u00b7fels", "Net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "F\u00fcr der Macht der Finsternis,", "tokens": ["F\u00fcr", "der", "Macht", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Die mir