{"dta.poem.9607": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Streit der jungen und alten jungfern/  \n welchen von beyden der vorzug  \n geb\u00fchre.  \n Vortrag der jungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr schwestern/ die ihr schon mehr jahre k\u00f6nnet zehlen/", "tokens": ["Ihr", "schwes\u00b7tern", "/", "die", "ihr", "schon", "mehr", "jah\u00b7re", "k\u00f6n\u00b7net", "zeh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "PRELS", "PPER", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als gute z\u00e4hne noch in eurem munde stehn;", "tokens": ["Als", "gu\u00b7te", "z\u00e4h\u00b7ne", "noch", "in", "eu\u00b7rem", "mun\u00b7de", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was habt ihr wol vor recht auff unsre lust zu schm\u00e4hlen?", "tokens": ["Was", "habt", "ihr", "wol", "vor", "recht", "auff", "uns\u00b7re", "lust", "zu", "schm\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Meint ihr/ wir sollen euch gleich aus dem wege gehn?", "tokens": ["Meint", "ihr", "/", "wir", "sol\u00b7len", "euch", "gleich", "aus", "dem", "we\u00b7ge", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein! eh wir eurem trotz den stoltzen willen lassen/", "tokens": ["Nein", "!", "eh", "wir", "eu\u00b7rem", "trotz", "den", "stolt\u00b7zen", "wil\u00b7len", "las\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KOUS", "PPER", "PPOSAT", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Soll ein gesch\u00e4rfftes stahl des streites richter seyn;", "tokens": ["Soll", "ein", "ge\u00b7sch\u00e4rff\u00b7tes", "stahl", "des", "strei\u00b7tes", "rich\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Warum soll unsern schertz ein scheeles auge hassen?", "tokens": ["Wa\u00b7rum", "soll", "un\u00b7sern", "schertz", "ein", "schee\u00b7les", "au\u00b7ge", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir jungen bilden uns mehr als ihr alten ein.", "tokens": ["Wir", "jun\u00b7gen", "bil\u00b7den", "uns", "mehr", "als", "ihr", "al\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVFIN", "PPER", "PIAT", "KOKOM", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr habt zwar freylich schon viel mehr als wir erfahren/", "tokens": ["Ihr", "habt", "zwar", "frey\u00b7lich", "schon", "viel", "mehr", "als", "wir", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Weil unsre jugend euch noch erstlich lehrgeld zahlt;", "tokens": ["Weil", "uns\u00b7re", "ju\u00b7gend", "euch", "noch", "erst\u00b7lich", "lehr\u00b7geld", "zahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein wie? kommt auch wol verstand noch vor den jahren?", "tokens": ["Al\u00b7lein", "wie", "?", "kommt", "auch", "wol", "ver\u00b7stand", "noch", "vor", "den", "jah\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "$.", "VVFIN", "ADV", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum macht ihr euch selbst alt/ wen\u0303 ihr mit klugheit prahlt.", "tokens": ["Drum", "macht", "ihr", "euch", "selbst", "alt", "/", "we\u00f1", "ihr", "mit", "klug\u00b7heit", "prahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$(", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was aber acht man doch ein altes ungeheuer?", "tokens": ["Was", "a\u00b7ber", "acht", "man", "doch", "ein", "al\u00b7tes", "un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "CARD", "PIS", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein frisch und junges ding zieht man den alten f\u00fcr;", "tokens": ["Ein", "frisch", "und", "jun\u00b7ges", "ding", "zieht", "man", "den", "al\u00b7ten", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "VVFIN", "PIS", "ART", "ADJA", "APPR", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ein alter hering kost nicht so viel als ein neuer/", "tokens": ["Ein", "al\u00b7ter", "he\u00b7ring", "kost", "nicht", "so", "viel", "als", "ein", "neu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "PIAT", "KOKOM", "ART", "ADJA", "$("], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ein junges pferd das gilt mehr als ein altes thier.", "tokens": ["Ein", "jun\u00b7ges", "pferd", "das", "gilt", "mehr", "als", "ein", "al\u00b7tes", "thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VVFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer steckt die nase gern zu faulen pomerantzen?", "tokens": ["Wer", "steckt", "die", "na\u00b7se", "gern", "zu", "fau\u00b7len", "po\u00b7me\u00b7rant\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wer liebt ein altes licht/ das wie der teuffel stinckt?", "tokens": ["Wer", "liebt", "ein", "al\u00b7tes", "licht", "/", "das", "wie", "der", "teuf\u00b7fel", "stinckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$(", "PDS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer will doch allererst ein altes schlo\u00df verschantzen/", "tokens": ["Wer", "will", "doch", "al\u00b7le\u00b7rerst", "ein", "al\u00b7tes", "schlo\u00df", "ver\u00b7schant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das allbereits zerf\u00e4llt und im morast versinckt?", "tokens": ["Das", "all\u00b7be\u00b7reits", "zer\u00b7f\u00e4llt", "und", "im", "mo\u00b7rast", "ver\u00b7sinckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "KON", "APPRART", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Drum must ihr alten nun uns jungen m\u00e4dchen weichen/", "tokens": ["Drum", "must", "ihr", "al\u00b7ten", "nun", "uns", "jun\u00b7gen", "m\u00e4d\u00b7chen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "ADJA", "ADV", "PPER", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr seyd schon halb verdorrt/ wir seynd noch frisch und gr\u00fcn/", "tokens": ["Ihr", "seyd", "schon", "halb", "ver\u00b7dorrt", "/", "wir", "seynd", "noch", "frisch", "und", "gr\u00fcn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$(", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr seyd kaum schwartzem bley/ wir golde zu vergleichen/", "tokens": ["Ihr", "seyd", "kaum", "schwart\u00b7zem", "bley", "/", "wir", "gol\u00b7de", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$(", "PPER", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Um unsre scheitel bl\u00fcht der herrlichste je\u00dfmin:", "tokens": ["Um", "uns\u00b7re", "schei\u00b7tel", "bl\u00fcht", "der", "herr\u00b7lichs\u00b7te", "je\u00df\u00b7min", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In unserm busen find man schnee und brand beysammen/", "tokens": ["In", "un\u00b7serm", "bu\u00b7sen", "find", "man", "schnee", "und", "brand", "bey\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der hal\u00df der \u00fcbertrifft den allerwei\u00dften schwan/", "tokens": ["Der", "hal\u00df", "der", "\u00fc\u00b7bert\u00b7rifft", "den", "al\u00b7ler\u00b7wei\u00df\u00b7ten", "schwan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Au\u00df unsern augen gehn die st\u00e4rcksten liebes-flammen", "tokens": ["Au\u00df", "un\u00b7sern", "au\u00b7gen", "gehn", "die", "st\u00e4rcks\u00b7ten", "lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und z\u00fcnden wie ein blitz der m\u00e4nner hertzen an:", "tokens": ["Und", "z\u00fcn\u00b7den", "wie", "ein", "blitz", "der", "m\u00e4n\u00b7ner", "hert\u00b7zen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die wangen bl\u00fchen uns voll lieblicher narcissen/", "tokens": ["Die", "wan\u00b7gen", "bl\u00fc\u00b7hen", "uns", "voll", "lieb\u00b7li\u00b7cher", "nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Auff denen liljen-milch und rosen-purpur lacht.", "tokens": ["Auff", "de\u00b7nen", "lil\u00b7jen\u00b7milch", "und", "ro\u00b7sen\u00b7pur\u00b7pur", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Wie mancher pflegt vergn\u00fcgt auf unsern mund zu k\u00fcssen/", "tokens": ["Wie", "man\u00b7cher", "pflegt", "ver\u00b7gn\u00fcgt", "auf", "un\u00b7sern", "mund", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der ihm die seel entz\u00fcckt/ das hertz voll feuer macht.", "tokens": ["Der", "ihm", "die", "seel", "ent\u00b7z\u00fcckt", "/", "das", "hertz", "voll", "feu\u00b7er", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$(", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir k\u00f6nnen ohne zwang die st\u00e4rcksten \u00fcberwinden/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "oh\u00b7ne", "zwang", "die", "st\u00e4rcks\u00b7ten", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wir schlagen unsren feind durch einen holden strahl;", "tokens": ["Wir", "schla\u00b7gen", "un\u00b7sren", "feind", "durch", "ei\u00b7nen", "hol\u00b7den", "strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die liebsten k\u00f6nnen wir so fest als ketten binden", "tokens": ["Die", "liebs\u00b7ten", "k\u00f6n\u00b7nen", "wir", "so", "fest", "als", "ket\u00b7ten", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "ADV", "ADJD", "KOKOM", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und f\u00fchren sie erl\u00e4chtzt zum frischen liebes-thal.", "tokens": ["Und", "f\u00fch\u00b7ren", "sie", "er\u00b7l\u00e4chtzt", "zum", "fri\u00b7schen", "lie\u00b7bes\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dann rufft ein ieder uns nicht anders als/ mein engel/", "tokens": ["Dann", "rufft", "ein", "ie\u00b7der", "uns", "nicht", "an\u00b7ders", "als", "/", "mein", "en\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PPER", "PTKNEG", "ADV", "KOUS", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mein hertzgen/ sch\u00e4tzgen/ kind/ mein morgenstern/ mein licht/", "tokens": ["Mein", "hertz\u00b7gen", "/", "sch\u00e4tz\u00b7gen", "/", "kind", "/", "mein", "mor\u00b7gens\u00b7tern", "/", "mein", "licht", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$(", "VVFIN", "$(", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Und streut auff unsern schoo\u00df die sch\u00f6nsten nelcken-stengel/", "tokens": ["Und", "streut", "auff", "un\u00b7sern", "schoo\u00df", "die", "sch\u00f6ns\u00b7ten", "nel\u00b7cken\u00b7sten\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dergleichen eure hand/ ihr alten/ niemahls bricht