{"dta.poem.3607": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Maria .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind sieh an die Br\u00fcste mein,               ", "tokens": ["Mein", "Kind", "sieh", "an", "die", "Br\u00fcs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein S\u00fcnder la\u00df verloren seyn.", "tokens": ["Kein", "S\u00fcn\u00b7der", "la\u00df", "ver\u00b7lo\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mutter, sieh an die Wunden,", "tokens": ["Mut\u00b7ter", ",", "sieh", "an", "die", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die ich f\u00fcr dein S\u00fcnd trag alle Stunden.", "tokens": ["Die", "ich", "f\u00fcr", "dein", "S\u00fcnd", "trag", "al\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vater, la\u00df dir die Wunden mein,", "tokens": ["Va\u00b7ter", ",", "la\u00df", "dir", "die", "Wun\u00b7den", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ein Opfer f\u00fcr die S\u00fcnde seyn.", "tokens": ["Ein", "Op\u00b7fer", "f\u00fcr", "die", "S\u00fcn\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sohn, lieber Sohn mein,", "tokens": ["Sohn", ",", "lie\u00b7ber", "Sohn", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "PPOSAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Alles was du begehrst, das soll seyn.", "tokens": ["Al\u00b7les", "was", "du", "be\u00b7gehrst", ",", "das", "soll", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}