{"textgrid.poem.48821": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "46. An die Sonne, da\u00df sie nicht eilen wolle, als er von ihnen in den Garten geladen war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df wird euch sein ein Tag von vielen meiner sch\u00f6nen,", "tokens": ["Di\u00df", "wird", "euch", "sein", "ein", "Tag", "von", "vie\u00b7len", "mei\u00b7ner", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "ART", "NN", "APPR", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die mir mein Gl\u00fccke g\u00f6nnt in einer fremden Welt.", "tokens": ["die", "mir", "mein", "Gl\u00fc\u00b7cke", "g\u00f6nnt", "in", "ei\u00b7ner", "frem\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sch\u00f6nsten rufen mir, die Sch\u00f6nsten um den ", "tokens": ["Die", "Sch\u00f6ns\u00b7ten", "ru\u00b7fen", "mir", ",", "die", "Sch\u00f6ns\u00b7ten", "um", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Sch\u00f6nsten rufen mir, ", "tokens": ["die", "Sch\u00f6ns\u00b7ten", "ru\u00b7fen", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So wird ", "tokens": ["So", "wird"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wolan! Ich wei\u00df den Ort, Priapus hat das Zelt", "tokens": ["Wo\u00b7lan", "!", "Ich", "wei\u00df", "den", "Ort", ",", "Pria\u00b7pus", "hat", "das", "Zelt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "NE", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "mit Gr\u00fcnem aufgesteckt und Flora hat bestellt,", "tokens": ["mit", "Gr\u00fc\u00b7nem", "auf\u00b7ge\u00b7steckt", "und", "Flo\u00b7ra", "hat", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wornach ein Mensch, wie ich, sieh pfleget oft zu sehnen.", "tokens": ["wor\u00b7nach", "ein", "Mensch", ",", "wie", "ich", ",", "sieh", "pfle\u00b7get", "oft", "zu", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "PPER", "$,", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Freund aller Freundlichkeit, der du die Tage machst", "tokens": ["Freund", "al\u00b7ler", "Freund\u00b7lich\u00b7keit", ",", "der", "du", "die", "Ta\u00b7ge", "machst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "und unsre Freude mehrst, wenn du, o Klarheit lachst,", "tokens": ["und", "uns\u00b7re", "Freu\u00b7de", "mehrst", ",", "wenn", "du", ",", "o", "Klar\u00b7heit", "lachst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPER", "$,", "FM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "halt deinen Wagen an von wegen meiner Wonnen!", "tokens": ["halt", "dei\u00b7nen", "Wa\u00b7gen", "an", "von", "we\u00b7gen", "mei\u00b7ner", "Won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Halt, edler Faeton, halt an und eile nicht!", "tokens": ["Halt", ",", "ed\u00b7ler", "Fae\u00b7ton", ",", "halt", "an", "und", "ei\u00b7le", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "G\u00f6nn' unsrer Fr\u00f6lichkeit ein nicht zu kurzes Licht.", "tokens": ["G\u00f6nn'", "uns\u00b7rer", "Fr\u00f6\u00b7lich\u00b7keit", "ein", "nicht", "zu", "kur\u00b7zes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "PTKNEG", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df bitten neben mir, o Sonne, so viel Sonnen.", "tokens": ["Di\u00df", "bit\u00b7ten", "ne\u00b7ben", "mir", ",", "o", "Son\u00b7ne", ",", "so", "viel", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "$,", "FM", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}