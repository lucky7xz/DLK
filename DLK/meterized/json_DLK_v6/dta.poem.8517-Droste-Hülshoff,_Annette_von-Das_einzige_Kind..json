{"dta.poem.8517": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das einzige Kind.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O schau, wie um ihr W&#228;ngelein", "tokens": ["O", "schau", ",", "wie", "um", "ihr", "W", "&#228;", "nge\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "APPR", "PPOSAT", "XY", "XY", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein tr\u00e4umendes L\u00e4cheln bebt,", "tokens": ["Ein", "tr\u00e4u\u00b7men\u00b7des", "L\u00e4\u00b7cheln", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sieht sie nicht aus wie ein Engelein,", "tokens": ["Sieht", "sie", "nicht", "aus", "wie", "ein", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das \u00fcber der Krippe schwebt.", "tokens": ["Das", "\u00fc\u00b7ber", "der", "Krip\u00b7pe", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oft f\u00fcrcht\u2019 ich, sie sei f\u00fcr die Welt zu gut,", "tokens": ["Oft", "f\u00fcrcht'", "ich", ",", "sie", "sei", "f\u00fcr", "die", "Welt", "zu", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sprich, Liebe, sind wir wohl blind?", "tokens": ["Sprich", ",", "Lie\u00b7be", ",", "sind", "wir", "wohl", "blind", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein wenig blind f\u00fcr das eigne Blut,", "tokens": ["Ein", "we\u00b7nig", "blind", "f\u00fcr", "das", "eig\u00b7ne", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Unser liebendes, einziges Kind?", "tokens": ["Un\u00b7ser", "lie\u00b7ben\u00b7des", ",", "ein\u00b7zi\u00b7ges", "Kind", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Der Gatte f\u00fchlt den Meister und Herrn,", "tokens": ["Der", "Gat\u00b7te", "f\u00fchlt", "den", "Meis\u00b7ter", "und", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Giebt allen M\u00e4ngeln ihr Recht,", "tokens": ["Giebt", "al\u00b7len", "M\u00e4n\u00b7geln", "ihr", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie spielt er den Philosophen so gern", "tokens": ["Wie", "spielt", "er", "den", "Phi\u00b7lo\u00b7so\u00b7phen", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ger\u00e4th er ihm schlecht!", "tokens": ["Und", "wie", "ge\u00b7r\u00e4\u00b7th", "er", "ihm", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nennt es ein Murmelchen anderen gleich,", "tokens": ["Nennt", "es", "ein", "Mur\u00b7mel\u00b7chen", "an\u00b7de\u00b7ren", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PIS", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Dran gar nichts zu loben ist,", "tokens": ["Dran", "gar", "nichts", "zu", "lo\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Inde\u00df er streichelt die L\u00f6ckchen reich", "tokens": ["In\u00b7de\u00df", "er", "strei\u00b7chelt", "die", "L\u00f6ck\u00b7chen", "reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ihm die Fingerchen k\u00fc\u00dft.", "tokens": ["Und", "ihm", "die", "Fin\u00b7ge\u00b7rchen", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}