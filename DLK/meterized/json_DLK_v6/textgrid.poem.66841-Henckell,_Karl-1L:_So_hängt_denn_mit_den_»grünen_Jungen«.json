{"textgrid.poem.66841": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: So h\u00e4ngt denn mit den \u00bbgr\u00fcnen Jungen\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So h\u00e4ngt denn mit den \u00bbgr\u00fcnen Jungen\u00ab", "tokens": ["So", "h\u00e4ngt", "denn", "mit", "den", "\u00bb", "gr\u00fc\u00b7nen", "Jun\u00b7gen", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mich und schindet mein Gedicht!", "tokens": ["Auch", "mich", "und", "schin\u00b7det", "mein", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schach eurem Spott! Er ist mi\u00dflungen.", "tokens": ["Schach", "eu\u00b7rem", "Spott", "!", "Er", "ist", "mi\u00df\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr merkt den neuen Pulsschlag nicht.", "tokens": ["Ihr", "merkt", "den", "neu\u00b7en", "Puls\u00b7schlag", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, \u00bbJungen\u00ab haben hei\u00df empfunden,", "tokens": ["Ja", ",", "\u00bb", "Jun\u00b7gen", "\u00ab", "ha\u00b7ben", "hei\u00df", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "$(", "NN", "$(", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was alt und kalt ihr nie gesp\u00fcrt,", "tokens": ["Was", "alt", "und", "kalt", "ihr", "nie", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wild bluteten die Herzenswunden,", "tokens": ["Wild", "blu\u00b7te\u00b7ten", "die", "Her\u00b7zens\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Widerhaken aufger\u00fchrt.", "tokens": ["Von", "Wi\u00b7der\u00b7ha\u00b7ken", "auf\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Parzenfluch zerrissner Zeiten", "tokens": ["Der", "Par\u00b7zen\u00b7fluch", "zer\u00b7riss\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Peitscht' unser berstendes Gef\u00fchl", "tokens": ["Peitscht'", "un\u00b7ser", "bers\u00b7ten\u00b7des", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bis in die tiefsten Heimlichkeiten", "tokens": ["Bis", "in", "die", "tiefs\u00b7ten", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zu sinnverwirrendem Gew\u00fchl.", "tokens": ["Zu", "sinn\u00b7ver\u00b7wir\u00b7ren\u00b7dem", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Odyssee im B\u00fccherriemen", "tokens": ["Die", "O\u00b7dys\u00b7see", "im", "B\u00fc\u00b7cher\u00b7rie\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und Roms Rhetoren in der Hand,", "tokens": ["Und", "Roms", "Rhe\u00b7to\u00b7ren", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Entdeckten wir die neuen Kiemen", "tokens": ["Ent\u00b7deck\u00b7ten", "wir", "die", "neu\u00b7en", "Kie\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Schon an der gl\u00fchenden Schl\u00e4fenwand.", "tokens": ["Schon", "an", "der", "gl\u00fc\u00b7hen\u00b7den", "Schl\u00e4\u00b7fen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wie frostig fieberten die Stunden,", "tokens": ["Wie", "fros\u00b7tig", "fie\u00b7ber\u00b7ten", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie schauderte die tote M\u00fch,", "tokens": ["Wie", "schau\u00b7der\u00b7te", "die", "to\u00b7te", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn wir den zarten Geist geschunden", "tokens": ["Wenn", "wir", "den", "zar\u00b7ten", "Geist", "ge\u00b7schun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit Ochsenziemern sp\u00e4t und fr\u00fch!", "tokens": ["Mit", "Och\u00b7sen\u00b7zie\u00b7mern", "sp\u00e4t", "und", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wir f\u00fchlten in die Flut der Massen \u2013", "tokens": ["Wir", "f\u00fchl\u00b7ten", "in", "die", "Flut", "der", "Mas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Gro\u00dfstadt ist kein Internat \u2013", "tokens": ["Die", "Gro\u00df\u00b7stadt", "ist", "kein", "In\u00b7ter\u00b7nat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und mit Err\u00f6ten, mit Erblassen", "tokens": ["Und", "mit", "Er\u00b7r\u00f6\u00b7ten", ",", "mit", "Er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sind wir der Zwingburg scheu genaht.", "tokens": ["Sind", "wir", "der", "Zwing\u00b7burg", "scheu", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In weiche Rinden dumm-verlogen", "tokens": ["In", "wei\u00b7che", "Rin\u00b7den", "dum\u00b7mver\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ward eingegerbt der Spuk und Spott,", "tokens": ["Ward", "ein\u00b7ge\u00b7gerbt", "der", "Spuk", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Des Lebens Wogen aber flogen", "tokens": ["Des", "Le\u00b7bens", "Wo\u00b7gen", "a\u00b7ber", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Fern dem gequ\u00e4lten Griechengott.", "tokens": ["Fern", "dem", "ge\u00b7qu\u00e4l\u00b7ten", "Grie\u00b7chen\u00b7gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcndeutschland hoch! Wir reihten wacker", "tokens": ["Gr\u00fcn\u00b7deutschland", "hoch", "!", "Wir", "reih\u00b7ten", "wa\u00b7cker"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon damals gr\u00fcndeutsch Reim an Reim,", "tokens": ["Schon", "da\u00b7mals", "gr\u00fcn\u00b7deutsch", "Reim", "an", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf frischgebrochnem Musenacker", "tokens": ["Auf", "frischge\u00b7broch\u00b7nem", "Mu\u00b7se\u00b7na\u00b7cker"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spro\u00df unsre Traumsaat, Keim an Keim.", "tokens": ["Spro\u00df", "uns\u00b7re", "Traum\u00b7saat", ",", "Keim", "an", "Keim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir st\u00f6hnten Verse, wir umrissen", "tokens": ["Wir", "st\u00f6hn\u00b7ten", "Ver\u00b7se", ",", "wir", "um\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In Prosa, was das Herz zerst\u00fcckt,", "tokens": ["In", "Pro\u00b7sa", ",", "was", "das", "Herz", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir schrieben Briefe dem Gewissen", "tokens": ["Wir", "schrie\u00b7ben", "Brie\u00b7fe", "dem", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und suchten Seelen, gleichbedr\u00fcckt.", "tokens": ["Und", "such\u00b7ten", "See\u00b7len", ",", "gleich\u00b7be\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O Schmerz, der unsrer Jugend S\u00e4ulen", "tokens": ["O", "Schmerz", ",", "der", "uns\u00b7rer", "Ju\u00b7gend", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit wahrer F\u00f6hngewalt durchkracht!", "tokens": ["Mit", "wah\u00b7rer", "F\u00f6hn\u00b7ge\u00b7walt", "durch\u00b7kracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer h\u00f6rt nicht das Verh\u00e4ngnis heulen,", "tokens": ["Wer", "h\u00f6rt", "nicht", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dem Moloch wehrlos dargebracht?", "tokens": ["Dem", "Mo\u00b7loch", "wehr\u00b7los", "dar\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Knaben haben's nur empfunden,", "tokens": ["Die", "Kna\u00b7ben", "ha\u00b7ben's", "nur", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ihr Kopf war hei\u00df, ihr Herz war schwer,", "tokens": ["Ihr", "Kopf", "war", "hei\u00df", ",", "ihr", "Herz", "war", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie siedeten aus ihren Wunden \u2013", "tokens": ["Sie", "sie\u00b7de\u00b7ten", "aus", "ih\u00b7ren", "Wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Jetzt sind wir keine Knaben mehr.", "tokens": ["Jetzt", "sind", "wir", "kei\u00b7ne", "Kna\u00b7ben", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Jetzt ist das W\u00e4hnen Mann geworden,", "tokens": ["Jetzt", "ist", "das", "W\u00e4h\u00b7nen", "Mann", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Erkenntnis ward des F\u00fchlens Braut,", "tokens": ["Er\u00b7kennt\u00b7nis", "ward", "des", "F\u00fch\u00b7lens", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Jetzt wird in ehernen Akkorden", "tokens": ["Jetzt", "wird", "in", "e\u00b7her\u00b7nen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das kommende Jahrhundert laut:", "tokens": ["Das", "kom\u00b7men\u00b7de", "Jahr\u00b7hun\u00b7dert", "laut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es hat ein Hammer aufgeschlagen", "tokens": ["Es", "hat", "ein", "Ham\u00b7mer", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im menschlichen Maschinensaal,", "tokens": ["Im", "menschli\u00b7chen", "Ma\u00b7schi\u00b7nen\u00b7saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Ambo\u00df klang, und fortgetragen", "tokens": ["Der", "Am\u00b7bo\u00df", "klang", ",", "und", "fort\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sein Get\u00f6n von Tal zu Tal.", "tokens": ["Wird", "sein", "Ge\u00b7t\u00f6n", "von", "Tal", "zu", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Berge zittern seinem Dr\u00f6hnen,", "tokens": ["Die", "Ber\u00b7ge", "zit\u00b7tern", "sei\u00b7nem", "Dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Meere w\u00e4lzen seinen Ruf;", "tokens": ["Die", "Mee\u00b7re", "w\u00e4l\u00b7zen", "sei\u00b7nen", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er bebt ans Ohr der Erde S\u00f6hnen", "tokens": ["Er", "bebt", "ans", "Ohr", "der", "Er\u00b7de", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und lebt im Sch\u00f6nen, das er schuf.", "tokens": ["Und", "lebt", "im", "Sch\u00f6\u00b7nen", ",", "das", "er", "schuf"