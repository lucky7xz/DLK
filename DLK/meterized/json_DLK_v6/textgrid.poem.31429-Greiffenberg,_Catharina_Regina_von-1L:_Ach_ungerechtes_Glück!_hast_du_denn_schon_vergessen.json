{"textgrid.poem.31429": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach ungerechtes Gl\u00fcck! hast du denn schon vergessen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ungerechtes Gl\u00fcck! hast du denn schon vergessen", "tokens": ["Ach", "un\u00b7ge\u00b7rech\u00b7tes", "Gl\u00fcck", "!", "hast", "du", "denn", "schon", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dein alte Wankel-Art und steten unbestand /", "tokens": ["dein", "al\u00b7te", "Wan\u00b7kel\u00b7Art", "und", "ste\u00b7ten", "un\u00b7be\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df du mich also qu\u00e4lst mit unerm\u00fcdter Hand?", "tokens": ["da\u00df", "du", "mich", "al\u00b7so", "qu\u00e4lst", "mit", "un\u00b7er\u00b7m\u00fcd\u00b7ter", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist denn der wechsel aus / der dich so lang besessen?", "tokens": ["ist", "denn", "der", "wech\u00b7sel", "aus", "/", "der", "dich", "so", "lang", "be\u00b7ses\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wilst du mein Herzen Blut durch thr\u00e4nen au\u00dfher pressen.", "tokens": ["wilst", "du", "mein", "Her\u00b7zen", "Blut", "durch", "thr\u00e4\u00b7nen", "au\u00df\u00b7her", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "du l\u00f6sest nur der freud' / und nicht des Elends / band.", "tokens": ["du", "l\u00f6\u00b7sest", "nur", "der", "freud'", "/", "und", "nicht", "des", "E\u00b7lends", "/", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "PTKNEG", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ach leider Ich versink in diesem Jammer strand.", "tokens": ["ach", "lei\u00b7der", "Ich", "ver\u00b7sink", "in", "die\u00b7sem", "Jam\u00b7mer", "strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "es ist die Vngl\u00fccks Flut zu tieff / und nicht zu messen.", "tokens": ["es", "ist", "die", "Vn\u00b7gl\u00fccks", "Flut", "zu", "tieff", "/", "und", "nicht", "zu", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "PTKA", "ADJD", "$(", "KON", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich siehe keine H\u00fclf und Rettung aus der Noht", "tokens": ["Ich", "sie\u00b7he", "kei\u00b7ne", "H\u00fclf", "und", "Ret\u00b7tung", "aus", "der", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "vor mir das Meer / die Berg' aufseiten / ruckwerts Feinde.", "tokens": ["vor", "mir", "das", "Meer", "/", "die", "Ber\u00b7g'", "auf\u00b7sei\u00b7ten", "/", "ruck\u00b7werts", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$(", "ADV", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "wann seine wunder-Macht mir nicht erzeigt mein Gott /", "tokens": ["wann", "sei\u00b7ne", "wun\u00b7der\u00b7Macht", "mir", "nicht", "er\u00b7zeigt", "mein", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "so ists mit mir geschehn; doch / hab' ich den zum Freunde /", "tokens": ["so", "ists", "mit", "mir", "ge\u00b7schehn", ";", "doch", "/", "hab'", "ich", "den", "zum", "Freun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "ADV", "$(", "VAFIN", "PPER", "ART", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "es geh' auch wie es woll / so bin ich schon vergn\u00fcgt.", "tokens": ["es", "geh'", "auch", "wie", "es", "woll", "/", "so", "bin", "ich", "schon", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "ADV", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein dapfers Herz auch wol im gr\u00f6sten Vngl\u00fcck siegt.", "tokens": ["Ein", "dap\u00b7fers", "Herz", "auch", "wol", "im", "gr\u00f6s\u00b7ten", "Vn\u00b7gl\u00fcck", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}