{"dta.poem.7424": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vive le Roi   !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit diesem L\u00fcgenwunsch ihn doch berauscht!", "tokens": ["Mit", "die\u00b7sem", "L\u00fc\u00b7gen\u00b7wunsch", "ihn", "doch", "be\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie gierig haben stets bei seinen F", "tokens": ["Wie", "gie\u00b7rig", "ha\u00b7ben", "stets", "bei", "sei\u00b7nen", "F"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Furcht, Interesse, Eitelkeit gelauscht!", "tokens": ["Furcht", ",", "In\u00b7ter\u00b7es\u00b7se", ",", "Ei\u00b7tel\u00b7keit", "ge\u00b7lauscht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich mag den Herren ihre Kreuze g\u00f6nnen,", "tokens": ["Ich", "mag", "den", "Her\u00b7ren", "ih\u00b7re", "Kreu\u00b7ze", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich sie so zu Hofe traben seh',", "tokens": ["Wenn", "ich", "sie", "so", "zu", "Ho\u00b7fe", "tra\u00b7ben", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und steh' bei Seit', um rufen noch zu k\u00f6nnen:", "tokens": ["Und", "steh'", "bei", "Seit'", ",", "um", "ru\u00b7fen", "noch", "zu", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KOUI", "VVFIN", "ADV", "PTKZU", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Hochmut eines Erdengotts gefr\u00f6hnt;", "tokens": ["Dem", "Hoch\u00b7mut", "ei\u00b7nes", "Er\u00b7den\u00b7gotts", "ge\u00b7fr\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ward ihr lauter Jubel doch so leise,", "tokens": ["Wie", "ward", "ihr", "lau\u00b7ter", "Ju\u00b7bel", "doch", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als drauf der Leoniden Ruf ert\u00f6nt!", "tokens": ["Als", "drauf", "der", "Leo\u00b7ni\u00b7den", "Ruf", "er\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O heil'ger Ruf, der noch in unsern Tagen", "tokens": ["O", "heil'\u00b7ger", "Ruf", ",", "der", "noch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So pr\u00e4chtig klingt, wie bei Thermopil\u00e4;", "tokens": ["So", "pr\u00e4ch\u00b7tig", "klingt", ",", "wie", "bei", "Ther\u00b7mo\u00b7pi\u00b7l\u00e4", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PWAV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch unsre Fahne soll als Wahlspruch tragen:", "tokens": ["Auch", "uns\u00b7re", "Fah\u00b7ne", "soll", "als", "Wahl\u00b7spruch", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von unsern Burgen, wenn am eignen Herd", "tokens": ["Von", "un\u00b7sern", "Bur\u00b7gen", ",", "wenn", "am", "eig\u00b7nen", "Herd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ihres F\u00fcrsten Namen die Vasallen", "tokens": ["In", "ih\u00b7res", "F\u00fcrs\u00b7ten", "Na\u00b7men", "die", "Va\u00b7sal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erw\u00fcrgte unsrer gn\u00e4d'gen Herren Schwert!", "tokens": ["Er\u00b7w\u00fcrg\u00b7te", "uns\u00b7rer", "gn\u00e4d'\u00b7gen", "Her\u00b7ren", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch heben n\u00e4chtlich sie bei Mondenschimmer", "tokens": ["Noch", "he\u00b7ben", "n\u00e4cht\u00b7lich", "sie", "bei", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die blut'gen Klingen fluchend in die H\u00f6h',", "tokens": ["Die", "blut'\u00b7gen", "Klin\u00b7gen", "flu\u00b7chend", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch l\u00e4chelnd schreibt der Wandrer auf die Tr\u00fcmmer:", "tokens": ["Doch", "l\u00e4\u00b7chelnd", "schreibt", "der", "Wand\u00b7rer", "auf", "die", "Tr\u00fcm\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Rache Ruf im eitelen Refrain;", "tokens": ["Der", "Ra\u00b7che", "Ruf", "im", "ei\u00b7te\u00b7len", "Re\u00b7frain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ja das ew'ge Kind recht ruhig schlafe,", "tokens": ["Da\u00df", "ja", "das", "ew'\u00b7ge", "Kind", "recht", "ru\u00b7hig", "schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seht ihr, ", "tokens": ["Seht", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "Doch bricht das Wetter aus, so lang beschworen,", "tokens": ["Doch", "bricht", "das", "Wet\u00b7ter", "aus", ",", "so", "lang", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist er verlassen, ohne Schmeichler \u2014 Weh!", "tokens": ["Ist", "er", "ver\u00b7las\u00b7sen", ",", "oh\u00b7ne", "Schmeich\u00b7ler", "Weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KOUI", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann donnert ihm vernichtend in die Ohren:", "tokens": ["Dann", "don\u00b7nert", "ihm", "ver\u00b7nich\u00b7tend", "in", "die", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}