{"textgrid.poem.51167": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie des Rauches Silbers\u00e4umchen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie des Rauches Silbers\u00e4umchen", "tokens": ["Wie", "des", "Rau\u00b7ches", "Sil\u00b7ber\u00b7s\u00e4um\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Vesuv den Himmel sucht!", "tokens": ["Vom", "Ve\u00b7suv", "den", "Him\u00b7mel", "sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feigenb\u00e4umlein, Feigenb\u00e4umchen,", "tokens": ["Fei\u00b7gen\u00b7b\u00e4um\u00b7lein", ",", "Fei\u00b7gen\u00b7b\u00e4um\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie s\u00fc\u00df ist deine Frucht!", "tokens": ["Und", "wie", "s\u00fc\u00df", "ist", "dei\u00b7ne", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein k\u00fchlender Zephir f\u00e4chelt", "tokens": ["Und", "ein", "k\u00fch\u00b7len\u00b7der", "Ze\u00b7phir", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "\u00dcber den warmen Lavagrund,", "tokens": ["\u00dc\u00b7ber", "den", "war\u00b7men", "La\u00b7va\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Drauf die Madonna niederl\u00e4chelt", "tokens": ["Drauf", "die", "Ma\u00b7don\u00b7na", "nie\u00b7der\u00b7l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dem feingeschnitzten Mund.", "tokens": ["Mit", "dem", "fein\u00b7ge\u00b7schnitz\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt ein lustiger M\u00f6nch gegangen", "tokens": ["Kommt", "ein", "lus\u00b7ti\u00b7ger", "M\u00f6nch", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit dem vollen Tr\u00e4nenkrug;", "tokens": ["Mit", "dem", "vol\u00b7len", "Tr\u00e4\u00b7nen\u00b7krug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt ein Weib mit Purpurwangen", "tokens": ["Kommt", "ein", "Weib", "mit", "Pur\u00b7pur\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit n\u00e4chtlichem Lockenflug;", "tokens": ["Und", "mit", "n\u00e4cht\u00b7li\u00b7chem", "Lo\u00b7cken\u00b7flug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Sch\u00f6n ist's unter dem Feigenbaum,", "tokens": ["Sch\u00f6n", "ist's", "un\u00b7ter", "dem", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Wo der Berg in Liebe brennt!", "tokens": ["Wo", "der", "Berg", "in", "Lie\u00b7be", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00fcben leuchten, wie ein Traum,", "tokens": ["Dr\u00fc\u00b7ben", "leuch\u00b7ten", ",", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ischia, Capri und Sorrent.", "tokens": ["I\u00b7schia", ",", "Cap\u00b7ri", "und", "Sor\u00b7rent", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Sind ihre Locken die dunkle Nacht,", "tokens": ["Sind", "ih\u00b7re", "Lo\u00b7cken", "die", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist seine Glatze der Mondenschein,", "tokens": ["Ist", "sei\u00b7ne", "Glat\u00b7ze", "der", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und es k\u00f6nnen die Sternenpracht", "tokens": ["Und", "es", "k\u00f6n\u00b7nen", "die", "Ster\u00b7nen\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihre gl\u00fchenden Augen sein.", "tokens": ["Ih\u00b7re", "gl\u00fc\u00b7hen\u00b7den", "Au\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Also schaffen am hellen Tag", "tokens": ["Al\u00b7so", "schaf\u00b7fen", "am", "hel\u00b7len", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sie die heimliche stille Nacht;", "tokens": ["Sie", "die", "heim\u00b7li\u00b7che", "stil\u00b7le", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Was doch alles geschehen mag,", "tokens": ["Was", "doch", "al\u00b7les", "ge\u00b7sche\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wenn man's klug und sinnig macht!", "tokens": ["Wenn", "man's", "klug", "und", "sin\u00b7nig", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur die h\u00f6lzerne Madonne", "tokens": ["Nur", "die", "h\u00f6l\u00b7zer\u00b7ne", "Ma\u00b7don\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmachtet in der hei\u00dfen Sonne;", "tokens": ["Schmach\u00b7tet", "in", "der", "hei\u00b7\u00dfen", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df auch sie genie\u00dfe der Ruh,", "tokens": ["Da\u00df", "auch", "sie", "ge\u00b7nie\u00b7\u00dfe", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wirft das Weib ihr den Schleier zu.", "tokens": ["Wirft", "das", "Weib", "ihr", "den", "Schlei\u00b7er", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Lachend \u00fcber die See her blinken", "tokens": ["La\u00b7chend", "\u00fc\u00b7ber", "die", "See", "her", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ischia, Capri und Sorrent \u2013", "tokens": ["I\u00b7schia", ",", "Cap\u00b7ri", "und", "Sor\u00b7rent", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "S\u00fc\u00df und selig ist zu trinken,", "tokens": ["S\u00fc\u00df", "und", "se\u00b7lig", "ist", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was man Christi Tr\u00e4nen nennt!", "tokens": ["Was", "man", "Chris\u00b7ti", "Tr\u00e4\u00b7nen", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}