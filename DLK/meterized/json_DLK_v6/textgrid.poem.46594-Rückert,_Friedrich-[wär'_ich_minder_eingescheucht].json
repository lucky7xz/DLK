{"textgrid.poem.46594": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[w\u00e4r' ich minder eingescheucht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich minder eingescheucht,", "tokens": ["W\u00e4r'", "ich", "min\u00b7der", "ein\u00b7ge\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesmal w\u00fcrd' ich hoffen;", "tokens": ["Dies\u00b7mal", "w\u00fcrd'", "ich", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn so sch\u00f6n war, wie mir deucht,", "tokens": ["Denn", "so", "sch\u00f6n", "war", ",", "wie", "mir", "deucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie der Himmel offen.", "tokens": ["Nie", "der", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch auf meinem letzten Gang,", "tokens": ["Doch", "auf", "mei\u00b7nem", "letz\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Weg, dem schroffen,", "tokens": ["Auf", "dem", "Weg", ",", "dem", "schrof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist f\u00fcr jeden frohen Drang", "tokens": ["Ist", "f\u00fcr", "je\u00b7den", "fro\u00b7hen", "Drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uebel eingetroffen.", "tokens": ["Ue\u00b7bel", "ein\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, es scheint, mein Hoffen macht", "tokens": ["Ja", ",", "es", "scheint", ",", "mein", "Hof\u00b7fen", "macht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh aus allen Stoffen,", "tokens": ["Weh", "aus", "al\u00b7len", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich will mit Vorbedacht", "tokens": ["Und", "ich", "will", "mit", "Vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber nichts mehr hoffen.", "tokens": ["Lie\u00b7ber", "nichts", "mehr", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}