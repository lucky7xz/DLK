{"textgrid.poem.48222": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Meine Gr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Erbbegr\u00e4bnis mich stolz erfreut,", "tokens": ["Kein", "Erb\u00b7be\u00b7gr\u00e4b\u00b7nis", "mich", "stolz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meine Gr\u00e4ber liegen weit zerstreut,", "tokens": ["Mei\u00b7ne", "Gr\u00e4\u00b7ber", "lie\u00b7gen", "weit", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weit zerstreut \u00fcber Stadt und Land,", "tokens": ["Weit", "zer\u00b7streut", "\u00fc\u00b7ber", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Aber all in m\u00e4rkischem Sand.", "tokens": ["A\u00b7ber", "all", "in", "m\u00e4r\u00b7ki\u00b7schem", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Verfallene H\u00fcgel, die Schwalben ziehn,", "tokens": ["Ver\u00b7fal\u00b7le\u00b7ne", "H\u00fc\u00b7gel", ",", "die", "Schwal\u00b7ben", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor\u00fcber schl\u00e4ngelt sich der Rhin,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "schl\u00e4n\u00b7gelt", "sich", "der", "Rhin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00dcber wei\u00dfe Steine, zerbr\u00f6ckelt all,", "tokens": ["\u00dc\u00b7ber", "wei\u00b7\u00dfe", "Stei\u00b7ne", ",", "zer\u00b7br\u00f6\u00b7ckelt", "all", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PIAT", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blickt der alte Ruppiner Wall,", "tokens": ["Blickt", "der", "al\u00b7te", "Rup\u00b7pi\u00b7ner", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Buchen stehn, die Eichen rauschen,", "tokens": ["Die", "Bu\u00b7chen", "stehn", ",", "die", "Ei\u00b7chen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gr\u00e4berb\u00fcsche Zwiesprach tauschen,", "tokens": ["Die", "Gr\u00e4\u00b7ber\u00b7b\u00fc\u00b7sche", "Zwie\u00b7sprach", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Haferfelder weit auf und ab \u2013", "tokens": ["Und", "Ha\u00b7fer\u00b7fel\u00b7der", "weit", "auf", "und", "ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Da ist meiner Mutter Grab.", "tokens": ["Da", "ist", "mei\u00b7ner", "Mut\u00b7ter", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und ein andrer Platz, dem verbunden ich bin:", "tokens": ["Und", "ein", "an\u00b7drer", "Platz", ",", "dem", "ver\u00b7bun\u00b7den", "ich", "bin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "PPER", "VAFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Berglehnen, die Oder flie\u00dft dran hin,", "tokens": ["Berg\u00b7leh\u00b7nen", ",", "die", "O\u00b7der", "flie\u00dft", "dran", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "NE", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zieht vor\u00fcber in tr\u00e4gem Lauf,", "tokens": ["Zieht", "vor\u00b7\u00fc\u00b7ber", "in", "tr\u00e4\u00b7gem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Gelbe Mummeln schwimmen darauf.", "tokens": ["Gel\u00b7be", "Mum\u00b7meln", "schwim\u00b7men", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Am Ufer Werft und Schilf und Rohr,", "tokens": ["Am", "U\u00b7fer", "Werft", "und", "Schilf", "und", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und am Abhange schimmern Kreuze hervor,", "tokens": ["Und", "am", "Ab\u00b7han\u00b7ge", "schim\u00b7mern", "Kreu\u00b7ze", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auf eines f\u00e4llt heller Sonnenschein \u2013", "tokens": ["Auf", "ei\u00b7nes", "f\u00e4llt", "hel\u00b7ler", "Son\u00b7nen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da hat mein Vater seinen Stein.", "tokens": ["Da", "hat", "mein", "Va\u00b7ter", "sei\u00b7nen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Dritte, seines Todes froh,", "tokens": ["Der", "Drit\u00b7te", ",", "sei\u00b7nes", "To\u00b7des", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt auf dem weiten Teltow-Plateau,", "tokens": ["Liegt", "auf", "dem", "wei\u00b7ten", "Tel\u00b7tow\u00b7Pla\u00b7teau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "D\u00e4cher von Ziegel, D\u00e4cher von Schiefer,", "tokens": ["D\u00e4\u00b7cher", "von", "Zie\u00b7gel", ",", "D\u00e4\u00b7cher", "von", "Schie\u00b7fer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dann und wann eine Kr\u00fcppelkiefer,", "tokens": ["Dann", "und", "wann", "ei\u00b7ne", "Kr\u00fcp\u00b7pel\u00b7kie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein stiller Graben die Wasserscheide,", "tokens": ["Ein", "stil\u00b7ler", "Gra\u00b7ben", "die", "Was\u00b7ser\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Birken hier, und da eine Weide,", "tokens": ["Bir\u00b7ken", "hier", ",", "und", "da", "ei\u00b7ne", "Wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Zuletzt eine Pappel am Horizont,", "tokens": ["Zu\u00b7letzt", "ei\u00b7ne", "Pap\u00b7pel", "am", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Im Abendstrahle sie sich sonnt.", "tokens": ["Im", "A\u00b7bend\u00b7strah\u00b7le", "sie", "sich", "sonnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf den Gr\u00e4bern Blumen und Aschenkr\u00fcge,", "tokens": ["Auf", "den", "Gr\u00e4\u00b7bern", "Blu\u00b7men", "und", "A\u00b7schen\u00b7kr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Vor\u00fcber in Ferne rasseln die Z\u00fcge,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "in", "Fer\u00b7ne", "ras\u00b7seln", "die", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Still bleibt das Grab und der Schl\u00e4fer drin \u2013", "tokens": ["Still", "bleibt", "das", "Grab", "und", "der", "Schl\u00e4\u00b7fer", "drin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ADV", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der Wind, der Wind geht dr\u00fcber hin.", "tokens": ["Der", "Wind", ",", "der", "Wind", "geht", "dr\u00fc\u00b7ber", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}