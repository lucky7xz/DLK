{"textgrid.poem.46953": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Geister der gefall'nen Freiheitshelden,", "tokens": ["Die", "Geis\u00b7ter", "der", "ge\u00b7fall'\u00b7nen", "Frei\u00b7heits\u00b7hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Laut rufen sie hernieder aus Walhalle:", "tokens": ["Laut", "ru\u00b7fen", "sie", "her\u00b7nie\u00b7der", "aus", "Wal\u00b7hal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbviel S\u00e4nger sind auf Erden, die mit Schalle", "tokens": ["\u00bb", "viel", "S\u00e4n\u00b7ger", "sind", "auf", "Er\u00b7den", ",", "die", "mit", "Schal\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "VAFIN", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von unsrem Preis den Nachgeblieb'nen melden.", "tokens": ["Von", "uns\u00b7rem", "Preis", "den", "Nach\u00b7ge\u00b7blie\u00b7b'\u00b7nen", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auf, holt von ihnen zu des Himmels Felden", "tokens": ["Auf", ",", "holt", "von", "ih\u00b7nen", "zu", "des", "Him\u00b7mels", "Fel\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herauf uns einen, der uns sei f\u00fcr alle,", "tokens": ["Her\u00b7auf", "uns", "ei\u00b7nen", ",", "der", "uns", "sei", "f\u00fcr", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "$,", "PRELS", "PPER", "VAFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df er uns singe, was uns wohlgefalle,", "tokens": ["Da\u00df", "er", "uns", "sin\u00b7ge", ",", "was", "uns", "wohl\u00b7ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Beim Mahle zwischen Hermann und Thusnelden.\u00ab", "tokens": ["Beim", "Mah\u00b7le", "zwi\u00b7schen", "Her\u00b7mann", "und", "Thus\u00b7nel\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da sank im Kampfgew\u00fchl ein Held vom Rosse,", "tokens": ["Da", "sank", "im", "Kampf\u00b7ge\u00b7w\u00fchl", "ein", "Held", "vom", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den hoben auf das ihre zwei Walk\u00fcren,", "tokens": ["Den", "ho\u00b7ben", "auf", "das", "ih\u00b7re", "zwei", "Wal\u00b7k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PRELS", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und f\u00fchrten ihn empor samt Schwert und Leier.", "tokens": ["Und", "f\u00fchr\u00b7ten", "ihn", "em\u00b7por", "samt", "Schwert", "und", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nun sitzt er droben im krystallnen Schlosse,", "tokens": ["Nun", "sitzt", "er", "dro\u00b7ben", "im", "krys\u00b7tall\u00b7nen", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wo ich ihn sehe goldne Saiten r\u00fchren,", "tokens": ["Wo", "ich", "ihn", "se\u00b7he", "gold\u00b7ne", "Sai\u00b7ten", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn Geister mir vom Auge ziehn den Schleier.", "tokens": ["Wenn", "Geis\u00b7ter", "mir", "vom", "Au\u00b7ge", "ziehn", "den", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}