{"textgrid.poem.64092": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Ein K\u00f6nigsspiel", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sa\u00df der K\u00f6nig Artaxerxes", "tokens": ["Sa\u00df", "der", "K\u00f6\u00b7nig", "Ar\u00b7ta\u00b7xe\u00b7rxes"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In dem goldnen Haus zu Susa", "tokens": ["In", "dem", "gold\u00b7nen", "Haus", "zu", "Su\u00b7sa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem hohen Purpurthrone:", "tokens": ["Auf", "dem", "ho\u00b7hen", "Pur\u00b7pur\u00b7th\u00b7ro\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Im geflochtnen Barte Perlen,", "tokens": ["Im", "ge\u00b7flocht\u00b7nen", "Bar\u00b7te", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die Stirn das Diadema,", "tokens": ["Um", "die", "Stirn", "das", "Di\u00b7a\u00b7de\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "In der Hand das goldne Zepter", "tokens": ["In", "der", "Hand", "das", "gold\u00b7ne", "Zep\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Herzen \u00dcbermut. \u2013", "tokens": ["Und", "im", "Her\u00b7zen", "\u00dc\u00b7ber\u00b7mut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf den Polstern vor ihm knieten", "tokens": ["Auf", "den", "Pols\u00b7tern", "vor", "ihm", "knie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Reiches erste F\u00fcrsten,", "tokens": ["Sei\u00b7nes", "Rei\u00b7ches", "ers\u00b7te", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edle, Feldherrn und Satrapen:", "tokens": ["Ed\u00b7le", ",", "Feld\u00b7herrn", "und", "Sa\u00b7tra\u00b7pen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und er winkte dem Dadanes,", "tokens": ["Und", "er", "wink\u00b7te", "dem", "Da\u00b7da\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der der k\u00fchnste seiner Krieger,", "tokens": ["Der", "der", "k\u00fchns\u00b7te", "sei\u00b7ner", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der treu'ste der Satrapen", "tokens": ["Und", "der", "treu'\u00b7ste", "der", "Sa\u00b7tra\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und der Feldherrn bester war.", "tokens": ["Und", "der", "Feld\u00b7herrn", "bes\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmich gel\u00fcstet,\u00ab sprach der K\u00f6nig,", "tokens": ["\u00bb", "mich", "ge\u00b7l\u00fcs\u00b7tet", ",", "\u00ab", "sprach", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVPP", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbmich gel\u00fcstet, o Dadanes,", "tokens": ["\u00bb", "mich", "ge\u00b7l\u00fcs\u00b7tet", ",", "o", "Da\u00b7da\u00b7nes", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVPP", "$,", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines wei\u00dfen Edelfalken,", "tokens": ["Dei\u00b7nes", "wei\u00b7\u00dfen", "E\u00b7del\u00b7fal\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den du selbst dir abgerichtet,", "tokens": ["Den", "du", "selbst", "dir", "ab\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der auch Antilopen beizet:", "tokens": ["Der", "auch", "An\u00b7ti\u00b7lo\u00b7pen", "bei\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gibst du, Feldherr, wohl den Vogel", "tokens": ["Gibst", "du", ",", "Feld\u00b7herr", ",", "wohl", "den", "Vo\u00b7gel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinem K\u00f6nig zum Geschenk?\u00ab", "tokens": ["Dei\u00b7nem", "K\u00f6\u00b7nig", "zum", "Ge\u00b7schenk", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unbew\u00f6lkt blieb des Dadanes", "tokens": ["Un\u00b7be\u00b7w\u00f6lkt", "blieb", "des", "Da\u00b7da\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohe Stirn, da er sich neigte:", "tokens": ["Ho\u00b7he", "Stirn", ",", "da", "er", "sich", "neig\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbteuer war mir jener Vogel,", "tokens": ["\u00bb", "teu\u00b7er", "war", "mir", "je\u00b7ner", "Vo\u00b7gel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich selbst mir abgerichtet,", "tokens": ["Den", "ich", "selbst", "mir", "ab\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der auch Antilopen beizet:", "tokens": ["Der", "auch", "An\u00b7ti\u00b7lo\u00b7pen", "bei\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber wenn dich sein gel\u00fcstet,", "tokens": ["A\u00b7ber", "wenn", "dich", "sein", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gro\u00dfer K\u00f6nig, ist er dein.\u00ab", "tokens": ["Gro\u00b7\u00dfer", "K\u00f6\u00b7nig", ",", "ist", "er", "dein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbmich gel\u00fcstet,\u00ab sprach der K\u00f6nig,", "tokens": ["\u00bb", "mich", "ge\u00b7l\u00fcs\u00b7tet", ",", "\u00ab", "sprach", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVPP", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbmich gel\u00fcstet, o Dadanes,", "tokens": ["\u00bb", "mich", "ge\u00b7l\u00fcs\u00b7tet", ",", "o", "Da\u00b7da\u00b7nes", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVPP", "$,", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines schwarzen Partherhengstes,", "tokens": ["Dei\u00b7nes", "schwar\u00b7zen", "Par\u00b7ther\u00b7hengs\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der nicht scheut die Elefanten,", "tokens": ["Der", "nicht", "scheut", "die", "E\u00b7lef\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den du rittst in sieben Schlachten,", "tokens": ["Den", "du", "rittst", "in", "sie\u00b7ben", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den dein Vater schon geritten, \u2013", "tokens": ["Den", "dein", "Va\u00b7ter", "schon", "ge\u00b7rit\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schenkst dem K\u00f6nig du das Ro\u00df?\u00ab", "tokens": ["Schenkst", "dem", "K\u00f6\u00b7nig", "du", "das", "Ro\u00df", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leise furchte nur Dadanes", "tokens": ["Lei\u00b7se", "furch\u00b7te", "nur", "Da\u00b7da\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Brau'n, da er sich neigte:", "tokens": ["Sei\u00b7ne", "Brau'n", ",", "da", "er", "sich", "neig\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbteuer war mir jener Rappe,", "tokens": ["\u00bb", "teu\u00b7er", "war", "mir", "je\u00b7ner", "Rap\u00b7pe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den mein Vater schon geritten,", "tokens": ["Den", "mein", "Va\u00b7ter", "schon", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der in sieben hei\u00dfen Schlachten", "tokens": ["Der", "in", "sie\u00b7ben", "hei\u00b7\u00dfen", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich zum Siege trug \u2013 f\u00fcr dich \u2013!: \u2013", "tokens": ["Mich", "zum", "Sie\u00b7ge", "trug", "\u2013", "f\u00fcr", "dich", "\u2013", "!", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$(", "APPR", "PPER", "$(", "$.", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gro\u00dfer K\u00f6nig \u2013 nimm ihn hin!\u00ab", "tokens": ["Gro\u00b7\u00dfer", "K\u00f6\u00b7nig", "\u2013", "nimm", "ihn", "hin", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbmich gel\u00fcstet,\u00ab sprach der K\u00f6nig,", "tokens": ["\u00bb", "mich", "ge\u00b7l\u00fcs\u00b7tet", ",", "\u00ab", "sprach", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVPP", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic