{"textgrid.poem.37443": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnftig", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O komm herbei, du goldne Zeit,", "tokens": ["O", "komm", "her\u00b7bei", ",", "du", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle, die jetzt bummeln,", "tokens": ["Wenn", "al\u00b7le", ",", "die", "jetzt", "bum\u00b7meln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sch\u00f6ner Unparteilichkeit", "tokens": ["In", "sch\u00f6\u00b7ner", "Un\u00b7par\u00b7tei\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bei der Arbeit tummeln.", "tokens": ["Sich", "bei", "der", "Ar\u00b7beit", "tum\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der L\u00e4rm, womit der Musikant", "tokens": ["Der", "L\u00e4rm", ",", "wo\u00b7mit", "der", "Mu\u00b7si\u00b7kant"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns st\u00f6rt, wird dann geringer.", "tokens": ["Uns", "st\u00f6rt", ",", "wird", "dann", "ge\u00b7rin\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer D\u00fcnger fuhr, wer Garben band,", "tokens": ["Wer", "D\u00fcn\u00b7ger", "fuhr", ",", "wer", "Gar\u00b7ben", "band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem kr\u00fcmmen sich die Finger.", "tokens": ["Dem", "kr\u00fcm\u00b7men", "sich", "die", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}