{"dta.poem.8471": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Laura.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.85", "nl:0.14"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wo Lucindens H\u00fclle starrt,", "tokens": ["Wo", "Lu\u00b7cin\u00b7dens", "H\u00fcl\u00b7le", "starrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ihr Geist von seiner Sklavenkette", "tokens": ["Wo", "ihr", "Geist", "von", "sei\u00b7ner", "Skla\u00b7ven\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Losgekettet ward.", "tokens": ["Los\u00b7ge\u00b7ket\u00b7tet", "ward", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Helle deinen Thr\u00e4nenblick. Am Throne,", "tokens": ["Hel\u00b7le", "dei\u00b7nen", "Thr\u00e4\u00b7nen\u00b7blick", ".", "Am", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Gottvers\u00f6hner thront,", "tokens": ["Wo", "der", "Gott\u00b7ver\u00b7s\u00f6h\u00b7ner", "thront", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist Lucinde mit der Siegeskrone,", "tokens": ["Ist", "Lu\u00b7cin\u00b7de", "mit", "der", "Sie\u00b7ges\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wohl ihr! schon belohnt.", "tokens": ["Wohl", "ihr", "!", "schon", "be\u00b7lohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denke dieser bleichen Todesmiene,", "tokens": ["Den\u00b7ke", "die\u00b7ser", "blei\u00b7chen", "To\u00b7des\u00b7mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dieses Lagers, wo du weinst,", "tokens": ["Die\u00b7ses", "La\u00b7gers", ",", "wo", "du", "weinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann du wieder auf der Narrenb\u00fchne", "tokens": ["Wann", "du", "wie\u00b7der", "auf", "der", "Nar\u00b7ren\u00b7b\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Stadt erscheinst.", "tokens": ["Dei\u00b7ner", "Stadt", "er\u00b7scheinst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ihres Kampfes denk' und ihres R\u00f6chelns,", "tokens": ["Ih\u00b7res", "Kamp\u00b7fes", "denk'", "und", "ih\u00b7res", "R\u00f6\u00b7chelns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erdgedanken zu zerstreun;", "tokens": ["Erd\u00b7ge\u00b7dan\u00b7ken", "zu", "zer\u00b7streun", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres Glaubens, ihres lezten L\u00e4chelns,", "tokens": ["Ih\u00b7res", "Glau\u00b7bens", ",", "ih\u00b7res", "lez\u00b7ten", "L\u00e4\u00b7chelns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Gottes dich zu freun.", "tokens": ["Got\u00b7tes", "dich", "zu", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}