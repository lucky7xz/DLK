{"dta.poem.5941": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedoch spricht die Vernunft das Urtheil gegen ihn.", "tokens": ["Je\u00b7doch", "spricht", "die", "Ver\u00b7nunft", "das", "Ur\u00b7theil", "ge\u00b7gen", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wofern er will in Zweifel ziehn", "tokens": ["Wo\u00b7fern", "er", "will", "in", "Zwei\u00b7fel", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Wesen, das geschickt die St\u00e4ubgen zu regieren,", "tokens": ["Ein", "We\u00b7sen", ",", "das", "ge\u00b7schickt", "die", "St\u00e4ub\u00b7gen", "zu", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die er in leeren Raum l\u00e4sst allenthalben wallen;", "tokens": ["Die", "er", "in", "lee\u00b7ren", "Raum", "l\u00e4sst", "al\u00b7len\u00b7thal\u00b7ben", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie k\u00f6nnen C\u00f6rper doch, die immer abw\u00e4rts fallen,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "C\u00f6r\u00b7per", "doch", ",", "die", "im\u00b7mer", "ab\u00b7w\u00e4rts", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NE", "ADV", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um ungez\u00e4hlte Welte zu formiren,", "tokens": ["Um", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Wel\u00b7te", "zu", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich zu vereinen sich bem\u00fchn?", "tokens": ["Sich", "zu", "ver\u00b7ei\u00b7nen", "sich", "be\u00b7m\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie kan er doch mit Recht verlangen und gedencken,", "tokens": ["Wie", "kan", "er", "doch", "mit", "Recht", "ver\u00b7lan\u00b7gen", "und", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df sie, indem sie sich gerad herunter sencken,", "tokens": ["Da\u00df", "sie", ",", "in\u00b7dem", "sie", "sich", "ge\u00b7rad", "her\u00b7un\u00b7ter", "sen\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sich selber k\u00f6nnten abwerts lencken?", "tokens": ["Sich", "sel\u00b7ber", "k\u00f6nn\u00b7ten", "ab\u00b7werts", "len\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie h\u00e4tten sonder End herunter sincken m\u00fcssen,", "tokens": ["Sie", "h\u00e4t\u00b7ten", "son\u00b7der", "End", "her\u00b7un\u00b7ter", "sin\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APZR", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ohn im geringsten sich zu binden und zu schl\u00fcssen,", "tokens": ["Ohn", "im", "ge\u00b7rings\u00b7ten", "sich", "zu", "bin\u00b7den", "und", "zu", "schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "PRF", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wofern man nicht auf solch ein Wesen dencket,", "tokens": ["Wo\u00b7fern", "man", "nicht", "auf", "solch", "ein", "We\u00b7sen", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "APPR", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das mit Bedacht sie abwerts lencket.", "tokens": ["Das", "mit", "Be\u00b7dacht", "sie", "ab\u00b7werts", "len\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}