{"textgrid.poem.56787": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bis an die Kniee stehn im Strom", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis an die Kniee stehn im Strom", "tokens": ["Bis", "an", "die", "Kni\u00b7ee", "stehn", "im", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die beiden Riesen Kraft und Ma\u00df:", "tokens": ["die", "bei\u00b7den", "Rie\u00b7sen", "Kraft", "und", "Ma\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihren breiten Nacken ruht", "tokens": ["Auf", "ih\u00b7ren", "brei\u00b7ten", "Na\u00b7cken", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Br\u00fccke st\u00e4hlernes Geb\u00e4lk.", "tokens": ["der", "Br\u00fc\u00b7cke", "st\u00e4h\u00b7ler\u00b7nes", "Ge\u00b7b\u00e4lk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Beine breit in Grund gestemmt,", "tokens": ["Bei\u00b7ne", "breit", "in", "Grund", "ge\u00b7stemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4nde auf des andern Schulter,", "tokens": ["H\u00e4n\u00b7de", "auf", "des", "an\u00b7dern", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stirn an Stirne fest gepre\u00dft,", "tokens": ["Stirn", "an", "Stir\u00b7ne", "fest", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "stehn sie da und schaun hinunter.", "tokens": ["stehn", "sie", "da", "und", "schaun", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da flieht die Welle ruhlos hin,", "tokens": ["Da", "flieht", "die", "Wel\u00b7le", "ruh\u00b7los", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wei\u00dfe Segel ziehn einher,", "tokens": ["und", "wei\u00b7\u00dfe", "Se\u00b7gel", "ziehn", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und dunkle Schlote w\u00f6lken Rauch,", "tokens": ["und", "dunk\u00b7le", "Schlo\u00b7te", "w\u00f6l\u00b7ken", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und Schollen t\u00fcrmt des Winters Frost.", "tokens": ["und", "Schol\u00b7len", "t\u00fcrmt", "des", "Win\u00b7ters", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber unbewegten Blicks", "tokens": ["A\u00b7ber", "un\u00b7be\u00b7weg\u00b7ten", "Blicks"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "stehn die muskelfrohen H\u00fcnen;", "tokens": ["stehn", "die", "mus\u00b7kel\u00b7fro\u00b7hen", "H\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "leis nur zuckt des Einen Leib", "tokens": ["leis", "nur", "zuckt", "des", "Ei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "stampft es droben donnernd dr\u00fcber.", "tokens": ["stampft", "es", "dro\u00b7ben", "don\u00b7nernd", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Der andre aber pre\u00dft die Stirn", "tokens": ["Der", "and\u00b7re", "a\u00b7ber", "pre\u00dft", "die", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nur fester, fester nur die Faust:", "tokens": ["nur", "fes\u00b7ter", ",", "fes\u00b7ter", "nur", "die", "Faust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kennt des Bruders trotzig Herz,", "tokens": ["Er", "kennt", "des", "Bru\u00b7ders", "trot\u00b7zig", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das tief im Kern die Menschheit ha\u00dft.", "tokens": ["das", "tief", "im", "Kern", "die", "Menschheit", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}