{"textgrid.poem.46548": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn du an das Knie die setztest]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du an das Knie die setztest", "tokens": ["Wenn", "du", "an", "das", "Knie", "die", "setz\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Mutter, sie ergetztest", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", ",", "sie", "er\u00b7getz\u00b7test"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit unschuld'gen Plaudereien,", "tokens": ["Mit", "un\u00b7schuld'\u00b7gen", "Plau\u00b7de\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Waren es mit lautem Schreien", "tokens": ["Wa\u00b7ren", "es", "mit", "lau\u00b7tem", "Schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Br\u00fcder, die dazwischen st\u00fcrzten,", "tokens": ["Br\u00fc\u00b7der", ",", "die", "da\u00b7zwi\u00b7schen", "st\u00fcrz\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dich \u00fcberall verk\u00fcrzten.", "tokens": ["Die", "dich", "\u00fc\u00b7be\u00b7rall", "ver\u00b7k\u00fcrz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Mutter schnell entrissne!", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "schnell", "ent\u00b7riss\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da um dich die Grambeflissne", "tokens": ["Da", "um", "dich", "die", "Gram\u00b7be\u00b7fliss\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wollt' in Kummerangedenken", "tokens": ["Wollt'", "in", "Kum\u00b7me\u00b7ran\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sich vertiefen, sich versenken,", "tokens": ["Sich", "ver\u00b7tie\u00b7fen", ",", "sich", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ward sie von den Br\u00fcdern wieder", "tokens": ["Ward", "sie", "von", "den", "Br\u00fc\u00b7dern", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aufgest\u00f6ret, die danieder", "tokens": ["Auf\u00b7ge\u00b7st\u00f6\u00b7ret", ",", "die", "da\u00b7nie\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sterbkrank, als du starbest, lagen.", "tokens": ["Sterb\u00b7krank", ",", "als", "du", "star\u00b7best", ",", "la\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "So in Tod- wie Lebenstagen", "tokens": ["So", "in", "To\u00b7d", "wie", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Hast du, Sanfte, Noth gelitten,", "tokens": ["Hast", "du", ",", "Sanf\u00b7te", ",", "Noth", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wie die Taub' in Geier Mitten.", "tokens": ["Wie", "die", "Taub'", "in", "Gei\u00b7er", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Gott sei Dank, da\u00df sie genesen!", "tokens": ["Gott", "sei", "Dank", ",", "da\u00df", "sie", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.18": {"text": "Und nun trittst du, liebes Wesen,", "tokens": ["Und", "nun", "trittst", "du", ",", "lie\u00b7bes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "In des Hauses Trauerstille", "tokens": ["In", "des", "Hau\u00b7ses", "Trau\u00b7er\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Unsichtbar, wie einst mein Wille", "tokens": ["Un\u00b7sicht\u00b7bar", ",", "wie", "einst", "mein", "Wil\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.21": {"text": "War, du solltest sichtbar walten,", "tokens": ["War", ",", "du", "soll\u00b7test", "sicht\u00b7bar", "wal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Lauten Braus im Einklang halten,", "tokens": ["Lau\u00b7ten", "Braus", "im", "Ein\u00b7klang", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Bild der Anmuth und der Sitte", "tokens": ["Bild", "der", "An\u00b7muth", "und", "der", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.24": {"text": "In der jungen Wilden Mitte.", "tokens": ["In", "der", "jun\u00b7gen", "Wil\u00b7den", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Werden sie dich fr\u00fch vergessen,", "tokens": ["Wer\u00b7den", "sie", "dich", "fr\u00fch", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.26": {"text": "Den Verlust nie ganz ermessen,", "tokens": ["Den", "Ver\u00b7lust", "nie", "ganz", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wie ich ihn ermessen habe:", "tokens": ["Wie", "ich", "ihn", "er\u00b7mes\u00b7sen", "ha\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Doch ein Hauch von deinem Grabe", "tokens": ["Doch", "ein", "Hauch", "von", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Soll mir siegreich helfen k\u00e4mpfen,", "tokens": ["Soll", "mir", "sieg\u00b7reich", "hel\u00b7fen", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Jugendliche St\u00fcrme d\u00e4mpfen.", "tokens": ["Ju\u00b7gend\u00b7li\u00b7che", "St\u00fcr\u00b7me", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Br\u00fcder, denkt, euch ist das Leben", "tokens": ["Br\u00fc\u00b7der", ",", "denkt", ",", "euch", "ist", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "F\u00fcr der Schwester Tod gegeben.", "tokens": ["F\u00fcr", "der", "Schwes\u00b7ter", "Tod", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Wenn ihr aus den Bettlein steiget", "tokens": ["Wenn", "ihr", "aus", "den", "Bet\u00b7tlein", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Neulebendig; schweiget, schweiget,", "tokens": ["Neu\u00b7le\u00b7ben\u00b7dig", ";", "schwei\u00b7get", ",", "schwei\u00b7get", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Junge Geier, muntre Schreier,", "tokens": ["Jun\u00b7ge", "Gei\u00b7er", ",", "mun\u00b7tre", "Schrei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Unsrer Taube Todtenfeier!", "tokens": ["Uns\u00b7rer", "Tau\u00b7be", "Tod\u00b7ten\u00b7fei\u00b7er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}