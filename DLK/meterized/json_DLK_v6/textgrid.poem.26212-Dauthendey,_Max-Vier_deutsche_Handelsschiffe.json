{"textgrid.poem.26212": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Vier deutsche Handelsschiffe", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier deutsche Handelsschiffe liegen rauchlos dort,", "tokens": ["Vier", "deut\u00b7sche", "Han\u00b7dels\u00b7schif\u00b7fe", "lie\u00b7gen", "rauch\u00b7los", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Str\u00f6mung dreht sie stets am gleichen Ort.", "tokens": ["Die", "Str\u00f6\u00b7mung", "dreht", "sie", "stets", "am", "glei\u00b7chen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind verankert, haben Weile, warten ab,", "tokens": ["Sie", "sind", "ver\u00b7an\u00b7kert", ",", "ha\u00b7ben", "Wei\u00b7le", ",", "war\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VAFIN", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis sich gef\u00fcllt das Riesenmassengrab", "tokens": ["Bis", "sich", "ge\u00b7f\u00fcllt", "das", "Rie\u00b7sen\u00b7mas\u00b7sen\u00b7grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fern in der Heimat, das der Krieg gegraben,", "tokens": ["Fern", "in", "der", "Hei\u00b7mat", ",", "das", "der", "Krieg", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sich die Raben sattgefressen haben.", "tokens": ["Und", "sich", "die", "Ra\u00b7ben", "satt\u00b7ge\u00b7fres\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}