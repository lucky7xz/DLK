{"textgrid.poem.31637": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "9. Ich f\u00f6rchte mich nicht vor viel hundert tausend", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist der Herr mit / mir zu helffen / ey so hat kein Furcht hier stat!", "tokens": ["Ist", "der", "Herr", "mit", "/", "mir", "zu", "helf\u00b7fen", "/", "ey", "so", "hat", "kein", "Furcht", "hier", "stat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "$(", "PPER", "PTKZU", "VVINF", "$(", "NE", "ADV", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Er ist ja die Wei\u00dfheit selber und die St\u00e4rk' in wahrer That /", "tokens": ["Er", "ist", "ja", "die", "Wei\u00df\u00b7heit", "sel\u00b7ber", "und", "die", "St\u00e4rk'", "in", "wah\u00b7rer", "That", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "m\u00e4chtig ist er in dem Streit / von und mit ihm kommt das Siegen.", "tokens": ["m\u00e4ch\u00b7tig", "ist", "er", "in", "dem", "Streit", "/", "von", "und", "mit", "ihm", "kommt", "das", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "APPR", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Last derhalben alle Welt / H\u00f6ll und Teuffel / mit uns kriegen", "tokens": ["Last", "der\u00b7hal\u00b7ben", "al\u00b7le", "Welt", "/", "H\u00f6ll", "und", "Teuf\u00b7fel", "/", "mit", "uns", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "$(", "NN", "KON", "NN", "$(", "APPR", "PPER", "VVINF"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Feur vom Himmel wird sie fressen / wie die zweymal funffzig Mann:", "tokens": ["Feur", "vom", "Him\u00b7mel", "wird", "sie", "fres\u00b7sen", "/", "wie", "die", "zwey\u00b7mal", "funff\u00b7zig", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPER", "VVINF", "$(", "KOKOM", "ART", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "theils wird Rohtes Meer ert\u00f6dten / theils ein Eselsbacken-Zahn.", "tokens": ["theils", "wird", "Roh\u00b7tes", "Meer", "er\u00b7t\u00f6d\u00b7ten", "/", "theils", "ein", "E\u00b7sels\u00b7ba\u00b7cken\u00b7Zahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "VVFIN", "$(", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}