{"dta.poem.17356": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "206.  In wem der Sohn GOttes ge-  \n bohren ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wem alle ding ein ding und lauter Friede sind/", "tokens": ["Wem", "al\u00b7le", "ding", "ein", "ding", "und", "lau\u00b7ter", "Frie\u00b7de", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ART", "NN", "KON", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem ist wahrlich schon gebohrn das Jungfraun Kind.", "tokens": ["In", "dem", "ist", "wahr\u00b7lich", "schon", "ge\u00b7bohrn", "das", "Jung\u00b7fraun", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "ADV", "ADV", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}