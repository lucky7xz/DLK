{"textgrid.poem.41230": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Friedens Mutter ist Bescheidenheit,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Friedens Mutter ist Bescheidenheit,", "tokens": ["Des", "Frie\u00b7dens", "Mut\u00b7ter", "ist", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Scham des Barden beste Feierzier.", "tokens": ["Und", "Scham", "des", "Bar\u00b7den", "bes\u00b7te", "Fei\u00b7er\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Sohn! ich tadle Lobbegierde nicht.", "tokens": ["Mein", "Sohn", "!", "ich", "tad\u00b7le", "Lob\u00b7be\u00b7gier\u00b7de", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lob ist der Seelenstachel be\u00df'rer Art,", "tokens": ["Lob", "ist", "der", "See\u00b7len\u00b7sta\u00b7chel", "be\u00df'\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ohne diesen Stachel schlummerten", "tokens": ["Und", "oh\u00b7ne", "die\u00b7sen", "Sta\u00b7chel", "schlum\u00b7mer\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Die gr\u00f6\u00dften Thaten der Vergangenheit,", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "Tha\u00b7ten", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die besten Lieder unerweckten Schlaf.", "tokens": ["Die", "bes\u00b7ten", "Lie\u00b7der", "un\u00b7er\u00b7weck\u00b7ten", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch niemal hoffe der von Andern Lob,", "tokens": ["Doch", "nie\u00b7mal", "hof\u00b7fe", "der", "von", "An\u00b7dern", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Eigenlob von trunk'ner Lippe tr\u00e4uft.", "tokens": ["Dem", "Ei\u00b7gen\u00b7lob", "von", "trunk'\u00b7ner", "Lip\u00b7pe", "tr\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie, wie tr\u00e4ufelte dir Eigenlob", "tokens": ["Und", "wie", ",", "wie", "tr\u00e4u\u00b7fel\u00b7te", "dir", "Ei\u00b7gen\u00b7lob"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "PWAV", "VVFIN", "PPER", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Von deiner Lippe? Zeichnetest du vor,", "tokens": ["Von", "dei\u00b7ner", "Lip\u00b7pe", "?", "Zeich\u00b7ne\u00b7test", "du", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als dich Allvater schuf? Verstand, Gef\u00fchl,", "tokens": ["Als", "dich", "All\u00b7va\u00b7ter", "schuf", "?", "Ver\u00b7stand", ",", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gedankenschwung, und Bildekraft, und Ohr,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7schwung", ",", "und", "Bil\u00b7de\u00b7kraft", ",", "und", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Saitenf\u00e4higkeit \u2013 gab er sie dir?", "tokens": ["Und", "Sai\u00b7ten\u00b7f\u00e4\u00b7hig\u00b7keit", "\u2013", "gab", "er", "sie", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mu\u00dft' er sie geben dir? Und wenn sie nun", "tokens": ["Mu\u00dft'", "er", "sie", "ge\u00b7ben", "dir", "?", "Und", "wenn", "sie", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "PPER", "$.", "KON", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sie alle Gaben seiner Willk\u00fchr sind,", "tokens": ["Sie", "al\u00b7le", "Ga\u00b7ben", "sei\u00b7ner", "Will\u00b7k\u00fchr", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Und du sie n\u00fctzest, thust du mehr, als Pflicht?", "tokens": ["Und", "du", "sie", "n\u00fct\u00b7zest", ",", "thust", "du", "mehr", ",", "als", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Pflichten sind der Laster Gegensatz.", "tokens": ["Die", "Pflich\u00b7ten", "sind", "der", "Las\u00b7ter", "Ge\u00b7gen\u00b7satz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der gr\u00f6\u00dften eines ist Undankbarkeit.", "tokens": ["Der", "gr\u00f6\u00df\u00b7ten", "ei\u00b7nes", "ist", "Un\u00b7dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du fassest jetzo deinen Wanderstab,", "tokens": ["Du", "fas\u00b7sest", "jet\u00b7zo", "dei\u00b7nen", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wallest deines Vaters Giebel zu.", "tokens": ["Und", "wal\u00b7lest", "dei\u00b7nes", "Va\u00b7ters", "Gie\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun sage, wirst du wohl auf jeder Flur", "tokens": ["Nun", "sa\u00b7ge", ",", "wirst", "du", "wohl", "auf", "je\u00b7der", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Blumenpfl\u00fccken weilen? Wirst du dich", "tokens": ["Mit", "Blu\u00b7men\u00b7pfl\u00fc\u00b7cken", "wei\u00b7len", "?", "Wirst", "du", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In jeder Quelle spiegeln, dich, zu ruhn,", "tokens": ["In", "je\u00b7der", "Quel\u00b7le", "spie\u00b7geln", ",", "dich", ",", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In jedem Schatten niederwerfen? Nein!", "tokens": ["In", "je\u00b7dem", "Schat\u00b7ten", "nie\u00b7der\u00b7wer\u00b7fen", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Vaters Harren, und der Mutter Wunsch", "tokens": ["Des", "Va\u00b7ters", "Har\u00b7ren", ",", "und", "der", "Mut\u00b7ter", "Wunsch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dich bald zu sehn befl\u00fcgelt deinen Fu\u00df.", "tokens": ["Dich", "bald", "zu", "sehn", "be\u00b7fl\u00fc\u00b7gelt", "dei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wirf einst jedes Lied, das dir ger\u00e4th,", "tokens": ["So", "wirf", "einst", "je\u00b7des", "Lied", ",", "das", "dir", "ge\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Schnell \u00fcber deinen R\u00fccken, sieh nicht um,", "tokens": ["Schnell", "\u00fc\u00b7ber", "dei\u00b7nen", "R\u00fc\u00b7cken", ",", "sieh", "nicht", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "VVIMP", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sieh nur vor dir hin, wo ein neuer Spro\u00df", "tokens": ["Sieh", "nur", "vor", "dir", "hin", ",", "wo", "ein", "neu\u00b7er", "Spro\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr deine Schl\u00e4fe dir entgegengr\u00fcnt.", "tokens": ["F\u00fcr", "dei\u00b7ne", "Schl\u00e4\u00b7fe", "dir", "ent\u00b7ge\u00b7gen\u00b7gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bald laubt sich hinter deinen Pfaden her", "tokens": ["Bald", "laubt", "sich", "hin\u00b7ter", "dei\u00b7nen", "Pfa\u00b7den", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein junger Eichenhain f\u00fcr dich empor.", "tokens": ["Ein", "jun\u00b7ger", "Ei\u00b7chen\u00b7hain", "f\u00fcr", "dich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du siehst ihn nicht; allein ihn sieht dein Volk,", "tokens": ["Du", "siehst", "ihn", "nicht", ";", "al\u00b7lein", "ihn", "sieht", "dein", "Volk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und preiset deine Lieder desto mehr,", "tokens": ["Und", "prei\u00b7set", "dei\u00b7ne", "Lie\u00b7der", "des\u00b7to", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Je mehr du selbst von deinen Liedern schweigst.", "tokens": ["Je", "mehr", "du", "selbst", "von", "dei\u00b7nen", "Lie\u00b7dern", "schweigst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gib Ehre denen, deren Harfenruhm", "tokens": ["Gib", "Eh\u00b7re", "de\u00b7nen", ",", "de\u00b7ren", "Har\u00b7fen\u00b7ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NN", "PDS", "$,", "PRELAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im ganzen Erbe Teuts, wie Sonnen, stralt.", "tokens": ["Im", "gan\u00b7zen", "Er\u00b7be", "Teuts", ",", "wie", "Son\u00b7nen", ",", "stralt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind Allvaters helles Bild, die Zier", "tokens": ["Sie", "sind", "All\u00b7va\u00b7ters", "hel\u00b7les", "Bild", ",", "die", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschheit, Lehrer von Jahrhunderten.", "tokens": ["Der", "Menschheit", ",", "Leh\u00b7rer", "von", "Jahr\u00b7hun\u00b7der\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Beneide jene nicht, die, weit verstreut", "tokens": ["Be\u00b7nei\u00b7de", "je\u00b7ne", "nicht", ",", "die", ",", "weit", "ver\u00b7streut"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "PDS", "PTKNEG", "$,", "PRELS", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Gauen, gleichen Weg mit dir", "tokens": ["Durch", "al\u00b7le", "Gau\u00b7en", ",", "glei\u00b7chen", "Weg", "mit", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Bardenkunst bewandeln. Freue dich", "tokens": ["In", "Bar\u00b7den\u00b7kunst", "be\u00b7wan\u00b7deln", ".", "Freu\u00b7e", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vielmehr auch ihres Namens, wenn er steigt.", "tokens": ["Viel\u00b7mehr", "auch", "ih\u00b7res", "Na\u00b7mens", ",", "wenn", "er", "steigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn Oder, Elbe, Weser, Donau, Spree", "tokens": ["Denn", "O\u00b7der", ",", "El\u00b7be", ",", "We\u00b7ser", ",", "Do\u00b7nau", ",", "Spree"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NE", "$,", "NE", "$,", "NN", "$,", "NE", "$,", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sind alle deutsche Fl\u00fc\u00dfe. Jedes Lob", "tokens": ["Sind", "al\u00b7le", "deut\u00b7sche", "Fl\u00fc\u00b7\u00dfe", ".", "Je\u00b7des", "Lob"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Barden ist des Vaterlandes Lob.", "tokens": ["Des", "Bar\u00b7den", "ist", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Tritt einst ein schw\u00e4cherer Versucher auf,", "tokens": ["Tritt", "einst", "ein", "schw\u00e4\u00b7che\u00b7rer", "Ver\u00b7su\u00b7cher", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bringt ein ungereiftes Lied im Volk,", "tokens": ["Und", "bringt", "ein", "un\u00b7ge\u00b7reif\u00b7tes", "Lied", "im", "Volk", ","], "token_info": ["word", "wor