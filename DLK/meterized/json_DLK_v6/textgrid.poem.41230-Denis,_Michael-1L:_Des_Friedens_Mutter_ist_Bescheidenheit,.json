word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "FM", "ADV", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich mich nicht sch\u00fctzen, bin des Tadels werth,", "tokens": ["Ich", "mich", "nicht", "sch\u00fct\u00b7zen", ",", "bin", "des", "Ta\u00b7dels", "werth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKNEG", "VVINF", "$,", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und, was ich unternahm, wird bald wie Duft,", "tokens": ["Und", ",", "was", "ich", "un\u00b7ter\u00b7nahm", ",", "wird", "bald", "wie", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Vor meines Volkes Augen sich zerstreu'n.", "tokens": ["Vor", "mei\u00b7nes", "Vol\u00b7kes", "Au\u00b7gen", "sich", "zer\u00b7streu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Allein gelang es mir, den hohen Gang,", "tokens": ["Al\u00b7lein", "ge\u00b7lang", "es", "mir", ",", "den", "ho\u00b7hen", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Den du einst gingst, dir m\u00e4nnlich nachzugehn,", "tokens": ["Den", "du", "einst", "gingst", ",", "dir", "m\u00e4nn\u00b7lich", "nach\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Den Kindern Teut's dein lange schlummernd Lied", "tokens": ["Den", "Kin\u00b7dern", "Teut's", "dein", "lan\u00b7ge", "schlum\u00b7mernd", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "PPOSAT", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "So, wie es war, zu wecken, o dann tritt,", "tokens": ["So", ",", "wie", "es", "war", ",", "zu", "we\u00b7cken", ",", "o", "dann", "tritt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "FM", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dann tritt es vor der Nachwelt Richterstuhl", "tokens": ["Dann", "tritt", "es", "vor", "der", "Nach\u00b7welt", "Rich\u00b7ter\u00b7stuhl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Mit heit'rer Zuversicht, und steht so fest", "tokens": ["Mit", "heit'\u00b7rer", "Zu\u00b7ver\u00b7sicht", ",", "und", "steht", "so", "fest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Dem Tadel, wie den Wogen Morvens Fels.", "tokens": ["Dem", "Ta\u00b7del", ",", "wie", "den", "Wo\u00b7gen", "Mor\u00b7vens", "Fels", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So denk' und handle stets, mein Sohn! Dann wird", "tokens": ["So", "denk'", "und", "hand\u00b7le", "stets", ",", "mein", "Sohn", "!", "Dann", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch deine Seele, gleich der glatten See,", "tokens": ["Auch", "dei\u00b7ne", "See\u00b7le", ",", "gleich", "der", "glat\u00b7ten", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Worinn des Mondes holder Silberblick", "tokens": ["Wo\u00b7rinn", "des", "Mon\u00b7des", "hol\u00b7der", "Sil\u00b7berb\u00b7lick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sich l\u00e4chelnd mahlt, zufrieden, ruhevoll", "tokens": ["Sich", "l\u00e4\u00b7chelnd", "mahlt", ",", "zu\u00b7frie\u00b7den", ",", "ru\u00b7he\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch alle deine Lebenstage seyn.", "tokens": ["Durch", "al\u00b7le", "dei\u00b7ne", "Le\u00b7bens\u00b7ta\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}