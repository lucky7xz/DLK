{"textgrid.poem.36264": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Fr\u00f6hliche", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie frisch vom Regen mit erneuter Bl\u00fcte", "tokens": ["Wie", "frisch", "vom", "Re\u00b7gen", "mit", "er\u00b7neu\u00b7ter", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gr\u00fcnen Kinder bl\u00fchen im Gefilde,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Kin\u00b7der", "bl\u00fc\u00b7hen", "im", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So spielt im heitern Licht die innre G\u00fcte,", "tokens": ["So", "spielt", "im", "hei\u00b7tern", "Licht", "die", "inn\u00b7re", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn s\u00fc\u00dfer Rede Tau uns netzet milde,", "tokens": ["Wenn", "s\u00fc\u00b7\u00dfer", "Re\u00b7de", "Tau", "uns", "net\u00b7zet", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im bunten Glanze froh ein leicht Gem\u00fcte", "tokens": ["Im", "bun\u00b7ten", "Glan\u00b7ze", "froh", "ein", "leicht", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Scherze dichtet fl\u00fcchtige Gebilde,", "tokens": ["Zum", "Scher\u00b7ze", "dich\u00b7tet", "fl\u00fcch\u00b7ti\u00b7ge", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo Leichtsinn oft den leichten Sinn versch\u00f6net,", "tokens": ["Wo", "Leicht\u00b7sinn", "oft", "den", "leich\u00b7ten", "Sinn", "ver\u00b7sch\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Witz sich zierlich selbst zum Schein verh\u00f6hnet.", "tokens": ["Der", "Witz", "sich", "zier\u00b7lich", "selbst", "zum", "Schein", "ver\u00b7h\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}