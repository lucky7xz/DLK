{"textgrid.poem.54715": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "29. An Eugenien", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Spigel schenck ich euch/ O Spigel h\u00f6chster Zucht/", "tokens": ["Den", "Spi\u00b7gel", "schenck", "ich", "euch", "/", "O", "Spi\u00b7gel", "h\u00f6chs\u00b7ter", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$(", "NE", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem ihr schawen m\u00f6gt was ich bi\u00dfher gesucht.", "tokens": ["In", "dem", "ihr", "scha\u00b7wen", "m\u00f6gt", "was", "ich", "bi\u00df\u00b7her", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan iemand euch was mehr Wohl-Edle Jungfrau geben/", "tokens": ["Kan", "ie\u00b7mand", "euch", "was", "mehr", "Wohl\u00b7Ed\u00b7le", "Jung\u00b7frau", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "PIS", "PIAT", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als di\u00df in dem ihr euch seht gehn und stehn und leben?", "tokens": ["Als", "di\u00df", "in", "dem", "ihr", "euch", "seht", "gehn", "und", "stehn", "und", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "VVINF", "KON", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch k\u00f6nnt ihr/ wenn ihr gebt/ was ich so hoch begehrt", "tokens": ["Doch", "k\u00f6nnt", "ihr", "/", "wenn", "ihr", "gebt", "/", "was", "ich", "so", "hoch", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$(", "PWS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir geben/ was in euch mir doppelt mich gewehrt.", "tokens": ["Mir", "ge\u00b7ben", "/", "was", "in", "euch", "mir", "dop\u00b7pelt", "mich", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "PWS", "APPR", "PPER", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}