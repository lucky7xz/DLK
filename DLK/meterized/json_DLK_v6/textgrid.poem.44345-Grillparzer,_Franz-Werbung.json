{"textgrid.poem.44345": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Werbung", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, willst du mich geh\u00f6ren,", "tokens": ["M\u00e4d\u00b7chen", ",", "willst", "du", "mich", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So sprich ja und schlag nur ein!", "tokens": ["So", "sprich", "ja", "und", "schlag", "nur", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kann nicht seufzen, kann nicht schw\u00f6ren:", "tokens": ["Kann", "nicht", "seuf\u00b7zen", ",", "kann", "nicht", "schw\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du \u2013 gut! Wenn nicht \u2013 mags sein!", "tokens": ["Willst", "du", "\u2013", "gut", "!", "Wenn", "nicht", "\u2013", "mags", "sein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "ADJD", "$.", "KOUS", "PTKNEG", "$(", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gold hab ich nicht aufzuweisen,", "tokens": ["Gold", "hab", "ich", "nicht", "auf\u00b7zu\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Lieder zahlen auch;", "tokens": ["A\u00b7ber", "Lie\u00b7der", "zah\u00b7len", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dich loben, will dich preisen,", "tokens": ["Will", "dich", "lo\u00b7ben", ",", "will", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's bei Dichtern heitrer Brauch.", "tokens": ["Wie's", "bei", "Dich\u00b7tern", "hei\u00b7trer", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch gef\u00e4llts dir einst zu brechen,", "tokens": ["Doch", "ge\u00b7f\u00e4llts", "dir", "einst", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tut mit Ma\u00df und h\u00fctte dich.", "tokens": ["Tut", "mit", "Ma\u00df", "und", "h\u00fct\u00b7te", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lied, das schmeichelt, kann auch stechen,", "tokens": ["Lied", ",", "das", "schmei\u00b7chelt", ",", "kann", "auch", "ste\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dichters Gram ist bald verschlafen,", "tokens": ["Dich\u00b7ters", "Gram", "ist", "bald", "ver\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Kunst ist Trostes reich,", "tokens": ["Sei\u00b7ne", "Kunst", "ist", "Tros\u00b7tes", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Lieder, die dich strafen,", "tokens": ["Und", "die", "Lie\u00b7der", ",", "die", "dich", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6sten heilend ihn zugleich.", "tokens": ["Tr\u00f6s\u00b7ten", "hei\u00b7lend", "ihn", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}