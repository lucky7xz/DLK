{"textgrid.poem.32872": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein schmachtend Auge scheint mir, Chloe, l\u00e4ngst zu sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein schmachtend Auge scheint mir, Chloe, l\u00e4ngst zu sagen,", "tokens": ["Dein", "schmach\u00b7tend", "Au\u00b7ge", "scheint", "mir", ",", "Chloe", ",", "l\u00e4ngst", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df du nicht unempfindlich bist,", "tokens": ["Da\u00df", "du", "nicht", "un\u00b7emp\u00b7find\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df der Wunsch von meinen stillen Klagen", "tokens": ["Und", "da\u00df", "der", "Wunsch", "von", "mei\u00b7nen", "stil\u00b7len", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir kenntlich worden ist:", "tokens": ["Dir", "kennt\u00b7lich", "wor\u00b7den", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch was dein schmachtend Aug mir scheinet zu gew\u00e4hren,", "tokens": ["Doch", "was", "dein", "schmach\u00b7tend", "Aug", "mir", "schei\u00b7net", "zu", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich zu fodern nie gewagt:", "tokens": ["Hab", "ich", "zu", "fo\u00b7dern", "nie", "ge\u00b7wagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! frage mich, la\u00df dir von mir erkl\u00e4ren,", "tokens": ["O", "!", "fra\u00b7ge", "mich", ",", "la\u00df", "dir", "von", "mir", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Was dir mein Auge klagt!", "tokens": ["Was", "dir", "mein", "Au\u00b7ge", "klagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}