{"textgrid.poem.58655": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will man der b\u00f6sen Eigenlieb' nichts in der Welt mehr geben,", "tokens": ["Will", "man", "der", "b\u00f6\u00b7sen", "Ei\u00b7gen\u00b7lieb'", "nichts", "in", "der", "Welt", "mehr", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "PIS", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So suchet die verschmitzte Schlang' im Christentum ihr Leben,", "tokens": ["So", "su\u00b7chet", "die", "ver\u00b7schmitz\u00b7te", "Schlang'", "im", "Chris\u00b7ten\u00b7tum", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Nach gro\u00dfen Dingen trachtet sie, nach Gaben, Licht und Schein,", "tokens": ["Nach", "gro\u00b7\u00dfen", "Din\u00b7gen", "trach\u00b7tet", "sie", ",", "nach", "Ga\u00b7ben", ",", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Man w\u00fcnschet einen hohen Stand, man will was Gro\u00dfes sein.", "tokens": ["Man", "w\u00fcn\u00b7schet", "ei\u00b7nen", "ho\u00b7hen", "Stand", ",", "man", "will", "was", "Gro\u00b7\u00dfes", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "PIS", "VMFIN", "PIS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "O liebe Seel', begehr es nicht! Gott liebet zwar die Frommen,", "tokens": ["O", "lie\u00b7be", "Seel'", ",", "be\u00b7gehr", "es", "nicht", "!", "Gott", "lie\u00b7bet", "zwar", "die", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Doch \u00fcber Fleisch und Eigenheit mu\u00df Tod und Ungl\u00fcck kommen.", "tokens": ["Doch", "\u00fc\u00b7ber", "Fleisch", "und", "Ei\u00b7gen\u00b7heit", "mu\u00df", "Tod", "und", "Un\u00b7gl\u00fcck", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Herr, deine Glorie mich erfreut, f\u00fcr mich ich gar nichts such',", "tokens": ["Herr", ",", "dei\u00b7ne", "Glo\u00b7rie", "mich", "er\u00b7freut", ",", "f\u00fcr", "mich", "ich", "gar", "nichts", "such'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "APPR", "PPER", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Gibst du mir meine Seel' zur Beut', so bin ich gro\u00df genug.", "tokens": ["Gibst", "du", "mir", "mei\u00b7ne", "Seel'", "zur", "Beut'", ",", "so", "bin", "ich", "gro\u00df", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}