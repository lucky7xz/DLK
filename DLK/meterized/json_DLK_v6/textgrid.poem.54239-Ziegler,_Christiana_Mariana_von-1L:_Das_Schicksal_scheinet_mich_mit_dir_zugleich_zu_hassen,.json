{"textgrid.poem.54239": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Schicksal scheinet mich mit dir zugleich zu hassen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Schicksal scheinet mich mit dir zugleich zu hassen,", "tokens": ["Das", "Schick\u00b7sal", "schei\u00b7net", "mich", "mit", "dir", "zu\u00b7gleich", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und des Verh\u00e4ngni\u00df Schlu\u00df benimmt mir allen Muth.", "tokens": ["Und", "des", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "Schlu\u00df", "be\u00b7nimmt", "mir", "al\u00b7len", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df bey deinen Schertz mich, Liebster, nicht zu lassen,", "tokens": ["Ich", "wei\u00df", "bey", "dei\u00b7nen", "Schertz", "mich", ",", "Liebs\u00b7ter", ",", "nicht", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil deine Wunde mir zugleich mit wehe thut.", "tokens": ["Weil", "dei\u00b7ne", "Wun\u00b7de", "mir", "zu\u00b7gleich", "mit", "we\u00b7he", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Fama mich zuvor lie\u00df mit Erschrecken wissen,", "tokens": ["Was", "Fa\u00b7ma", "mich", "zu\u00b7vor", "lie\u00df", "mit", "Er\u00b7schre\u00b7cken", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "ADV", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das ist es, was dein Brief mir selbsten zugesteht.", "tokens": ["Das", "ist", "es", ",", "was", "dein", "Brief", "mir", "selbs\u00b7ten", "zu\u00b7ge\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich soll, bedenck es selbst, dein Antlitz leider! missen,", "tokens": ["Ich", "soll", ",", "be\u00b7denck", "es", "selbst", ",", "dein", "Ant\u00b7litz", "lei\u00b7der", "!", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ADV", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Fu\u00df, der sonst so schnell gleich muntern Rehen geht,", "tokens": ["Dein", "Fu\u00df", ",", "der", "sonst", "so", "schnell", "gleich", "mun\u00b7tern", "Re\u00b7hen", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mu\u00df, weil man ihn verletzt, in Band und Fesseln liegen;", "tokens": ["Mu\u00df", ",", "weil", "man", "ihn", "ver\u00b7letzt", ",", "in", "Band", "und", "Fes\u00b7seln", "lie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein recht verfluchter Stahl streckt seine Freyheit hin.", "tokens": ["Ein", "recht", "ver\u00b7fluch\u00b7ter", "Stahl", "streckt", "sei\u00b7ne", "Frey\u00b7heit", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O! d\u00fcrfft ich mich zu dir, Annehmlichster, verf\u00fcgen,", "tokens": ["O", "!", "d\u00fcrfft", "ich", "mich", "zu", "dir", ",", "An\u00b7nehm\u00b7lichs\u00b7ter", ",", "ver\u00b7f\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PRF", "APPR", "PPER", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Du w\u00fcrdest warlich sehn, wie Jammers-voll ich bin,", "tokens": ["Du", "w\u00fcr\u00b7dest", "war\u00b7lich", "sehn", ",", "wie", "Jam\u00b7mer\u00b7svoll", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "PWAV", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Hertze f\u00fchlt zugleich durch Sympathie das Eisen,", "tokens": ["Mein", "Hert\u00b7ze", "f\u00fchlt", "zu\u00b7gleich", "durch", "Sym\u00b7pa\u00b7thie", "das", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df dir und deinen Fu\u00df so grossen Tort gethan.", "tokens": ["Da\u00df", "dir", "und", "dei\u00b7nen", "Fu\u00df", "so", "gros\u00b7sen", "Tort", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach k\u00f6nt ich mir ein Glied von meinen Leibe reissen,", "tokens": ["Ach", "k\u00f6nt", "ich", "mir", "ein", "Glied", "von", "mei\u00b7nen", "Lei\u00b7be", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich b\u00f6th es dir zum Dienst mit allen Freuden an,", "tokens": ["Ich", "b\u00f6th", "es", "dir", "zum", "Dienst", "mit", "al\u00b7len", "Freu\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nur da\u00df ich dich vom Schmertz befreyet k\u00f6nte sehen,", "tokens": ["Nur", "da\u00df", "ich", "dich", "vom", "Schmertz", "be\u00b7fre\u00b7yet", "k\u00f6n\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn meine Leidenschafft ist, glaub es, allzu gro\u00df,", "tokens": ["Denn", "mei\u00b7ne", "Lei\u00b7den\u00b7schafft", "ist", ",", "glaub", "es", ",", "all\u00b7zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch was erbieth ich mich? Di\u00df kan wohl nicht geschehen,", "tokens": ["Doch", "was", "er\u00b7bieth", "ich", "mich", "?", "Di\u00df", "kan", "wohl", "nicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "$.", "PDS", "VMFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich g\u00e4be, th\u00e4t ich es, vor aller Welt mich blo\u00df.", "tokens": ["Ich", "g\u00e4\u00b7be", ",", "th\u00e4t", "ich", "es", ",", "vor", "al\u00b7ler", "Welt", "mich", "blo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "APPR", "PIAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum mu\u00df ich heimlich nur mit dir die Schmertzen theilen;", "tokens": ["Drum", "mu\u00df", "ich", "heim\u00b7lich", "nur", "mit", "dir", "die", "Schmert\u00b7zen", "thei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was darf die gantze Welt hiervon ein Zeuge seyn?", "tokens": ["Was", "darf", "die", "gant\u00b7ze", "Welt", "hier\u00b7von", "ein", "Zeu\u00b7ge", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "PAV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gnug, da\u00df dir diese bang- und halb verwirrten Zeilen", "tokens": ["Gnug", ",", "da\u00df", "dir", "die\u00b7se", "bang", "und", "halb", "ver\u00b7wirr\u00b7ten", "Zei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PDAT", "TRUNC", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Beyleid ins geheim, Geliebter Lesbus, weyhn.", "tokens": ["Mein", "Bey\u00b7leid", "ins", "ge\u00b7heim", ",", "Ge\u00b7lieb\u00b7ter", "Les\u00b7bus", ",", "weyhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJD", "$,", "NN", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hyg\u00e4a wird inde\u00df vor deine Heilung sorgen,", "tokens": ["Hy\u00b7g\u00e4a", "wird", "in\u00b7de\u00df", "vor", "dei\u00b7ne", "Hei\u00b7lung", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Ich flehe selbige hierum mit Thr\u00e4nen an.", "tokens": ["Ich", "fle\u00b7he", "sel\u00b7bi\u00b7ge", "hie\u00b7rum", "mit", "Thr\u00e4\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Vorspruch plagt sie recht von Abend bi\u00df an Morgen,", "tokens": ["Mein", "Vor\u00b7spruch", "plagt", "sie", "recht", "von", "A\u00b7bend", "bi\u00df", "an", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil ich dich l\u00e4nger nicht, mein Freund, entbehren kan.", "tokens": ["Weil", "ich", "dich", "l\u00e4n\u00b7ger", "nicht", ",", "mein", "Freund", ",", "ent\u00b7beh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich schliesse meinen Brief und wart auf dein Genesen,", "tokens": ["Ich", "schlies\u00b7se", "mei\u00b7nen", "Brief", "und", "wart", "auf", "dein", "Ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich zehle Stund und Tag, wenn ich dich sprechen kan.", "tokens": ["Ich", "zeh\u00b7le", "Stund", "und", "Tag", ",", "wenn", "ich", "dich", "spre\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du solst die Freud alsdenn aus meinen Augen lesen,", "tokens": ["Du", "solst", "die", "Freud", "als\u00b7denn", "aus", "mei\u00b7nen", "Au\u00b7gen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So sehr als Sylvia zuvor um dich gethan.", "tokens": ["So", "sehr", "als", "Syl\u00b7via", "zu\u00b7vor", "um", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NE", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}