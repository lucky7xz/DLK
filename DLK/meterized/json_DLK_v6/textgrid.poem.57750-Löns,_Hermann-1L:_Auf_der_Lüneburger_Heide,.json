{"textgrid.poem.57750": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der L\u00fcneburger Heide,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der L\u00fcneburger Heide,", "tokens": ["Auf", "der", "L\u00fc\u00b7ne\u00b7bur\u00b7ger", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem wundersch\u00f6nen Land", "tokens": ["In", "dem", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging ich auf und ging ich unter,", "tokens": ["Ging", "ich", "auf", "und", "ging", "ich", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allerlei am Weg ich fand;", "tokens": ["Al\u00b7ler\u00b7lei", "am", "Weg", "ich", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Valleri, vallera,", "tokens": ["Val\u00b7le\u00b7ri", ",", "val\u00b7le\u00b7ra", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und juchheirassa,", "tokens": ["Und", "juch\u00b7hei\u00b7ras\u00b7sa", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bester Schatz, bester Schatz,", "tokens": ["Bes\u00b7ter", "Schatz", ",", "bes\u00b7ter", "Schatz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Denn du wei\u00dft es wei\u00dft es ja.", "tokens": ["Denn", "du", "wei\u00dft", "es", "wei\u00dft", "es", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Br\u00fcder, la\u00dft die Gl\u00e4ser klingen,", "tokens": ["Br\u00fc\u00b7der", ",", "la\u00dft", "die", "Gl\u00e4\u00b7ser", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn der Muskatellerwein", "tokens": ["Denn", "der", "Mus\u00b7ka\u00b7tel\u00b7ler\u00b7wein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wird vom langen Stehen sauer,", "tokens": ["Wird", "vom", "lan\u00b7gen", "Ste\u00b7hen", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgetrunken mu\u00df er sein;", "tokens": ["Aus\u00b7ge\u00b7trun\u00b7ken", "mu\u00df", "er", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Valleri, vallera,", "tokens": ["Val\u00b7le\u00b7ri", ",", "val\u00b7le\u00b7ra", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und juchheirassa,", "tokens": ["Und", "juch\u00b7hei\u00b7ras\u00b7sa", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bester Schatz, bester Schatz,", "tokens": ["Bes\u00b7ter", "Schatz", ",", "bes\u00b7ter", "Schatz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Denn du wei\u00dft es wei\u00dft es ja.", "tokens": ["Denn", "du", "wei\u00dft", "es", "wei\u00dft", "es", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Bracken und die bellen,", "tokens": ["Und", "die", "Bra\u00b7cken", "und", "die", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und die B\u00fcchse und die knallt,", "tokens": ["Und", "die", "B\u00fcch\u00b7se", "und", "die", "knallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rote Hirsche woll'n wir jagen", "tokens": ["Ro\u00b7te", "Hir\u00b7sche", "woll'n", "wir", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem gr\u00fcnen, gr\u00fcnen Wald;", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Valleri, vallera,", "tokens": ["Val\u00b7le\u00b7ri", ",", "val\u00b7le\u00b7ra", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und juchheirassa,", "tokens": ["Und", "juch\u00b7hei\u00b7ras\u00b7sa", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bester Schatz, bester Schatz,", "tokens": ["Bes\u00b7ter", "Schatz", ",", "bes\u00b7ter", "Schatz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Denn du wei\u00dft es wei\u00dft es ja.", "tokens": ["Denn", "du", "wei\u00dft", "es", "wei\u00dft", "es", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ei du H\u00fcbsche, ei du Feine,", "tokens": ["Ei", "du", "H\u00fcb\u00b7sche", ",", "ei", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ei du Bild, wie Milch und Blut,", "tokens": ["Ei", "du", "Bild", ",", "wie", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns're Herzen woll'n wir tauschen,", "tokens": ["Un\u00b7s'\u00b7re", "Her\u00b7zen", "woll'n", "wir", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Denn du glaubst nicht, wie das tut;", "tokens": ["Denn", "du", "glaubst", "nicht", ",", "wie", "das", "tut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Valleri, vallera,", "tokens": ["Val\u00b7le\u00b7ri", ",", "val\u00b7le\u00b7ra", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und juchheirassa,", "tokens": ["Und", "juch\u00b7hei\u00b7ras\u00b7sa", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bester Schatz, bester Schatz,", "tokens": ["Bes\u00b7ter", "Schatz", ",", "bes\u00b7ter", "Schatz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Denn du wei\u00dft es wei\u00dft es ja.", "tokens": ["Denn", "du", "wei\u00dft", "es", "wei\u00dft", "es", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}