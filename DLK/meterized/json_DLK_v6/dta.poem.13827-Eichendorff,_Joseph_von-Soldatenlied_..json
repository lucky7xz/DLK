{"dta.poem.13827": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was zieht da f\u00fcr schreckliches Sausen,", "tokens": ["Was", "zieht", "da", "f\u00fcr", "schreck\u00b7li\u00b7ches", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie Pfeifen durch Sturmes Wehn?", "tokens": ["Wie", "Pfei\u00b7fen", "durch", "Stur\u00b7mes", "Wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das wendet das Herz recht vor Grausen,", "tokens": ["Das", "wen\u00b7det", "das", "Herz", "recht", "vor", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als sollte die Welt vergeh'n.", "tokens": ["Als", "soll\u00b7te", "die", "Welt", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}