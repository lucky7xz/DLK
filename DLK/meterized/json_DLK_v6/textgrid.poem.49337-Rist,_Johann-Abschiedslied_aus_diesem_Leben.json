{"textgrid.poem.49337": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedslied aus diesem Leben", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, Welt, du must zur\u00fccke stehn", "tokens": ["Nun", ",", "Welt", ",", "du", "must", "zu\u00b7r\u00fc\u00b7cke", "stehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allen deinen Sch\u00e4tzen;", "tokens": ["Mit", "al\u00b7len", "dei\u00b7nen", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Freuden wil ich schlafen gehn,", "tokens": ["Mit", "Freu\u00b7den", "wil", "ich", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Leichnam sol man setzen", "tokens": ["Den", "Leich\u00b7nam", "sol", "man", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ins Grab hinein, da keine Pein", "tokens": ["Ins", "Grab", "hin\u00b7ein", ",", "da", "kei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinf\u00fcr' ihn wird verletzen.", "tokens": ["Hin\u00b7f\u00fcr'", "ihn", "wird", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Seelichen fleugt himmelan,", "tokens": ["Mein", "See\u00b7li\u00b7chen", "fleugt", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Leib schl\u00e4ft in der Erden,", "tokens": ["Der", "Leib", "schl\u00e4ft", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Bis da\u00df er mit der Seelen kan", "tokens": ["Bis", "da\u00df", "er", "mit", "der", "See\u00b7len", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wiedr\u00fcm verkn\u00fcpfet werden;", "tokens": ["Wie\u00b7dr\u00fcm", "ver\u00b7kn\u00fcp\u00b7fet", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Inmittelst sol er ruhen wol", "tokens": ["In\u00b7mit\u00b7telst", "sol", "er", "ru\u00b7hen", "wol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ohn' einige Beschwerden.", "tokens": ["Ohn'", "ei\u00b7ni\u00b7ge", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O was f\u00fcr Reichtum werd' ich doch", "tokens": ["O", "was", "f\u00fcr", "Reich\u00b7tum", "werd'", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "NE", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In jenner Welt besitzen!", "tokens": ["In", "jen\u00b7ner", "Welt", "be\u00b7sit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Hinf\u00fcro wird des Kreuzes Joch", "tokens": ["Hin\u00b7f\u00fc\u00b7ro", "wird", "des", "Kreu\u00b7zes", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Mich nimmermehr erhitzen;", "tokens": ["Mich", "nim\u00b7mer\u00b7mehr", "er\u00b7hit\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Es wird die S\u00fcnd ein Gottes Kind", "tokens": ["Es", "wird", "die", "S\u00fcnd", "ein", "Got\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nicht k\u00f6nnen mehr beschmitzen.", "tokens": ["Nicht", "k\u00f6n\u00b7nen", "mehr", "be\u00b7schmit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "O was f\u00fcr Ehr' und Herlichkeit", "tokens": ["O", "was", "f\u00fcr", "Ehr'", "und", "Her\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Wird mir daselbst gegeben!", "tokens": ["Wird", "mir", "da\u00b7selbst", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wie lieblich werd' ich nach der Zeit", "tokens": ["Wie", "lieb\u00b7lich", "werd'", "ich", "nach", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Im Hause Gottes leben!", "tokens": ["Im", "Hau\u00b7se", "Got\u00b7tes", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "In welchem Glanz werd' ich doch ganz", "tokens": ["In", "wel\u00b7chem", "Glanz", "werd'", "ich", "doch", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Verkleidet ewig schweben!", "tokens": ["Ver\u00b7klei\u00b7det", "e\u00b7wig", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Wie gro\u00df wird sein der Liebe Macht", "tokens": ["Wie", "gro\u00df", "wird", "sein", "der", "Lie\u00b7be", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ohn' einiges Betriegen!", "tokens": ["Ohn'", "ei\u00b7ni\u00b7ges", "Be\u00b7trie\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Wie herlich meiner Glieder Pracht,", "tokens": ["Wie", "her\u00b7lich", "mei\u00b7ner", "Glie\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So durch die Wolken fliegen!", "tokens": ["So", "durch", "die", "Wol\u00b7ken", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "H\u00e4tt' ich nur schon die Freudenkron'", "tokens": ["H\u00e4tt'", "ich", "nur", "schon", "die", "Freu\u00b7den\u00b7kron'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "In Gottes Sal erstiegen!", "tokens": ["In", "Got\u00b7tes", "Sal", "er\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wie gro\u00df wird dort die Wollust sein,", "tokens": ["Wie", "gro\u00df", "wird", "dort", "die", "Wol\u00b7lust", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die gar nicht Eitles heget!", "tokens": ["Die", "gar", "nicht", "Eit\u00b7les", "he\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Ein Himmelskind bleibt allzeit rein,", "tokens": ["Ein", "Him\u00b7mel\u00b7skind", "bleibt", "all\u00b7zeit", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sein Herz wird nie beweget", "tokens": ["Sein", "Herz", "wird", "nie", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Von Ha\u00df und Neid; auch alles Leid", "tokens": ["Von", "Ha\u00df", "und", "Neid", ";", "auch", "al\u00b7les", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wird dort rein abgeleget.", "tokens": ["Wird", "dort", "rein", "ab\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Wie werd' ich auch der Jugendkraft", "tokens": ["Wie", "werd'", "ich", "auch", "der", "Ju\u00b7gend\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So trefflich wol empfinden!", "tokens": ["So", "treff\u00b7lich", "wol", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Es wird ein s\u00fc\u00dfer Lebenssaft", "tokens": ["Es", "wird", "ein", "s\u00fc\u00b7\u00dfer", "Le\u00b7bens\u00b7saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Von Neuem mich verbinden,", "tokens": ["Von", "Neu\u00b7em", "mich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "So da\u00df noch Not, noch Schmerz, noch Tod", "tokens": ["So", "da\u00df", "noch", "Not", ",", "noch", "Schmerz", ",", "noch", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Mein Herz kan \u00fcberwinden.", "tokens": ["Mein", "Herz", "kan", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Wie werd' ich k\u00fcnftig sein so klug,", "tokens": ["Wie", "werd'", "ich", "k\u00fcnf\u00b7tig", "sein", "so", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VAINF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Wenn ich mag Christum sehen", "tokens": ["Wenn", "ich", "mag", "Chris\u00b7tum", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "NE", "VVINF"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Und alle Sachen kan genug", "tokens": ["Und", "al\u00b7le", "Sa\u00b7chen", "kan", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Dem Grunde nach verstehen!", "tokens": ["Dem", "Grun\u00b7de", "nach", "ver\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Wie wol wird mir denn f\u00fcr und f\u00fcr", "tokens": ["Wie", "wol", "wird", "mir", "denn", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "In Gottes Reich geschehen!", "tokens": ["In", "Got\u00b7tes", "Reich", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Wie trefflich wird der Freiheit Schatz", "tokens": ["Wie", "treff\u00b7lich", "wird", "der", "Frei\u00b7heit", "Schatz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Nach dieser Knechtschaft prangen!", "tokens": ["Nach", "die\u00b7ser", "Knecht\u00b7schaft", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Drum trag ich auch nach diesem Platz'", "tokens": ["Drum", "trag", "ich", "auch", "nach", "die\u00b7sem", "Platz'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ein sehnliches Verlangen,", "tokens": ["Ein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Ach w\u00e4r' ich nur des Lebens Uhr", "tokens": ["Ach", "w\u00e4r'", "ich", "nur", "des", "Le\u00b7bens", "Uhr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Einst v\u00f6llig durchgegangen!", "tokens": ["Einst", "v\u00f6l\u00b7lig", "durch\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Wie wird mir dort die werte Schar", "tokens": ["Wie", "wird", "mir", "dort", "die", "wer\u00b7te", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Der Engel und der Frommen", "tokens": ["Der", "En\u00b7gel", "und", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Mein Herz ergetzen immerdar,", "tokens": ["Mein", "Herz", "er\u00b7get\u00b7zen", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Wenn ich bin aufgenommen!", "tokens": ["Wenn", "ich", "bin", "auf\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "M\u00f6cht' ich nur bald, mein Aufenthalt,", "tokens": ["M\u00f6cht'", "ich", "nur", "bald", ",", "mein", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Herr Jesu, zu dir kommen!", "tokens": ["Herr", "Je\u00b7su", ",", "zu", "dir", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.61": {"text": "Mein Gott, wie werd' ich jauchzen dort,", "tokens": ["Mein", "Gott", ",", "wie", "werd'", "ich", "jauch\u00b7zen", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Wie werd' ich mich erquicken,", "tokens": ["Wie", "werd'", "ich", "mich", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Wenn ich an deinem sch\u00f6nsten Ort", "tokens": ["Wenn", "ich", "an", "dei\u00b7nem", "sch\u00f6ns\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Dich selber werd' erblicken!", "tokens": ["Dich", "sel\u00b7ber", "werd'", "er\u00b7bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Ich wil mit Lust an meine Brust", "tokens": ["Ich", "wil", "mit", "Lust", "an", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Dich, o mein Heiland, dr\u00fccken.", "tokens": ["Dich", ",", "o", "mein", "Hei\u00b7land", ",", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Ich wil nach dieser kurzen Zeit", "tokens": ["Ich", "wil", "nach", "die\u00b7ser", "kur\u00b7zen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Dich unaufh\u00f6rlich preisen,", "tokens": ["Dich", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.69": {"text": "Du heilige Dreifaltigkeit,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Drei\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.70": {"text": "Und deinen Knecht mich weisen;", "tokens": ["Und", "dei\u00b7nen", "Knecht", "mich", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Du wirst ja mich auch ewiglich", "tokens": ["Du", "wirst", "ja", "mich", "auch", "e\u00b7wig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Mit Freud' und Wonne speisen.", "tokens": ["Mit", "Freud'", "und", "Won\u00b7ne", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.73": {"text": "Hinfort, o Welt, kenn' ich dich nicht,", "tokens": ["Hin\u00b7fort", ",", "o", "Welt", ",", "kenn'", "ich", "dich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.74": {"text": "Ich wei\u00df ein ander Leben:", "tokens": ["Ich", "wei\u00df", "ein", "an\u00b7der", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.75": {"text": "Dem Himmel wil ich meine Pflicht", "tokens": ["Dem", "Him\u00b7mel", "wil", "ich", "mei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.76": {"text": "Nun ganz f\u00fcr eigen geben,", "tokens": ["Nun", "ganz", "f\u00fcr", "ei\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.77": {"text": "Der wird geschwind mich armes Kind", "tokens": ["Der", "wird", "ge\u00b7schwind", "mich", "ar\u00b7mes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.78": {"text": "Zur Herlichkeit erheben.", "tokens": ["Zur", "Her\u00b7lich\u00b7keit", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.79": {"text": "Kom denn, o hocherw\u00fcnschter Tag,", "tokens": ["Kom", "denn", ",", "o", "ho\u00b7cher\u00b7w\u00fcnschter", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.80": {"text": "Mich herzlich zu befreien,", "tokens": ["Mich", "herz\u00b7lich", "zu", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.81": {"text": "Kom, liebstes St\u00fcndlein, das mich mag", "tokens": ["Kom", ",", "liebs\u00b7tes", "St\u00fcnd\u00b7lein", ",", "das", "mich", "mag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.82": {"text": "Zum Himmelsf\u00fcrsten weihen.", "tokens": ["Zum", "Him\u00b7mels\u00b7f\u00fcrs\u00b7ten", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.83": {"text": "Kom bald heran, damit ich kan", "tokens": ["Kom", "bald", "he\u00b7ran", ",", "da\u00b7mit", "ich", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.84": {"text": "Dein ewigs Lob ausschreien.", "tokens": ["Dein", "e\u00b7wigs", "Lob", "aus\u00b7schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}