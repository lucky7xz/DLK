{"dta.poem.18332": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die liedervolle Maienzeit", "tokens": ["Die", "lie\u00b7der\u00b7vol\u00b7le", "Mai\u00b7en\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gaukelnd schwebt um mich der Falter,", "tokens": ["Und", "gau\u00b7kelnd", "schwebt", "um", "mich", "der", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Sinnbild der Unsterblichkeit.", "tokens": ["Das", "Sinn\u00b7bild", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Drum lebt mir wohl, ihr Pergamente", "tokens": ["Drum", "lebt", "mir", "wohl", ",", "ihr", "Per\u00b7ga\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der winterlichen Hirntortur,", "tokens": ["Der", "win\u00b7ter\u00b7li\u00b7chen", "Hirn\u00b7tor\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich lockt ins Reich der Elemente", "tokens": ["Mich", "lockt", "ins", "Reich", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die neuerstandne Lenznatur.", "tokens": ["Die", "neu\u00b7er\u00b7stand\u00b7ne", "Lenz\u00b7na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umspielt von silberbleichem Lichte,", "tokens": ["Um\u00b7spielt", "von", "sil\u00b7ber\u00b7blei\u00b7chem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Grabfeld nach verlorner Schlacht,", "tokens": ["Ein", "Grab\u00b7feld", "nach", "ver\u00b7lor\u00b7ner", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Todtentanz ist die Geschichte,", "tokens": ["Ein", "Tod\u00b7ten\u00b7tanz", "ist", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Todtentanz um Mitternacht.", "tokens": ["Ein", "Tod\u00b7ten\u00b7tanz", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es bleibt der Ruhm, wie er auch gl\u00e4nze,", "tokens": ["Es", "bleibt", "der", "Ruhm", ",", "wie", "er", "auch", "gl\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Blendwerk nur, ein eitler Schein;", "tokens": ["Ein", "Blend\u00b7werk", "nur", ",", "ein", "eit\u00b7ler", "Schein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr gilt als tausend welke Kr\u00e4nze", "tokens": ["Mehr", "gilt", "als", "tau\u00b7send", "wel\u00b7ke", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOKOM", "CARD", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir dieses Lebens goldnes Sein!", "tokens": ["Mir", "die\u00b7ses", "Le\u00b7bens", "gold\u00b7nes", "Sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}