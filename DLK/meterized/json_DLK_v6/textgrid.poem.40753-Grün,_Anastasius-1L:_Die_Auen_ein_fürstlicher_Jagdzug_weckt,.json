{"textgrid.poem.40753": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Auen ein f\u00fcrstlicher Jagdzug weckt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Auen ein f\u00fcrstlicher Jagdzug weckt,", "tokens": ["Die", "Au\u00b7en", "ein", "f\u00fcrst\u00b7li\u00b7cher", "Jagd\u00b7zug", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Inmitten die kaiserlich' Majest\u00e4t;", "tokens": ["In\u00b7mit\u00b7ten", "die", "kai\u00b7ser\u00b7lich'", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die B\u00e4ume sich neigen, doch nicht aus Respekt,", "tokens": ["Die", "B\u00e4u\u00b7me", "sich", "nei\u00b7gen", ",", "doch", "nicht", "aus", "Res\u00b7pekt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es beugt sie der Wind, der die Wipfel verdreht;", "tokens": ["Es", "beugt", "sie", "der", "Wind", ",", "der", "die", "Wip\u00b7fel", "ver\u00b7dreht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Himmel, unartig, schickt b\u00f6ses Wetter,", "tokens": ["Der", "Him\u00b7mel", ",", "un\u00b7ar\u00b7tig", ",", "schickt", "b\u00f6\u00b7ses", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Schwer fallen die Tropfen, hinwirbeln die Bl\u00e4tter;", "tokens": ["Schwer", "fal\u00b7len", "die", "Trop\u00b7fen", ",", "hin\u00b7wir\u00b7beln", "die", "Bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da ruft der Durchlauchtigst' auf seinem Gaul:", "tokens": ["Da", "ruft", "der", "Durch\u00b7lauch\u00b7tigst'", "auf", "sei\u00b7nem", "Gaul", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbah, schaut's, jetzt regnet's mir gar ins Maul!\u00ab", "tokens": ["\u00bb", "ah", ",", "schaut's", ",", "jetzt", "reg\u00b7net's", "mir", "gar", "ins", "Maul", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Inde\u00df die ", "tokens": ["In\u00b7de\u00df", "die"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Verbreiten im Zug, la\u00dft ein Monument", "tokens": ["Ver\u00b7brei\u00b7ten", "im", "Zug", ",", "la\u00dft", "ein", "Mo\u00b7nu\u00b7ment"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VVIMP", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Aus jener Zeit, sein Bild, uns beschaun;", "tokens": ["Aus", "je\u00b7ner", "Zeit", ",", "sein", "Bild", ",", "uns", "be\u00b7schaun", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich trag's in der Tasche, Siebzehner man's nennt.", "tokens": ["Ich", "trag's", "in", "der", "Ta\u00b7sche", ",", "Sieb\u00b7zeh\u00b7ner", "man's", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "PIS", "VVFIN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein Lorberkranz in Perr\u00fcckenwildni\u00df", "tokens": ["Ein", "Lor\u00b7ber\u00b7kranz", "in", "Per\u00b7r\u00fc\u00b7cken\u00b7wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und eine Lippe, sonst nichts! \u2013 so sein Bildni\u00df,", "tokens": ["Und", "ei\u00b7ne", "Lip\u00b7pe", ",", "sonst", "nichts", "!", "\u2013", "so", "sein", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "PIS", "$.", "$(", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Draus m\u00e4nniglich sieht, wie dem frommen Mann", "tokens": ["Draus", "m\u00e4n\u00b7nig\u00b7lich", "sieht", ",", "wie", "dem", "from\u00b7men", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Gar leicht in den Mund das Wasser rann.", "tokens": ["Gar", "leicht", "in", "den", "Mund", "das", "Was\u00b7ser", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ihr Hoflakaien, nun rennt und sprengt:", "tokens": ["Ihr", "Ho\u00b7fla\u00b7kai\u00b7en", ",", "nun", "rennt", "und", "sprengt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Regenschirm ist's, was retten kann!", "tokens": ["Ein", "Re\u00b7gen\u00b7schirm", "ist's", ",", "was", "ret\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hofmarschall beschlie\u00dft ganz still: Der Mann,", "tokens": ["Hof\u00b7mar\u00b7schall", "be\u00b7schlie\u00dft", "ganz", "still", ":", "Der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der des Kaisers Hut gemacht, der h\u00e4ngt!", "tokens": ["Der", "des", "Kai\u00b7sers", "Hut", "ge\u00b7macht", ",", "der", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Hofmedicus denkt: Nach dem Ebenma\u00dfe", "tokens": ["Hof\u00b7me\u00b7di\u00b7cus", "denkt", ":", "Nach", "dem", "E\u00b7ben\u00b7ma\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Wohnt friedlich der Mund im Schatten der Nase,", "tokens": ["Wohnt", "fried\u00b7lich", "der", "Mund", "im", "Schat\u00b7ten", "der", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durchlauchtigste Nase verschm\u00e4ht das System;", "tokens": ["Durch\u00b7lauch\u00b7tigs\u00b7te", "Na\u00b7se", "ver\u00b7schm\u00e4ht", "das", "Sys\u00b7tem", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie stell' ich nun dieses der Nase genehm?", "tokens": ["Wie", "stell'", "ich", "nun", "die\u00b7ses", "der", "Na\u00b7se", "ge\u00b7nehm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PDAT", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n tr\u00f6stet den Kaiser der Hofjesuit:", "tokens": ["Sch\u00f6n", "tr\u00f6s\u00b7tet", "den", "Kai\u00b7ser", "der", "Hof\u00b7je\u00b7su\u00b7it", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbder Priester dir Weihbronn entgegenh\u00e4lt,", "tokens": ["\u00bb", "der", "Pries\u00b7ter", "dir", "Weih\u00b7bronn", "ent\u00b7ge\u00b7gen\u00b7h\u00e4lt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn die Majest\u00e4t in die Kirche tritt;", "tokens": ["Wenn", "die", "Ma\u00b7jes\u00b7t\u00e4t", "in", "die", "Kir\u00b7che", "tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ein Dom des Herrn ist Wald und Feld,", "tokens": ["Ein", "Dom", "des", "Herrn", "ist", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott selber hat hier den Weihbrunn ergo\u00dfen", "tokens": ["Gott", "sel\u00b7ber", "hat", "hier", "den", "Weih\u00b7brunn", "er\u00b7go\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zu gr\u00fc\u00dfen dich, den Frommen, den Gro\u00dfen!\u00ab", "tokens": ["Zu", "gr\u00fc\u00b7\u00dfen", "dich", ",", "den", "From\u00b7men", ",", "den", "Gro\u00b7\u00dfen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der Kaiser wird grimmig, wie K\u00f6nig Saul:", "tokens": ["Der", "Kai\u00b7ser", "wird", "grim\u00b7mig", ",", "wie", "K\u00f6\u00b7nig", "Saul", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "\u00bbzum Teufel! mir regnet's noch immer ins Maul!\u00ab", "tokens": ["\u00bb", "zum", "Teu\u00b7fel", "!", "mir", "reg\u00b7net's", "noch", "im\u00b7mer", "ins", "Maul", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Der Eine erstarrte, der Andere lief,", "tokens": ["Der", "Ei\u00b7ne", "er\u00b7starr\u00b7te", ",", "der", "An\u00b7de\u00b7re", "lief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der rang die H\u00e4nde, der stand wie im Bann;", "tokens": ["Der", "rang", "die", "H\u00e4n\u00b7de", ",", "der", "stand", "wie", "im", "Bann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Eichbaum lehnt' in Gedanken tief", "tokens": ["Am", "Eich\u00b7baum", "lehnt'", "in", "Ge\u00b7dan\u00b7ken", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der G\u00fcnstling des Herrn und sann und sann;", "tokens": ["Der", "G\u00fcnst\u00b7ling", "des", "Herrn", "und", "sann", "und", "sann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf springt er jetzt, heiliger Sendung trunken,", "tokens": ["Auf", "springt", "er", "jetzt", ",", "hei\u00b7li\u00b7ger", "Sen\u00b7dung", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Stirn ihm umspr\u00fchn der Erleuchtung Funken:", "tokens": ["Die", "Stirn", "ihm", "um\u00b7spr\u00fchn", "der", "Er\u00b7leuch\u00b7tung", "Fun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "\u00bbmein allergro\u00dfm\u00e4chtigster Kaiser geruh'", "tokens": ["\u00bb", "mein", "al\u00b7ler\u00b7gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "Kai\u00b7ser", "ge\u00b7ruh'"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und schlie\u00dfe die Lippen huldreichst zu!\u00ab", "tokens": ["Und", "schlie\u00b7\u00dfe", "die", "Lip\u00b7pen", "huld\u00b7reichst", "zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Lobsinge, du heiliges r\u00f6misches Reich!", "tokens": ["Lob\u00b7sin\u00b7ge", ",", "du", "hei\u00b7li\u00b7ges", "r\u00f6\u00b7mi\u00b7sches", "Reich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie leicht du zu schirmen, zu retten bist!", "tokens": ["Wie", "leicht", "du", "zu", "schir\u00b7men", ",", "zu", "ret\u00b7ten", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gescho\u00df der Karthaunen und Schwerterstreich", "tokens": ["Ge\u00b7scho\u00df", "der", "Kar\u00b7thau\u00b7nen", "und", "Schwer\u00b7ter\u00b7streich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Trifft nicht wie ein W\u00f6rtlein zu rechter Frist;", "tokens": ["Trifft", "nicht", "wie", "ein", "W\u00f6rt\u00b7lein", "zu", "rech\u00b7ter", "Frist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Send' immer dir's Gott zur rechten Stunde,", "tokens": ["Send'", "im\u00b7mer", "dir's", "Gott", "zur", "rech\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und F\u00fcrsten, die horchen dem rechten Munde", "tokens": ["Und", "F\u00fcrs\u00b7ten", ",", "die", "hor\u00b7chen", "dem", "rech\u00b7ten", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", 