{"textgrid.poem.48370": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Melrose-Abbey", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und willst du des Zaubers sicher sein,", "tokens": ["Und", "willst", "du", "des", "Zau\u00b7bers", "si\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So besuche Melros' bei Mondenschein;", "tokens": ["So", "be\u00b7su\u00b7che", "Mel\u00b7ros'", "bei", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die goldne Sonne, des Tages Licht,", "tokens": ["Die", "gold\u00b7ne", "Son\u00b7ne", ",", "des", "Ta\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie passen zu seinen Tr\u00fcmmern nicht.", "tokens": ["Sie", "pas\u00b7sen", "zu", "sei\u00b7nen", "Tr\u00fcm\u00b7mern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn die B\u00f6gen und Nischen im Schatten stehn,", "tokens": ["Wenn", "die", "B\u00f6\u00b7gen", "und", "Ni\u00b7schen", "im", "Schat\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Die Ecken und Pfeiler wie Silber sehn,", "tokens": ["Die", "E\u00b7cken", "und", "Pfei\u00b7ler", "wie", "Sil\u00b7ber", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wenn das wei\u00dfe, kalte, zitternde Licht", "tokens": ["Wenn", "das", "wei\u00b7\u00dfe", ",", "kal\u00b7te", ",", "zit\u00b7tern\u00b7de", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Um den Mittelturm seine Girlanden flicht,", "tokens": ["Um", "den", "Mit\u00b7tel\u00b7turm", "sei\u00b7ne", "Gir\u00b7lan\u00b7den", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+---+-+", "measure": "anapaest.init"}, "line.9": {"text": "Wenn die Strebepfeiler sich wechselnd reihn,", "tokens": ["Wenn", "die", "Stre\u00b7be\u00b7pfei\u00b7ler", "sich", "wech\u00b7selnd", "reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Halb Ebenholz, halb Elfenbein,", "tokens": ["Halb", "E\u00b7ben\u00b7holz", ",", "halb", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn's schneeig auf allen Gr\u00e4bern liegt", "tokens": ["Wenn's", "schne\u00b7e\u00b7ig", "auf", "al\u00b7len", "Gr\u00e4\u00b7bern", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und die wei\u00dfen Figuren noch wei\u00dfer umschmiegt,", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Fi\u00b7gu\u00b7ren", "noch", "wei\u00b7\u00dfer", "um\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Wenn das Rauschen des Tweed, weitab geh\u00f6rt,", "tokens": ["Wenn", "das", "Rau\u00b7schen", "des", "Tweed", ",", "weit\u00b7ab", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NE", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Wie Summen die n\u00e4chtige Stille st\u00f6rt \u2013", "tokens": ["Wie", "Sum\u00b7men", "die", "n\u00e4ch\u00b7ti\u00b7ge", "Stil\u00b7le", "st\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Ja, dann tritt ein: bei ", "tokens": ["Ja", ",", "dann", "tritt", "ein", ":", "bei"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PTKVZ", "$.", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Besuche Melros' und \u2013 ", "tokens": ["Be\u00b7su\u00b7che", "Mel\u00b7ros'", "und", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}