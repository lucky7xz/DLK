{"textgrid.poem.57194": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[bedenke, Freund, was wir zusammen sprachen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bedenke, Freund, was wir zusammen sprachen.", "tokens": ["Be\u00b7den\u00b7ke", ",", "Freund", ",", "was", "wir", "zu\u00b7sam\u00b7men", "spra\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War's wert, da\u00df wir den Bann des Schweigens brachen,", "tokens": ["Wa\u00b7r's", "wert", ",", "da\u00df", "wir", "den", "Bann", "des", "Schwei\u00b7gens", "bra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "um solche Nichtigkeiten auszutauschen?", "tokens": ["um", "sol\u00b7che", "Nich\u00b7tig\u00b7kei\u00b7ten", "aus\u00b7zu\u00b7tau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So schw\u00e4tzen wohl zwei V\u00f6gel miteinander,", "tokens": ["So", "schw\u00e4t\u00b7zen", "wohl", "zwei", "V\u00f6\u00b7gel", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "derweil in unabl\u00e4ssigem Gewander", "tokens": ["der\u00b7weil", "in", "un\u00b7ab\u00b7l\u00e4s\u00b7si\u00b7gem", "Ge\u00b7wan\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "des Stromes strenge Wogen meerw\u00e4rts rauschen.", "tokens": ["des", "Stro\u00b7mes", "stren\u00b7ge", "Wo\u00b7gen", "meer\u00b7w\u00e4rts", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erwacht in dir nicht ein Gef\u00fchl der Leere,", "tokens": ["Er\u00b7wacht", "in", "dir", "nicht", "ein", "Ge\u00b7f\u00fchl", "der", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "erw\u00e4gst du, wie so auftut Jahre, \u2013 Jahre", "tokens": ["er\u00b7w\u00e4gst", "du", ",", "wie", "so", "auf\u00b7tut", "Jah\u00b7re", ",", "\u2013", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "NN", "$,", "$(", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nichts als Geschw\u00e4tz aus dir sich und dem andern,", "tokens": ["nichts", "als", "Ge\u00b7schw\u00e4tz", "aus", "dir", "sich", "und", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "APPR", "PPER", "PRF", "KON", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "indessen nach der Gottheit Scho\u00df und Meere", "tokens": ["in\u00b7des\u00b7sen", "nach", "der", "Got\u00b7theit", "Scho\u00df", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Geistesweisheit sternenspiegelklare", "tokens": ["der", "Geis\u00b7tes\u00b7weis\u00b7heit", "ster\u00b7nen\u00b7spie\u00b7gel\u00b7kla\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gew\u00e4sser ruhlos und gewaltig wandern?", "tokens": ["Ge\u00b7w\u00e4s\u00b7ser", "ruh\u00b7los", "und", "ge\u00b7wal\u00b7tig", "wan\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}