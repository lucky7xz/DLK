{"textgrid.poem.66350": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da liegt, ach Gott! da drunten liegt", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt, ach Gott! da drunten liegt", "tokens": ["Da", "liegt", ",", "ach", "Gott", "!", "da", "drun\u00b7ten", "liegt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "XY", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die z\u00e4rtlichste der M\u00fctter!", "tokens": ["Die", "z\u00e4rt\u00b7lichs\u00b7te", "der", "M\u00fct\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich, ans Todtenkreuz geschmiegt,", "tokens": ["Und", "ich", ",", "ans", "Tod\u00b7ten\u00b7kreuz", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Starr' hier und klage bitter.", "tokens": ["Starr'", "hier", "und", "kla\u00b7ge", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Mutter, Mutter, die du mich", "tokens": ["O", "Mut\u00b7ter", ",", "Mut\u00b7ter", ",", "die", "du", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Herzen hast getragen,", "tokens": ["Am", "Her\u00b7zen", "hast", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo bist du? gute Mutter, sprich,", "tokens": ["Wo", "bist", "du", "?", "gu\u00b7te", "Mut\u00b7ter", ",", "sprich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rst du auch meine Klagen?", "tokens": ["H\u00f6rst", "du", "auch", "mei\u00b7ne", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6r doch, dein Minchen jammert hier!", "tokens": ["H\u00f6r", "doch", ",", "dein", "Min\u00b7chen", "jam\u00b7mert", "hier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umfa\u00dft dein Grab mit Thr\u00e4nen;", "tokens": ["Um\u00b7fa\u00dft", "dein", "Grab", "mit", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O solltest du dich nicht nach mir,", "tokens": ["O", "soll\u00b7test", "du", "dich", "nicht", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinem Minchen sehnen?", "tokens": ["Nach", "dei\u00b7nem", "Min\u00b7chen", "seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schau, wie ich in die Welt hinein", "tokens": ["Schau", ",", "wie", "ich", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So ganz verlassen walle;", "tokens": ["So", "ganz", "ver\u00b7las\u00b7sen", "wal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In diesem kleinen B\u00fcndelein", "tokens": ["In", "die\u00b7sem", "klei\u00b7nen", "B\u00fcn\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind meine G\u00fcter alle.", "tokens": ["Sind", "mei\u00b7ne", "G\u00fc\u00b7ter", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdich segne Gott, dich segne Gott,\u00ab", "tokens": ["\u00bb", "dich", "seg\u00b7ne", "Gott", ",", "dich", "seg\u00b7ne", "Gott", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprachst du mit leisem Stammeln;", "tokens": ["Sprachst", "du", "mit", "lei\u00b7sem", "Stam\u00b7meln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbvertrau nur ihm, dich wird der Tod", "tokens": ["\u00bb", "ver\u00b7trau", "nur", "ihm", ",", "dich", "wird", "der", "Tod"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "PPER", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wieder zu mir sammeln.", "tokens": ["Bald", "wie\u00b7der", "zu", "mir", "sam\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nimm diese Bibel, oft hab' ich", "tokens": ["Nimm", "die\u00b7se", "Bi\u00b7bel", ",", "oft", "hab'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Thr\u00e4nen sie benetzet;", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "sie", "be\u00b7net\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! \u00fcber alles hab' ich dich,", "tokens": ["Ach", "!", "\u00fc\u00b7ber", "al\u00b7les", "hab'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PIS", "VAFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du theures Buch, gesch\u00e4tzet.", "tokens": ["Du", "theu\u00b7res", "Buch", ",", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sei, liebes Minchen, tugendhaft", "tokens": ["Sei", ",", "lie\u00b7bes", "Min\u00b7chen", ",", "tu\u00b7gend\u00b7haft"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leb' ein Engelleben;", "tokens": ["Und", "leb'", "ein", "En\u00b7gel\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! Jesus Christus wird dir Kraft", "tokens": ["Ach", "!", "Je\u00b7sus", "Chris\u00b7tus", "wird", "dir", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NE", "NE", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu jeder Tugend geben.", "tokens": ["Zu", "je\u00b7der", "Tu\u00b7gend", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, Herzensminchen, schl\u00fcpfrig ist", "tokens": ["O", ",", "Her\u00b7zens\u00b7min\u00b7chen", ",", "schl\u00fcpf\u00b7rig", "ist"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bahn, auf der du wandelst;", "tokens": ["Die", "Bahn", ",", "auf", "der", "du", "wan\u00b7delst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du nicht fromm und redlich bist,", "tokens": ["Wenn", "du", "nicht", "fromm", "und", "red\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gut und edel handelst.", "tokens": ["Und", "gut", "und", "e\u00b7del", "han\u00b7delst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Flieh jede kleine Erdenlust,", "tokens": ["Flieh", "je\u00b7de", "klei\u00b7ne", "Er\u00b7den\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist soll dich umschweben.", "tokens": ["Mein", "Geist", "soll", "dich", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn du eine S\u00fcnde thust,", "tokens": ["Und", "wenn", "du", "ei\u00b7ne", "S\u00fcn\u00b7de", "thust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wirst du heimlich beben.", "tokens": ["So", "wirst", "du", "heim\u00b7lich", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch bist du gut, so wird ein Wind", "tokens": ["Doch", "bist", "du", "gut", ",", "so", "wird", "ein", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinen Locken spielen,", "tokens": ["In", "dei\u00b7nen", "Lo\u00b7cken", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tief im Herzen, wirst du, Kind,", "tokens": ["Und", "tief", "im", "Her\u00b7zen", ",", "wirst", "du", ",", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels S\u00e4useln f\u00fchlen.\u00ab", "tokens": ["Des", "Him\u00b7mels", "S\u00e4u\u00b7seln", "f\u00fch\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So sprachst du, Mutter, sahst auf mich,", "tokens": ["So", "sprachst", "du", ",", "Mut\u00b7ter", ",", "sahst", "auf", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bleich ward deine Wange;", "tokens": ["Und", "bleich", "ward", "dei\u00b7ne", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber bog mich \u00fcber dich", "tokens": ["Ich", "a\u00b7ber", "bog", "mich", "\u00fc\u00b7ber", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schrie und heulte lange.", "tokens": ["Und", "schrie", "und", "heul\u00b7te", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da lieg' ich nun, da lieg' ich nun", "tokens": ["Da", "lieg'", "ich", "nun", ",", "da", "lieg'", "ich", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gestreckt auf deinem Grabe;", "tokens": ["Ge\u00b7streckt", "auf", "dei\u00b7nem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Mutter! neben dir zu ruhn,", "tokens": ["O", "Mut\u00b7ter", "!", "ne\u00b7ben", "dir", "zu", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfleh mir diese Gabe.", "tokens": ["Er\u00b7fleh", "mir", "die\u00b7se", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Doch nein, ich richte mich empor;", "tokens": ["Doch", "nein", ",", "ich", "rich\u00b7te", "mich", "em\u00b7por", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott will ich lassen walten;", "tokens": ["Gott", "will", "ich", "las\u00b7sen", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich die Mutter gleich verlor,", "tokens": ["Wenn", "ich", "die", "Mut\u00b7ter", "gleich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann Er mich doch erhalten.", "tokens": ["Kann", "Er", "mich", "doch", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "So will ich denn mein B\u00fcndelein", "tokens": ["So", "will", "ich", "denn", "mein", "B\u00fcn\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schwachen H\u00e4nden fassen;", "tokens": ["Mit", "schwa\u00b7chen", "H\u00e4n\u00b7den", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kann Gott ein armes Waiselein,", "tokens": ["Kann", "Gott", "ein", "ar\u00b7mes", "Wai\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ihm vertraut, verlassen?", "tokens": ["Das", "ihm", "ver\u00b7traut", ",", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nun liebe Mutter, gute Nacht!", "tokens": ["Nun", "lie\u00b7be", "Mut\u00b7ter", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Geist soll um mich wehen,", "tokens": ["Dein", "Geist", "soll", "um", "mich", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn der und Gottes Auge wacht,", "tokens": ["Wenn", "der", "und", "Got\u00b7tes", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was kann mir Leids geschehen?", "tokens": ["Was", "kann", "mir", "Leids", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Bald kommt vielleicht der liebe Tod,", "tokens": ["Bald", "kommt", "viel\u00b7leicht", "der", "lie\u00b7be", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach heute st\u00fcrb' ich lieber;", "tokens": ["Ach", "heu\u00b7te", "st\u00fcrb'", "ich", "lie\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann bin ich frei von aller Noth", "tokens": ["Dann", "bin", "ich", "frei", "von", "al\u00b7ler", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und flieg zu dir hin\u00fcber.", "tokens": ["Und", "flieg", "zu", "dir", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Bleib' ich nur keusch, und gut, und fromm,", "tokens": ["Bleib'", "ich", "nur", "keusch", ",", "und", "gut", ",", "und", "fromm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wirst du mit der Miene", "tokens": ["So", "wirst", "du", "mit", "der", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Engel sprechen: Tochter, komm,", "tokens": ["Der", "En\u00b7gel", "spre\u00b7chen", ":", "Toch\u00b7ter", ",", "komm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, liebe Wilhelmine!", "tokens": ["Komm", ",", "lie\u00b7be", "Wil\u00b7hel\u00b7mi\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}