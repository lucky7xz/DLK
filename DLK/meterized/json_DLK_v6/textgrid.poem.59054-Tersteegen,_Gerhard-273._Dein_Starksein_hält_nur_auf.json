{"textgrid.poem.59054": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "273. Dein Starksein h\u00e4lt nur auf", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selbstgehn bringt viel M\u00fch und Klagen;", "tokens": ["Selbst\u00b7gehn", "bringt", "viel", "M\u00fch", "und", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So geht's langsam, liebes Kind.", "tokens": ["So", "geht's", "lang\u00b7sam", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dich von der Mutter tragen,", "tokens": ["La\u00df", "dich", "von", "der", "Mut\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann geht's sanft und doch geschwind!", "tokens": ["Dann", "geht's", "sanft", "und", "doch", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}