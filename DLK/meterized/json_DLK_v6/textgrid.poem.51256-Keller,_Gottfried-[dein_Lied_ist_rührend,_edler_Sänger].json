{"textgrid.poem.51256": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "[dein Lied ist r\u00fchrend, edler S\u00e4nger]", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Lied ist r\u00fchrend, edler S\u00e4nger,", "tokens": ["Dein", "Lied", "ist", "r\u00fch\u00b7rend", ",", "ed\u00b7ler", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch z\u00fcrne dem Genossen nicht,", "tokens": ["Doch", "z\u00fcr\u00b7ne", "dem", "Ge\u00b7nos\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird ihm darob das Herz nicht b\u00e4nger,", "tokens": ["Wird", "ihm", "da\u00b7rob", "das", "Herz", "nicht", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das, dir erwidernd, also spricht:", "tokens": ["Das", ",", "dir", "er\u00b7wi\u00b7dernd", ",", "al\u00b7so", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PPER", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Poesie ist angeboren,", "tokens": ["Die", "Poe\u00b7sie", "ist", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sie erkennt kein Dort und Hier;", "tokens": ["Und", "sie", "er\u00b7kennt", "kein", "Dort", "und", "Hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, ging' die Seele mir verloren,", "tokens": ["Ja", ",", "ging'", "die", "See\u00b7le", "mir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchr zur H\u00f6lle selbst mit mir.", "tokens": ["Sie", "f\u00fchr", "zur", "H\u00f6l\u00b7le", "selbst", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Inzwischen sieht's auf dieser Erde", "tokens": ["I\u00b7nzwi\u00b7schen", "sieht's", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch lange nicht so graulich aus,", "tokens": ["Noch", "lan\u00b7ge", "nicht", "so", "grau\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manchmal scheint mir, da\u00df das: Werde!", "tokens": ["Und", "manch\u00b7mal", "scheint", "mir", ",", "da\u00df", "das", ":", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PDS", "$.", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6n' erst recht dem \u00bbDichterhaus\u00ab.", "tokens": ["Er\u00b7t\u00f6n'", "erst", "recht", "dem", "\u00bb", "Dich\u00b7ter\u00b7haus", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "$(", "FM.la", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon schafft der Geist sich Sturmesschwingen", "tokens": ["Schon", "schafft", "der", "Geist", "sich", "Stur\u00b7mes\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spannt Eliaswagen an:", "tokens": ["Und", "spannt", "E\u00b7li\u00b7as\u00b7wa\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst tr\u00e4umend du im Grase singen,", "tokens": ["Willst", "tr\u00e4u\u00b7mend", "du", "im", "Gra\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hindert dich, Poet, daran?", "tokens": ["Wer", "hin\u00b7dert", "dich", ",", "Po\u00b7et", ",", "da\u00b7ran", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich im Sch\u00e4ferkleide,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", "im", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herfahrend, \u2013 doch mein Feuerdrach'", "tokens": ["Her\u00b7fah\u00b7rend", ",", "\u2013", "doch", "mein", "Feu\u00b7er\u00b7drach'"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tr\u00e4gt mich vorbei, die dunkle Heide", "tokens": ["Tr\u00e4gt", "mich", "vor\u00b7bei", ",", "die", "dunk\u00b7le", "Hei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Geister schaun uns nach.", "tokens": ["Und", "dei\u00b7ne", "Geis\u00b7ter", "schaun", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was deine alten Pergamente", "tokens": ["Was", "dei\u00b7ne", "al\u00b7ten", "Per\u00b7ga\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von tollem Zauber kund dir tun,", "tokens": ["Von", "tol\u00b7lem", "Zau\u00b7ber", "kund", "dir", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das seh ich durch die Elemente", "tokens": ["Das", "seh", "ich", "durch", "die", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Geistes Dienst verwirklicht nun.", "tokens": ["In", "Geis\u00b7tes", "Dienst", "ver\u00b7wirk\u00b7licht", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich seh sie keuchend gl\u00fchn und spr\u00fchen,", "tokens": ["Ich", "seh", "sie", "keu\u00b7chend", "gl\u00fchn", "und", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stahlschimmernd bauen Land und Stadt,", "tokens": ["Stahl\u00b7schim\u00b7mernd", "bau\u00b7en", "Land", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes das Menschenkind zu bl\u00fchen", "tokens": ["In\u00b7des", "das", "Men\u00b7schen\u00b7kind", "zu", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen wieder Mu\u00dfe hat.", "tokens": ["Und", "sin\u00b7gen", "wie\u00b7der", "Mu\u00b7\u00dfe", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wenn vielleicht in hundert Jahren", "tokens": ["Und", "wenn", "viel\u00b7leicht", "in", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Luftschiff hoch mit Griechenwein", "tokens": ["Ein", "Luft\u00b7schiff", "hoch", "mit", "Grie\u00b7chen\u00b7wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs Morgenrot k\u00e4m hergefahren \u2013", "tokens": ["Durchs", "Mor\u00b7gen\u00b7rot", "k\u00e4m", "her\u00b7ge\u00b7fah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer m\u00f6chte da nicht F\u00e4hrmann sein?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "da", "nicht", "F\u00e4hr\u00b7mann", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann b\u00f6g ich mich, ein sel'ger Zecher,", "tokens": ["Dann", "b\u00f6g", "ich", "mich", ",", "ein", "sel'\u00b7ger", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber Bord, von Kr\u00e4nzen schwer,", "tokens": ["Wohl", "\u00fc\u00b7ber", "Bord", ",", "von", "Kr\u00e4n\u00b7zen", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und g\u00f6sse langsam meinen Becher", "tokens": ["Und", "g\u00f6s\u00b7se", "lang\u00b7sam", "mei\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab in das verla\u00dfne Meer.", "tokens": ["Hin\u00b7ab", "in", "das", "ver\u00b7la\u00df\u00b7ne", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}