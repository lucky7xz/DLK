{"textgrid.poem.54585": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "[widmung]", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nemt von mir/ werther Freund/ vor offt erzeigte Gunst/", "tokens": ["Nemt", "von", "mir", "/", "wert\u00b7her", "Freund", "/", "vor", "offt", "er\u00b7zeig\u00b7te", "Gunst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$(", "ADJA", "NN", "$(", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht eyteln Rauch der Welt/ nicht au\u00dfgeschm\u00fcnckten Dunst:", "tokens": ["Nicht", "ey\u00b7teln", "Rauch", "der", "Welt", "/", "nicht", "au\u00df\u00b7ge\u00b7schm\u00fcnck\u00b7ten", "Dunst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ART", "NN", "$(", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nem't diesen/ der/ da Er vns mehr nicht konte geben/", "tokens": ["Nem't", "die\u00b7sen", "/", "der", "/", "da", "Er", "vns", "mehr", "nicht", "kon\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$(", "ART", "$(", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich selbst gab in den Todt vor vnser aller Leben.", "tokens": ["Sich", "selbst", "gab", "in", "den", "Todt", "vor", "vn\u00b7ser", "al\u00b7ler", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "