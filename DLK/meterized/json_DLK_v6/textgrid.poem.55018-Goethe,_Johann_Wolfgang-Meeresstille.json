{"textgrid.poem.55018": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Meeresstille", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefe Stille herrscht im Wasser,", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "herrscht", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Regung ruht das Meer,", "tokens": ["Oh\u00b7ne", "Re\u00b7gung", "ruht", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bek\u00fcmmert sieht der Schiffer", "tokens": ["Und", "be\u00b7k\u00fcm\u00b7mert", "sieht", "der", "Schif\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glatte Fl\u00e4che ringsumher.", "tokens": ["Glat\u00b7te", "Fl\u00e4\u00b7che", "rings\u00b7um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Luft von keiner Seite!", "tokens": ["Kei\u00b7ne", "Luft", "von", "kei\u00b7ner", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Todesstille f\u00fcrchterlich!", "tokens": ["To\u00b7des\u00b7stil\u00b7le", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der ungeheuern Weite", "tokens": ["In", "der", "un\u00b7ge\u00b7heu\u00b7ern", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reget keine Welle sich.", "tokens": ["Re\u00b7get", "kei\u00b7ne", "Wel\u00b7le", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}