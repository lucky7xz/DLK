{"textgrid.poem.43811": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein Staar", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Staar ist ein gescheidtes Thier,", "tokens": ["Mein", "Staar", "ist", "ein", "ge\u00b7scheid\u00b7tes", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schwatzet gar zu gern mit mir.", "tokens": ["Er", "schwat\u00b7zet", "gar", "zu", "gern", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum hat er Morgens mich geseh'n,", "tokens": ["Kaum", "hat", "er", "Mor\u00b7gens", "mich", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eh' ich will zur Schule geh'n,", "tokens": ["Noch", "eh'", "ich", "will", "zur", "Schu\u00b7le", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ruft er immerfort mir zu:", "tokens": ["So", "ruft", "er", "im\u00b7mer\u00b7fort", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei flei\u00dfig, flei\u00dfig, du du du!", "tokens": ["Sei", "flei\u00b7\u00dfig", ",", "flei\u00b7\u00dfig", ",", "du", "du", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJD", "$,", "PPER", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und komm' ich aus der Schul' heraus,", "tokens": ["Und", "komm'", "ich", "aus", "der", "Schul'", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eh' ich tret' hinein ins Haus,", "tokens": ["Noch", "eh'", "ich", "tret'", "hin\u00b7ein", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr, das ist doch wunderbar!", "tokens": ["F\u00fcr\u00b7wahr", ",", "das", "ist", "doch", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00fcpft entgegen mir mein Staar,", "tokens": ["So", "h\u00fcpft", "ent\u00b7ge\u00b7gen", "mir", "mein", "Staar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ruft dann immerfort mir zu:", "tokens": ["Und", "ruft", "dann", "im\u00b7mer\u00b7fort", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei flei\u00dfig, flei\u00dfig, du du du!", "tokens": ["Sei", "flei\u00b7\u00dfig", ",", "flei\u00b7\u00dfig", ",", "du", "du", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJD", "$,", "PPER", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So will ich denn recht flei\u00dfig sein,", "tokens": ["So", "will", "ich", "denn", "recht", "flei\u00b7\u00dfig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will Griechisch lernen und Latein,", "tokens": ["Will", "Grie\u00b7chisch", "ler\u00b7nen", "und", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein deutscher t\u00fcchtiger Scholar,", "tokens": ["Ein", "deut\u00b7scher", "t\u00fcch\u00b7ti\u00b7ger", "Scho\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob ich h\u00f6rte meinen Staar", "tokens": ["Als", "ob", "ich", "h\u00f6r\u00b7te", "mei\u00b7nen", "Staar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und er mir t\u00e4glich riefe zu:", "tokens": ["Und", "er", "mir", "t\u00e4g\u00b7lich", "rie\u00b7fe", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei flei\u00dfig, flei\u00dfig, du du du!", "tokens": ["Sei", "flei\u00b7\u00dfig", ",", "flei\u00b7\u00dfig", ",", "du", "du", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJD", "$,", "PPER", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}