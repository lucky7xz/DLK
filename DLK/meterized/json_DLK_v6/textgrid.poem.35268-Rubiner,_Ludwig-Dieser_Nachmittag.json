{"textgrid.poem.35268": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Dieser Nachmittag", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine Orange, wie in dem vornehmen, betteln verboten, E\u00dfwarenverkauf,", "tokens": ["Es", "war", "ei\u00b7ne", "O\u00b7ran\u00b7ge", ",", "wie", "in", "dem", "vor\u00b7neh\u00b7men", ",", "bet\u00b7teln", "ver\u00b7bo\u00b7ten", ",", "E\u00df\u00b7wa\u00b7ren\u00b7ver\u00b7kauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "APPR", "ART", "ADJA", "$,", "ADJD", "VVPP", "$,", "NN", "$,"], "meter": "-+---+-+---+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Es war ein wildes Zehnmarkst\u00fcck wie hinter dem Fenster der Wechselbank,", "tokens": ["Es", "war", "ein", "wil\u00b7des", "Zehn\u00b7mark\u00b7st\u00fcck", "wie", "hin\u00b7ter", "dem", "Fens\u00b7ter", "der", "Wech\u00b7sel\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Ein rotes rundes Glas Bier aus einem Aschingerschank,", "tokens": ["Ein", "ro\u00b7tes", "run\u00b7des", "Glas", "Bier", "aus", "ei\u00b7nem", "A\u00b7schin\u00b7ger\u00b7schank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-++-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "Ein Schinken, ein Mund, Weiberbrust, ein Hut mit 'nem Band, ein Loch, das rot klafft,", "tokens": ["Ein", "Schin\u00b7ken", ",", "ein", "Mund", ",", "Wei\u00b7ber\u00b7brust", ",", "ein", "Hut", "mit", "'nem", "Band", ",", "ein", "Loch", ",", "das", "rot", "klafft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein weiches buntes Kissen. Ein Vogel im K\u00e4fig. Eine Tabakpfeife pafft.", "tokens": ["Ein", "wei\u00b7ches", "bun\u00b7tes", "Kis\u00b7sen", ".", "Ein", "Vo\u00b7gel", "im", "K\u00e4\u00b7fig", ".", "Ei\u00b7ne", "Ta\u00b7bak\u00b7pfei\u00b7fe", "pafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "ART", "NE", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Eine T\u00fcr offen zu 'nem menschenleeren Kleiderladen,", "tokens": ["Ei\u00b7ne", "T\u00fcr", "of\u00b7fen", "zu", "'nem", "men\u00b7schen\u00b7lee\u00b7ren", "Klei\u00b7der\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-++--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ein rotes Boot am lauen Flu\u00df zum Baden.", "tokens": ["Ein", "ro\u00b7tes", "Boot", "am", "lau\u00b7en", "Flu\u00df", "zum", "Ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An diesem Nachmittag sah der arme Mob das Licht.", "tokens": ["An", "die\u00b7sem", "Nach\u00b7mit\u00b7tag", "sah", "der", "ar\u00b7me", "Mob", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es lief vor ihm her. Die anderen sahen es nicht.", "tokens": ["Es", "lief", "vor", "ihm", "her", ".", "Die", "an\u00b7de\u00b7ren", "sa\u00b7hen", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus zerschlissenen Winkeln in den St\u00e4dten der Welt brach g\u00f6ttlicher Glockenschwung.", "tokens": ["Aus", "zer\u00b7schlis\u00b7se\u00b7nen", "Win\u00b7keln", "in", "den", "St\u00e4d\u00b7ten", "der", "Welt", "brach", "g\u00f6tt\u00b7li\u00b7cher", "Glo\u00b7cken\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "O seliges Fliegen: Pustblumen im Rauch, die Stengel gefesselt und kahl,", "tokens": ["O", "se\u00b7li\u00b7ges", "Flie\u00b7gen", ":", "Pust\u00b7blu\u00b7men", "im", "Rauch", ",", "die", "Sten\u00b7gel", "ge\u00b7fes\u00b7selt", "und", "kahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NN", "APPRART", "NN", "$,", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "+---+--+--+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Die zitternden Meere zerlumpten Leibs reckten gedunsene K\u00f6pfe zum himmlischen Strahl.", "tokens": ["Die", "zit\u00b7tern\u00b7den", "Mee\u00b7re", "zer\u00b7lump\u00b7ten", "Leibs", "reck\u00b7ten", "ge\u00b7dun\u00b7se\u00b7ne", "K\u00f6p\u00b7fe", "zum", "himm\u00b7li\u00b7schen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "ADJA", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.5": {"line.1": {"text": "Sie schluchzten faltig und heiser, Riesenstimmen schrien \u00fcber die Erde: die Zeit ist erf\u00fcllt!", "tokens": ["Sie", "schluchz\u00b7ten", "fal\u00b7tig", "und", "hei\u00b7ser", ",", "Rie\u00b7sen\u00b7stim\u00b7men", "schri\u00b7en", "\u00fc\u00b7ber", "die", "Er\u00b7de", ":", "die", "Zeit", "ist", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sie hatten wie Tote am Dunkel gesogen, sie warteten auf das Wunder und waren stinkend verreckt.", "tokens": ["Sie", "hat\u00b7ten", "wie", "To\u00b7te", "am", "Dun\u00b7kel", "ge\u00b7so\u00b7gen", ",", "sie", "war\u00b7te\u00b7ten", "auf", "das", "Wun\u00b7der", "und", "wa\u00b7ren", "stin\u00b7kend", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "APPRART", "NN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+--+--+-+--+-+--+", "measure": "amphibrach.penta.plus"}}}}}