{"dta.poem.14167": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Gru\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und als die V\u00f6gelein sangen", "tokens": ["Und", "als", "die", "V\u00f6\u00b7ge\u00b7lein", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fr\u00fchmorgens, sie weinte so sehr,", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", ",", "sie", "wein\u00b7te", "so", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich aber war weit schon gegangen,", "tokens": ["Ich", "a\u00b7ber", "war", "weit", "schon", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun sieht sie mich nimmermehr!", "tokens": ["Nun", "sieht", "sie", "mich", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}