{"textgrid.poem.37045": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dir, dem noch der bleiche Mund", "tokens": ["Wohl", "dir", ",", "dem", "noch", "der", "blei\u00b7che", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Amme Lieder singt,", "tokens": ["Der", "Am\u00b7me", "Lie\u00b7der", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den noch der kleine Schlummergott", "tokens": ["Den", "noch", "der", "klei\u00b7ne", "Schlum\u00b7mer\u00b7gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Schwanenarme schlingt.", "tokens": ["In", "Schwa\u00b7nen\u00b7ar\u00b7me", "schlingt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl dir, dein kleiner Busen kennt", "tokens": ["Wohl", "dir", ",", "dein", "klei\u00b7ner", "Bu\u00b7sen", "kennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Flitterprunk der Welt,", "tokens": ["Den", "Flit\u00b7ter\u00b7prunk", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Amorn nicht, den losen Gott,", "tokens": ["Und", "A\u00b7morn", "nicht", ",", "den", "lo\u00b7sen", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schlaue Netze stellt.", "tokens": ["Der", "schlau\u00b7e", "Net\u00b7ze", "stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch bald entfliegt, mit Adlerflug,", "tokens": ["Doch", "bald", "ent\u00b7fliegt", ",", "mit", "Ad\u00b7ler\u00b7flug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die s\u00fc\u00dfe, goldne Zeit,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", ",", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tag und Nacht der sanfte Schlaf", "tokens": ["Die", "Tag", "und", "Nacht", "der", "sanf\u00b7te", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Mohnlaub \u00fcberstreut.", "tokens": ["Mit", "Mohn\u00b7laub", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann plagt ein m\u00fcrrischer Pedant", "tokens": ["Dann", "plagt", "ein", "m\u00fcr\u00b7ri\u00b7scher", "Pe\u00b7dant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein K\u00f6pfchen mit Latein,", "tokens": ["Dein", "K\u00f6pf\u00b7chen", "mit", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "So sehr Mamachen auf ihn schm\u00e4hlt,", "tokens": ["So", "sehr", "Ma\u00b7ma\u00b7chen", "auf", "ihn", "schm\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in die Nacht hinein.", "tokens": ["Bis", "in", "die", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du fluchst dem ehrlichen Terenz", "tokens": ["Du", "fluchst", "dem", "ehr\u00b7li\u00b7chen", "Te\u00b7renz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch oft in seiner Gruft,", "tokens": ["Noch", "oft", "in", "sei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er von deinem Steckenpferd", "tokens": ["Wenn", "er", "von", "dei\u00b7nem", "Ste\u00b7cken\u00b7pferd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich in die Schule ruft.", "tokens": ["Dich", "in", "die", "Schu\u00b7le", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du w\u00fcnschest oft, wenn Cicero", "tokens": ["Du", "w\u00fcn\u00b7schest", "oft", ",", "wenn", "Ci\u00b7ce\u00b7ro"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein s\u00fc\u00dfes Spiel verr\u00fcckt,", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Spiel", "ver\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00e4tt er doch, der b\u00f6se Mann,", "tokens": ["O", "h\u00e4tt", "er", "doch", ",", "der", "b\u00f6\u00b7se", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Tagslicht nie erblickt.", "tokens": ["Das", "Tags\u00b7licht", "nie", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ruh sanft, so lange dir das Lied", "tokens": ["Ruh", "sanft", ",", "so", "lan\u00b7ge", "dir", "das", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Amme noch erschallt,", "tokens": ["Der", "Am\u00b7me", "noch", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die s\u00fc\u00dfe Morgend\u00e4mmerung", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der Kindheit fliehet bald.", "tokens": ["Der", "Kind\u00b7heit", "flie\u00b7het", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}