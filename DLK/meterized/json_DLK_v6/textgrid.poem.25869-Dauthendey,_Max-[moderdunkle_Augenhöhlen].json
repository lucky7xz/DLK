{"textgrid.poem.25869": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[moderdunkle Augenh\u00f6hlen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Moderdunkle Augenh\u00f6hlen,", "tokens": ["Mo\u00b7der\u00b7dunk\u00b7le", "Au\u00b7gen\u00b7h\u00f6h\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finster stiert ein wei\u00dfer Sch\u00e4del.", "tokens": ["Fins\u00b7ter", "stiert", "ein", "wei\u00b7\u00dfer", "Sch\u00e4\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Echo toter Willen", "tokens": ["Und", "die", "E\u00b7cho", "to\u00b7ter", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grinsen blau wie Sterngefunkel", "tokens": ["Grin\u00b7sen", "blau", "wie", "Stern\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem schwarzen Stirngew\u00f6lbe.", "tokens": ["Von", "dem", "schwar\u00b7zen", "Stirn\u00b7ge\u00b7w\u00f6l\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wasserjungfern, riesenhafte,", "tokens": ["Was\u00b7ser\u00b7jung\u00b7fern", ",", "rie\u00b7sen\u00b7haf\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klirren mit den blauen Fl\u00fcgeln,", "tokens": ["Klir\u00b7ren", "mit", "den", "blau\u00b7en", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flirren durch die Sch\u00e4delh\u00f6hlen,", "tokens": ["Flir\u00b7ren", "durch", "die", "Sch\u00e4\u00b7del\u00b7h\u00f6h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Girren um die Gunst des Todes.", "tokens": ["Gir\u00b7ren", "um", "die", "Gunst", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}