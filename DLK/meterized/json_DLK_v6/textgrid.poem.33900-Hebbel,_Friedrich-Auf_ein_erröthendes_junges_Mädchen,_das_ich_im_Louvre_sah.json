{"textgrid.poem.33900": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein err\u00f6thendes junges M\u00e4dchen, das ich im Louvre sah", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lie\u00df mein Auge auf dem deinen ruh'n,", "tokens": ["Ich", "lie\u00df", "mein", "Au\u00b7ge", "auf", "dem", "dei\u00b7nen", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ward zur Purpurflamme dein Gesicht;", "tokens": ["Da", "ward", "zur", "Pur\u00b7pur\u00b7flam\u00b7me", "dein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du warst ein Kind, ein M\u00e4dchen bist du nun,", "tokens": ["Du", "warst", "ein", "Kind", ",", "ein", "M\u00e4d\u00b7chen", "bist", "du", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So weig're auch die M\u00e4dchenfrucht mir nicht.", "tokens": ["So", "weig'\u00b7re", "auch", "die", "M\u00e4d\u00b7chen\u00b7frucht", "mir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Mund ist reif jetzt f\u00fcr den ersten Ku\u00df,", "tokens": ["Dein", "Mund", "ist", "reif", "jetzt", "f\u00fcr", "den", "ers\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er gleicht der Herzenskirsche, die zersprang", "tokens": ["Er", "gleicht", "der", "Her\u00b7zens\u00b7kir\u00b7sche", ",", "die", "zer\u00b7sprang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor aller Feuers\u00e4fte letztem Schu\u00df,", "tokens": ["Vor", "al\u00b7ler", "Feu\u00b7er\u00b7s\u00e4f\u00b7te", "letz\u00b7tem", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nun verspritzt, was sie so hei\u00df durchdrang.", "tokens": ["Und", "nun", "ver\u00b7spritzt", ",", "was", "sie", "so", "hei\u00df", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hab' ein Recht auf ihn, ich hab' in dir", "tokens": ["Ich", "hab'", "ein", "Recht", "auf", "ihn", ",", "ich", "hab'", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Glut, die ihn gezeitigt hat, geweckt,", "tokens": ["Die", "Glut", ",", "die", "ihn", "ge\u00b7zei\u00b7tigt", "hat", ",", "ge\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum raub' ich ihn mit kecker Lippe mir,", "tokens": ["Drum", "raub'", "ich", "ihn", "mit", "ke\u00b7cker", "Lip\u00b7pe", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie V\u00f6gel Beeren, die kein Laub mehr deckt.", "tokens": ["Wie", "V\u00f6\u00b7gel", "Bee\u00b7ren", ",", "die", "kein", "Laub", "mehr", "deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$,", "PRELS", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vielleicht vollendet dieser Ku\u00df mein Gl\u00fcck,", "tokens": ["Viel\u00b7leicht", "voll\u00b7en\u00b7det", "die\u00b7ser", "Ku\u00df", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst durch ihn dir deiner ganz bewu\u00dft,", "tokens": ["Du", "wirst", "durch", "ihn", "dir", "dei\u00b7ner", "ganz", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PPER", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie du M\u00e4dchen wardst vor meinem Blick,", "tokens": ["Und", "wie", "du", "M\u00e4d\u00b7chen", "wardst", "vor", "mei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wirst du auch noch Weib an meiner Brust!", "tokens": ["So", "wirst", "du", "auch", "noch", "Weib", "an", "mei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}