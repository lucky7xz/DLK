{"textgrid.poem.29493": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ietzt Heraclitus lebte, w\u00fcrd er f\u00fcr das weinen lachen,", "tokens": ["Wann", "ietzt", "Her\u00b7a\u00b7cli\u00b7tus", "leb\u00b7te", ",", "w\u00fcrd", "er", "f\u00fcr", "das", "wei\u00b7nen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "ART", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und Democritus na\u00df Augen f\u00fcr gewohntes lachen machen,", "tokens": ["Und", "De\u00b7mo\u00b7cri\u00b7tus", "na\u00df", "Au\u00b7gen", "f\u00fcr", "ge\u00b7wohn\u00b7tes", "la\u00b7chen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "NN", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Weil die Welt so gar gewandelt Sinnen, Sitten, Arten, Sachen.", "tokens": ["Weil", "die", "Welt", "so", "gar", "ge\u00b7wan\u00b7delt", "Sin\u00b7nen", ",", "Sit\u00b7ten", ",", "Ar\u00b7ten", ",", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVPP", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}