{"textgrid.poem.42903": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Flugzeug am Winterhimmel", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fliege im Flockengewimmel.", "tokens": ["Ich", "flie\u00b7ge", "im", "Flo\u00b7cken\u00b7ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach, guter Himmel, la\u00df das doch sein!", "tokens": ["Ach", ",", "gu\u00b7ter", "Him\u00b7mel", ",", "la\u00df", "das", "doch", "sein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVIMP", "PDS", "ADV", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich Flugriese bin nur klein V\u00f6gelein", "tokens": ["Ich", "Flug\u00b7rie\u00b7se", "bin", "nur", "klein", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gegen dich, sch\u00fcttender Himmel.", "tokens": ["Ge\u00b7gen", "dich", ",", "sch\u00fct\u00b7ten\u00b7der", "Him\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sag Schneegest\u00f6ber, ich b\u00e4te es sehr,", "tokens": ["Sag", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", ",", "ich", "b\u00e4\u00b7te", "es", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein wenig nachzulassen.", "tokens": ["Ein", "we\u00b7nig", "nach\u00b7zu\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn meine Fl\u00fcgel tragen schon schwer", "tokens": ["Denn", "mei\u00b7ne", "Fl\u00fc\u00b7gel", "tra\u00b7gen", "schon", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "An sechs ganz dicken Insassen.", "tokens": ["An", "sechs", "ganz", "di\u00b7cken", "In\u00b7sas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Die spielen Karten in meinem Leib", "tokens": ["Die", "spie\u00b7len", "Kar\u00b7ten", "in", "mei\u00b7nem", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und trinken, weil sie so frieren.", "tokens": ["Und", "trin\u00b7ken", ",", "weil", "sie", "so", "frie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wollen nach Zoppot, um Zeitvertreib", "tokens": ["Und", "wol\u00b7len", "nach", "Zop\u00b7pot", ",", "um", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NE", "$,", "KOUI", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und \u00d6rtliches zu studieren.", "tokens": ["Und", "\u00d6rt\u00b7li\u00b7ches", "zu", "stu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Und k\u00e4me ich dort nicht p\u00fcnktlich hin,", "tokens": ["Und", "k\u00e4\u00b7me", "ich", "dort", "nicht", "p\u00fcnkt\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die w\u00fcrden es niemals verzeihen.", "tokens": ["Die", "w\u00fcr\u00b7den", "es", "nie\u00b7mals", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Lieber Himmel, wenn ich gelandet bin,", "tokens": ["Lie\u00b7ber", "Him\u00b7mel", ",", "wenn", "ich", "ge\u00b7lan\u00b7det", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dann darfst du gern wieder schneien.", "tokens": ["Dann", "darfst", "du", "gern", "wie\u00b7der", "schnei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}