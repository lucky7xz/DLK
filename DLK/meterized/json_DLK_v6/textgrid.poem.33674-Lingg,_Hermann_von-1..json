{"textgrid.poem.33674": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem Flammenmeer umgeben,", "tokens": ["Von", "dem", "Flam\u00b7men\u00b7meer", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die Abendgluten weben,", "tokens": ["Das", "die", "A\u00b7bend\u00b7glu\u00b7ten", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stern der Liebe, scheinst du doch!", "tokens": ["Stern", "der", "Lie\u00b7be", ",", "scheinst", "du", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so mild und sanft daneben", "tokens": ["Wie", "so", "mild", "und", "sanft", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie siegreich scheinst du doch!", "tokens": ["Und", "wie", "sieg\u00b7reich", "scheinst", "du", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wolken seh' ich sich erheben,", "tokens": ["Wol\u00b7ken", "seh'", "ich", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die dunkeln scheinst du doch!", "tokens": ["Durch", "die", "dun\u00b7keln", "scheinst", "du", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch in mein umnachtet Leben", "tokens": ["Auch", "in", "mein", "um\u00b7nach\u00b7tet", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Immer wieder scheinst du doch;", "tokens": ["Im\u00b7mer", "wie\u00b7der", "scheinst", "du", "doch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber insgeheim zu beben,", "tokens": ["A\u00b7ber", "ins\u00b7ge\u00b7heim", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Stern der Liebe, scheinst du doch!", "tokens": ["Stern", "der", "Lie\u00b7be", ",", "scheinst", "du", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}