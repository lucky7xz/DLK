{"textgrid.poem.53614": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An den deutschen Mond", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guter Mond, du gehst so stille", "tokens": ["Gu\u00b7ter", "Mond", ",", "du", "gehst", "so", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Abendwolken hin!", "tokens": ["Durch", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst die lange \u00c4ppelzille", "tokens": ["Siehst", "die", "lan\u00b7ge", "\u00c4p\u00b7pel\u00b7zil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Venuspriesterin.", "tokens": ["Und", "die", "Ve\u00b7nus\u00b7pries\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst Passanten und die Bummler", "tokens": ["Siehst", "Pas\u00b7san\u00b7ten", "und", "die", "Bumm\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die b\u00f6sen Geldscheinschummler.", "tokens": ["Und", "die", "b\u00f6\u00b7sen", "Geld\u00b7schein\u00b7schumm\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist das alles schon gewohnt,", "tokens": ["Bist", "das", "al\u00b7les", "schon", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Guter Mond, guter Mond \u2013!", "tokens": ["Gu\u00b7ter", "Mond", ",", "gu\u00b7ter", "Mond", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Segelst langsam ob den D\u00e4chern,", "tokens": ["Se\u00b7gelst", "lang\u00b7sam", "ob", "den", "D\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst in Fenster und B\u00fcros,", "tokens": ["Siehst", "in", "Fens\u00b7ter", "und", "B\u00fc\u00b7ros", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wo die Akten in den F\u00e4chern", "tokens": ["Wo", "die", "Ak\u00b7ten", "in", "den", "F\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstern: \u00bbWir sind Nosken los!\u00ab", "tokens": ["Fl\u00fcs\u00b7tern", ":", "\u00bb", "Wir", "sind", "Nos\u00b7ken", "los", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst in Fenster der Kasernen,", "tokens": ["Siehst", "in", "Fens\u00b7ter", "der", "Ka\u00b7ser\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo sie Schwarz-Rot-Gold entfernen . . .", "tokens": ["Wo", "sie", "Schwa\u00b7rz\u00b7Rot\u00b7Gold", "ent\u00b7fer\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$.", "$.", "$."], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.7": {"text": "Bist das alles schon gewohnt,", "tokens": ["Bist", "das", "al\u00b7les", "schon", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Guter Mond, guter Mond \u2013!", "tokens": ["Gu\u00b7ter", "Mond", ",", "gu\u00b7ter", "Mond", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Kugelst dich am Firmamente", "tokens": ["Ku\u00b7gelst", "dich", "am", "Fir\u00b7ma\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber unsre gro\u00dfe Stadt,", "tokens": ["\u00dc\u00b7ber", "uns\u00b7re", "gro\u00b7\u00dfe", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst die dicke, schwere Rente,", "tokens": ["Siehst", "die", "di\u00b7cke", ",", "schwe\u00b7re", "Ren\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Ludendorff noch hat.", "tokens": ["Die", "der", "Lu\u00b7den\u00b7dorff", "noch", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst auch n\u00e4chstens, wenn es sp\u00e4ter,", "tokens": ["Siehst", "auch", "n\u00e4chs\u00b7tens", ",", "wenn", "es", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche freien Hochverr\u00e4ter . . .", "tokens": ["Man\u00b7che", "frei\u00b7en", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ter", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist das alles schon gewohnt,", "tokens": ["Bist", "das", "al\u00b7les", "schon", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Guter Mond, guter Mond \u2013!", "tokens": ["Gu\u00b7ter", "Mond", ",", "gu\u00b7ter", "Mond", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Aber k\u00e4me pl\u00f6tzlich einer,", "tokens": ["A\u00b7ber", "k\u00e4\u00b7me", "pl\u00f6tz\u00b7lich", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der trotz L\u00e4rmen und Gezisch", "tokens": ["Der", "trotz", "L\u00e4r\u00b7men", "und", "Ge\u00b7zisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00fcge \u2013 wie noch leider keiner \u2013", "tokens": ["Schl\u00fc\u00b7ge", "\u2013", "wie", "noch", "lei\u00b7der", "kei\u00b7ner", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "ADV", "ADV", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Faust auf unsern Tisch \u2013", "tokens": ["Mit", "der", "Faust", "auf", "un\u00b7sern", "Tisch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sagt der: \u00bbMilit\u00e4r kann gehen!\u00ab", "tokens": ["Sagt", "der", ":", "\u00bb", "Mi\u00b7li\u00b7t\u00e4r", "kann", "ge\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "$.", "$(", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ei, dann bliebst du sicher stehen!", "tokens": ["Ei", ",", "dann", "bliebst", "du", "si\u00b7cher", "ste\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn das bist du nicht gewohnt,", "tokens": ["Denn", "das", "bist", "du", "nicht", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Guter Mond, guter Mond \u2013!", "tokens": ["Gu\u00b7ter", "Mond", ",", "gu\u00b7ter", "Mond", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}