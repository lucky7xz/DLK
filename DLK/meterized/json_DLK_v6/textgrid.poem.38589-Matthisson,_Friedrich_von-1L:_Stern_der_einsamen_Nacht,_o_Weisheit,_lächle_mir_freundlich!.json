{"textgrid.poem.38589": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stern der einsamen Nacht, o Weisheit, l\u00e4chle mir freundlich!", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stern der einsamen Nacht, o Weisheit, l\u00e4chle mir freundlich!", "tokens": ["Stern", "der", "ein\u00b7sa\u00b7men", "Nacht", ",", "o", "Weis\u00b7heit", ",", "l\u00e4ch\u00b7le", "mir", "freund\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Leite mein wankendes Schiff sicher durch Wogen und Sturm;", "tokens": ["Lei\u00b7te", "mein", "wan\u00b7ken\u00b7des", "Schiff", "si\u00b7cher", "durch", "Wo\u00b7gen", "und", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bis auf dem Eiland der Ruh', ein bl\u00fchendes Tempe mich aufnimmt,", "tokens": ["Bis", "auf", "dem", "Ei\u00b7land", "der", "Ruh'", ",", "ein", "bl\u00fc\u00b7hen\u00b7des", "Tem\u00b7pe", "mich", "auf\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo kein Gew\u00f6lk deines Strals himmlische Reine mehr tr\u00fcbt.", "tokens": ["Wo", "kein", "Ge\u00b7w\u00f6lk", "dei\u00b7nes", "Strals", "himm\u00b7li\u0