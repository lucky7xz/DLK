{"textgrid.poem.39494": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Tode rettet nur den Tod Verachten.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Tode rettet nur den Tod Verachten.", "tokens": ["Vom", "To\u00b7de", "ret\u00b7tet", "nur", "den", "Tod", "Ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem, der ihn f\u00fcrchtet, trinkt er zu bei'm Mahle,", "tokens": ["Dem", ",", "der", "ihn", "f\u00fcrch\u00b7tet", ",", "trinkt", "er", "zu", "bei'm", "Mah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der grause Gast, aus blinkendem Pokale,", "tokens": ["Der", "grau\u00b7se", "Gast", ",", "aus", "blin\u00b7ken\u00b7dem", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df rings der Freude Kerzen sich umnachten.", "tokens": ["Da\u00df", "rings", "der", "Freu\u00b7de", "Ker\u00b7zen", "sich", "um\u00b7nach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wenn ihn Muth und Kraft zu reizen trachten,", "tokens": ["Doch", "wenn", "ihn", "Muth", "und", "Kraft", "zu", "rei\u00b7zen", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So weicht er aus, und trifft mit scheuem Stahle;", "tokens": ["So", "weicht", "er", "aus", ",", "und", "trifft", "mit", "scheu\u00b7em", "Stah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn h\u00f6hnt der Krieg, ob er Tribut ihm zahle,", "tokens": ["Ihn", "h\u00f6hnt", "der", "Krieg", ",", "ob", "er", "Tri\u00b7but", "ihm", "zah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber ihn siegprangen blut'ge Schlachten.", "tokens": ["Und", "\u00fc\u00b7ber", "ihn", "sieg\u00b7pran\u00b7gen", "blut'\u00b7ge", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Drum habet Dank, ihr gro\u00dfen Todver\u00e4chter!", "tokens": ["Drum", "ha\u00b7bet", "Dank", ",", "ihr", "gro\u00b7\u00dfen", "Tod\u00b7ver\u00b7\u00e4ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch, die ihr einsam euer Blut gespendet", "tokens": ["Euch", ",", "die", "ihr", "ein\u00b7sam", "eu\u00b7er", "Blut", "ge\u00b7spen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Zeus Befreier, soll der Einz'le danken;", "tokens": ["Dem", "Zeus", "Be\u00b7frei\u00b7er", ",", "soll", "der", "Einz'\u00b7le", "dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Nationen, R\u00f6mer, euch, und Franken!", "tokens": ["Die", "Na\u00b7ti\u00b7o\u00b7nen", ",", "R\u00f6\u00b7mer", ",", "euch", ",", "und", "Fran\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "PPER", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Kampf mit Schrecken, die der Orkus sendet,", "tokens": ["Im", "Kampf", "mit", "Schre\u00b7cken", ",", "die", "der", "Or\u00b7kus", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seid ihr die Phalanx sterblicher Geschlechter.", "tokens": ["Seid", "ihr", "die", "Pha\u00b7lanx", "sterb\u00b7li\u00b7cher", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}