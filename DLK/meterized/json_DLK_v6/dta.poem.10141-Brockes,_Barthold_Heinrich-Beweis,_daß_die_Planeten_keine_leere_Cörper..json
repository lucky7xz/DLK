{"dta.poem.10141": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beweis, da\u00df die Planeten keine leere  \n C\u00f6rper.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit \u00fcberzeuglich-wahren Schl\u00fcssen, und vielen Gr\u00fcn-\nden nicht allein,", "tokens": ["Mit", "\u00fc\u00b7ber\u00b7zeug\u00b7lich\u00b7wah\u00b7ren", "Schl\u00fcs\u00b7sen", ",", "und", "vie\u00b7len", "Gr\u00fcn", "den", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PIAT", "TRUNC", "ART", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zeigt man, da\u00df der Planeten C\u00f6rper erf\u00fcllet und bewoh-", "tokens": ["Zeigt", "man", ",", "da\u00df", "der", "Pla\u00b7ne\u00b7ten", "C\u00f6r\u00b7per", "er\u00b7f\u00fcl\u00b7let", "und", "be\u00b7woh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "NE", "VVFIN", "KON", "TRUNC"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Man kann den grossen Satz so gar, aus ", "tokens": ["Man", "kann", "den", "gros\u00b7sen", "Satz", "so", "gar", ",", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$,", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Weil wir f\u00fcr die so grosse C\u00f6rper derselben Sch\u00f6pfer", "tokens": ["Weil", "wir", "f\u00fcr", "die", "so", "gros\u00b7se", "C\u00f6r\u00b7per", "der\u00b7sel\u00b7ben", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und alles doch unwiedersprechlich, zu Seinen Ehren, ist ge-", "tokens": ["Und", "al\u00b7les", "doch", "un\u00b7wie\u00b7der\u00b7sprech\u00b7lich", ",", "zu", "Sei\u00b7nen", "Eh\u00b7ren", ",", "ist", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,", "VAFIN", "TRUNC"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "So folgt daraus: Da\u00df sonder Zweifel Gesch\u00f6pfe dort her-", "tokens": ["So", "folgt", "da\u00b7raus", ":", "Da\u00df", "son\u00b7der", "Zwei\u00b7fel", "Ge\u00b7sch\u00f6p\u00b7fe", "dort", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "$.", "KOUS", "ADJA", "NN", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die das, was wir verrichten sollten, in Lust und Ehr-Furcht", "tokens": ["Die", "das", ",", "was", "wir", "ver\u00b7rich\u00b7ten", "soll\u00b7ten", ",", "in", "Lust", "und", "Ehr\u00b7Furcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PDS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und den Anbetungs-werthen Sch\u00f6pfer verehren, dancken,", "tokens": ["Und", "den", "An\u00b7be\u00b7tungs\u00b7wert\u00b7hen", "Sch\u00f6p\u00b7fer", "ver\u00b7eh\u00b7ren", ",", "dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,", "VVINF", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}}}}}