{"dta.poem.3280": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An das Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Falsches Gl\u00fcck, das unter finstern Str\u00e4u-", "tokens": ["Fal\u00b7sches", "Gl\u00fcck", ",", "das", "un\u00b7ter", "fins\u00b7tern", "Str\u00e4u"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "chen", "tokens": ["chen"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Sich verbirgt, wo k\u00fchne T\u00fccke schlei-", "tokens": ["Sich", "ver\u00b7birgt", ",", "wo", "k\u00fch\u00b7ne", "T\u00fc\u00b7cke", "schlei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "PWAV", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "chen!", "tokens": ["chen", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Sollt\u2019, o Abgott niedrer Seelen!", "tokens": ["Sollt'", ",", "o", "Ab\u00b7gott", "nie\u00b7drer", "See\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollt ich mich in deinem Dienste qu\u00e4len?", "tokens": ["Sollt", "ich", "mich", "in", "dei\u00b7nem", "Diens\u00b7te", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dich wird nie die scheue Tugend finden;", "tokens": ["Dich", "wird", "nie", "die", "scheu\u00b7e", "Tu\u00b7gend", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du wirst stets vor ihrem Blick verschwinden:", "tokens": ["Du", "wirst", "stets", "vor", "ih\u00b7rem", "Blick", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber auf bebl\u00fchmten Wegen", "tokens": ["A\u00b7ber", "auf", "be\u00b7bl\u00fchm\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Taumelst du den Thoren selbst entgegen.", "tokens": ["Tau\u00b7melst", "du", "den", "Tho\u00b7ren", "selbst", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Kann ich mich doch ohne dich vergn\u00fcgen!", "tokens": ["Kann", "ich", "mich", "doch", "oh\u00b7ne", "dich", "ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wie schnell mu\u00df alles Leid verfliegen,", "tokens": ["Und", "wie", "schnell", "mu\u00df", "al\u00b7les", "Leid", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VMFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich unter Freunden singe!", "tokens": ["Wenn", "ich", "un\u00b7ter", "Freun\u00b7den", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6re selbst, wie meine Cyther klinge!", "tokens": ["H\u00f6\u00b7re", "selbst", ",", "wie", "mei\u00b7ne", "Cy\u00b7ther", "klin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wen besing ich, als den Gott der Reben?", "tokens": ["Wen", "be\u00b7sing", "ich", ",", "als", "den", "Gott", "der", "Re\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Diese Rosen, die mein Haupt umgeben,", "tokens": ["Die\u00b7se", "Ro\u00b7sen", ",", "die", "mein", "Haupt", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Gl\u00e4ser frohe Menge", "tokens": ["Die\u00b7ser", "Gl\u00e4\u00b7ser", "fro\u00b7he", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ihm heilig, und er liebt Ges\u00e4nge.", "tokens": ["Sind", "ihm", "hei\u00b7lig", ",", "und", "er", "liebt", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Faunen! tanzt vor mir mit frohen Spr\u00fcngen!", "tokens": ["Fau\u00b7nen", "!", "tanzt", "vor", "mir", "mit", "fro\u00b7hen", "Spr\u00fcn\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von Ly\u00e4ens Liebe will ich singen:", "tokens": ["Von", "Ly\u00b7\u00e4\u00b7ens", "Lie\u00b7be", "will", "ich", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seine Sch\u00f6ne war noch bl\u00f6de,", "tokens": ["Sei\u00b7ne", "Sch\u00f6\u00b7ne", "war", "noch", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War voll Unschuld und aus Unschuld spr\u00f6de.", "tokens": ["War", "voll", "Un\u00b7schuld", "und", "aus", "Un\u00b7schuld", "spr\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber Bacchus wurde kaum zur Traube;", "tokens": ["A\u00b7ber", "Bac\u00b7chus", "wur\u00b7de", "kaum", "zur", "Trau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie l\u00fcstern nahm sie ihn vom Laube!", "tokens": ["O", "wie", "l\u00fcs\u00b7tern", "nahm", "sie", "ihn", "vom", "Lau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie begl\u00fcckte seine Triebe;", "tokens": ["Sie", "be\u00b7gl\u00fcck\u00b7te", "sei\u00b7ne", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch immer dient sein Wein der Liebe.", "tokens": ["Und", "noch", "im\u00b7mer", "dient", "sein", "Wein", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "S\u00fcsser Ton! wem sollt er nicht gefallen?", "tokens": ["S\u00fcs\u00b7ser", "Ton", "!", "wem", "sollt", "er", "nicht", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur von Lust soll meine Cyther schallen,", "tokens": ["Nur", "von", "Lust", "soll", "mei\u00b7ne", "Cy\u00b7ther", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich hier am k\u00fchlen Bache,", "tokens": ["Wenn", "ich", "hier", "am", "k\u00fch\u00b7len", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hingestreckt auf weichen Bluhmen, lache:", "tokens": ["Hin\u00b7ge\u00b7streckt", "auf", "wei\u00b7chen", "Bluh\u00b7men", ",", "la\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Hier im Busch, in sichren Finsternissen,", "tokens": ["Hier", "im", "Busch", ",", "in", "sich\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ich oft, berauscht von Wein und K\u00fcssen,", "tokens": ["Wo", "ich", "oft", ",", "be\u00b7rauscht", "von", "Wein", "und", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die ich um kein Gl\u00fcck vertausche,", "tokens": ["Die", "ich", "um", "kein", "Gl\u00fcck", "ver\u00b7tau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "An der Phyllis vollem Busen lausche.", "tokens": ["An", "der", "Phyl\u00b7lis", "vol\u00b7lem", "Bu\u00b7sen", "lau\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Fahre hin, du sorgenreiches Gl\u00fccke!", "tokens": ["Fah\u00b7re", "hin", ",", "du", "sor\u00b7gen\u00b7rei\u00b7ches", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer dich kennt, buhlt nicht durch Bubenst\u00fccke", "tokens": ["Wer", "dich", "kennt", ",", "buhlt", "nicht", "durch", "Bu\u00b7ben\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um das fl\u00fcchtige Vergn\u00fcgen,", "tokens": ["Um", "das", "fl\u00fcch\u00b7ti\u00b7ge", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir im Schoos, verliebt in Rauch, zu liegen.", "tokens": ["Dir", "im", "Schoos", ",", "ver\u00b7liebt", "in", "Rauch", ",", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}