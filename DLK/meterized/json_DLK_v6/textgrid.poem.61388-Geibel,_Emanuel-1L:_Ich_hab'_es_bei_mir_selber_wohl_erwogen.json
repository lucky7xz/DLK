{"textgrid.poem.61388": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab' es bei mir selber wohl erwogen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' es bei mir selber wohl erwogen", "tokens": ["Ich", "hab'", "es", "bei", "mir", "sel\u00b7ber", "wohl", "er\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einer langen, schlummerlosen Nacht,", "tokens": ["In", "ei\u00b7ner", "lan\u00b7gen", ",", "schlum\u00b7mer\u00b7lo\u00b7sen", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Liebe, die mir S\u00fc\u00dfes viel gebracht,", "tokens": ["Da\u00df", "Lie\u00b7be", ",", "die", "mir", "S\u00fc\u00b7\u00dfes", "viel", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich dennoch um mein bestes Gl\u00fcck betrogen.", "tokens": ["Mich", "den\u00b7noch", "um", "mein", "bes\u00b7tes", "Gl\u00fcck", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Denn seit der Zeit, da\u00df ihrer ich gepflogen,", "tokens": ["Denn", "seit", "der", "Zeit", ",", "da\u00df", "ih\u00b7rer", "ich", "ge\u00b7pflo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlor ich Ruhe, Heiterkeit, Bedacht;", "tokens": ["Ver\u00b7lor", "ich", "Ru\u00b7he", ",", "Hei\u00b7ter\u00b7keit", ",", "Be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald war mein Sinn zu wilder Glut entfacht", "tokens": ["Bald", "war", "mein", "Sinn", "zu", "wil\u00b7der", "Glut", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bald in Schmerzen fern hinausgezogen.", "tokens": ["Und", "bald", "in", "Schmer\u00b7zen", "fern", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Darum beschlo\u00df ich, sonder Ungeduld", "tokens": ["Da\u00b7rum", "be\u00b7schlo\u00df", "ich", ",", "son\u00b7der", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem holden Reiz auf immer zu entsagen", "tokens": ["Dem", "hol\u00b7den", "Reiz", "auf", "im\u00b7mer", "zu", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und abzutun der Neigung s\u00fc\u00dfe Schuld.", "tokens": ["Und", "ab\u00b7zu\u00b7tun", "der", "Nei\u00b7gung", "s\u00fc\u00b7\u00dfe", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Ruhe sollst fortan, mein Herz, du schlagen", "tokens": ["In", "Ru\u00b7he", "sollst", "for\u00b7tan", ",", "mein", "Herz", ",", "du", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und statt des Schattens fl\u00fccht'ger Erdenhuld", "tokens": ["Und", "statt", "des", "Schat\u00b7tens", "fl\u00fccht'\u00b7ger", "Er\u00b7den\u00b7huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Ewigkeit in deiner Tiefe tragen.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "in", "dei\u00b7ner", "Tie\u00b7fe", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}