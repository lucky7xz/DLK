{"dta.poem.9849": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff das vom T\u00fcrcken bekriegte  \n Ungern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll itzt der erben krei\u00df nicht beben und erzirtern/", "tokens": ["Soll", "itzt", "der", "er\u00b7ben", "krei\u00df", "nicht", "be\u00b7ben", "und", "er\u00b7zir\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sich des himmels grimm mit allen ungewittern", "tokens": ["Da", "sich", "des", "him\u00b7mels", "grimm", "mit", "al\u00b7len", "un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat wider uns ger\u00fcst? da T\u00fcrck und Seyth erwacht/", "tokens": ["Hat", "wi\u00b7der", "uns", "ge\u00b7r\u00fcst", "?", "da", "T\u00fcrck", "und", "Seyth", "er\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$.", "KOUS", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gantz Pannonien zu einer leiche macht.", "tokens": ["Und", "gantz", "Pan\u00b7no\u00b7ni\u00b7en", "zu", "ei\u00b7ner", "lei\u00b7che", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Ister/ so zuvor gekr\u00f6nt von unsern siegen", "tokens": ["Der", "Is\u00b7ter", "/", "so", "zu\u00b7vor", "ge\u00b7kr\u00f6nt", "von", "un\u00b7sern", "sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und fremdem blut gef\u00e4rbt/ mu\u00df nun an fesseln liegen/", "tokens": ["Und", "frem\u00b7dem", "blut", "ge\u00b7f\u00e4rbt", "/", "mu\u00df", "nun", "an", "fes\u00b7seln", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$(", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die schnelle welle hebt ihr schwaches haupt empor/", "tokens": ["Die", "schnel\u00b7le", "wel\u00b7le", "hebt", "ihr", "schwa\u00b7ches", "haupt", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil vor den lorber-crautz ver\u00e4chtlich schilf und rohr", "tokens": ["Weil", "vor", "den", "lor\u00b7ber\u00b7crautz", "ver\u00b7\u00e4cht\u00b7lich", "schilf", "und", "rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die stoltze scheitel dr\u00fcckt. Das ufer klingt von klage/", "tokens": ["Die", "stolt\u00b7ze", "schei\u00b7tel", "dr\u00fcckt", ".", "Das", "u\u00b7fer", "klingt", "von", "kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und jenseits starrt von blut die schwesterliche wage/", "tokens": ["Und", "jen\u00b7seits", "starrt", "von", "blut", "die", "schwes\u00b7ter\u00b7li\u00b7che", "wa\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem der Neutra-strom der freyheit purpur tr\u00e4gt/", "tokens": ["In\u00b7dem", "der", "Neu\u00b7tra\u00b7strom", "der", "frey\u00b7heit", "pur\u00b7pur", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den unser krieges-mann daselbsten abgelegt/", "tokens": ["Den", "un\u00b7ser", "krie\u00b7ge\u00b7smann", "da\u00b7selbs\u00b7ten", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als Ottomannes blitz Neuh\u00e4usel angefallen/", "tokens": ["Als", "Ot\u00b7to\u00b7man\u00b7nes", "blitz", "Neu\u00b7h\u00e4u\u00b7sel", "an\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unaufh\u00f6rlich lie\u00df die mord-gesch\u00fctz erschallen/", "tokens": ["Und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "lie\u00df", "die", "mord\u00b7ge\u00b7sch\u00fctz", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da ieder augenblick mit donner schwanger ging/", "tokens": ["Da", "ie\u00b7der", "au\u00b7gen\u00b7blick", "mit", "don\u00b7ner", "schwan\u00b7ger", "ging", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und an des himmels dach unendlich feuer hing.", "tokens": ["Und", "an", "des", "him\u00b7mels", "dach", "un\u00b7end\u00b7lich", "feu\u00b7er", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PAV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach schauplatz aller noth und frevler grausamkeiten!", "tokens": ["Ach", "schau\u00b7platz", "al\u00b7ler", "noth", "und", "frev\u00b7ler", "grau\u00b7sam\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dort sahe man den feind/ hier wenig christen streiten/", "tokens": ["Dort", "sa\u00b7he", "man", "den", "feind", "/", "hier", "we\u00b7nig", "chris\u00b7ten", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$(", "ADV", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So eintzig und allein ein enger wall beschlo\u00df/", "tokens": ["So", "eint\u00b7zig", "und", "al\u00b7lein", "ein", "en\u00b7ger", "wall", "be\u00b7schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.20": {"text": "Wann sich dort Asien mit donnerndem gescho\u00df", "tokens": ["Wann", "sich", "dort", "A\u00b7sien", "mit", "don\u00b7nern\u00b7dem", "ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Abscheulich h\u00f6ren lie\u00df/ und spie\u00df und schwerdter k", "tokens": ["Ab\u00b7scheu\u00b7lich", "h\u00f6\u00b7ren", "lie\u00df", "/", "und", "spie\u00df", "und", "schwerd\u00b7ter", "k"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VVFIN", "$(", "KON", "VVFIN", "KON", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Wann kugeln in der luft mit lichtem schwefel sprungen/", "tokens": ["Wann", "ku\u00b7geln", "in", "der", "luft", "mit", "lich\u00b7tem", "schwe\u00b7fel", "sprun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und den ge\u00e4ngstigten und jammerreichen tag", "tokens": ["Und", "den", "ge\u00b7\u00e4ngs\u00b7tig\u00b7ten", "und", "jam\u00b7mer\u00b7rei\u00b7chen", "tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ermordter c\u00f6rper zahl fast wie zu schw\u00e4rtzen pflag.", "tokens": ["Er\u00b7mord\u00b7ter", "c\u00f6r\u00b7per", "zahl", "fast", "wie", "zu", "schw\u00e4rt\u00b7zen", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "KOKOM", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Uns reitzte tapfrer muth vors vaterland zu sterben/", "tokens": ["Uns", "reitz\u00b7te", "tapf\u00b7rer", "muth", "vors", "va\u00b7ter\u00b7land", "zu", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den bluthund unser land von grund aus zu verderben/", "tokens": ["Den", "blut\u00b7hund", "un\u00b7ser", "land", "von", "grund", "aus", "zu", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drum st\u00fcrtzt er leich auf leich/ und wie zur fr\u00fchliings-zeit", "tokens": ["Drum", "st\u00fcrtzt", "er", "leich", "auf", "leich", "/", "und", "wie", "zur", "fr\u00fch\u00b7li\u00b7ings\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "ADJD", "$(", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.28": {"text": "Der m\u00e4der blum und gra\u00df mit seiner sens abmeyt.", "tokens": ["Der", "m\u00e4\u00b7der", "blum", "und", "gra\u00df", "mit", "sei\u00b7ner", "sens", "ab\u00b7meyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "So hat der sebel auch viel menschen aufgefressen/", "tokens": ["So", "hat", "der", "se\u00b7bel", "auch", "viel", "men\u00b7schen", "auf\u00b7ge\u00b7fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Wiewol Neuh\u00e4usels ruhm so gar nicht zu vergessen/", "tokens": ["Wie\u00b7wol", "Neu\u00b7h\u00e4u\u00b7sels", "ruhm", "so", "gar", "nicht", "zu", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es stritte weil es kont/ und gab es tropfen blut/", "tokens": ["Es", "strit\u00b7te", "weil", "es", "kont", "/", "und", "gab", "es", "trop\u00b7fen", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VMFIN", "$(", "KON", "VVFIN", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So speyte sie gewi\u00df der T\u00fcrcken gr\u00f6\u00dfre fluth", "tokens": ["So", "spey\u00b7te", "sie", "ge\u00b7wi\u00df", "der", "T\u00fcr\u00b7cken", "gr\u00f6\u00df\u00b7re", "fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Von ihm geprest heraus. Und ist es \u00fcberwunden/", "tokens": ["Von", "ihm", "ge\u00b7prest", "he\u00b7raus", ".", "Und", "ist", "es", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PTKVZ", "$.", "KON", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So hat Scyth und Hircan auch da den sarg gefunden.", "tokens": ["So", "hat", "Scyth", "und", "Hir\u00b7can", "auch", "da", "den", "sarg", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "KON", "NE", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Das feld/ so zwar von gra\u00df/ doch nicht von todten rein/", "tokens": ["Das", "feld", "/", "so", "zwar", "von", "gra\u00df", "/", "doch", "nicht", "von", "tod\u00b7ten", "rein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "APPR", "NN", "$(", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Kan gar mit gutem recht des feindes kirchhof seyn.", "tokens": ["Kan", "gar", "mit", "gu\u00b7tem", "recht", "des", "fein\u00b7des", "kirch\u00b7hof", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die abgefleischte bein und abgeschlagne sch\u00e4del", "tokens": ["Die", "ab\u00b7ge\u00b7fleischte", "bein", "und", "ab\u00b7ge\u00b7schlag\u00b7ne", "sch\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Bezeugen diese that/ da\u00df unser kriegsmann edel", "tokens": ["Be\u00b7zeu\u00b7gen", "die\u00b7se", "that", "/", "da\u00df", "un\u00b7ser", "kriegs\u00b7mann", "e\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und hurtig in dem kampf sich iederzeit gestalt/", "tokens": ["Und", "hur\u00b7tig", "in", "dem", "kampf", "sich", "ie\u00b7der\u00b7zeit", "ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "VVFIN", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wird schon sein werthes blut/ so wird sein ruhm nicht kalt.", "tokens": ["Wird", "schon", "sein", "wert\u00b7hes", "blut", "/", "so", "wird", "sein", "ruhm", "nicht", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Beschreckte Christenheit von deinem \u00e4rgsten feinde/", "tokens": ["Be\u00b7schreck\u00b7te", "Chris\u00b7ten\u00b7heit", "von", "dei\u00b7nem", "\u00e4rgs\u00b7ten", "fein\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ermuntre deinen sinn und suche den zum freunde/", "t