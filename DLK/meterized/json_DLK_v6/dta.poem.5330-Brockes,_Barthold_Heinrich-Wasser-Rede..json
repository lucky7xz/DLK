{"dta.poem.5330": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wasser-Rede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war der Spring-Brunn abgeschlossen, daher auf\nunbewegter Fluht", "tokens": ["Es", "war", "der", "Spring\u00b7Brunn", "ab\u00b7ge\u00b7schlos\u00b7sen", ",", "da\u00b7her", "auf", "un\u00b7be\u00b7weg\u00b7ter", "Fluht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein gr\u00fcner Wieder-Schein gar lieblich in einer duncklen", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Wie\u00b7der\u00b7Schein", "gar", "lieb\u00b7lich", "in", "ei\u00b7ner", "dunck\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der, da er der geschor\u2019nen Hecken begr\u00fcnte Sch\u00f6nheit se-", "tokens": ["Der", ",", "da", "er", "der", "ge\u00b7schor'\u00b7nen", "He\u00b7cken", "be\u00b7gr\u00fcn\u00b7te", "Sch\u00f6n\u00b7heit", "se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Uns ihre holde Pracht, verdoppelt, als wie in einem", "tokens": ["Uns", "ih\u00b7re", "hol\u00b7de", "Pracht", ",", "ver\u00b7dop\u00b7pelt", ",", "als", "wie", "in", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "$,", "KOUS", "KOKOM", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es fiel, bey der so sch\u00f6nen B\u00e4ume so deutlich vorgestelltem", "tokens": ["Es", "fiel", ",", "bey", "der", "so", "sch\u00f6\u00b7nen", "B\u00e4u\u00b7me", "so", "deut\u00b7lich", "vor\u00b7ge\u00b7stell\u00b7tem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Der, sonder Farben, blos im Scheine von Farben sich", "tokens": ["Der", ",", "son\u00b7der", "Far\u00b7ben", ",", "blos", "im", "Schei\u00b7ne", "von", "Far\u00b7ben", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KON", "NN", "$,", "ADV", "APPRART", "NN", "APPR", "NN", "PRF"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die klare Fluth zeigt meinen Blicken,", "tokens": ["Die", "kla\u00b7re", "Fluth", "zeigt", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich an ihrer Zier erqvicken,", "tokens": ["Die", "sich", "an", "ih\u00b7rer", "Zier", "er\u00b7qvi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Sch\u00f6pfers Himmels und der Erden", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Him\u00b7mels", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kraft, Majest\u00e4t und Allmacht an;", "tokens": ["Kraft", ",", "Ma\u00b7jes\u00b7t\u00e4t", "und", "All\u00b7macht", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Als welcher ja so schnell, wie sie", "tokens": ["Als", "wel\u00b7cher", "ja", "so", "schnell", ",", "wie", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRELS", "ADV", "ADV", "ADJD", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Scheines fl\u00fcchtige Copie", "tokens": ["Des", "Schei\u00b7nes", "fl\u00fcch\u00b7ti\u00b7ge", "Co\u00b7pie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entstehen l\u00e4\u00dft; das Urbild werden,", "tokens": ["Ent\u00b7ste\u00b7hen", "l\u00e4\u00dft", ";", "das", "Ur\u00b7bild", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und es aus Nichts, so schnell entstehen lassen", "tokens": ["Und", "es", "aus", "Nichts", ",", "so", "schnell", "ent\u00b7ste\u00b7hen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIS", "$,", "ADV", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Indem ich, mit dergleichen Dencken, bey diesem Wasser-", "tokens": ["In\u00b7dem", "ich", ",", "mit", "derg\u00b7lei\u00b7chen", "Den\u00b7cken", ",", "bey", "die\u00b7sem", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIS", "VVINF", "$,", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So drehet jemand ungefehr der Wasser-R\u00f6hre Schl\u00fcssel", "tokens": ["So", "dre\u00b7het", "je\u00b7mand", "un\u00b7ge\u00b7fehr", "der", "Was\u00b7ser\u00b7R\u00f6h\u00b7re", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wodurch ich, durch des regen Strahls schnell \u00fcber sich ge-", "tokens": ["Wo\u00b7durch", "ich", ",", "durch", "des", "re\u00b7gen", "Strahls", "schnell", "\u00fc\u00b7ber", "sich", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die sanfte Still\u2019, in neuer Anmuth, schnell unterbrochen", "tokens": ["Die", "sanf\u00b7te", "Still'", ",", "in", "neu\u00b7er", "An\u00b7muth", ",", "schnell", "un\u00b7ter\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie nun, mit zischendem Gemurmel, das rege Wasser", "tokens": ["Wie", "nun", ",", "mit", "zi\u00b7schen\u00b7dem", "Ge\u00b7mur\u00b7mel", ",", "das", "re\u00b7ge", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So deucht mich da\u00df, mit hohlem Tone, mir dieses in die", "tokens": ["So", "deucht", "mich", "da\u00df", ",", "mit", "hoh\u00b7lem", "To\u00b7ne", ",", "mir", "die\u00b7ses", "in", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "PDAT", "APPR", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Die stumme Fluht f\u00e4ngt an zu sprechen,", "tokens": ["Die", "stum\u00b7me", "Fluht", "f\u00e4ngt", "an", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Tropfen, die sich rauschend brechen.", "tokens": ["In", "Trop\u00b7fen", ",", "die", "sich", "rau\u00b7schend", "bre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was sagt sie: ", "tokens": ["Was", "sagt", "sie", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "So dein als meines Sch\u00f6pfers Macht!", "tokens": ["So", "dein", "als", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}