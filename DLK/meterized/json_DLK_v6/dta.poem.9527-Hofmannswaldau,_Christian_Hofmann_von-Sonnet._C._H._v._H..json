{"dta.poem.9527": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gantz traurig/ halb entz\u00fcckt und mit geschr\u00e4nckten f\u00fcssen/", "tokens": ["Gantz", "trau\u00b7rig", "/", "halb", "ent\u00b7z\u00fcckt", "und", "mit", "ge\u00b7schr\u00e4nck\u00b7ten", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADJD", "VVPP", "KON", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sa\u00df Sylvius und sprach: Ich f\u00fchle todes-macht/", "tokens": ["Sa\u00df", "Syl\u00b7vius", "und", "sprach", ":", "Ich", "f\u00fch\u00b7le", "to\u00b7des\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Die so mich in das joch der s\u00fcssen pein gebracht/", "tokens": ["Die", "so", "mich", "in", "das", "joch", "der", "s\u00fcs\u00b7sen", "pein", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wei\u00df ich diesen tag nicht billich zu begr\u00fcssen.", "tokens": ["Die", "wei\u00df", "ich", "die\u00b7sen", "tag", "nicht", "bil\u00b7lich", "zu", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach da\u00df die stunden nicht wie meine thr\u00e4nen fl\u00fcssen!", "tokens": ["Ach", "da\u00df", "die", "stun\u00b7den", "nicht", "wie", "mei\u00b7ne", "thr\u00e4\u00b7nen", "fl\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "ADJA", "PTKNEG", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df das verh\u00e4ngni\u00df nicht mit mir ein ende macht/", "tokens": ["Da\u00df", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "nicht", "mit", "mir", "ein", "en\u00b7de", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil alles \u00fcber mir in einem nun erwacht/", "tokens": ["Weil", "al\u00b7les", "\u00fc\u00b7ber", "mir", "in", "ei\u00b7nem", "nun", "er\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "ART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mein verdammtes licht kan keinen trost geniessen.", "tokens": ["Und", "mein", "ver\u00b7damm\u00b7tes", "licht", "kan", "kei\u00b7nen", "trost", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sa\u00df er und entschlief/ die augen fielen zu/", "tokens": ["So", "sa\u00df", "er", "und", "ent\u00b7schlief", "/", "die", "au\u00b7gen", "fie\u00b7len", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$(", "ART", "NN", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er war ohn allem trost/ er ruht ohn alle ruh.", "tokens": ["Er", "war", "ohn", "al\u00b7lem", "trost", "/", "er", "ruht", "ohn", "al\u00b7le", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er schlief dem auge nach/ es wachten pein und schmertzen/", "tokens": ["Er", "schlief", "dem", "au\u00b7ge", "nach", "/", "es", "wach\u00b7ten", "pein", "und", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$(", "PPER", "VVFIN", "NE", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jhm stie\u00df ein s\u00fc\u00dfer schall die matten augen auf.", "tokens": ["Jhm", "stie\u00df", "ein", "s\u00fc\u00b7\u00dfer", "schall", "die", "mat\u00b7ten", "au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Sylvius getrost und bemme deinen lauf.", "tokens": ["Mein", "Syl\u00b7vius", "ge\u00b7trost", "und", "bem\u00b7me", "dei\u00b7nen", "lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVPP", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nicht suche Lesbien/ sie wohnt in deinem hertzen.", "tokens": ["Nicht", "su\u00b7che", "Les\u00b7bi\u00b7en", "/", "sie", "wohnt", "in", "dei\u00b7nem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}