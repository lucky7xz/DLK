{"dta.poem.7658": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe lang genug gelernt, um ausgelernt", "tokens": ["Ich", "ha\u00b7be", "lang", "ge\u00b7nug", "ge\u00b7lernt", ",", "um", "aus\u00b7ge\u00b7lernt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,", "KOUI", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu haben, doch vom Ziel bin ich noch weit entfernt.", "tokens": ["Zu", "ha\u00b7ben", ",", "doch", "vom", "Ziel", "bin", "ich", "noch", "weit", "ent\u00b7fernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,", "ADV", "APPRART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich lebe nur um noch zu lernen, und begraben", "tokens": ["Ich", "le\u00b7be", "nur", "um", "noch", "zu", "ler\u00b7nen", ",", "und", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "PTKZU", "VVINF", "$,", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird man zuletzt mich doch, ohn' ausgelernt zu haben.", "tokens": ["Wird", "man", "zu\u00b7letzt", "mich", "doch", ",", "ohn'", "aus\u00b7ge\u00b7lernt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "PPER", "ADV", "$,", "KOUI", "VVPP", "PTKZU", "VAINF", "$."], "m