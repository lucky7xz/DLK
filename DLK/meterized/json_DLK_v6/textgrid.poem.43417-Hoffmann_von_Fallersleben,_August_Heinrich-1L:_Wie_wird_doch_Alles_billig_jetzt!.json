{"textgrid.poem.43417": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wird doch Alles billig jetzt!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird doch Alles billig jetzt!", "tokens": ["Wie", "wird", "doch", "Al\u00b7les", "bil\u00b7lig", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wohlfeil Alles noch zuletzt!", "tokens": ["Wie", "wohl\u00b7feil", "Al\u00b7les", "noch", "zu\u00b7letzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kammerherrensch\u00fcssel kann", "tokens": ["Den", "Kam\u00b7mer\u00b7her\u00b7ren\u00b7sch\u00fcs\u00b7sel", "kann"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt leicht gewinnen mancher Mann:", "tokens": ["Jetzt", "leicht", "ge\u00b7win\u00b7nen", "man\u00b7cher", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt braucht man nur zw\u00f6lf Ahnen.", "tokens": ["Jetzt", "braucht", "man", "nur", "zw\u00f6lf", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, du Hoffnungsstrahl!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "Hoff\u00b7nungs\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zw\u00f6lf Ahnen ist gar keine Zahl \u2013", "tokens": ["Zw\u00f6lf", "Ah\u00b7nen", "ist", "gar", "kei\u00b7ne", "Zahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe zw\u00f6lftehalb ja schon,", "tokens": ["Ich", "ha\u00b7be", "zw\u00f6lf\u00b7te\u00b7halb", "ja", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die volle Zahl hat einst mein Sohn:", "tokens": ["Die", "vol\u00b7le", "Zahl", "hat", "einst", "mein", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt braucht man nur zw\u00f6lf Ahnen.", "tokens": ["Jetzt", "braucht", "man", "nur", "zw\u00f6lf", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Vater selig nahm, o weh!", "tokens": ["Mein", "Va\u00b7ter", "se\u00b7lig", "nahm", ",", "o", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein b\u00fcrgerliches Kind zur Eh'.", "tokens": ["Ein", "b\u00fcr\u00b7ger\u00b7li\u00b7ches", "Kind", "zur", "Eh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, h\u00e4tt' er ", "tokens": ["Ach", ",", "h\u00e4tt'", "er"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "So k\u00f6nnt' er sein mein zw\u00f6lfter Ahn \u2013", "tokens": ["So", "k\u00f6nnt'", "er", "sein", "mein", "zw\u00f6lf\u00b7ter", "Ahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt braucht man nur zw\u00f6lf Ahnen.", "tokens": ["Jetzt", "braucht", "man", "nur", "zw\u00f6lf", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}