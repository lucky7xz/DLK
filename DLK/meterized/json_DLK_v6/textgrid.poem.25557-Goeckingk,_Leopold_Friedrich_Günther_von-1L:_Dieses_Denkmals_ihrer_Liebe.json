{"textgrid.poem.25557": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dieses Denkmals ihrer Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses Denkmals ihrer Liebe", "tokens": ["Die\u00b7ses", "Denk\u00b7mals", "ih\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freuet einst sich dann die Welt,", "tokens": ["Freu\u00b7et", "einst", "sich", "dann", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich weinend mich betr\u00fcbe,", "tokens": ["Wenn", "ich", "wei\u00b7nend", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie, ach! der ", "tokens": ["Da\u00df", "sie", ",", "ach", "!", "der"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "ITJ", "$.", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dieses Denkmal aufgestellt!", "tokens": ["Die\u00b7ses", "Denk\u00b7mal", "auf\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}