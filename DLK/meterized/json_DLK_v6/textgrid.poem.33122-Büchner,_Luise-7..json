{"textgrid.poem.33122": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch heitres Leben wogte auf dem Schlosse", "tokens": ["Welch", "heit\u00b7res", "Le\u00b7ben", "wog\u00b7te", "auf", "dem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am andern Tag, erg\u00f6tzlich war's zu schau'n;", "tokens": ["Am", "an\u00b7dern", "Tag", ",", "er\u00b7g\u00f6tz\u00b7lich", "wa\u00b7r's", "zu", "schau'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von allen Seiten kamen hoch zu Rosse", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "ka\u00b7men", "hoch", "zu", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herbei die Ritter und die Edelfrau'n.", "tokens": ["Her\u00b7bei", "die", "Rit\u00b7ter", "und", "die", "E\u00b7del\u00b7frau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Herr von Greyerz stolz, mit reichem Trosse", "tokens": ["Der", "Herr", "von", "Grey\u00b7erz", "stolz", ",", "mit", "rei\u00b7chem", "Tros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von seiner Burg zu Oron kam er traun,", "tokens": ["Von", "sei\u00b7ner", "Burg", "zu", "O\u00b7ron", "kam", "er", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Castellan von Chillon ist erschienen,", "tokens": ["Der", "Cas\u00b7tel\u00b7lan", "von", "Chil\u00b7lon", "ist", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Herr von Tavel selbst ist unter ihnen.", "tokens": ["Und", "Herr", "von", "Ta\u00b7vel", "selbst", "ist", "un\u00b7ter", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "ADV", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Frau Bertha, reich geschm\u00fccket zu dem Feste,", "tokens": ["Frau", "Bert\u00b7ha", ",", "reich", "ge\u00b7schm\u00fc\u00b7cket", "zu", "dem", "Fes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umringt von Pagen in der Halle stand,", "tokens": ["Um\u00b7ringt", "von", "Pa\u00b7gen", "in", "der", "Hal\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit holdem Gru\u00df empfing sie ihre G\u00e4ste,", "tokens": ["Mit", "hol\u00b7dem", "Gru\u00df", "emp\u00b7fing", "sie", "ih\u00b7re", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit heitrem Wort und warmem Druck der Hand;", "tokens": ["Mit", "heit\u00b7rem", "Wort", "und", "war\u00b7mem", "Druck", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch f\u00fcr Herr Corsant hob sie auf das Beste,", "tokens": ["Doch", "f\u00fcr", "Herr", "Cor\u00b7sant", "hob", "sie", "auf", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur Tafel durft' er f\u00fchren sie galant,", "tokens": ["Zur", "Ta\u00b7fel", "durft'", "er", "f\u00fch\u00b7ren", "sie", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als Bote ihres Gatten, der bestellet", "tokens": ["Als", "Bo\u00b7te", "ih\u00b7res", "Gat\u00b7ten", ",", "der", "be\u00b7stel\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bald seine R\u00fcckkehr, ward er vorgestellet.", "tokens": ["Bald", "sei\u00b7ne", "R\u00fcck\u00b7kehr", ",", "ward", "er", "vor\u00b7ge\u00b7stel\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ging der Becher flei\u00dfig durch die Runde,", "tokens": ["Es", "ging", "der", "Be\u00b7cher", "flei\u00b7\u00dfig", "durch", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neckisch scholl Gel\u00e4chter rings und Wort,", "tokens": ["Und", "ne\u00b7ckisch", "scholl", "Ge\u00b7l\u00e4ch\u00b7ter", "rings", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "NN", "ADV", "KON", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Herr Corsant mu\u00dfte geben manche Kunde", "tokens": ["Herr", "Cor\u00b7sant", "mu\u00df\u00b7te", "ge\u00b7ben", "man\u00b7che", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VMFIN", "VVINF", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Hofe und dem lust'gen Leben dort.", "tokens": ["Vom", "Ho\u00b7fe", "und", "dem", "lust'\u00b7gen", "Le\u00b7ben", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In raschem Flug entschwebte Stund' um Stunde,", "tokens": ["In", "ra\u00b7schem", "Flug", "ent\u00b7schweb\u00b7te", "Stund'", "um", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch keiner r\u00fcckte von der Tafel fort,", "tokens": ["Doch", "kei\u00b7ner", "r\u00fcck\u00b7te", "von", "der", "Ta\u00b7fel", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als rosig schon der Sonne letztes Strahlen", "tokens": ["Als", "ro\u00b7sig", "schon", "der", "Son\u00b7ne", "letz\u00b7tes", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Begann des Sees weites Rund zu malen.", "tokens": ["Be\u00b7gann", "des", "Sees", "wei\u00b7tes", "Rund", "zu", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Der Ritter war der Frohste wohl von allen,", "tokens": ["Der", "Rit\u00b7ter", "war", "der", "Frohs\u00b7te", "wohl", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von seiner Schuld schwieg er wohlweislich schlau;", "tokens": ["Von", "sei\u00b7ner", "Schuld", "schwieg", "er", "wohl\u00b7weis\u00b7lich", "schlau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch sie lie\u00df nicht ein einzig W\u00f6rtlein fallen \u2013", "tokens": ["Auch", "sie", "lie\u00df", "nicht", "ein", "ein\u00b7zig", "W\u00f6rt\u00b7lein", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hat gewi\u00df verzieh'n, die holde Frau \u2013", "tokens": ["Sie", "hat", "ge\u00b7wi\u00df", "ver\u00b7zieh'n", ",", "die", "hol\u00b7de", "Frau", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und seine Blicke oft hin\u00fcber wallen,", "tokens": ["Und", "sei\u00b7ne", "Bli\u00b7cke", "oft", "hin\u00b7\u00fc\u00b7ber", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo strahlt Jolanthens Auge treu und blau,", "tokens": ["Wo", "strahlt", "Jo\u00b7lan\u00b7thens", "Au\u00b7ge", "treu", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu Bertha neigt er sich mit leiser Frage,", "tokens": ["Zu", "Bert\u00b7ha", "neigt", "er", "sich", "mit", "lei\u00b7ser", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie ihm, wer die sch\u00f6ne Jungfrau, sage.", "tokens": ["Da\u00df", "sie", "ihm", ",", "wer", "die", "sch\u00f6\u00b7ne", "Jung\u00b7frau", ",", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PWS", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Frau Bertha sah ihn an mit ernster Miene:", "tokens": ["Frau", "Bert\u00b7ha", "sah", "ihn", "an", "mit", "erns\u00b7ter", "Mie\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbjolanthe ist es, meine Muhme traut;", "tokens": ["\u00bb", "jo\u00b7lan\u00b7the", "ist", "es", ",", "mei\u00b7ne", "Muh\u00b7me", "traut", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum letzten Mal ist sie bei mir erschienen,", "tokens": ["Zum", "letz\u00b7ten", "Mal", "ist", "sie", "bei", "mir", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn bald wird sie dem Himmel angetraut,", "tokens": ["Denn", "bald", "wird", "sie", "dem", "Him\u00b7mel", "an\u00b7ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald wird sie ihm allein nur fromm noch dienen", "tokens": ["Bald", "wird", "sie", "ihm", "al\u00b7lein", "nur", "fromm", "noch", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Kloster d'Orbe als seine keusche Braut.", "tokens": ["Im", "Klos\u00b7ter", "d'\u00b7Or\u00b7be", "als", "sei\u00b7ne", "keu\u00b7sche", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Schwer werd' ich dann die treue Freundin missen,", "tokens": ["Schwer", "werd'", "ich", "dann", "die", "treu\u00b7e", "Freun\u00b7din", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Die ach! auf immer meinem Arm entrissen.\u00ab", "tokens": ["Die", "ach", "!", "auf", "im\u00b7mer", "mei\u00b7nem", "Arm", "ent\u00b7ris\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "XY", "$.", "APPR", "ADV", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbwie, edle Dame,\u00ab rief er mit Erschrecken,", "tokens": ["\u00bb", "wie", ",", "ed\u00b7le", "Da\u00b7me", ",", "\u00ab", "rief", "er", "mit", "Er\u00b7schre\u00b7cken", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "ADJA", "NN", "$,", "$(", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwie k\u00f6nnt Ihr dulden solches Thun f\u00fcrwahr?", "tokens": ["\u00bb", "wie", "k\u00f6nnt", "Ihr", "dul\u00b7den", "sol\u00b7ches", "Thun", "f\u00fcr\u00b7wahr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Schleier sollte neidisch bald verdecken", "tokens": ["Der", "Schlei\u00b7er", "soll\u00b7te", "nei\u00b7disch", "bald", "ver\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies blaue Aug', die edle Stirne klar?", "tokens": ["Dies", "blau\u00b7e", "Aug'", ",", "die", "ed\u00b7le", "Stir\u00b7ne", "klar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sollte sich die Scheere z\u00fcngelnd strecken", "tokens": ["Es", "soll\u00b7te", "sich", "die", "Schee\u00b7re", "z\u00fcn\u00b7gelnd", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach diesem weichen, goldnen Lockenhaar?", "tokens": ["Nach", "die\u00b7sem", "wei\u00b7chen", ",", "gold\u00b7nen", "Lo\u00b7cken\u00b7haar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So holde Rose soll in Schmerz und Trauern", "tokens": ["So", "hol\u00b7de", "Ro\u00b7se", "soll", "in", "Schmerz", "und", "Trau\u00b7ern"], "token_info": ["word", "word", "word",