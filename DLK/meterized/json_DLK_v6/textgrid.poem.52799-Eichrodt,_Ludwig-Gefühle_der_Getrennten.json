{"textgrid.poem.52799": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00fchle der Getrennten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig ist es einsam sein!", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam sitzt man auf der Wache,", "tokens": ["Ein\u00b7sam", "sitzt", "man", "auf", "der", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Vogel auf dem Dache,", "tokens": ["Wie", "der", "Vo\u00b7gel", "auf", "dem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und guckt in die Welt hinein,", "tokens": ["Und", "guckt", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Traurig ist es einsam sein.", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Traurig ist es einsam sein!", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir ausgeh'n oder kommen,", "tokens": ["Wenn", "wir", "aus\u00b7geh'n", "o\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wird kein Gatte wahrgenommen,", "tokens": ["Wird", "kein", "Gat\u00b7te", "wahr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O dies r\u00fchret ungemein,", "tokens": ["O", "dies", "r\u00fch\u00b7ret", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Traurig ist es, einsam sein.", "tokens": ["Trau\u00b7rig", "ist", "es", ",", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurig ist es einsam sein!", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Niesen uns begegnet,", "tokens": ["Wenn", "das", "Nie\u00b7sen", "uns", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist kein Mensch da, der uns segnet,", "tokens": ["Ist", "kein", "Mensch", "da", ",", "der", "uns", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Worte wir uns freu'n,", "tokens": ["Des\u00b7sen", "Wor\u00b7te", "wir", "uns", "freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Traurig ist es, einsam sein.", "tokens": ["Trau\u00b7rig", "ist", "es", ",", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Traurig ist es einsam sein!", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur verbund'ne Seelen tragen", "tokens": ["Nur", "ver\u00b7bun\u00b7d'\u00b7ne", "See\u00b7len", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In den schw\u00fclen Erdentagen", "tokens": ["In", "den", "schw\u00fc\u00b7len", "Er\u00b7den\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichter ihren Sorgenstein,", "tokens": ["Leich\u00b7ter", "ih\u00b7ren", "Sor\u00b7gen\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Traurig ist es einsam sein.", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Traurig ist es einsam sein!", "tokens": ["Trau\u00b7rig", "ist", "es", "ein\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Schlafenden dort drau\u00dfen", "tokens": ["Mit", "den", "Schla\u00b7fen\u00b7den", "dort", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen Lebende nicht hausen,", "tokens": ["K\u00f6n\u00b7nen", "Le\u00b7ben\u00b7de", "nicht", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese m\u00fcssen wieder frei'n,", "tokens": ["Die\u00b7se", "m\u00fcs\u00b7sen", "wie\u00b7der", "frei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Traurig ist es, einsam sein.", "tokens": ["Trau\u00b7rig", "ist", "es", ",", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}