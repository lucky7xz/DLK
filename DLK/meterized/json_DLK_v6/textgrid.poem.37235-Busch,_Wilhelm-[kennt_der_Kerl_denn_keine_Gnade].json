{"textgrid.poem.37235": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[kennt der Kerl denn keine Gnade]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt der Kerl denn keine Gnade?", "tokens": ["Kennt", "der", "Kerl", "denn", "kei\u00b7ne", "Gna\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll er uns mit seiner Suade,", "tokens": ["Soll", "er", "uns", "mit", "sei\u00b7ner", "Sua\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch sein breites Explizieren,", "tokens": ["Durch", "sein", "brei\u00b7tes", "Ex\u00b7pli\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwadronieren, Disputieren,", "tokens": ["Schwad\u00b7ro\u00b7nie\u00b7ren", ",", "Dis\u00b7pu\u00b7tie\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll er uns denn stets genieren", "tokens": ["Soll", "er", "uns", "denn", "stets", "ge\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser s\u00e4uselnde Philister,", "tokens": ["Die\u00b7ser", "s\u00e4u\u00b7seln\u00b7de", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Beim Genu\u00df des edlen Weins?", "tokens": ["Beim", "Ge\u00b7nu\u00df", "des", "ed\u00b7len", "Weins", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Pump ihn an, und pl\u00f6tzlich ist er", "tokens": ["Pump", "ihn", "an", ",", "und", "pl\u00f6tz\u00b7lich", "ist", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kurz und b\u00fcndig wie Glock eins.", "tokens": ["Kurz", "und", "b\u00fcn\u00b7dig", "wie", "Glock", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "NN", "CARD", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}}}}