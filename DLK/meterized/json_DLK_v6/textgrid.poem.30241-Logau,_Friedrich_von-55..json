{"textgrid.poem.30241": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krieg hat der M\u00e4nner Zahl gemindert", "tokens": ["Krieg", "hat", "der", "M\u00e4n\u00b7ner", "Zahl", "ge\u00b7min\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Menschen-Wachsthum sehr verhindert.", "tokens": ["Und", "Men\u00b7schen\u00b7Wachst\u00b7hum", "sehr", "ver\u00b7hin\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Weiber sollt hier Rath zu schaffen,", "tokens": ["Ihr", "Wei\u00b7ber", "sollt", "hier", "Rath", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sinnen recht zusammen raffen", "tokens": ["Die", "Sin\u00b7nen", "recht", "zu\u00b7sam\u00b7men", "raf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und euch fein rund und kurtz erkl\u00e4ren,", "tokens": ["Und", "euch", "fein", "rund", "und", "kurtz", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob ihr stets Zwilling wolt geb\u00e4ren,", "tokens": ["Ob", "ihr", "stets", "Zwil\u00b7ling", "wolt", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sonst oder M\u00e4nnern nicht verargen,", "tokens": ["Sonst", "o\u00b7der", "M\u00e4n\u00b7nern", "nicht", "ver\u00b7ar\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sie nur nicht mit einer kargen.", "tokens": ["Da\u00df", "sie", "nur", "nicht", "mit", "ei\u00b7ner", "kar\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}