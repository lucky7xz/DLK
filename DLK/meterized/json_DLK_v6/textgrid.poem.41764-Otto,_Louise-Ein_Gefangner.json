{"textgrid.poem.41764": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gefangner", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Erzgebirges Kamme", "tokens": ["Auf", "des", "Erz\u00b7ge\u00b7bir\u00b7ges", "Kam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand der Berge freier Sohn,", "tokens": ["Stand", "der", "Ber\u00b7ge", "frei\u00b7er", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hohen Geistes Feuerflamme", "tokens": ["Ho\u00b7hen", "Geis\u00b7tes", "Feu\u00b7er\u00b7flam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien aus seinem Aug' zu lohn,", "tokens": ["Schien", "aus", "sei\u00b7nem", "Aug'", "zu", "lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter goldnen Lockenhaaren", "tokens": ["Un\u00b7ter", "gold\u00b7nen", "Lo\u00b7cken\u00b7haa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00e4nzt auf hoher Stirn geschrieben:", "tokens": ["Gl\u00e4nzt", "auf", "ho\u00b7her", "Stirn", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem Dienst des Ewigwahren", "tokens": ["In", "dem", "Dienst", "des", "E\u00b7wig\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich fest und treu geblieben.", "tokens": ["Bin", "ich", "fest", "und", "treu", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Held vergangner Zeiten", "tokens": ["Wie", "ein", "Held", "ver\u00b7gang\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwert und Leier in dem Arm,", "tokens": ["Schwert", "und", "Lei\u00b7er", "in", "dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr das Recht bereit zu streiten", "tokens": ["F\u00fcr", "das", "Recht", "be\u00b7reit", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen m\u00e4cht'ger Feinde Schwarm,", "tokens": ["Ge\u00b7gen", "m\u00e4cht'\u00b7ger", "Fein\u00b7de", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Liede, mit dem Schwerte,", "tokens": ["Mit", "dem", "Lie\u00b7de", ",", "mit", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Schrift voll hohem Sinne,", "tokens": ["Mit", "der", "Schrift", "voll", "ho\u00b7hem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den Verlassnen ein Gef\u00e4hrte,", "tokens": ["Den", "Ver\u00b7lass\u00b7nen", "ein", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treu und keusch im Dienst der Minne.", "tokens": ["Treu", "und", "keusch", "im", "Dienst", "der", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also auf der Heimat Fluren", "tokens": ["Al\u00b7so", "auf", "der", "Hei\u00b7mat", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt er sinnend her um sich", "tokens": ["Blickt", "er", "sin\u00b7nend", "her", "um", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPR", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei des Fr\u00fchlings ersten Spuren,", "tokens": ["Bei", "des", "Fr\u00fch\u00b7lings", "ers\u00b7ten", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der Winter zagend wich \u2013:", "tokens": ["Da", "der", "Win\u00b7ter", "za\u00b7gend", "wich", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbein Gefangner!\u00ab h\u00f6rt man fl\u00fcstern,", "tokens": ["\u00bb", "ein", "Ge\u00b7fang\u00b7ner", "!", "\u00ab", "h\u00f6rt", "man", "fl\u00fcs\u00b7tern", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei's auch nur von seinem H\u00fcter,", "tokens": ["Sei's", "auch", "nur", "von", "sei\u00b7nem", "H\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder von des Waldes R\u00fcstern,", "tokens": ["O\u00b7der", "von", "des", "Wal\u00b7des", "R\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihn gr\u00fc\u00dfen als Gebieter.", "tokens": ["Die", "ihn", "gr\u00fc\u00b7\u00dfen", "als", "Ge\u00b7bie\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Gefangner schon seit Jahren", "tokens": ["Ein", "Ge\u00b7fang\u00b7ner", "schon", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verurteilt jahrelang,", "tokens": ["Und", "ver\u00b7ur\u00b7teilt", "jah\u00b7re\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil, die Freiheit zu bewahren,", "tokens": ["Weil", ",", "die", "Frei\u00b7heit", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er das Schwert im Kampfe schwang \u2013", "tokens": ["Er", "das", "Schwert", "im", "Kamp\u00b7fe", "schwang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun heut von seiner Kette", "tokens": ["Und", "nun", "heut", "von", "sei\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den ", "tokens": ["Auf", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Zu des Vaters Sterbebette", "tokens": ["Zu", "des", "Va\u00b7ters", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gab sein W\u00e4chter ihm Geleit.", "tokens": ["Gab", "sein", "W\u00e4ch\u00b7ter", "ihm", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Horch! ihn gr\u00fc\u00dft der Freiheit Lerche,", "tokens": ["Horch", "!", "ihn", "gr\u00fc\u00dft", "der", "Frei\u00b7heit", "Ler\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von fern er nur geh\u00f6rt,", "tokens": ["Die", "von", "fern", "er", "nur", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fc\u00dfend stehn die alten Berge", "tokens": ["Gr\u00fc\u00b7\u00dfend", "stehn", "die", "al\u00b7ten", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Heimat hoch verehrt.", "tokens": ["Sei\u00b7ner", "Hei\u00b7mat", "hoch", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem dunklen Tannenwalde", "tokens": ["In", "dem", "dunk\u00b7len", "Tan\u00b7nen\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00f6nt's im Lispeln und im Rauschen:", "tokens": ["T\u00f6nt's", "im", "Lis\u00b7peln", "und", "im", "Rau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh! noch bin ich ganz der alte,", "tokens": ["Sieh", "!", "noch", "bin", "ich", "ganz", "der", "al\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Willst mich nicht dem Kerker tauschen?", "tokens": ["Willst", "mich", "nicht", "dem", "Ker\u00b7ker", "tau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl bekannt hier alle Wege,", "tokens": ["Wohl", "be\u00b7kannt", "hier", "al\u00b7le", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manch ein sichrer Zufluchtsort;", "tokens": ["Manch", "ein", "sich\u00b7rer", "Zu\u00b7fluch\u00b7tsort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fliehe auf vertrautem Stege,", "tokens": ["Flie\u00b7he", "auf", "ver\u00b7trau\u00b7tem", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flieh' von Deinem W\u00e4chter fort!", "tokens": ["Flieh'", "von", "Dei\u00b7nem", "W\u00e4ch\u00b7ter", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also rufen tausend Laute", "tokens": ["Al\u00b7so", "ru\u00b7fen", "tau\u00b7send", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lockend in das freie Leben", "tokens": ["Lo\u00b7ckend", "in", "das", "frei\u00b7e", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem, der nur den Kerker schaute,", "tokens": ["Dem", ",", "der", "nur", "den", "Ker\u00b7ker", "schau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der noch lang ihn soll umgeben.", "tokens": ["Der", "noch", "lang", "ihn", "soll", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "VMFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fra\u00df der Gram am Vaterherzen", "tokens": ["Fra\u00df", "der", "Gram", "am", "Va\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den langgefangnen Sohn,", "tokens": ["Um", "den", "lang\u00b7ge\u00b7fang\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er stirbt in Sehnsuchtsschmerzen,", "tokens": ["Da\u00df", "er", "stirbt", "in", "Sehn\u00b7suchts\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Tod nicht auch bedrohn", "tokens": ["Wird", "der", "Tod", "nicht", "auch", "be\u00b7drohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seiner Mutter teures Leben,", "tokens": ["Sei\u00b7ner", "Mut\u00b7ter", "teu\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er fort im Kerker schmachtet?", "tokens": ["Wenn", "er", "fort", "im", "Ker\u00b7ker", "schmach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird die Braut \u2013 er sieht sie schweben,", "tokens": ["Wird", "die", "Braut", "\u2013", "er", "sieht", "sie", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Winken \u2013 und sein Blick umnachtet.", "tokens": ["Win\u00b7ken", "\u2013", "und", "sein", "Blick", "um\u00b7nach\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und er tr\u00e4umt von s\u00fc\u00dfen Wonnen,", "tokens": ["Und", "er", "tr\u00e4umt", "von", "s\u00fc\u00b7\u00dfen", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht die zitternde Gestalt,", "tokens": ["Sieht", "die", "zit\u00b7tern\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus ihrer Augen Bronnen", "tokens": ["Wie", "aus", "ih\u00b7rer", "Au\u00b7gen", 