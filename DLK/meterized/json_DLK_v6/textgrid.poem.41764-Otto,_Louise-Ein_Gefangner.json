, "wie\u00b7der", "in", "Dein", "Haus", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Habe Dank f\u00fcr diese Stunden,", "tokens": ["Ha\u00b7be", "Dank", "f\u00fcr", "die\u00b7se", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zum Troste mir geworden!\u00ab", "tokens": ["Die", "zum", "Tros\u00b7te", "mir", "ge\u00b7wor\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VAPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Frei und stolz und ungebunden", "tokens": ["Frei", "und", "stolz", "und", "un\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kehrt er zu des Kerkers Pforten.", "tokens": ["Kehrt", "er", "zu", "des", "Ker\u00b7kers", "Pfor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}