{"textgrid.poem.60458": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind geboren, die Mutter tot", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Mutterliebe hat dem Leben", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mut\u00b7ter\u00b7lie\u00b7be", "hat", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Opfer hier, ein bl\u00fchend Kind gegeben,", "tokens": ["Ein", "Op\u00b7fer", "hier", ",", "ein", "bl\u00fc\u00b7hend", "Kind", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vertrauend und mit innigstem Verlangen,", "tokens": ["Ver\u00b7trau\u00b7end", "und", "mit", "in\u00b7nigs\u00b7tem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df alle G\u00f6tter huldvoll es empfangen;", "tokens": ["Da\u00df", "al\u00b7le", "G\u00f6t\u00b7ter", "huld\u00b7voll", "es", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch als sie weihend will den Segen sprechen", "tokens": ["Doch", "als", "sie", "wei\u00b7hend", "will", "den", "Se\u00b7gen", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihres Herzens hei\u00dfem \u00dcberwallen,", "tokens": ["In", "ih\u00b7res", "Her\u00b7zens", "hei\u00b7\u00dfem", "\u00dc\u00b7berw\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Lie\u00df ihre Hand, vor Freude zitternd, fallen", "tokens": ["Lie\u00df", "ih\u00b7re", "Hand", ",", "vor", "Freu\u00b7de", "zit\u00b7ternd", ",", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Mutterleib, die Opferschale brechen.", "tokens": ["Den", "Mut\u00b7ter\u00b7leib", ",", "die", "Op\u00b7fer\u00b7scha\u00b7le", "bre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}