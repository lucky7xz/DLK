{"textgrid.poem.51006": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsj\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4sset sich mit aller Kraft", "tokens": ["Es", "l\u00e4s\u00b7set", "sich", "mit", "al\u00b7ler", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Horn im Walde h\u00f6ren;", "tokens": ["Ein", "Horn", "im", "Wal\u00b7de", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich krieg ein altes Rohr beim Schaft", "tokens": ["Ich", "krieg", "ein", "al\u00b7tes", "Rohr", "beim", "Schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlendre in die F\u00f6hren.", "tokens": ["Und", "schlend\u00b7re", "in", "die", "F\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wald, der macht mir vielen Spa\u00df,", "tokens": ["Der", "Wald", ",", "der", "macht", "mir", "vie\u00b7len", "Spa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er flunkert in der Sonnen;", "tokens": ["Er", "flun\u00b7kert", "in", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Reif hat wie mit Jungfernglas", "tokens": ["Der", "Reif", "hat", "wie", "mit", "Jung\u00b7fern\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nadeln \u00fcbersponnen.", "tokens": ["Die", "Na\u00b7deln", "\u00fc\u00b7bers\u00b7pon\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da h\u00fcpft ein junger Has daher", "tokens": ["Da", "h\u00fcpft", "ein", "jun\u00b7ger", "Has", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spielt vor mir im Grase;", "tokens": ["Und", "spielt", "vor", "mir", "im", "Gra\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich brenne so von ungef\u00e4hr", "tokens": ["Ich", "bren\u00b7ne", "so", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schrot ihm auf die Nase.", "tokens": ["Mein", "Schrot", "ihm", "auf", "die", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Es ist, als schrie er: \u00bbGott vergelt's!\u00ab", "tokens": ["Es", "ist", ",", "als", "schrie", "er", ":", "\u00bb", "Gott", "ver\u00b7gelt's", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "$.", "$(", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit kl\u00e4glicher Geb\u00e4rde;", "tokens": ["Mit", "kl\u00e4g\u00b7li\u00b7cher", "Ge\u00b7b\u00e4r\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein rotes Bl\u00fctlein f\u00e4rbt den Pelz", "tokens": ["Sein", "ro\u00b7tes", "Bl\u00fct\u00b7lein", "f\u00e4rbt", "den", "Pelz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht sich in die Erde.", "tokens": ["Und", "macht", "sich", "in", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was stierst du so, du Heidekind,", "tokens": ["Was", "stierst", "du", "so", ",", "du", "Hei\u00b7de\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sterben immer d\u00fcmmer?", "tokens": ["Im", "Ster\u00b7ben", "im\u00b7mer", "d\u00fcm\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du siehst mich, wie die andern sind,", "tokens": ["Du", "siehst", "mich", ",", "wie", "die", "an\u00b7dern", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht besser und nicht schlimmer!", "tokens": ["Nicht", "bes\u00b7ser", "und", "nicht", "schlim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als das H\u00e4slein ausgeschnappt,", "tokens": ["Und", "als", "das", "H\u00e4s\u00b7lein", "aus\u00b7ge\u00b7schnappt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich es heimgetragen \u2013", "tokens": ["Hab", "ich", "es", "heim\u00b7ge\u00b7tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch freilich schon genug gehabt", "tokens": ["Doch", "frei\u00b7lich", "schon", "ge\u00b7nug", "ge\u00b7habt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VAPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Weidmanns Heil und Jagen!", "tokens": ["Von", "Weid\u00b7manns", "Heil", "und", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}