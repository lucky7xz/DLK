{"textgrid.poem.33012": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Was ist's nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Was ist's nicht!", "tokens": ["Das", "Was", "ist's", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "VAFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer etwas kann,", "tokens": ["Wer", "et\u00b7was", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "zwingt sich den spr\u00f6desten Stoff zu Willen,", "tokens": ["zwingt", "sich", "den", "spr\u00f6\u00b7des\u00b7ten", "Stoff", "zu", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "zwingt zu lebendig frischem Quell", "tokens": ["zwingt", "zu", "le\u00b7ben\u00b7dig", "fri\u00b7schem", "Quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKA", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "das Felsgestein, das sonnzergl\u00fchte,", "tokens": ["das", "Fels\u00b7ge\u00b7stein", ",", "das", "sonn\u00b7zer\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "zwingt das verdorrteste Reis am Weg", "tokens": ["zwingt", "das", "ver\u00b7dorr\u00b7tes\u00b7te", "Reis", "am", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "zu Reim und Bl\u00fcte", "tokens": ["zu", "Reim", "und", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "mit blo\u00dfem Wort ... er will und spricht's,", "tokens": ["mit", "blo\u00b7\u00dfem", "Wort", "...", "er", "will", "und", "spricht's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PPER", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "und ... \u00fcberflammt von tausend Sonnen,", "tokens": ["und", "...", "\u00fc\u00b7berf\u00b7lammt", "von", "tau\u00b7send", "Son\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "befreit er eine Welt voll Wonnen", "tokens": ["be\u00b7freit", "er", "ei\u00b7ne", "Welt", "voll", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "aus leerem, d\u00e4mmergrauem Nichts!", "tokens": ["aus", "lee\u00b7rem", ",", "d\u00e4m\u00b7mer\u00b7grau\u00b7em", "Nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Was ist's nicht! Das Wie allein", "tokens": ["Das", "Was", "ist's", "nicht", "!", "Das", "Wie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PWS", "VAFIN", "PTKNEG", "$.", "ART", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wird Kranz und Krone dir verleihn!", "tokens": ["wird", "Kranz", "und", "Kro\u00b7ne", "dir", "ver\u00b7leihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stoff ist nur Stoff, in blinder Haft ...", "tokens": ["Stoff", "ist", "nur", "Stoff", ",", "in", "blin\u00b7der", "Haft", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dein Wille erst wird seine Kraft,", "tokens": ["dein", "Wil\u00b7le", "erst", "wird", "sei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dein Wort erst wird sein Werde!", "tokens": ["dein", "Wort", "erst", "wird", "sein", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist die gleiche Handvoll Erde ...", "tokens": ["Es", "ist", "die", "glei\u00b7che", "Hand\u00b7voll", "Er\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein Gott wird Menschen daraus schaffen,", "tokens": ["ein", "Gott", "wird", "Men\u00b7schen", "da\u00b7raus", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ein St\u00fcmper ... Affen!", "tokens": ["ein", "St\u00fcm\u00b7per", "...", "Af\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}