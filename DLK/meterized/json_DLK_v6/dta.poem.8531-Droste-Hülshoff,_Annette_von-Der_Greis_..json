{"dta.poem.8531": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.57", "ca:0.14", "et:0.14", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allah! la\u00df des Greises Loos", "tokens": ["Al\u00b7lah", "!", "la\u00df", "des", "Grei\u00b7ses", "Loos"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich nicht, des Greises, erleben!", "tokens": ["Mich", "nicht", ",", "des", "Grei\u00b7ses", ",", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus dem Haupte das Haar ihm f\u00e4llt", "tokens": ["Aus", "dem", "Haup\u00b7te", "das", "Haar", "ihm", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und des Bartes k\u00f6stliche Zierde.", "tokens": ["Und", "des", "Bar\u00b7tes", "k\u00f6st\u00b7li\u00b7che", "Zier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach, und Zillah\u2019s liebe Gestalt", "tokens": ["Ach", ",", "und", "Zil\u00b7lah's", "lie\u00b7be", "Ge\u00b7stalt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "NE", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und Zillah\u2019s schwebende Stimme!", "tokens": ["Und", "Zil\u00b7lah's", "schwe\u00b7ben\u00b7de", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kalt und f\u00fchllos st\u00f6\u00dft er\u2019s zur\u00fcck,", "tokens": ["Kalt", "und", "f\u00fchl\u00b7los", "st\u00f6\u00dft", "er's", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie das Riff der Nachtigall T\u00f6ne.", "tokens": ["Wie", "das", "Riff", "der", "Nach\u00b7ti\u00b7gall", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}