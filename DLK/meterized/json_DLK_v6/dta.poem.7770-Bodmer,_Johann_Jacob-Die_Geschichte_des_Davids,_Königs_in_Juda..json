{"dta.poem.7770": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte Jsrael kaum Amaleck vertrieben,", "tokens": ["Es", "hat\u00b7te", "Js\u00b7rael", "kaum", "A\u00b7ma\u00b7leck", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(wiewohl in allem nicht nach Gottes Richtschnur blieben,)", "tokens": ["(", "wie\u00b7wohl", "in", "al\u00b7lem", "nicht", "nach", "Got\u00b7tes", "Richt\u00b7schnur", "blie\u00b7ben", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "APPR", "PIS", "PTKNEG", "APPR", "NN", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als der Philister Heer, schon wiederum mit Macht", "tokens": ["Als", "der", "Phi\u00b7lis\u00b7ter", "Heer", ",", "schon", "wie\u00b7de\u00b7rum", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Juda bey Socho, ein grosses Schrecken bracht.", "tokens": ["In", "Ju\u00b7da", "bey", "So\u00b7cho", ",", "ein", "gros\u00b7ses", "Schre\u00b7cken", "bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie zogen schnell herauf mit vielen Ro\u00df und Wagen,", "tokens": ["Sie", "zo\u00b7gen", "schnell", "her\u00b7auf", "mit", "vie\u00b7len", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Juda sein Geschlecht von Hau\u00df und Hof zu jagen.", "tokens": ["Des", "Ju\u00b7da", "sein", "Ge\u00b7schlecht", "von", "Hau\u00df", "und", "Hof", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Asecka ward es inn, allda ihr Lager war,", "tokens": ["As\u00b7ec\u00b7ka", "ward", "es", "inn", ",", "all\u00b7da", "ihr", "La\u00b7ger", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja Damin bliebe nicht entfreyet der Gefahr.", "tokens": ["Ja", "Da\u00b7min", "blie\u00b7be", "nicht", "ent\u00b7fre\u00b7yet", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PAV", "VVFIN", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Es ahnte ihnen schon, da\u00df aus des Juda Lenden", "tokens": ["Es", "ahn\u00b7te", "ih\u00b7nen", "schon", ",", "da\u00df", "aus", "des", "Ju\u00b7da", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NE", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Held entspringen solt, den Gott selbst w\u00fcrde senden", "tokens": ["Ein", "Held", "ent\u00b7sprin\u00b7gen", "solt", ",", "den", "Gott", "selbst", "w\u00fcr\u00b7de", "sen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "ART", "NN", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu ihrem Ungl\u00fccks-Fall, drum galt\u2019 es Juda mehr,", "tokens": ["Zu", "ih\u00b7rem", "Un\u00b7gl\u00fccks\u00b7Fall", ",", "drum", "galt'", "es", "Ju\u00b7da", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "VVFIN", "PPER", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als andern St\u00e4mmen, weil f\u00fcr die der Ha\u00df zu sehr", "tokens": ["Als", "an\u00b7dern", "St\u00e4m\u00b7men", ",", "weil", "f\u00fcr", "die", "der", "Ha\u00df", "zu", "sehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUS", "APPR", "PRELS", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In der Gaditer Hertz die Wurtzeln l\u00e4ngst gefasset;", "tokens": ["In", "der", "Ga\u00b7di\u00b7ter", "Hertz", "die", "Wurt\u00b7zeln", "l\u00e4ngst", "ge\u00b7fas\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was noch nicht war gesehn, das wurde schon gehasset;", "tokens": ["Was", "noch", "nicht", "war", "ge\u00b7sehn", ",", "das", "wur\u00b7de", "schon", "ge\u00b7has\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VAFIN", "VVPP", "$,", "PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dann Dagon hatte viel von einem Held gesagt,", "tokens": ["Dann", "Da\u00b7gon", "hat\u00b7te", "viel", "von", "ei\u00b7nem", "Held", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Der sie zuschrecken k\u00e4m. Di\u00df hatte es gemacht,", "tokens": ["Der", "sie", "zu\u00b7schre\u00b7cken", "k\u00e4m", ".", "Di\u00df", "hat\u00b7te", "es", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "$.", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie auch, da\u00df Agags Schmach und Saul erlangtes Siegen", "tokens": ["Wie", "auch", ",", "da\u00df", "A\u00b7gags", "Schmach", "und", "Saul", "er\u00b7lang\u00b7tes", "Sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "NN", "NN", "KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch die\u00df bew\u00e4hrte Volck solt wieder unterliegen.", "tokens": ["Durch", "die\u00df", "be\u00b7w\u00e4hr\u00b7te", "Volck", "solt", "wie\u00b7der", "un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Schimpf that ihnen weh, da\u00df der ihr Bundsverwandt", "tokens": ["Der", "Schimpf", "that", "ih\u00b7nen", "weh", ",", "da\u00df", "der", "ihr", "Bunds\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch Benjamins Geschlecht gelegt war in den Sand.", "tokens": ["Durch", "Ben\u00b7ja\u00b7mins", "Ge\u00b7schlecht", "ge\u00b7legt", "war", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gleichwie ein Wolcken-Bruch viel Land und Ort bedecket,", "tokens": ["Gleich\u00b7wie", "ein", "Wol\u00b7cken\u00b7Bruch", "viel", "Land", "und", "Ort", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und eh man sichs versieht, die sichersten erschrecket:", "tokens": ["Und", "eh", "man", "sichs", "ver\u00b7sieht", ",", "die", "si\u00b7chers\u00b7ten", "er\u00b7schre\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PIS", "VVFIN", "$,", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "So gieng es hie auch zu: die M\u00e4chtigen von Gad,", "tokens": ["So", "gieng", "es", "hie", "auch", "zu", ":", "die", "M\u00e4ch\u00b7ti\u00b7gen", "von", "Gad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die waren schon im Land, eh mans erfahren hat.", "tokens": ["Die", "wa\u00b7ren", "schon", "im", "Land", ",", "eh", "mans", "er\u00b7fah\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "PIS", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}