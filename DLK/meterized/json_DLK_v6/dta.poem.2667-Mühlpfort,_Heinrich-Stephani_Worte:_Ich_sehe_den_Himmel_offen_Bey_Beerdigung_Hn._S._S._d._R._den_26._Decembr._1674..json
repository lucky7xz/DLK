{"dta.poem.2667": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stephani Worte:  \n  Ich sehe den Himmel offen/  \n Bey Beerdigung Hn. S. S. d. R. den 26.  \n Decembr. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt seh' ich h\u00f6chst erfreut die Schos des Himmels\noffen/", "tokens": ["Jetzt", "seh'", "ich", "h\u00f6chst", "er\u00b7freut", "die", "Schos", "des", "Him\u00b7mels", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gehe durch mein Grab zu jenem grossen Licht!", "tokens": ["Und", "ge\u00b7he", "durch", "mein", "Grab", "zu", "je\u00b7nem", "gros\u00b7sen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Gnadenreiche Zeit! o seeligstes Verhoffen!", "tokens": ["O", "Gna\u00b7den\u00b7rei\u00b7che", "Zeit", "!", "o", "see\u00b7ligs\u00b7tes", "Ver\u00b7hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jetzt kan ich erst recht sehn nun mir mein Auge bricht.", "tokens": ["Jetzt", "kan", "ich", "erst", "recht", "sehn", "nun", "mir", "mein", "Au\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun kalter Todes-Schwei\u00df auff meinen Gliedern klebet/", "tokens": ["Nun", "kal\u00b7ter", "To\u00b7des\u00b7Schwei\u00df", "auff", "mei\u00b7nen", "Glie\u00b7dern", "kle\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So brennt mein Hertze mehr die Glaubens-Fackel an.", "tokens": ["So", "brennt", "mein", "Hert\u00b7ze", "mehr", "die", "Glau\u00b7bens\u00b7Fa\u00b7ckel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob die verzehrte Lung\u2019 auch kaum mehr Athem hebet/", "tokens": ["Ob", "die", "ver\u00b7zehr\u00b7te", "Lung'", "auch", "kaum", "mehr", "A\u00b7them", "he\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fchrt mich doch GOttes Geist auf einer guten Bahn.", "tokens": ["F\u00fchrt", "mich", "doch", "Got\u00b7tes", "Geist", "auf", "ei\u00b7ner", "gu\u00b7ten", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verlangte Finstern\u00fc\u00df du kanst mich nicht erschrecken/", "tokens": ["Ver\u00b7lang\u00b7te", "Fins\u00b7ter\u00b7n\u00fc\u00df", "du", "kanst", "mich", "nicht", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr Schatten voller Heil/ nehmt hin die Hand-voll Staub/", "tokens": ["Ihr", "Schat\u00b7ten", "vol\u00b7ler", "Heil", "/", "nehmt", "hin", "die", "Han\u00b7dvoll", "Staub", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Stern ist gar nicht schwer so meinen Leib sol decken/", "tokens": ["Der", "Stern", "ist", "gar", "nicht", "schwer", "so", "mei\u00b7nen", "Leib", "sol", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist noch was Fleisch an mir das sey der W\u00fcr mer Raub.", "tokens": ["Ist", "noch", "was", "Fleisch", "an", "mir", "das", "sey", "der", "W\u00fcr", "mer", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "NN", "APPR", "PPER", "PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier wil ich gantz nicht weg. Aus dieser engen H\u00f6le", "tokens": ["Hier", "wil", "ich", "gantz", "nicht", "weg", ".", "Aus", "die\u00b7ser", "en\u00b7gen", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hoff\u2019 ich ins weite Reich des Himmels einzugehn.", "tokens": ["Hoff'", "ich", "ins", "wei\u00b7te", "Reich", "des", "Him\u00b7mels", "ein\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Vorschmack hat schon l\u00e4ngst empfunden meine Seele/", "tokens": ["Den", "Vor\u00b7schmack", "hat", "schon", "l\u00e4ngst", "emp\u00b7fun\u00b7den", "mei\u00b7ne", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn sehnlich sie gew\u00fcnscht f\u00fcr GOttes Stuhl zu stehn.", "tokens": ["Wenn", "sehn\u00b7lich", "sie", "ge\u00b7w\u00fcnscht", "f\u00fcr", "Got\u00b7tes", "Stuhl", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVPP", "APPR", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ists m\u00f6glich da\u00df so scharff die Sterbenden noch sehen?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "so", "scharff", "die", "Ster\u00b7ben\u00b7den", "noch", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ADV", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ja/ Glaubens-Augen sind in letzten Z\u00fcgen klar.", "tokens": ["Ja", "/", "Glau\u00b7bens\u00b7Au\u00b7gen", "sind", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", "klar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "NN", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Versichert Sterbliche/ da\u00df mein unendlich flehen", "tokens": ["Ver\u00b7si\u00b7chert", "Sterb\u00b7li\u00b7che", "/", "da\u00df", "mein", "un\u00b7end\u00b7lich", "fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "NN", "$(", "KOUS", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach einem sanfften Tod vor vielen Jahren war.", "tokens": ["Nach", "ei\u00b7nem", "sanff\u00b7ten", "Tod", "vor", "vie\u00b7len", "Jah\u00b7ren", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich konte mich so sehr nicht in die Welt vergaffen/", "tokens": ["Ich", "kon\u00b7te", "mich", "so", "sehr", "nicht", "in", "die", "Welt", "ver\u00b7gaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ich das ewige geschlagen in den Wind/", "tokens": ["Da\u00df", "ich", "das", "e\u00b7wi\u00b7ge", "ge\u00b7schla\u00b7gen", "in", "den", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wuste schon den Lohn/ den sie pflegt zu verschaffen/", "tokens": ["Ich", "wus\u00b7te", "schon", "den", "Lohn", "/", "den", "sie", "pflegt", "zu", "ver\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Da\u00df Diener ihrer Pracht verblendte Thoren sind.", "tokens": ["Da\u00df", "Die\u00b7ner", "ih\u00b7rer", "Pracht", "ver\u00b7blend\u00b7te", "Tho\u00b7ren", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und freilich sehen wir den Schau-Platz dieser Erden", "tokens": ["Und", "frei\u00b7lich", "se\u00b7hen", "wir", "den", "Schau\u00b7Platz", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit bl\u00f6dem Angesicht und tuncklen Augen an.", "tokens": ["Mit", "bl\u00f6\u00b7dem", "An\u00b7ge\u00b7sicht", "und", "tunck\u00b7len", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sind Gauckler nach der Zeit/ verstellen die Geberden/", "tokens": ["Sind", "Gauck\u00b7ler", "nach", "der", "Zeit", "/", "ver\u00b7stel\u00b7len", "die", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und leben andrer Sinn und Meynung unterthan.", "tokens": ["Und", "le\u00b7ben", "an\u00b7drer", "Sinn", "und", "Mey\u00b7nung", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das allerth\u00f6richste ist da\u00df wir di\u00df nicht achten/", "tokens": ["Das", "al\u00b7lert\u00b7h\u00f6\u00b7richs\u00b7te", "ist", "da\u00df", "wir", "di\u00df", "nicht", "ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOUS", "PPER", "PDS", "PTKNEG", "VVINF", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Was uns zum Untergang den ersten Fallstrick legt;", "tokens": ["Was", "uns", "zum", "Un\u00b7ter\u00b7gang", "den", "ers\u00b7ten", "Fall\u00b7strick", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nach Hoheit/ Ehre/ Stand/ und solchen G\u00fctern trachten/", "tokens": ["Nach", "Ho\u00b7heit", "/", "Eh\u00b7re", "/", "Stand", "/", "und", "sol\u00b7chen", "G\u00fc\u00b7tern", "trach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auff die die Eitelkeit ihr Bildn\u00fc\u00df doch gepregt.", "tokens": ["Auff", "die", "die", "Ei\u00b7tel\u00b7keit", "ihr", "Bild\u00b7n\u00fc\u00df", "doch", "ge\u00b7pregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir meynen/ unserm Witz steh\u2019 auch der Abgrund offen/", "tokens": ["Wir", "mey\u00b7nen", "/", "un\u00b7serm", "Witz", "steh'", "auch", "der", "Ab\u00b7grund", "of\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und den Begier den sey noch Ziel noch Mas gesteckt.", "tokens": ["Und", "den", "Be\u00b7gier", "den", "sey", "noch", "Ziel", "noch", "Mas", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VAFIN", "ADV", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wenn wir nun allhier ein ewig bleiben hoffen/", "tokens": ["Und", "wenn", "wir", "nun", "all\u00b7hier", "ein", "e\u00b7wig", "blei\u00b7ben", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ART", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So kommt der blasse Tod der uns in Sarck hinstreckt.", "tokens": ["So", "kommt", "der", "blas\u00b7se", "Tod", "der", "uns", "in", "Sarck", "hins\u00b7treckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bey solcher Sicherheit denckt niemand an das sterben:", "tokens": ["Bey", "sol\u00b7cher", "Si\u00b7cher\u00b7heit", "denckt", "nie\u00b7mand", "an", "das", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wir hengen K\u00fcnsten nach die meistens sch\u00e4dlich seyn:", "tokens": ["Wir", "hen\u00b7gen", "K\u00fcns\u00b7ten", "nach", "die", "meis\u00b7tens", "sch\u00e4d\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie vielen dient ihr Witz und Wissen zum Verderben?", "tokens": ["Wie", "vie\u00b7len", "dient", "ihr", "Witz", "und", "Wis\u00b7sen", "zum", "Ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Wei\u00dfheit dieser Welt ist ein betr\u00fcglich Schein.", "tokens": ["Die", "Wei\u00df\u00b7heit", "die\u00b7ser", "Welt", "ist", "ein", "be\u00b7tr\u00fcg\u00b7lich", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"t