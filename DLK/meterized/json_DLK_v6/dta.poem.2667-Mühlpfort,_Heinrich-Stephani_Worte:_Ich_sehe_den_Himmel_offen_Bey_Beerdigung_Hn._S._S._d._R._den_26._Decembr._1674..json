:"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Die allersch\u00f6nste Kunst bleibt ein bereiter Tod.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Kunst", "bleibt", "ein", "be\u00b7rei\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der Klugheit Inbegrieff/ der Wissenschafften F\u00fclle", "tokens": ["Der", "Klug\u00b7heit", "In\u00b7be\u00b7grieff", "/", "der", "Wis\u00b7sen\u00b7schaff\u00b7ten", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ist/ wenn die letzte Fahrt gerichtet ist zu GOtt.", "tokens": ["Ist", "/", "wenn", "die", "letz\u00b7te", "Fahrt", "ge\u00b7rich\u00b7tet", "ist", "zu", "Gott", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "ART", "ADJA", "NN", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Da heist der Weisen Schul ein Marck voll Narretheyen/", "tokens": ["Da", "heist", "der", "Wei\u00b7sen", "Schul", "ein", "Marck", "voll", "Nar\u00b7ret\u00b7he\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.50": {"text": "Es schweigt Pythagoras/ und Plato wird ein Thor;", "tokens": ["Es", "schweigt", "Py\u00b7tha\u00b7go\u00b7ras", "/", "und", "Pla\u00b7to", "wird", "ein", "Thor", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "KON", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Da kan nichts heilsames der harte Zeno schreyen/", "tokens": ["Da", "kan", "nichts", "heil\u00b7sa\u00b7mes", "der", "har\u00b7te", "Ze\u00b7no", "schre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJA", "ART", "ADJA", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.52": {"text": "Und Seneca ger\u00e4th zu der verdammten Chor;", "tokens": ["Und", "Se\u00b7ne\u00b7ca", "ge\u00b7r\u00e4\u00b7th", "zu", "der", "ver\u00b7damm\u00b7ten", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.53": {"text": "Was hilfft uns nun die Welt? was hilfft uns dieses Tichten/", "tokens": ["Was", "hilfft", "uns", "nun", "die", "Welt", "?", "was", "hilfft", "uns", "die\u00b7ses", "Tich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Das uns zu Wundern hier f\u00fcr Menschen hat gemacht?", "tokens": ["Das", "uns", "zu", "Wun\u00b7dern", "hier", "f\u00fcr", "Men\u00b7schen", "hat", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Was nutzt es da\u00df man sich l\u00e4st Ehren-Mahl auffrichten/", "tokens": ["Was", "nutzt", "es", "da\u00df", "man", "sich", "l\u00e4st", "Eh\u00b7ren\u00b7Mahl", "auf\u00b7frich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "PIS", "PRF", "VVFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die offt die Nachwelt schilt und mehr der Neid verlacht?", "tokens": ["Die", "offt", "die", "Nach\u00b7welt", "schilt", "und", "mehr", "der", "Neid", "ver\u00b7lacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Es schleust den Himmel zu und macht die H\u00f6llen offen/", "tokens": ["Es", "schleust", "den", "Him\u00b7mel", "zu", "und", "macht", "die", "H\u00f6l\u00b7len", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wo nicht auff bessern Grund die Sterbe-Kunst gebaut:", "tokens": ["Wo", "nicht", "auff", "bes\u00b7sern", "Grund", "die", "Ster\u00b7be\u00b7Kunst", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ach nein! mein Sinn ist nie in solchem Wahn ersoffen/", "tokens": ["Ach", "nein", "!", "mein", "Sinn", "ist", "nie", "in", "sol\u00b7chem", "Wahn", "er\u00b7sof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Ich sag\u2019 es frey mir hat stets f\u00fcr der Welt gegraut.", "tokens": ["Ich", "sag'", "es", "frey", "mir", "hat", "stets", "f\u00fcr", "der", "Welt", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Und solt ich nicht die Welt den S\u00fcnden-Kercker lassen/", "tokens": ["Und", "solt", "ich", "nicht", "die", "Welt", "den", "S\u00fcn\u00b7den\u00b7Ker\u00b7cker", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Den Noth-Stall/ welcher stets hat meinen Leib gekr\u00e4nckt.", "tokens": ["Den", "Noth\u00b7Stall", "/", "wel\u00b7cher", "stets", "hat", "mei\u00b7nen", "Leib", "ge\u00b7kr\u00e4nckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und solt\u2019 ich nicht mit Lust des Himmels Schlo\u00df umbfassen?", "tokens": ["Und", "solt'", "ich", "nicht", "mit", "Lust", "des", "Him\u00b7mels", "Schlo\u00df", "umb\u00b7fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Das Heil und Seeligkeit auch mir Verblichnen schenckt.", "tokens": ["Das", "Heil", "und", "See\u00b7lig\u00b7keit", "auch", "mir", "Ver\u00b7blich\u00b7nen", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Frolocket doch mit mir: ", "tokens": ["Fro\u00b7lo\u00b7cket", "doch", "mit", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.66": {"text": "Mein JEsus nimmet mich zum Himmels-B\u00fcrger an.", "tokens": ["Mein", "Je\u00b7sus", "nim\u00b7met", "mich", "zum", "Him\u00b7mels\u00b7B\u00fcr\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Ach g\u00f6nne mir/ mein Schatz/ den Wechsel/ den ich troffen/", "tokens": ["Ach", "g\u00f6n\u00b7ne", "mir", "/", "mein", "Schatz", "/", "den", "Wech\u00b7sel", "/", "den", "ich", "trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Da\u00df ich aus meinem Bett\u2019 in Himmel steigen kan.", "tokens": ["Da\u00df", "ich", "aus", "mei\u00b7nem", "Bett'", "in", "Him\u00b7mel", "stei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ich sehe mich ins Buch des Lebens eingeschrieben/", "tokens": ["Ich", "se\u00b7he", "mich", "ins", "Buch", "des", "Le\u00b7bens", "ein\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Welch Schreiber auf der Welt schreibt diese Vorschrifft nach:", "tokens": ["Welch", "Schrei\u00b7ber", "auf", "der", "Welt", "schreibt", "die\u00b7se", "Vor\u00b7schrifft", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Dem ich bin treu gewest/ der wil mich ewig lieben/", "tokens": ["Dem", "ich", "bin", "treu", "ge\u00b7west", "/", "der", "wil", "mich", "e\u00b7wig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ADJD", "VVPP", "$(", "ART", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und macht mir ietzt mein Grab zu einem Schlaf-Gemach.", "tokens": ["Und", "macht", "mir", "ietzt", "mein", "Grab", "zu", "ei\u00b7nem", "Schlaf\u00b7Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPE