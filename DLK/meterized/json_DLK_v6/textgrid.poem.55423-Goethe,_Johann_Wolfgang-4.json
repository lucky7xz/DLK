{"textgrid.poem.55423": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4ume leuchtend, B\u00e4ume blendend,", "tokens": ["B\u00e4u\u00b7me", "leuch\u00b7tend", ",", "B\u00e4u\u00b7me", "blen\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberall das S\u00fc\u00dfe spendend,", "tokens": ["\u00dc\u00b7be\u00b7rall", "das", "S\u00fc\u00b7\u00dfe", "spen\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Glanze sich bewegend,", "tokens": ["In", "dem", "Glan\u00b7ze", "sich", "be\u00b7we\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alt und junges Herz erregend \u2013", "tokens": ["Alt", "und", "jun\u00b7ges", "Herz", "er\u00b7re\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solch ein Fest ist uns bescheret,", "tokens": ["Solch", "ein", "Fest", "ist", "uns", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mancher Gaben Schmuck verehret;", "tokens": ["Man\u00b7cher", "Ga\u00b7ben", "Schmuck", "ver\u00b7eh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Staunend schaun wir auf und nieder,", "tokens": ["Stau\u00b7nend", "schaun", "wir", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin und her und immer wieder.", "tokens": ["Hin", "und", "her", "und", "im\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber, F\u00fcrst, wenn dir's begegnet", "tokens": ["A\u00b7ber", ",", "F\u00fcrst", ",", "wenn", "dir's", "be\u00b7geg\u00b7net"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Abend so dich segnet,", "tokens": ["Und", "ein", "A\u00b7bend", "so", "dich", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df als Lichter, da\u00df als Flammen", "tokens": ["Da\u00df", "als", "Lich\u00b7ter", ",", "da\u00df", "als", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "$,", "KOUS", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dir gl\u00e4nzten allzusammen", "tokens": ["Vor", "dir", "gl\u00e4nz\u00b7ten", "all\u00b7zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles, was du ausgerichtet,", "tokens": ["Al\u00b7les", ",", "was", "du", "aus\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle, die sich dir verpflichtet:", "tokens": ["Al\u00b7le", ",", "die", "sich", "dir", "ver\u00b7pflich\u00b7tet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PRF", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit erh\u00f6hten Geistesblicken", "tokens": ["Mit", "er\u00b7h\u00f6h\u00b7ten", "Geis\u00b7tes\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchltest herrliches Entz\u00fccken.", "tokens": ["F\u00fchl\u00b7test", "herr\u00b7li\u00b7ches", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}