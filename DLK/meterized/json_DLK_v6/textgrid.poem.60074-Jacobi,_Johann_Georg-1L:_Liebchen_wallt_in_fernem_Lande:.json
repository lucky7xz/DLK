{"textgrid.poem.60074": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen wallt in fernem Lande:", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen wallt in fernem Lande:", "tokens": ["Lieb\u00b7chen", "wallt", "in", "fer\u00b7nem", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine K\u00fcsse geb' ich dir,", "tokens": ["Mei\u00b7ne", "K\u00fcs\u00b7se", "geb'", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldnes Ringlein! dich zum Pfande", "tokens": ["Gold\u00b7nes", "Rin\u00b7glein", "!", "dich", "zum", "Pfan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df sie, unter K\u00fcssen, mir.", "tokens": ["Lie\u00df", "sie", ",", "un\u00b7ter", "K\u00fcs\u00b7sen", ",", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! da kam sie, leiser, trauter;", "tokens": ["Ach", "!", "da", "kam", "sie", ",", "lei\u00b7ser", ",", "trau\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ein Auge, rein und hold;", "tokens": ["Hatt'", "ein", "Au\u00b7ge", ",", "rein", "und", "hold", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Herz! ein Herz, so lauter,", "tokens": ["Und", "ein", "Herz", "!", "ein", "Herz", ",", "so", "lau\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nes Ringlein! wie dein Gold.", "tokens": ["Sch\u00f6\u00b7nes", "Rin\u00b7glein", "!", "wie", "dein", "Gold", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebchen gab dich mir, und sagte:", "tokens": ["Lieb\u00b7chen", "gab", "dich", "mir", ",", "und", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich schw\u00f6r' es dir: Ich wagte,", "tokens": ["Und", "ich", "schw\u00f6r'", "es", "dir", ":", "Ich", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir zu Gunsten, all mein Blut.", "tokens": ["Dir", "zu", "Guns\u00b7ten", ",", "all", "mein", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Goldnes Ringlein! s\u00fc\u00dfes, liebes!", "tokens": ["Gold\u00b7nes", "Rin\u00b7glein", "!", "s\u00fc\u00b7\u00dfes", ",", "lie\u00b7bes", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machst, da\u00df mir die Sonne scheint;", "tokens": ["Machst", ",", "da\u00df", "mir", "die", "Son\u00b7ne", "scheint", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt ein W\u00f6lkchen oft, ein tr\u00fcbes,", "tokens": ["Kommt", "ein", "W\u00f6lk\u00b7chen", "oft", ",", "ein", "tr\u00fc\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat's in kurzem ausgeweint.", "tokens": ["Hat's", "in", "kur\u00b7zem", "aus\u00b7ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du beginnst die sch\u00f6ne Kette,", "tokens": ["Du", "be\u00b7ginnst", "die", "sch\u00f6\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die man von der Treu empf\u00e4ngt,", "tokens": ["Die", "man", "von", "der", "Treu", "emp\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so fest am Sterbebette", "tokens": ["Die", "so", "fest", "am", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem letzten Ringlein h\u00e4ngt,", "tokens": ["Mit", "dem", "letz\u00b7ten", "Rin\u00b7glein", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo du noch, den matten Blicken", "tokens": ["Wo", "du", "noch", ",", "den", "mat\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimmernd, Wonn' und Hoffnung bist,", "tokens": ["Schim\u00b7mernd", ",", "Wonn'", "und", "Hoff\u00b7nung", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil in Welten voll Entz\u00fccken", "tokens": ["Weil", "in", "Wel\u00b7ten", "voll", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebchen mich hin\u00fcber k\u00fc\u00dft.", "tokens": ["Lieb\u00b7chen", "mich", "hin\u00b7\u00fc\u00b7ber", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}