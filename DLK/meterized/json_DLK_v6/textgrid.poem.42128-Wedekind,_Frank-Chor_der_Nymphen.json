{"textgrid.poem.42128": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Chor der Nymphen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit Jahrtausenden", "tokens": ["Seit", "Jahr\u00b7tau\u00b7sen\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Weilen wir hier", "tokens": ["Wei\u00b7len", "wir", "hier"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "An diesem Teiche.", "tokens": ["An", "die\u00b7sem", "Tei\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Immer das gleiche", "tokens": ["Im\u00b7mer", "das", "glei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schauen wir.", "tokens": ["Schau\u00b7en", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Verlockende Worte", "tokens": ["Ver\u00b7lo\u00b7cken\u00b7de", "Wor\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Von Lust und Freuden", "tokens": ["Von", "Lust", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "F\u00fchrten die Menschen", "tokens": ["F\u00fchr\u00b7ten", "die", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Zu allen Zeiten", "tokens": ["Zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Zu diesem Orte.", "tokens": ["Zu", "die\u00b7sem", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die r\u00f6mischen Frauen", "tokens": ["Die", "r\u00f6\u00b7mi\u00b7schen", "Frau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Wo sind sie geblieben?", "tokens": ["Wo", "sind", "sie", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.13": {"text": "Wir sehn sie nicht mehr.", "tokens": ["Wir", "sehn", "sie", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Hier kamen sie her,", "tokens": ["Hier", "ka\u00b7men", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Um in den lauen", "tokens": ["Um", "in", "den", "lau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Fluten zu lieben.", "tokens": ["Flu\u00b7ten", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch unsre Genossen", "tokens": ["Auch", "uns\u00b7re", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Himmel entsprossen,", "tokens": ["Dem", "Him\u00b7mel", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Oreaden", "tokens": ["Die", "O\u00b7rea\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "In Busch und B\u00e4umen", "tokens": ["In", "Busch", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sie pflegten zu baden", "tokens": ["Sie", "pfleg\u00b7ten", "zu", "ba\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hier und zu tr\u00e4umen.", "tokens": ["Hier", "und", "zu", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die zottigen Faune,", "tokens": ["Die", "zot\u00b7ti\u00b7gen", "Fau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit denen wir liebten,", "tokens": ["Mit", "de\u00b7nen", "wir", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Im Jagen uns \u00fcbten", "tokens": ["Im", "Ja\u00b7gen", "uns", "\u00fcb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "In wilder Laune.", "tokens": ["In", "wil\u00b7der", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Sie alle schwebten,", "tokens": ["Sie", "al\u00b7le", "schweb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Die einst hier lebten,", "tokens": ["Die", "einst", "hier", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Zum Himmel wieder,", "tokens": ["Zum", "Him\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Aus diesen Triften", "tokens": ["Aus", "die\u00b7sen", "Trif\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Empor zu den L\u00fcften,", "tokens": ["Em\u00b7por", "zu", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Zu ihrem Gebieter.", "tokens": ["Zu", "ih\u00b7rem", "Ge\u00b7bie\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}