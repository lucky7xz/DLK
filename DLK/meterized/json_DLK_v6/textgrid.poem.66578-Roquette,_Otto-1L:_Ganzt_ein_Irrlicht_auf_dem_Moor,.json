{"textgrid.poem.66578": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganzt ein Irrlicht auf dem Moor,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganzt ein Irrlicht auf dem Moor,", "tokens": ["Ganzt", "ein", "Irr\u00b7licht", "auf", "dem", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritter Wahn ist fortgerissen:", "tokens": ["Rit\u00b7ter", "Wahn", "ist", "fort\u00b7ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nster Stern, den ich erkor,", "tokens": ["Sch\u00f6ns\u00b7ter", "Stern", ",", "den", "ich", "er\u00b7kor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchte mir in Finsternissen!", "tokens": ["Leuch\u00b7te", "mir", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unke ruft: Kehr' um, jung's Blut,", "tokens": ["Un\u00b7ke", "ruft", ":", "Kehr'", "um", ",", "jung's", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Muhm' ist nicht geheuer!", "tokens": ["Mei\u00b7ne", "Muhm'", "ist", "nicht", "ge\u00b7heu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben kichert's: Hast du Muth,", "tokens": ["Dr\u00fc\u00b7ben", "ki\u00b7chert's", ":", "Hast", "du", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohn' ich's dir, mein Vielgetreuer!", "tokens": ["Lohn'", "ich's", "dir", ",", "mein", "Viel\u00b7ge\u00b7treu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIS", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wild Gel\u00e4chter, Nebeldunst \u2013", "tokens": ["Wild", "Ge\u00b7l\u00e4ch\u00b7ter", ",", "Ne\u00b7bel\u00b7dunst", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritter Wahn schrei't auf im Sumpfe.", "tokens": ["Rit\u00b7ter", "Wahn", "schrei't", "auf", "im", "Sump\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unke ruft: Sag du von Gunst,", "tokens": ["Un\u00b7ke", "ruft", ":", "Sag", "du", "von", "Gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommst du weg mit nassem Strumpfe!", "tokens": ["Kommst", "du", "weg", "mit", "nas\u00b7sem", "Strump\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}