{"textgrid.poem.39711": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "[1]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Thau auch gl\u00e4nzt in Blumenkelch verh\u00fcllt,", "tokens": ["Wie", "Thau", "auch", "gl\u00e4nzt", "in", "Blu\u00b7men\u00b7kelch", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich n\u00e4hrt von seiner Wiege s\u00fc\u00dfen D\u00fcften,", "tokens": ["Sich", "n\u00e4hrt", "von", "sei\u00b7ner", "Wie\u00b7ge", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann leise ihrer Bl\u00e4tter Nacht entschwillt,", "tokens": ["Dann", "lei\u00b7se", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "Nacht", "ent\u00b7schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entf\u00fchret von des Abends freien L\u00fcften,", "tokens": ["Ent\u00b7f\u00fch\u00b7ret", "von", "des", "A\u00b7bends", "frei\u00b7en", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So strahlend von des ewgen Feuers Bild,", "tokens": ["So", "strah\u00b7lend", "von", "des", "ew\u00b7gen", "Feu\u00b7ers", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Perlenthau in dunkler Erde Kl\u00fcften,", "tokens": ["Ein", "Per\u00b7lent\u00b7hau", "in", "dunk\u00b7ler", "Er\u00b7de", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Novalis leise ihrem Schoos entquillt;", "tokens": ["No\u00b7va\u00b7lis", "lei\u00b7se", "ih\u00b7rem", "Schoos", "ent\u00b7quillt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gesellt sich zu den freien Himmelsl\u00fcften.", "tokens": ["Ge\u00b7sellt", "sich", "zu", "den", "frei\u00b7en", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie tragen ihn auf leichtbeschwingten Woogen", "tokens": ["Sie", "tra\u00b7gen", "ihn", "auf", "leicht\u00b7be\u00b7schwing\u00b7ten", "Woo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Geleiten ihn zu Iris Farbenbogen", "tokens": ["Ge\u00b7lei\u00b7ten", "ihn", "zu", "I\u00b7ris", "Far\u00b7ben\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zu der dunkel gl\u00fch'nden Abendr\u00f6the.", "tokens": ["Und", "zu", "der", "dun\u00b7kel", "gl\u00fch'n\u00b7den", "A\u00b7bend\u00b7r\u00f6\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er badet sich in ihren heil'gen Fluthen,", "tokens": ["Er", "ba\u00b7det", "sich", "in", "ih\u00b7ren", "heil'\u00b7gen", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergehet wonnig mit in ihren Gluthen", "tokens": ["Ver\u00b7ge\u00b7het", "won\u00b7nig", "mit", "in", "ih\u00b7ren", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ernster, heil'ger sieht die Abendr\u00f6the.", "tokens": ["Und", "erns\u00b7ter", ",", "heil'\u00b7ger", "sieht", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}