{"textgrid.poem.55338": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Modernes", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie aber kann sich Hans van Eyck", "tokens": ["\u00bb", "wie", "a\u00b7ber", "kann", "sich", "Hans", "van", "Eyck"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "ADV", "VMFIN", "PRF", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Phidias nur messen?\u00ab", "tokens": ["Mit", "Phi\u00b7di\u00b7as", "nur", "mes\u00b7sen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft, so lehr ich, alsogleich", "tokens": ["Ihr", "m\u00fc\u00dft", ",", "so", "lehr", "ich", ",", "al\u00b7so\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen um den andern vergessen.", "tokens": ["Ei\u00b7nen", "um", "den", "an\u00b7dern", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn w\u00e4rt ihr stets bei ", "tokens": ["Denn", "w\u00e4rt", "ihr", "stets", "bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie k\u00f6nntet ihr noch immer lieben?", "tokens": ["Wie", "k\u00f6nn\u00b7tet", "ihr", "noch", "im\u00b7mer", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist die Kunst, das ist die Welt,", "tokens": ["Das", "ist", "die", "Kunst", ",", "das", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df eins ums andere gef\u00e4llt.", "tokens": ["Da\u00df", "eins", "ums", "an\u00b7de\u00b7re", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}