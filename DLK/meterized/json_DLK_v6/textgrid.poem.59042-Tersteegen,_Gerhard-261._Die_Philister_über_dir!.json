{"textgrid.poem.59042": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "261. Die Philister \u00fcber dir!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, nicht zu weit, ach, nicht zu nah!", "tokens": ["Ach", ",", "nicht", "zu", "weit", ",", "ach", ",", "nicht", "zu", "nah", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "PTKA", "ADJD", "$,", "ITJ", "$,", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trau nichts der argen Delila!", "tokens": ["Trau", "nichts", "der", "ar\u00b7gen", "De\u00b7li\u00b7la", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bleib stehn im Geist, gefa\u00dft und munter,", "tokens": ["Bleib", "stehn", "im", "Geist", ",", "ge\u00b7fa\u00dft", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sink nicht in die Natur hinunter!", "tokens": ["Sink", "nicht", "in", "die", "Na\u00b7tur", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}