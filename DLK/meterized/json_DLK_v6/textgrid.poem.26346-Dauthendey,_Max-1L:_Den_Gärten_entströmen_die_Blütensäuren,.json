{"textgrid.poem.26346": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den G\u00e4rten entstr\u00f6men die Bl\u00fctens\u00e4uren,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den G\u00e4rten entstr\u00f6men die Bl\u00fctens\u00e4uren,", "tokens": ["Den", "G\u00e4r\u00b7ten", "ent\u00b7str\u00f6\u00b7men", "die", "Bl\u00fc\u00b7ten\u00b7s\u00e4u\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Blumen und Grillen ihre Liebe beteuren,", "tokens": ["Die", "Blu\u00b7men", "und", "Gril\u00b7len", "ih\u00b7re", "Lie\u00b7be", "be\u00b7teu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es schwellen die Sterne wie silberne Fr\u00fcchte,", "tokens": ["Es", "schwel\u00b7len", "die", "Ster\u00b7ne", "wie", "sil\u00b7ber\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es wachsen wie Qualen die hei\u00dfen S\u00fcchte.", "tokens": ["Es", "wach\u00b7sen", "wie", "Qua\u00b7len", "die", "hei\u00b7\u00dfen", "S\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mir fiebert das Heimweh, ich kann's nicht mehr tragen,", "tokens": ["Mir", "fie\u00b7bert", "das", "Heim\u00b7weh", ",", "ich", "kann's", "nicht", "mehr", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich m\u00f6chte den Sprung zum Tode wagen.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "den", "Sprung", "zum", "To\u00b7de", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}