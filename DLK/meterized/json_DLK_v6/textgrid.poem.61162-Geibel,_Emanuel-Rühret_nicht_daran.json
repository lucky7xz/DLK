{"textgrid.poem.61162": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fchret nicht daran", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo still ein Herz voll Liebe gl\u00fcht", "tokens": ["Wo", "still", "ein", "Herz", "voll", "Lie\u00b7be", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O r\u00fchret, r\u00fchret nicht daran!", "tokens": ["O", "r\u00fch\u00b7ret", ",", "r\u00fch\u00b7ret", "nicht", "da\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Gottesfunken l\u00f6scht nicht aus!", "tokens": ["Den", "Got\u00b7tes\u00b7fun\u00b7ken", "l\u00f6scht", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrwahr, es ist nicht wohlgetan.", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "ist", "nicht", "wohl\u00b7ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn's irgend auf dem Erdenrund", "tokens": ["Wenn's", "ir\u00b7gend", "auf", "dem", "Er\u00b7den\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unentweihtes Pl\u00e4tzchen gibt,", "tokens": ["Ein", "un\u00b7ent\u00b7weih\u00b7tes", "Pl\u00e4tz\u00b7chen", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist's ein junges Menschenherz,", "tokens": ["So", "ist's", "ein", "jun\u00b7ges", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das fromm zum ersten Male liebt.", "tokens": ["Das", "fromm", "zum", "ers\u00b7ten", "Ma\u00b7le", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O g\u00f6nnet ihm den Fr\u00fchlingstraum,", "tokens": ["O", "g\u00f6n\u00b7net", "ihm", "den", "Fr\u00fch\u00b7lings\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dem's voll ros'ger Bl\u00fcten steht!", "tokens": ["In", "dem's", "voll", "ros'\u00b7ger", "Bl\u00fc\u00b7ten", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wi\u00dft nicht, welch ein Paradies", "tokens": ["Ihr", "wi\u00dft", "nicht", ",", "welch", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diesem Traum verloren geht.", "tokens": ["Mit", "die\u00b7sem", "Traum", "ver\u00b7lo\u00b7ren", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es brach schon manch ein starkes Herz,", "tokens": ["Es", "brach", "schon", "manch", "ein", "star\u00b7kes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da man sein Lieben ihm entri\u00df,", "tokens": ["Da", "man", "sein", "Lie\u00b7ben", "ihm", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manches duldend wandte sich", "tokens": ["Und", "man\u00b7ches", "dul\u00b7dend", "wand\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVPP", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ward voll Ha\u00df und Finsternis;", "tokens": ["Und", "ward", "voll", "Ha\u00df", "und", "Fins\u00b7ter\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und manches, das sich blutend schlo\u00df,", "tokens": ["Und", "man\u00b7ches", ",", "das", "sich", "blu\u00b7tend", "schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schrie laut nach Lust in seiner Not", "tokens": ["Schrie", "laut", "nach", "Lust", "in", "sei\u00b7ner", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warf sich in den Staub der Welt;", "tokens": ["Und", "warf", "sich", "in", "den", "Staub", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6ne Gott in ihm war tot.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Gott", "in", "ihm", "war", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann weint ihr wohl und klagt euch an;", "tokens": ["Dann", "weint", "ihr", "wohl", "und", "klagt", "euch", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch keine Tr\u00e4ne hei\u00dfer Reu'", "tokens": ["Doch", "kei\u00b7ne", "Tr\u00e4\u00b7ne", "hei\u00b7\u00dfer", "Reu'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht eine welke Rose bl\u00fchn,", "tokens": ["Macht", "ei\u00b7ne", "wel\u00b7ke", "Ro\u00b7se", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckt ein totes Herz aufs neu.", "tokens": ["Er\u00b7weckt", "ein", "to\u00b7tes", "Herz", "aufs", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}