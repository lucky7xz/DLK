{"textgrid.poem.64615": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "242. An Verschiedene", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob G** zu Folge ", "tokens": ["Ob", "G", "*", "*", "zu", "Fol\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["KOUS", "NE", "XY", "XY", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die beste Welt mit Recht verflucht,", "tokens": ["Die", "bes\u00b7te", "Welt", "mit", "Recht", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist noch unentschieden.", "tokens": ["Das", "ist", "noch", "un\u00b7ent\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch ist er, seit ihn R**'s Hand", "tokens": ["Doch", "ist", "er", ",", "seit", "ihn", "R", "*", "*", "'s", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "PPER", "XY", "XY", "XY", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Frau und Pastorat verband,", "tokens": ["Mit", "Frau", "und", "Pas\u00b7to\u00b7rat", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Ob ", "tokens": ["Ob"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So klein ist, als ihn H** macht,", "tokens": ["So", "klein", "ist", ",", "als", "ihn", "H", "*", "*", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "XY", "XY", "XY", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das la\u00df ich unentschieden.", "tokens": ["Das", "la\u00df", "ich", "un\u00b7ent\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch spr\u00e4che Leibnitz irgendwo:", "tokens": ["Doch", "spr\u00e4\u00b7che", "Leib\u00b7nitz", "ir\u00b7gend\u00b7wo", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Heber flie\u00dft im Vacuo,", "tokens": ["Der", "He\u00b7ber", "flie\u00dft", "im", "Va\u00b7cuo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r' H** es zufrieden.", "tokens": ["W\u00e4r'", "H", "*", "*", "es", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "XY", "XY", "XY", "PPER", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngst foderte wohl ", "tokens": ["L\u00e4ngst", "fo\u00b7der\u00b7te", "wohl"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was man ihm jetzo erst gew\u00e4hrt:", "tokens": ["Was", "man", "ihm", "jet\u00b7zo", "erst", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das la\u00df ich unentschieden.", "tokens": ["Das", "la\u00df", "ich", "un\u00b7ent\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man es gleichwohl sp\u00e4t erfand,", "tokens": ["Da\u00df", "man", "es", "gleich\u00b7wohl", "sp\u00e4t", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihn nicht hin ans Meer", "tokens": ["Und", "ihn", "nicht", "hin", "ans", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das sind wir wohl zufrieden.", "tokens": ["Das", "sind", "wir", "wohl", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob D** die Woche sechsmal flucht,", "tokens": ["Ob", "D", "*", "*", "die", "Wo\u00b7che", "sechs\u00b7mal", "flucht", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "XY", "XY", "XY", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihn der Bursch zu fr\u00fch besucht,", "tokens": ["Da\u00df", "ihn", "der", "Bursch", "zu", "fr\u00fch", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das la\u00df ich unentschieden.", "tokens": ["Das", "la\u00df", "ich", "un\u00b7ent\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um vier Uhr Sonntags aufzustehn,", "tokens": ["Um", "vier", "Uhr", "Sonn\u00b7tags", "auf\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Sch\u00f6nen auf das Land zu gehn,", "tokens": ["Mit", "Sch\u00f6\u00b7nen", "auf", "das", "Land", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist er wohl zufrieden.", "tokens": ["Das", "ist", "er", "wohl", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}