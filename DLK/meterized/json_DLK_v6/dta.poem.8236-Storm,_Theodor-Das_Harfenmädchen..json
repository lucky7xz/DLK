{"dta.poem.8236": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Harfenm\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Da warst du gar zierlich und jung,", "tokens": ["Da", "warst", "du", "gar", "zier\u00b7lich", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ein s\u00fc\u00df' schwarz\u00e4ugiges Dirnlein,", "tokens": ["Ein", "s\u00fc\u00df'", "schwarz\u00b7\u00e4u\u00b7gi\u00b7ges", "Dirn\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Liebe verst\u00e4ndig genung.", "tokens": ["Zur", "Lie\u00b7be", "ver\u00b7st\u00e4n\u00b7dig", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wenn dir die Mutter zu singen", "tokens": ["Und", "wenn", "dir", "die", "Mut\u00b7ter", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Harfe zu spielen gebot,", "tokens": ["Und", "Har\u00b7fe", "zu", "spie\u00b7len", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So scheutest du dich vor den Leuten", "tokens": ["So", "scheu\u00b7test", "du", "dich", "vor", "den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klagtest mir heimlich die Noth.", "tokens": ["Und", "klag\u00b7test", "mir", "heim\u00b7lich", "die", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "\u201ewann treff ich dich wieder und wo doch?\u201c \u2014", "tokens": ["\u201e", "wann", "treff", "ich", "dich", "wie\u00b7der", "und", "wo", "doch", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "KON", "PWAV", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eam Schlosse, wenn's dunkel ist.\u201c", "tokens": ["\u201e", "am", "Schlos\u00b7se", ",", "wenn's", "dun\u00b7kel", "ist", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "$,", "KOUS", "ADJD", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Abends bin ich gekommen", "tokens": ["Und", "A\u00b7bends", "bin", "ich", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und habe dich fr\u00f6hlich gek\u00fc\u00dft.", "tokens": ["Und", "ha\u00b7be", "dich", "fr\u00f6h\u00b7lich", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sind sieben Jahr vergangen,", "tokens": ["Sind", "sie\u00b7ben", "Jahr", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich dich nicht gesehn;", "tokens": ["Da\u00df", "ich", "dich", "nicht", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bleich doch sind deine Wangen,", "tokens": ["Wie", "bleich", "doch", "sind", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und waren so bl\u00fchend und sch\u00f6n!", "tokens": ["Und", "wa\u00b7ren", "so", "bl\u00fc\u00b7hend", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Wie greifst du so keck in die Saiten", "tokens": ["Wie", "greifst", "du", "so", "keck", "in", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schaust und \u00e4ugelst umher!", "tokens": ["Und", "schaust", "und", "\u00e4u\u00b7gelst", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das sind die kindlich scheuen,", "tokens": ["Das", "sind", "die", "kind\u00b7lich", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die leuchtenden Augen nicht mehr.", "tokens": ["Die", "leuch\u00b7ten\u00b7den", "Au\u00b7gen", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Doch kann ich den Blick nicht wenden,", "tokens": ["Doch", "kann", "ich", "den", "Blick", "nicht", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du einst so reizende Maid;", "tokens": ["Du", "einst", "so", "rei\u00b7zen\u00b7de", "Maid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir ist, als schaut' ich hin\u00fcber", "tokens": ["Mir", "ist", ",", "als", "schaut'", "ich", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tief, tief in vergangene Zeit.", "tokens": ["Tief", ",", "tief", "in", "ver\u00b7gan\u00b7ge\u00b7ne", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}