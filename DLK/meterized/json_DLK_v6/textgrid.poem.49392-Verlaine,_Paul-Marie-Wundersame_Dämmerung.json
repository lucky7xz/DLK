{"textgrid.poem.49392": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Wundersame D\u00e4mmerung", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnerung in D\u00e4mmerlicht vergl\u00fchend", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "in", "D\u00e4m\u00b7mer\u00b7licht", "ver\u00b7gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zittert und loht am fernen Himmelsrand", "tokens": ["Zit\u00b7tert", "und", "loht", "am", "fer\u00b7nen", "Him\u00b7mels\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Hoffnung, die geheimnisvoll bald fliehend", "tokens": ["Der", "Hoff\u00b7nung", ",", "die", "ge\u00b7heim\u00b7nis\u00b7voll", "bald", "flie\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald wachsend flammt, wie eine Scheidewand.", "tokens": ["Bald", "wach\u00b7send", "flammt", ",", "wie", "ei\u00b7ne", "Schei\u00b7de\u00b7wand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie mancher Blume farbenbunt Gewand,", "tokens": ["Wie", "man\u00b7cher", "Blu\u00b7me", "far\u00b7ben\u00b7bunt", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Dalie, Tulpe, Lilie erbl\u00fchend,", "tokens": ["Wie", "Da\u00b7lie", ",", "Tul\u00b7pe", ",", "Li\u00b7lie", "er\u00b7bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Ein Gitter rings umrankend und umziehend", "tokens": ["Ein", "Git\u00b7ter", "rings", "um\u00b7ran\u00b7kend", "und", "um\u00b7zie\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit gift'gem Hauch, der all mein Wesen bannt;", "tokens": ["Mit", "gift'\u00b7gem", "Hauch", ",", "der", "all", "mein", "We\u00b7sen", "bannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIAT", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Voll schweren Wohlgeruchs, der zu mir fand,", "tokens": ["Voll", "schwe\u00b7ren", "Wohl\u00b7ge\u00b7ruchs", ",", "der", "zu", "mir", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus Dalie, Tulpe, Lilie erbl\u00fchend,", "tokens": ["Aus", "Da\u00b7lie", ",", "Tul\u00b7pe", ",", "Li\u00b7lie", "er\u00b7bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ertr\u00e4nkend Seele, Sinne und Verstand,", "tokens": ["Er\u00b7tr\u00e4n\u00b7kend", "See\u00b7le", ",", "Sin\u00b7ne", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bis mich mit schwerer Ohnmacht \u00fcbermannt", "tokens": ["Bis", "mich", "mit", "schwe\u00b7rer", "Ohn\u00b7macht", "\u00fc\u00b7berm\u00b7annt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erinnerung in D\u00e4mmerlicht vergl\u00fchend.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "in", "D\u00e4m\u00b7mer\u00b7licht", "ver\u00b7gl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}