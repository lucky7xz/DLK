{"textgrid.poem.62702": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen L\u00fcgner", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kunt Orpheus dort mit seiner Leyer,", "tokens": ["Kunt", "Or\u00b7pheus", "dort", "mit", "sei\u00b7ner", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die B\u00e4ume zum Bewegen bringen;", "tokens": ["Die", "B\u00e4u\u00b7me", "zum", "Be\u00b7we\u00b7gen", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kanst du dieses auch erzwingen", "tokens": ["So", "kanst", "du", "die\u00b7ses", "auch", "er\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwar mit leichter M\u00fch:", "tokens": ["Und", "zwar", "mit", "leich\u00b7ter", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn jener brauchte doch die H\u00e4nd darzu;", "tokens": ["Denn", "je\u00b7ner", "brauch\u00b7te", "doch", "die", "H\u00e4nd", "dar\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du aber, wenn du fr\u00fch, und vor der Ruh", "tokens": ["Du", "a\u00b7ber", ",", "wenn", "du", "fr\u00fch", ",", "und", "vor", "der", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Zung und Lippen regest;", "tokens": ["Die", "Zung", "und", "Lip\u00b7pen", "re\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So mu\u00df die Lufft sich stark erheben;", "tokens": ["So", "mu\u00df", "die", "Lufft", "sich", "stark", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die B\u00e4ume fangen an zu beben;", "tokens": ["Die", "B\u00e4u\u00b7me", "fan\u00b7gen", "an", "zu", "be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So bleibest du ein grosser Wunders-Mann,", "tokens": ["So", "blei\u00b7best", "du", "ein", "gros\u00b7ser", "Wun\u00b7der\u00b7sMann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der mehr als Orpheus kan.", "tokens": ["Der", "mehr", "als", "Or\u00b7pheus", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NE", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}