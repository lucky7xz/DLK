{"dta.poem.7066": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Busse, Reu, Zerknirschungen des Hertzens,", "tokens": ["Die", "Bus\u00b7se", ",", "Reu", ",", "Zer\u00b7knir\u00b7schun\u00b7gen", "des", "Hert\u00b7zens", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die, ob sie gleich in einer frommen Brust", "tokens": ["Die", ",", "ob", "sie", "gleich", "in", "ei\u00b7ner", "from\u00b7men", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht immer ohne Lust,", "tokens": ["Nicht", "im\u00b7mer", "oh\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch selten ohne Dorn des herbsten Schmertzens,", "tokens": ["Doch", "sel\u00b7ten", "oh\u00b7ne", "Dorn", "des", "herbs\u00b7ten", "Schmert\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf dieser Welt", "tokens": ["Auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sich eingestellt;", "tokens": ["Sich", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die h\u00f6ren uunmehr auf sammt Unruh, Schwachheit, Gram.", "tokens": ["Die", "h\u00f6\u00b7ren", "u\u00b7un\u00b7mehr", "auf", "sammt", "Un\u00b7ruh", ",", "Schwach\u00b7heit", ",", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die wegen ihrer Schuld gar offt empfund\u2019ne Scham", "tokens": ["Die", "we\u00b7gen", "ih\u00b7rer", "Schuld", "gar", "offt", "emp\u00b7fun\u00b7d'\u00b7ne", "Scham"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Empfinden sie nicht mehr. Versuchung, Eitelkeit", "tokens": ["Emp\u00b7fin\u00b7den", "sie", "nicht", "mehr", ".", "Ver\u00b7su\u00b7chung", ",", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "$.", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sind ihnen unbekannt. Sie f\u00fchlen ferner nicht,", "tokens": ["Sind", "ih\u00b7nen", "un\u00b7be\u00b7kannt", ".", "Sie", "f\u00fch\u00b7len", "fer\u00b7ner", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie eine Neigung stets der andern widerspricht.", "tokens": ["Wie", "ei\u00b7ne", "Nei\u00b7gung", "stets", "der", "an\u00b7dern", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}