{"textgrid.poem.32318": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "4. An den Herrn N**", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, noch sind ich und du dem Gl\u00fccke", "tokens": ["Freund", ",", "noch", "sind", "ich", "und", "du", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leichter Schleiderball.", "tokens": ["Ein", "leich\u00b7ter", "Schlei\u00b7der\u00b7ball", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch belebt auf seine T\u00fccke", "tokens": ["Und", "doch", "be\u00b7lebt", "auf", "sei\u00b7ne", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein bei\u00dfend Lied den Widerhall?", "tokens": ["Kein", "bei\u00b7\u00dfend", "Lied", "den", "Wi\u00b7der\u00b7hall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tor gedeiht, der Sp\u00f6tter steiget,", "tokens": ["Der", "Tor", "ge\u00b7deiht", ",", "der", "Sp\u00f6t\u00b7ter", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem B\u00f6sen fehlt kein Heil.", "tokens": ["Dem", "B\u00f6\u00b7sen", "fehlt", "kein", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verdienst steht nach, und f\u00fchlt gebeuget", "tokens": ["Ver\u00b7dienst", "steht", "nach", ",", "und", "f\u00fchlt", "ge\u00b7beu\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein lohnend Amt dem Golde feil.", "tokens": ["Ein", "loh\u00b7nend", "Amt", "dem", "Gol\u00b7de", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf, Freund! die Gei\u00dfel zu erfassen,", "tokens": ["Auf", ",", "Freund", "!", "die", "Gei\u00b7\u00dfel", "zu", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die dort vermodern will.", "tokens": ["Die", "dort", "ver\u00b7mo\u00b7dern", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit Juvenal sie fallen lassen,", "tokens": ["Seit", "Ju\u00b7ve\u00b7nal", "sie", "fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt sie, Triumph ihr Laster! still.", "tokens": ["Liegt", "sie", ",", "Tri\u00b7umph", "ihr", "Las\u00b7ter", "!", "still", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PPOSAT", "NN", "$.", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Geduld! Schon rauscht sie durch die L\u00fcfte,", "tokens": ["Ge\u00b7duld", "!", "Schon", "rauscht", "sie", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blutgierig rauscht sie her!", "tokens": ["Blut\u00b7gie\u00b7rig", "rauscht", "sie", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbergt, verbergt die blo\u00dfe H\u00fcfte!", "tokens": ["Ver\u00b7bergt", ",", "ver\u00b7bergt", "die", "blo\u00b7\u00dfe", "H\u00fcf\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Schmi\u00df ein gift'ger Schw\u00e4r!", "tokens": ["Ein", "je\u00b7der", "Sch\u00b7mi\u00df", "ein", "gift'\u00b7ger", "Schw\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Erst r\u00e4che dich, dich Freund der Musen.", "tokens": ["Erst", "r\u00e4\u00b7che", "dich", ",", "dich", "Freund", "der", "Mu\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du r\u00e4chest sie in dir!", "tokens": ["Du", "r\u00e4\u00b7chest", "sie", "in", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch dann auch mich, in dessen Busen", "tokens": ["Doch", "dann", "auch", "mich", ",", "in", "des\u00b7sen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Geist sich regt, zu gut f\u00fcr hier.", "tokens": ["Ein", "Geist", "sich", "regt", ",", "zu", "gut", "f\u00fcr", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "PTKA", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleicht, da\u00df einst in andern Welten", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "einst", "in", "an\u00b7dern", "Wel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir minder elend sind.", "tokens": ["Wir", "min\u00b7der", "e\u00b7lend", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tugend wird doch irgends gelten.", "tokens": ["Die", "Tu\u00b7gend", "wird", "doch", "ir\u00b7gends", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gute k\u00f6mmt nicht gern geschwind.", "tokens": ["Das", "Gu\u00b7te", "k\u00f6mmt", "nicht", "gern", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}