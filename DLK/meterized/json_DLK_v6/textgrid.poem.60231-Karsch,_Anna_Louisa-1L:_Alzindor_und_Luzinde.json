{"textgrid.poem.60231": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alzindor und Luzinde", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alzindor und Luzinde", "tokens": ["Al\u00b7zin\u00b7dor", "und", "Lu\u00b7zin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Genossen lange Zeit,", "tokens": ["Ge\u00b7nos\u00b7sen", "lan\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besch\u00fctzt von Cypris Kinde,", "tokens": ["Be\u00b7sch\u00fctzt", "von", "Cyp\u00b7ris", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Gl\u00fcck der Z\u00e4rtlichkeit:", "tokens": ["Das", "Gl\u00fcck", "der", "Z\u00e4rt\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Mutter bliebs verborgen,", "tokens": ["Der", "Mut\u00b7ter", "bliebs", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie lieblich manche Nacht", "tokens": ["Wie", "lieb\u00b7lich", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis an den grauen Morgen", "tokens": ["Bis", "an", "den", "grau\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Tochter zugebracht.", "tokens": ["Die", "Toch\u00b7ter", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der J\u00fcngling stieg behende", "tokens": ["Der", "J\u00fcng\u00b7ling", "stieg", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Fenster ein und aus:", "tokens": ["Zum", "Fens\u00b7ter", "ein", "und", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So klettert an die W\u00e4nde", "tokens": ["So", "klet\u00b7tert", "an", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auf das Taubenhaus", "tokens": ["Und", "auf", "das", "Tau\u00b7ben\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die blickbeflammte Katze", "tokens": ["Die", "blick\u00b7be\u00b7flamm\u00b7te", "Kat\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Nachts mit k\u00fchner List,", "tokens": ["Des", "Nachts", "mit", "k\u00fch\u00b7ner", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie er zu seinem Schatze", "tokens": ["Wie", "er", "zu", "sei\u00b7nem", "Schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinaufgeklettert ist.", "tokens": ["Hin\u00b7auf\u00b7ge\u00b7klet\u00b7tert", "ist", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was sie dort alles thaten,", "tokens": ["Was", "sie", "dort", "al\u00b7les", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Wonne ganz berauscht,", "tokens": ["Von", "Won\u00b7ne", "ganz", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das m\u00f6gen die errathen,", "tokens": ["Das", "m\u00f6\u00b7gen", "die", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nie der Mond belauscht", "tokens": ["Die", "nie", "der", "Mond", "be\u00b7lauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey schlaugestohlnen K\u00fc\u00dfen,", "tokens": ["Bey", "schlau\u00b7ge\u00b7stohl\u00b7nen", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die niemals nachgedacht,", "tokens": ["Die", "nie\u00b7mals", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was ohne Vulkans Wissen", "tokens": ["Was", "oh\u00b7ne", "Vul\u00b7kans", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mars bey der Venus macht.", "tokens": ["Mars", "bey", "der", "Ve\u00b7nus", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch gro\u00dfes Gl\u00fcck ist, leider!", "tokens": ["Doch", "gro\u00b7\u00dfes", "Gl\u00fcck", "ist", ",", "lei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie aller Welt bekannt,", "tokens": ["Wie", "al\u00b7ler", "Welt", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht ohne bittre Neider,", "tokens": ["Nicht", "oh\u00b7ne", "bitt\u00b7re", "Nei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht frey von Unbestand.", "tokens": ["Nicht", "frey", "von", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Alzindors Freund, voll T\u00fccke,", "tokens": ["Al\u00b7zin\u00b7dors", "Freund", ",", "voll", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gab insgeheim sich M\u00fch,", "tokens": ["Gab", "ins\u00b7ge\u00b7heim", "sich", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "PRF", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das er ihr Herz ber\u00fccke;", "tokens": ["Das", "er", "ihr", "Herz", "be\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ihn verschm\u00e4hte sie.", "tokens": ["Und", "ihn", "ver\u00b7schm\u00e4h\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sucht er sich zu r\u00e4chen,", "tokens": ["Da", "sucht", "er", "sich", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Art der jungen Herrn,", "tokens": ["Nach", "Art", "der", "jun\u00b7gen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die viel aus Prahlsucht sprechen", "tokens": ["Die", "viel", "aus", "Prahl\u00b7sucht", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Sch\u00f6nen, die sie gern", "tokens": ["Von", "Sch\u00f6\u00b7nen", ",", "die", "sie", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Schmeichelkunst betrogen. \u2013", "tokens": ["Durch", "Schmei\u00b7chel\u00b7kunst", "be\u00b7tro\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00f6rt, wie der H\u00f6llenbrand", "tokens": ["H\u00f6rt", ",", "wie", "der", "H\u00f6l\u00b7len\u00b7brand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Alzindors Ohr belogen", "tokens": ["Al\u00b7zin\u00b7dors", "Ohr", "be\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und leichten Glauben fand!", "tokens": ["Und", "leich\u00b7ten", "Glau\u00b7ben", "fand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von Bosheit angetrieben,", "tokens": ["Von", "Bos\u00b7heit", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht sein verw\u00fcnschter Mund:", "tokens": ["Spricht", "sein", "ver\u00b7w\u00fcnschter", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lucind' hat mir geschrieben,", "tokens": ["Lu\u00b7cind'", "hat", "mir", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df ich den Liebesbund", "tokens": ["Da\u00df", "ich", "den", "Lie\u00b7bes\u00b7bund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihr vollziehen solle,", "tokens": ["Mit", "ihr", "voll\u00b7zie\u00b7hen", "sol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und da\u00df sie schon darzu", "tokens": ["Und", "da\u00df", "sie", "schon", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Mittel finden wolle,", "tokens": ["Ein", "Mit\u00b7tel", "fin\u00b7den", "wol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie man es heimlich thu.", "tokens": ["Wie", "man", "es", "heim\u00b7lich", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Alzindor wird durchdrungen", "tokens": ["Al\u00b7zin\u00b7dor", "wird", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von gr\u00e4ulich wilder Wuth. \u2013", "tokens": ["Von", "gr\u00e4u\u00b7lich", "wil\u00b7der", "Wuth", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie nach Verlust des Jungen,", "tokens": ["Wie", "nach", "Ver\u00b7lust", "des", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die L\u00f6winn J\u00e4gerblut", "tokens": ["Die", "L\u00f6\u00b7winn", "J\u00e4\u00b7ger\u00b7blut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Walde br\u00fcllend fodert,", "tokens": ["Im", "Wal\u00b7de", "br\u00fcl\u00b7lend", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So fodert er voll Glut,", "tokens": ["So", "fo\u00b7dert", "er", "voll", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die schr\u00f6cklich in ihm lodert,", "tokens": ["Die", "schr\u00f6ck\u00b7lich", "in", "ihm", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lucindens Busenblut.", "tokens": ["Lu\u00b7cin\u00b7dens", "Bu\u00b7sen\u00b7blut", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O! Weh, o! Schreck, o! Jammer,", "tokens": ["O", "!", "Weh", ",", "o", "!", "Schreck", ",", "o", "!", "Jam\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "FM", "$.", "NN", "$,", "FM", "$.", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit blo\u00dfem Degen k\u00f6mmt", "tokens": ["Mit", "blo\u00b7\u00dfem", "De\u00b7gen", "k\u00f6mmt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schnell in ihre Kammer,", "tokens": ["Er", "schnell", "in", "ih\u00b7re", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und st\u00fcrzet, ungehemmt", "tokens": ["Und", "st\u00fcr\u00b7zet", ",", "un\u00b7ge\u00b7hemmt"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von ihrer s\u00fc\u00dfen Stimme,", "tokens": ["Von", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Sturmwind auf sie zu;", "tokens": ["Wie", "Sturm\u00b7wind", "auf", "sie", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fragt mit Donnerstimme:", "tokens": ["Und", "fragt", "mit", "Don\u00b7ner\u00b7stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sag' an: Wem schreibest du?", "tokens": ["Sag'", "an", ":", "Wem", "schrei\u00b7best", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Lucinde spricht gelassen:", "tokens": ["Lu\u00b7cin\u00b7de", "spricht", "ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deinen Freund schrieb ich.", "tokens": ["An", "dei\u00b7nen", "Freund", "schrieb", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha! nun mu\u00dft du erblassen,", "tokens": ["Ha", "!", "nun", "mu\u00dft", "du", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ruft er; und m\u00f6rderlich", "tokens": ["Ruft", "er", ";", "und", "m\u00f6r\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KON", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "F\u00e4hrt ihr bey sanften L\u00e4cheln", "tokens": ["F\u00e4hrt", "ihr", "bey", "sanf\u00b7ten", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Degen stark und tief", "tokens": ["Der", "De\u00b7gen", "stark", "und", "tief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ins Herz; und ach! mit R\u00f6cheln", "tokens": ["Ins", "Herz", ";", "und", "ach", "!", "mit", "R\u00f6\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "XY", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lallt sie: Hier ist \u2013 der \u2013 Brief.", "tokens": ["Lallt", "sie", ":", "Hier", "ist", "\u2013", "der", "\u2013", "Brief", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "$(", "ART", "$(", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Sie sinkt, und l\u00e4\u00dft im Sinken", "tokens": ["Sie", "sinkt", ",", "und", "l\u00e4\u00dft", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Auge, brechendmatt,", "tokens": ["Ihr", "Au\u00b7ge", ",", "bre\u00b7chend\u00b7matt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch seine Blicke trinken.", "tokens": ["Noch", "sei\u00b7ne", "Bli\u00b7cke", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er liest das Ungl\u00fccksblatt:", "tokens": ["Er", "liest", "das", "Un\u00b7gl\u00fccks\u00b7blatt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem L\u00fcgner war geschrieben:", "tokens": ["Dem", "L\u00fcg\u00b7ner", "war", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herr, plagt mich l\u00e4nger nicht!", "tokens": ["Herr", ",", "plagt", "mich", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur einen kann ich lieben,", "tokens": ["Nur", "ei\u00b7nen", "kann", "ich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dieser seyd ihr nicht.", "tokens": ["Und", "die\u00b7ser", "seyd", "ihr", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Scheusal! \u2013 ruft er pl\u00f6tzlich:", "tokens": ["O", "Scheu\u00b7sal", "!", "\u2013", "ruft", "er", "pl\u00f6tz\u00b7lich", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stirb nach, hier liegt dein Weib!", "tokens": ["Stirb", "nach", ",", "hier", "liegt", "dein", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf sticht er sich entsetzlich,", "tokens": ["Drauf", "sticht", "er", "sich", "ent\u00b7setz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Kato, durch den Leib;", "tokens": ["Wie", "Ka\u00b7to", ",", "durch", "den", "Leib", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00e4llt auf Lucindens Leiche,", "tokens": ["F\u00e4llt", "auf", "Lu\u00b7cin\u00b7dens", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stirbt \u00e4chzend, und verflucht", "tokens": ["Stirbt", "\u00e4ch\u00b7zend", ",", "und", "ver\u00b7flucht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "KON", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nunmehr in Plutos Reiche", "tokens": ["Nun\u00b7mehr", "in", "Plu\u00b7tos", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Zorn der Eifersucht.", "tokens": ["Den", "Zorn", "der", "Ei\u00b7fer\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}