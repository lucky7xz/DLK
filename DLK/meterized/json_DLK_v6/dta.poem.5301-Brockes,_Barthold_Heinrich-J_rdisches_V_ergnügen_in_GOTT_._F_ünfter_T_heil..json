{"dta.poem.5301": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da mehrentheils ein Blatt auf zweyen andern lieget,", "tokens": ["Da", "meh\u00b7ren\u00b7theils", "ein", "Blatt", "auf", "zwe\u00b7yen", "an\u00b7dern", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich dadurch so dicht und feste f\u00fcget,", "tokens": ["Und", "sich", "da\u00b7durch", "so", "dicht", "und", "fes\u00b7te", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "ADV", "ADJD", "KON", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df diese Dichtigkeit auch dichte Schatten zeuget,", "tokens": ["Da\u00df", "die\u00b7se", "Dich\u00b7tig\u00b7keit", "auch", "dich\u00b7te", "Schat\u00b7ten", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df vor des Sommers schw\u00fchlem Blitzen,", "tokens": ["Da\u00df", "vor", "des", "Som\u00b7mers", "schw\u00fch\u00b7lem", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie uns, durch ihre Zucht, die k\u00fchlen Schatten, sch\u00fctzen.", "tokens": ["Sie", "uns", ",", "durch", "ih\u00b7re", "Zucht", ",", "die", "k\u00fch\u00b7len", "Schat\u00b7ten", ",", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein solches junges Blat,", "tokens": ["Es", "ist", "ein", "sol\u00b7ches", "jun\u00b7ges", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da es so lieblich gr\u00fcn, so frisch, so zart und glatt,", "tokens": ["Da", "es", "so", "lieb\u00b7lich", "gr\u00fcn", ",", "so", "frisch", ",", "so", "zart", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht sonder Anmuth anzusehn.", "tokens": ["Nicht", "son\u00b7der", "An\u00b7muth", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Absonderlich wenn sich das Licht,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "wenn", "sich", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf die nicht minder glatten H\u00f6hen,", "tokens": ["Auf", "die", "nicht", "min\u00b7der", "glat\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wodurch die Adern strich-weis gehen", "tokens": ["Wo\u00b7durch", "die", "A\u00b7dern", "strich\u00b7weis", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und sie vertieffe, lieblich bricht,", "tokens": ["Und", "sie", "ver\u00b7tief\u00b7fe", ",", "lieb\u00b7lich", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da denn an den vertieften Gr\u00e4ntzen,", "tokens": ["Da", "denn", "an", "den", "ver\u00b7tief\u00b7ten", "Gr\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Oft kleine Blicke schimmernd gl\u00e4ntzen,", "tokens": ["Oft", "klei\u00b7ne", "Bli\u00b7cke", "schim\u00b7mernd", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wodurch, wenn ihre Zierd und Menge sich vereinet,", "tokens": ["Wo\u00b7durch", ",", "wenn", "ih\u00b7re", "Zierd", "und", "Men\u00b7ge", "sich", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der gantze Baum oft \u00fcbersilbert scheinet.", "tokens": ["Der", "gant\u00b7ze", "Baum", "oft", "\u00fc\u00b7ber\u00b7sil\u00b7bert", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Man wird zur holden Fr\u00fchlings-Zeit,", "tokens": ["Man", "wird", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da alles voll von Glantz und Lieblichkeit,", "tokens": ["Da", "al\u00b7les", "voll", "von", "Glantz", "und", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von der lieb-\u00e4ugelnden Natur,", "tokens": ["Von", "der", "lie\u00b7b\u00e4u\u00b7geln\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Aus jedes Bl\u00fcmchens holder Pracht,", "tokens": ["Aus", "je\u00b7des", "Bl\u00fcm\u00b7chens", "hol\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bald durch der Farben Schmuck, und bald durch die Figur,", "tokens": ["Bald", "durch", "der", "Far\u00b7ben", "Schmuck", ",", "und", "bald", "durch", "die", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bald in den frisch-bethauten Feldern,", "tokens": ["Bald", "in", "den", "frischbe\u00b7thau\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Bald in den Bl\u00e4tter-reichen W\u00e4ldern,", "tokens": ["Bald", "in", "den", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ja \u00fcberall recht angelacht.", "tokens": ["Ja", "\u00fc\u00b7be\u00b7rall", "recht", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So la\u00dft uns dem, der ihre Pracht", "tokens": ["So", "la\u00dft", "uns", "dem", ",", "der", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Zu unsrer Augen-Lust gemacht,", "tokens": ["Zu", "uns\u00b7rer", "Au\u00b7gen\u00b7Lust", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "(da in der Lentzen-Zeit die Welt so wunder-sch\u00f6n)", "tokens": ["(", "da", "in", "der", "Lent\u00b7zen\u00b7Zeit", "die", "Welt", "so", "wun\u00b7der\u00b7sch\u00f6n", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie doch, in unsrer Lust, zum wahren Ruhm besehn!", "tokens": ["Sie", "doch", ",", "in", "uns\u00b7rer", "Lust", ",", "zum", "wah\u00b7ren", "Ruhm", "be\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}