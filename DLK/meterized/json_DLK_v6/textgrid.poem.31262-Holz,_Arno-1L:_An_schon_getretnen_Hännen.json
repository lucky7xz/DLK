{"textgrid.poem.31262": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: An schon getretnen H\u00e4nnen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An schon getretnen H\u00e4nnen", "tokens": ["An", "schon", "ge\u00b7tret\u00b7nen", "H\u00e4n\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mag ich mich nicht verbr\u00e4nnen/", "tokens": ["mag", "ich", "mich", "nicht", "ver\u00b7br\u00e4n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "ich b\u00fcn ein ", "tokens": ["ich", "b\u00fcn", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "und nicht kein Kokkel-Hahn!", "tokens": ["und", "nicht", "kein", "Kok\u00b7kel\u00b7Hahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mehrsten schihlen oder hincken/", "tokens": ["Die", "mehrs\u00b7ten", "schih\u00b7len", "o\u00b7der", "hin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die Kerrels gar nach Knoblach stincken/", "tokens": ["die", "Ker\u00b7rels", "gar", "nach", "Kno\u00b7blach", "stin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "noch Nehreres/ zum mindsten hihr/", "tokens": ["noch", "Neh\u00b7re\u00b7res", "/", "zum", "minds\u00b7ten", "hihr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ich gl\u00e4ube wohl/ verspahrt man mir.", "tokens": ["ich", "gl\u00e4u\u00b7be", "wohl", "/", "ver\u00b7spahrt", "man", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am allerz\u00e4hrtsten schm\u00e4kken", "tokens": ["Am", "al\u00b7ler\u00b7z\u00e4hrts\u00b7ten", "schm\u00e4k\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die L\u00e4mmgens/ die noch l\u00e4kken/", "tokens": ["die", "L\u00e4mm\u00b7gens", "/", "die", "noch", "l\u00e4k\u00b7ken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "nichts ist zu keiner Zeit", "tokens": ["nichts", "ist", "zu", "kei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "von solcher Nettigkeit.", "tokens": ["von", "sol\u00b7cher", "Net\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das d\u00e4ttscht uns noch mit keinen Krallen/", "tokens": ["Das", "d\u00e4ttscht", "uns", "noch", "mit", "kei\u00b7nen", "Kral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "das will durch Redlig-seyn gefallen/", "tokens": ["das", "will", "durch", "Red\u00b7lig\u00b7seyn", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "das lacht und trukkt uns in die Hand", "tokens": ["das", "lacht", "und", "trukkt", "uns", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sein Himmel-blaues Buhsen-Band!", "tokens": ["sein", "Him\u00b7mel\u00b7blau\u00b7es", "Buh\u00b7sen\u00b7Band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Zokkerckens! ihr Pfl\u00e4umckens!", "tokens": ["Ihr", "Zok\u00b7ker\u00b7ckens", "!", "ihr", "Pfl\u00e4um\u00b7ckens", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ihr g\u00f6ldnen \u00d6pffel-B\u00e4umckens!", "tokens": ["ihr", "g\u00f6ld\u00b7nen", "\u00d6pf\u00b7fel\u00b7B\u00e4um\u00b7ckens", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ich b\u00fcn euch ohnfarbirt", "tokens": ["ich", "b\u00fcn", "euch", "ohn\u00b7far\u00b7birt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "von Hertzen ", "tokens": ["von", "Hert\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ihr seyd mit euren s\u00fcssen Fr\u00e4zzgens", "tokens": ["Ihr", "seyd", "mit", "eu\u00b7ren", "s\u00fcs\u00b7sen", "Fr\u00e4zz\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "fast niedlicher al\u00df ", "tokens": ["fast", "nied\u00b7li\u00b7cher", "al\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.7": {"text": "ihr zihrt die bundt-bebl\u00fchmte Au", "tokens": ["ihr", "zihrt", "die", "bundt\u00b7be\u00b7bl\u00fchm\u00b7te", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "noch lihblicher al\u00df Morgen-Tau!", "tokens": ["noch", "lih\u00b7bli\u00b7cher", "al\u00df", "Mor\u00b7gen\u00b7Tau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Mit Matz in eine Sch\u00fcssel", "tokens": ["Mit", "Matz", "in", "ei\u00b7ne", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "t\u00fcnck ich nicht meinen R\u00fcssel/", "tokens": ["t\u00fcnck", "ich", "nicht", "mei\u00b7nen", "R\u00fcs\u00b7sel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ich wehr for solche Kuhr", "tokens": ["ich", "wehr", "for", "sol\u00b7che", "Kuhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "zu \u00f6klichter Naduhr.", "tokens": ["zu", "\u00f6k\u00b7lich\u00b7ter", "Na\u00b7duhr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dr\u00fcmb so bem\u00fch ich meine Fehder", "tokens": ["Dr\u00fcmb", "so", "be\u00b7m\u00fch", "ich", "mei\u00b7ne", "Feh\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "nur for subtiles Jungffern-Lehder", "tokens": ["nur", "for", "sub\u00b7ti\u00b7les", "Jungffern\u00b7Leh\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "und lasse durchau\u00df ohnbel\u00e4kkt", "tokens": ["und", "las\u00b7se", "durch\u00b7au\u00df", "ohn\u00b7be\u00b7l\u00e4kkt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ein D\u00f6pffgen/ das schon zubed\u00e4kkt!", "tokens": ["ein", "D\u00f6pff\u00b7gen", "/", "das", "schon", "zu\u00b7be\u00b7d\u00e4kkt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}