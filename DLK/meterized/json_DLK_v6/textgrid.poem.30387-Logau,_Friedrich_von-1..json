{"textgrid.poem.30387": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1652, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sein Vaterland errettet, diesen kr\u00f6nte Rom mit Grase.", "tokens": ["Der", "sein", "Va\u00b7ter\u00b7land", "er\u00b7ret\u00b7tet", ",", "die\u00b7sen", "kr\u00f6n\u00b7te", "Rom", "mit", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "PDAT", "ADJA", "NE", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Blieb uns auch so viel von gr\u00fcnem, da\u00df man wo zusammen lase,", "tokens": ["Blieb", "uns", "auch", "so", "viel", "von", "gr\u00fc\u00b7nem", ",", "da\u00df", "man", "wo", "zu\u00b7sam\u00b7men", "la\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "$,", "KOUS", "PIS", "PWAV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was zu einem Krantze noth denen, die das Vaterland", "tokens": ["Was", "zu", "ei\u00b7nem", "Krant\u00b7ze", "noth", "de\u00b7nen", ",", "die", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PDS", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "(sonsten aber nichts davon) gleichwol liessen, da\u00df es stand?", "tokens": ["(", "sons\u00b7ten", "a\u00b7ber", "nichts", "da\u00b7von", ")", "gleich\u00b7wol", "lies\u00b7sen", ",", "da\u00df", "es", "stand", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "PAV", "$(", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}