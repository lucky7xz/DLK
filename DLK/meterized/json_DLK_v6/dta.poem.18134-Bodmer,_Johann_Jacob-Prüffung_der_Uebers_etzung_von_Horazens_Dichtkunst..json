{"dta.poem.18134": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Pr\u00fcffung  der  Uebers etzung  \n von Horazens Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Maler und Poet folgt seiner Phantasey;", "tokens": ["Ein", "Ma\u00b7ler", "und", "Po\u00b7et", "folgt", "sei\u00b7ner", "Phan\u00b7ta\u00b7sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kan sich seiner Kunst nach eigner Lust bedienen,", "tokens": ["Er", "kan", "sich", "sei\u00b7ner", "Kunst", "nach", "eig\u00b7ner", "Lust", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich durch Geist und Wiz, was ihm beliebt, erk\u00fchne\u0303.", "tokens": ["Und", "sich", "durch", "Geist", "und", "Wiz", ",", "was", "ihm", "be\u00b7liebt", ",", "er\u00b7k\u00fchn\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN", "$,", "PWS", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}