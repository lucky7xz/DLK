{"textgrid.poem.42133": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Debutant", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du die hohe, dunkle Gartenpforte,", "tokens": ["Kennst", "du", "die", "ho\u00b7he", ",", "dunk\u00b7le", "Gar\u00b7ten\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ernst verschwiegen an der Stra\u00dfe steht?", "tokens": ["Die", "ernst", "ver\u00b7schwie\u00b7gen", "an", "der", "Stra\u00b7\u00dfe", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl niemand ahnte, welche s\u00fc\u00dfen Worte", "tokens": ["Wohl", "nie\u00b7mand", "ahn\u00b7te", ",", "wel\u00b7che", "s\u00fc\u00b7\u00dfen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrem Schutz der Abendwind verweht.", "tokens": ["In", "ih\u00b7rem", "Schutz", "der", "Ab\u00b7end\u00b7wind", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort trat ich ein; von freudigem Erwarten", "tokens": ["Dort", "trat", "ich", "ein", ";", "von", "freu\u00b7di\u00b7gem", "Er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwoll mir das Herz wie dem beschenkten Kind;", "tokens": ["Schwoll", "mir", "das", "Herz", "wie", "dem", "be\u00b7schenk\u00b7ten", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein leises Fl\u00fcstern wehte durch den Garten", "tokens": ["Ein", "lei\u00b7ses", "Fl\u00fcs\u00b7tern", "weh\u00b7te", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von guten Geistern, die dort heimisch sind.", "tokens": ["Von", "gu\u00b7ten", "Geis\u00b7tern", ",", "die", "dort", "hei\u00b7misch", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf schatt'ger Bank lie\u00df ich mich zaudernd nieder", "tokens": ["Auf", "schatt'\u00b7ger", "Bank", "lie\u00df", "ich", "mich", "zau\u00b7dernd", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trank der Rose wollustschweren Duft;", "tokens": ["Und", "trank", "der", "Ro\u00b7se", "wol\u00b7lust\u00b7schwe\u00b7ren", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob meinem Haupt knistert es im Flieder;", "tokens": ["Ob", "mei\u00b7nem", "Haupt", "knis\u00b7tert", "es", "im", "Flie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwei V\u00f6glein zwitschern durch die Abendluft.", "tokens": ["Zwei", "V\u00f6\u00b7glein", "zwit\u00b7schern", "durch", "die", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie aber ward mir, als du vor mich tratst,", "tokens": ["Wie", "a\u00b7ber", "ward", "mir", ",", "als", "du", "vor", "mich", "tratst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein G\u00f6tterbild aus fernen Griechenzeiten,", "tokens": ["Ein", "G\u00f6t\u00b7ter\u00b7bild", "aus", "fer\u00b7nen", "Grie\u00b7chen\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als du bedeutungsvoll und l\u00e4chelnd batst,", "tokens": ["Als", "du", "be\u00b7deu\u00b7tungs\u00b7voll", "und", "l\u00e4\u00b7chelnd", "batst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich tiefer in den Garten zu begleiten.", "tokens": ["Dich", "tie\u00b7fer", "in", "den", "Gar\u00b7ten", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort wurde mir aus Abend und aus Morgen", "tokens": ["Dort", "wur\u00b7de", "mir", "aus", "A\u00b7bend", "und", "aus", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der erste Lebenstag, den ich gelebt \u2013", "tokens": ["Der", "ers\u00b7te", "Le\u00b7bens\u00b7tag", ",", "den", "ich", "ge\u00b7lebt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oh, da\u00df so lange mir das Gl\u00fcck verborgen,", "tokens": ["Oh", ",", "da\u00df", "so", "lan\u00b7ge", "mir", "das", "Gl\u00fcck", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach dem das Herz dem Knaben schon gebebt!", "tokens": ["Nach", "dem", "das", "Herz", "dem", "Kna\u00b7ben", "schon", "ge\u00b7bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Oh, Ella, Ella, tausend Seligkeiten", "tokens": ["Oh", ",", "El\u00b7la", ",", "El\u00b7la", ",", "tau\u00b7send", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "NE", "$,", "NE", "$,", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einen einz'gen Atemzug gedr\u00e4ngt;", "tokens": ["In", "ei\u00b7nen", "einz'\u00b7gen", "A\u00b7tem\u00b7zug", "ge\u00b7dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Triebe aus der Menschheit fr\u00fchsten Zeiten,", "tokens": ["Die", "Trie\u00b7be", "aus", "der", "Menschheit", "fr\u00fchs\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Von wonnekund'ger G\u00f6tterhand gelenkt;", "tokens": ["Von", "won\u00b7ne\u00b7kun\u00b7d'\u00b7ger", "G\u00f6t\u00b7ter\u00b7hand", "ge\u00b7lenkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Kindheit ahnungsvolle, lose Spiele", "tokens": ["Der", "Kind\u00b7heit", "ah\u00b7nungs\u00b7vol\u00b7le", ",", "lo\u00b7se", "Spie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwandelt in unendlichen Genu\u00df;", "tokens": ["Ver\u00b7wan\u00b7delt", "in", "un\u00b7end\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oh, Ella, alle himmlischen Gef\u00fchle", "tokens": ["Oh", ",", "El\u00b7la", ",", "al\u00b7le", "himm\u00b7li\u00b7schen", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem einz'gen Liebesku\u00df \u2013", "tokens": ["In", "ei\u00b7nem", "einz'\u00b7gen", "Lie\u00b7bes\u00b7ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Welch hohes Wort, das Menschengeist ersann,", "tokens": ["Welch", "ho\u00b7hes", "Wort", ",", "das", "Men\u00b7schen\u00b7geist", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welch reicher Dank mag diese Stunde lohnen!", "tokens": ["Welch", "rei\u00b7cher", "Dank", "mag", "die\u00b7se", "Stun\u00b7de", "loh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df ewig mich in deinem Garten wohnen,", "tokens": ["La\u00df", "e\u00b7wig", "mich", "in", "dei\u00b7nem", "Gar\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist alles, was die Lippe stammeln kann.", "tokens": ["Ist", "al\u00b7les", ",", "was", "die", "Lip\u00b7pe", "stam\u00b7meln", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In seiner B\u00fcsche stillem Heiligtum", "tokens": ["In", "sei\u00b7ner", "B\u00fc\u00b7sche", "stil\u00b7lem", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nahm ich, als Balsam jeder Erdenqual,", "tokens": ["Nahm", "ich", ",", "als", "Bal\u00b7sam", "je\u00b7der", "Er\u00b7den\u00b7qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von deinem Mund das heilige Abendmahl", "tokens": ["Von", "dei\u00b7nem", "Mund", "das", "hei\u00b7li\u00b7ge", "A\u00b7bend\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum gro\u00dfen Liebesevangelium.", "tokens": ["Zum", "gro\u00b7\u00dfen", "Lie\u00b7be\u00b7se\u00b7van\u00b7ge\u00b7li\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}