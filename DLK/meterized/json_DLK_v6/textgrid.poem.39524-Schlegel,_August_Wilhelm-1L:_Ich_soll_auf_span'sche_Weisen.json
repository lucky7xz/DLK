{"textgrid.poem.39524": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich soll auf span'sche Weisen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich soll auf span'sche Weisen", "tokens": ["Ich", "soll", "auf", "span'\u00b7sche", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deutscher Sprache", "tokens": ["In", "deut\u00b7scher", "Spra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Euch leichte Lieder singen,", "tokens": ["Euch", "leich\u00b7te", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von sanftem Klange.", "tokens": ["Von", "sanf\u00b7tem", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Allein ich f\u00fcrchte,", "tokens": ["Al\u00b7lein", "ich", "f\u00fcrch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sie werden euch, je deutscher,", "tokens": ["Sie", "wer\u00b7den", "euch", ",", "je", "deut\u00b7scher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So span'scher d\u00fcnken.", "tokens": ["So", "span'\u00b7scher", "d\u00fcn\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}