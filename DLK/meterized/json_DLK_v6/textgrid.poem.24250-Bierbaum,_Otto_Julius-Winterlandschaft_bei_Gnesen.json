{"textgrid.poem.24250": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Winterlandschaft bei Gnesen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vierundzwanzig Tage", "tokens": ["Vie\u00b7rund\u00b7zwan\u00b7zig", "Ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hat es schon geschneit.", "tokens": ["Hat", "es", "schon", "ge\u00b7schneit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist eine Plage!", "tokens": ["Das", "ist", "ei\u00b7ne", "Pla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ach: du liebe Zeit,", "tokens": ["Ach", ":", "du", "lie\u00b7be", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wohin ich seh,", "tokens": ["Wo\u00b7hin", "ich", "seh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00dcberall Schnee,", "tokens": ["\u00dc\u00b7be\u00b7rall", "Schnee", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schnee weit und breit.", "tokens": ["Schnee", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Aber besonders in der N\u00e4h", "tokens": ["A\u00b7ber", "be\u00b7son\u00b7ders", "in", "der", "N\u00e4h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von Gnesen.", "tokens": ["Von", "Gne\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Gleich gro\u00dfen gespenstischen Besen", "tokens": ["Gleich", "gro\u00b7\u00dfen", "ge\u00b7spens\u00b7ti\u00b7schen", "Be\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Recken sich Pappeln", "tokens": ["Re\u00b7cken", "sich", "Pap\u00b7peln"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRF", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In die graue, leere", "tokens": ["In", "die", "grau\u00b7e", ",", "lee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Atmosph\u00e4re,", "tokens": ["At\u00b7mo\u00b7sph\u00e4\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Drauf sitzen elf kohlpechrabenschwarze Raben.", "tokens": ["Drauf", "sit\u00b7zen", "elf", "kohl\u00b7pech\u00b7ra\u00b7ben\u00b7schwar\u00b7ze", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die haben", "tokens": ["Die", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Jeder zwei Fl\u00fcgel, mit denen sie rappeln.", "tokens": ["Je\u00b7der", "zwei", "Fl\u00fc\u00b7gel", ",", "mit", "de\u00b7nen", "sie", "rap\u00b7peln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "O! weh!", "tokens": ["O", "!", "weh", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Schwarze Raben im wei\u00dfen Schnee!", "tokens": ["Schwar\u00b7ze", "Ra\u00b7ben", "im", "wei\u00b7\u00dfen", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "W\u00e4ren sie Menschen, sagten sie oh! und ah!", "tokens": ["W\u00e4\u00b7ren", "sie", "Men\u00b7schen", ",", "sag\u00b7ten", "sie", "oh", "!", "und", "ah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "ADJD", "$.", "KON", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aber es sind Raben, drum sagen sie krah!", "tokens": ["A\u00b7ber", "es", "sind", "Ra\u00b7ben", ",", "drum", "sa\u00b7gen", "sie", "krah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,", "PAV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das hei\u00dft bei ihnen sowohl ja,", "tokens": ["Das", "hei\u00dft", "bei", "ih\u00b7nen", "so\u00b7wohl", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als auch nein.", "tokens": ["Als", "auch", "nein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKANT", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Im \u00fcbrigen kann es uns einerlei sein,", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "kann", "es", "uns", "ei\u00b7ner\u00b7lei", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PRF", "PIS", "VAINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Denn wir ", "tokens": ["Denn", "wir"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aus einem kleinen Walde von links kommen acht Knaben", "tokens": ["Aus", "ei\u00b7nem", "klei\u00b7nen", "Wal\u00b7de", "von", "links", "kom\u00b7men", "acht", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im G\u00e4nsemarsche durch den Schnee.", "tokens": ["Im", "G\u00e4n\u00b7se\u00b7mar\u00b7sche", "durch", "den", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die haben", "tokens": ["Die", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ihre Nasen erfroren,", "tokens": ["Ih\u00b7re", "Na\u00b7sen", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Desgleichen die Ohren,", "tokens": ["Des\u00b7glei\u00b7chen", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und alle Heiterkeit verloren,", "tokens": ["Und", "al\u00b7le", "Hei\u00b7ter\u00b7keit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn auch die Beine tun ihnen weh.", "tokens": ["Denn", "auch", "die", "Bei\u00b7ne", "tun", "ih\u00b7nen", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Doch kann man es ihrem Sprechen anh\u00f6ren,", "tokens": ["Doch", "kann", "man", "es", "ih\u00b7rem", "Spre\u00b7chen", "an\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wie das bedauerlich h\u00e4ufig ist in diesen Landen.", "tokens": ["Wie", "das", "be\u00b7dau\u00b7er\u00b7lich", "h\u00e4u\u00b7fig", "ist", "in", "die\u00b7sen", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJD", "ADJD", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Au\u00dferdem ist ein Wolf vorhanden.", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "ist", "ein", "Wolf", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NE", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Nach einer Weile fliegen die Raben", "tokens": ["Nach", "ei\u00b7ner", "Wei\u00b7le", "flie\u00b7gen", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fort,", "tokens": ["Fort", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und auch die acht Knaben", "tokens": ["Und", "auch", "die", "acht", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "CARD", "NN"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.4": {"text": "Sind nicht mehr hier, sondern dort.", "tokens": ["Sind", "nicht", "mehr", "hier", ",", "son\u00b7dern", "dort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "$,", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "(ich meine: an einem anderen Ort.)", "tokens": ["(", "ich", "mei\u00b7ne", ":", "an", "ei\u00b7nem", "an\u00b7de\u00b7ren", "Ort", ".", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Schnee schmilzt, und der Wolf krepiert.", "tokens": ["Der", "Schnee", "schmilzt", ",", "und", "der", "Wolf", "kre\u00b7piert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich frage mich blo\u00df, was das ", "tokens": ["Ich", "fra\u00b7ge", "mich", "blo\u00df", ",", "was", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ART"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}