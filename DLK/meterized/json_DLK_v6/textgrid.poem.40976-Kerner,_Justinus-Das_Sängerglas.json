{"textgrid.poem.40976": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das S\u00e4ngerglas", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Glas, aus dem der S\u00e4nger trinket,", "tokens": ["Das", "Glas", ",", "aus", "dem", "der", "S\u00e4n\u00b7ger", "trin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei ein lichtheller Bergkristall,", "tokens": ["Sei", "ein", "licht\u00b7hel\u00b7ler", "Berg\u00b7kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Glas, das t\u00f6nt und sonnig blinket;", "tokens": ["Ein", "Glas", ",", "das", "t\u00f6nt", "und", "son\u00b7nig", "blin\u00b7ket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz ein Gef\u00e4\u00df aus Licht und Schall.", "tokens": ["Ganz", "ein", "Ge\u00b7f\u00e4\u00df", "aus", "Licht", "und", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Berges Kl\u00fcften, wo Metalle", "tokens": ["In", "Ber\u00b7ges", "Kl\u00fcf\u00b7ten", ",", "wo", "Me\u00b7tal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Still reifen durch des Feuers Macht,", "tokens": ["Still", "rei\u00b7fen", "durch", "des", "Feu\u00b7ers", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ert\u00f6nen funkelnde Kristalle", "tokens": ["Er\u00b7t\u00f6\u00b7nen", "fun\u00b7keln\u00b7de", "Kris\u00b7tal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und halten Geister treue Wacht.", "tokens": ["Und", "hal\u00b7ten", "Geis\u00b7ter", "treu\u00b7e", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zum Liede schaffender Erdgeister", "tokens": ["Zum", "Lie\u00b7de", "schaf\u00b7fen\u00b7der", "Erd\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ert\u00f6nt dort der Kristalle Klang,", "tokens": ["Er\u00b7t\u00f6nt", "dort", "der", "Kris\u00b7tal\u00b7le", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Weisen alter Sangesmeister,", "tokens": ["Wie", "Wei\u00b7sen", "al\u00b7ter", "San\u00b7ges\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die in Erdtiefen ruhn schon lang.", "tokens": ["Die", "in", "Erd\u00b7tie\u00b7fen", "ruhn", "schon", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "--++-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcck auf! hebt aus dem Scho\u00df der Erden", "tokens": ["Gl\u00fcck", "auf", "!", "hebt", "aus", "dem", "Scho\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Klingenden! scheut Geister nicht!", "tokens": ["Die", "Klin\u00b7gen\u00b7den", "!", "scheut", "Geis\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bringt sie ins Feuer, da\u00df sie werden", "tokens": ["Bringt", "sie", "ins", "Feu\u00b7er", ",", "da\u00df", "sie", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pokale voller Klang und Licht!", "tokens": ["Po\u00b7ka\u00b7le", "vol\u00b7ler", "Klang", "und", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf da\u00df sie schl\u00fcrfen mit dem Wein", "tokens": ["Auf", "da\u00df", "sie", "schl\u00fcr\u00b7fen", "mit", "dem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimnisse des Lichts und Klanges", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7se", "des", "Lichts", "und", "Klan\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der schaffenden Erdgeister ein;", "tokens": ["Der", "schaf\u00b7fen\u00b7den", "Erd\u00b7geis\u00b7ter", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Da\u00df sie in Weines Spiegel schauen", "tokens": ["Da\u00df", "sie", "in", "Wei\u00b7nes", "Spie\u00b7gel", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unterird'schen Wunder viel:", "tokens": ["Der", "un\u00b7ter\u00b7ird'\u00b7schen", "Wun\u00b7der", "viel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Goldburgen und kristallne Auen,", "tokens": ["Gold\u00b7bur\u00b7gen", "und", "kris\u00b7tall\u00b7ne", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wasser und der Feuer Spiel,", "tokens": ["Der", "Was\u00b7ser", "und", "der", "Feu\u00b7er", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Berggeister, schwebend durch die G\u00e4nge,", "tokens": ["Berg\u00b7geis\u00b7ter", ",", "schwe\u00b7bend", "durch", "die", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhellt von funkelndem Gestein; \u2013", "tokens": ["Er\u00b7hellt", "von", "fun\u00b7keln\u00b7dem", "Ge\u00b7stein", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Licht und Klang in die Ges\u00e4nge", "tokens": ["Da\u00df", "Licht", "und", "Klang", "in", "die", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einstr\u00f6m' aus dem Kristall voll Wein.", "tokens": ["Ein\u00b7str\u00f6m'", "aus", "dem", "Kris\u00b7tall", "voll", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "So ein Kristall, ber\u00fchrt vom Munde", "tokens": ["So", "ein", "Kris\u00b7tall", ",", "be\u00b7r\u00fchrt", "vom", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Des S\u00e4ngers, t\u00f6nt wie Elfensang,", "tokens": ["Des", "S\u00e4n\u00b7gers", ",", "t\u00f6nt", "wie", "El\u00b7fen\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in des S\u00e4ngers Todesstunde", "tokens": ["Und", "in", "des", "S\u00e4n\u00b7gers", "To\u00b7dess\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerspringt er mit der Wehmut Klang.", "tokens": ["Zer\u00b7springt", "er", "mit", "der", "Weh\u00b7mut", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}