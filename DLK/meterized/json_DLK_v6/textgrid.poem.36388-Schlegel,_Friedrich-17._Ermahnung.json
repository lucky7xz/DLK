.tri"}}, "stanza.14": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn loben Fl\u00fc\u00df' und Bronnen;", "tokens": ["Ihn", "lo\u00b7ben", "Fl\u00fc\u00df'", "und", "Bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr B\u00e4che all' und W\u00e4sserlein,", "tokens": ["Ihr", "B\u00e4\u00b7che", "all'", "und", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So Gang und Lauf gewonnen.", "tokens": ["So", "Gang", "und", "Lauf", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Schau da, was reines Wasser-Glas", "tokens": ["Schau", "da", ",", "was", "rei\u00b7nes", "Was\u00b7ser\u00b7Glas"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWS", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit Freuden k\u00f6mmt gezogen;", "tokens": ["Mit", "Freu\u00b7den", "k\u00f6mmt", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was manche flie\u00dfend' Silber-Ga\u00df',", "tokens": ["Was", "man\u00b7che", "flie\u00b