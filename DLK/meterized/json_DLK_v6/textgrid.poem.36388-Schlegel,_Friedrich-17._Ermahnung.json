{"textgrid.poem.36388": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17. Ermahnung", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf, Gott will gelobet sein,", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6pfer hoch von Ehren;", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "hoch", "von", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft uns die Laut' und Harfen rein,", "tokens": ["La\u00dft", "uns", "die", "Laut'", "und", "Har\u00b7fen", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Saiten s\u00fc\u00df vermehren.", "tokens": ["Mit", "Sai\u00b7ten", "s\u00fc\u00df", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonn' mit edlem Strahlenkranz", "tokens": ["Die", "Sonn'", "mit", "ed\u00b7lem", "Strah\u00b7len\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Tag den Sch\u00f6pfer weiset,", "tokens": ["Am", "Tag", "den", "Sch\u00f6p\u00b7fer", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mond mit rundem Sternentanz", "tokens": ["Der", "Mond", "mit", "run\u00b7dem", "Ster\u00b7nen\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sch\u00f6pfer n\u00e4chtlich preiset.", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "n\u00e4cht\u00b7lich", "prei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie leuchtend mu\u00df er selber sein", "tokens": ["Wie", "leuch\u00b7tend", "mu\u00df", "er", "sel\u00b7ber", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VMFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im wundervollen Schimmer,", "tokens": ["Im", "wun\u00b7der\u00b7vol\u00b7len", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da jener lichten Fackeln Schein", "tokens": ["Da", "je\u00b7ner", "lich\u00b7ten", "Fa\u00b7ckeln", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So golden strahlet immer.", "tokens": ["So", "gol\u00b7den", "strah\u00b7let", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du blaues Feld und Wasen,", "tokens": ["Du", "blau\u00b7es", "Feld", "und", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Euch Himmel ich da droben mein',", "tokens": ["Euch", "Him\u00b7mel", "ich", "da", "dro\u00b7ben", "mein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Zelt von Glas geblasen.", "tokens": ["Ihr", "Zelt", "von", "Glas", "ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und ihr unsichtbar'n Wasser klar,", "tokens": ["Und", "ihr", "un\u00b7sicht\u00b7ba\u00b7r'n", "Was\u00b7ser", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So droben aller Wegen,", "tokens": ["So", "dro\u00b7ben", "al\u00b7ler", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf euerm Pfad bleibt immerdar", "tokens": ["Auf", "eu\u00b7erm", "Pfad", "bleibt", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Himmeln \u00fcberlegen.", "tokens": ["Den", "Him\u00b7meln", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn loben Wind und Regen,", "tokens": ["Ihn", "lo\u00b7ben", "Wind", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Lob in Blitz und Wetterschein,", "tokens": ["Sein", "Lob", "in", "Blitz", "und", "Wet\u00b7ter\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in den Donnerschl\u00e4gen.", "tokens": ["Und", "in", "den", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und alles voll von seiner Macht", "tokens": ["Und", "al\u00b7les", "voll", "von", "sei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut \u00fcberall erschallet;", "tokens": ["Laut", "\u00fc\u00b7be\u00b7rall", "er\u00b7schal\u00b7let", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Meer in steter Wellenjagd", "tokens": ["Das", "Meer", "in", "ste\u00b7ter", "Wel\u00b7len\u00b7jagd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tosen Lob ihm hallet.", "tokens": ["Mit", "To\u00b7sen", "Lob", "ihm", "hal\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft auch musizieret,", "tokens": ["Die", "Luft", "auch", "mu\u00b7si\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Morgenr\u00f6t' sich stellet ein", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6t'", "sich", "stel\u00b7let", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Rosenrot gezieret.", "tokens": ["Mit", "Ro\u00b7sen\u00b7rot", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die wohlgemalten V\u00f6glein schwank", "tokens": ["Die", "wohl\u00b7ge\u00b7mal\u00b7ten", "V\u00f6\u00b7glein", "schwank"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Z\u00fcnglein s\u00fc\u00df anstimmen,", "tokens": ["Ihr", "Z\u00fcn\u00b7glein", "s\u00fc\u00df", "an\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Sch\u00f6pfer sagen Lob und Dank,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "sa\u00b7gen", "Lob", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, ab, in L\u00fcften schwimmen.", "tokens": ["Auf", ",", "ab", ",", "in", "L\u00fcf\u00b7ten", "schwim\u00b7men", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm Lilien sch\u00f6n und Rosen", "tokens": ["Ihm", "Li\u00b7li\u00b7en", "sch\u00f6n", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "KON", "NN"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.3": {"text": "Im wei\u00df und Purpur-M\u00e4ntelein", "tokens": ["Im", "wei\u00df", "und", "Pur\u00b7pur\u00b7M\u00e4n\u00b7te\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar lieb und freundlich kosen.", "tokens": ["Gar", "lieb", "und", "freund\u00b7lich", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie l\u00e4cheln ihm gar sch\u00f6n gef\u00e4rbt", "tokens": ["Sie", "l\u00e4\u00b7cheln", "ihm", "gar", "sch\u00f6n", "ge\u00b7f\u00e4rbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In duft'gen Blumeng\u00e4rten,", "tokens": ["In", "duft'\u00b7gen", "Blu\u00b7men\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sch\u00f6nheit sie von Gott geerbt", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "sie", "von", "Gott", "ge\u00b7erbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Samt ihren Mitgef\u00e4hrten.", "tokens": ["Samt", "ih\u00b7ren", "Mit\u00b7ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Auf, auf, Gott will gelobet sein!", "tokens": ["Auf", ",", "auf", ",", "Gott", "will", "ge\u00b7lo\u00b7bet", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will sein von uns gepriesen;", "tokens": ["Will", "sein", "von", "uns", "ge\u00b7prie\u00b7s