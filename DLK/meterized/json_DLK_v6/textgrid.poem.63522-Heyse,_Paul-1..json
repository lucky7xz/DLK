{"textgrid.poem.63522": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spazier' ich so die Gass' entlang,", "tokens": ["Spa\u00b7zier'", "ich", "so", "die", "Gass'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn kaum der Tag verrauschet,", "tokens": ["Wenn", "kaum", "der", "Tag", "ver\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da heb' ich an einen hellen Sang,", "tokens": ["Da", "heb'", "ich", "an", "ei\u00b7nen", "hel\u00b7len", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Mond geht auf und lauschet.", "tokens": ["Der", "Mond", "geht", "auf", "und", "lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo zwei und zwei beisammen sind,", "tokens": ["Wo", "zwei", "und", "zwei", "bei\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "KON", "CARD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da stiehlt das Lied sich ein geschwind,", "tokens": ["Da", "stiehlt", "das", "Lied", "sich", "ein", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo einsam weint ein Mutterkind,", "tokens": ["Wo", "ein\u00b7sam", "weint", "ein", "Mut\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem scheucht's die Nachtgespenster.", "tokens": ["Dem", "scheucht's", "die", "Nacht\u00b7ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So weit der Sonn- und Mondenschein", "tokens": ["So", "weit", "der", "Sonn", "und", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag auf die Erde blicken,", "tokens": ["Mag", "auf", "die", "Er\u00b7de", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will sich zusammen nichts so fein", "tokens": ["Will", "sich", "zu\u00b7sam\u00b7men", "nichts", "so", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADV", "PIS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Lieb' und Lieder schicken.", "tokens": ["Wie", "Lieb'", "und", "Lie\u00b7der", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das wu\u00dft' auch K\u00f6nig David wohl", "tokens": ["Das", "wu\u00dft'", "auch", "K\u00f6\u00b7nig", "Da\u00b7vid", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sang zur Harf' in Dur und Moll", "tokens": ["Und", "sang", "zur", "Ha\u00b7rf'", "in", "Dur", "und", "Moll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Gar meisterlich und wundervoll", "tokens": ["Gar", "meis\u00b7ter\u00b7lich", "und", "wun\u00b7der\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6nsten Serenaden.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Se\u00b7re\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und das geschah vor alters schon,", "tokens": ["Und", "das", "ge\u00b7schah", "vor", "al\u00b7ters", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist heut noch Brauch geblieben.", "tokens": ["Ist", "heut", "noch", "Brauch", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich mein', ich s\u00e4\u00df auf Davids Thron,", "tokens": ["Ich", "mein'", ",", "ich", "s\u00e4\u00df", "auf", "Da\u00b7vids", "Thron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing' ich ein Lied vom Lieben.", "tokens": ["Sing'", "ich", "ein", "Lied", "vom", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Thronen Glanz in Staub verweht,", "tokens": ["Der", "Thro\u00b7nen", "Glanz", "in", "Staub", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Reich der Liebe nie vergeht,", "tokens": ["Das", "Reich", "der", "Lie\u00b7be", "nie", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer das Singen recht versteht,", "tokens": ["Und", "wer", "das", "Sin\u00b7gen", "recht", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist aller Herzen K\u00f6nig.", "tokens": ["Ist", "al\u00b7ler", "Her\u00b7zen", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}