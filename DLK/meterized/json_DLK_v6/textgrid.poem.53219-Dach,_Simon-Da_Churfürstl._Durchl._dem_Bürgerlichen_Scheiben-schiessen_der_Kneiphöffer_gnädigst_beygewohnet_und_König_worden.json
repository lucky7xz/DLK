{"textgrid.poem.53219": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Da Churf\u00fcrstl. Durchl. dem B\u00fcrgerlichen Scheiben-schiessen der Kneiph\u00f6ffer gn\u00e4digst beygewohnet und K\u00f6nig worden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck zu dem K\u00f6nige! Gl\u00fcck zu!", "tokens": ["Gl\u00fcck", "zu", "dem", "K\u00f6\u00b7ni\u00b7ge", "!", "Gl\u00fcck", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gl\u00fcck zu dem thewren Helden,", "tokens": ["Gl\u00fcck", "zu", "dem", "thew\u00b7ren", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Preussen Landes h\u00f6chsten Ruh", "tokens": ["Des", "Preus\u00b7sen", "Lan\u00b7des", "h\u00f6chs\u00b7ten", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eine Zeit kan melden!", "tokens": ["Als", "ei\u00b7ne", "Zeit", "kan", "mel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er wei\u00df zu treffen auff das Ziel,", "tokens": ["Er", "wei\u00df", "zu", "tref\u00b7fen", "auff", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm sol man Ehre geben,", "tokens": ["Ihm", "sol", "man", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er hat der Gnad' und Hoheit viel", "tokens": ["Er", "hat", "der", "Gnad'", "und", "Ho\u00b7heit", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und auch das Gl\u00fcck daneben.", "tokens": ["Und", "auch", "das", "Gl\u00fcck", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er leget' an, die B\u00fcchse klang,", "tokens": ["Er", "le\u00b7get'", "an", ",", "die", "B\u00fcch\u00b7se", "klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schwartze ward durchschossen,", "tokens": ["Das", "Schwart\u00b7ze", "ward", "durch\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darauff hat sein Gesundheit-Tranck", "tokens": ["Dar\u00b7auff", "hat", "sein", "Ge\u00b7sund\u00b7heit\u00b7Tranck"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So manches Sinn begossen,", "tokens": ["So", "man\u00b7ches", "Sinn", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Pulfer und die Kugel schien", "tokens": ["Das", "Pul\u00b7fer", "und", "die", "Ku\u00b7gel", "schien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Helden Macht zu kennen,", "tokens": ["Des", "Hel\u00b7den", "Macht", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sprach: Kneiphoff auff! Ihr sollet Ihn", "tokens": ["Sprach", ":", "Knei\u00b7phoff", "auff", "!", "Ihr", "sol\u00b7let", "Ihn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch ewren K\u00f6nig nennen.", "tokens": ["Auch", "ew\u00b7ren", "K\u00f6\u00b7nig", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der gantze Raht und B\u00fcrgerschafft", "tokens": ["Der", "gant\u00b7ze", "Raht", "und", "B\u00fcr\u00b7ger\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind froh ob diesem Schiessen,", "tokens": ["Sind", "froh", "ob", "die\u00b7sem", "Schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wollen, da\u00df der Reben-Safft", "tokens": ["Und", "wol\u00b7len", ",", "da\u00df", "der", "Re\u00b7ben\u00b7\u00b7S\u00b7afft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sol desto milder fliessen,", "tokens": ["Sol", "des\u00b7to", "mil\u00b7der", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie werden dieses Gnaden-St\u00fcck", "tokens": ["Sie", "wer\u00b7den", "die\u00b7ses", "Gna\u00b7den\u00b7St\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald in das Stadt-Buch schreiben,", "tokens": ["Bald", "in", "das", "Stadt\u00b7Buch", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ihres Landes-Vaters Sieg", "tokens": ["Und", "ih\u00b7res", "Lan\u00b7des\u00b7Va\u00b7ters", "Sieg"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Nach-Welt einverleiben.", "tokens": ["Der", "Nach\u00b7Welt", "ein\u00b7ver\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nimb gn\u00e4digst hin, du wehrter Held,", "tokens": ["Nimb", "gn\u00e4\u00b7digst", "hin", ",", "du", "wehr\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorau\u00df den Krantz der Ehren,", "tokens": ["Vor\u00b7au\u00df", "den", "Krantz", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann auch den Becher und das Geld,", "tokens": ["Dann", "auch", "den", "Be\u00b7cher", "und", "das", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kanst recht schiessen lehren:", "tokens": ["Du", "kanst", "recht", "schies\u00b7sen", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist nicht gnug, du Sternen-Pfand,", "tokens": ["Es", "ist", "nicht", "gnug", ",", "du", "Ster\u00b7nen\u00b7Pfand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Preussen Dir mu\u00df gr\u00fcnen,", "tokens": ["Da\u00df", "Preus\u00b7sen", "Dir", "mu\u00df", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beherrschest wei\u00dflich Leut und Land,", "tokens": ["Be\u00b7herr\u00b7schest", "wei\u00df\u00b7lich", "Leut", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gl\u00fcck mu\u00df Dir auch dienen.", "tokens": ["Das", "Gl\u00fcck", "mu\u00df", "Dir", "auch", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun sey gegr\u00fcsst! Dein hohes Hau\u00df", "tokens": ["Nun", "sey", "ge\u00b7gr\u00fcs\u00b7st", "!", "Dein", "ho\u00b7hes", "Hau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fcss jetzt und ewig bl\u00fchen,", "tokens": ["M\u00fcss", "jetzt", "und", "e\u00b7wig", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00fcss' unter Deinem Schutz durchau\u00df", "tokens": ["M\u00fcss'", "un\u00b7ter", "Dei\u00b7nem", "Schutz", "durch\u00b7au\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wolfahrt sich umbziehen!", "tokens": ["Mit", "Wol\u00b7fahrt", "sich", "um\u00b7bzie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und halt in Gnaden mir zu gut", "tokens": ["Und", "halt", "in", "Gna\u00b7den", "mir", "zu", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich Dein Lob erklinge,", "tokens": ["Da\u00df", "ich", "Dein", "Lob", "er\u00b7klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erkecke frewdig meinen Muht", "tokens": ["Er\u00b7ke\u00b7cke", "frew\u00b7dig", "mei\u00b7nen", "Muht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nach- wie vor-mals singe.", "tokens": ["Und", "nach", "wie", "vor\u00b7mals", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gl\u00fcck zu dem K\u00f6nige! Gl\u00fcck zu!", "tokens": ["Gl\u00fcck", "zu", "dem", "K\u00f6\u00b7ni\u00b7ge", "!", "Gl\u00fcck", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gl\u00fcck zu dem thewren Helden!", "tokens": ["Gl\u00fcck", "zu", "dem", "thew\u00b7ren", "Hel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der PreussenLande h\u00f6chsten Ruh", "tokens": ["Der", "Preus\u00b7sen", "Lan\u00b7de", "h\u00f6chs\u00b7ten", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eine Zeit kan melden,", "tokens": ["Als", "ei\u00b7ne", "Zeit", "kan", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er wei\u00df zu treffen auff das Ziel,", "tokens": ["Er", "wei\u00df", "zu", "tref\u00b7fen", "auff", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm sol man Ehre geben,", "tokens": ["Ihm", "sol", "man", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er hat der Gnad und Hoheit viel", "tokens": ["Er", "hat", "der", "Gnad", "und", "Ho\u00b7heit", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und auch das Gl\u00fcck daneben.", "tokens": ["Und", "auch", "das", "Gl\u00fcck", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hie dieser Willkomm ist dem werthen Sch\u00fctzen-Orden", "tokens": ["Hie", "die\u00b7ser", "Will\u00b7komm", "ist", "dem", "wert\u00b7hen", "Sch\u00fct\u00b7zen\u00b7Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Kneiphoff-K\u00f6nigsberg zu gut gestifftet worden,", "tokens": ["Im", "Knei\u00b7phoff\u00b7K\u00f6\u00b7nigs\u00b7berg", "zu", "gut", "ge\u00b7stiff\u00b7tet", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKA", "ADJD", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da, al\u00df G\u00f6rg Wilhelm selbst, der F\u00fcrsten Licht und Art", "tokens": ["Da", ",", "al\u00df", "G\u00f6rg", "Wil\u00b7helm", "selbst", ",", "der", "F\u00fcrs\u00b7ten", "Licht", "und", "Art"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "NE", "NE", "ADV", "$,", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Held zu Brandenburg, im Schiessen K\u00f6nig ward.", "tokens": ["Und", "Held", "zu", "Bran\u00b7den\u00b7burg", ",", "im", "Schies\u00b7sen", "K\u00f6\u00b7nig", "ward", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$,", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "L\u00e4st jemand sich darau\u00df nach Hoffes Brauch beschencken,", "tokens": ["L\u00e4st", "je\u00b7mand", "sich", "dar\u00b7au\u00df", "nach", "Hof\u00b7fes", "Brauch", "be\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PAV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sol der Sch\u00fctzen stets in Ehren zu gedencken", "tokens": ["Der", "sol", "der", "Sch\u00fct\u00b7zen", "stets", "in", "Eh\u00b7ren", "zu", "ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Daneben schuldig seyn, sol w\u00fcnschen auch dazu", "tokens": ["Da\u00b7ne\u00b7ben", "schul\u00b7dig", "seyn", ",", "sol", "w\u00fcn\u00b7schen", "auch", "da\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "VAINF", "$,", "VMFIN", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem Hause Brandenburg Gl\u00fcck, Heyl und alle Ruh!", "tokens": ["Dem", "Hau\u00b7se", "Bran\u00b7den\u00b7burg", "Gl\u00fcck", ",", "Heyl", "und", "al\u00b7le", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}