{"textgrid.poem.46542": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da sind die Neujahrsgratulanten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sind die Neujahrsgratulanten,", "tokens": ["Da", "sind", "die", "Neu\u00b7jahrs\u00b7gra\u00b7tu\u00b7lan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Th\u00fcrmer und Stadtmusikanten,", "tokens": ["Die", "Th\u00fcr\u00b7mer", "und", "Stadt\u00b7mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zum neuen Jahr sie w\u00fcnschen Gl\u00fcck", "tokens": ["Zum", "neu\u00b7en", "Jahr", "sie", "w\u00fcn\u00b7schen", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fordern ihr Sechsbatzenst\u00fcck.", "tokens": ["Und", "for\u00b7dern", "ihr", "Sechs\u00b7bat\u00b7zen\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Th\u00fcrmer und Stadtmusikanten,", "tokens": ["Ihr", "Th\u00fcr\u00b7mer", "und", "Stadt\u00b7mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr w\u00fcnscht als Neujahrsgratulanten", "tokens": ["Ihr", "w\u00fcnscht", "als", "Neu\u00b7jahrs\u00b7gra\u00b7tu\u00b7lan\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sp\u00e4t mir Gl\u00fcck ins neue Jahr,", "tokens": ["Zu", "sp\u00e4t", "mir", "Gl\u00fcck", "ins", "neu\u00b7e", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPER", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es starb mir noch im alten gar.", "tokens": ["Es", "starb", "mir", "noch", "im", "al\u00b7ten", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Th\u00fcrmer, Kirchner und Verwandten,", "tokens": ["Ihr", "Th\u00fcr\u00b7mer", ",", "Kirch\u00b7ner", "und", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt recht als Neujahrsgratulanten,", "tokens": ["Kommt", "recht", "als", "Neu\u00b7jahrs\u00b7gra\u00b7tu\u00b7lan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stimmt an den hellen Glockenlaut", "tokens": ["Stimmt", "an", "den", "hel\u00b7len", "Glo\u00b7cken\u00b7laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmelskirchgang meiner Braut!", "tokens": ["Zum", "Him\u00b7mels\u00b7kirch\u00b7gang", "mei\u00b7ner", "Braut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ihr, o ihr Stadtmusikanten,", "tokens": ["Und", "ihr", ",", "o", "ihr", "Stadt\u00b7mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Geht, fiedelt allen sonst Bekannten!", "tokens": ["Geht", ",", "fie\u00b7delt", "al\u00b7len", "sonst", "Be\u00b7kann\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie nun tanzt den Hochzeitsreih'n,", "tokens": ["Wo", "sie", "nun", "tanzt", "den", "Hoch\u00b7zeits\u00b7reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedarf's nicht eurer Fiedelei'n.", "tokens": ["Be\u00b7da\u00b7rf's", "nicht", "eu\u00b7rer", "Fie\u00b7de\u00b7lei'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}