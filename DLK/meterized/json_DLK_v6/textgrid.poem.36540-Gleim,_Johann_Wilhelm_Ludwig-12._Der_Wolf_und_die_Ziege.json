{"textgrid.poem.36540": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "12. Der Wolf und die Ziege", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eines Felsen steiler H\u00f6h',", "tokens": ["Auf", "ei\u00b7nes", "Fel\u00b7sen", "stei\u00b7ler", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die weder Gras noch fetten Klee", "tokens": ["Die", "we\u00b7der", "Gras", "noch", "fet\u00b7ten", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KON", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Hungrigen zur Speise gab,", "tokens": ["Dem", "Hung\u00b7ri\u00b7gen", "zur", "Spei\u00b7se", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand eine Ziege.", "tokens": ["Stand", "ei\u00b7ne", "Zie\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Komm herab,", "tokens": ["Komm", "her\u00b7ab", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Du Kleine, Schmale, Liebliche!", "tokens": ["Du", "Klei\u00b7ne", ",", "Schma\u00b7le", ",", "Lieb\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Rief R\u00e4uber Wolf zu ihr hinauf;", "tokens": ["Rief", "R\u00e4u\u00b7ber", "Wolf", "zu", "ihr", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was stehst du doch da oben drauf?", "tokens": ["Was", "stehst", "du", "doch", "da", "o\u00b7ben", "drauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dort triffst du keinen guten Fra\u00df", "tokens": ["Dort", "triffst", "du", "kei\u00b7nen", "gu\u00b7ten", "Fra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr deinen lieben Magen an!", "tokens": ["F\u00fcr", "dei\u00b7nen", "lie\u00b7ben", "Ma\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hier unten w\u00e4chst so sch\u00f6nes Gras,", "tokens": ["Hier", "un\u00b7ten", "w\u00e4chst", "so", "sch\u00f6\u00b7nes", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df man sich artig m\u00e4sten kann;", "tokens": ["Da\u00df", "man", "sich", "ar\u00b7tig", "m\u00e4s\u00b7ten", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch stehn an kleinen Wasserf\u00e4llen,", "tokens": ["Auch", "stehn", "an", "klei\u00b7nen", "Was\u00b7ser\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Viel junge B\u00e4umchen abzusch\u00e4len,", "tokens": ["Viel", "jun\u00b7ge", "B\u00e4um\u00b7chen", "ab\u00b7zu\u00b7sch\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die, glaub' ich, schmeckten dir so s\u00fc\u00df,", "tokens": ["Die", ",", "glaub'", "ich", ",", "schmeck\u00b7ten", "dir", "so", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie Zuckerrohr! es ist gewi\u00df!", "tokens": ["Wie", "Zu\u00b7cker\u00b7rohr", "!", "es", "ist", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du Liebliche! \u2013", "tokens": ["Du", "Lieb\u00b7li\u00b7che", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$.", "$("], "meter": "-+--", "measure": "dactylic.init"}, "line.18": {"text": "Herr Wolf, sie sind", "tokens": ["Herr", "Wolf", ",", "sie", "sind"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Sehr g\u00fctig, wahrlich! Geben Sie", "tokens": ["Sehr", "g\u00fc\u00b7tig", ",", "wahr\u00b7lich", "!", "Ge\u00b7ben", "Sie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "$.", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sich aber doch nur keine M\u00fch", "tokens": ["Sich", "a\u00b7ber", "doch", "nur", "kei\u00b7ne", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Um meinen Magen! denn ich bin", "tokens": ["Um", "mei\u00b7nen", "Ma\u00b7gen", "!", "denn", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In Wahrheit keine Schmauserin!", "tokens": ["In", "Wahr\u00b7heit", "kei\u00b7ne", "Schmau\u00b7se\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich halt' es mit gesunden Kr\u00e4utern,", "tokens": ["Ich", "halt'", "es", "mit", "ge\u00b7sun\u00b7den", "Kr\u00e4u\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und mag mit fettem Gras und Klee", "tokens": ["Und", "mag", "mit", "fet\u00b7tem", "Gras", "und", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht eben meinen Leib erweitern;", "tokens": ["Nicht", "e\u00b7ben", "mei\u00b7nen", "Leib", "er\u00b7wei\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ich klettre gern! Herr Wolf, Adieu!", "tokens": ["Ich", "klett\u00b7re", "gern", "!", "Herr", "Wolf", ",", "A\u00b7die\u00b7u", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "NN", "NE", "$,", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}