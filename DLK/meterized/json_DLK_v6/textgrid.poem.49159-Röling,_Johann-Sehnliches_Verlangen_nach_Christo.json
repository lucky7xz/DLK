{"textgrid.poem.49159": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Sehnliches Verlangen nach Christo", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin voll heiliger Begier,", "tokens": ["Ich", "bin", "voll", "hei\u00b7li\u00b7ger", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, Jesu, zu umfangen,", "tokens": ["Dich", ",", "Je\u00b7su", ",", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und f\u00fchle mehr als gnug in mir,", "tokens": ["Und", "f\u00fch\u00b7le", "mehr", "als", "gnug", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies jetzt gleich zu erlangen.", "tokens": ["Dies", "jetzt", "gleich", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gedenke, wie mir denn zu Muth',", "tokens": ["Ge\u00b7den\u00b7ke", ",", "wie", "mir", "denn", "zu", "Muth'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schwachheit dr\u00fcckt zwar noch das Blut,", "tokens": ["Die", "Schwach\u00b7heit", "dr\u00fcckt", "zwar", "noch", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die dir ergebne Seele", "tokens": ["Die", "dir", "er\u00b7geb\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht frei ganz ihrer H\u00f6hle.", "tokens": ["Steht", "frei", "ganz", "ih\u00b7rer", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach komm', mein Heiland, komme bald,", "tokens": ["Ach", "komm'", ",", "mein", "Hei\u00b7land", ",", "kom\u00b7me", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie g\u00e4nzlich zu erl\u00f6sen,", "tokens": ["Sie", "g\u00e4nz\u00b7lich", "zu", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fchre mich, mein Aufenthalt,", "tokens": ["Und", "f\u00fch\u00b7re", "mich", ",", "mein", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir aus allem B\u00f6sen,", "tokens": ["Zu", "dir", "aus", "al\u00b7lem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus allem B\u00f6sen dieser Welt", "tokens": ["Aus", "al\u00b7lem", "B\u00f6\u00b7sen", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dein erw\u00fcnschtes Himmelszelt,", "tokens": ["In", "dein", "er\u00b7w\u00fcnschtes", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Aus meinen schweren Leiden", "tokens": ["Aus", "mei\u00b7nen", "schwe\u00b7ren", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu deinen s\u00fc\u00dfen Freuden.", "tokens": ["Zu", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Schatten ist es nur von mir,", "tokens": ["Ein", "Schat\u00b7ten", "ist", "es", "nur", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den man noch sp\u00fcrt zu leben,", "tokens": ["Den", "man", "noch", "sp\u00fcrt", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ich mich, mein Erl\u00f6ser, dir", "tokens": ["Denn", "ich", "mich", ",", "mein", "Er\u00b7l\u00f6\u00b7ser", ",", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon l\u00e4ngst hab' \u00fcbergeben.", "tokens": ["Schon", "l\u00e4ngst", "hab'", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinen Wunden ist mir wol,", "tokens": ["In", "dei\u00b7nen", "Wun\u00b7den", "ist", "mir", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier saug' ich deines Bluts mich voll,", "tokens": ["Hier", "saug'", "ich", "dei\u00b7nes", "Bluts", "mich", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hierinnen will ich sterben", "tokens": ["Hie\u00b7rin\u00b7nen", "will", "ich", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um deinen Geist zu erben.", "tokens": ["Um", "dei\u00b7nen", "Geist", "zu", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach komm', mein Heiland, komme bald,", "tokens": ["Ach", "komm'", ",", "mein", "Hei\u00b7land", ",", "kom\u00b7me", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fclle mein Verlangen!", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wol, du kommst, mein Aufenthalt,", "tokens": ["Nun", "wol", ",", "du", "kommst", ",", "mein", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie soll ich dich umfangen?", "tokens": ["Wie", "soll", "ich", "dich", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Komm', Liebster, komm' und herze mich,", "tokens": ["Komm'", ",", "Liebs\u00b7ter", ",", "komm'", "und", "her\u00b7ze", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm', Liebster, komm' ich herze dich,", "tokens": ["Komm'", ",", "Liebs\u00b7ter", ",", "komm'", "ich", "her\u00b7ze", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm', lass' uns lieben Zweie", "tokens": ["Komm'", ",", "lass'", "uns", "lie\u00b7ben", "Zwei\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns stellen zu der Freie.", "tokens": ["Uns", "stel\u00b7len", "zu", "der", "Frei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du hast mich, eh' die Welt gegr\u00fcndt,", "tokens": ["Du", "hast", "mich", ",", "eh'", "die", "Welt", "ge\u00b7gr\u00fcndt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deiner Braut erkoren;", "tokens": ["Zu", "dei\u00b7ner", "Braut", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dies' Ehr' hab' ich betr\u00fcbtes Kind", "tokens": ["Dies'", "Ehr'", "hab'", "ich", "be\u00b7tr\u00fcb\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Adam zwar verloren,", "tokens": ["In", "A\u00b7dam", "zwar", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da aber mich dein heilges Bad", "tokens": ["Da", "a\u00b7ber", "mich", "dein", "heil\u00b7ges", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von meiner Schuld befreiet hat,", "tokens": ["Von", "mei\u00b7ner", "Schuld", "be\u00b7frei\u00b7et", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist mir dieser Orden", "tokens": ["So", "ist", "mir", "die\u00b7ser", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch sch\u00f6ner wieder worden.", "tokens": ["Noch", "sch\u00f6\u00b7ner", "wie\u00b7der", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Komm' denn, mein Heil und sei gegr\u00fcsst,", "tokens": ["Komm'", "denn", ",", "mein", "Heil", "und", "sei", "ge\u00b7gr\u00fcs\u00b7st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm', mein Licht, meine Sonne,", "tokens": ["Komm'", ",", "mein", "Licht", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Komm', sei zu tausendmal gek\u00fc\u00dft!", "tokens": ["Komm'", ",", "sei", "zu", "tau\u00b7send\u00b7mal", "ge\u00b7k\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freude, diese Wonne", "tokens": ["Die", "Freu\u00b7de", ",", "die\u00b7se", "Won\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht, da\u00df mir Honig alle Noth", "tokens": ["Macht", ",", "da\u00df", "mir", "Ho\u00b7nig", "al\u00b7le", "Noth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und Zucker selbst der bittre Tod;", "tokens": ["Und", "Zu\u00b7cker", "selbst", "der", "bitt\u00b7re", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn nun wird mir dein Leben,", "tokens": ["Denn", "nun", "wird", "mir", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da ich meins aufgegeben.", "tokens": ["Da", "ich", "meins", "auf\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}