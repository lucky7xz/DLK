{"textgrid.poem.47815": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gedenkst du noch?", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenkst du noch, wenn in der Fr\u00fchlingsnacht", "tokens": ["Ge\u00b7denkst", "du", "noch", ",", "wenn", "in", "der", "Fr\u00fch\u00b7lings\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus unserm Kammerfenster wir hernieder", "tokens": ["Aus", "un\u00b7serm", "Kam\u00b7mer\u00b7fens\u00b7ter", "wir", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Garten schauten, wo geheimnisvoll", "tokens": ["Zum", "Gar\u00b7ten", "schau\u00b7ten", ",", "wo", "ge\u00b7heim\u00b7nis\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Dunkel dufteten Jasmin und Flieder?", "tokens": ["Im", "Dun\u00b7kel", "duf\u00b7te\u00b7ten", "Jas\u00b7min", "und", "Flie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Sternenhimmel \u00fcber uns so weit,", "tokens": ["Der", "Ster\u00b7nen\u00b7him\u00b7mel", "\u00fc\u00b7ber", "uns", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und du so jung; unmerklich geht die Zeit.", "tokens": ["Und", "du", "so", "jung", ";", "un\u00b7mer\u00b7klich", "geht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "$.", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie still die Luft! Des Regenpfeifers Schrei", "tokens": ["Wie", "still", "die", "Luft", "!", "Des", "Re\u00b7gen\u00b7pfei\u00b7fers", "Schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Scholl klar her\u00fcber von dem Meeresstrande;", "tokens": ["Scholl", "klar", "her\u00b7\u00fc\u00b7ber", "von", "dem", "Mee\u00b7res\u00b7stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber unsrer B\u00e4ume Wipfel sahn", "tokens": ["Und", "\u00fc\u00b7ber", "uns\u00b7rer", "B\u00e4u\u00b7me", "Wip\u00b7fel", "sahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir schweigend in die d\u00e4mmerigen Lande.", "tokens": ["Wir", "schwei\u00b7gend", "in", "die", "d\u00e4m\u00b7me\u00b7ri\u00b7gen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nun wird es wieder Fr\u00fchling um uns her,", "tokens": ["Nun", "wird", "es", "wie\u00b7der", "Fr\u00fch\u00b7ling", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur eine Heimat haben wir nicht mehr.", "tokens": ["Nur", "ei\u00b7ne", "Hei\u00b7mat", "ha\u00b7ben", "wir", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun horch ich oft, schlaflos in tiefer Nacht,", "tokens": ["Nun", "horch", "ich", "oft", ",", "schlaf\u00b7los", "in", "tie\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob nicht der Wind zur R\u00fcckfahrt m\u00f6ge wehen.", "tokens": ["Ob", "nicht", "der", "Wind", "zur", "R\u00fcck\u00b7fahrt", "m\u00f6\u00b7ge", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer in der Heimat erst sein Haus gebaut,", "tokens": ["Wer", "in", "der", "Hei\u00b7mat", "erst", "sein", "Haus", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der sollte nicht mehr in die Fremde gehen!", "tokens": ["Der", "soll\u00b7te", "nicht", "mehr", "in", "die", "Frem\u00b7de", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach dr\u00fcben ist sein Auge stets gewandt:", "tokens": ["Nach", "dr\u00fc\u00b7ben", "ist", "sein", "Au\u00b7ge", "stets", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch eines blieb \u2013 wir gehen Hand in Hand.", "tokens": ["Doch", "ei\u00b7nes", "blieb", "\u2013", "wir", "ge\u00b7hen", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$(", "PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}