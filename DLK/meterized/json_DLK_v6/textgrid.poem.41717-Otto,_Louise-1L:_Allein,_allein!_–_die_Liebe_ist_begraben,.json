{"textgrid.poem.41717": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allein, allein! \u2013 die Liebe ist begraben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allein, allein! \u2013 die Liebe ist begraben,", "tokens": ["Al\u00b7lein", ",", "al\u00b7lein", "!", "\u2013", "die", "Lie\u00b7be", "ist", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich selbst bin nur die bleiche Trauerweide,", "tokens": ["Ich", "selbst", "bin", "nur", "die", "blei\u00b7che", "Trau\u00b7er\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deren Zweige sich verwandelt haben", "tokens": ["In", "de\u00b7ren", "Zwei\u00b7ge", "sich", "ver\u00b7wan\u00b7delt", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Liebesjubel, meine Liebesfreude!", "tokens": ["Mein", "Lie\u00b7bes\u00b7ju\u00b7bel", ",", "mei\u00b7ne", "Lie\u00b7bes\u00b7freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und was mich sonst an andre Herzen band", "tokens": ["Und", "was", "mich", "sonst", "an", "and\u00b7re", "Her\u00b7zen", "band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich hie\u00df als Epheu einen Stamm", "tokens": ["Mich", "hie\u00df", "als", "E\u00b7pheu", "ei\u00b7nen", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das hab ich all als nicht'gen Traum erkannt:", "tokens": ["Das", "hab", "ich", "all", "als", "nicht'\u00b7gen", "Traum", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Epheu mu\u00df allein im Freien schwanken.", "tokens": ["Der", "E\u00b7pheu", "mu\u00df", "al\u00b7lein", "im", "Frei\u00b7en", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein, allein! doch Du bist mir geblieben,", "tokens": ["Al\u00b7lein", ",", "al\u00b7lein", "!", "doch", "Du", "bist", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die mit dem Kind zu Spiel und Fest gegangen,", "tokens": ["Die", "mit", "dem", "Kind", "zu", "Spiel", "und", "Fest", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die f\u00fcr der Jungfrau fr\u00fchlingselig Lieben", "tokens": ["Die", "f\u00fcr", "der", "Jung\u00b7frau", "fr\u00fch\u00b7ling\u00b7se\u00b7lig", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die T\u00f6ne fand, die nur von Liebe klangen!", "tokens": ["Die", "T\u00f6\u00b7ne", "fand", ",", "die", "nur", "von", "Lie\u00b7be", "klan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du, die mir ihren Zauberstab verlieh", "tokens": ["Du", ",", "die", "mir", "ih\u00b7ren", "Zau\u00b7ber\u00b7stab", "ver\u00b7lieh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Nacht zu hellen, wo sie mich umdunkelt", "tokens": ["Die", "Nacht", "zu", "hel\u00b7len", ",", "wo", "sie", "mich", "um\u00b7dun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du bist mir treu, bist mein, o Poesie!", "tokens": ["Du", "bist", "mir", "treu", ",", "bist", "mein", ",", "o", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPOSAT", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sei auch der Stern, der diese Nacht mir funkelt!", "tokens": ["Sei", "auch", "der", "Stern", ",", "der", "die\u00b7se", "Nacht", "mir", "fun\u00b7kelt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, sei ein Stern an meinem Abendhimmel", "tokens": ["Ja", ",", "sei", "ein", "Stern", "an", "mei\u00b7nem", "A\u00b7bend\u00b7him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei du mir selbst ein milder Hesperus,", "tokens": ["Sei", "du", "mir", "selbst", "ein", "mil\u00b7der", "Hes\u00b7pe\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch in des Lebens, in der Zeit Gewimmel", "tokens": ["Doch", "in", "des", "Le\u00b7bens", ",", "in", "der", "Zeit", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Strahl Andern mit des Morgensternes Gru\u00df!", "tokens": ["Strahl", "An\u00b7dern", "mit", "des", "Mor\u00b7gens\u00b7ter\u00b7nes", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob abendlich mein Aug' in Thr\u00e4nen taut", "tokens": ["Ob", "a\u00b7bend\u00b7lich", "mein", "Aug'", "in", "Thr\u00e4\u00b7nen", "taut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob in mir Nacht \u2013 was brauchts die Welt zu wissen?", "tokens": ["Ob", "in", "mir", "Nacht", "\u2013", "was", "brauchts", "die", "Welt", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "$(", "PWS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Welt, f\u00fcr die ein neuer Morgen graut,", "tokens": ["Die", "Welt", ",", "f\u00fcr", "die", "ein", "neu\u00b7er", "Mor\u00b7gen", "graut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der sie aus Traum und Schlummer aufgerissen?", "tokens": ["Der", "sie", "aus", "Traum", "und", "Schlum\u00b7mer", "auf\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und diesem Morgen jauchz auch ich entgegen,", "tokens": ["Und", "die\u00b7sem", "Mor\u00b7gen", "jauchz", "auch", "ich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo wir der Freiheit Sonnenaufgang feiern,", "tokens": ["Wo", "wir", "der", "Frei\u00b7heit", "Son\u00b7nen\u00b7auf\u00b7gang", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den hei\u00dfen Erntetag, wo reichen Segen", "tokens": ["Den", "hei\u00b7\u00dfen", "Ern\u00b7te\u00b7tag", ",", "wo", "rei\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von langer Saat wir sammeln in die Scheuern.", "tokens": ["Von", "lan\u00b7ger", "Saat", "wir", "sam\u00b7meln", "in", "die", "Scheu\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Los, das einer jungen Bl\u00fcte fiel \u2013", "tokens": ["Das", "Los", ",", "das", "ei\u00b7ner", "jun\u00b7gen", "Bl\u00fc\u00b7te", "fiel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer wird nach dem bei solcher Ernte fragen?", "tokens": ["Wer", "wird", "nach", "dem", "bei", "sol\u00b7cher", "Ern\u00b7te", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ob sie verwelkt, geknickt an ihrem Stiel \u2013", "tokens": ["Ob", "sie", "ver\u00b7welkt", ",", "ge\u00b7knickt", "an", "ih\u00b7rem", "Stiel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nehmt sie zum Festkranz auf den Erntewagen!", "tokens": ["Nehmt", "sie", "zum", "Fest\u00b7kranz", "auf", "den", "Ern\u00b7te\u00b7wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nein, nicht allein! \u2013 will mich auch niemand lieben,", "tokens": ["Nein", ",", "nicht", "al\u00b7lein", "!", "\u2013", "will", "mich", "auch", "nie\u00b7mand", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "ADV", "$.", "$(", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will niemand meines Herzens Qual verstehen,", "tokens": ["Will", "nie\u00b7mand", "mei\u00b7nes", "Her\u00b7zens", "Qual", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df jedes Band zerrei\u00dfen und zerstieben,", "tokens": ["Mu\u00df", "je\u00b7des", "Band", "zer\u00b7rei\u00b7\u00dfen", "und", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weithin zerflatternd in die L\u00fcfte wehen.", "tokens": ["Weit\u00b7hin", "zer\u00b7flat\u00b7ternd", "in", "die", "L\u00fcf\u00b7te", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So nehm' ich dieses Herz, das ungez\u00e4hmte", "tokens": ["So", "nehm'", "ich", "die\u00b7ses", "Herz", ",", "das", "un\u00b7ge\u00b7z\u00e4hm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und leg es meinem Vaterland zu F\u00fc\u00dfen \u2013", "tokens": ["Und", "leg", "es", "mei\u00b7nem", "Va\u00b7ter\u00b7land", "zu", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das sich um eines Menschen Schicksal gr\u00e4mte", "tokens": ["Das", "sich", "um", "ei\u00b7nes", "Men\u00b7schen", "Schick\u00b7sal", "gr\u00e4m\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ART", "NN", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dies Herz soll nur dem Ganzen sich erschlie\u00dfen,", "tokens": ["Dies", "Herz", "soll", "nur", "dem", "Gan\u00b7zen", "sich", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und an die Armen sei's dahin gegeben,", "tokens": ["Und", "an", "die", "Ar\u00b7men", "sei's", "da\u00b7hin", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die obdachlos vor pr\u00e4chtgen H\u00e4usern stehen,", "tokens": ["Die", "ob\u00b7dach\u00b7los", "vor", "pr\u00e4cht\u00b7gen", "H\u00e4u\u00b7sern", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hungerbleich die leere Hand erheben,", "tokens": ["Und", "hun\u00b7ger\u00b7bleich", "die", "lee\u00b7re", "Hand", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf die ver\u00e4chtlich stolz die Reichen sehen;", "tokens": ["Auf", "die", "ver\u00b7\u00e4cht\u00b7lich", "stolz", "die", "Rei\u00b7chen", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die kleine M\u00fcnze, die ich euch kann geben", "tokens": ["Die", "klei\u00b7ne", "M\u00fcn\u00b7ze", ",", "die", "ich", "euch", "kann", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Armen lindert wenig Euren Schmerz \u2013", "tokens": ["Ihr", "Ar\u00b7men", "lin\u00b7dert", "we\u00b7nig", "Eu\u00b7ren", "Schmerz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch h\u00f6r' ich Euer Rufen, Euer Flehen,", "tokens": ["Doch", "h\u00f6r'", "ich", "Eu\u00b7er", "Ru\u00b7fen", ",", "Eu\u00b7er", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", 