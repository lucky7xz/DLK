{"textgrid.poem.34778": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir standen an der Stra\u00dfeneck'", "tokens": ["Wir", "stan\u00b7den", "an", "der", "Stra\u00b7\u00dfen\u00b7eck'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber eine Stunde;", "tokens": ["Wohl", "\u00fc\u00b7ber", "ei\u00b7ne", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sprachen voller Z\u00e4rtlichkeit", "tokens": ["Wir", "spra\u00b7chen", "vol\u00b7ler", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von unsrem Seelenbunde.", "tokens": ["Von", "uns\u00b7rem", "See\u00b7len\u00b7bun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sagten uns vielhundertmal,", "tokens": ["Wir", "sag\u00b7ten", "uns", "viel\u00b7hun\u00b7dert\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wir einander lieben;", "tokens": ["Da\u00df", "wir", "ein\u00b7an\u00b7der", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir standen an der Stra\u00dfeneck',", "tokens": ["Wir", "stan\u00b7den", "an", "der", "Stra\u00b7\u00dfen\u00b7eck'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sind da stehngeblieben.", "tokens": ["Und", "sind", "da", "stehn\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die G\u00f6ttin der Gelegenheit,", "tokens": ["Die", "G\u00f6t\u00b7tin", "der", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie 'n Z\u00f6fchen, flink und heiter,", "tokens": ["Wie", "'n", "Z\u00f6f\u00b7chen", ",", "flink", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Kam sie vorbei und sah uns stehn,", "tokens": ["Kam", "sie", "vor\u00b7bei", "und", "sah", "uns", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lachend ging sie weiter.", "tokens": ["Und", "la\u00b7chend", "ging", "sie", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}