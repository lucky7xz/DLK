{"textgrid.poem.65052": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du fragest, welches Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du fragest, welches Gl\u00fcck", "tokens": ["Der", "du", "fra\u00b7gest", ",", "wel\u00b7ches", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Liebe lohnen werde,", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "loh\u00b7nen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weich' Unw\u00fcrdiger! zur\u00fcck", "tokens": ["Weich'", "Un\u00b7w\u00fcr\u00b7di\u00b7ger", "!", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "punct", "word"], "pos": ["PIAT", "NN", "$.", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem heil'gen Opferherde!", "tokens": ["Von", "dem", "heil'\u00b7gen", "Op\u00b7fer\u00b7her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn so lang' nach Freuden noch", "tokens": ["Denn", "so", "lang'", "nach", "Freu\u00b7den", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Strebt dein Sinnen und Verlangen,", "tokens": ["Strebt", "dein", "Sin\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist du aus der Selbstsucht Joch", "tokens": ["Bist", "du", "aus", "der", "Selbst\u00b7sucht", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht zur Freiheit eingegangen.", "tokens": ["Nicht", "zur", "Frei\u00b7heit", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst, wenn alle Lust und Qual", "tokens": ["Erst", ",", "wenn", "al\u00b7le", "Lust", "und", "Qual"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Blick in Nichts zerronnen,", "tokens": ["Dei\u00b7nem", "Blick", "in", "Nichts", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Liebe Weihestrahl", "tokens": ["Wird", "der", "Lie\u00b7be", "Wei\u00b7hes\u00b7trahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4uternd dein Gem\u00fcth durchsonnen,", "tokens": ["L\u00e4u\u00b7ternd", "dein", "Ge\u00b7m\u00fcth", "durch\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und beginnen wird in dir", "tokens": ["Und", "be\u00b7gin\u00b7nen", "wird", "in", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderbar erh\u00f6htes Leben,", "tokens": ["Wun\u00b7der\u00b7bar", "er\u00b7h\u00f6h\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jenseitsfrieden in dem Hier,", "tokens": ["Jen\u00b7seits\u00b7frie\u00b7den", "in", "dem", "Hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn du deiner dich begeben.", "tokens": ["Wenn", "du", "dei\u00b7ner", "dich", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn du, statt zu fordern, giebst,", "tokens": ["Wenn", "du", ",", "statt", "zu", "for\u00b7dern", ",", "giebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du, selig selbstvergessen,", "tokens": ["Wenn", "du", ",", "se\u00b7lig", "selbst\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Gluth, womit du liebst,", "tokens": ["An", "der", "Gluth", ",", "wo\u00b7mit", "du", "liebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Wonne wei\u00dft zu messen,", "tokens": ["Dei\u00b7ne", "Won\u00b7ne", "wei\u00dft", "zu", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn das Herz in deiner Brust", "tokens": ["Wenn", "das", "Herz", "in", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Segensstrahlen rings entsendet,", "tokens": ["Se\u00b7gens\u00b7strah\u00b7len", "rings", "ent\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seines Reichthums sich bewu\u00dft,", "tokens": ["Sei\u00b7nes", "Reicht\u00b7hums", "sich", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADJD", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Durch die Gaben, die es spendet. \u2013", "tokens": ["Durch", "die", "Ga\u00b7ben", ",", "die", "es", "spen\u00b7det", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}