{"textgrid.poem.62286": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast noch eine Freude mir beschieden,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast noch eine Freude mir beschieden,", "tokens": ["Hast", "noch", "ei\u00b7ne", "Freu\u00b7de", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wirst noch eine gr\u00f6\u00df're mir bescheiden,", "tokens": ["Wirst", "noch", "ei\u00b7ne", "gr\u00f6\u00df'\u00b7re", "mir", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jene Abschaffung vom schwersten Leiden,", "tokens": ["Je\u00b7ne", "Ab\u00b7schaf\u00b7fung", "vom", "schwers\u00b7ten", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jene Roheit, ach hienieden:", "tokens": ["Je\u00b7ne", "Ro\u00b7heit", ",", "ach", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ITJ", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jenes lebende Begrabenwerden,", "tokens": ["Je\u00b7nes", "le\u00b7ben\u00b7de", "Be\u00b7gra\u00b7ben\u00b7wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Allerschrecklichstes auf Erden. \u2013", "tokens": ["Al\u00b7ler\u00b7schreck\u00b7lichs\u00b7tes", "auf", "Er\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}