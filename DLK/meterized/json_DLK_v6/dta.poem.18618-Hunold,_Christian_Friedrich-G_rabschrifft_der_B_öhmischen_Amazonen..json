{"dta.poem.18618": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "G rabschrifft der  B \u00f6hmischen  \n Amazonen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Bald soll ein Weib kein Mensch und bald ein Engel seyn/", "tokens": ["Bald", "soll", "ein", "Weib", "kein", "Mensch", "und", "bald", "ein", "En\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PIAT", "NN", "KON", "ADV", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem die Schmeicheley sich will gef\u00e4llig weisen.", "tokens": ["Nach\u00b7dem", "die", "Schmei\u00b7che\u00b7ley", "sich", "will", "ge\u00b7f\u00e4l\u00b7lig", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch die Warheit trifft offt wie bey diesen ein:", "tokens": ["Je\u00b7doch", "die", "War\u00b7heit", "trifft", "offt", "wie", "bey", "die\u00b7sen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "KOKOM", "APPR", "PDAT", "PTKVZ", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Da\u00df Engel von Gestalt im Hertzen Teuffel heissen.", "tokens": ["Da\u00df", "En\u00b7gel", "von", "Ge\u00b7stalt", "im", "Hert\u00b7zen", "Teuf\u00b7fel", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "wo