{"textgrid.poem.26225": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Kriegsnot w\u00fctet in meinem Herzen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bin ich verdammt zum Lesen hier,", "tokens": ["Wie", "bin", "ich", "ver\u00b7dammt", "zum", "Le\u00b7sen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zum Kriegserleben auf Zeitungspapier,", "tokens": ["Zum", "Kriegs\u00b7er\u00b7le\u00b7ben", "auf", "Zei\u00b7tungs\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich folge den Heeren nur zwischen den Zeilen,", "tokens": ["Ich", "fol\u00b7ge", "den", "Hee\u00b7ren", "nur", "zwi\u00b7schen", "den", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durchstampfe mit Buchstaben Schlachtfeld und Meilen,", "tokens": ["Durch\u00b7stamp\u00b7fe", "mit", "Buch\u00b7sta\u00b7ben", "Schlacht\u00b7feld", "und", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Verliere, gewinne zu Land und Meer", "tokens": ["Ver\u00b7lie\u00b7re", ",", "ge\u00b7win\u00b7ne", "zu", "Land", "und", "Meer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und wende das Zeitungsblatt hin und her.", "tokens": ["Und", "wen\u00b7de", "das", "Zei\u00b7tungs\u00b7blatt", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und doch f\u00fchl' ich aller Verwundeten Schmerzen.", "tokens": ["Und", "doch", "f\u00fchl'", "ich", "al\u00b7ler", "Ver\u00b7wun\u00b7de\u00b7ten", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Kriegsnot w\u00fctet in meinem Herzen. \u2013", "tokens": ["Die", "Kriegs\u00b7not", "w\u00fc\u00b7tet", "in", "mei\u00b7nem", "Her\u00b7zen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich wei\u00df nicht: legt' ich das Blatt aus der Hand,", "tokens": ["Ich", "wei\u00df", "nicht", ":", "legt'", "ich", "das", "Blatt", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Oder flog's vor mir auf in zornigem Brand?", "tokens": ["O\u00b7der", "flo\u00b7g's", "vor", "mir", "auf", "in", "zor\u00b7ni\u00b7gem", "Brand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}}}}