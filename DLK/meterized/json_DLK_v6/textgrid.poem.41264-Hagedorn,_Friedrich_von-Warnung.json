{"textgrid.poem.41264": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leichtlich wird man hintergangen!", "tokens": ["Wie", "leicht\u00b7lich", "wird", "man", "hin\u00b7ter\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch das Verh\u00e4ngni\u00df l\u00e4\u00dft geschehn,", "tokens": ["Doch", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "l\u00e4\u00dft", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, die uns gerne hintergehn,", "tokens": ["Da\u00df", ",", "die", "uns", "ger\u00b7ne", "hin\u00b7ter\u00b7gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft mit Ger\u00e4usch und vielen Worten prangen.", "tokens": ["Oft", "mit", "Ge\u00b7r\u00e4usch", "und", "vie\u00b7len", "Wor\u00b7ten", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So macht die Schrecklichste der Schlangen", "tokens": ["So", "macht", "die", "Schreck\u00b7lichs\u00b7te", "der", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich, mit ihr, schon n\u00e4hernde Gefahr", "tokens": ["Die", "sich", ",", "mit", "ihr", ",", "schon", "n\u00e4\u00b7hern\u00b7de", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "$,", "APPR", "PPER", "$,", "ADV", "ADJA", "NN"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Durch ihr Geklapper offenbar.", "tokens": ["Durch", "ihr", "Ge\u00b7klap\u00b7per", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}