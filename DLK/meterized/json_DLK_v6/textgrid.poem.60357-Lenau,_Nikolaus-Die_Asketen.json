{"textgrid.poem.60357": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Asketen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O spottet nicht der traurigen Asketen,", "tokens": ["O", "spot\u00b7tet", "nicht", "der", "trau\u00b7ri\u00b7gen", "As\u00b7ke\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie den Leib mit scharfen Leiden plagen,", "tokens": ["Da\u00df", "sie", "den", "Leib", "mit", "schar\u00b7fen", "Lei\u00b7den", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fc\u00dfen Erdenfreuden sich versagen,", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Er\u00b7den\u00b7freu\u00b7den", "sich", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fl\u00fcchtigen, nur allzuschnell verwehten!", "tokens": ["Die", "fl\u00fcch\u00b7ti\u00b7gen", ",", "nur", "all\u00b7zu\u00b7schnell", "ver\u00b7weh\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nebst solchen, die das Futter gierig m\u00e4hten,", "tokens": ["Nebst", "sol\u00b7chen", ",", "die", "das", "Fut\u00b7ter", "gie\u00b7rig", "m\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit des verlornen Paradieses Tagen,", "tokens": ["Seit", "des", "ver\u00b7lor\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7ses", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat eine Schar von Herzen stets geschlagen,", "tokens": ["Hat", "ei\u00b7ne", "Schar", "von", "Her\u00b7zen", "stets", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, abgewandt, die Weide hier verschm\u00e4hten.", "tokens": ["Die", ",", "ab\u00b7ge\u00b7wandt", ",", "die", "Wei\u00b7de", "hier", "ver\u00b7schm\u00e4h\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein sch\u00fcchternes Gef\u00fchl: \u203aWir sind gefallen!\u2039", "tokens": ["Ein", "sch\u00fcch\u00b7ter\u00b7nes", "Ge\u00b7f\u00fchl", ":", "\u203a", "Wir", "sind", "ge\u00b7fal\u00b7len", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4lt sie vom lauten Freudenmarkt zur\u00fcck,", "tokens": ["H\u00e4lt", "sie", "vom", "lau\u00b7ten", "Freu\u00b7den\u00b7markt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hei\u00dft sie den Pfad einsamer Dornen wallen.", "tokens": ["Hei\u00dft", "sie", "den", "Pfad", "ein\u00b7sa\u00b7mer", "Dor\u00b7nen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4chst ihr Ernst, wenn sie vor\u00fcberstreifen", "tokens": ["Es", "w\u00e4chst", "ihr", "Ernst", ",", "wenn", "sie", "vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An einem unverdienten Erdengl\u00fcck;", "tokens": ["An", "ei\u00b7nem", "un\u00b7ver\u00b7dien\u00b7ten", "Er\u00b7den\u00b7gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Scham verbietet, keck darnach zu greifen.", "tokens": ["Die", "Scham", "ver\u00b7bie\u00b7tet", ",", "keck", "dar\u00b7nach", "zu", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}