{"textgrid.poem.53345": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was haben wir zu sorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was haben wir zu sorgen,", "tokens": ["Was", "ha\u00b7ben", "wir", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn uns heut oder morgen", "tokens": ["Wenn", "uns", "heut", "o\u00b7der", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Leibes H\u00fctte bricht?", "tokens": ["Des", "Lei\u00b7bes", "H\u00fct\u00b7te", "bricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie mu\u00df zerbrochen werden,", "tokens": ["Sie", "mu\u00df", "zer\u00b7bro\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist aus sehr schwacher Erden", "tokens": ["Ist", "aus", "sehr", "schwa\u00b7cher", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wehrt die L\u00e4nge nicht.", "tokens": ["Und", "wehrt", "die", "L\u00e4n\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir wissen, da\u00df wir haben", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "wir", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein reiches Haus von Gaben", "tokens": ["Ein", "rei\u00b7ches", "Haus", "von", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Himmel pr\u00e4chtig stehn,", "tokens": ["Im", "Him\u00b7mel", "pr\u00e4ch\u00b7tig", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gebawt durch Gottes St\u00e4rcke,", "tokens": ["Ge\u00b7bawt", "durch", "Got\u00b7tes", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht durch der H\u00e4nde Wercke,", "tokens": ["Nicht", "durch", "der", "H\u00e4n\u00b7de", "Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df nimmer ein-wird-gehn.", "tokens": ["Da\u00df", "nim\u00b7mer", "ein\u00b7wird\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Hau\u00df da Gn\u00fcge, Leben", "tokens": ["Ein", "Hau\u00df", "da", "Gn\u00fc\u00b7ge", ",", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und solche Frewden schweben,", "tokens": ["Und", "sol\u00b7che", "Frew\u00b7den", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die keines Ohr geh\u00f6rt,", "tokens": ["Die", "kei\u00b7nes", "Ohr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kein Aug' hat eingenommen,", "tokens": ["Kein", "Aug'", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In keines Hertz sind kommen", "tokens": ["In", "kei\u00b7nes", "Hertz", "sind", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd keine Zung gelehrt.", "tokens": ["Vnd", "kei\u00b7ne", "Zung", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In die Behausung sehnen", "tokens": ["In", "die", "Be\u00b7hau\u00b7sung", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir uns aus diesen Thr\u00e4nen", "tokens": ["Wir", "uns", "aus", "die\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und uns verlangt allein,", "tokens": ["Und", "uns", "ver\u00b7langt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df damit unsre Seele", "tokens": ["Da\u00df", "da\u00b7mit", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr diese Leimen-H\u00f6le", "tokens": ["F\u00fcr", "die\u00b7se", "Lei\u00b7men\u00b7H\u00f6le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "M\u00f6g \u00fcberkleidet seyn.", "tokens": ["M\u00f6g", "\u00fc\u00b7berk\u00b7lei\u00b7det", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gott aber, dem f\u00fcr allen", "tokens": ["Gott", "a\u00b7ber", ",", "dem", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir hie im Leibe wallen,", "tokens": ["Wir", "hie", "im", "Lei\u00b7be", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schenck' uns des Glaubens Kleid,", "tokens": ["Schenck", "uns", "des", "Glau\u00b7bens", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df wir nicht nackend gehen", "tokens": ["Da\u00df", "wir", "nicht", "na\u00b7ckend", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd S\u00fcnden-he\u00dflich stehen", "tokens": ["Vnd", "S\u00fcn\u00b7den\u00b7he\u00df\u00b7lich", "ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geh\u00f6hnt in Ewigkeit.", "tokens": ["Ge\u00b7h\u00f6hnt", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wird dann die H\u00fctt aus Knochen", "tokens": ["Wird", "dann", "die", "H\u00fctt", "aus", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns endlich abgebrochen,", "tokens": ["Uns", "end\u00b7lich", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchr' Er die Seel' heraus", "tokens": ["F\u00fchr'", "Er", "die", "Seel'", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und lasse sie den Wagen", "tokens": ["Und", "las\u00b7se", "sie", "den", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der lieben Engel tragen", "tokens": ["Der", "lie\u00b7ben", "En\u00b7gel", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In seiner Frewden Hau\u00df.", "tokens": ["In", "sei\u00b7ner", "Frew\u00b7den", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da wollen wir von Leben", "tokens": ["Da", "wol\u00b7len", "wir", "von", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Lust berauscht erheben", "tokens": ["Und", "Lust", "be\u00b7rauscht", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Stimm- und Seiten-Klangk,", "tokens": ["Der", "Stim\u00b7m", "und", "Sei\u00b7ten\u00b7Klangk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und singen, Ihm geh\u00f6re", "tokens": ["Und", "sin\u00b7gen", ",", "Ihm", "ge\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht, Wei\u00dfheit, Herrschaft, Ehre", "tokens": ["Macht", ",", "Wei\u00df\u00b7heit", ",", "Herr\u00b7schaft", ",", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd aller Liebe Danck.", "tokens": ["Vnd", "al\u00b7ler", "Lie\u00b7be", "Danck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}