{"textgrid.poem.46090": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Rose bl\u00fcht] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose bl\u00fcht, selig die fromme Biene,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "se\u00b7lig", "die", "from\u00b7me", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Bl\u00e4tter keuschen Busen sinkt", "tokens": ["Die", "in", "der", "Bl\u00e4t\u00b7ter", "keu\u00b7schen", "Bu\u00b7sen", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und milden Tau und linden Honig trinkt,", "tokens": ["Und", "mil\u00b7den", "Tau", "und", "lin\u00b7den", "Ho\u00b7nig", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selig die Magd, die dir o Rose diene!", "tokens": ["Se\u00b7lig", "die", "Magd", ",", "die", "dir", "o", "Ro\u00b7se", "die\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "FM", "FM", "FM", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "In Freuden schwebet ihr Gem\u00fct,", "tokens": ["In", "Freu\u00b7den", "schwe\u00b7bet", "ihr", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ihre Rose bl\u00fcht.", "tokens": ["Weil", "ih\u00b7re", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fcht, Gott la\u00df doch milde gl\u00fchen", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "Gott", "la\u00df", "doch", "mil\u00b7de", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVIMP", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonne Licht, h\u00fcll' Ros' und R\u00f6selein", "tokens": ["Der", "Son\u00b7ne", "Licht", ",", "h\u00fcll'", "Ros'", "und", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gen Frost und Glut in deine Gnade ein,", "tokens": ["Gen", "Frost", "und", "Glut", "in", "dei\u00b7ne", "Gna\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df alle Lieb' in dieser Rose bl\u00fchen,", "tokens": ["La\u00df", "al\u00b7le", "Lieb'", "in", "die\u00b7ser", "Ro\u00b7se", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann singt das ganze hohe Lied:", "tokens": ["Dann", "singt", "das", "gan\u00b7ze", "ho\u00b7he", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach unsre Rose bl\u00fcht!", "tokens": ["Ach", "uns\u00b7re", "Ro\u00b7se", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie rosigt bl\u00fcht das R\u00f6slein aller Rosen", "tokens": ["Wie", "ro\u00b7sigt", "bl\u00fcht", "das", "R\u00f6s\u00b7lein", "al\u00b7ler", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lacht mit solcher Herzempfindlichkeit,", "tokens": ["Und", "lacht", "mit", "sol\u00b7cher", "Her\u00b7zemp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df selbst die Lilie ihr zu Dienst sich weiht,", "tokens": ["Da\u00df", "selbst", "die", "Li\u00b7lie", "ihr", "zu", "Dienst", "sich", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit keiner andern Blume zu liebkosen,", "tokens": ["Mit", "kei\u00b7ner", "an\u00b7dern", "Blu\u00b7me", "zu", "lieb\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil aller Unschuld Seelenfried", "tokens": ["Weil", "al\u00b7ler", "Un\u00b7schuld", "See\u00b7len\u00b7fried"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus diesem R\u00f6slein bl\u00fcht.", "tokens": ["Aus", "die\u00b7sem", "R\u00f6s\u00b7lein", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}