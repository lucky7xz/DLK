{"textgrid.poem.63520": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "H\u00fctet euch!", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein St\u00fcndlein sind sie beisammen gewest,", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "sind", "sie", "bei\u00b7sam\u00b7men", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVIZU", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein St\u00fcndlein l\u00e4uft so geschwind,", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "l\u00e4uft", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sa\u00dfen schon Herz sich im Herzen fest,", "tokens": ["Und", "sa\u00b7\u00dfen", "schon", "Herz", "sich", "im", "Her\u00b7zen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Denn die Liebe die kommt wie der Wind.", "tokens": ["Denn", "die", "Lie\u00b7be", "die", "kommt", "wie", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Du junger Gesell, nun h\u00fcte dich fein,", "tokens": ["Du", "jun\u00b7ger", "Ge\u00b7sell", ",", "nun", "h\u00fc\u00b7te", "dich", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun h\u00fcte dich, sch\u00f6nes Kind,", "tokens": ["Nun", "h\u00fc\u00b7te", "dich", ",", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und verriegelt gut eures Herzens Schrein \u2013", "tokens": ["Und", "ver\u00b7rie\u00b7gelt", "gut", "eu\u00b7res", "Her\u00b7zens", "Schrein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn die Liebe die geht wie der Wind.", "tokens": ["Denn", "die", "Lie\u00b7be", "die", "geht", "wie", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}