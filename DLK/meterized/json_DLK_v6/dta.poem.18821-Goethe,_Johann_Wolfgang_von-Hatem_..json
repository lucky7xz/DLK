{"dta.poem.18821": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.51", "en:0.48"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Warum du nur oft so unhold bist?", "tokens": ["Wa\u00b7rum", "du", "nur", "oft", "so", "un\u00b7hold", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Du weisst dass der Leib ein Kerker ist,", "tokens": ["Du", "weisst", "dass", "der", "Leib", "ein", "Ker\u00b7ker", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Seele hat man hinein betrogen,", "tokens": ["Die", "See\u00b7le", "hat", "man", "hin\u00b7ein", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da hat sie nicht freye Ellebogen.", "tokens": ["Da", "hat", "sie", "nicht", "frey\u00b7e", "El\u00b7le\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will sie sich da- und dorthin retten:", "tokens": ["Will", "sie", "sich", "da", "und", "dor\u00b7thin", "ret\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "TRUNC", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schn\u00fcrt man den Kerker selbst in Ketten,", "tokens": ["Schn\u00fcrt", "man", "den", "Ker\u00b7ker", "selbst", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Da ist das Liebchen doppelt gef\u00e4hrdet,", "tokens": ["Da", "ist", "das", "Lieb\u00b7chen", "dop\u00b7pelt", "ge\u00b7f\u00e4hr\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Desshalb sie sich oft so seltsam geb\u00e4rdet.", "tokens": ["Dess\u00b7halb", "sie", "sich", "oft", "so", "selt\u00b7sam", "ge\u00b7b\u00e4r\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}