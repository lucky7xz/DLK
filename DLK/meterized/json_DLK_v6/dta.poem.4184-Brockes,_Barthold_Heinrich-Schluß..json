{"dta.poem.4184": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich, unergr\u00fcndlichs Meer der Weisheit, Dich, GOtt,", "tokens": ["Dich", ",", "un\u00b7er\u00b7gr\u00fcnd\u00b7lichs", "Meer", "der", "Weis\u00b7heit", ",", "Dich", ",", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "ART", "NN", "$,", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du allerh\u00f6chstes Guht,", "tokens": ["Du", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Guht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erh\u00f6hn die allertiefste Tiefen, erweist, beweist und preist die", "tokens": ["Er\u00b7h\u00f6hn", "die", "al\u00b7ler\u00b7tiefs\u00b7te", "Tie\u00b7fen", ",", "er\u00b7weist", ",", "be\u00b7weist", "und", "preist", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Fluht.", "tokens": ["Fluht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nicht nur der schuppigten Bewohner der Grund- und", "tokens": ["Nicht", "nur", "der", "schup\u00b7pig\u00b7ten", "Be\u00b7woh\u00b7ner", "der", "Grun\u00b7d", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "ART", "TRUNC", "KON"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Grenzen-losen Meere,", "tokens": ["Gren\u00b7zen\u00b7lo\u00b7sen", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Des unersch\u00f6pflichen Beh\u00e4lters, gesammter Theil\u2019 und", "tokens": ["Des", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7chen", "Be\u00b7h\u00e4l\u00b7ters", ",", "ge\u00b7samm\u00b7ter", "Theil'", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Tropfen Heere", "tokens": ["Trop\u00b7fen", "Hee\u00b7re"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Sind Zeugen Deiner weisen Macht. Willt Du ihr Zeugni\u00df", "tokens": ["Sind", "Zeu\u00b7gen", "Dei\u00b7ner", "wei\u00b7sen", "Macht", ".", "Willt", "Du", "ihr", "Zeug\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nicht verstehn;", "tokens": ["nicht", "ver\u00b7stehn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "O, Mensch, willt du derselben Wunder, Dem, Der sie thut,", "tokens": ["O", ",", "Mensch", ",", "willt", "du", "der\u00b7sel\u00b7ben", "Wun\u00b7der", ",", "Dem", ",", "Der", "sie", "thut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VMFIN", "PPER", "PDAT", "NN", "$,", "PDS", "$,", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "zum Ruhm, nicht sehn,", "tokens": ["zum", "Ruhm", ",", "nicht", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Und GOtt erheben? Ja, dadurch dich selbst zugleich in GOtt", "tokens": ["Und", "Gott", "er\u00b7he\u00b7ben", "?", "Ja", ",", "da\u00b7durch", "dich", "selbst", "zu\u00b7gleich", "in", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$.", "PTKANT", "$,", "PAV", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "erh\u00f6hn?", "tokens": ["er\u00b7h\u00f6hn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}