{"dta.poem.9202": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die verloschne Liebe.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wir jungen leute sind wohl narren/", "tokens": ["Wir", "jun\u00b7gen", "leu\u00b7te", "sind", "wohl", "nar\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann uns die liebe fressen will/", "tokens": ["Wann", "uns", "die", "lie\u00b7be", "fres\u00b7sen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat ein jeder einen sparren", "tokens": ["Da", "hat", "ein", "je\u00b7der", "ei\u00b7nen", "spar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu wenig oder doch zu viel/", "tokens": ["Zu", "we\u00b7nig", "o\u00b7der", "doch", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "ADV", "PTKA", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habs versucht ein halbes jahr/", "tokens": ["Ich", "habs", "ver\u00b7sucht", "ein", "hal\u00b7bes", "jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df/ wie mir zu muthe war.", "tokens": ["Ich", "wei\u00df", "/", "wie", "mir", "zu", "mu\u00b7the", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun mu\u00df ich meiner selbsten lachen/", "tokens": ["Nun", "mu\u00df", "ich", "mei\u00b7ner", "selbs\u00b7ten", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df wir uns solchen kummer machen/", "tokens": ["Da\u00df", "wir", "uns", "sol\u00b7chen", "kum\u00b7mer", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich lege lust und eitelkeit", "tokens": ["Ich", "le\u00b7ge", "lust", "und", "ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu meines m\u00e4dgens f\u00fcssen nieder/", "tokens": ["Zu", "mei\u00b7nes", "m\u00e4d\u00b7gens", "f\u00fcs\u00b7sen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und suche die gelegenheit/", "tokens": ["Und", "su\u00b7che", "die", "ge\u00b7le\u00b7gen\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So gar geschwinde wohl nicht wieder/", "tokens": ["So", "gar", "ge\u00b7schwin\u00b7de", "wohl", "nicht", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "ADV", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich halte mein triumph-geschrey/", "tokens": ["Ich", "hal\u00b7te", "mein", "tri\u00b7um\u00b7ph\u00b7ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Ich war verliebt/ nun bin ich frey.", "tokens": ["Ich", "war", "ver\u00b7liebt", "/", "nun", "bin", "ich", "frey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}