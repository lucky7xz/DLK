{"textgrid.poem.63931": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Bischof und der Bettelbube", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst ging ein Bischof durch die Stadt;", "tokens": ["Einst", "ging", "ein", "Bi\u00b7schof", "durch", "die", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bettelbube zu ihm trat,", "tokens": ["Ein", "Bet\u00b7tel\u00b7bu\u00b7be", "zu", "ihm", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zog vor ihm ab gar tief den Hut,", "tokens": ["Zog", "vor", "ihm", "ab", "gar", "tief", "den", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagte: Herr, seyn Sie so gut,", "tokens": ["Und", "sag\u00b7te", ":", "Herr", ",", "seyn", "Sie", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis an den Hals steck' ich in Schulden,", "tokens": ["Bis", "an", "den", "Hals", "steck'", "ich", "in", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schenken Sie mir einen Gulden", "tokens": ["Und", "schen\u00b7ken", "Sie", "mir", "ei\u00b7nen", "Gul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu diesem lieben Neuenjahr;", "tokens": ["Zu", "die\u00b7sem", "lie\u00b7ben", "Ne\u00b7u\u00b7en\u00b7jahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das w\u00e4r' ein christlich Werk, f\u00fcrwahr!", "tokens": ["Das", "w\u00e4r'", "ein", "christ\u00b7lich", "Werk", ",", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was! (schrie der Bischof eifersvoll)", "tokens": ["Was", "!", "(", "schrie", "der", "Bi\u00b7schof", "ei\u00b7fers\u00b7voll", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "$(", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich glaube, Junge, du bist toll;", "tokens": ["Ich", "glau\u00b7be", ",", "Jun\u00b7ge", ",", "du", "bist", "toll", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Gulden, bei so schlechter Zeit,", "tokens": ["Ein", "Gul\u00b7den", ",", "bei", "so", "schlech\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist wahrlich keine Kleinigkeit!", "tokens": ["Ist", "wahr\u00b7lich", "kei\u00b7ne", "Klei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nun, Herr, (fiel ihm der Bettler ein)", "tokens": ["Nun", ",", "Herr", ",", "(", "fiel", "ihm", "der", "Bett\u00b7ler", "ein", ")"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So m\u00f6gen's denn acht Groschen seyn.", "tokens": ["So", "m\u00f6\u00b7gen's", "denn", "acht", "Gro\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "CARD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nichts, nichts! (versetzt der Bischof d'rauf)", "tokens": ["Nichts", ",", "nichts", "!", "(", "ver\u00b7setzt", "der", "Bi\u00b7schof", "d'\u00b7rauf", ")"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "$.", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Geh' fort und halte mich nicht auf!", "tokens": ["Geh'", "fort", "und", "hal\u00b7te", "mich", "nicht", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr Gnaden! Einen Groschen dann \u2013", "tokens": ["Ihr", "Gna\u00b7den", "!", "Ei\u00b7nen", "Gro\u00b7schen", "dann", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Fort, fort! auch den nicht \u2013 Nun wohlan!", "tokens": ["Fort", ",", "fort", "!", "auch", "den", "nicht", "\u2013", "Nun", "wo\u00b7hlan", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$.", "ADV", "ART", "PTKNEG", "$(", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Sie sehn, wie ich mich handeln lasse,", "tokens": ["Sie", "sehn", ",", "wie", "ich", "mich", "han\u00b7deln", "las\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Hellerchen? \u2013 Geh' deiner Stra\u00dfe,", "tokens": ["Ein", "Hel\u00b7ler\u00b7chen", "?", "\u2013", "Geh'", "dei\u00b7ner", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NE", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Nichts, gar nichts! Das ist etwas arg,", "tokens": ["Nichts", ",", "gar", "nichts", "!", "Das", "ist", "et\u00b7was", "arg", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "PIS", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "(sprach d'rauf der Bube). Sie sind karg!", "tokens": ["(", "sprach", "d'\u00b7rauf", "der", "Bu\u00b7be", ")", ".", "Sie", "sind", "karg", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "ART", "NN", "$(", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Doch lassen Sie sich denn bewegen,", "tokens": ["Doch", "las\u00b7sen", "Sie", "sich", "denn", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und geben mir nur Ihren Segen!", "tokens": ["Und", "ge\u00b7ben", "mir", "nur", "Ih\u00b7ren", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Den sollst du haben, lieber Sohn,", "tokens": ["Den", "sollst", "du", "ha\u00b7ben", ",", "lie\u00b7ber", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAFIN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "(erwiederte mit s\u00fc\u00dfem Ton", "tokens": ["(", "er\u00b7wie\u00b7der\u00b7te", "mit", "s\u00fc\u00b7\u00dfem", "Ton"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.27": {"text": "Der Geistliche) knie' hin vor mir,", "tokens": ["Der", "Geist\u00b7li\u00b7che", ")", "knie'", "hin", "vor", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den besten Segen geb' ich dir!", "tokens": ["Den", "bes\u00b7ten", "Se\u00b7gen", "geb'", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So? sprach der Bursche ganz verwegen,", "tokens": ["So", "?", "sprach", "der", "Bur\u00b7sche", "ganz", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Behalten Sie nur Ihren Segen!", "tokens": ["Be\u00b7hal\u00b7ten", "Sie", "nur", "Ih\u00b7ren", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ich hab' ihn zu geschwind begehrt;", "tokens": ["Ich", "hab'", "ihn", "zu", "ge\u00b7schwind", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "W\u00e4r' er nur einen Heller werth,", "tokens": ["W\u00e4r'", "er", "nur", "ei\u00b7nen", "Hel\u00b7ler", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Sie g\u00e4ben ihn, hochw\u00fcrd'ger Herr,", "tokens": ["Sie", "g\u00e4\u00b7ben", "ihn", ",", "hoch\u00b7w\u00fcrd'\u00b7ger", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.34": {"text": "Gewi\u00df nicht so gutwillig her.", "tokens": ["Ge\u00b7wi\u00df", "nicht", "so", "gut\u00b7wil\u00b7lig", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}