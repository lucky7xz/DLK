{"dta.poem.16054": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein herrliches Gef\u00fchl ist es, in sich empfinden,", "tokens": ["Ein", "herr\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "ist", "es", ",", "in", "sich", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Lichter tauchen auf, und dunkle Wolken schwinden;", "tokens": ["Wie", "Lich\u00b7ter", "tau\u00b7chen", "auf", ",", "und", "dunk\u00b7le", "Wol\u00b7ken", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Obauch der Lichter Glanz nicht mag zu sehn gestatten,", "tokens": ["O\u00b7bauch", "der", "Lich\u00b7ter", "Glanz", "nicht", "mag", "zu", "sehn", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKNEG", "VMFIN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von den Wolken noch geblieben sind die Schatten:", "tokens": ["Und", "von", "den", "Wol\u00b7ken", "noch", "ge\u00b7blie\u00b7ben", "sind", "die", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du kniest am Heiligthum der halbenth\u00fcllten Wahrheit,", "tokens": ["Du", "kniest", "am", "Hei\u00b7lig\u00b7thum", "der", "hal\u00b7bent\u00b7h\u00fcll\u00b7ten", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehst vertrauenvoll entgegen voller Klarheit.", "tokens": ["Und", "siehst", "ver\u00b7trau\u00b7en\u00b7voll", "ent\u00b7ge\u00b7gen", "vol\u00b7ler", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}