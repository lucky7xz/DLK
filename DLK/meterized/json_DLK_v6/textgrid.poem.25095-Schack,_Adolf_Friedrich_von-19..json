{"textgrid.poem.25095": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Schlucht hat schon zu dichten", "tokens": ["In", "der", "Schlucht", "hat", "schon", "zu", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haufen sich das Laub get\u00fcrmt.", "tokens": ["Hau\u00b7fen", "sich", "das", "Laub", "ge\u00b7t\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend neu der Herbstwind Schichten", "tokens": ["W\u00e4h\u00b7rend", "neu", "der", "Herbst\u00b7wind", "Schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welker Bl\u00e4tter niederst\u00fcrmt.", "tokens": ["Wel\u00b7ker", "Bl\u00e4t\u00b7ter", "nie\u00b7ders\u00b7t\u00fcrmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber durch das Sturmgetose", "tokens": ["A\u00b7ber", "durch", "das", "Sturm\u00b7ge\u00b7to\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Moderdunst der Kluft", "tokens": ["Und", "den", "Mo\u00b7der\u00b7dunst", "der", "Kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haucht noch einsam eine Rose", "tokens": ["Haucht", "noch", "ein\u00b7sam", "ei\u00b7ne", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Kelches s\u00fc\u00dfen Duft.", "tokens": ["Ih\u00b7res", "Kel\u00b7ches", "s\u00fc\u00b7\u00dfen", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe! Aus begrabnen Jahren", "tokens": ["Lie\u00b7be", "!", "Aus", "be\u00b7grab\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In mein Leben, l\u00e4ngst verdorrt,", "tokens": ["In", "mein", "Le\u00b7ben", ",", "l\u00e4ngst", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauchst du deine wunderbaren", "tokens": ["Hauchst", "du", "dei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Milden D\u00fcfte fort und fort.", "tokens": ["Mil\u00b7den", "D\u00fcf\u00b7te", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}