{"textgrid.poem.48235": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aber wir lassen es andere machen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Chinese ('s sind schon an 200 Jahr)", "tokens": ["Ein", "Chi\u00b7ne\u00b7se", "(", "'s", "sind", "schon", "an", "200", "Jahr", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "number", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VAFIN", "ADV", "APPR", "CARD", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Frankreich auf einem Hofball war.", "tokens": ["In", "Fran\u00b7kreich", "auf", "ei\u00b7nem", "Hof\u00b7ball", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die einen frugen ihn: ob er das kenne?", "tokens": ["Und", "die", "ei\u00b7nen", "fru\u00b7gen", "ihn", ":", "ob", "er", "das", "ken\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "PPER", "$.", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die andern frugen ihn: wie man es nenne?", "tokens": ["Und", "die", "an\u00b7dern", "fru\u00b7gen", "ihn", ":", "wie", "man", "es", "nen\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "PPER", "$.", "PWAV", "PIS", "PPER", "VVFIN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbwir nennen es tanzen\u00ab, sprach er mit Lachen,", "tokens": ["\u00bb", "wir", "nen\u00b7nen", "es", "tan\u00b7zen", "\u00ab", ",", "sprach", "er", "mit", "La\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VVINF", "$(", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbaber wir lassen es ", "tokens": ["\u00bb", "a\u00b7ber", "wir", "las\u00b7sen", "es"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und dieses Wort seit langer Frist,", "tokens": ["Und", "die\u00b7ses", "Wort", "seit", "lan\u00b7ger", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir immer in Erinnerung ist.", "tokens": ["Mir", "im\u00b7mer", "in", "E\u00b7rin\u00b7ne\u00b7rung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich seh' das Rennen, ich seh' das Jagen,", "tokens": ["Ich", "seh'", "das", "Ren\u00b7nen", ",", "ich", "seh'", "das", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn mich die Menschen umdr\u00e4ngen und fragen:", "tokens": ["Und", "wenn", "mich", "die", "Men\u00b7schen", "um\u00b7dr\u00e4n\u00b7gen", "und", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u00bbwas tust du nicht mit? Warum stehst du beiseit'?\u00ab", "tokens": ["\u00bb", "was", "tust", "du", "nicht", "mit", "?", "Wa\u00b7rum", "stehst", "du", "bei\u00b7seit'", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So sag ich: \u00bbAlles hat seine Zeit.", "tokens": ["So", "sag", "ich", ":", "\u00bb", "Al\u00b7les", "hat", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Auch die Jagd nach dem Gl\u00fcck. All derlei Sachen,", "tokens": ["Auch", "die", "Jagd", "nach", "dem", "Gl\u00fcck", ".", "All", "der\u00b7lei", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$.", "NN", "PIAT", "NN", "$,"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ich lasse sie l\u00e4ngst durch andere machen.\u00ab", "tokens": ["Ich", "las\u00b7se", "sie", "l\u00e4ngst", "durch", "an\u00b7de\u00b7re", "ma\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}