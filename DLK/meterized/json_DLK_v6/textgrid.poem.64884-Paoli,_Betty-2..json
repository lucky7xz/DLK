{"textgrid.poem.64884": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du, wenn in Glanzesf\u00fclle", "tokens": ["Sahst", "du", ",", "wenn", "in", "Glan\u00b7zes\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmetterling zum Himmel flog,", "tokens": ["Schmet\u00b7ter\u00b7ling", "zum", "Him\u00b7mel", "flog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er seine Raupenh\u00fclle", "tokens": ["Da\u00df", "er", "sei\u00b7ne", "Rau\u00b7pen\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4ter abermals bezog?", "tokens": ["Sp\u00e4\u00b7ter", "a\u00b7ber\u00b7mals", "be\u00b7zog", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag die Zukunft niederrei\u00dfen,", "tokens": ["Mag", "die", "Zu\u00b7kunft", "nie\u00b7der\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mein Hoffen auferbaut:", "tokens": ["Was", "mein", "Hof\u00b7fen", "au\u00b7fer\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie kann ich dir Freundin hei\u00dfen,", "tokens": ["Nie", "kann", "ich", "dir", "Freun\u00b7din", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ich Schwester jetzt und Braut!", "tokens": ["Dem", "ich", "Schwes\u00b7ter", "jetzt", "und", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nie w\u00fcrd' ich's zu tragen wissen,", "tokens": ["Nie", "w\u00fcrd'", "ich's", "zu", "tra\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem theuern Angesicht", "tokens": ["In", "dem", "theu\u00b7ern", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Je den Schimmer zu vermissen,", "tokens": ["Je", "den", "Schim\u00b7mer", "zu", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der jetzt liebend daraus bricht.", "tokens": ["Der", "jetzt", "lie\u00b7bend", "da\u00b7raus", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und aus offner, tiefer Wunde", "tokens": ["Und", "aus", "off\u00b7ner", ",", "tie\u00b7fer", "Wun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6mte hei\u00df mein Herzblut fort,", "tokens": ["Str\u00f6m\u00b7te", "hei\u00df", "mein", "Herz\u00b7blut", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt ich je von deinem Munde", "tokens": ["H\u00f6rt", "ich", "je", "von", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Matter Freundschaft k\u00fchles Wort. \u2013", "tokens": ["Mat\u00b7ter", "Freund\u00b7schaft", "k\u00fch\u00b7les", "Wort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer zu tiefstem Weh erkoren,", "tokens": ["Wer", "zu", "tiefs\u00b7tem", "Weh", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird von h\u00f6chster Kraft beseelt;", "tokens": ["Wird", "von", "h\u00f6chs\u00b7ter", "Kraft", "be\u00b7seelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Krone einst verloren,", "tokens": ["Ist", "die", "Kro\u00b7ne", "einst", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei der Schleier rasch erw\u00e4hlt!", "tokens": ["Sei", "der", "Schlei\u00b7er", "rasch", "er\u00b7w\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}