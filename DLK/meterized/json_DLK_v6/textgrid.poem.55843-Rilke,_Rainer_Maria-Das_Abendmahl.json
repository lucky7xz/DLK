{"textgrid.poem.55843": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Abendmahl", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sind versammelt, staunende Verst\u00f6rte,", "tokens": ["Sie", "sind", "ver\u00b7sam\u00b7melt", ",", "stau\u00b7nen\u00b7de", "Ver\u00b7st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "um ihn, der wie ein Weiser sich beschlie\u00dft", "tokens": ["um", "ihn", ",", "der", "wie", "ein", "Wei\u00b7ser", "sich", "be\u00b7schlie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PRELS", "KOKOM", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und der sich fortnimmt denen er geh\u00f6rte", "tokens": ["und", "der", "sich", "fort\u00b7nimmt", "de\u00b7nen", "er", "ge\u00b7h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELS", "PRF", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und der an ihnen fremd vor\u00fcberflie\u00dft.", "tokens": ["und", "der", "an", "ih\u00b7nen", "fremd", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die alte Einsamkeit kommt \u00fcber ihn,", "tokens": ["Die", "al\u00b7te", "Ein\u00b7sam\u00b7keit", "kommt", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die ihn erzog zu seinem tiefen Handeln;", "tokens": ["die", "ihn", "er\u00b7zog", "zu", "sei\u00b7nem", "tie\u00b7fen", "Han\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "nun wird er wieder durch den \u00d6lwald wandeln,", "tokens": ["nun", "wird", "er", "wie\u00b7der", "durch", "den", "\u00d6l\u00b7wald", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und die ihn lieben werden vor ihm fliehn.", "tokens": ["und", "die", "ihn", "lie\u00b7ben", "wer\u00b7den", "vor", "ihm", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "VVINF", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er hat sie zu dem letzten Tisch entboten", "tokens": ["Er", "hat", "sie", "zu", "dem", "letz\u00b7ten", "Tisch", "ent\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und (wie ein Schu\u00df die V\u00f6gel aus den Schoten", "tokens": ["und", "(", "wie", "ein", "Schu\u00df", "die", "V\u00f6\u00b7gel", "aus", "den", "Scho\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "KOKOM", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "scheucht) scheucht er ihre H\u00e4nde aus den Broten", "tokens": ["scheucht", ")", "scheucht", "er", "ih\u00b7re", "H\u00e4n\u00b7de", "aus", "den", "Bro\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit seinem Wort: sie fliegen zu ihm her;", "tokens": ["mit", "sei\u00b7nem", "Wort", ":", "sie", "flie\u00b7gen", "zu", "ihm", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "sie flattern bange durch die Tafelrunde", "tokens": ["sie", "flat\u00b7tern", "ban\u00b7ge", "durch", "die", "Ta\u00b7fel\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und suchen einen Ausgang. Aber ", "tokens": ["und", "su\u00b7chen", "ei\u00b7nen", "Aus\u00b7gang", ".", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ist \u00fcberall wie eine D\u00e4mmerstunde.", "tokens": ["ist", "\u00fc\u00b7be\u00b7rall", "wie", "ei\u00b7ne", "D\u00e4m\u00b7mer\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}