{"textgrid.poem.24116": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Kinder sich fassen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Kinder sich fassen", "tokens": ["Wie", "Kin\u00b7der", "sich", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "An ihren unschuldigen H\u00e4nden,", "tokens": ["An", "ih\u00b7ren", "un\u00b7schul\u00b7di\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So, meine Stunden,", "tokens": ["So", ",", "mei\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "W\u00fcnscht ich euch eilenden,", "tokens": ["W\u00fcnscht", "ich", "euch", "ei\u00b7len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Da\u00df ihr, zum heitersten Spiele verkettet,", "tokens": ["Da\u00df", "ihr", ",", "zum", "hei\u00b7ters\u00b7ten", "Spie\u00b7le", "ver\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Tanzend euch z\u00f6gt \u00fcber blumige Wiesen,", "tokens": ["Tan\u00b7zend", "euch", "z\u00f6gt", "\u00fc\u00b7ber", "blu\u00b7mi\u00b7ge", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Klarheit \u00fcber euch,", "tokens": ["Klar\u00b7heit", "\u00fc\u00b7ber", "euch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Unschuld in euch,", "tokens": ["Un\u00b7schuld", "in", "euch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Reiner Seele voll,", "tokens": ["Rei\u00b7ner", "See\u00b7le", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Junger Frische voll,", "tokens": ["Jun\u00b7ger", "Fri\u00b7sche", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Lachend.", "tokens": ["La\u00b7chend", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ach, mein Himmel ist nicht so klar,", "tokens": ["Ach", ",", "mein", "Him\u00b7mel", "ist", "nicht", "so", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und meine Stunden sind nicht so rein;", "tokens": ["Und", "mei\u00b7ne", "Stun\u00b7den", "sind", "nicht", "so", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unschuld, Kindheit, Spiel und Tanz", "tokens": ["Un\u00b7schuld", ",", "Kind\u00b7heit", ",", "Spiel", "und", "Tanz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind mir wie entschwebende Wolken,", "tokens": ["Sind", "mir", "wie", "ent\u00b7schwe\u00b7ben\u00b7de", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denen die Arme zum Himmel auf", "tokens": ["De\u00b7nen", "die", "Ar\u00b7me", "zum", "Him\u00b7mel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Meine Sehnsucht weinend breitet,", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", "wei\u00b7nend", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der J\u00fcngling am tosenden Flusse,", "tokens": ["Wie", "der", "J\u00fcng\u00b7ling", "am", "to\u00b7sen\u00b7den", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Der den M\u00e4rchenv\u00f6geln nachblickt.", "tokens": ["Der", "den", "M\u00e4r\u00b7chen\u00b7v\u00f6\u00b7geln", "nach\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich einmal dem Meister gliche,", "tokens": ["Da\u00df", "ich", "ein\u00b7mal", "dem", "Meis\u00b7ter", "gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der euch malte, Kinder und dich,", "tokens": ["Der", "euch", "mal\u00b7te", ",", "Kin\u00b7der", "und", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "NN", "KON", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Betender J\u00fcngling!", "tokens": ["Be\u00b7ten\u00b7der", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Stille und G\u00fcte,", "tokens": ["Stil\u00b7le", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Klarheit und Kraft,", "tokens": ["Klar\u00b7heit", "und", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Spielender Ernst und schaffende Treue", "tokens": ["Spie\u00b7len\u00b7der", "Ernst", "und", "schaf\u00b7fen\u00b7de", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wohnen und walten an seinem Herde;", "tokens": ["Woh\u00b7nen", "und", "wal\u00b7ten", "an", "sei\u00b7nem", "Her\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Gl\u00fcck ward hier lebendige Gottheit,", "tokens": ["Gl\u00fcck", "ward", "hier", "le\u00b7ben\u00b7di\u00b7ge", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Weilende, heimische, dauernde: Ordnung;", "tokens": ["Wei\u00b7len\u00b7de", ",", "hei\u00b7mi\u00b7sche", ",", "dau\u00b7ern\u00b7de", ":", "Ord\u00b7nung", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "VVFIN", "$.", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Gl\u00fcck, das fliegende, ward hier fest.", "tokens": ["Gl\u00fcck", ",", "das", "flie\u00b7gen\u00b7de", ",", "ward", "hier", "fest", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Schlichter Mann im wei\u00dfen Haare,", "tokens": ["Schlich\u00b7ter", "Mann", "im", "wei\u00b7\u00dfen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich deine H\u00e4nde dr\u00fccken,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dank im Herzen, stummen Mundes:", "tokens": ["Dank", "im", "Her\u00b7zen", ",", "stum\u00b7men", "Mun\u00b7des", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segne mich mit deinen klaren,", "tokens": ["Seg\u00b7ne", "mich", "mit", "dei\u00b7nen", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Guten Augen, schlichter Mann!", "tokens": ["Gu\u00b7ten", "Au\u00b7gen", ",", "schlich\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}