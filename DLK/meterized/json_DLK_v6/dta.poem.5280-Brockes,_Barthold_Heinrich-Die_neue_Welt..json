{"dta.poem.5280": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die Kr\u00e4fte meiner Seelen", "tokens": ["Wann", "die", "Kr\u00e4f\u00b7te", "mei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Fr\u00fchlings sch\u00f6ner Pracht,", "tokens": ["Mit", "des", "Fr\u00fch\u00b7lings", "sch\u00f6\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem zum Ruhm, der sie gemacht,", "tokens": ["Dem", "zum", "Ruhm", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in froher Lust verm\u00e4hlen;", "tokens": ["Sich", "in", "fro\u00b7her", "Lust", "ver\u00b7m\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bringen sie in holder Zier", "tokens": ["Brin\u00b7gen", "sie", "in", "hol\u00b7der", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Creatur, in mir", "tokens": ["Aus", "der", "Crea\u00b7tur", ",", "in", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eine gleichsam neue Welt,", "tokens": ["Ei\u00b7ne", "gleich\u00b7sam", "neu\u00b7e", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Welche geistig ist, herf\u00fcr.", "tokens": ["Wel\u00b7che", "geis\u00b7tig", "ist", ",", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Diese bleibt in meinen Sinn,", "tokens": ["Die\u00b7se", "bleibt", "in", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und sie ist mir w\u00fcrcklich eigen;", "tokens": ["Und", "sie", "ist", "mir", "w\u00fcrck\u00b7lich", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mein Ged\u00e4chtni\u00df kann sie zeigen,", "tokens": ["Mein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "kann", "sie", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn auch jene gleich dahin.", "tokens": ["Wenn", "auch", "je\u00b7ne", "gleich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "In ihr kann man wundersch\u00f6n,", "tokens": ["In", "ihr", "kann", "man", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wenn man will, den Fr\u00fchling sehn.", "tokens": ["Wenn", "man", "will", ",", "den", "Fr\u00fch\u00b7ling", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Die Bewohner dieser Welt", "tokens": ["Die", "Be\u00b7woh\u00b7ner", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sind betrachtende Jdeen,", "tokens": ["Sind", "be\u00b7trach\u00b7ten\u00b7de", "Jdeen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Denen sie, je mehr sie sehen,", "tokens": ["De\u00b7nen", "sie", ",", "je", "mehr", "sie", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "Auch stets desto mehr gef\u00e4llt;", "tokens": ["Auch", "stets", "des\u00b7to", "mehr", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Sind Erk\u00e4nntni\u00df, Gegen-Liebe,", "tokens": ["Sind", "Er\u00b7k\u00e4nnt\u00b7ni\u00df", ",", "Ge\u00b7gen\u00b7Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Danck und Andacht volle Triebe;", "tokens": ["Danck", "und", "An\u00b7dacht", "vol\u00b7le", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Eine Sucht, sie auszudr\u00fccken;", "tokens": ["Ei\u00b7ne", "Sucht", ",", "sie", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sind ein inniglich Entz\u00fccken", "tokens": ["Sind", "ein", "in\u00b7nig\u00b7lich", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Uber alle Wunder-F\u00fclle;", "tokens": ["U\u00b7ber", "al\u00b7le", "Wun\u00b7der\u00b7F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sind ein feuerreicher Wille,", "tokens": ["Sind", "ein", "feu\u00b7er\u00b7rei\u00b7cher", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Voller Luft in allen Dingen", "tokens": ["Vol\u00b7ler", "Luft", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "GoTTES Willen zu vollbringen;", "tokens": ["GoT\u00b7TES", "Wil\u00b7len", "zu", "voll\u00b7brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Anmuth, und Gelassenheit;", "tokens": ["An\u00b7muth", ",", "und", "Ge\u00b7las\u00b7sen\u00b7heit", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Ein gegr\u00fcndetes Vertrauen,", "tokens": ["Ein", "ge\u00b7gr\u00fcn\u00b7de\u00b7tes", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Auch dereinst, nach dieser Zeit,", "tokens": ["Auch", "de\u00b7reinst", ",", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Noch was herrlichers zu schauen.", "tokens": ["Noch", "was", "herr\u00b7li\u00b7chers", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}