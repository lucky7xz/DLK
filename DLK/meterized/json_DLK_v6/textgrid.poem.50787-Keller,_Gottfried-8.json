{"textgrid.poem.50787": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als endlich sie, nach langem, schwankem Lauf,", "tokens": ["Als", "end\u00b7lich", "sie", ",", "nach", "lan\u00b7gem", ",", "schwan\u00b7kem", "Lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Grab noch hoben diesen Deckel auf:", "tokens": ["Am", "Grab", "noch", "ho\u00b7ben", "die\u00b7sen", "De\u00b7ckel", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In jenem Augenblick hab ich gesehn,", "tokens": ["In", "je\u00b7nem", "Au\u00b7gen\u00b7blick", "hab", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie just die Sonne schied im Untergehn.", "tokens": ["Wie", "just", "die", "Son\u00b7ne", "schied", "im", "Un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erleuchtet von dem abendroten Strahl", "tokens": ["Er\u00b7leuch\u00b7tet", "von", "dem", "a\u00b7ben\u00b7dro\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich all die Gesichter noch einmal,", "tokens": ["Sah", "ich", "all", "die", "Ge\u00b7sich\u00b7ter", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den Turmknopf oben in der goldnen Ruh \u2013", "tokens": ["Den", "Turm\u00b7knopf", "o\u00b7ben", "in", "der", "gold\u00b7nen", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein Blitz, sie schlugen wieder zu.", "tokens": ["Es", "war", "ein", "Blitz", ",", "sie", "schlu\u00b7gen", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah auch zwischen Auf- und Niederschlag,", "tokens": ["Ich", "sah", "auch", "zwi\u00b7schen", "Auf", "und", "Nie\u00b7der\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df M\u00e4rzschnee dicht auf allen Gr\u00e4bern lag;", "tokens": ["Da\u00df", "M\u00e4rz\u00b7schnee", "dicht", "auf", "al\u00b7len", "Gr\u00e4\u00b7bern", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Wetter mu\u00df seither gebrochen sein,", "tokens": ["Das", "Wet\u00b7ter", "mu\u00df", "sei\u00b7ther", "ge\u00b7bro\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Denn feucht dringt es durch diesen engen Schrein.", "tokens": ["Denn", "feucht", "dringt", "es", "durch", "die\u00b7sen", "en\u00b7gen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6r ein Knistern, wie wenn, m\u00e4hlich, leis,", "tokens": ["Ich", "h\u00f6r", "ein", "Knis\u00b7tern", ",", "wie", "wenn", ",", "m\u00e4h\u00b7lich", ",", "leis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOKOM", "KOUS", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich Schollen l\u00f6sen aus des Winters Eis;", "tokens": ["Sich", "Schol\u00b7len", "l\u00f6\u00b7sen", "aus", "des", "Win\u00b7ters", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00f6r ein feines Rieseln, wie wenn sacht", "tokens": ["Ich", "h\u00f6r", "ein", "fei\u00b7nes", "Rie\u00b7seln", ",", "wie", "wenn", "sacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Erdreich aus dem starren Schlaf erwacht.", "tokens": ["Das", "Er\u00b7dreich", "aus", "dem", "star\u00b7ren", "Schlaf", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "O wehe, wehe mir! nun darf es k\u00fchn", "tokens": ["O", "we\u00b7he", ",", "we\u00b7he", "mir", "!", "nun", "darf", "es", "k\u00fchn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus in Gottes freien Himmel bl\u00fchn!", "tokens": ["Hin\u00b7aus", "in", "Got\u00b7tes", "frei\u00b7en", "Him\u00b7mel", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O wehe mir! ich bin ja auch erwacht", "tokens": ["O", "we\u00b7he", "mir", "!", "ich", "bin", "ja", "auch", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kann nicht regen mich in Grabesnacht!", "tokens": ["Und", "kann", "nicht", "re\u00b7gen", "mich", "in", "Gra\u00b7bes\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJA", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie jedes Samenkorn sich m\u00e4chtig dehnt,", "tokens": ["Wie", "je\u00b7des", "Sa\u00b7men\u00b7korn", "sich", "m\u00e4ch\u00b7tig", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der junge Halm nach jungem Licht sich sehnt,", "tokens": ["Der", "jun\u00b7ge", "Halm", "nach", "jun\u00b7gem", "Licht", "sich", "sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reck ich meinen armen, armen Leib \u2013", "tokens": ["So", "reck", "ich", "mei\u00b7nen", "ar\u00b7men", ",", "ar\u00b7men", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Oh, 's ist ein fruchtlos grimmer Zeitvertreib!", "tokens": ["Oh", ",", "'s", "ist", "ein", "frucht\u00b7los", "grim\u00b7mer", "Zeit\u00b7ver\u00b7treib", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt man nicht klopfen laut da obenw\u00e4rts", "tokens": ["H\u00f6rt", "man", "nicht", "klop\u00b7fen", "laut", "da", "o\u00b7ben\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "VVFIN", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hier mein lebendiges begrabnes Herz?", "tokens": ["Hier", "mein", "le\u00b7ben\u00b7di\u00b7ges", "be\u00b7grab\u00b7nes", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "O w\u00fc\u00dften sie, wie es da unten tut!", "tokens": ["O", "w\u00fc\u00df\u00b7ten", "sie", ",", "wie", "es", "da", "un\u00b7ten", "tut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fluch \u00fcber die gedankenlose Brut!", "tokens": ["Fluch", "\u00fc\u00b7ber", "die", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7se", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie munter quillt der k\u00fchle Erdensaft!", "tokens": ["Wie", "mun\u00b7ter", "quillt", "der", "k\u00fch\u00b7le", "Er\u00b7den\u00b7saft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "L\u00f6sch aus nur meines Lebens Fieberkraft!", "tokens": ["L\u00f6sch", "aus", "nur", "mei\u00b7nes", "Le\u00b7bens", "Fie\u00b7ber\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu allen Fugen rinnt es mir herein,", "tokens": ["Zu", "al\u00b7len", "Fu\u00b7gen", "rinnt", "es", "mir", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und oben ist's nun warmer Fr\u00fchlingsschein.", "tokens": ["Und", "o\u00b7ben", "ist's", "nun", "war\u00b7mer", "Fr\u00fch\u00b7lings\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}