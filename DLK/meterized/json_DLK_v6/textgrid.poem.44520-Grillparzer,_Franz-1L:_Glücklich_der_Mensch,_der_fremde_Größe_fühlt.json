{"textgrid.poem.44520": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fccklich der Mensch, der fremde Gr\u00f6\u00dfe f\u00fchlt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich der Mensch, der fremde Gr\u00f6\u00dfe f\u00fchlt", "tokens": ["Gl\u00fcck\u00b7lich", "der", "Mensch", ",", "der", "frem\u00b7de", "Gr\u00f6\u00b7\u00dfe", "f\u00fchlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sie durch Liebe macht zu seiner eignen.", "tokens": ["Und", "sie", "durch", "Lie\u00b7be", "macht", "zu", "sei\u00b7ner", "eig\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn gro\u00df zu sein ist wenigen geg\u00f6nnt,", "tokens": ["Denn", "gro\u00df", "zu", "sein", "ist", "we\u00b7ni\u00b7gen", "ge\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VAINF", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wer dem fremden Wert die Brust verschlie\u00dft,", "tokens": ["Und", "wer", "dem", "frem\u00b7den", "Wert", "die", "Brust", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der lebt in einem \u00f6den Selbst allein,", "tokens": ["Der", "lebt", "in", "ei\u00b7nem", "\u00f6\u00b7den", "Selbst", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Darbender, wohl etwa ein Gemeiner.", "tokens": ["Ein", "Dar\u00b7ben\u00b7der", ",", "wohl", "et\u00b7wa", "ein", "Ge\u00b7mei\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem Land auch Heil, das sie gebar, ges\u00e4ugt", "tokens": ["Dem", "Land", "auch", "Heil", ",", "das", "sie", "ge\u00b7bar", ",", "ge\u00b7s\u00e4ugt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und aufgezogen an den Mutterbr\u00fcsten!", "tokens": ["Und", "auf\u00b7ge\u00b7zo\u00b7gen", "an", "den", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn die Natur gibt nur der Gr\u00f6\u00dfe Geist,", "tokens": ["Denn", "die", "Na\u00b7tur", "gibt", "nur", "der", "Gr\u00f6\u00b7\u00dfe", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den K\u00f6rper bildet an ihr die Umgebung,", "tokens": ["Den", "K\u00f6r\u00b7per", "bil\u00b7det", "an", "ihr", "die", "Um\u00b7ge\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "In der sie allererst den Tag geschaut,", "tokens": ["In", "der", "sie", "al\u00b7le\u00b7rerst", "den", "Tag", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Freunde Schar, der Mitgebornen Kreis,", "tokens": ["Der", "Freun\u00b7de", "Schar", ",", "der", "Mit\u00b7ge\u00b7bor\u00b7nen", "Kreis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die sie mit Blick und Laut zuerst begr\u00fc\u00dft,", "tokens": ["Die", "sie", "mit", "Blick", "und", "Laut", "zu\u00b7erst", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mit frommem Sinn bereitet ihre St\u00e4tte.", "tokens": ["Mit", "from\u00b7mem", "Sinn", "be\u00b7rei\u00b7tet", "ih\u00b7re", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "F\u00fcr Menschen \u2013 nur durch Menschen \u2013 wird der Mensch.", "tokens": ["F\u00fcr", "Men\u00b7schen", "\u2013", "nur", "durch", "Men\u00b7schen", "\u2013", "wird", "der", "Mensch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADV", "APPR", "NN", "$(", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Darob auch mancher, mit der Hoheit Siegel", "tokens": ["Da\u00b7rob", "auch", "man\u00b7cher", ",", "mit", "der", "Ho\u00b7heit", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Bezeichnet von der Sch\u00f6pferin Natur,", "tokens": ["Be\u00b7zeich\u00b7net", "von", "der", "Sch\u00f6p\u00b7fe\u00b7rin", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Noch sp\u00e4t durch irgend eine b\u00f6se Narbe,", "tokens": ["Noch", "sp\u00e4t", "durch", "ir\u00b7gend", "ei\u00b7ne", "b\u00f6\u00b7se", "Nar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Durch einer Gliedma\u00df widrig wildes Zucken,", "tokens": ["Durch", "ei\u00b7ner", "Glied\u00b7ma\u00df", "wid\u00b7rig", "wil\u00b7des", "Zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Durch etwas, das nicht sch\u00f6n, ob stumm, verk\u00fcndet,", "tokens": ["Durch", "et\u00b7was", ",", "das", "nicht", "sch\u00f6n", ",", "ob", "stumm", ",", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PTKNEG", "ADJD", "$,", "KOUS", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie karg der Boden war, in dem die Pflanze", "tokens": ["Wie", "karg", "der", "Bo\u00b7den", "war", ",", "in", "dem", "die", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Des harten Daseins tr\u00fcbe Nahrung sog.", "tokens": ["Des", "har\u00b7ten", "Da\u00b7seins", "tr\u00fc\u00b7be", "Nah\u00b7rung", "so\u00b7g."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "XY"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Drum sind wir stolz, obgleich dem\u00fctig auch:", "tokens": ["Drum", "sind", "wir", "stolz", ",", "ob\u00b7gleich", "de\u00b7m\u00fc\u00b7tig", "auch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Denn hier ward er geboren, den wir feiern!", "tokens": ["Denn", "hier", "ward", "er", "ge\u00b7bo\u00b7ren", ",", "den", "wir", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "In dieses schlichten Landes engen Grenzen", "tokens": ["In", "die\u00b7ses", "schlich\u00b7ten", "Lan\u00b7des", "en\u00b7gen", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Scholl ihm zuerst des Lebens Herold: Ton.", "tokens": ["Scholl", "ihm", "zu\u00b7erst", "des", "Le\u00b7bens", "He\u00b7rold", ":", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "ART", "NN", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Von diesen T\u00fcrmen schwoll ein gl\u00e4ubig L\u00e4uten", "tokens": ["Von", "die\u00b7sen", "T\u00fcr\u00b7men", "schwoll", "ein", "gl\u00e4u\u00b7big", "L\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und lehrt' ihn glauben an die Ahnungen,", "tokens": ["Und", "lehrt'", "ihn", "glau\u00b7ben", "an", "die", "Ah\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Die ohne andre B\u00fcrgen als sich selbst,", "tokens": ["Die", "oh\u00b7ne", "and\u00b7re", "B\u00fcr\u00b7gen", "als", "sich", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "KOUS", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und nur bewiesen, weil sie sich gestaltet,", "tokens": ["Und", "nur", "be\u00b7wie\u00b7sen", ",", "weil", "sie", "sich", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Zur Wirklichkeit verherrlichen den Traum.", "tokens": ["Zur", "Wirk\u00b7lich\u00b7keit", "ver\u00b7herr\u00b7li\u00b7chen", "den", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Von diesen Bergen zog der Gottesatem,", "tokens": ["Von", "die\u00b7sen", "Ber\u00b7gen", "zog", "der", "Got\u00b7te\u00b7sa\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Gew\u00fcrzt mit Kr\u00e4utern und mit Blumenduft,", "tokens": ["Ge\u00b7w\u00fcrzt", "mit", "Kr\u00e4u\u00b7tern", "und", "mit", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "In seine jugendlich gehobne Brust.", "tokens": ["In", "sei\u00b7ne", "ju\u00b7gend\u00b7lich", "ge\u00b7hob\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Darum ist er geworden auch wie sie,", "tokens": ["Da\u00b7rum", "ist", "er", "ge\u00b7wor\u00b7den", "auch", "wie", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VAPP", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Wie diese Berge, seiner Wiege H\u00fcter.", "tokens": ["Wie", "die\u00b7se", "Ber\u00b7ge", ",", "sei\u00b7ner", "Wie\u00b7ge", "H\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wohl gibt es h\u00f6hre, doch sie decket Eis,", "tokens": ["Wohl", "gibt", "es", "h\u00f6h\u00b7re", ",", "doch", "sie", "de\u00b7cket", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Gewaltgere, allein das scheue Leben,", "tokens": ["Ge\u00b7walt\u00b7ge\u00b7re", ",", "al\u00b7lein", "das", "scheu\u00b7e", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Es findet f\u00fcr den Fu\u00dftritt keine Spur", "tokens": ["Es", "fin\u00b7det", "f\u00fcr", "den", "Fu\u00df\u00b7tritt", "kei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und flieht mit Schaudern die erhabne W\u00fcste.", "tokens": ["Und", "flieht", "mit", "Schau\u00b7dern", "die", "er\u00b7hab\u00b7ne", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Er aber klomm so hoch, als Leben reicht,", "tokens": ["Er", "a\u00b7ber", "klomm", "so", "hoch", ",", "als", "Le\u00b7ben", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und stieg so tief, als Leben bl\u00fcht und duftet,", "tokens": ["Und", "stieg", "so", "tief", ",", "als", "Le\u00b7ben", "bl\u00fcht", "und", "duf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und so ward ihm der ewig frische Kranz,", "tokens": ["Und", "so", "ward", "ihm", "der", "e\u00b7wig", "fri\u00b7sche", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Den die Natur ihm wand und mit ihm teilet.", "tokens": ["Den", "die", "Na\u00b7tur", "ihm", "wand", "und", "mit", "ihm", "tei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Nicht was der Mensch in seinem D\u00fcnkel denkt,", "tokens": ["Nicht", "was", "der", "Mensch", "in", "sei\u00b7nem", "D\u00fcn\u00b7kel", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Was Gott verk\u00f6rpert in der Sch\u00f6pfung dachte,", "tokens": ["Was", "Gott", "ver\u00b7k\u00f6r\u00b7pert", "in", "der", "Sch\u00f6p\u00b7fung", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "War ihm der Leitstern seines edlen Tuns.", "tokens": ["War", "ihm", "der", "Leits\u00b7tern", "sei\u00b7nes", "ed\u00b7len", "Tuns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc