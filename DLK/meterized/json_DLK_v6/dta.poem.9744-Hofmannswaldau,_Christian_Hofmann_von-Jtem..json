{"dta.poem.9744": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njemand der klage ja das gl\u00fccke f\u00e4lschlich an/", "tokens": ["Nje\u00b7mand", "der", "kla\u00b7ge", "ja", "das", "gl\u00fc\u00b7cke", "f\u00e4lschlich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "ADV", "PDS", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df es zu sparsam sey mit allen seinen gaben/", "tokens": ["Da\u00df", "es", "zu", "spar\u00b7sam", "sey", "mit", "al\u00b7len", "sei\u00b7nen", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "APPR", "PIAT", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die crone/ die ich trug/ ist ohne m\u00fch zu haben/", "tokens": ["Die", "cro\u00b7ne", "/", "die", "ich", "trug", "/", "ist", "oh\u00b7ne", "m\u00fch", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "APPR", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie ein freundlich weib im schlaff erwerben kan.", "tokens": ["Weil", "sie", "ein", "freund\u00b7lich", "weib", "im", "schlaff", "er\u00b7wer\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART"