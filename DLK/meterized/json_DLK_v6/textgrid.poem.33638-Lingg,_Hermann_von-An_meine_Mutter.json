{"textgrid.poem.33638": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "An meine Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Maitag war's, doch tr\u00fcb und tot,", "tokens": ["Ein", "Mai\u00b7tag", "wa\u00b7r's", ",", "doch", "tr\u00fcb", "und", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwer auf den L\u00e4ndern lag die Not,", "tokens": ["Schwer", "auf", "den", "L\u00e4n\u00b7dern", "lag", "die", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf allen V\u00f6lkern lag ein Kummer,", "tokens": ["Auf", "al\u00b7len", "V\u00f6l\u00b7kern", "lag", "ein", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da schlossest, treue Mutter, du", "tokens": ["Da", "schlos\u00b7sest", ",", "treu\u00b7e", "Mut\u00b7ter", ",", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sorgem\u00fcden Augen zu", "tokens": ["Die", "sor\u00b7ge\u00b7m\u00fc\u00b7den", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum langen, sorgelosen Schlummer.", "tokens": ["Zum", "lan\u00b7gen", ",", "sor\u00b7ge\u00b7lo\u00b7sen", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um dein so ernstes Angesicht", "tokens": ["Um", "dein", "so", "erns\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Glorie schien das bleiche Licht", "tokens": ["Wie", "Glo\u00b7rie", "schien", "das", "blei\u00b7che", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schwarzumflorten Trauerkerzen.", "tokens": ["Der", "schwar\u00b7zum\u00b7flor\u00b7ten", "Trau\u00b7er\u00b7ker\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n du warst, wie tr\u00fcb der Tag!", "tokens": ["Wie", "sch\u00f6n", "du", "warst", ",", "wie", "tr\u00fcb", "der", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Fr\u00fchlings erste Blume lag", "tokens": ["Des", "Fr\u00fch\u00b7lings", "ers\u00b7te", "Blu\u00b7me", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf deinem stillgewordnen Herzen.", "tokens": ["Auf", "dei\u00b7nem", "still\u00b7ge\u00b7word\u00b7nen", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du kaltes stilles Herz, das mich", "tokens": ["Du", "kal\u00b7tes", "stil\u00b7les", "Herz", ",", "das", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So warm geliebt, so m\u00fctterlich,", "tokens": ["So", "warm", "ge\u00b7liebt", ",", "so", "m\u00fct\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Weh oft fast f\u00fcr mich gebrochen,", "tokens": ["Vor", "Weh", "oft", "fast", "f\u00fcr", "mich", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O mu\u00df ich's glauben, bist du fort?", "tokens": ["O", "mu\u00df", "ich's", "glau\u00b7ben", ",", "bist", "du", "fort", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Blick, kein Gru\u00df! Dein letztes Wort", "tokens": ["Kein", "Blick", ",", "kein", "Gru\u00df", "!", "Dein", "letz\u00b7tes", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War f\u00fcr die Ewigkeit gesprochen.", "tokens": ["War", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonst sahst du jeden Schmerz und Wahn", "tokens": ["Sonst", "sahst", "du", "je\u00b7den", "Schmerz", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ferne meiner Seele nahn", "tokens": ["Von", "fer\u00b7ne", "mei\u00b7ner", "See\u00b7le", "nahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen Augen, deinen frommen;", "tokens": ["Mit", "dei\u00b7nen", "Au\u00b7gen", ",", "dei\u00b7nen", "from\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie machte stets mein Gl\u00fcck dich reich!", "tokens": ["Wie", "mach\u00b7te", "stets", "mein", "Gl\u00fcck", "dich", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wurde stets bei dir ich weich!", "tokens": ["Wie", "wur\u00b7de", "stets", "bei", "dir", "ich", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "APPR", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und all das soll nicht wiederkommen?", "tokens": ["Und", "all", "das", "soll", "nicht", "wie\u00b7der\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PDS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Um dich \u2013 es sei mein letzter Schmerz.", "tokens": ["Um", "dich", "\u2013", "es", "sei", "mein", "letz\u00b7ter", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$(", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fortan wird f\u00fcr mein lautlos Herz", "tokens": ["For\u00b7tan", "wird", "f\u00fcr", "mein", "laut\u00b7los", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde nichts mehr sein als Erde;", "tokens": ["Die", "Er\u00b7de", "nichts", "mehr", "sein", "als", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "VAINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlaf wohl, o Mutter! Mein Trost ist,", "tokens": ["Schlaf", "wohl", ",", "o", "Mut\u00b7ter", "!", "Mein", "Trost", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df, wie's auch kommt, nach kurzer Frist,", "tokens": ["Da\u00df", ",", "wie's", "auch", "kommt", ",", "nach", "kur\u00b7zer", "Frist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo du jetzt bist, auch ich sein werde.", "tokens": ["Wo", "du", "jetzt", "bist", ",", "auch", "ich", "sein", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "ADV", "PPER", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}