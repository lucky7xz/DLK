{"textgrid.poem.66420": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An den vornehmen M.", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mit ", "tokens": ["Du", "bist", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ei nun! das glaub' ich dir.", "tokens": ["Ei", "nun", "!", "das", "glaub'", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ja allbekannt,", "tokens": ["Es", "ist", "ja", "all\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zeus war mitunter auch ein Stier.", "tokens": ["Zeus", "war", "mi\u00b7tun\u00b7ter", "auch", "ein", "Stier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}