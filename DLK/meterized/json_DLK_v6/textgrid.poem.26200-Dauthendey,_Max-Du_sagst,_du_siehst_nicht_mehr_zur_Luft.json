{"textgrid.poem.26200": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du sagst, du siehst nicht mehr zur Luft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, du siehst nicht mehr zur Luft,", "tokens": ["Du", "sagst", ",", "du", "siehst", "nicht", "mehr", "zur", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil jetzt die Wiesenblumen bl\u00fchen!", "tokens": ["Weil", "jetzt", "die", "Wie\u00b7sen\u00b7blu\u00b7men", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, da\u00df jede dich nur ruft,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "je\u00b7de", "dich", "nur", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der rote Mohn will dir nur gl\u00fchen.", "tokens": ["Der", "ro\u00b7te", "Mohn", "will", "dir", "nur", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und all die tausend Wiesenkleinen", "tokens": ["Und", "all", "die", "tau\u00b7send", "Wie\u00b7sen\u00b7klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich bl\u00fchend nur um dich vereinen.", "tokens": ["Sich", "bl\u00fc\u00b7hend", "nur", "um", "dich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch manchmal f\u00e4llt noch zu den Steinen", "tokens": ["Doch", "manch\u00b7mal", "f\u00e4llt", "noch", "zu", "den", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ganz nebenbei ein Blick von dir,", "tokens": ["Ganz", "ne\u00b7ben\u00b7bei", "ein", "Blick", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und dann bl\u00fchn alle Steine mir.", "tokens": ["Und", "dann", "bl\u00fchn", "al\u00b7le", "Stei\u00b7ne", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}