{"textgrid.poem.45080": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df sie sich br\u00fcsten mit erzwungnen Gaben,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie sich br\u00fcsten mit erzwungnen Gaben,", "tokens": ["La\u00df", "sie", "sich", "br\u00fcs\u00b7ten", "mit", "er\u00b7zwung\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Ziel erstreben mit gewagten W\u00fcrfen,", "tokens": ["Das", "Ziel", "er\u00b7stre\u00b7ben", "mit", "ge\u00b7wag\u00b7ten", "W\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du spielst f\u00fcr H\u00f6rer, die das Sch\u00f6ne ", "tokens": ["Du", "spielst", "f\u00fcr", "H\u00f6\u00b7rer", ",", "die", "das", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die andern nur f\u00fcr solche, dies bed\u00fcrfen.", "tokens": ["Die", "an\u00b7dern", "nur", "f\u00fcr", "sol\u00b7che", ",", "dies", "be\u00b7d\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPR", "PIAT", "$,", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}