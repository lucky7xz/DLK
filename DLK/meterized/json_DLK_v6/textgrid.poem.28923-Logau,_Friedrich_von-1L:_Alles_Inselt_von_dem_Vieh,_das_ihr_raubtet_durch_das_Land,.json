{"textgrid.poem.28923": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles Inselt von dem Vieh, das ihr raubtet durch das Land,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles Inselt von dem Vieh, das ihr raubtet durch das Land,", "tokens": ["Al\u00b7les", "In\u00b7selt", "von", "dem", "Vieh", ",", "das", "ihr", "raub\u00b7tet", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Asche von gesammtem Ort, den ihr setztet in den Brand,", "tokens": ["A\u00b7sche", "von", "ge\u00b7samm\u00b7tem", "Ort", ",", "den", "ihr", "setz\u00b7tet", "in", "den", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gebe Seiffe nicht genug, auch die Oder reichte nicht", "tokens": ["Ge\u00b7be", "Seif\u00b7fe", "nicht", "ge\u00b7nug", ",", "auch", "die", "O\u00b7der", "reich\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKNEG", "ADV", "$,", "ADV", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Abzuwaschen innren Fleck, dr\u00fcber das Gewissen richt;", "tokens": ["Ab\u00b7zu\u00b7wa\u00b7schen", "inn\u00b7ren", "Fleck", ",", "dr\u00fc\u00b7ber", "das", "Ge\u00b7wis\u00b7sen", "richt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "F\u00fchlt es selbsten, was es ist, ich verschweig es ietzt mit Flei\u00df,", "tokens": ["F\u00fchlt", "es", "selbs\u00b7ten", ",", "was", "es", "ist", ",", "ich", "ver\u00b7schweig", "es", "ietzt", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Weil Gott, was ihr ihm und uns mitgespielet, selbsten wei\u00df.", "tokens": ["Weil", "Gott", ",", "was", "ihr", "ihm", "und", "uns", "mit\u00b7ge\u00b7spie\u00b7let", ",", "selbs\u00b7ten", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWS", "PPER", "PPER", "KON", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}