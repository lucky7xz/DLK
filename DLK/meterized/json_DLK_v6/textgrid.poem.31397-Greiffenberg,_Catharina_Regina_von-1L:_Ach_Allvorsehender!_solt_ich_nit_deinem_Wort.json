{"textgrid.poem.31397": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Allvorsehender! solt ich nit deinem Wort", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Allvorsehender! solt ich nit deinem Wort", "tokens": ["Ach", "All\u00b7vor\u00b7se\u00b7hen\u00b7der", "!", "solt", "ich", "nit", "dei\u00b7nem", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Warheit / Herz bestrickt / Vernunfft-besiegt / nachleben?", "tokens": ["der", "War\u00b7heit", "/", "Herz", "be\u00b7strickt", "/", "Ver\u00b7nunf\u00b7ft\u00b7be\u00b7siegt", "/", "nach\u00b7le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "VVPP", "$(", "VVPP", "$(", "VVIZU", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "kanst du nit / was du vor gegeben / wider geben?", "tokens": ["kanst", "du", "nit", "/", "was", "du", "vor", "ge\u00b7ge\u00b7ben", "/", "wi\u00b7der", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "APPR", "VVPP", "$(", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wann tausend S\u00f6hn ert\u00f6dt / so lebst du noch mein Hort!", "tokens": ["wann", "tau\u00b7send", "S\u00f6hn", "er\u00b7t\u00f6dt", "/", "so", "lebst", "du", "noch", "mein", "Hort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An dich bind ich mein Heil / an kein Gesch\u00f6pf noch Ort.", "tokens": ["An", "dich", "bind", "ich", "mein", "Heil", "/", "an", "kein", "Ge\u00b7sch\u00f6pf", "noch", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "APPR", "PIAT", "NN", "ADV", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Es wird mein Same wol / wie Sternen M\u00e4nge / schweben.", "tokens": ["Es", "wird", "mein", "Sa\u00b7me", "wol", "/", "wie", "Ster\u00b7nen", "M\u00e4n\u00b7ge", "/", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$(", "KOKOM", "NN", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "dem Menschen-Stoff / der Erd / kanst neuen Geist einweben.", "tokens": ["dem", "Men\u00b7schen\u00b7Stoff", "/", "der", "Erd", "/", "kanst", "neu\u00b7en", "Geist", "ein\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "VMFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ist dieser schon geschlacht / die w\u00fcrkungskraft ist dort.", "tokens": ["ist", "die\u00b7ser", "schon", "ge\u00b7schlacht", "/", "die", "w\u00fcr\u00b7kungs\u00b7kraft", "ist", "dort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADV", "VVPP", "$(", "ART", "ADJD", "VAFIN", "ADV", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Zur Schein-Verrichtung jtzt des Bunds / den Grund ich lege:", "tokens": ["Zur", "Schein\u00b7Ver\u00b7rich\u00b7tung", "jtzt", "des", "Bunds", "/", "den", "Grund", "ich", "le\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$(", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.10": {"text": "der Warheits Pfeiler bleibt / dein Wort / vergeht die Erd.", "tokens": ["der", "War\u00b7heits", "Pfei\u00b7ler", "bleibt", "/", "dein", "Wort", "/", "ver\u00b7geht", "die", "Erd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gehorsam / Glaub / Gedult nur treffen Gottes Wege.", "tokens": ["Ge\u00b7hor\u00b7sam", "/", "Glaub", "/", "Ge\u00b7dult", "nur", "tref\u00b7fen", "Got\u00b7tes", "We\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "ADV", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch alle Ordnungs wei\u00df / der H\u00f6chste spricht: Es werd'!", "tokens": ["Durch", "al\u00b7le", "Ord\u00b7nungs", "wei\u00df", "/", "der", "H\u00f6chs\u00b7te", "spricht", ":", "Es", "werd'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(wie in dem Bau der Welt) von dem was Er versprochen.", "tokens": ["(", "wie", "in", "dem", "Bau", "der", "Welt", ")", "von", "dem", "was", "Er", "ver\u00b7spro\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "$(", "APPR", "ART", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ehe wird der Himmel selbst / als Gottes Zusag brochen.", "tokens": ["E\u00b7he", "wird", "der", "Him\u00b7mel", "selbst", "/", "als", "Got\u00b7tes", "Zu\u00b7sag", "bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$(", "KOUS", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}