{"textgrid.poem.37791": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Des Sultans T\u00f6chterlein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sultan hatt' ein T\u00f6chterlein,", "tokens": ["Der", "Sul\u00b7tan", "hatt'", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die war fr\u00fch aufgestanden,", "tokens": ["Die", "war", "fr\u00fch", "auf\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl um zu pfl\u00fccken die Bl\u00fcmelein", "tokens": ["Wohl", "um", "zu", "pfl\u00fc\u00b7cken", "die", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUI", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In ihres Vaters Garten.", "tokens": ["In", "ih\u00b7res", "Va\u00b7ters", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sie die sch\u00f6nen Bl\u00fcmelein", "tokens": ["Da", "sie", "die", "sch\u00f6\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00e4nzen sah im Thaue,", "tokens": ["So", "gl\u00e4n\u00b7zen", "sah", "im", "Thau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer mag der Bl\u00fcmlein Meister seyn,", "tokens": ["Wer", "mag", "der", "Bl\u00fcm\u00b7lein", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedachte die Jungfraue.", "tokens": ["Ge\u00b7dach\u00b7te", "die", "Jung\u00b7frau\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er mu\u00df ein gro\u00dfer Meister seyn,", "tokens": ["Er", "mu\u00df", "ein", "gro\u00b7\u00dfer", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herr von gro\u00dfen Werthen,", "tokens": ["Ein", "Herr", "von", "gro\u00b7\u00dfen", "Wert\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der da die sch\u00f6nen Bl\u00fcmelein", "tokens": ["Der", "da", "die", "sch\u00f6\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft wachsen aus der Erden.", "tokens": ["L\u00e4\u00dft", "wach\u00b7sen", "aus", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich hab' ihn tief im Herzen lieb,", "tokens": ["Ich", "hab'", "ihn", "tief", "im", "Her\u00b7zen", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O d\u00fcrft ich ihn anschauen!", "tokens": ["O", "d\u00fcrft", "ich", "ihn", "an\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gern lie\u00df ich meines Vaters Reich", "tokens": ["Gern", "lie\u00df", "ich", "mei\u00b7nes", "Va\u00b7ters", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollt sein G\u00e4rtlein bauen.", "tokens": ["Und", "wollt", "sein", "G\u00e4rt\u00b7lein", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da kam zu ihr um Mitternacht", "tokens": ["Da", "kam", "zu", "ihr", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein heller Mann gegangen,", "tokens": ["Ein", "hel\u00b7ler", "Mann", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbthu auf, thu auf, viel sch\u00f6ne Magd,", "tokens": ["\u00bb", "thu", "auf", ",", "thu", "auf", ",", "viel", "sch\u00f6\u00b7ne", "Magd", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lieb bin ich umfangen.\u00ab", "tokens": ["Mit", "Lieb", "bin", "ich", "um\u00b7fan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und schnell die Magd ihr Bettlein lie\u00df,", "tokens": ["Und", "schnell", "die", "Magd", "ihr", "Bet\u00b7tlein", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Fenster th\u00e4t sie gehen,", "tokens": ["Zum", "Fens\u00b7ter", "th\u00e4t", "sie", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sah Jesum ihr viel sch\u00f6nes Lieb", "tokens": ["Sah", "Je\u00b7sum", "ihr", "viel", "sch\u00f6\u00b7nes", "Lieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So herrlich vor sich stehen.", "tokens": ["So", "herr\u00b7lich", "vor", "sich", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie \u00f6ffnet ihm voll Freudigkeit,", "tokens": ["Sie", "\u00f6ff\u00b7net", "ihm", "voll", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie neigt sich tief zur Erden,", "tokens": ["Sie", "neigt", "sich", "tief", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bot ihm freundlich gute Zeit,", "tokens": ["Und", "bot", "ihm", "freund\u00b7lich", "gu\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sittsamen Geberden.", "tokens": ["Mit", "sitt\u00b7sa\u00b7men", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "\u00bbwoher, woher, o J\u00fcngling sch\u00f6n?", "tokens": ["\u00bb", "wo\u00b7her", ",", "wo\u00b7her", ",", "o", "J\u00fcng\u00b7ling", "sch\u00f6n", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWAV", "$,", "FM", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Vaters Reichen", "tokens": ["In", "mei\u00b7nes", "Va\u00b7ters", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag keiner dir zu Seite gehn,", "tokens": ["Mag", "kei\u00b7ner", "dir", "zu", "Sei\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich keiner dir vergleichen.\u00ab", "tokens": ["Sich", "kei\u00b7ner", "dir", "ver\u00b7glei\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "PIS", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbviel sch\u00f6ne Magd, du dachtest mein,", "tokens": ["\u00bb", "viel", "sch\u00f6\u00b7ne", "Magd", ",", "du", "dach\u00b7test", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dich bin ich gekommen", "tokens": ["Um", "dich", "bin", "ich", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus meines Vaters K\u00f6nigreich,", "tokens": ["Aus", "mei\u00b7nes", "Va\u00b7ters", "K\u00f6\u00b7nig\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der Meister der Blumen.\u00ab", "tokens": ["Ich", "bin", "der", "Meis\u00b7ter", "der", "Blu\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbo Herr, o Herr, wie weit, wie weit", "tokens": ["\u00bb", "o", "Herr", ",", "o", "Herr", ",", "wie", "weit", ",", "wie", "weit"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ists zu des Vaters Garten?", "tokens": ["Ists", "zu", "des", "Va\u00b7ters", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort m\u00f6gt ich wohl in Ewigkeit", "tokens": ["Dort", "m\u00f6gt", "ich", "wohl", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nen Blumen warten.\u00ab", "tokens": ["Der", "sch\u00f6\u00b7nen", "Blu\u00b7men", "war\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbmein Garten liegt in Ewigkeit", "tokens": ["\u00bb", "mein", "Gar\u00b7ten", "liegt", "in", "E\u00b7wig\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und noch viel tausend Meilen,", "tokens": ["Und", "noch", "viel", "tau\u00b7send", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da will ich dir zum Brautgeschmeid", "tokens": ["Da", "will", "ich", "dir", "zum", "Braut\u00b7ge\u00b7schmeid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kr\u00e4nzlein roth ertheilen.\u00ab", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "roth", "er\u00b7thei\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da nahm er von dem Finger sein", "tokens": ["Da", "nahm", "er", "von", "dem", "Fin\u00b7ger", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ring von Sonnengolde", "tokens": ["Ein", "Ring", "von", "Son\u00b7nen\u00b7gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fragt, ob Sultans T\u00f6chterlein", "tokens": ["Und", "fragt", ",", "ob", "Sul\u00b7tans", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Br\u00e4utlein werden wollte.", "tokens": ["Sein", "Br\u00e4ut\u00b7lein", "wer\u00b7den", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und da sie ihm die Liebe bot,", "tokens": ["Und", "da", "sie", "ihm", "die", "Lie\u00b7be", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Wunden sich ergossen.", "tokens": ["Sein", "Wun\u00b7den", "sich", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo Lieb, wie ist dein Herz so roth,", "tokens": ["\u00bb", "o", "Lieb", ",", "wie", "ist", "dein", "Herz", "so", "roth", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein H\u00e4nde tragen Rosen.\u00ab", "tokens": ["Dein", "H\u00e4n\u00b7de", "tra\u00b7gen", "Ro\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbmein Herz, das ist um dich so roth,", "tokens": ["\u00bb", "mein", "Herz", ",", "das", "ist", "um", "dich", "so", "roth", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dich trag ich die Rosen,", "tokens": ["F\u00fcr", "dich", "trag", "ich", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich brach sie dir im Liebestod,", "tokens": ["Ich", "brach", "sie", "dir", "im", "Lie\u00b7be\u00b7stod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich mein Blut vergossen.", "tokens": ["Als", "ich", "mein", "Blut", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Mein Vater ruft, nun sch\u00fcrz dich Braut,