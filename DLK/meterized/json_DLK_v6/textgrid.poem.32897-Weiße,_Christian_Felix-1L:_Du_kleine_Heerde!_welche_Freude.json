{"textgrid.poem.32897": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleine Heerde! welche Freude", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleine Heerde! welche Freude", "tokens": ["Du", "klei\u00b7ne", "Heer\u00b7de", "!", "wel\u00b7che", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt dir des Fr\u00fchlings junge Zier!", "tokens": ["Bringt", "dir", "des", "Fr\u00fch\u00b7lings", "jun\u00b7ge", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einer blumenvollen Weide", "tokens": ["Auf", "ei\u00b7ner", "blu\u00b7men\u00b7vol\u00b7len", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scherzt Lieb und Unschuld frey in dir.", "tokens": ["Scherzt", "Lieb", "und", "Un\u00b7schuld", "frey", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du buhlest nicht um Geld und Ehren,", "tokens": ["Du", "buh\u00b7lest", "nicht", "um", "Geld", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und blos die Liebe liebt in dir:", "tokens": ["Und", "blos", "die", "Lie\u00b7be", "liebt", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir lassen uns die Klugheit lehren,", "tokens": ["Wir", "las\u00b7sen", "uns", "die", "Klug\u00b7heit", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Natur vergessen wir!", "tokens": ["Und", "die", "Na\u00b7tur", "ver\u00b7ges\u00b7sen", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}