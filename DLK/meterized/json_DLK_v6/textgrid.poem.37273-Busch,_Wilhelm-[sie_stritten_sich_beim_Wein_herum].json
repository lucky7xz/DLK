{"textgrid.poem.37273": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[sie stritten sich beim Wein herum]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie stritten sich beim Wein herum,", "tokens": ["Sie", "strit\u00b7ten", "sich", "beim", "Wein", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was das nun wieder w\u00e4re;", "tokens": ["Was", "das", "nun", "wie\u00b7der", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das mit dem Darwin w\u00e4r gar zu dumm", "tokens": ["Das", "mit", "dem", "Dar\u00b7win", "w\u00e4r", "gar", "zu", "dumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NE", "VAFIN", "ADV", "PTKA", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und wider die menschliche Ehre.", "tokens": ["Und", "wi\u00b7der", "die", "menschli\u00b7che", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie tranken manchen Humpen aus,", "tokens": ["Sie", "tran\u00b7ken", "man\u00b7chen", "Hum\u00b7pen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie stolperten aus den T\u00fcren,", "tokens": ["Sie", "stol\u00b7per\u00b7ten", "aus", "den", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sie grunzten vernehmlich und kamen zu Haus", "tokens": ["Sie", "grunz\u00b7ten", "ver\u00b7nehm\u00b7lich", "und", "ka\u00b7men", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gekrochen auf allen vieren.", "tokens": ["Ge\u00b7kro\u00b7chen", "auf", "al\u00b7len", "vie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}