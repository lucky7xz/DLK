{"textgrid.poem.40134": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Menschenthorheit", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nixe in der stillen Flut,", "tokens": ["Die", "Ni\u00b7xe", "in", "der", "stil\u00b7len", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie wei\u00df nicht, da\u00df der Tod", "tokens": ["sie", "wei\u00df", "nicht", ",", "da\u00df", "der", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "aus ihrem wasserdunklen Blick", "tokens": ["aus", "ih\u00b7rem", "was\u00b7ser\u00b7dunk\u00b7len", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den Erdens\u00f6hnen droht.", "tokens": ["den", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", "droht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie kann nicht lieben, hassen nicht:", "tokens": ["Sie", "kann", "nicht", "lie\u00b7ben", ",", "has\u00b7sen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du Menschenkind allein", "tokens": ["du", "Men\u00b7schen\u00b7kind", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "schaust in das r\u00e4tseltiefe Auge", "tokens": ["schaust", "in", "das", "r\u00e4t\u00b7sel\u00b7tie\u00b7fe", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Eigne Seele hinein.", "tokens": ["die", "Eig\u00b7ne", "See\u00b7le", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da schwillt und quillt entgegen dir", "tokens": ["Da", "schwillt", "und", "quillt", "ent\u00b7ge\u00b7gen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verdoppelt Gl\u00fcck und Weh;", "tokens": ["ver\u00b7dop\u00b7pelt", "Gl\u00fcck", "und", "Weh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "du schaust und schaust, vergehst, versinkst, \u2013", "tokens": ["du", "schaust", "und", "schaust", ",", "ver\u00b7gehst", ",", "ver\u00b7sinkst", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "verwundert rauscht der See.", "tokens": ["ver\u00b7wun\u00b7dert", "rauscht", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}