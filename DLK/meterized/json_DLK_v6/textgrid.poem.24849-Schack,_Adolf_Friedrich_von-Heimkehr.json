{"textgrid.poem.24849": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leiser schwanken die Aeste;", "tokens": ["Lei\u00b7ser", "schwan\u00b7ken", "die", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Der Kahn fliegt uferw\u00e4rts;", "tokens": ["Der", "Kahn", "fliegt", "u\u00b7fer\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heim kehrt die Taube zum Neste;", "tokens": ["Heim", "kehrt", "die", "Tau\u00b7be", "zum", "Nes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Zu dir kehrt heim mein Herz.", "tokens": ["Zu", "dir", "kehrt", "heim", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Genug am schimmernden Tage,", "tokens": ["Ge\u00b7nug", "am", "schim\u00b7mern\u00b7den", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn rings das Leben l\u00e4rmt,", "tokens": ["Wenn", "rings", "das", "Le\u00b7ben", "l\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit irrem Fl\u00fcgelschlage", "tokens": ["Mit", "ir\u00b7rem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist es ins Weite geschw\u00e4rmt.", "tokens": ["Ist", "es", "ins", "Wei\u00b7te", "ge\u00b7schw\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Doch nun die Sonne geschieden", "tokens": ["Doch", "nun", "die", "Son\u00b7ne", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Stille sich senkt auf den Hain,", "tokens": ["Und", "Stil\u00b7le", "sich", "senkt", "auf", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "F\u00fchlt es: bei dir ist der Frieden,", "tokens": ["F\u00fchlt", "es", ":", "bei", "dir", "ist", "der", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "APPR", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Ruhe bei dir allein.", "tokens": ["Die", "Ru\u00b7he", "bei", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}