{"textgrid.poem.52562": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie soll ich lernen ihn vergessen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich lernen ihn vergessen", "tokens": ["Wie", "soll", "ich", "ler\u00b7nen", "ihn", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den hei\u00dfen, einen, letzten Ku\u00df!", "tokens": ["Den", "hei\u00b7\u00dfen", ",", "ei\u00b7nen", ",", "letz\u00b7ten", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O schilt ihn, Theure, nicht vermessen", "tokens": ["O", "schilt", "ihn", ",", "Theu\u00b7re", ",", "nicht", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "$,", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schmerzvoll s\u00fc\u00dfen Abschiedsgru\u00df!", "tokens": ["So", "schmerz\u00b7voll", "s\u00fc\u00b7\u00dfen", "Ab\u00b7schieds\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer wollte weise sich bewachen,", "tokens": ["Wer", "woll\u00b7te", "wei\u00b7se", "sich", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenns heilig in dem Busen brennt \u2013?", "tokens": ["Wenns", "hei\u00b7lig", "in", "dem", "Bu\u00b7sen", "brennt", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt mit ihren sieben Sachen", "tokens": ["Die", "Welt", "mit", "ih\u00b7ren", "sie\u00b7ben", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlang der festliche Moment.", "tokens": ["Ver\u00b7schlang", "der", "fest\u00b7li\u00b7che", "Mo\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df, lasse mir von deinem Bilde", "tokens": ["La\u00df", ",", "las\u00b7se", "mir", "von", "dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die selige Erinnerung!", "tokens": ["Die", "se\u00b7li\u00b7ge", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "++-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Des Auges Hoheit, Muth und Milde!", "tokens": ["Des", "Au\u00b7ges", "Ho\u00b7heit", ",", "Muth", "und", "Mil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Gestalt beseelter Schwung!", "tokens": ["Und", "der", "Ge\u00b7stalt", "be\u00b7seel\u00b7ter", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja f\u00fchret wieder uns zusammen", "tokens": ["Ja", "f\u00fch\u00b7ret", "wie\u00b7der", "uns", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "ADV", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dereinst ein g\u00fctiges Geschick,", "tokens": ["De\u00b7reinst", "ein", "g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich glaube, jene \u00e4chten Flammen", "tokens": ["Ich", "glau\u00b7be", ",", "je\u00b7ne", "\u00e4ch\u00b7ten", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie rufen den Moment zur\u00fcck.", "tokens": ["Sie", "ru\u00b7fen", "den", "Mo\u00b7ment", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann Wonne! liebend hangen d\u00fcrfen", "tokens": ["Dann", "Won\u00b7ne", "!", "lie\u00b7bend", "han\u00b7gen", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deinem Mund und hehrer Lust", "tokens": ["An", "dei\u00b7nem", "Mund", "und", "heh\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geheimni\u00dfvollen Nektar schl\u00fcrfen,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7len", "Nek\u00b7tar", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, aus dem Athem deiner Brust!", "tokens": ["Ach", ",", "aus", "dem", "A\u00b7them", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was auch die Stunde von uns fodre,", "tokens": ["Was", "auch", "die", "Stun\u00b7de", "von", "uns", "fod\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was des Gef\u00fchls beschwingte Kraft", "tokens": ["Was", "des", "Ge\u00b7f\u00fchls", "be\u00b7schwing\u00b7te", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 Als ewge Poesie verlodre", "tokens": ["\u2013", "Als", "ew\u00b7ge", "Poe\u00b7sie", "ver\u00b7lod\u00b7re"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Feuer unsrer Leidenschaft!", "tokens": ["Das", "Feu\u00b7er", "uns\u00b7rer", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}