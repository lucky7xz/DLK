{"textgrid.poem.58593": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "So widerf\u00e4hrt es mir in meinem Unverstand.", "tokens": ["So", "wi\u00b7der\u00b7f\u00e4hrt", "es", "mir", "in", "mei\u00b7nem", "Un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Er denkt nicht, weil er gro\u00df an seiner Mutter Hand.", "tokens": ["Er", "denkt", "nicht", ",", "weil", "er", "gro\u00df", "an", "sei\u00b7ner", "Mut\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich unverm\u00f6gend ", "tokens": ["Ich", "un\u00b7ver\u00b7m\u00f6\u00b7gend"], "token_info": ["word", "word"], "pos": ["PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und harren, bis sein Geist mir neue Kr\u00e4fte reicht;", "tokens": ["Und", "har\u00b7ren", ",", "bis", "sein", "Geist", "mir", "neu\u00b7e", "Kr\u00e4f\u00b7te", "reicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Selbst gehen kann ich nicht. M\u00f6cht' mich die Mutter tragen,", "tokens": ["Selbst", "ge\u00b7hen", "kann", "ich", "nicht", ".", "M\u00f6cht'", "mich", "die", "Mut\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Dann lief' und fl\u00f6g' ich wohl aus mir und allem leicht.", "tokens": ["Dann", "lie\u00b7f'", "und", "fl\u00f6g'", "ich", "wohl", "aus", "mir", "und", "al\u00b7lem", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "PIS", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}