{"textgrid.poem.34818": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesanglos war ich und beklommen", "tokens": ["Ge\u00b7sang\u00b7los", "war", "ich", "und", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lange Zeit \u2013 nun dicht ich wieder!", "tokens": ["So", "lan\u00b7ge", "Zeit", "\u2013", "nun", "dicht", "ich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "ADV", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Tr\u00e4nen, die uns pl\u00f6tzlich kommen,", "tokens": ["Wie", "Tr\u00e4\u00b7nen", ",", "die", "uns", "pl\u00f6tz\u00b7lich", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kommen pl\u00f6tzlich auch die Lieder.", "tokens": ["So", "kom\u00b7men", "pl\u00f6tz\u00b7lich", "auch", "die", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Melodisch kann ich wieder klagen", "tokens": ["Me\u00b7lo\u00b7disch", "kann", "ich", "wie\u00b7der", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von gro\u00dfem Lieben, gr\u00f6\u00dferm Leiden,", "tokens": ["Von", "gro\u00b7\u00dfem", "Lie\u00b7ben", ",", "gr\u00f6\u00b7\u00dferm", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Herzen, die sich schlecht vertragen", "tokens": ["Von", "Her\u00b7zen", ",", "die", "sich", "schlecht", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch brechen, wenn sie scheiden.", "tokens": ["Und", "den\u00b7noch", "bre\u00b7chen", ",", "wenn", "sie", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Manchmal ist mir, als f\u00fchlt' ich wehen", "tokens": ["Manch\u00b7mal", "ist", "mir", ",", "als", "f\u00fchlt'", "ich", "we\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00dcber dem Haupt die deutschen Eichen \u2013", "tokens": ["\u00dc\u00b7ber", "dem", "Haupt", "die", "deut\u00b7schen", "Ei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie fl\u00fcstern gar von Wiedersehen \u2013", "tokens": ["Sie", "fl\u00fcs\u00b7tern", "gar", "von", "Wie\u00b7der\u00b7se\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sind nur Tr\u00e4ume \u2013 sie verbleichen.", "tokens": ["Das", "sind", "nur", "Tr\u00e4u\u00b7me", "\u2013", "sie", "ver\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Manchmal ist mir, als h\u00f6rt' ich singen", "tokens": ["Manch\u00b7mal", "ist", "mir", ",", "als", "h\u00f6rt'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die alten, deutschen Nachtigallen \u2013", "tokens": ["Die", "al\u00b7ten", ",", "deut\u00b7schen", "Nach\u00b7ti\u00b7gal\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mich die T\u00f6ne sanft umschlingen! \u2013", "tokens": ["Wie", "mich", "die", "T\u00f6\u00b7ne", "sanft", "um\u00b7schlin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sind nur Tr\u00e4ume \u2013 sie verhallen.", "tokens": ["Das", "sind", "nur", "Tr\u00e4u\u00b7me", "\u2013", "sie", "ver\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo sind die Rosen, deren Liebe", "tokens": ["Wo", "sind", "die", "Ro\u00b7sen", ",", "de\u00b7ren", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich einst begl\u00fcckt? \u2013 All ihre Bl\u00fcte", "tokens": ["Mich", "einst", "be\u00b7gl\u00fcckt", "?", "\u2013", "All", "ih\u00b7re", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$.", "$(", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist l\u00e4ngst verwelkt! \u2013 Gespenstisch tr\u00fcbe", "tokens": ["Ist", "l\u00e4ngst", "ver\u00b7welkt", "!", "\u2013", "Ge\u00b7spens\u00b7tisch", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "$(", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spukt noch ihr Duft mir im Gem\u00fcte.", "tokens": ["Spukt", "noch", "ihr", "Duft", "mir", "im", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}}}}