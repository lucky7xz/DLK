{"textgrid.poem.65045": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wandelst grollend durch das Weltgetriebe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wandelst grollend durch das Weltgetriebe", "tokens": ["Du", "wan\u00b7delst", "grol\u00b7lend", "durch", "das", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stolz darauf, die Menschheit zu verachten.", "tokens": ["Und", "stolz", "da\u00b7rauf", ",", "die", "Menschheit", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "$,", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kaum gut genug d\u00fcnkt dich ihr Thun und Trachten", "tokens": ["Kaum", "gut", "ge\u00b7nug", "d\u00fcnkt", "dich", "ihr", "Thun", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr deines Spottes scharfe Gei\u00dfelhiebe.", "tokens": ["F\u00fcr", "dei\u00b7nes", "Spot\u00b7tes", "schar\u00b7fe", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O da\u00df der th\u00f6richt eitle Wahn zerstiebe!", "tokens": ["O", "da\u00df", "der", "th\u00f6\u00b7richt", "eit\u00b7le", "Wahn", "zer\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht l\u00e4nger la\u00df' ihn deinen Blick umnachten!", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "la\u00df'", "ihn", "dei\u00b7nen", "Blick", "um\u00b7nach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das echte Gold ruht in des Herzens Schachten", "tokens": ["Das", "ech\u00b7te", "Gold", "ruht", "in", "des", "Her\u00b7zens", "Schach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes Kunstwerks Seele ist die Liebe. \u2013", "tokens": ["Und", "je\u00b7des", "Kunst\u00b7werks", "See\u00b7le", "ist", "die", "Lie\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des K\u00fcnstlers Sendung ist es Licht zu fl\u00f6\u00dfen", "tokens": ["Des", "K\u00fcnst\u00b7lers", "Sen\u00b7dung", "ist", "es", "Licht", "zu", "fl\u00f6\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und milder noch den Unheilvollen, B\u00f6sen.", "tokens": ["Und", "mil\u00b7der", "noch", "den", "Un\u00b7heil\u00b7vol\u00b7len", ",", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An seiner Gluht soll ja ihr Frost erwarmen;", "tokens": ["An", "sei\u00b7ner", "Gluht", "soll", "ja", "ihr", "Frost", "er\u00b7war\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie aber k\u00f6nnte Jener sie erl\u00f6sen,", "tokens": ["Wie", "a\u00b7ber", "k\u00f6nn\u00b7te", "Je\u00b7ner", "sie", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PDAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sie nicht liebt mit g\u00f6ttlichem Erbarmen?", "tokens": ["Der", "sie", "nicht", "liebt", "mit", "g\u00f6tt\u00b7li\u00b7chem", "Er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}