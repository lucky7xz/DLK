{"textgrid.poem.65878": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "152", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich dich lieb' ist Meineid; wei\u00dft du: doch", "tokens": ["Da\u00df", "ich", "dich", "lieb'", "ist", "Mei\u00b7neid", ";", "wei\u00dft", "du", ":", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VAFIN", "NN", "$.", "VVFIN", "PPER", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zwiefach meineidig du, mir Liebe schw\u00f6rend,", "tokens": ["Zwie\u00b7fach", "mei\u00b7nei\u00b7dig", "du", ",", "mir", "Lie\u00b7be", "schw\u00f6\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Brachst mit der Tat dein Bettgel\u00fcbde, noch", "tokens": ["Brachst", "mit", "der", "Tat", "dein", "Bett\u00b7ge\u00b7l\u00fcb\u00b7de", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Den neuen Liebesbund in neuem Ha\u00df zerst\u00f6rend!", "tokens": ["Den", "neu\u00b7en", "Lie\u00b7bes\u00b7bund", "in", "neu\u00b7em", "Ha\u00df", "zer\u00b7st\u00f6\u00b7rend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch ich, der zwanzig Eide bricht, wie k\u00f6nnt' ich", "tokens": ["Doch", "ich", ",", "der", "zwan\u00b7zig", "Ei\u00b7de", "bricht", ",", "wie", "k\u00f6nnt'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,", "PWAV", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir zwei ver\u00fcbeln? L\u00fcg' ich doch weit mehr;", "tokens": ["Dir", "zwei", "ver\u00b7\u00fc\u00b7beln", "?", "L\u00fcg'", "ich", "doch", "weit", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "VVINF", "$.", "NN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was ich schw\u00f6ren mag, mi\u00dfhandelt dich best\u00e4ndig:", "tokens": ["Und", "was", "ich", "schw\u00f6\u00b7ren", "mag", ",", "mi\u00df\u00b7han\u00b7delt", "dich", "be\u00b7st\u00e4n\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein bestes Wort machst du bedeutungsleer.", "tokens": ["Mein", "bes\u00b7tes", "Wort", "machst", "du", "be\u00b7deu\u00b7tungs\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn ich beschwur mit teuern Seelenschw\u00fcren", "tokens": ["Denn", "ich", "be\u00b7schwur", "mit", "teu\u00b7ern", "See\u00b7len\u00b7schw\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dein teures Lieben, deine G\u00fct' und Treu;", "tokens": ["Dein", "teu\u00b7res", "Lie\u00b7ben", ",", "dei\u00b7ne", "G\u00fct'", "und", "Treu", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich lieh der Blindheit Augen, dich zu zieren:", "tokens": ["Ich", "lieh", "der", "Blind\u00b7heit", "Au\u00b7gen", ",", "dich", "zu", "zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Verleugnen mu\u00dften sie, da\u00df wahr ihr Zeugnis sei:", "tokens": ["Ver\u00b7leug\u00b7nen", "mu\u00df\u00b7ten", "sie", ",", "da\u00df", "wahr", "ihr", "Zeug\u00b7nis", "sei", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn ich schwur, du sei'st sch\u00f6n: o grober Trug,", "tokens": ["Denn", "ich", "schwur", ",", "du", "sei'st", "sch\u00f6n", ":", "o", "gro\u00b7ber", "Trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$.", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Natur zu l\u00e4stern mit so schn\u00f6dem Lug!", "tokens": ["Na\u00b7tur", "zu", "l\u00e4s\u00b7tern", "mit", "so", "schn\u00f6\u00b7dem", "Lug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}