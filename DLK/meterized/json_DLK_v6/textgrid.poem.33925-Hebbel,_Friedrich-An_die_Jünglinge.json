{"textgrid.poem.33925": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die J\u00fcnglinge", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trinkt des Weines dunkle Kraft,", "tokens": ["Trinkt", "des", "Wei\u00b7nes", "dunk\u00b7le", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die euch durch die Seele flie\u00dft", "tokens": ["Die", "euch", "durch", "die", "See\u00b7le", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu heil'ger Rechenschaft", "tokens": ["Und", "zu", "heil'\u00b7ger", "Re\u00b7chen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie im Innersten erschlie\u00dft!", "tokens": ["Sie", "im", "In\u00b7ners\u00b7ten", "er\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Blickt hinab nun in den Grund,", "tokens": ["Blickt", "hin\u00b7ab", "nun", "in", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem das Leben still entsteigt,", "tokens": ["Dem", "das", "Le\u00b7ben", "still", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Forscht mit Ernst, ob es gesund", "tokens": ["Forscht", "mit", "Ernst", ",", "ob", "es", "ge\u00b7sund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedem H\u00f6chsten sich verzweigt.", "tokens": ["Je\u00b7dem", "H\u00f6chs\u00b7ten", "sich", "ver\u00b7zweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht an einen schaur'gen Ort,", "tokens": ["Geht", "an", "ei\u00b7nen", "schaur'\u00b7gen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denkt an aller Ehren Strau\u00df,", "tokens": ["Denkt", "an", "al\u00b7ler", "Eh\u00b7ren", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprecht dann laut das Sch\u00f6pfungswort,", "tokens": ["Sprecht", "dann", "laut", "das", "Sch\u00f6p\u00b7fungs\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprecht das Wort: es werde! aus.", "tokens": ["Sprecht", "das", "Wort", ":", "es", "wer\u00b7de", "!", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "$.", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, es werde! spricht auch Gott,", "tokens": ["Ja", ",", "es", "wer\u00b7de", "!", "spricht", "auch", "Gott", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$.", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Segen senkt sich still,", "tokens": ["Und", "sein", "Se\u00b7gen", "senkt", "sich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn, den macht er nicht zum Spott,", "tokens": ["Denn", ",", "den", "macht", "er", "nicht", "zum", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sich selbst vollenden will.", "tokens": ["Der", "sich", "selbst", "voll\u00b7en\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Betet dann, doch betet nur", "tokens": ["Be\u00b7tet", "dann", ",", "doch", "be\u00b7tet", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu euch selbst, und ihr beschw\u00f6rt", "tokens": ["Zu", "euch", "selbst", ",", "und", "ihr", "be\u00b7schw\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der eigenen Natur", "tokens": ["Aus", "der", "ei\u00b7ge\u00b7nen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Einen Geist, der euch erh\u00f6rt.", "tokens": ["Ei\u00b7nen", "Geist", ",", "der", "euch", "er\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leben hei\u00dft, tief einsam sein;", "tokens": ["Le\u00b7ben", "hei\u00dft", ",", "tief", "ein\u00b7sam", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In die spr\u00f6de Knospe dr\u00e4ngt", "tokens": ["In", "die", "spr\u00f6\u00b7de", "Knos\u00b7pe", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich kein Tropfe Thaus hinein,", "tokens": ["Sich", "kein", "Trop\u00b7fe", "Thaus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh' sie inn're Glut zersprengt.", "tokens": ["Eh'", "sie", "inn'\u00b7re", "Glut", "zer\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott dem Herrn ist's ein Triumph,", "tokens": ["Gott", "dem", "Herrn", "ist's", "ein", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr nicht vor ihm vergeht,", "tokens": ["Wenn", "ihr", "nicht", "vor", "ihm", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr, statt im Staube dumpf", "tokens": ["Wenn", "ihr", ",", "statt", "im", "Stau\u00b7be", "dumpf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinzuknieen, herrlich steht,", "tokens": ["Hin\u00b7zu\u00b7kni\u00b7e\u00b7en", ",", "herr\u00b7lich", "steht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wenn ihr stolz, dem Baume gleich,", "tokens": ["Wenn", "ihr", "stolz", ",", "dem", "Bau\u00b7me", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch nicht unter Bl\u00fcten b\u00fcckt,", "tokens": ["Euch", "nicht", "un\u00b7ter", "Bl\u00fc\u00b7ten", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Last des Segens euch", "tokens": ["Wenn", "die", "Last", "des", "Se\u00b7gens", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Erst hinab zur Erde dr\u00fcckt.", "tokens": ["Erst", "hin\u00b7ab", "zur", "Er\u00b7de", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fort den Wein! Wer noch nicht flammt,", "tokens": ["Fort", "den", "Wein", "!", "Wer", "noch", "nicht", "flammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht seines Kusses werth,", "tokens": ["Ist", "nicht", "sei\u00b7nes", "Kus\u00b7ses", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wer selbst vom Feuer stammt,", "tokens": ["Und", "wer", "selbst", "vom", "Feu\u00b7er", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Steht schon lange glutverkl\u00e4rt.", "tokens": ["Steht", "schon", "lan\u00b7ge", "glut\u00b7ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euch geziemt nur Eine Lust,", "tokens": ["Euch", "ge\u00b7ziemt", "nur", "Ei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein Gang durch Sturm und Nacht,", "tokens": ["Nur", "ein", "Gang", "durch", "Sturm", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der aus eurer dunklen Brust", "tokens": ["Der", "aus", "eu\u00b7rer", "dunk\u00b7len", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Sternenhimmel macht.", "tokens": ["Ei\u00b7nen", "Ster\u00b7nen\u00b7him\u00b7mel", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}