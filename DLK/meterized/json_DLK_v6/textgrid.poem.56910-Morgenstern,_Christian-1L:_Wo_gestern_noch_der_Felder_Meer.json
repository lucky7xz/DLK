{"textgrid.poem.56910": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo gestern noch der Felder Meer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gestern noch der Felder Meer", "tokens": ["Wo", "ge\u00b7stern", "noch", "der", "Fel\u00b7der", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "gewogt in allen Farben,", "tokens": ["ge\u00b7wogt", "in", "al\u00b7len", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "steht heut in Reih und Glied ein Heer", "tokens": ["steht", "heut", "in", "Reih", "und", "Glied", "ein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "festlich geg\u00fcrteter Garben.", "tokens": ["fest\u00b7lich", "ge\u00b7g\u00fcr\u00b7te\u00b7ter", "Gar\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Es will der goldne Heeresbann", "tokens": ["Es", "will", "der", "gold\u00b7ne", "Hee\u00b7res\u00b7bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vor Frost und Hungers W\u00fcten", "tokens": ["vor", "Frost", "und", "Hun\u00b7gers", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das ganze Dorf mit Maus und Mann", "tokens": ["das", "gan\u00b7ze", "Dorf", "mit", "Maus", "und", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bis \u00fcbers Jahr beh\u00fcten.", "tokens": ["bis", "\u00fc\u00b7bers", "Jahr", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und liegen die Bataillone erst", "tokens": ["Und", "lie\u00b7gen", "die", "Ba\u00b7tail\u00b7lo\u00b7ne", "erst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "im sichern Scheunquartiere,", "tokens": ["im", "si\u00b7chern", "Scheun\u00b7quar\u00b7tie\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "du f\u00e4ndst, und wenn du der K\u00f6nig w\u00e4rst,", "tokens": ["du", "f\u00e4ndst", ",", "und", "wenn", "du", "der", "K\u00f6\u00b7nig", "w\u00e4rst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "nicht be\u00dfre Grenadiere.", "tokens": ["nicht", "be\u00df\u00b7re", "Gre\u00b7na\u00b7die\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}