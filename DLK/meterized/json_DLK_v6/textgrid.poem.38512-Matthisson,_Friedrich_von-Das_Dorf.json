{"textgrid.poem.38512": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Dorf", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt es still, im saatengr\u00fcnen Thale,", "tokens": ["Da", "liegt", "es", "still", ",", "im", "saa\u00b7ten\u00b7gr\u00fc\u00b7nen", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das D\u00f6rfchen von Geb\u00fcsch umkr\u00e4nzt,", "tokens": ["Das", "D\u00f6rf\u00b7chen", "von", "Ge\u00b7b\u00fcsch", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die D\u00e4cher roth vom Abendd\u00e4mrungsstrale,", "tokens": ["Die", "D\u00e4\u00b7cher", "roth", "vom", "A\u00b7bend\u00b7d\u00e4m\u00b7rungs\u00b7stra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der durch die Lindenwipfel gl\u00e4nzt!", "tokens": ["Der", "durch", "die", "Lin\u00b7den\u00b7wip\u00b7fel", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort wohnt, in niedrer, weinumrankter H\u00fctte,", "tokens": ["Dort", "wohnt", ",", "in", "nie\u00b7drer", ",", "wei\u00b7num\u00b7rank\u00b7ter", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Gottes Engeln stets umschwebt,", "tokens": ["Von", "Got\u00b7tes", "En\u00b7geln", "stets", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein M\u00e4dchen, guter, frommer, deutscher Sitte,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "gu\u00b7ter", ",", "from\u00b7mer", ",", "deut\u00b7scher", "Sit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr die mein Herz im Stillen bebt!", "tokens": ["F\u00fcr", "die", "mein", "Herz", "im", "Stil\u00b7len", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie k\u00fcmmert nicht der stolzen Stadt Get\u00fcmmel,", "tokens": ["Sie", "k\u00fcm\u00b7mert", "nicht", "der", "stol\u00b7zen", "Stadt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht eitler Mode Flitterglanz!", "tokens": ["Nicht", "eit\u00b7ler", "Mo\u00b7de", "Flit\u00b7ter\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der maibebl\u00fcmte Garten ist ihr Himmel,", "tokens": ["Der", "mai\u00b7be\u00b7bl\u00fcm\u00b7te", "Gar\u00b7ten", "ist", "ihr", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr ganzer Schmuk ein Veilchenkranz!", "tokens": ["Ihr", "gan\u00b7zer", "Schmuk", "ein", "Veil\u00b7chen\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann durch den Bl\u00fctenbaum das Fr\u00fchroth schimmert,", "tokens": ["Wann", "durch", "den", "Bl\u00fc\u00b7ten\u00b7baum", "das", "Fr\u00fch\u00b7roth", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entwandelt sie zum Nelkenbeet,", "tokens": ["Ent\u00b7wan\u00b7delt", "sie", "zum", "Nel\u00b7ken\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und pfl\u00fckt ein Str\u00e4uschen, frisch und thaubeflimmert,", "tokens": ["Und", "pfl\u00fckt", "ein", "Str\u00e4u\u00b7schen", ",", "frisch", "und", "thau\u00b7be\u00b7flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom lieben Morgenwind umweht!", "tokens": ["Vom", "lie\u00b7ben", "Mor\u00b7gen\u00b7wind", "um\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wann die milde Maienabendstille", "tokens": ["Und", "wann", "die", "mil\u00b7de", "Mai\u00b7en\u00b7a\u00b7bend\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Thaugew\u00f6lke niederfleu\u00dft,", "tokens": ["Vom", "Thau\u00b7ge\u00b7w\u00f6l\u00b7ke", "nie\u00b7der\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Horcht sie, am Bach, dem Trauerlied der Grille,", "tokens": ["Horcht", "sie", ",", "am", "Bach", ",", "dem", "Trau\u00b7er\u00b7lied", "der", "Gril\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das durch die D\u00e4mrung sich ergeu\u00dft!", "tokens": ["Das", "durch", "die", "D\u00e4m\u00b7rung", "sich", "er\u00b7geu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Freut jedes Strauches sich und jeder Quelle,", "tokens": ["Freut", "je\u00b7des", "Strau\u00b7ches", "sich", "und", "je\u00b7der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PRF", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihrer kleinen Sch\u00e4ferflur,", "tokens": ["Auf", "ih\u00b7rer", "klei\u00b7nen", "Sch\u00e4\u00b7fer\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jedes Bl\u00fcmchens, jeder Rasenstelle,", "tokens": ["Und", "je\u00b7des", "Bl\u00fcm\u00b7chens", ",", "je\u00b7der", "Ra\u00b7sen\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sanfte Tochter der Natur!", "tokens": ["Die", "sanf\u00b7te", "Toch\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verlebe deines sch\u00f6nen Lebens Tage,", "tokens": ["Ver\u00b7le\u00b7be", "dei\u00b7nes", "sch\u00f6\u00b7nen", "Le\u00b7bens", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gutes, frommes M\u00e4dchen du!", "tokens": ["Du", "gu\u00b7tes", ",", "from\u00b7mes", "M\u00e4d\u00b7chen", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In steter Unschuldsfreude, sonder Klage,", "tokens": ["In", "ste\u00b7ter", "Un\u00b7schulds\u00b7freu\u00b7de", ",", "son\u00b7der", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis hin zur stillen Grabesruh;", "tokens": ["Bis", "hin", "zur", "stil\u00b7len", "Gra\u00b7be\u00b7sruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da siegbekr\u00f6nt dein Geist dem lichten Throne", "tokens": ["Da", "sieg\u00b7be\u00b7kr\u00f6nt", "dein", "Geist", "dem", "lich\u00b7ten", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Mitlers sich entgegenschwingt,", "tokens": ["Des", "Mit\u00b7lers", "sich", "ent\u00b7ge\u00b7gen\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hohe Jubel, in der Ueberwinderkrone,", "tokens": ["Und", "ho\u00b7he", "Ju\u00b7bel", ",", "in", "der", "Ue\u00b7ber\u00b7win\u00b7der\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem grossen Gottvers\u00f6hner singt!", "tokens": ["Dem", "gros\u00b7sen", "Gott\u00b7ver\u00b7s\u00f6h\u00b7ner", "singt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da werd' ich einst gewis dich wieder finden,", "tokens": ["Da", "werd'", "ich", "einst", "ge\u00b7wis", "dich", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Gottes mich und deiner freun!", "tokens": ["Und", "Got\u00b7tes", "mich", "und", "dei\u00b7ner", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "KON", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Edens Palmen meine Schl\u00e4f umwinden,", "tokens": ["Mit", "E\u00b7dens", "Pal\u00b7men", "mei\u00b7ne", "Schl\u00e4f", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begl\u00fckt, begl\u00fckt auf ewig sein!", "tokens": ["Be\u00b7gl\u00fckt", ",", "be\u00b7gl\u00fckt", "auf", "e\u00b7wig", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}