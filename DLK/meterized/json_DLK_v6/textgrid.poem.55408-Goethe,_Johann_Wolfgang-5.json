{"textgrid.poem.55408": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgeherrschet hat die Sonne,", "tokens": ["Aus\u00b7ge\u00b7herr\u00b7schet", "hat", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem F\u00fchrer kommt die Menge:", "tokens": ["Zu", "dem", "F\u00fch\u00b7rer", "kommt", "die", "Men\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbauf, Gesellen, sch\u00f6pfet Wasser,", "tokens": ["\u00bb", "auf", ",", "Ge\u00b7sel\u00b7len", ",", "sch\u00f6p\u00b7fet", "Was\u00b7ser", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Teilt euch in das Abendbrot!", "tokens": ["Teilt", "euch", "in", "das", "A\u00b7bend\u00b7brot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lamprakos, du aber, Neffe,", "tokens": ["Lamp\u00b7ra\u00b7kos", ",", "du", "a\u00b7ber", ",", "Nef\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Setze dich an meine Seite;", "tokens": ["Set\u00b7ze", "dich", "an", "mei\u00b7ne", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Trage k\u00fcnftig diese Waffen,", "tokens": ["Tra\u00b7ge", "k\u00fcnf\u00b7tig", "die\u00b7se", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du nun bist der Kapitan.", "tokens": ["Du", "nun", "bist", "der", "Ka\u00b7pi\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und ihr andern braven Krieger,", "tokens": ["Und", "ihr", "an\u00b7dern", "bra\u00b7ven", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fasset den verwaisten S\u00e4bel,", "tokens": ["Fas\u00b7set", "den", "ver\u00b7wais\u00b7ten", "S\u00e4\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Hauet gr\u00fcne Fichtenzweige,", "tokens": ["Hau\u00b7et", "gr\u00fc\u00b7ne", "Fich\u00b7ten\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Flechtet sie zum Lager mir;", "tokens": ["Flech\u00b7tet", "sie", "zum", "La\u00b7ger", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "F\u00fchrt den Beichtiger zur Stelle,", "tokens": ["F\u00fchrt", "den", "Beich\u00b7ti\u00b7ger", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df ich ihm bekennen m\u00f6ge,", "tokens": ["Da\u00df", "ich", "ihm", "be\u00b7ken\u00b7nen", "m\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ihm enth\u00fclle, welchen Taten", "tokens": ["Ihm", "ent\u00b7h\u00fcl\u00b7le", ",", "wel\u00b7chen", "Ta\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ich mein Leben zugekehrt:", "tokens": ["Ich", "mein", "Le\u00b7ben", "zu\u00b7ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Drei\u00dfig Jahr bin Armatole,", "tokens": ["Drei\u00b7\u00dfig", "Jahr", "bin", "Ar\u00b7ma\u00b7to\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zwanzig Jahr ein K\u00e4mpfer schon;", "tokens": ["Zwan\u00b7zig", "Jahr", "ein", "K\u00e4mp\u00b7fer", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nun will mich der Tod erschleichen,", "tokens": ["Nun", "will", "mich", "der", "Tod", "er\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Das ich wohl zufrieden bin.", "tokens": ["Das", "ich", "wohl", "zu\u00b7frie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Frisch nun mir das Grab bereitet,", "tokens": ["Frisch", "nun", "mir", "das", "Grab", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df es hoch sei und ger\u00e4umig,", "tokens": ["Da\u00df", "es", "hoch", "sei", "und", "ge\u00b7r\u00e4u\u00b7mig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Aufrecht, da\u00df ich fechten k\u00f6nne,", "tokens": ["Auf\u00b7recht", ",", "da\u00df", "ich", "fech\u00b7ten", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "K\u00f6nne laden die Pistolen.", "tokens": ["K\u00f6n\u00b7ne", "la\u00b7den", "die", "Pis\u00b7to\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Rechts will ich ein Fenster offen,", "tokens": ["Rechts", "will", "ich", "ein", "Fens\u00b7ter", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Da\u00df die Schwalbe Fr\u00fchling k\u00fcnde,", "tokens": ["Da\u00df", "die", "Schwal\u00b7be", "Fr\u00fch\u00b7ling", "k\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df die Nachtigall vom Maien", "tokens": ["Da\u00df", "die", "Nach\u00b7ti\u00b7gall", "vom", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Allerlieblichstes berichte.\u00ab", "tokens": ["Al\u00b7ler\u00b7lieb\u00b7lichs\u00b7tes", "be\u00b7rich\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}