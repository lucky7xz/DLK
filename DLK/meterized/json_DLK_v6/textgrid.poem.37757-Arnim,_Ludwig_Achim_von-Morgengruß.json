{"textgrid.poem.37757": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Morgengru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wachet ihr Treuen", "tokens": ["Wa\u00b7chet", "ihr", "Treu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich bin zur\u00fcck", "tokens": ["Ich", "bin", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Euch zu erfreuen", "tokens": ["Euch", "zu", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wincket das Gl\u00fcck", "tokens": ["Win\u00b7cket", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Winckt mit den Fahnen", "tokens": ["Winckt", "mit", "den", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Droben am Thor", "tokens": ["Dro\u00b7ben", "am", "Thor"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Goldene Bahnen", "tokens": ["Gol\u00b7de\u00b7ne", "Bah\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zeichnet es vor", "tokens": ["Zeich\u00b7net", "es", "vor"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Reisset den Schleier", "tokens": ["Reis\u00b7set", "den", "Schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der mich umwand", "tokens": ["Der", "mich", "um\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Zeigt mir den Freier", "tokens": ["Zeigt", "mir", "den", "Frei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Hier in dem Land.", "tokens": ["Hier", "in", "dem", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}