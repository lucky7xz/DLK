{"textgrid.poem.51998": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund, was vor ein Geist wil dir nach Hoffe ruffen?", "tokens": ["Mein", "Freund", ",", "was", "vor", "ein", "Geist", "wil", "dir", "nach", "Hof\u00b7fe", "ruf\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin sonst keiner kommt, der nicht dein Handwerck fleucht?", "tokens": ["Da\u00b7hin", "sonst", "kei\u00b7ner", "kommt", ",", "der", "nicht", "dein", "Hand\u00b7werck", "fleucht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIS", "VVFIN", "$,", "PRELS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist arm, wie wiltu dir die Befehl erhoffen,", "tokens": ["Du", "bist", "arm", ",", "wie", "wil\u00b7tu", "dir", "die", "Be\u00b7fehl", "er\u00b7hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann nicht der Cantzelist vergoldte Becher reucht?", "tokens": ["Wann", "nicht", "der", "Cant\u00b7ze\u00b7list", "ver\u00b7gold\u00b7te", "Be\u00b7cher", "reucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du bist deutsch: niemand wird vor dich den Wirth betr\u00fcgen", "tokens": ["Du", "bist", "deutsch", ":", "nie\u00b7mand", "wird", "vor", "dich", "den", "Wirth", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PIS", "VAFIN", "APPR", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn du nicht wilt nach Rauch mit ihm zu Marckte gehn.", "tokens": ["Wenn", "du", "nicht", "wilt", "nach", "Rauch", "mit", "ihm", "zu", "Marck\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "APPR", "NN", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du bist treu: Was wiltu von gutten Le\u00fcten l\u00fcgen,", "tokens": ["Du", "bist", "treu", ":", "Was", "wil\u00b7tu", "von", "gut\u00b7ten", "Le\u00fc\u00b7ten", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PWS", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn der Fiscal sie heist auff Gutt und Leben stehn?", "tokens": ["Wenn", "der", "Fis\u00b7cal", "sie", "heist", "auff", "Gutt", "und", "Le\u00b7ben", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Du bist fromm. Kanstu auch die Seel an Nagel hencken,", "tokens": ["Du", "bist", "fromm", ".", "Kans\u00b7tu", "auch", "die", "Seel", "an", "Na\u00b7gel", "hen\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "VMFIN", "ADV", "ART", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn von dem Eyde dich der Pater lo\u00dfgemacht?", "tokens": ["Wenn", "von", "dem", "Ey\u00b7de", "dich", "der", "Pa\u00b7ter", "lo\u00df\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du bist keusch: Wird die Frau den Herren wollen lencken,", "tokens": ["Du", "bist", "keusch", ":", "Wird", "die", "Frau", "den", "Her\u00b7ren", "wol\u00b7len", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "VAFIN", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wann ihr dein Styl auch nicht ein Abschrifft vorgebracht?", "tokens": ["Wann", "ihr", "dein", "Styl", "auch", "nicht", "ein", "Ab\u00b7schrifft", "vor\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du bist karg: Wie wiltu bey Gunst und Gelde bleiben,", "tokens": ["Du", "bist", "karg", ":", "Wie", "wil\u00b7tu", "bey", "Gunst", "und", "Gel\u00b7de", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PWAV", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Wann nicht der Parasit mit dir zu Tische sitzt?", "tokens": ["Wann", "nicht", "der", "Pa\u00b7ra\u00b7sit", "mit", "dir", "zu", "Ti\u00b7sche", "sitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du bist plumb: Wie wirstu die Sache sicher treiben,", "tokens": ["Du", "bist", "plumb", ":", "Wie", "wirs\u00b7tu", "die", "Sa\u00b7che", "si\u00b7cher", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PWAV", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann auch des Nachbarn Kind auff deine Worte spitzt?", "tokens": ["Wann", "auch", "des", "Nach\u00b7barn", "Kind", "auff", "dei\u00b7ne", "Wor\u00b7te", "spitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Freund, wo du der bist, und ich dich habe troffen,", "tokens": ["Mein", "Freund", ",", "wo", "du", "der", "bist", ",", "und", "ich", "dich", "ha\u00b7be", "trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "VAFIN", "$,", "KON", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So stelle deine Rei\u00df auff solche Farben ein:", "tokens": ["So", "stel\u00b7le", "dei\u00b7ne", "Rei\u00df", "auff", "sol\u00b7che", "Far\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und giengen diese hin, la\u00df di\u00df dir abwerts ruffen:", "tokens": ["Und", "gien\u00b7gen", "die\u00b7se", "hin", ",", "la\u00df", "di\u00df", "dir", "ab\u00b7werts", "ruf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKVZ", "$,", "VVIMP", "PDS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Man sagt, als wann du schwartz zu Hoffe soltest seyn.", "tokens": ["Man", "sagt", ",", "als", "wann", "du", "schwartz", "zu", "Hof\u00b7fe", "sol\u00b7test", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PWAV", "PPER", "ADJD", "PTKZU", "VVFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}