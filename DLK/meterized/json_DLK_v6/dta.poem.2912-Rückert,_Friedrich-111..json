{"dta.poem.2912": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "111.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist das Licht, das hold des Daseyns Nacht erheitert?", "tokens": ["Was", "ist", "das", "Licht", ",", "das", "hold", "des", "Da\u00b7seyns", "Nacht", "er\u00b7hei\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Athem, der die Brust zum Himmel dir erweitert?", "tokens": ["Der", "A\u00b7them", ",", "der", "die", "Brust", "zum", "Him\u00b7mel", "dir", "er\u00b7wei\u00b7tert", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Freude, die dich gut und weise macht, vollkommen;", "tokens": ["Die", "Freu\u00b7de", ",", "die", "dich", "gut", "und", "wei\u00b7se", "macht", ",", "voll\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADV", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Gegentheil allein macht eng und dumpf, beklommen.", "tokens": ["Ihr", "Ge\u00b7gen\u00b7theil", "al\u00b7lein", "macht", "eng", "und", "dumpf", ",", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Solang du Freude f\u00fchlst, f\u00fchlst du dich in Zunahme,", "tokens": ["So\u00b7lang", "du", "Freu\u00b7de", "f\u00fchlst", ",", "f\u00fchlst", "du", "dich", "in", "Zu\u00b7nah\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in Abnahme nur, wenn du erliegst dem Grame.", "tokens": ["Und", "in", "Ab\u00b7nah\u00b7me", "nur", ",", "wenn", "du", "er\u00b7liegst", "dem", "Gra\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wem noch in Zunahm' ist das Leben, der ist jung;", "tokens": ["Wem", "noch", "in", "Zu\u00b7nahm'", "ist", "das", "Le\u00b7ben", ",", "der", "ist", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und so ist alterlos der Freude Jugendschwung.", "tokens": ["Und", "so", "ist", "al\u00b7ter\u00b7los", "der", "Freu\u00b7de", "Ju\u00b7gend\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die ew'ge Jugend la\u00df vom Kummer dir nicht rauben;", "tokens": ["Die", "ew'\u00b7ge", "Ju\u00b7gend", "la\u00df", "vom", "Kum\u00b7mer", "dir", "nicht", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dft mit Freudigkeit nur an dich selber glauben.", "tokens": ["Du", "mu\u00dft", "mit", "Freu\u00b7dig\u00b7keit", "nur", "an", "dich", "sel\u00b7ber", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}