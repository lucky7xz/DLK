{"textgrid.poem.61548": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6chstes Leben", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O linder Fr\u00fchwind, Schein der Sonne,", "tokens": ["O", "lin\u00b7der", "Fr\u00fch\u00b7wind", ",", "Schein", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie f\u00fcllt ihr heut mir Herz und Sinn!", "tokens": ["Wie", "f\u00fcllt", "ihr", "heut", "mir", "Herz", "und", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getaucht in euch empfind' ich ganz die Wonne,", "tokens": ["Ge\u00b7taucht", "in", "euch", "emp\u00b7find'", "ich", "ganz", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das holde Wunder, da\u00df ich bin.", "tokens": ["Das", "hol\u00b7de", "Wun\u00b7der", ",", "da\u00df", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schwebt mein Geist in freudigem Gen\u00fcgen,", "tokens": ["Es", "schwebt", "mein", "Geist", "in", "freu\u00b7di\u00b7gem", "Ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gel\u00f6st von jeder M\u00fche, jedem Zwang;", "tokens": ["Ge\u00b7l\u00f6st", "von", "je\u00b7der", "M\u00fc\u00b7he", ",", "je\u00b7dem", "Zwang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er atmet nur in leisen Z\u00fcgen,", "tokens": ["Er", "at\u00b7met", "nur", "in", "lei\u00b7sen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein sein Atmen wird Gesang.", "tokens": ["Al\u00b7lein", "sein", "At\u00b7men", "wird", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie ein k\u00fchles Feuer im Gem\u00fcte", "tokens": ["Und", "wie", "ein", "k\u00fch\u00b7les", "Feu\u00b7er", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir spielend Ruhn und Tun in eins verkl\u00e4rt,", "tokens": ["Mir", "spie\u00b7lend", "Ruhn", "und", "Tun", "in", "eins", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "NN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchl' ich entz\u00fcckt: dies ist des Lebens Bl\u00fcte,", "tokens": ["F\u00fchl'", "ich", "ent\u00b7z\u00fcckt", ":", "dies", "ist", "des", "Le\u00b7bens", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und preise den, der mir auch das beschert.", "tokens": ["Und", "prei\u00b7se", "den", ",", "der", "mir", "auch", "das", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}