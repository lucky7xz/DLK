{"dta.poem.2830": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn an einander wir, o Freund, nicht \u00f6fter d\u00e4chten", "tokens": ["Wenn", "an", "ein\u00b7an\u00b7der", "wir", ",", "o", "Freund", ",", "nicht", "\u00f6f\u00b7ter", "d\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "PRF", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als schrieben, zweifelt' ich an unsrer Liebe M\u00e4chten.", "tokens": ["Als", "schrie\u00b7ben", ",", "zwei\u00b7felt'", "ich", "an", "uns\u00b7rer", "Lie\u00b7be", "M\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber zweifle nicht, ich wei\u00df mit Zuversicht:", "tokens": ["Ich", "a\u00b7ber", "zweif\u00b7le", "nicht", ",", "ich", "wei\u00df", "mit", "Zu\u00b7ver\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gibst mir, wie ich dir, tagt\u00e4glichen Bericht.", "tokens": ["Du", "gibst", "mir", ",", "wie", "ich", "dir", ",", "tag\u00b7t\u00e4g\u00b7li\u00b7chen", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ich empfang' ihn auch, wie du empf\u00e4ngst den meinen;", "tokens": ["Und", "ich", "emp\u00b7fang'", "ihn", "auch", ",", "wie", "du", "emp\u00b7f\u00e4ngst", "den", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir unterreden uns, wenn wir zu schweigen scheinen.", "tokens": ["Wir", "un\u00b7ter\u00b7re\u00b7den", "uns", ",", "wenn", "wir", "zu", "schwei\u00b7gen", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du wei\u00dft ja, wie ich war, drum wei\u00dft du, wie ich bin;", "tokens": ["Du", "wei\u00dft", "ja", ",", "wie", "ich", "war", ",", "drum", "wei\u00dft", "du", ",", "wie", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,", "PAV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie ich kannte dich, kenn' ich dich immerhin.", "tokens": ["Und", "wie", "ich", "kann\u00b7te", "dich", ",", "kenn'", "ich", "dich", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch wenn man ohne Schrift das Innre kann gewahren,", "tokens": ["Doch", "wenn", "man", "oh\u00b7ne", "Schrift", "das", "Inn\u00b7re", "kann", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Zeit zu Zeit will man was Aeu\u00dfres auch erfahren.", "tokens": ["Von", "Zeit", "zu", "Zeit", "will", "man", "was", "A\u00b7e\u00b7u\u00df\u00b7res", "auch", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VMFIN", "PIS", "PWS", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Denn unsre Freundschaft ist Gef\u00fchl ins Ferne zwar,", "tokens": ["Denn", "uns\u00b7re", "Freund\u00b7schaft", "ist", "Ge\u00b7f\u00fchl", "ins", "Fer\u00b7ne", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch kein Ferngesicht, wovor uns Gott bewahr!", "tokens": ["Je\u00b7doch", "kein", "Fern\u00b7ge\u00b7sicht", ",", "wo\u00b7vor", "uns", "Gott", "be\u00b7wahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PWAV", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum geb' ich Nachricht dir, da\u00df du mir Nachricht gebest,", "tokens": ["Drum", "geb'", "ich", "Nach\u00b7richt", "dir", ",", "da\u00df", "du", "mir", "Nach\u00b7richt", "ge\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Nicht, ob du mich noch liebst, nur, ob du auch noch lebest.", "tokens": ["Nicht", ",", "ob", "du", "mich", "noch", "liebst", ",", "nur", ",", "ob", "du", "auch", "noch", "le\u00b7best", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich leb' und freue mich noch jeder guten Stunde,", "tokens": ["Ich", "leb'", "und", "freu\u00b7e", "mich", "noch", "je\u00b7der", "gu\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von der b\u00f6sen nehm' ich lieber keine Kunde.", "tokens": ["Und", "von", "der", "b\u00f6\u00b7sen", "nehm'", "ich", "lie\u00b7ber", "kei\u00b7ne", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Noch minder g\u00e4b ich dir davon die Kunde gern,", "tokens": ["Noch", "min\u00b7der", "g\u00e4b", "ich", "dir", "da\u00b7von", "die", "Kun\u00b7de", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PAV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nah bliebe dir nur, was derweil mir schon ist fern.", "tokens": ["Nah", "blie\u00b7be", "dir", "nur", ",", "was", "der\u00b7weil", "mir", "schon", "ist", "fern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wie sollt' ich Dauer dem verleihn auf diesem Blatt,", "tokens": ["Wie", "sollt'", "ich", "Dau\u00b7er", "dem", "ver\u00b7leihn", "auf", "die\u00b7sem", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ART", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in der Wirklichkeit zum Gl\u00fcck nicht Dauer hat!", "tokens": ["Was", "in", "der", "Wirk\u00b7lich\u00b7keit", "zum", "Gl\u00fcck", "nicht", "Dau\u00b7er", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPRART", "NN", "PTKNEG", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}