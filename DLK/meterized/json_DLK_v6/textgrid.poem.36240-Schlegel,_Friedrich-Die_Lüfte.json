{"textgrid.poem.36240": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die L\u00fcfte", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00e4useln ach so linde!", "tokens": ["Wie", "s\u00e4u\u00b7seln", "ach", "so", "lin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir in den Bl\u00fcten,", "tokens": ["Wir", "in", "den", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und lindern hei\u00dfe Liebe", "tokens": ["Und", "lin\u00b7dern", "hei\u00b7\u00dfe", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In k\u00fchlen D\u00fcften.", "tokens": ["In", "k\u00fch\u00b7len", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn Blumen s\u00fc\u00df err\u00f6ten,", "tokens": ["Wenn", "Blu\u00b7men", "s\u00fc\u00df", "er\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besch\u00e4mt sich neigen,", "tokens": ["Be\u00b7sch\u00e4mt", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ber\u00fchrten wir die sch\u00f6nen", "tokens": ["Be\u00b7r\u00fchr\u00b7ten", "wir", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In leichter Eile.", "tokens": ["In", "leich\u00b7ter", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn wir dann Scherze s\u00e4useln", "tokens": ["Wenn", "wir", "dann", "Scher\u00b7ze", "s\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem, der sich gr\u00e4met,", "tokens": ["Dem", ",", "der", "sich", "gr\u00e4\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So wird die leise Freude", "tokens": ["So", "wird", "die", "lei\u00b7se", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn bald besch\u00e4men.", "tokens": ["Ihn", "bald", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}