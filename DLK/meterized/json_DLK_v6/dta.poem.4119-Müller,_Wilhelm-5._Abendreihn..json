{"dta.poem.4119": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "5.   Abendreihn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie blickst mir so traulich in's Herz herein?", "tokens": ["Wie", "blickst", "mir", "so", "trau\u00b7lich", "in's", "Herz", "her\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nun sprich, und la\u00df dich nicht lange fragen,", "tokens": ["Nun", "sprich", ",", "und", "la\u00df", "dich", "nicht", "lan\u00b7ge", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du hast mir gewi\u00df einen Gru\u00df zu sagen,", "tokens": ["Du", "hast", "mir", "ge\u00b7wi\u00df", "ei\u00b7nen", "Gru\u00df", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einen Gru\u00df von meinem Schatz.", "tokens": ["Ei\u00b7nen", "Gru\u00df", "von", "mei\u00b7nem", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00abwie sollt' ich bringen den Gru\u00df zu dir?", "tokens": ["\u00ab", "wie", "sollt'", "ich", "brin\u00b7gen", "den", "Gru\u00df", "zu", "dir", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00abdu hast ja keinen Schatz bei mir.", "tokens": ["\u00ab", "du", "hast", "ja", "kei\u00b7nen", "Schatz", "bei", "mir", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00abund was mir da unten die Bursche sagen,", "tokens": ["\u00ab", "und", "was", "mir", "da", "un\u00b7ten", "die", "Bur\u00b7sche", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00abund was mir die Frauen und M\u00e4dchen klagen,", "tokens": ["\u00ab", "und", "was", "mir", "die", "Frau\u00b7en", "und", "M\u00e4d\u00b7chen", "kla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u00abei, das versteh' ich nicht.\u00bb", "tokens": ["\u00ab", "ei", ",", "das", "ver\u00b7steh'", "ich", "nicht", ".", "\u00bb"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Hast Recht, mein lieber Mondenschein,", "tokens": ["Hast", "Recht", ",", "mein", "lie\u00b7ber", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du darfst auch Sch\u00e4tzchens Bote nicht sein,", "tokens": ["Du", "darfst", "auch", "Sch\u00e4tz\u00b7chens", "Bo\u00b7te", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Denn th\u00e4tst du zu tief ihr in's Auge sehn,", "tokens": ["Denn", "th\u00e4tst", "du", "zu", "tief", "ihr", "in's", "Au\u00b7ge", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKA", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Du k\u00f6nntest ja nimmermehr untergehn,", "tokens": ["Du", "k\u00f6nn\u00b7test", "ja", "nim\u00b7mer\u00b7mehr", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schienst ewig nur f\u00fcr sie.", "tokens": ["Schienst", "e\u00b7wig", "nur", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dies Liedchen ist ein Abendreihn,", "tokens": ["Dies", "Lied\u00b7chen", "ist", "ein", "A\u00b7ben\u00b7dreihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wandrer sang's im Vollmondschein;", "tokens": ["Ein", "Wand\u00b7rer", "sang's", "im", "Voll\u00b7mond\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die es lesen bei Kerzenlicht,", "tokens": ["Und", "die", "es", "le\u00b7sen", "bei", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Leute verstehn das Liedchen nicht,", "tokens": ["Die", "Leu\u00b7te", "ver\u00b7stehn", "das", "Lied\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ist doch kinderleicht.", "tokens": ["Und", "ist", "doch", "kin\u00b7der\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}