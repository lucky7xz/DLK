{"textgrid.poem.28761": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott schafft, erzeucht, tr\u00e4gt, speist, tr\u00e4nckt, labt, st\u00e4rckt, n\u00e4hrt, erquickt,", "tokens": ["Gott", "schafft", ",", "er\u00b7zeucht", ",", "tr\u00e4gt", ",", "speist", ",", "tr\u00e4nckt", ",", "labt", ",", "st\u00e4rckt", ",", "n\u00e4hrt", ",", "er\u00b7quickt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erh\u00e4lt, schenckt, sorgt, beschert, vermehrt, gewehret, schickt,", "tokens": ["Er\u00b7h\u00e4lt", ",", "schenckt", ",", "sorgt", ",", "be\u00b7schert", ",", "ver\u00b7mehrt", ",", "ge\u00b7weh\u00b7ret", ",", "schickt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Liebt, sch\u00fctzt, bewahrt, erl\u00f6st, beschattet, benedeyt,", "tokens": ["Liebt", ",", "sch\u00fctzt", ",", "be\u00b7wahrt", ",", "er\u00b7l\u00f6st", ",", "be\u00b7schat\u00b7tet", ",", "be\u00b7ne\u00b7deyt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schirmt, sichret, f\u00fchrt, regirt, errettet, hilfft, befreyt,", "tokens": ["Schirmt", ",", "sich\u00b7ret", ",", "f\u00fchrt", ",", "re\u00b7girt", ",", "er\u00b7ret\u00b7tet", ",", "hilfft", ",", "be\u00b7freyt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erleuchtet, unterweist, erfreut, sterbt und erweckt,", "tokens": ["Er\u00b7leuch\u00b7tet", ",", "un\u00b7ter\u00b7weist", ",", "er\u00b7freut", ",", "sterbt", "und", "er\u00b7weckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So da\u00df sich fort und fort sein Heil auff uns erstreckt.", "tokens": ["So", "da\u00df", "sich", "fort", "und", "fort", "sein", "Heil", "auff", "uns", "er\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "PTKVZ", "KON", "PTKVZ", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit allem dienstu, Gott, uns allen! ist auch wol,", "tokens": ["Mit", "al\u00b7lem", "diens\u00b7tu", ",", "Gott", ",", "uns", "al\u00b7len", "!", "ist", "auch", "wol", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "NN", "$,", "PPER", "PIAT", "$.", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der dir dient, einer nur und dient dir, wie er sol?", "tokens": ["Der", "dir", "dient", ",", "ei\u00b7ner", "nur", "und", "dient", "dir", ",", "wie", "er", "sol", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "ADV", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}