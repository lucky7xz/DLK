{"textgrid.poem.25060": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Holger", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich beseligt Tag auf Tage", "tokens": ["Wenn", "ich", "be\u00b7se\u00b7ligt", "Tag", "auf", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebannt in deine N\u00e4he bin,", "tokens": ["Ge\u00b7bannt", "in", "dei\u00b7ne", "N\u00e4\u00b7he", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dolores, kommt mir oft die Sage", "tokens": ["Do\u00b7lo\u00b7res", ",", "kommt", "mir", "oft", "die", "Sa\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von K\u00f6nig Holger in den Sinn.", "tokens": ["Von", "K\u00f6\u00b7nig", "Hol\u00b7ger", "in", "den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach S\u00fcden durch der St\u00fcrme W\u00fcten", "tokens": ["Nach", "S\u00fc\u00b7den", "durch", "der", "St\u00fcr\u00b7me", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlagen, fern von Isenland,", "tokens": ["Ver\u00b7schla\u00b7gen", ",", "fern", "von", "I\u00b7sen\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah er erstaunt sich unter Bl\u00fcten", "tokens": ["Sah", "er", "er\u00b7staunt", "sich", "un\u00b7ter", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Avalons begr\u00fcntem Strand.", "tokens": ["An", "A\u00b7va\u00b7lons", "be\u00b7gr\u00fcn\u00b7tem", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und gro\u00dfe goldne Fr\u00fcchte lachten", "tokens": ["Und", "gro\u00b7\u00dfe", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihn herab von dunklem Ast,", "tokens": ["Auf", "ihn", "her\u00b7ab", "von", "dunk\u00b7lem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Jungfraun f\u00fchrten den Erwachten", "tokens": ["Und", "Jung\u00b7fraun", "f\u00fchr\u00b7ten", "den", "Er\u00b7wach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer K\u00f6nigin Palast.", "tokens": ["In", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Entgegen trat im Marmorsaale", "tokens": ["Ent\u00b7ge\u00b7gen", "trat", "im", "Mar\u00b7mor\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Morgane hold dem Nordlandsohn,", "tokens": ["Mor\u00b7ga\u00b7ne", "hold", "dem", "Nord\u00b7land\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bot Wein ihm in krystallner Schale", "tokens": ["Bot", "Wein", "ihm", "in", "krys\u00b7tall\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lud ihn zu sich auf den Thron.", "tokens": ["Und", "lud", "ihn", "zu", "sich", "auf", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er blickte aufw\u00e4rts s\u00fc\u00df erschrocken", "tokens": ["Er", "blick\u00b7te", "auf\u00b7w\u00e4rts", "s\u00fc\u00df", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer Augen Himmelsglanz;", "tokens": ["In", "ih\u00b7rer", "Au\u00b7gen", "Him\u00b7mels\u00b7glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hernieder glitt auf seine Locken", "tokens": ["Her\u00b7nie\u00b7der", "glitt", "auf", "sei\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihrer Hand ein Blumenkranz;", "tokens": ["Aus", "ih\u00b7rer", "Hand", "ein", "Blu\u00b7men\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und fern dem Lande seiner Ahnen,", "tokens": ["Und", "fern", "dem", "Lan\u00b7de", "sei\u00b7ner", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wild die Nordseewoge sch\u00e4umt,", "tokens": ["Wo", "wild", "die", "Nord\u00b7see\u00b7wo\u00b7ge", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat K\u00f6nig Holger bei Morganen", "tokens": ["Hat", "K\u00f6\u00b7nig", "Hol\u00b7ger", "bei", "Mor\u00b7ga\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fortan Jahrhunderte vertr\u00e4umt.", "tokens": ["For\u00b7tan", "Jahr\u00b7hun\u00b7der\u00b7te", "ver\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}