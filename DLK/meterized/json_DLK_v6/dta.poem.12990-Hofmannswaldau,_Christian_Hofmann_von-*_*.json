{"dta.poem.12990": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drum bleib, Orbilius! wo du bisher gewesen,", "tokens": ["Drum", "bleib", ",", "Or\u00b7bi\u00b7lius", "!", "wo", "du", "bis\u00b7her", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NE", "$.", "PWAV", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und wage dich ja nicht in die galante welt:", "tokens": ["Und", "wa\u00b7ge", "dich", "ja", "nicht", "in", "die", "ga\u00b7lan\u00b7te", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du redest nicht was ihr, sie nicht, was dir gef\u00e4llt.", "tokens": ["Du", "re\u00b7dest", "nicht", "was", "ihr", ",", "sie", "nicht", ",", "was", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "$,", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was suchest du ihr denn was sch\u00f6nes vorzulesen.", "tokens": ["Was", "su\u00b7chest", "du", "ihr", "denn", "was", "sch\u00f6\u00b7nes", "vor\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "KON", "PWS", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was jener Frantzmann sagt, ist wol ein wahres wort:", "tokens": ["Was", "je\u00b7ner", "Frantz\u00b7mann", "sagt", ",", "ist", "wol", "ein", "wah\u00b7res", "wort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gelehrte, welche nie aus ihrer kammer weichen,", "tokens": ["Ge\u00b7lehr\u00b7te", ",", "wel\u00b7che", "nie", "aus", "ih\u00b7rer", "kam\u00b7mer", "wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind (f\u00fcchse! merckt es doch!) gespenstern zu vergleichen.", "tokens": ["Sind", "(", "f\u00fcch\u00b7se", "!", "merckt", "es", "doch", "!", ")", "ge\u00b7spens\u00b7tern", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$.", "$(", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo diese sp\u00fccken gehn, da rennet alles fort.", "tokens": ["Wo", "die\u00b7se", "sp\u00fc\u00b7cken", "gehn", ",", "da", "ren\u00b7net", "al\u00b7les", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVINF", "VVINF", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}