{"textgrid.poem.28402": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer uns f\u00fcr Unkraut h\u00e4lt und wil uns bald vertreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer uns f\u00fcr Unkraut h\u00e4lt und wil uns bald vertreiben,", "tokens": ["Wer", "uns", "f\u00fcr", "Un\u00b7kraut", "h\u00e4lt", "und", "wil", "uns", "bald", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thut nichts, als da\u00df er sich sam uns noch auff wird reiben;", "tokens": ["Thut", "nichts", ",", "als", "da\u00df", "er", "sich", "sam", "uns", "noch", "auff", "wird", "rei\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "KOKOM", "KOUS", "PPER", "PRF", "ADJD", "PPER", "ADV", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er warte bi\u00df zum Augst, da wird man deutlich kennen,", "tokens": ["Er", "war\u00b7te", "bi\u00df", "zum", "Augst", ",", "da", "wird", "man", "deut\u00b7lich", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,", "ADV", "VAFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer t\u00fcglich sey zur Ernt und w\u00fcrdig zum verbrennen.", "tokens": ["Wer", "t\u00fcg\u00b7lich", "sey", "zur", "Ernt", "und", "w\u00fcr\