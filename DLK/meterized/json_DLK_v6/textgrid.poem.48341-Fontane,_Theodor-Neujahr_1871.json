{"textgrid.poem.48341": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Neujahr 1871", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das alte Jahr \u2013 vom Turm hat's ausgeklungen,", "tokens": ["Das", "al\u00b7te", "Jahr", "\u2013", "vom", "Turm", "hat's", "aus\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf horcht im Traum der Dohlen dunkle Schar,", "tokens": ["Auf", "horcht", "im", "Traum", "der", "Doh\u00b7len", "dunk\u00b7le", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und klirrend sind die Pforten aufgesprungen", "tokens": ["Und", "klir\u00b7rend", "sind", "die", "Pfor\u00b7ten", "auf\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "(wie Waffen klirr'n) von einem neuen Jahr;", "tokens": ["(", "wie", "Waf\u00b7fen", "klirr'n", ")", "von", "ei\u00b7nem", "neu\u00b7en", "Jahr", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "NN", "VVINF", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Trennungsschnitt ist wieder eingedrungen", "tokens": ["Ein", "Tren\u00b7nungs\u00b7schnitt", "ist", "wie\u00b7der", "ein\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In das, was sein wird, und in das, was war,", "tokens": ["In", "das", ",", "was", "sein", "wird", ",", "und", "in", "das", ",", "was", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "VAINF", "VAFIN", "$,", "KON", "APPR", "PDS", "$,", "PWS", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und eh' wir Wunsch und Bitte vorw\u00e4rts schicken,", "tokens": ["Und", "eh'", "wir", "Wunsch", "und", "Bit\u00b7te", "vor\u00b7w\u00e4rts", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was l\u00e4g' uns n\u00e4her, als zur\u00fcckzublicken?", "tokens": ["Was", "l\u00e4g'", "uns", "n\u00e4\u00b7her", ",", "als", "zu\u00b7r\u00fcck\u00b7zu\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In welch ein Jahr! Es ruht das stille Schaffen,", "tokens": ["In", "welch", "ein", "Jahr", "!", "Es", "ruht", "das", "stil\u00b7le", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dinge sch\u00f6nes Gleichma\u00df ist gest\u00f6rt,", "tokens": ["Der", "Din\u00b7ge", "sch\u00f6\u00b7nes", "Gleich\u00b7ma\u00df", "ist", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Rhein zum Njemen klingt es: \u00bbZu den Waffen!", "tokens": ["Vom", "Rhein", "zum", "Nje\u00b7men", "klingt", "es", ":", "\u00bb", "Zu", "den", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Unrecht schreit, die Schmach ist unerh\u00f6rt\u00ab \u2013", "tokens": ["Das", "Un\u00b7recht", "schreit", ",", "die", "Schmach", "ist", "un\u00b7er\u00b7h\u00f6rt", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und bis zu ", "tokens": ["Und", "bis", "zu"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PTKZU"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Seit jenem Tag von Wei\u00dfenburg und W\u00f6rth,", "tokens": ["Seit", "je\u00b7nem", "Tag", "von", "Wei\u00b7\u00dfen\u00b7burg", "und", "W\u00f6rth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In jedem Kampf aufs neue ruhmbereichert,", "tokens": ["In", "je\u00b7dem", "Kampf", "aufs", "neu\u00b7e", "ruhm\u00b7be\u00b7rei\u00b7chert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was ward seit Spichern alles aufgespeichert!", "tokens": ["Was", "ward", "seit", "Spi\u00b7chern", "al\u00b7les", "auf\u00b7ge\u00b7spei\u00b7chert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dreimal vor Metz, in ungeheurem Ringen,", "tokens": ["Drei\u00b7mal", "vor", "Metz", ",", "in", "un\u00b7ge\u00b7heu\u00b7rem", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf, ab die Mosel fing das Ernten an,", "tokens": ["Auf", ",", "ab", "die", "Mo\u00b7sel", "fing", "das", "Ern\u00b7ten", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis an der Maas in eisernem Umschlingen", "tokens": ["Bis", "an", "der", "Maas", "in", "ei\u00b7ser\u00b7nem", "Um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deutschland den Ehr- und Erntekranz gewann;", "tokens": ["Deutschland", "den", "Ehr", "und", "Ern\u00b7te\u00b7kranz", "ge\u00b7wann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "An dieses Kranzes blut'gen \u00c4hren hingen", "tokens": ["An", "die\u00b7ses", "Kran\u00b7zes", "blut'\u00b7gen", "\u00c4h\u00b7ren", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Armeen: dreimalhunderttausend Mann,", "tokens": ["Ar\u00b7me\u00b7en", ":", "drei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Mann", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "CARD", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Gefangen all! Ein Kaiser ging verloren,", "tokens": ["Ge\u00b7fan\u00b7gen", "all", "!", "Ein", "Kai\u00b7ser", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein andrer: (Kaiser Wei\u00dfhart) ward geboren.", "tokens": ["Ein", "an\u00b7drer", ":(", "Kai\u00b7ser", "Wei\u00df\u00b7hart", ")", "ward", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "emoticon", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "NN", "NN", "$(", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das alte Jahr, in Kampf und Mut und Streben", "tokens": ["Das", "al\u00b7te", "Jahr", ",", "in", "Kampf", "und", "Mut", "und", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat's uns gefeit, gewappnet und gest\u00e4hlt,", "tokens": ["Hat's", "uns", "ge\u00b7feit", ",", "ge\u00b7wapp\u00b7net", "und", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du neues Jahr, o woll' auch ", "tokens": ["Du", "neu\u00b7es", "Jahr", ",", "o", "woll'", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "FM", "VMFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "La\u00df jenen \u00d6lzweig zu uns niederschweben,", "tokens": ["La\u00df", "je\u00b7nen", "\u00d6l\u00b7zweig", "zu", "uns", "nie\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf den ein jedes Herz jetzt hofft und z\u00e4hlt,", "tokens": ["Auf", "den", "ein", "je\u00b7des", "Herz", "jetzt", "hofft", "und", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "PIAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu allem, was das alte Jahr beschieden,", "tokens": ["Zu", "al\u00b7lem", ",", "was", "das", "al\u00b7te", "Jahr", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du neues Jahr, o gib uns ", "tokens": ["Du", "neu\u00b7es", "Jahr", ",", "o", "gib", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "FM", "VVIMP", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}