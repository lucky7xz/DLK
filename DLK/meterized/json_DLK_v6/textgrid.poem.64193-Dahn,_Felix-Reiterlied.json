{"textgrid.poem.64193": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Reiterlied", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glitzernder Sonnenstrahl", "tokens": ["Glit\u00b7zern\u00b7der", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Spielt auf des Helmes Stahl \u2013", "tokens": ["Spielt", "auf", "des", "Hel\u00b7mes", "Stahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tau auf den Wegen:", "tokens": ["Tau", "auf", "den", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Renne, mein Ro\u00df, geschwind!", "tokens": ["Ren\u00b7ne", ",", "mein", "Ro\u00df", ",", "ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Auf und dem Morgenwind", "tokens": ["Auf", "und", "dem", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Lustig entgegen!", "tokens": ["Lus\u00b7tig", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schimmernder Nebel h\u00e4lt", "tokens": ["Schim\u00b7mern\u00b7der", "Ne\u00b7bel", "h\u00e4lt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vor uns die weite Welt", "tokens": ["Vor", "uns", "die", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Duftig umflossen:", "tokens": ["Duf\u00b7tig", "um\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sprenget drauf an und ein!", "tokens": ["Spren\u00b7get", "drauf", "an", "und", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Alles mu\u00df unser sein", "tokens": ["Al\u00b7les", "mu\u00df", "un\u00b7ser", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPOSAT", "VAINF"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Was sie umschlossen.", "tokens": ["Was", "sie", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ob mich mein Liebchen rot,", "tokens": ["Ob", "mich", "mein", "Lieb\u00b7chen", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ob mich der bleiche Tod", "tokens": ["Ob", "mich", "der", "blei\u00b7che", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heut' noch erwarte:", "tokens": ["Heut'", "noch", "er\u00b7war\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Reite nur: \u2013 frage nicht! \u2013", "tokens": ["Rei\u00b7te", "nur", ":", "\u2013", "fra\u00b7ge", "nicht", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$.", "$(", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Lustig im Morgenlicht", "tokens": ["Lus\u00b7tig", "im", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Fliegt die Standarte!", "tokens": ["Fliegt", "die", "Stand\u00b7ar\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}