{"textgrid.poem.40233": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt sollt ihr h\u00f6ren ein ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt sollt ihr h\u00f6ren ein ", "tokens": ["Jetzt", "sollt", "ihr", "h\u00f6\u00b7ren", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "von Zagen und Erbarmen ", "tokens": ["von", "Za\u00b7gen", "und", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Winternachtsturm schreit im Ried", "tokens": ["Der", "Win\u00b7ter\u00b7nachts\u00b7turm", "schreit", "im", "Ried"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und peitscht das Schilf wie Heu umher;", "tokens": ["und", "peitscht", "das", "Schilf", "wie", "Heu", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "vor seinem Schnauben erstarrt das Moor,", "tokens": ["vor", "sei\u00b7nem", "Schnau\u00b7ben", "er\u00b7starrt", "das", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "zerknicken die Binsen, zerbricht das Rohr! \u2013", "tokens": ["zer\u00b7kni\u00b7cken", "die", "Bin\u00b7sen", ",", "zer\u00b7bricht", "das", "Rohr", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die H\u00fctte umheult er am Haiderand", "tokens": ["Die", "H\u00fct\u00b7te", "um\u00b7heult", "er", "am", "Hai\u00b7de\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und sch\u00fcttelt die Pfosten der rissigen Wand", "tokens": ["und", "sch\u00fct\u00b7telt", "die", "Pfos\u00b7ten", "der", "ris\u00b7si\u00b7gen", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "und rei\u00dft an den Haspen und Sparren,", "tokens": ["und", "rei\u00dft", "an", "den", "Has\u00b7pen", "und", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "da\u00df im Froste sie kreischen und knarren", "tokens": ["da\u00df", "im", "Fros\u00b7te", "sie", "krei\u00b7schen", "und", "knar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVINF", "KON", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "und drinnen am Ofen die Kinder erschauern", "tokens": ["und", "drin\u00b7nen", "am", "O\u00b7fen", "die", "Kin\u00b7der", "er\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "und dichter zum Schoo\u00dfe der Mutter sich kauern.", "tokens": ["und", "dich\u00b7ter", "zum", "Schoo\u00b7\u00dfe", "der", "Mut\u00b7ter", "sich", "kau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die streckt von Aengsten dumpf ger\u00fchrt", "tokens": ["Die", "streckt", "von", "A\u00b7engs\u00b7ten", "dumpf", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NE", "ADJD", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zum Vater, der finster mit hastiger Faust", "tokens": ["zum", "Va\u00b7ter", ",", "der", "fins\u00b7ter", "mit", "has\u00b7ti\u00b7ger", "Faust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Flugschriften zu St\u00f6\u00dfen und Ballen schn\u00fcrt,", "tokens": ["Flug\u00b7schrif\u00b7ten", "zu", "St\u00f6\u00b7\u00dfen", "und", "Bal\u00b7len", "schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "die bittenden zitternden H\u00e4nde:", "tokens": ["die", "bit\u00b7ten\u00b7den", "zit\u00b7tern\u00b7den", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u00bbach, Mann! geh ", "tokens": ["\u00bb", "ach", ",", "Mann", "!", "geh"], "token_info": ["punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Doch stumm aus dem Packen ein Blatt er zaust", "tokens": ["Doch", "stumm", "aus", "dem", "Pa\u00b7cken", "ein", "Blatt", "er", "zaust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "und weist ihr die Worte am Ende:", "tokens": ["und", "weist", "ihr", "die", "Wor\u00b7te", "am", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Mensch pre\u00dfte den Menschen in Schmach und Acht,", "tokens": ["Mensch", "pre\u00df\u00b7te", "den", "Men\u00b7schen", "in", "Schmach", "und", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "CARD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "weil Jeder nur immer sich selber bedacht;", "tokens": ["weil", "Je\u00b7der", "nur", "im\u00b7mer", "sich", "sel\u00b7ber", "be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "so habt ihr euch selber zu Knechten gemacht!", "tokens": ["so", "habt", "ihr", "euch", "sel\u00b7ber", "zu", "Knech\u00b7ten", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "drum schaart euch, ihr Schwachen, ", "tokens": ["drum", "schaart", "euch", ",", "ihr", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "St\u00fctzt R\u00fccken an R\u00fccken zum rettenden Heer,", "tokens": ["St\u00fctzt", "R\u00fc\u00b7cken", "an", "R\u00fc\u00b7cken", "zum", "ret\u00b7ten\u00b7den", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "so schwellen die Wellen zum donnernden Meer,", "tokens": ["so", "schwel\u00b7len", "die", "Wel\u00b7len", "zum", "don\u00b7nern\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "die F\u00fcnkchen zu sausenden Flammen! \u2013", "tokens": ["die", "F\u00fcnk\u00b7chen", "zu", "sau\u00b7sen\u00b7den", "Flam\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die Backen ihm zucken, hart er spricht:", "tokens": ["Die", "Ba\u00b7cken", "ihm", "zu\u00b7cken", ",", "hart", "er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdrum bettle nicht! drum qu\u00e4l' mich nicht!", "tokens": ["\u00bb", "drum", "bett\u00b7le", "nicht", "!", "drum", "qu\u00e4l'", "mich", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PTKNEG", "$.", "PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich hab's den Genossen geschworen!", "tokens": ["ich", "hab's", "den", "Ge\u00b7nos\u00b7sen", "ge\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Wahlruf mu\u00df ", "tokens": ["Der", "Wahl\u00b7ruf", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "sonst mach' ich den ", "tokens": ["sonst", "mach'", "ich", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "\u00bbgeh nicht, geh nicht! was schiert der Sieg", "tokens": ["\u00bb", "geh", "nicht", ",", "geh", "nicht", "!", "was", "schiert", "der", "Sieg"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$.", "PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dein Weib und die jammernden Kleinen!", "tokens": ["dein", "Weib", "und", "die", "jam\u00b7mern\u00b7den", "Klei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Geh nicht, geh nicht \u2013 die zweite Nacht", "tokens": ["Geh", "nicht", ",", "geh", "nicht", "\u2013", "die", "zwei\u00b7te", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Eis erst steht! o Gott, es kracht,", "tokens": ["das", "Eis", "erst", "steht", "!", "o", "Gott", ",", "es", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "FM", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es bricht! o sieh mich weinen!", "tokens": ["es", "bricht", "!", "o", "sieh", "mich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "FM", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "es schreit zum Himmel! dein Leben ist ", "tokens": ["es", "schreit", "zum", "Him\u00b7mel", "!", "dein", "Le\u00b7ben", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da flackert sein Auge von Zorn und Pein:", "tokens": ["Da", "fla\u00b7ckert", "sein", "Au\u00b7ge", "von", "Zorn", "und", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u00bbschrei lieber zu Teufel und H\u00f6lle!\u00ab", "tokens": ["\u00bb", "schrei", "lie\u00b7ber", "zu", "Teu\u00b7fel", "und", "H\u00f6l\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und grimmig wuchtet die Last er hoch", "tokens": ["und", "grim\u00b7mig", "wuch\u00b7tet", "die", "Last", "er", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und knirscht, schon tritt er die Schwelle:", "tokens": ["und", "knirscht", ",", "schon", "tritt", "er", "die", "Schwel\u00b7le", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbhat's etwa dein Herrgott zu ", "tokens": ["\u00bb", "hat's", "et\u00b7wa", "dein", "Herr\u00b7gott", "zu"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "ADV", "PPOSAT", "NN", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df Morgen um Morgen ich mu\u00df in den Schacht", "tokens": ["da\u00df", "Mor\u00b7gen", "um", "Mor\u00b7gen", "ich", "mu\u00df", "in", "den", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "die Knochen f\u00fcr'n Hungerlohn tragen?", "tokens": ["die", "Kno\u00b7chen", "f\u00fcr'n", "Hun\u00b7ger\u00b7lohn", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und sollte mein Leben nicht Eine Nacht", "tokens": ["und", "soll\u00b7te", "mein", "Le\u00b7ben", "nicht", "Ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "f\u00fcr Gl\u00fcck und Gerechtigkeit wagen?!", "tokens": ["f\u00fcr", "Gl\u00fcck", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "wa\u00b7gen", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Die Windsbraut st\u00f6hnt und \u00e4chzt im Schlot.", "tokens": ["Die", "Winds\u00b7braut", "st\u00f6hnt", "und", "\u00e4chzt", "im", "Schlot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondes Stirne blank und kalt", "tokens": ["Des", "Mon\u00b7des", "Stir\u00b7ne", "blank", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "am fahlen Horizonte droht.", "tokens": ["am", "fah\u00b7len", "Ho\u00b7ri\u00b7zon\u00b7te", "droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bergmann gl\u00fcht; er trieft von Schwei\u00df.", "tokens": ["Der", "Berg\u00b7mann", "gl\u00fcht", ";", "er", "trieft", "von", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Thr\u00e4nenstr\u00f6me flimmern im Eis", "tokens": ["Wie", "Thr\u00e4\u00b7nen\u00b7str\u00f6\u00b7me", "flim\u00b7mern", "im", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "p