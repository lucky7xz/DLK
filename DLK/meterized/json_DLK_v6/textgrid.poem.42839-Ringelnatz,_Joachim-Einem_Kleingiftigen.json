{"textgrid.poem.42839": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Einem Kleingiftigen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht, da\u00df ein Unverstandenes", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "ein", "Un\u00b7ver\u00b7stan\u00b7de\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Oder ein gar nicht Vorhandenes", "tokens": ["O\u00b7der", "ein", "gar", "nicht", "Vor\u00b7han\u00b7de\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "PTKNEG", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dich verdro\u00df.", "tokens": ["Dich", "ver\u00b7dro\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und nun m\u00f6chtest du heimlich erschie\u00dfen", "tokens": ["Und", "nun", "m\u00f6ch\u00b7test", "du", "heim\u00b7lich", "er\u00b7schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und noch den Schrei genie\u00dfen:", "tokens": ["Und", "noch", "den", "Schrei", "ge\u00b7nie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbdas war Tells Gescho\u00df!\u00ab", "tokens": ["\u00bb", "das", "war", "Tells", "Ge\u00b7scho\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "NN", "NN", "$.", "$("], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Aber ein Pup ist kein Blitz.", "tokens": ["A\u00b7ber", "ein", "Pup", "ist", "kein", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Du mu\u00dft dich schon anders entladen.", "tokens": ["Du", "mu\u00dft", "dich", "schon", "an\u00b7ders", "ent\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du mu\u00dft deinen eigenen Schaden", "tokens": ["Du", "mu\u00dft", "dei\u00b7nen", "ei\u00b7ge\u00b7nen", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Riskieren und Mut verraten", "tokens": ["Ris\u00b7kie\u00b7ren", "und", "Mut", "ver\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Oder wenigstens Witz.", "tokens": ["O\u00b7der", "we\u00b7nigs\u00b7tens", "Witz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "War's aber eine erkannte, bestimmte", "tokens": ["Wa\u00b7r's", "a\u00b7ber", "ei\u00b7ne", "er\u00b7kann\u00b7te", ",", "be\u00b7stimm\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "PIS", "VVFIN", "$,", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Angelegenheit, die dich ergrimmte,", "tokens": ["An\u00b7ge\u00b7le\u00b7gen\u00b7heit", ",", "die", "dich", "er\u00b7grimm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Etwa was Ungerechtes \u2013 \u2013 \u2013", "tokens": ["Et\u00b7wa", "was", "Un\u00b7ge\u00b7rech\u00b7tes", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PWS", "ADJA", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, wieviel Schlechtes", "tokens": ["Ach", ",", "wie\u00b7viel", "Schlech\u00b7tes"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Tatest du?!", "tokens": ["Ta\u00b7test", "du", "?!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und klapptest stillschweigend den Deckel zu.", "tokens": ["Und", "klapp\u00b7test", "still\u00b7schwei\u00b7gend", "den", "De\u00b7ckel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Hau doch in den Kartoffelsalat,", "tokens": ["Hau", "doch", "in", "den", "Kar\u00b7tof\u00b7fel\u00b7sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df die Sauce spritzt.", "tokens": ["Da\u00df", "die", "Sau\u00b7ce", "spritzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das ist ein schlechter Soldat,", "tokens": ["Das", "ist", "ein", "schlech\u00b7ter", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Blut ertr\u00e4umt", "tokens": ["Der", "Blut", "er\u00b7tr\u00e4umt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Rache schwitzt", "tokens": ["Und", "Ra\u00b7che", "schwitzt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und vor Wut sch\u00e4umt", "tokens": ["Und", "vor", "Wut", "sch\u00e4umt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und dabei auf dem Lokus sitzt.", "tokens": ["Und", "da\u00b7bei", "auf", "dem", "Lo\u00b7kus", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder leg' deinen Zorn, wenn du willst,", "tokens": ["O\u00b7der", "leg'", "dei\u00b7nen", "Zorn", ",", "wenn", "du", "willst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als etwas Echtes, wenn auch nicht Stubenreines,", "tokens": ["Als", "et\u00b7was", "Ech\u00b7tes", ",", "wenn", "auch", "nicht", "Stu\u00b7ben\u00b7rei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "KOUS", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An deine eigene Brust, da\u00df du ihn stillst,", "tokens": ["An", "dei\u00b7ne", "ei\u00b7ge\u00b7ne", "Brust", ",", "da\u00df", "du", "ihn", "stillst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie eine Mutter ihr Kleines.", "tokens": ["Wie", "ei\u00b7ne", "Mut\u00b7ter", "ihr", "Klei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Nach eines Jahrmarkts letzter Nacht", "tokens": ["Nach", "ei\u00b7nes", "Jahr\u00b7markts", "letz\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist in wenigen Stunden", "tokens": ["Ist", "in", "we\u00b7ni\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Eine ganze Stadt voll blendender Zauberpracht", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Stadt", "voll", "blen\u00b7den\u00b7der", "Zau\u00b7ber\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kl\u00e4glich verschwunden.", "tokens": ["Kl\u00e4g\u00b7lich", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}