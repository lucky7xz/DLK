{"dta.poem.12026": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich nur blenden nicht von denen die ersannen", "tokens": ["La\u00df", "dich", "nur", "blen\u00b7den", "nicht", "von", "de\u00b7nen", "die", "er\u00b7san\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "PTKNEG", "APPR", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denkformeln um darein Undenkbares zu bannen.", "tokens": ["Denk\u00b7for\u00b7meln", "um", "da\u00b7rein", "Un\u00b7denk\u00b7ba\u00b7res", "zu", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PAV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil sich kein H\u00f6chstes l\u00e4\u00dft aus H\u00f6herem erkl\u00e4ren,", "tokens": ["Weil", "sich", "kein", "H\u00f6chs\u00b7tes", "l\u00e4\u00dft", "aus", "H\u00f6\u00b7he\u00b7rem", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lassen sie das Ding sich selbst aus sich geb\u00e4ren.", "tokens": ["So", "las\u00b7sen", "sie", "das", "Ding", "sich", "selbst", "aus", "sich", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn in der That nun wird nur was schon war im Grunde,", "tokens": ["Wenn", "in", "der", "That", "nun", "wird", "nur", "was", "schon", "war", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "VAFIN", "ADV", "PWS", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist das Seyn erkl\u00e4rt, doch ists nicht klar im Grunde.", "tokens": ["So", "ist", "das", "Seyn", "er\u00b7kl\u00e4rt", ",", "doch", "ists", "nicht", "klar", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}