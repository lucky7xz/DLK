{"textgrid.poem.32734": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied einer Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trocknet, milde Fr\u00fchlingsl\u00fcfte,", "tokens": ["Trock\u00b7net", ",", "mil\u00b7de", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine vielen Thr\u00e4nen auf!", "tokens": ["Mei\u00b7ne", "vie\u00b7len", "Thr\u00e4\u00b7nen", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Send', o Abend, deine D\u00fcfte", "tokens": ["Send'", ",", "o", "A\u00b7bend", ",", "dei\u00b7ne", "D\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Zelle mir herauf!", "tokens": ["Zu", "der", "Zel\u00b7le", "mir", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Philomele stimmet", "tokens": ["A\u00b7ber", "Phi\u00b7lo\u00b7me\u00b7le", "stim\u00b7met"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder mich zum Klageton,", "tokens": ["Wie\u00b7der", "mich", "zum", "Kla\u00b7ge\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in frischen Z\u00e4hren schwimmet", "tokens": ["Und", "in", "fri\u00b7schen", "Z\u00e4h\u00b7ren", "schwim\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein erloschnes Auge schon!", "tokens": ["Mein", "er\u00b7lo\u00b7schnes", "Au\u00b7ge", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dank dir, liebe Philomele,", "tokens": ["Dank", "dir", ",", "lie\u00b7be", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du in mein Leiden weinst,", "tokens": ["Da\u00df", "du", "in", "mein", "Lei\u00b7den", "weinst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit einer frommen Seele", "tokens": ["Da\u00df", "mit", "ei\u00b7ner", "from\u00b7men", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du zu Klagen dich vereinst!", "tokens": ["Du", "zu", "Kla\u00b7gen", "dich", "ver\u00b7einst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Menschen, die mich schlau betrogen,", "tokens": ["Men\u00b7schen", ",", "die", "mich", "schlau", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kennen kein Erbarmen mehr!", "tokens": ["Ken\u00b7nen", "kein", "Er\u00b7bar\u00b7men", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Augen, die mir Liebe logen,", "tokens": ["Au\u00b7gen", ",", "die", "mir", "Lie\u00b7be", "lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind von Mitleidsthr\u00e4nen leer!", "tokens": ["Sind", "von", "Mit\u00b7leidst\u00b7hr\u00e4\u00b7nen", "leer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber treue Liebe f\u00fcllet,", "tokens": ["A\u00b7ber", "treu\u00b7e", "Lie\u00b7be", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Guter Mond am Himmel, dich!", "tokens": ["Gu\u00b7ter", "Mond", "am", "Him\u00b7mel", ",", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem Auge gleich, verh\u00fcllet", "tokens": ["Mei\u00b7nem", "Au\u00b7ge", "gleich", ",", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines in den Schleier sich!", "tokens": ["Dei\u00b7nes", "in", "den", "Schlei\u00b7er", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die bleiche Wange wallen", "tokens": ["Um", "die", "blei\u00b7che", "Wan\u00b7ge", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weinende Gew\u00f6lke nur;", "tokens": ["Wei\u00b7nen\u00b7de", "Ge\u00b7w\u00f6l\u00b7ke", "nur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Und in Perlentropfen fallen", "tokens": ["Und", "in", "Per\u00b7len\u00b7trop\u00b7fen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Thr\u00e4nen auf die Blumenflur.", "tokens": ["Thr\u00e4\u00b7nen", "auf", "die", "Blu\u00b7men\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rosen schlie\u00dfen, ungesehen,", "tokens": ["Ro\u00b7sen", "schlie\u00b7\u00dfen", ",", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im Klostergarten auf;", "tokens": ["Sich", "im", "Klos\u00b7ter\u00b7gar\u00b7ten", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromme Fr\u00fchlingswinde wehen", "tokens": ["From\u00b7me", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Wohlgeruch herauf!", "tokens": ["Ih\u00b7ren", "Wohl\u00b7ge\u00b7ruch", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbeklagt, wie ihr, verf\u00e4rbet", "tokens": ["Un\u00b7be\u00b7klagt", ",", "wie", "ihr", ",", "ver\u00b7f\u00e4r\u00b7bet"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich, ihr Rosen, mein Gesicht!", "tokens": ["Sich", ",", "ihr", "Ro\u00b7sen", ",", "mein", "Ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe Rosen, warum sterbet", "tokens": ["Lie\u00b7be", "Ro\u00b7sen", ",", "wa\u00b7rum", "ster\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr auf meinem Grabe nicht?", "tokens": ["Ihr", "auf", "mei\u00b7nem", "Gra\u00b7be", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}