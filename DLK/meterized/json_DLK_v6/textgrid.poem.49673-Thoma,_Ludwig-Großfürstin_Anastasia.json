{"textgrid.poem.49673": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dff\u00fcrstin Anastasia", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die junge Gro\u00dfmama,", "tokens": ["Das", "ist", "die", "jun\u00b7ge", "Gro\u00df\u00b7ma\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau F\u00fcrstin Anastasia,", "tokens": ["Frau", "F\u00fcrs\u00b7tin", "A\u00b7nas\u00b7ta\u00b7sia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sie war wohl chic und nett, ja, ja,", "tokens": ["Sie", "war", "wohl", "chic", "und", "nett", ",", "ja", ",", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "KON", "ADJD", "$,", "PTKANT", "$,", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist auch jetzt noch so la la,", "tokens": ["Und", "ist", "auch", "jetzt", "noch", "so", "la", "la", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ADV", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer sie sieht, und wer sie sah,", "tokens": ["Und", "wer", "sie", "sieht", ",", "und", "wer", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pfeift durch die Z\u00e4hne, tralala!", "tokens": ["Pfeift", "durch", "die", "Z\u00e4h\u00b7ne", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie geht, als wie zum Rendezvous,", "tokens": ["Sie", "geht", ",", "als", "wie", "zum", "Ren\u00b7dez\u00b7vous", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seidne Str\u00fcmpf' und St\u00f6ckelschuh',", "tokens": ["Hat", "seid\u00b7ne", "Str\u00fcmpf'", "und", "St\u00f6\u00b7ckel\u00b7schuh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Unterr\u00f6ckchen macht frou-frou,", "tokens": ["Ihr", "Un\u00b7ter\u00b7r\u00f6ck\u00b7chen", "macht", "frou\u00b7frou", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rt dem Rauschen gerne zu,", "tokens": ["Man", "h\u00f6rt", "dem", "Rau\u00b7schen", "ger\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denkt: Verflucht und sieh mal du!", "tokens": ["Und", "denkt", ":", "Ver\u00b7flucht", "und", "sieh", "mal", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "KON", "VVIMP", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich m\u00f6chte wohl und ei cou-cou!", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wohl", "und", "ei", "cou\u00b7cou", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun denkt euch wohl, die h\u00fcbsche Fee", "tokens": ["Nun", "denkt", "euch", "wohl", ",", "die", "h\u00fcb\u00b7sche", "Fee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam nach Berlin ins Schlo\u00df, o jeh!", "tokens": ["Kam", "nach", "Ber\u00b7lin", "ins", "Schlo\u00df", ",", "o", "jeh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "APPRART", "NN", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort trinkt man nur den d\u00fcnnsten Tee,", "tokens": ["Dort", "trinkt", "man", "nur", "den", "d\u00fcnns\u00b7ten", "Tee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gar so rein, wie frischer Schnee,", "tokens": ["Ist", "gar", "so", "rein", ",", "wie", "fri\u00b7scher", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seidne R\u00f6ck' und Str\u00fcmpf', ol\u00e9,", "tokens": ["Und", "seid\u00b7ne", "R\u00f6ck'", "und", "Str\u00fcmpf'", ",", "ol\u00e9", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die mag man nicht, du Jemine!", "tokens": ["Die", "mag", "man", "nicht", ",", "du", "Je\u00b7mi\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PTKNEG", "$,", "PPER", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Herrn vom Consistorio,", "tokens": ["Die", "Herrn", "vom", "Con\u00b7sis\u00b7to\u00b7rio", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die seufzen ouh! und st\u00f6hnen oh!", "tokens": ["Die", "seuf\u00b7zen", "ouh", "!", "und", "st\u00f6h\u00b7nen", "oh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die F\u00fcrstin ist so lebensfroh,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "ist", "so", "le\u00b7bens\u00b7froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn sie nie die S\u00fcnde floh,", "tokens": ["Als", "wenn", "sie", "nie", "die", "S\u00fcn\u00b7de", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihre R\u00f6cke rauschen so!", "tokens": ["Und", "ih\u00b7re", "R\u00f6\u00b7cke", "rau\u00b7schen", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sucht ihr Heil ganz anderswo.", "tokens": ["Sie", "sucht", "ihr", "Heil", "ganz", "an\u00b7ders\u00b7wo", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das n\u00e4selt, fl\u00fcstert ohne Ruh'", "tokens": ["Das", "n\u00e4\u00b7selt", ",", "fl\u00fcs\u00b7tert", "oh\u00b7ne", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fromme Augen blicken, huh!", "tokens": ["Und", "from\u00b7me", "Au\u00b7gen", "bli\u00b7cken", ",", "huh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kn\u00f6pft den Rock sich fr\u00f6stelnd zu.", "tokens": ["Man", "kn\u00f6pft", "den", "Rock", "sich", "fr\u00f6s\u00b7telnd", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die F\u00fcrstin aber denkt: nanu!", "tokens": ["Die", "F\u00fcrs\u00b7tin", "a\u00b7ber", "denkt", ":", "na\u00b7nu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt ihre Str\u00fcmpf' und St\u00f6ckelschuh',", "tokens": ["Nimmt", "ih\u00b7re", "Str\u00fcmpf'", "und", "St\u00f6\u00b7ckel\u00b7schuh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geht ins Hotel und macht frou-frou.", "tokens": ["Geht", "ins", "Ho\u00b7tel", "und", "macht", "frou\u00b7frou", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}