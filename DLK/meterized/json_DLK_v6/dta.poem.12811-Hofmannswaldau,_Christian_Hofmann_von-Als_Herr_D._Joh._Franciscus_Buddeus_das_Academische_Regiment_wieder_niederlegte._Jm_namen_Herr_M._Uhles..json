{"dta.poem.12811": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis ist der segen,", "tokens": ["Dis", "ist", "der", "se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PPOSS", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit welchem die ihr amt darnieder legen,", "tokens": ["Mit", "wel\u00b7chem", "die", "ihr", "amt", "dar\u00b7nie\u00b7der", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "PPOSAT", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die GOttes willen thun:", "tokens": ["Die", "Got\u00b7tes", "wil\u00b7len", "thun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die seine furcht zum angel-sterne setzen,", "tokens": ["Die", "sei\u00b7ne", "furcht", "zum", "an\u00b7gel\u00b7ster\u00b7ne", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mu\u00df nach abgenommner last lauter ehr und ruh ergetzen.", "tokens": ["Mu\u00df", "nach", "ab\u00b7ge\u00b7nomm\u00b7ner", "last", "lau\u00b7ter", "ehr", "und", "ruh", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "VVFIN", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}