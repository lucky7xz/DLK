{"textgrid.poem.25776": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Entt\u00e4uschung", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die junge Rose war erwacht", "tokens": ["Die", "jun\u00b7ge", "Ro\u00b7se", "war", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sehnsuchtschw\u00fcler Mondennacht.", "tokens": ["In", "sehn\u00b7sucht\u00b7schw\u00fc\u00b7ler", "Mon\u00b7den\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbbist du die Sonne! Du blaues Licht?\u00ab \u2013", "tokens": ["\u00bb", "bist", "du", "die", "Son\u00b7ne", "!", "Du", "blau\u00b7es", "Licht", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$.", "PPER", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie pre\u00dft voll Wonne ihr Bl\u00fctengesicht,", "tokens": ["Sie", "pre\u00dft", "voll", "Won\u00b7ne", "ihr", "Bl\u00fc\u00b7ten\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bebend in wogenden Qualen,", "tokens": ["Be\u00b7bend", "in", "wo\u00b7gen\u00b7den", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In die traumblassen, leblosen Strahlen.", "tokens": ["In", "die", "traum\u00b7blas\u00b7sen", ",", "leb\u00b7lo\u00b7sen", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbbist du die Sonne?\u00ab \u2013 \u2013 \u2013", "tokens": ["\u00bb", "bist", "du", "die", "Son\u00b7ne", "?", "\u00ab", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "$(", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Tag hat ihr die Sonne gebracht.", "tokens": ["Der", "Tag", "hat", "ihr", "die", "Son\u00b7ne", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Tag war zerronnen. Duftwei\u00dfe Nacht.", "tokens": ["Der", "Tag", "war", "zer\u00b7ron\u00b7nen", ".", "Duft\u00b7wei\u00b7\u00dfe", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbdas also Sonne? Empfindloses Gold?!", "tokens": ["\u00bb", "das", "al\u00b7so", "Son\u00b7ne", "?", "Emp\u00b7find\u00b7lo\u00b7ses", "Gold", "?!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mein Traum, Weichheit so schmiegsam und hold \u2013", "tokens": ["Und", "mein", "Traum", ",", "Weich\u00b7heit", "so", "schmieg\u00b7sam", "und", "hold", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "K\u00fc\u00dft, k\u00fcsset mich blaue Strahlen,", "tokens": ["K\u00fc\u00dft", ",", "k\u00fcs\u00b7set", "mich", "blau\u00b7e", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "L\u00f6schet die zehrenden Qualen,", "tokens": ["L\u00f6\u00b7schet", "die", "zeh\u00b7ren\u00b7den", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Seid Sonne, Sonne!!\u00ab \u2013", "tokens": ["Seid", "Son\u00b7ne", ",", "Son\u00b7ne", "!!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["VAIMP", "NN", "$,", "NN", "$.", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}