{"dta.poem.2923": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Irrthum bist du immer noch befangen,", "tokens": ["In", "ei\u00b7nem", "Irr\u00b7thum", "bist", "du", "im\u00b7mer", "noch", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob es gelte hier was eignes zu erlangen,", "tokens": ["Al\u00b7sob", "es", "gel\u00b7te", "hier", "was", "eig\u00b7nes", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PWS", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alsob es gelte durch Anstreben, K\u00e4mpfen, Ringen,", "tokens": ["Al\u00b7sob", "es", "gel\u00b7te", "durch", "An\u00b7stre\u00b7ben", ",", "K\u00e4mp\u00b7fen", ",", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Zu einem h\u00f6hern Werth mit Macht empor zu dringen.", "tokens": ["Zu", "ei\u00b7nem", "h\u00f6\u00b7hern", "Werth", "mit", "Macht", "em\u00b7por", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bescheide dich! hier ist nichts h\u00f6hers zu verlangen,", "tokens": ["Be\u00b7schei\u00b7de", "dich", "!", "hier", "ist", "nichts", "h\u00f6\u00b7hers", "zu", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als am Gemeinsamen Gemeinschaft zu erlangen,", "tokens": ["Als", "am", "Ge\u00b7mein\u00b7sa\u00b7men", "Ge\u00b7mein\u00b7schaft", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "An dem, was klein und gro\u00df den Menschen ist gemein,", "tokens": ["An", "dem", ",", "was", "klein", "und", "gro\u00df", "den", "Men\u00b7schen", "ist", "ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mensch zu seyn, das ist nicht gro\u00df und ist nicht klein.", "tokens": ["Ein", "Mensch", "zu", "seyn", ",", "das", "ist", "nicht", "gro\u00df", "und", "ist", "nicht", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "PDS", "VAFIN", "PTKNEG", "ADJD", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht, weil du klommest, bist du auf zu h\u00f6herm Grade,", "tokens": ["Nicht", ",", "weil", "du", "klom\u00b7mest", ",", "bist", "du", "auf", "zu", "h\u00f6\u00b7herm", "Gra\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestiegen bist du nur empor auf steilerm Pfade.", "tokens": ["Ge\u00b7stie\u00b7gen", "bist", "du", "nur", "em\u00b7por", "auf", "stei\u00b7lerm", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Begl\u00fcckt ist, der empor auf leichterem gekommen,", "tokens": ["Be\u00b7gl\u00fcckt", "ist", ",", "der", "em\u00b7por", "auf", "leich\u00b7te\u00b7rem", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PTKVZ", "APPR", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der oben ist und selbst nicht wei\u00df, da\u00df er geklommen.", "tokens": ["Der", "o\u00b7ben", "ist", "und", "selbst", "nicht", "wei\u00df", ",", "da\u00df", "er", "ge\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "KON", "ADV", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}