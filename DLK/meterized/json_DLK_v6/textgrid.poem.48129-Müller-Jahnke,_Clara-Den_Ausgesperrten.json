{"textgrid.poem.48129": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Den Ausgesperrten", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 Und hundert Tage und noch vielmehr . . .", "tokens": ["\u2013", "Und", "hun\u00b7dert", "Ta\u00b7ge", "und", "noch", "viel\u00b7mehr", ".", ".", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "CARD", "NN", "KON", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Herd ist kalt und die Lade leer.", "tokens": ["Der", "Herd", "ist", "kalt", "und", "die", "La\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Am Fest der Liebe kein Jubelton \u2013", "tokens": ["Am", "Fest", "der", "Lie\u00b7be", "kein", "Ju\u00b7bel\u00b7ton", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und die Friedensbotschaft ward Hohn, ward Hohn!", "tokens": ["und", "die", "Frie\u00b7dens\u00b7bot\u00b7schaft", "ward", "Hohn", ",", "ward", "Hohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$,", "VAFIN", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schwer h\u00e4ngt der Himmel, wie Schiefer grau,", "tokens": ["Schwer", "h\u00e4ngt", "der", "Him\u00b7mel", ",", "wie", "Schie\u00b7fer", "grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "\u00fcber den D\u00e4chern von Crimmitschau.", "tokens": ["\u00fc\u00b7ber", "den", "D\u00e4\u00b7chern", "von", "Crim\u00b7mit\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und Tausende harren, trotzig und stumm,", "tokens": ["Und", "Tau\u00b7sen\u00b7de", "har\u00b7ren", ",", "trot\u00b7zig", "und", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Feinde oben und Feinde ringsum! \u2013", "tokens": ["\u2013", "Fein\u00b7de", "o\u00b7ben", "und", "Fein\u00b7de", "ring\u00b7sum", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "ADV", "KON", "NN", "ADV", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und weint ein zitterndes Kind nach Brot,", "tokens": ["Und", "weint", "ein", "zit\u00b7tern\u00b7des", "Kind", "nach", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "so leiden sie dreifach des Krieges Not.", "tokens": ["so", "lei\u00b7den", "sie", "drei\u00b7fach", "des", "Krie\u00b7ges", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mit eherner Stirne, wie Mann so Frau,", "tokens": ["Mit", "e\u00b7her\u00b7ner", "Stir\u00b7ne", ",", "wie", "Mann", "so", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "stehen die Helden von Crimmitschau.", "tokens": ["ste\u00b7hen", "die", "Hel\u00b7den", "von", "Crim\u00b7mit\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie k\u00e4mpfen nicht mordend mit Pulver und Stahl:", "tokens": ["Sie", "k\u00e4mp\u00b7fen", "nicht", "mor\u00b7dend", "mit", "Pul\u00b7ver", "und", "Stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "sie geben ihr Herzblut in Hunger und Qual;", "tokens": ["sie", "ge\u00b7ben", "ihr", "Herz\u00b7blut", "in", "Hun\u00b7ger", "und", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "sie tragen die Fahne im heiligsten Krieg \u2013", "tokens": ["sie", "tra\u00b7gen", "die", "Fah\u00b7ne", "im", "hei\u00b7ligs\u00b7ten", "Krieg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "und die ", "tokens": ["und", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Der wandelt in bl\u00fchende Fr\u00fchlingsau", "tokens": ["Der", "wan\u00b7delt", "in", "bl\u00fc\u00b7hen\u00b7de", "Fr\u00fch\u00b7lin\u00b7gsau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "die feiernden S\u00e4le in Crimmitschau.", "tokens": ["die", "fei\u00b7ern\u00b7den", "S\u00e4\u00b7le", "in", "Crim\u00b7mit\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wir aber, ihr Braven, wie grimm das Gesicht", "tokens": ["Wir", "a\u00b7ber", ",", "ihr", "Bra\u00b7ven", ",", "wie", "grimm", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "der Zukunft euch drohe, wir ", "tokens": ["der", "Zu\u00b7kunft", "euch", "dro\u00b7he", ",", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir st\u00fctzen die Hand euch im harten Gefecht \u2013", "tokens": ["Wir", "st\u00fct\u00b7zen", "die", "Hand", "euch", "im", "har\u00b7ten", "Ge\u00b7fecht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "laut pochen die Pulse f\u00fcr Freiheit und Recht.", "tokens": ["laut", "po\u00b7chen", "die", "Pul\u00b7se", "f\u00fcr", "Frei\u00b7heit", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Millionen mit euch! \u2013 Und wie die Sonne im Blau", "tokens": ["Mil\u00b7lion\u00b7en", "mit", "euch", "!", "\u2013", "Und", "wie", "die", "Son\u00b7ne", "im", "Blau"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "$(", "KON", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "leuchtet die Weihnacht von Crimmitschau!", "tokens": ["leuch\u00b7tet", "die", "Weih\u00b7nacht", "von", "Crim\u00b7mit\u00b7schau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}