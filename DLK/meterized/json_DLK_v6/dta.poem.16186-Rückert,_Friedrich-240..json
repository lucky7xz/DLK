{"dta.poem.16186": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "240.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch jede Zeit hat umgeformt nach ihrem Brauch", "tokens": ["Noch", "je\u00b7de", "Zeit", "hat", "um\u00b7ge\u00b7formt", "nach", "ih\u00b7rem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weisheit alter Zeit, und so thun wir es auch.", "tokens": ["Die", "Weis\u00b7heit", "al\u00b7ter", "Zeit", ",", "und", "so", "thun", "wir", "es", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir nehmen sie nur an, wie wir sie brauchen k\u00f6nnen,", "tokens": ["Wir", "neh\u00b7men", "sie", "nur", "an", ",", "wie", "wir", "sie", "brau\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcgen mu\u00df sie sich, wo wir den Platz ihr g\u00f6nnen.", "tokens": ["Und", "f\u00fc\u00b7gen", "mu\u00df", "sie", "sich", ",", "wo", "wir", "den", "Platz", "ihr", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nimmt sie sich minder aus im Haus, als einst im Zelte,", "tokens": ["Nimmt", "sie", "sich", "min\u00b7der", "aus", "im", "Haus", ",", "als", "einst", "im", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "APPRART", "NN", "$,", "KOUS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist es besser, da\u00df sie so, als gar nicht, gelte.", "tokens": ["Doch", "ist", "es", "bes\u00b7ser", ",", "da\u00df", "sie", "so", ",", "als", "gar", "nicht", ",", "gel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "$,", "KOUS", "ADV", "PTKNEG", "$,", "VVFIN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}