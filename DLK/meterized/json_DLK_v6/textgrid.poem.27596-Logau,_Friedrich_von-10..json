{"textgrid.poem.27596": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mit Bacchus k\u00e4mpffen wil,", "tokens": ["Wer", "mit", "Bac\u00b7chus", "k\u00e4mpf\u00b7fen", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcte sich und trau nicht viel.", "tokens": ["H\u00fc\u00b7te", "sich", "und", "trau", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erstlich schl\u00e4gt er auff die Beine;", "tokens": ["Erst\u00b7lich", "schl\u00e4gt", "er", "auff", "die", "Bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft er dich, so bist du seine.", "tokens": ["Trifft", "er", "dich", ",", "so", "bist", "du", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}