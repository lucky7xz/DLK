{"textgrid.poem.53397": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Thu die Begierden in den Bann,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thu die Begierden in den Bann,", "tokens": ["Thu", "die", "Be\u00b7gier\u00b7den", "in", "den", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vnd sey der S\u00fcnden Obermann,", "tokens": ["Vnd", "sey", "der", "S\u00fcn\u00b7den", "O\u00b7ber\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib Gottes, frey, dein eigen,", "tokens": ["Bleib", "Got\u00b7tes", ",", "frey", ",", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hast du h\u00f6her es gebracht", "tokens": ["So", "hast", "du", "h\u00f6\u00b7her", "es", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als m\u00fcsten sich vor deiner Macht", "tokens": ["Als", "m\u00fcs\u00b7ten", "sich", "vor", "dei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ost, Nord und Abend neigen.", "tokens": ["Ost", ",", "Nord", "und", "A\u00b7bend", "nei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was herrscht ein K\u00f6nig weit umbher,", "tokens": ["Was", "herrscht", "ein", "K\u00f6\u00b7nig", "weit", "um\u00b7bher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Reich der Erden, und das Meer", "tokens": ["Das", "Reich", "der", "Er\u00b7den", ",", "und", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist seinem Raht zu wenig,", "tokens": ["Ist", "sei\u00b7nem", "Raht", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist er der schn\u00f6den L\u00fcste Knecht?", "tokens": ["Ist", "er", "der", "schn\u00f6\u00b7den", "L\u00fcs\u00b7te", "Knecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Herr seyn, dieses ist erst recht", "tokens": ["Ihr", "Herr", "seyn", ",", "die\u00b7ses", "ist", "erst", "recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAINF", "$,", "PDS", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein F\u00fcrst seyn oder K\u00f6nig.", "tokens": ["Ein", "F\u00fcrst", "seyn", "o\u00b7der", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Liebt ihr Gewaltigen Verstand,", "tokens": ["Liebt", "ihr", "Ge\u00b7wal\u00b7ti\u00b7gen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Habt ewren Zorn stets in der Hand,", "tokens": ["Habt", "ew\u00b7ren", "Zorn", "stets", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasst keine Lust euch treiben", "tokens": ["Lasst", "kei\u00b7ne", "Lust", "euch", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ohn die Gesetz und Urtheil f\u00fchrt,", "tokens": ["Ohn", "die", "Ge\u00b7setz", "und", "Ur\u00b7theil", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann lasst euch K\u00f6nig', als geb\u00fcrt,", "tokens": ["Dann", "lasst", "euch", "K\u00f6\u00b7nig'", ",", "als", "ge\u00b7b\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was ihr seyn wollt, schreiben.", "tokens": ["Und", "was", "ihr", "seyn", "wollt", ",", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr seyd es einem falschen Wahn,", "tokens": ["Ihr", "seyd", "es", "ei\u00b7nem", "fal\u00b7schen", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Todes-Furcht seyn unterthan,", "tokens": ["Der", "To\u00b7des\u00b7Furcht", "seyn", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt dies jetzt das begehren,", "tokens": ["Jetzt", "dies", "jetzt", "das", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beweglich seyn den Wellen gleich", "tokens": ["Be\u00b7weg\u00b7lich", "seyn", "den", "Wel\u00b7len", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAINF", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heisst dienen, m\u00fcst' auch alles Reich", "tokens": ["Heisst", "die\u00b7nen", ",", "m\u00fcst'", "auch", "al\u00b7les", "Reich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch Huldigung gewehren.", "tokens": ["Euch", "Hul\u00b7di\u00b7gung", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Wer aber macht uns also frey", "tokens": ["Wer", "a\u00b7ber", "macht", "uns", "al\u00b7so", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von der Begierden Tyranney?", "tokens": ["Von", "der", "Be\u00b7gier\u00b7den", "Ty\u00b7ran\u00b7ney", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kan Zeno es verrichten", "tokens": ["Kan", "Ze\u00b7no", "es", "ver\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NE", "PPER", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sampt andern die hievon gelehrt?", "tokens": ["Sampt", "an\u00b7dern", "die", "hie\u00b7von", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd ob man Plato, ob man h\u00f6rt", "tokens": ["Vnd", "ob", "man", "Pla\u00b7to", ",", "ob", "man", "h\u00f6rt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "NE", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich, Socrates? mit nichten,", "tokens": ["Dich", ",", "So\u00b7cra\u00b7tes", "?", "mit", "nich\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "APPR", "PIS", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Wie hochgewitzt sie sonst auch sind,", "tokens": ["Wie", "hoch\u00b7ge\u00b7witzt", "sie", "sonst", "auch", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hie sind sie ungeschickt und blind,", "tokens": ["Hie", "sind", "sie", "un\u00b7ge\u00b7schickt", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kan Moses uns erretten?", "tokens": ["Kan", "Mo\u00b7ses", "uns", "er\u00b7ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach nein, Er zwingt nur, flucht und dreut", "tokens": ["Ach", "nein", ",", "Er", "zwingt", "nur", ",", "flucht", "und", "dreut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schleusst uns in die Dienstbarkeit", "tokens": ["Vnd", "schleusst", "uns", "in", "die", "Dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einer ewign Ketten.", "tokens": ["Mit", "ei\u00b7ner", "e\u00b7wign", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Freyheit ist des Himmels Gut,", "tokens": ["Die", "Frey\u00b7heit", "ist", "des", "Him\u00b7mels", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott hat sie durch sein eignes Blut", "tokens": ["Gott", "hat", "sie", "durch", "sein", "eig\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehr thewer uns erworben,", "tokens": ["Sehr", "the\u00b7wer", "uns", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer das im Glauben fassen kan", "tokens": ["Wer", "das", "im", "Glau\u00b7ben", "fas\u00b7sen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist alsobald, ein freyer Mann,", "tokens": ["Ist", "al\u00b7so\u00b7bald", ",", "ein", "frey\u00b7er", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der S\u00fcnden abgestorben.", "tokens": ["Der", "S\u00fcn\u00b7den", "ab\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er giebt der schn\u00f6den Lust nicht raum,", "tokens": ["Er", "giebt", "der", "schn\u00f6\u00b7den", "Lust", "nicht", "raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt die Begierden streng' im Zaum,", "tokens": ["H\u00e4lt", "die", "Be\u00b7gier\u00b7den", "streng'", "im", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sucht einig Gott zu leben,", "tokens": ["Sucht", "ei\u00b7nig", "Gott", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Scham der Laster Joch zu ziehn,", "tokens": ["Hat", "Scham", "der", "Las\u00b7ter", "Joch", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem sein Heyland sich f\u00fcr ihn", "tokens": ["Nach\u00b7dem", "sein", "Hey\u00b7land", "sich", "f\u00fcr", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst in den Tod gegeben.", "tokens": ["Selbst", "in", "den", "Tod", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wer solcher Art ist fey gemacht", "tokens": ["Wer", "sol\u00b7cher", "Art", "ist", "fey", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt unter sich der Erden Pracht,", "tokens": ["Tritt", "un\u00b7ter", "sich", "der", "Er\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd kan nichts h\u00f6hers werben,", "tokens": ["Vnd", "kan", "nichts", "h\u00f6\u00b7hers", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist hie schon Gottes Engeln gleich", "tokens": ["Ist", "hie", "schon", "Got\u00b7tes", "En\u00b7geln", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd hat ein ewigs K\u00f6nigreich", "tokens": ["Vnd", "hat", "ein", "e\u00b7wigs", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd Priesterthum zu erben.", "tokens": ["Vnd", "Pries\u00b7ter\u00b7thum", "zu", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vnd st\u00fcrmt dies Leben zu ihm ein", "tokens": ["Vnd", "st\u00fcrmt", "dies", "Le\u00b7ben", "zu", "ihm", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "NN", "APPR", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Armut und mit Kranckheit Pein,", "tokens": ["Mit", "Ar\u00b7mut", "und", "mit", "Kran\u00b7ck\u00b7heit", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gedult macht ihn bescheiden,", "tokens": ["Ge\u00b7dult", "macht", "ihn", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tr\u00e4gt sein Creutz getrost und still,", "tokens": ["Er", "tr\u