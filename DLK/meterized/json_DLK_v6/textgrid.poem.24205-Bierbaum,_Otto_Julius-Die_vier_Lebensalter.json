{"textgrid.poem.24205": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die vier Lebensalter", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lag ein Kind unter einem Baum", "tokens": ["Es", "lag", "ein", "Kind", "un\u00b7ter", "ei\u00b7nem", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sah hinauf in den gr\u00fcnen Raum", "tokens": ["Und", "sah", "hin\u00b7auf", "in", "den", "gr\u00fc\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und l\u00e4chelte dazu.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sprach ich: H\u00f6r, du,", "tokens": ["Sprach", "ich", ":", "H\u00f6r", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "$,", "PPER", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Was tust du so?", "tokens": ["Was", "tust", "du", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sprach es: Ei, Mann, ich bin so froh,", "tokens": ["Sprach", "es", ":", "Ei", ",", "Mann", ",", "ich", "bin", "so", "froh", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "$,", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Weil da die V\u00f6glein singen,", "tokens": ["Weil", "da", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Zweig zu Zweige springen.", "tokens": ["Von", "Zweig", "zu", "Zwei\u00b7ge", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ist das nicht lustig? Ach, k\u00f6nnt ich hinein", "tokens": ["Ist", "das", "nicht", "lus\u00b7tig", "?", "Ach", ",", "k\u00f6nnt", "ich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$.", "ITJ", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In die gr\u00fcne Welt; doch ich bin noch zu klein.", "tokens": ["In", "die", "gr\u00fc\u00b7ne", "Welt", ";", "doch", "ich", "bin", "noch", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Meine Arme umfassen den Stamm noch nicht;", "tokens": ["Mei\u00b7ne", "Ar\u00b7me", "um\u00b7fas\u00b7sen", "den", "Stamm", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Sonst wollte ich bald oben sein", "tokens": ["Sonst", "woll\u00b7te", "ich", "bald", "o\u00b7ben", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bei den V\u00f6geln im gr\u00fcnen Licht.", "tokens": ["Bei", "den", "V\u00f6\u00b7geln", "im", "gr\u00fc\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Es stand ein J\u00fcngling unter einem Baum,", "tokens": ["Es", "stand", "ein", "J\u00fcng\u00b7ling", "un\u00b7ter", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seiner Lippe der erste Flaum,", "tokens": ["Auf", "sei\u00b7ner", "Lip\u00b7pe", "der", "ers\u00b7te", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In seinen Augen die erste Liebe.", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "die", "ers\u00b7te", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er schnitt ein Herz in die Rinde ein,", "tokens": ["Er", "schnitt", "ein", "Herz", "in", "die", "Rin\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das sollte Denkmal und Rahmen sein,", "tokens": ["Das", "soll\u00b7te", "Denk\u00b7mal", "und", "Rah\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Darein er ihren Namen schriebe.", "tokens": ["Da\u00b7rein", "er", "ih\u00b7ren", "Na\u00b7men", "schrie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4chelte dazu.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sprach ich: H\u00f6r, du,", "tokens": ["Sprach", "ich", ":", "H\u00f6r", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "$,", "PPER", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Was tust du so?", "tokens": ["Was", "tust", "du", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Sprach er: Mich macht die Liebe froh.", "tokens": ["Sprach", "er", ":", "Mich", "macht", "die", "Lie\u00b7be", "froh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das wachsende Leben soll umschlie\u00dfen", "tokens": ["Das", "wach\u00b7sen\u00b7de", "Le\u00b7ben", "soll", "um\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Den Namen, der mein Herz umschlie\u00dft.", "tokens": ["Den", "Na\u00b7men", ",", "der", "mein", "Herz", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Des Lebens S\u00e4fte sollen ihn durchflie\u00dfen,", "tokens": ["Des", "Le\u00b7bens", "S\u00e4f\u00b7te", "sol\u00b7len", "ihn", "durch\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der wie ein Gl\u00fccksschwall mich durchflie\u00dft.", "tokens": ["Der", "wie", "ein", "Gl\u00fccks\u00b7schwall", "mich", "durch\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dies soll ein Sinnbild meiner Liebe sein:", "tokens": ["Dies", "soll", "ein", "Sinn\u00b7bild", "mei\u00b7ner", "Lie\u00b7be", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Stark wie der Baum ist sie und also rein", "tokens": ["Stark", "wie", "der", "Baum", "ist", "sie", "und", "al\u00b7so", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VAFIN", "PPER", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wie seine S\u00e4fte, die in tausend Bl\u00fcten", "tokens": ["Wie", "sei\u00b7ne", "S\u00e4f\u00b7te", ",", "die", "in", "tau\u00b7send", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PRELS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "In jedem Jahre neu sich offenbaren;", "tokens": ["In", "je\u00b7dem", "Jah\u00b7re", "neu", "sich", "of\u00b7fen\u00b7ba\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Von jungen Jahren bis zu alten Jahren", "tokens": ["Von", "jun\u00b7gen", "Jah\u00b7ren", "bis", "zu", "al\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Will ich als Heiligtum sie selig h\u00fcten.", "tokens": ["Will", "ich", "als", "Hei\u00b7lig\u00b7tum", "sie", "se\u00b7lig", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es lag ein Mann unter einem Baum,", "tokens": ["Es", "lag", "ein", "Mann", "un\u00b7ter", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verschr\u00e4nkte die Arme wie im Traum", "tokens": ["Ver\u00b7schr\u00e4nk\u00b7te", "die", "Ar\u00b7me", "wie", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unter dem Haupte und sah hinan,", "tokens": ["Un\u00b7ter", "dem", "Haup\u00b7te", "und", "sah", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo viele V\u00f6gel sangen;", "tokens": ["Wo", "vie\u00b7le", "V\u00f6\u00b7gel", "san\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ruhig, ohne Verlangen sah in das Gr\u00fcne der Mann", "tokens": ["Ru\u00b7hig", ",", "oh\u00b7ne", "Ver\u00b7lan\u00b7gen", "sah", "in", "das", "Gr\u00fc\u00b7ne", "der", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUI", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und l\u00e4chelte dazu.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sprach ich: H\u00f6r, du,", "tokens": ["Sprach", "ich", ":", "H\u00f6r", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "$,", "PPER", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Was tust du so?", "tokens": ["Was", "tust", "du", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sprach er: Ich bin der Ruhe froh,", "tokens": ["Sprach", "er", ":", "Ich", "bin", "der", "Ru\u00b7he", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und da\u00df ich mich von einem Schmerz ermannte,", "tokens": ["Und", "da\u00df", "ich", "mich", "von", "ei\u00b7nem", "Schmerz", "er\u00b7mann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Den ich zuletzt doch als ein Gl\u00fcck erkannte.", "tokens": ["Den", "ich", "zu\u00b7letzt", "doch", "als", "ein", "Gl\u00fcck", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich war zu lang", "tokens": ["Ich", "war", "zu", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Im \u00dcberschwang,", "tokens": ["Im", "\u00dc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.14": {"text": "Vertaumelte mein Leben", "tokens": ["Ver\u00b7tau\u00b7mel\u00b7te", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In eines Traumes Schweben", "tokens": ["In", "ei\u00b7nes", "Trau\u00b7mes", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und wurde so des Lebens bar,", "tokens": ["Und", "wur\u00b7de", "so", "des", "Le\u00b7bens", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Unfest, unklar,", "tokens": ["Un\u00b7fest", ",", "un\u00b7klar", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Bis mir der Schmerz beschiedn war,", "tokens": ["Bis", "mir", "der", "Schmerz", "be\u00b7schiedn", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Der mich zur Erde m\u00e4chtig stie\u00df", "tokens": ["Der", "mich", "zur", "Er\u00b7de", "m\u00e4ch\u00b7tig", "stie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und mich den Sinn der Erde,", "tokens": ["Und", "mich", "den", "Sinn", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Das ewige Sei und Werde", "tokens": ["Das", "e\u00b7wi\u00b7ge", "Sei", "und", "Wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Dankbar erkennen lie\u00df.", "tokens": ["Dank\u00b7bar", "er\u00b7ken\u00b7nen", "lie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "Ich wei\u00df es nun:", "tokens": ["Ich", "wei\u00df", "es", "nun", ":"], "t