{"textgrid.poem.25740": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was du noch itzt nicht tr\u00e4umst, das wird geschehen:", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du noch itzt nicht tr\u00e4umst, das wird geschehen:", "tokens": ["Was", "du", "noch", "itzt", "nicht", "tr\u00e4umst", ",", "das", "wird", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst dereinst den J\u00fcngling sehen,", "tokens": ["Du", "wirst", "de\u00b7reinst", "den", "J\u00fcng\u00b7ling", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei dem dein Herz zum erstenmal", "tokens": ["Bei", "dem", "dein", "Herz", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdoppeln wird die Schl\u00e4ge, deine Wange", "tokens": ["Ver\u00b7dop\u00b7peln", "wird", "die", "Schl\u00e4\u00b7ge", ",", "dei\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verr\u00e4therisch wird gl\u00fchn, und bange", "tokens": ["Ver\u00b7r\u00e4\u00b7the\u00b7risch", "wird", "gl\u00fchn", ",", "und", "ban\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "VVINF", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Munde seyn, um seiner Worte Wahl.", "tokens": ["Dem", "Mun\u00b7de", "seyn", ",", "um", "sei\u00b7ner", "Wor\u00b7te", "Wahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,", "KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O! wenn du dann zum erstenmal im Bette", "tokens": ["O", "!", "wenn", "du", "dann", "zum", "ers\u00b7ten\u00b7mal", "im", "Bet\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "APPRART", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das L\u00e4uten in die Morgenmette,", "tokens": ["Das", "L\u00e4u\u00b7ten", "in", "die", "Mor\u00b7gen\u00b7met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des W\u00e4chters Tagverk\u00fcnden h\u00f6rst:", "tokens": ["Des", "W\u00e4ch\u00b7ters", "Tag\u00b7ver\u00b7k\u00fcn\u00b7den", "h\u00f6rst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So denk an mich! Denn Worte, die itzt keinen", "tokens": ["So", "denk", "an", "mich", "!", "Denn", "Wor\u00b7te", ",", "die", "itzt", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$.", "KON", "NN", "$,", "PRELS", "ADV", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Verstand f\u00fcr dich zu haben scheinen,", "tokens": ["Ver\u00b7stand", "f\u00fcr", "dich", "zu", "ha\u00b7ben", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verstehest du in solchen Stunden erst.", "tokens": ["Ver\u00b7ste\u00b7hest", "du", "in", "sol\u00b7chen", "Stun\u00b7den", "erst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So wisse denn: du liebest! Zwar zu lachen", "tokens": ["So", "wis\u00b7se", "denn", ":", "du", "lie\u00b7best", "!", "Zwar", "zu", "la\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "$.", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wird heute dich der Ausspruch machen,", "tokens": ["Wird", "heu\u00b7te", "dich", "der", "Aus\u00b7spruch", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Allein zu weinen sicher auch einmal.", "tokens": ["Al\u00b7lein", "zu", "wei\u00b7nen", "si\u00b7cher", "auch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Doch kosten soll er dir nur s\u00fc\u00dfe Z\u00e4hren,", "tokens": ["Doch", "kos\u00b7ten", "soll", "er", "dir", "nur", "s\u00fc\u00b7\u00dfe", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wirst du dein Herz nur dem gew\u00e4hren,", "tokens": ["Wirst", "du", "dein", "Herz", "nur", "dem", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der deine Achtung nicht durch R\u00e4nke stahl.", "tokens": ["Der", "dei\u00b7ne", "Ach\u00b7tung", "nicht", "durch", "R\u00e4n\u00b7ke", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wer seine Hand nicht gleich am Traualtare", "tokens": ["Wer", "sei\u00b7ne", "Hand", "nicht", "gleich", "am", "Trau\u00b7al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Dir bieten kann, vor dem bewahre", "tokens": ["Dir", "bie\u00b7ten", "kann", ",", "vor", "dem", "be\u00b7wah\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Dein Herz, und w\u00e4r' er ", "tokens": ["Dein", "Herz", ",", "und", "w\u00e4r'", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Ja, wenn auch du selbst ", "tokens": ["Ja", ",", "wenn", "auch", "du", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Und nie vom Pfad' der Tugend weichest:", "tokens": ["Und", "nie", "vom", "Pfad'", "der", "Tu\u00b7gend", "wei\u00b7chest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Ruhe fliegt doch allemal davon.", "tokens": ["Die", "Ru\u00b7he", "fliegt", "doch", "al\u00b7le\u00b7mal", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Es gibt vielleicht f\u00fcr \u00e4chte Menschenkenner", "tokens": ["Es", "gibt", "viel\u00b7leicht", "f\u00fcr", "\u00e4ch\u00b7te", "Men\u00b7schen\u00b7ken\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Nicht Einen Grandison, doch M\u00e4nner,", "tokens": ["Nicht", "Ei\u00b7nen", "Gran\u00b7di\u00b7son", ",", "doch", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die keinen H\u00e4ndedruck erlaubt", "tokens": ["Die", "kei\u00b7nen", "H\u00e4n\u00b7de\u00b7druck", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sich halten, lieber schweigend sich verzehren;", "tokens": ["Sich", "hal\u00b7ten", ",", "lie\u00b7ber", "schwei\u00b7gend", "sich", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Doch wirst du nur dein Gl\u00fcck vermehren,", "tokens": ["Doch", "wirst", "du", "nur", "dein", "Gl\u00fcck", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Wenn nie dein Herz an M\u00e4nnertugend glaubt.", "tokens": ["Wenn", "nie", "dein", "Herz", "an", "M\u00e4n\u00b7ner\u00b7tu\u00b7gend", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wenn aber einst, durch seine sch\u00f6ne Seele", "tokens": ["Wenn", "a\u00b7ber", "einst", ",", "durch", "sei\u00b7ne", "sch\u00f6\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein junger Mann dein Herz dir st\u00f6hle,", "tokens": ["Ein", "jun\u00b7ger", "Mann", "dein", "Herz", "dir", "st\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und du \u2013 f\u00fcr deinen Werth, ein klein", "tokens": ["Und", "du", "\u2013", "f\u00fcr", "dei\u00b7nen", "Werth", ",", "ein", "klein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$(", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Geschenk, so gro\u00df es ist! \u2013 die Hand ihm raubest:", "tokens": ["Ge\u00b7schenk", ",", "so", "gro\u00df", "es", "ist", "!", "\u2013", "die", "Hand", "ihm", "rau\u00b7best", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$.", "$(", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Je tugendhafter du ihn glaubest,", "tokens": ["Je", "tu\u00b7gend\u00b7haf\u00b7ter", "du", "ihn", "glau\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Je gl\u00fccklicher wirst du als Gattin seyn!", "tokens": ["Je", "gl\u00fcck\u00b7li\u00b7cher", "wirst", "du", "als", "Gat\u00b7tin", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}