{"textgrid.poem.64468": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "90. Gellerts hinterlassene Werke", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er lehrte drey\u00dfig Jahr die Sch\u00f6nen Witz und Tugend,", "tokens": ["Er", "lehr\u00b7te", "drey\u00b7\u00dfig", "Jahr", "die", "Sch\u00f6\u00b7nen", "Witz", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vernehmt auch, welchen Dank ihm eine Freundinn gab:", "tokens": ["Ver\u00b7nehmt", "auch", ",", "wel\u00b7chen", "Dank", "ihm", "ei\u00b7ne", "Freun\u00b7dinn", "gab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Getreu verwahrte sie die Fehler seiner Jugend,", "tokens": ["Ge\u00b7treu", "ver\u00b7wahr\u00b7te", "sie", "die", "Feh\u00b7ler", "sei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und setzte sie dem guten Mann aufs Grab.", "tokens": ["Und", "setz\u00b7te", "sie", "dem", "gu\u00b7ten", "Mann", "aufs", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}