{"textgrid.poem.59227": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fccksritter", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Fortuna spr\u00f6de tut,", "tokens": ["Wenn", "For\u00b7tu\u00b7na", "spr\u00f6\u00b7de", "tut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "La\u00df ich sie in Ruh,", "tokens": ["La\u00df", "ich", "sie", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Singe recht und trinke gut,", "tokens": ["Sin\u00b7ge", "recht", "und", "trin\u00b7ke", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Fortuna kriegt auch Mut,", "tokens": ["Und", "For\u00b7tu\u00b7na", "kriegt", "auch", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Setzt sich mit dazu.", "tokens": ["Setzt", "sich", "mit", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch ich geb mir keine M\u00fch:", "tokens": ["Doch", "ich", "geb", "mir", "kei\u00b7ne", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhe, noch eine her!\u00ab", "tokens": ["\u00bb", "he", ",", "noch", "ei\u00b7ne", "her", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "ADV", "ART", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kehr den R\u00fccken gegen sie,", "tokens": ["Kehr", "den", "R\u00fc\u00b7cken", "ge\u00b7gen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df hoch leben die und die \u2013", "tokens": ["La\u00df", "hoch", "le\u00b7ben", "die", "und", "die", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "VVFIN", "ART", "KON", "ART", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das verdrie\u00dft sie sehr.", "tokens": ["Das", "ver\u00b7drie\u00dft", "sie", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und bald r\u00fcckt sie sacht zu mir:", "tokens": ["Und", "bald", "r\u00fcckt", "sie", "sacht", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhast du deren mehr?\u00ab", "tokens": ["\u00bb", "hast", "du", "de\u00b7ren", "mehr", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "PDS", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie Sie sehn. \u2013 \u00bbDrei Kannen schier,", "tokens": ["Wie", "Sie", "sehn", ".", "\u2013", "\u00bb", "Drei", "Kan\u00b7nen", "schier", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$.", "$(", "$(", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das lauter Klebebier!\u00ab \u2013", "tokens": ["Und", "das", "lau\u00b7ter", "Kle\u00b7be\u00b7bier", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "'s wird mir gar nicht schwer.", "tokens": ["'s", "wird", "mir", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drauf sie zu mir l\u00e4chelt fein:", "tokens": ["Drauf", "sie", "zu", "mir", "l\u00e4\u00b7chelt", "fein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbbist ein ganzer Kerl!\u00ab", "tokens": ["\u00bb", "bist", "ein", "gan\u00b7zer", "Kerl", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ruft den Kellner, schreit nach Wein,", "tokens": ["Ruft", "den", "Kell\u00b7ner", ",", "schreit", "nach", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinkt mir zu und schenkt mir ein,", "tokens": ["Trinkt", "mir", "zu", "und", "schenkt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Echte Blum und Perl.", "tokens": ["Ech\u00b7te", "Blum", "und", "Perl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sie bezahlet Wein und Bier,", "tokens": ["Sie", "be\u00b7zah\u00b7let", "Wein", "und", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich, wieder gut,", "tokens": ["Und", "ich", ",", "wie\u00b7der", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchre sie am Arm mit mir", "tokens": ["F\u00fch\u00b7re", "sie", "am", "Arm", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Haus, wie 'n Kavalier,", "tokens": ["Aus", "dem", "Haus", ",", "wie", "'n", "Ka\u00b7va\u00b7lier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Alles zieht den Hut.", "tokens": ["Al\u00b7les", "zieht", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}