{"textgrid.poem.61210": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Holger D\u00e4nen klingt mir's in den Sinnen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Holger D\u00e4nen klingt mir's in den Sinnen", "tokens": ["Vom", "Hol\u00b7ger", "D\u00e4\u00b7nen", "klingt", "mir's", "in", "den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von Morgand, der K\u00f6nigin der Feien,", "tokens": ["Und", "von", "Mor\u00b7gand", ",", "der", "K\u00f6\u00b7ni\u00b7gin", "der", "Fei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die stete Jugend ihm lie\u00df angedeihen,", "tokens": ["Die", "ste\u00b7te", "Ju\u00b7gend", "ihm", "lie\u00df", "an\u00b7ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn in des Meers Kristallpalast zu minnen.", "tokens": ["Ihn", "in", "des", "Meers", "Kris\u00b7tall\u00b7pa\u00b7last", "zu", "min\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er aber floh mit schnellem Schiff von hinnen,", "tokens": ["Er", "a\u00b7ber", "floh", "mit", "schnel\u00b7lem", "Schiff", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Land ein rosig K\u00f6nigskind zu freien;", "tokens": ["Am", "Land", "ein", "ro\u00b7sig", "K\u00f6\u00b7nigs\u00b7kind", "zu", "frei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da brach der Zauber, und er stand im Reihen,", "tokens": ["Da", "brach", "der", "Zau\u00b7ber", ",", "und", "er", "stand", "im", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Goldhaar greis, sein Purpur Bettlerlinnen.", "tokens": ["Sein", "Gold\u00b7haar", "greis", ",", "sein", "Pur\u00b7pur", "Bett\u00b7ler\u00b7lin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die alte Sage will dein Bild dir zeigen,", "tokens": ["Die", "al\u00b7te", "Sa\u00b7ge", "will", "dein", "Bild", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O D\u00e4nemark, doch glaubst du keiner Sage,", "tokens": ["O", "D\u00e4\u00b7ne\u00b7mark", ",", "doch", "glaubst", "du", "kei\u00b7ner", "Sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da du die deutsche Maid begehrst zu eigen.", "tokens": ["Da", "du", "die", "deut\u00b7sche", "Maid", "be\u00b7gehrst", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohlauf denn, Holger, auf zum Brautgelage,", "tokens": ["Wohl\u00b7auf", "denn", ",", "Hol\u00b7ger", ",", "auf", "zum", "Braut\u00b7ge\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Hochzeitstanz, wo Schwerter sind die Geigen,", "tokens": ["Zum", "Hoch\u00b7zeit\u00b7stanz", ",", "wo", "Schwer\u00b7ter", "sind", "die", "Gei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df deine ganze Bl\u00f6\u00dfe kommt zutage!", "tokens": ["Da\u00df", "dei\u00b7ne", "gan\u00b7ze", "Bl\u00f6\u00b7\u00dfe", "kommt", "zu\u00b7ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}