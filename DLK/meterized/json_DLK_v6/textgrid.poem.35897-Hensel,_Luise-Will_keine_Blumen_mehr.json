{"textgrid.poem.35897": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Will keine Blumen mehr", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sommerrosen bl\u00fchen", "tokens": ["Die", "Som\u00b7mer\u00b7ro\u00b7sen", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und duften um mich her;", "tokens": ["Und", "duf\u00b7ten", "um", "mich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' sie all' vergl\u00fchen,", "tokens": ["Ich", "seh'", "sie", "all'", "ver\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will keine Blumen mehr.", "tokens": ["Will", "kei\u00b7ne", "Blu\u00b7men", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Bruder mein that ziehen", "tokens": ["Der", "Bru\u00b7der", "mein", "that", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit K\u00f6nigs stolzem Heer,", "tokens": ["Mit", "K\u00f6\u00b7nigs", "stol\u00b7zem", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft einsam mich verbl\u00fchen,", "tokens": ["L\u00e4\u00dft", "ein\u00b7sam", "mich", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will keine Blumen mehr.", "tokens": ["Will", "kei\u00b7ne", "Blu\u00b7men", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die blanken Waffen spr\u00fchen", "tokens": ["Die", "blan\u00b7ken", "Waf\u00b7fen", "spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit Funken um ihn her;", "tokens": ["Weit", "Fun\u00b7ken", "um", "ihn", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz thut ihm ergl\u00fchen,", "tokens": ["Das", "Herz", "thut", "ihm", "er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will keine Blumen mehr.", "tokens": ["Will", "kei\u00b7ne", "Blu\u00b7men", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Silbersterne bl\u00fchen", "tokens": ["Und", "Sil\u00b7bers\u00b7ter\u00b7ne", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Helm und Brustschild her,", "tokens": ["Um", "Helm", "und", "Brust\u00b7schild", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die blitzend ihn umziehen,", "tokens": ["Die", "blit\u00b7zend", "ihn", "um\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will keine Blumen mehr.", "tokens": ["Will", "kei\u00b7ne", "Blu\u00b7men", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Sommerrosen gl\u00fchen", "tokens": ["Die", "Som\u00b7mer\u00b7ro\u00b7sen", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und duften all' so sehr;", "tokens": ["Und", "duf\u00b7ten", "all'", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' sie all' verbl\u00fchen,", "tokens": ["Ich", "seh'", "sie", "all'", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will keine Blumen mehr.", "tokens": ["Will", "kei\u00b7ne", "Blu\u00b7men", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}