{"textgrid.poem.25070": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In den Apenninen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter gr\u00fcner Eichen Aesten", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7ner", "Ei\u00b7chen", "A\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und der Pinien dunklen Kronen,", "tokens": ["Und", "der", "Pi\u00b7ni\u00b7en", "dunk\u00b7len", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In den ewigen Pal\u00e4sten", "tokens": ["In", "den", "e\u00b7wi\u00b7gen", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Natur hier la\u00df uns wohnen", "tokens": ["Der", "Na\u00b7tur", "hier", "la\u00df", "uns", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, wo zwischen Lorbeerrosen,", "tokens": ["Und", ",", "wo", "zwi\u00b7schen", "Lor\u00b7beer\u00b7ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zwischen wilden Erdbeerb\u00e4umen", "tokens": ["Zwi\u00b7schen", "wil\u00b7den", "Erd\u00b7beer\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Thalhinab die B\u00e4che tosen,", "tokens": ["Thal\u00b7hi\u00b7nab", "die", "B\u00e4\u00b7che", "to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einsam, weltvergessen tr\u00e4umen.", "tokens": ["Ein\u00b7sam", ",", "welt\u00b7ver\u00b7ges\u00b7sen", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen Kranz von Lotos schlingen", "tokens": ["Ei\u00b7nen", "Kranz", "von", "Lo\u00b7tos", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen wir in unsre Locken,", "tokens": ["Wol\u00b7len", "wir", "in", "uns\u00b7re", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ums Haupt uns duftend klingen", "tokens": ["Und", "ums", "Haupt", "uns", "duf\u00b7tend", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen seine Bl\u00fctenglocken,", "tokens": ["Sol\u00b7len", "sei\u00b7ne", "Bl\u00fc\u00b7ten\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4hrend beim Gesumm der Bienen,", "tokens": ["W\u00e4h\u00b7rend", "beim", "Ge\u00b7summ", "der", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei dem Schall der Hirtenpfeifen,", "tokens": ["Bei", "dem", "Schall", "der", "Hir\u00b7ten\u00b7pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir der d\u00fcstern Apenninen", "tokens": ["Wir", "der", "d\u00fcs\u00b7tern", "A\u00b7pen\u00b7ni\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Felsenwildnisse durchstreifen.", "tokens": ["Fel\u00b7sen\u00b7wild\u00b7nis\u00b7se", "durch\u00b7strei\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald der Wipfel m\u00e4cht'gem Brausen", "tokens": ["Bald", "der", "Wip\u00b7fel", "m\u00e4cht'\u00b7gem", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem Lispeln, all dem Regen", "tokens": ["Und", "dem", "Lis\u00b7peln", ",", "all", "dem", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauschen wir, bald in den Pausen", "tokens": ["Lau\u00b7schen", "wir", ",", "bald", "in", "den", "Pau\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Unsrer eignen Herzen Schl\u00e4gen,", "tokens": ["Uns\u00b7rer", "eig\u00b7nen", "Her\u00b7zen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit hohem Klopfen sollen", "tokens": ["Und", "mit", "ho\u00b7hem", "Klop\u00b7fen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie einander Kunde geben,", "tokens": ["Sie", "ein\u00b7an\u00b7der", "Kun\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie wir, f\u00fcr die Welt verschollen,", "tokens": ["Wie", "wir", ",", "f\u00fcr", "die", "Welt", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer nur dem andern leben.", "tokens": ["Ei\u00b7ner", "nur", "dem", "an\u00b7dern", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}