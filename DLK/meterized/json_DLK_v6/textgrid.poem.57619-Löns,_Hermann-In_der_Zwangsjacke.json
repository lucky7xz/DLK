{"textgrid.poem.57619": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "In der Zwangsjacke", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An beiden F\u00e4usten Eisenfesseln,", "tokens": ["An", "bei\u00b7den", "F\u00e4us\u00b7ten", "Ei\u00b7sen\u00b7fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dicke Eisenzaun vor dir,", "tokens": ["Der", "di\u00b7cke", "Ei\u00b7sen\u00b7zaun", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Rechten Dorn, zur Linken Nesseln,", "tokens": ["Zur", "Rech\u00b7ten", "Dorn", ",", "zur", "Lin\u00b7ken", "Nes\u00b7seln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hinter dir die Eichent\u00fcr,", "tokens": ["Und", "hin\u00b7ter", "dir", "die", "Ei\u00b7chen\u00b7t\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kerkermeisterin, die weise", "tokens": ["Die", "Ker\u00b7ker\u00b7meis\u00b7te\u00b7rin", ",", "die", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und trockne Frau, die Sitte spricht:", "tokens": ["Und", "trock\u00b7ne", "Frau", ",", "die", "Sit\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbnur still mein S\u00f6hnchen, leise, leise,", "tokens": ["\u00bb", "nur", "still", "mein", "S\u00f6hn\u00b7chen", ",", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPOSAT", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn all dein Toben hilft dir nicht.\u00ab", "tokens": ["Denn", "all", "dein", "To\u00b7ben", "hilft", "dir", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbnur nicht so wild und nicht so wehrig,", "tokens": ["\u00bb", "nur", "nicht", "so", "wild", "und", "nicht", "so", "weh\u00b7rig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKNEG", "ADV", "ADJD", "KON", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst doch endlich noch gescheit,", "tokens": ["Du", "wirst", "doch", "end\u00b7lich", "noch", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es macht der Hunger dich gelehrig", "tokens": ["Es", "macht", "der", "Hun\u00b7ger", "dich", "ge\u00b7leh\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00fcrbe dich die Einsamkeit,", "tokens": ["Und", "m\u00fcr\u00b7be", "dich", "die", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erstick' das hei\u00dfe Tatensehnen,", "tokens": ["Er\u00b7stick'", "das", "hei\u00b7\u00dfe", "Ta\u00b7ten\u00b7seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Mitleidsschmerz, den Schaffensmut,", "tokens": ["Den", "Mit\u00b7leids\u00b7schmerz", ",", "den", "Schaf\u00b7fens\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schneid' los das Herz von seinen Pl\u00e4nen", "tokens": ["Schneid'", "los", "das", "Herz", "von", "sei\u00b7nen", "Pl\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du stirbst nicht von dem bi\u00dfchen Blut.", "tokens": ["Du", "stirbst", "nicht", "von", "dem", "bi\u00df\u00b7chen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel bess're noch als du schon waren", "tokens": ["Viel", "bess'\u00b7re", "noch", "als", "du", "schon", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier eingesperrt und wurden zahm,", "tokens": ["Hier", "ein\u00b7ge\u00b7sperrt", "und", "wur\u00b7den", "zahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Tagen, Wochen oder Jahren", "tokens": ["Nach", "Ta\u00b7gen", ",", "Wo\u00b7chen", "o\u00b7der", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Durchschnittsmensch zum Vorschein kam.", "tokens": ["Der", "Durch\u00b7schnitts\u00b7mensch", "zum", "Vor\u00b7schein", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ab und zu, da wollt' wohl einer", "tokens": ["Doch", "ab", "und", "zu", ",", "da", "wollt'", "wohl", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ", "$,", "ADV", "VMFIN", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht bieder, brav und f\u00fcgsam sein \u2013", "tokens": ["Nicht", "bie\u00b7der", ",", "brav", "und", "f\u00fcg\u00b7sam", "sein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herausgekommen ist noch keiner,", "tokens": ["Her\u00b7aus\u00b7ge\u00b7kom\u00b7men", "ist", "noch", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie rannten sich die Sch\u00e4del ein.\u00ab", "tokens": ["Sie", "rann\u00b7ten", "sich", "die", "Sch\u00e4\u00b7del", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbnun sieh, das ist doch unvern\u00fcnftig,", "tokens": ["\u00bb", "nun", "sieh", ",", "das", "ist", "doch", "un\u00b7ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und du wirst obendrein verlacht,", "tokens": ["Und", "du", "wirst", "o\u00b7ben\u00b7drein", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Romantik ist heut nicht mehr z\u00fcnftig,", "tokens": ["Ro\u00b7man\u00b7tik", "ist", "heut", "nicht", "mehr", "z\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heut wird nur ein Erfolg gemacht, \u2013", "tokens": ["Heut", "wird", "nur", "ein", "Er\u00b7folg", "ge\u00b7macht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst schon ruhiger, mein Liebchen,", "tokens": ["Du", "wirst", "schon", "ru\u00b7hi\u00b7ger", ",", "mein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und willst heraus, ei, wie das eilt!", "tokens": ["Und", "willst", "he\u00b7raus", ",", "ei", ",", "wie", "das", "eilt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "ITJ", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Klatsch, Publikum! das b\u00f6se B\u00fcbchen", "tokens": ["Klatsch", ",", "Pub\u00b7li\u00b7kum", "!", "das", "b\u00f6\u00b7se", "B\u00fcb\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist von der Unart schnell geheilt!\u00ab", "tokens": ["Ist", "von", "der", "Un\u00b7art", "schnell", "ge\u00b7heilt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}