{"textgrid.poem.60076": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gr\u00fcner Platz, von unsern H\u00e4nden", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcner Platz, von unsern H\u00e4nden", "tokens": ["Gr\u00fc\u00b7ner", "Platz", ",", "von", "un\u00b7sern", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angebaut f\u00fcr Spiel und Ruh,", "tokens": ["An\u00b7ge\u00b7baut", "f\u00fcr", "Spiel", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leicht umz\u00e4unt mit Rosen-W\u00e4nden,", "tokens": ["Leicht", "um\u00b7z\u00e4unt", "mit", "Ro\u00b7sen\u00b7W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebes, trautes G\u00e4rtchen du!", "tokens": ["Lie\u00b7bes", ",", "trau\u00b7tes", "G\u00e4rt\u00b7chen", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes unsrer Jugend-Feste,", "tokens": ["Je\u00b7des", "uns\u00b7rer", "Ju\u00b7gend\u00b7Fes\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die kein Neid verderben kann,", "tokens": ["Die", "kein", "Neid", "ver\u00b7der\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lachen durch die Bl\u00fcthen-Aeste", "tokens": ["La\u00b7chen", "durch", "die", "Bl\u00fcthen\u00b7Aes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Mond und Sonne freundlich an.", "tokens": ["Mond", "und", "Son\u00b7ne", "freund\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcner Platz! vor wenig Lenzen", "tokens": ["Gr\u00fc\u00b7ner", "Platz", "!", "vor", "we\u00b7nig", "Len\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lagst du noch als W\u00fcsteney;", "tokens": ["Lagst", "du", "noch", "als", "W\u00fcs\u00b7te\u00b7ney", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur mit Dorn- und Distel-Kr\u00e4nzen", "tokens": ["Nur", "mit", "Dorn", "und", "Dis\u00b7tel\u00b7Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcckte dich der junge May;", "tokens": ["Schm\u00fcck\u00b7te", "dich", "der", "jun\u00b7ge", "May", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo f\u00fcr Blumen Nesseln sprossen,", "tokens": ["Wo", "f\u00fcr", "Blu\u00b7men", "Nes\u00b7seln", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wallte keines M\u00e4dchens Fu\u00df;", "tokens": ["Wall\u00b7te", "kei\u00b7nes", "M\u00e4d\u00b7chens", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deines Bachs Gew\u00e4sser flossen", "tokens": ["Dei\u00b7nes", "Bachs", "Ge\u00b7w\u00e4s\u00b7ser", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Lied und ohne Gru\u00df.", "tokens": ["Oh\u00b7ne", "Lied", "und", "oh\u00b7ne", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! vielleicht in fernen Jahren", "tokens": ["Ach", "!", "viel\u00b7leicht", "in", "fer\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat ein Fremder dich zerst\u00f6rt;", "tokens": ["Hat", "ein", "Frem\u00b7der", "dich", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Reihent\u00e4nze waren,", "tokens": ["Wo", "die", "Rei\u00b7hen\u00b7t\u00e4n\u00b7ze", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Grille nur geh\u00f6rt;", "tokens": ["Wird", "die", "Gril\u00b7le", "nur", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Am verla\u00dfnen Ufer stehen", "tokens": ["Am", "ver\u00b7la\u00df\u00b7nen", "U\u00b7fer", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese B\u00e4um' entbl\u00e4ttert da,", "tokens": ["Die\u00b7se", "B\u00e4um'", "ent\u00b7bl\u00e4t\u00b7tert", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Wachholderb\u00fcsche wehen,", "tokens": ["Und", "Wach\u00b7hol\u00b7der\u00b7b\u00fc\u00b7sche", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wo man unsre Lauben sah.", "tokens": ["Wo", "man", "uns\u00b7re", "Lau\u00b7ben", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Banger, schauernder Gedanke!", "tokens": ["Ban\u00b7ger", ",", "schau\u00b7ern\u00b7der", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so treulich wir gepflegt,", "tokens": ["Was", "so", "treu\u00b7lich", "wir", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fctten-Dach, und Epheu-Ranke,", "tokens": ["H\u00fct\u00b7ten\u00b7Dach", ",", "und", "E\u00b7pheu\u00b7Ran\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So gew\u00fcnscht, und so gehegt;", "tokens": ["So", "ge\u00b7w\u00fcnscht", ",", "und", "so", "ge\u00b7hegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles einst in leere L\u00fcfte!", "tokens": ["Al\u00b7les", "einst", "in", "lee\u00b7re", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weggesunken jede Spur!", "tokens": ["Weg\u00b7ge\u00b7sun\u00b7ken", "je\u00b7de", "Spur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit hin\u00fcber durch die Gr\u00fcfte", "tokens": ["Mit", "hin\u00b7\u00fc\u00b7ber", "durch", "die", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geht das Herz voll Liebe nur!", "tokens": ["Geht", "das", "Herz", "voll", "Lie\u00b7be", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}