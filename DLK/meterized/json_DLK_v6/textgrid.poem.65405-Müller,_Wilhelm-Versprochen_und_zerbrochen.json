{"textgrid.poem.65405": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Versprochen und zerbrochen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manches Glas bezahl' ich hier,", "tokens": ["Wie", "man\u00b7ches", "Glas", "be\u00b7zahl'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hab' es nicht zerbrochen!", "tokens": ["Und", "hab'", "es", "nicht", "zer\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch nicht ", "tokens": ["Auch", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und hast so viel versprochen.", "tokens": ["Und", "hast", "so", "viel", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und k\u00fcssest du mich heute nicht,", "tokens": ["Und", "k\u00fcs\u00b7sest", "du", "mich", "heu\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich bis morgen zechen;", "tokens": ["Will", "ich", "bis", "mor\u00b7gen", "ze\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mir die Geduld zerbricht,", "tokens": ["Und", "wenn", "mir", "die", "Ge\u00b7duld", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag auch ein Krug zerbrechen.", "tokens": ["Mag", "auch", "ein", "Krug", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}