{"textgrid.poem.54923": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist mein Schaf;", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mein Schaf;", "tokens": ["Du", "bist", "mein", "Schaf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich bin dir niemals b\u00f6se.", "tokens": ["Ich", "bin", "dir", "nie\u00b7mals", "b\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er ist baff;", "tokens": ["Und", "er", "ist", "baff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Er schaut ins Weltgekr\u00f6se.", "tokens": ["Er", "schaut", "ins", "Welt\u00b7ge\u00b7kr\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist mein Schaf,", "tokens": ["Du", "bist", "mein", "Schaf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Erl\u00f6se ihn, erl\u00f6se", "tokens": ["Er\u00b7l\u00f6\u00b7se", "ihn", ",", "er\u00b7l\u00f6\u00b7se"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch mich von dem Get\u00f6se", "tokens": ["Auch", "mich", "von", "dem", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der auferstandnen Jugendzeit;", "tokens": ["Der", "auf\u00b7er\u00b7stand\u00b7nen", "Ju\u00b7gend\u00b7zeit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie steht vor mir im Leichenkleid.", "tokens": ["Sie", "steht", "vor", "mir", "im", "Lei\u00b7chen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}