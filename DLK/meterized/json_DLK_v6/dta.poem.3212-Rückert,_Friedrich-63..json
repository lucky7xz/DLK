{"dta.poem.3212": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst r\u00fchrte zwischen Schlaf und Wachen mich ein Schimmer,", "tokens": ["J\u00fcngst", "r\u00fchr\u00b7te", "zwi\u00b7schen", "Schlaf", "und", "Wa\u00b7chen", "mich", "ein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sah die Meinigen im kerzenhellen Zimmer.", "tokens": ["Ich", "sah", "die", "Mei\u00b7ni\u00b7gen", "im", "ker\u00b7zen\u00b7hel\u00b7len", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie trieben ihr Gesch\u00e4ft und trieben ihre Spiele,", "tokens": ["Sie", "trie\u00b7ben", "ihr", "Ge\u00b7sch\u00e4ft", "und", "trie\u00b7ben", "ih\u00b7re", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich freut' es, wie so froh sie waren und so viele.", "tokens": ["Mich", "freut'", "es", ",", "wie", "so", "froh", "sie", "wa\u00b7ren", "und", "so", "vie\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "PPER", "VAFIN", "KON", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nebenaus von dem Getriebe war ein Nischchen", "tokens": ["Doch", "ne\u00b7be\u00b7naus", "von", "dem", "Ge\u00b7trie\u00b7be", "war", "ein", "Nischchen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gew\u00f6lbet in der Wand, darin gestellt ein Tischchen.", "tokens": ["Ge\u00b7w\u00f6l\u00b7bet", "in", "der", "Wand", ",", "da\u00b7rin", "ge\u00b7stellt", "ein", "Tischchen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PAV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bei d\u00e4mmerlichem Schein dort sa\u00dfen zwo Gestalten,", "tokens": ["Bei", "d\u00e4m\u00b7mer\u00b7li\u00b7chem", "Schein", "dort", "sa\u00b7\u00dfen", "zwo", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jugendliche schlank mit vorgeb\u00fcckter Alten.", "tokens": ["Die", "Ju\u00b7gend\u00b7li\u00b7che", "schlank", "mit", "vor\u00b7ge\u00b7b\u00fcck\u00b7ter", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die schienen ihr Gespr\u00e4ch und ihr Gesch\u00e4ft zu treiben", "tokens": ["Die", "schie\u00b7nen", "ihr", "Ge\u00b7spr\u00e4ch", "und", "ihr", "Ge\u00b7sch\u00e4ft", "zu", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr sich, doch theilnahmlos umher auch nicht zu bleiben.", "tokens": ["F\u00fcr", "sich", ",", "doch", "theil\u00b7nahm\u00b7los", "um\u00b7her", "auch", "nicht", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "$,", "ADV", "ADJD", "PTKVZ", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich kannte sie gar wol, es war die schlichte G\u00fcte", "tokens": ["Ich", "kann\u00b7te", "sie", "gar", "wol", ",", "es", "war", "die", "schlich\u00b7te", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alten Mutter und der Schwester Jugendbl\u00fcte.", "tokens": ["Der", "al\u00b7ten", "Mut\u00b7ter", "und", "der", "Schwes\u00b7ter", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Auch wundert' ich mich nicht, wie sie hieher gekommen,", "tokens": ["Auch", "wun\u00b7dert'", "ich", "mich", "nicht", ",", "wie", "sie", "hie\u00b7her", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "PWAV", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nacheinander Beid' ein Grab hatt' aufgenommen.", "tokens": ["Die", "na\u00b7ch\u00b7ein\u00b7an\u00b7der", "Beid'", "ein", "Grab", "hatt'", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "So habt ihr nun gemacht die vorgehabte Reise,", "tokens": ["So", "habt", "ihr", "nun", "ge\u00b7macht", "die", "vor\u00b7ge\u00b7hab\u00b7te", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seid, wo ihr gewollt, in meinem Lebenskreise.", "tokens": ["Und", "seid", ",", "wo", "ihr", "ge\u00b7wollt", ",", "in", "mei\u00b7nem", "Le\u00b7bens\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PPER", "VMPP", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dort sitzen sie und sehn still in den Kreis herein,", "tokens": ["Dort", "sit\u00b7zen", "sie", "und", "sehn", "still", "in", "den", "Kreis", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welchem Niemand sie gewahrt als ich allein.", "tokens": ["Aus", "wel\u00b7chem", "Nie\u00b7mand", "sie", "ge\u00b7wahrt", "als", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPER", "VVPP", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Nicht Miene machen sie noch Regung, herzuschreiten,", "tokens": ["Nicht", "Mie\u00b7ne", "ma\u00b7chen", "sie", "noch", "Re\u00b7gung", ",", "her\u00b7zu\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "ADV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufrieden, mit dem Blick von dort uns zu begleiten.", "tokens": ["Zu\u00b7frie\u00b7den", ",", "mit", "dem", "Blick", "von", "dort", "uns", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Schutzgeistern \u00e4hnlich, die uns ungesehn umwalten,", "tokens": ["Schutz\u00b7geis\u00b7tern", "\u00e4hn\u00b7lich", ",", "die", "uns", "un\u00b7ge\u00b7sehn", "um\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Bildern an der Wand, die ihren Platz behalten.", "tokens": ["Und", "Bil\u00b7dern", "an", "der", "Wand", ",", "die", "ih\u00b7ren", "Platz", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "So l\u00e4cheln sie herein, begn\u00fcgt und unbeklommen,", "tokens": ["So", "l\u00e4\u00b7cheln", "sie", "her\u00b7ein", ",", "be\u00b7gn\u00fcgt", "und", "un\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Froh, im Familienkreis zu seyn mitaufgenommen.", "tokens": ["Froh", ",", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7kreis", "zu", "seyn", "mit\u00b7auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "PTKZU", "VAINF", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}