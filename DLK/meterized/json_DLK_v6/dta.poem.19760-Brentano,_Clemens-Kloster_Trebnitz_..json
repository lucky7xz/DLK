{"dta.poem.19760": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kloster Trebnitz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der edel Herzog Heinrich zu Pferd               ", "tokens": ["Der", "e\u00b7del", "Her\u00b7zog", "Hein\u00b7rich", "zu", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NE", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "St\u00fcrzt in den Sumpf gar tief, tief, tief.", "tokens": ["St\u00fcrzt", "in", "den", "Sumpf", "gar", "tief", ",", "tief", ",", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Seines Lebens er sich schier verwehrt,", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "er", "sich", "schier", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als Gott sein Engel rief, rief, rief.", "tokens": ["Als", "Gott", "sein", "En\u00b7gel", "rief", ",", "rief", ",", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Engel nahm ein K\u00f6hlertracht,", "tokens": ["Der", "En\u00b7gel", "nahm", "ein", "K\u00f6h\u00b7ler\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trat zum Sumpf hinan, an, an.", "tokens": ["Und", "trat", "zum", "Sumpf", "hi\u00b7nan", ",", "an", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schnell dem Herrn ein Aestlein bracht:", "tokens": ["Und", "schnell", "dem", "Herrn", "ein", "A\u00b7est\u00b7lein", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u201eda halt der Herr sich dran, dran, dran.", "tokens": ["\u201e", "da", "halt", "der", "Herr", "sich", "dran", ",", "dran", ",", "dran", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PRF", "PAV", "$,", "PAV", "$,", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als der Herzog g'rettet war,", "tokens": ["Und", "als", "der", "Her\u00b7zog", "g'\u00b7ret\u00b7tet", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da kniet er freudig hin, hin, hin.", "tokens": ["Da", "kniet", "er", "freu\u00b7dig", "hin", ",", "hin", ",", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eo Herr wie ist es wunderbar,", "tokens": ["\u201e", "o", "Herr", "wie", "ist", "es", "wun\u00b7der\u00b7bar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "KOKOM", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eda\u00df ich gerettet bin, bin, bin.\u201c", "tokens": ["\u201e", "da\u00df", "ich", "ge\u00b7ret\u00b7tet", "bin", ",", "bin", ",", "bin", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "VAFIN", "$,", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eund bin ich denn gerettet nun,", "tokens": ["\u201e", "und", "bin", "ich", "denn", "ge\u00b7ret\u00b7tet", "nun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ADV", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ebau ich ein Kloster dir, dir, dir,", "tokens": ["\u201e", "bau", "ich", "ein", "Klos\u00b7ter", "dir", ",", "dir", ",", "dir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "PPER", "$,", "PPER", "$,", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u201eda\u00df man dir dien in Fried und Ruh,", "tokens": ["\u201e", "da\u00df", "man", "dir", "di\u00b7en", "in", "Fried", "und", "Ruh", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201eauf diesem Flecklein hier, hier, hier.", "tokens": ["\u201e", "auf", "die\u00b7sem", "Flec\u00b7klein", "hier", ",", "hier", ",", "hier", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Kloster war gar sch\u00f6n gebaut,", "tokens": ["Das", "Klos\u00b7ter", "war", "gar", "sch\u00f6n", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des freut sich wer es sah, sah, sah.", "tokens": ["Des", "freut", "sich", "wer", "es", "sah", ",", "sah", ",", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manche fromme Gottesbraut,", "tokens": ["Und", "man\u00b7che", "from\u00b7me", "Got\u00b7tes\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam hin von fern und uah, nah, nah.", "tokens": ["Kam", "hin", "von", "fern", "und", "u\u00b7ah", ",", "nah", ",", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJD", "KON", "NE", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "\u201ewas begehrt ihr edle Jungfrauen mehr?", "tokens": ["\u201e", "was", "be\u00b7gehrt", "ihr", "ed\u00b7le", "Jung\u00b7frau\u00b7en", "mehr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201eder Herzog fragt sie dann, dann, dann.", "tokens": ["\u201e", "der", "Her\u00b7zog", "fragt", "sie", "dann", ",", "dann", ",", "dann", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NE", "VVFIN", "PPER", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewir b'd\u00fcrfen nichts und nimmermehr", "tokens": ["\u201e", "wir", "d\u00fcr\u00b7fe", "nichts", "und", "nim\u00b7mer\u00b7mehr"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIS", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edieweil wir alles han, han, han.", "tokens": ["\u201e", "die\u00b7weil", "wir", "al\u00b7les", "han", ",", "han", ",", "han", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIS", "VAFIN", "$,", "VAFIN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eund weil euch denn nichts noth mehr ist,", "tokens": ["\u201e", "und", "weil", "euch", "denn", "nichts", "noth", "mehr", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "ADV", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eso sey denn dieser Nam, Nam, Nam,", "tokens": ["\u201e", "so", "sey", "denn", "die\u00b7ser", "Nam", ",", "Nam", ",", "Nam", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PDAT", "NN", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201etrebnitz, das hie\u00df, wir b'd\u00fcrfen nichts,", "tokens": ["\u201e", "treb\u00b7nitz", ",", "das", "hie\u00df", ",", "wir", "d\u00fcr\u00b7fe", "nichts", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PDS", "VVFIN", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eden Namen es bekam, kam, kam.\u201c", "tokens": ["\u201e", "den", "Na\u00b7men", "es", "be\u00b7kam", ",", "kam", ",", "kam", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}