{"textgrid.poem.42333": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es knallet alles was lebet,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es knallet alles was lebet,", "tokens": ["Es", "knal\u00b7let", "al\u00b7les", "was", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was in den L\u00fcften schwebet,", "tokens": ["Was", "in", "den", "L\u00fcf\u00b7ten", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es knallt die ganze Welt;", "tokens": ["Es", "knallt", "die", "gan\u00b7ze", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein M\u00e4dchen von zw\u00f6lf Jahren,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "von", "zw\u00f6lf", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit zwanzig Stoppelhaaren,", "tokens": ["Mit", "zwan\u00b7zig", "Stop\u00b7pel\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Fuchsschwanz schon gef\u00e4llt.", "tokens": ["Der", "Fuchs\u00b7schwanz", "schon", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was machen nicht f\u00fcr Mienen", "tokens": ["Was", "ma\u00b7chen", "nicht", "f\u00fcr", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hasen und Kaninchen,", "tokens": ["Die", "Ha\u00b7sen", "und", "Ka\u00b7nin\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was tut der Sperling nicht?", "tokens": ["Was", "tut", "der", "Sper\u00b7ling", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Hengst macht junge F\u00fcllen,", "tokens": ["Der", "Hengst", "macht", "jun\u00b7ge", "F\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Boll hat seinen Willen,", "tokens": ["Der", "Boll", "hat", "sei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ihn der Kitzel sticht.", "tokens": ["Wenn", "ihn", "der", "Kit\u00b7zel", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Elefant von hinten", "tokens": ["Der", "E\u00b7le\u00b7fant", "von", "hin\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df auch das Loch zu finden,", "tokens": ["Wei\u00df", "auch", "das", "Loch", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Kater braucht kein Licht;", "tokens": ["Der", "Ka\u00b7ter", "braucht", "kein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der B\u00e4r bohrt seine Frau", "tokens": ["Der", "B\u00e4r", "bohrt", "sei\u00b7ne", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Lust wohl in den Rauh,", "tokens": ["Mit", "Lust", "wohl", "in", "den", "Rauh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Warum denn Menschen nicht?", "tokens": ["Wa\u00b7rum", "denn", "Men\u00b7schen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Selbst Juno mu\u00dfte lachen,", "tokens": ["Selbst", "Ju\u00b7no", "mu\u00df\u00b7te", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Jupiter wollt machen", "tokens": ["Als", "Ju\u00b7pi\u00b7ter", "wollt", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr einen dicken Bauch;", "tokens": ["Ihr", "ei\u00b7nen", "di\u00b7cken", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch l\u00e4\u00dft sie sich's gefallen", "tokens": ["Doch", "l\u00e4\u00dft", "sie", "sich's", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00e4\u00dft sich tapfer knallen", "tokens": ["Und", "l\u00e4\u00dft", "sich", "tap\u00b7fer", "knal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei einem Rosenstrauch.", "tokens": ["Bei", "ei\u00b7nem", "Ro\u00b7sen\u00b7strauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Diana, m\u00fcd vom Jagen,", "tokens": ["Di\u00b7a\u00b7na", ",", "m\u00fcd", "vom", "Ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft sich den Spie\u00df behagen,", "tokens": ["L\u00e4\u00dft", "sich", "den", "Spie\u00df", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie steckt ihn selbst hinein.", "tokens": ["Sie", "steckt", "ihn", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wer will ihr gefallen,", "tokens": ["Doch", "wer", "will", "ihr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der mu\u00df dabei vor allen", "tokens": ["Der", "mu\u00df", "da\u00b7bei", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PAV", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Recht wohl beschlagen sein.", "tokens": ["Recht", "wohl", "be\u00b7schla\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Merkur, der G\u00f6tterbote!", "tokens": ["Mer\u00b7kur", ",", "der", "G\u00f6t\u00b7ter\u00b7bo\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist auch von solchem Schrote,", "tokens": ["Ist", "auch", "von", "sol\u00b7chem", "Schro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er ausfliegen soll,", "tokens": ["Wenn", "er", "aus\u00b7flie\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Besucht er jede Nymphe", "tokens": ["Be\u00b7sucht", "er", "je\u00b7de", "Nym\u00b7phe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gibt ihr wackre Tr\u00fcmpfe,", "tokens": ["Und", "gibt", "ihr", "wack\u00b7re", "Tr\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Spritzt ihr die B\u00fcchse voll.", "tokens": ["Spritzt", "ihr", "die", "B\u00fcch\u00b7se", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch Cupido, der Kleine,", "tokens": ["Auch", "Cu\u00b7pi\u00b7do", ",", "der", "Klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Greift Venus zwischen die Beine,", "tokens": ["Greift", "Ve\u00b7nus", "zwi\u00b7schen", "die", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sein Schw\u00e4nzchen wird ihm hart;", "tokens": ["Sein", "Schw\u00e4nz\u00b7chen", "wird", "ihm", "hart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er spritzt den edlen Samen,", "tokens": ["Er", "spritzt", "den", "ed\u00b7len", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In aller G\u00f6tter Namen,", "tokens": ["In", "al\u00b7ler", "G\u00f6t\u00b7ter", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Mutter in den Bart.", "tokens": ["Der", "Mut\u00b7ter", "in", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vulkan, in seiner Kammer,", "tokens": ["Vul\u00b7kan", ",", "in", "sei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit seinem Schmiedehammer,", "tokens": ["Mit", "sei\u00b7nem", "Schmie\u00b7de\u00b7ham\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df auch mit an den Tanz;", "tokens": ["Mu\u00df", "auch", "mit", "an", "den", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Schwanz, wenn er geschwollen,", "tokens": ["Sein", "Schwanz", ",", "wenn", "er", "ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00e4lt f\u00fcnfundzwanzig Zollen,", "tokens": ["H\u00e4lt", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "Zol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O auserlesner Schwanz!", "tokens": ["O", "au\u00b7ser\u00b7les\u00b7ner", "Schwanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Charon, beim \u00dcberfahren,", "tokens": ["Cha\u00b7ron", ",", "beim", "\u00dc\u00b7berf\u00b7ah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fuchst alles rauch von Haaren,", "tokens": ["Fuchst", "al\u00b7les", "rauch", "von", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schont auch die Votzen nicht;", "tokens": ["Schont", "auch", "die", "Vot\u00b7zen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Pluto fuchst Proserpinen,", "tokens": ["Plu\u00b7to", "fuchst", "Pro\u00b7ser\u00b7pi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und Luchse fuchst Luchsinnen,", "tokens": ["Und", "Luch\u00b7se", "fuchst", "Luc\u00b7hsin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warum denn Menschen nicht?", "tokens": ["Wa\u00b7rum", "denn", "Men\u00b7schen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr Nonnen und ihr Pfaffen!", "tokens": ["Ihr", "Non\u00b7nen", "und", "ihr", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sollt beisammen schlafen,", "tokens": ["Ihr", "sollt", "bei\u00b7sam\u00b7men", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft Messe Messe sein;", "tokens": ["La\u00dft", "Mes\u00b7se", "Mes\u00b7se", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So oft die Glocken l\u00e4uten,", "tokens": ["So", "oft", "die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So oft sollt ihr euch reiten,", "tokens": ["So", "oft", "sollt", "ihr", "euch", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steckt ihn fein tief hinein.", "tokens": ["Steckt", "ihn", "fein", "tief", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.11": {"line.1": {"text": "Bemerket diese Worte,", "tokens": ["Be\u00b7mer\u00b7ket", "die\u00b7se", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.2": {"text": "Ihr Jungfern aller Orte,", "tokens": ["Ihr", "Jung\u00b7fern", "al\u00b7ler", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt meine Lehren doch;", "tokens": ["H\u00f6rt", "mei\u00b7ne", "Leh\u00b7ren", "doch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verla\u00dft die samtnen Dinger", "tokens": ["Ver\u00b7la\u00dft", "die", "samt\u00b7nen", "Din\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und steckt statt eurem Finger", "tokens": ["Und", "steckt", "statt", "eu\u00b7rem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den rechten Schwanz ins Loch.", "tokens": ["Den", "rech\u00b7ten", "Schwanz", "ins", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}