{"textgrid.poem.39076": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zensur", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur immer frisch verboten,", "tokens": ["Nur", "im\u00b7mer", "frisch", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nur immer konfisziert!", "tokens": ["nur", "im\u00b7mer", "kon\u00b7fis\u00b7ziert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ging' es auch nach Noten,", "tokens": ["Und", "ging'", "es", "auch", "nach", "No\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ihr weckt doch nicht die Toten,", "tokens": ["ihr", "weckt", "doch", "nicht", "die", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "das Leben triumphiert!", "tokens": ["das", "Le\u00b7ben", "tri\u00b7um\u00b7phiert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr traurigen Kapuzen,", "tokens": ["Ihr", "trau\u00b7ri\u00b7gen", "Ka\u00b7pu\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ihr aller Wahrheit Feind,", "tokens": ["ihr", "al\u00b7ler", "Wahr\u00b7heit", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ihr wollt den Adler stutzen,", "tokens": ["ihr", "wollt", "den", "Ad\u00b7ler", "stut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Sonne wollt ihr putzen,", "tokens": ["die", "Son\u00b7ne", "wollt", "ihr", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "weil sie zu hell euch scheint?!", "tokens": ["weil", "sie", "zu", "hell", "euch", "scheint", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Umsonst! Ihr k\u00f6nnt nicht hindern,", "tokens": ["Um\u00b7sonst", "!", "Ihr", "k\u00f6nnt", "nicht", "hin\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auch nicht das kleinste Wort!", "tokens": ["auch", "nicht", "das", "kleins\u00b7te", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00f6nnt den Ha\u00df nicht mindern,", "tokens": ["Ihr", "k\u00f6nnt", "den", "Ha\u00df", "nicht", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ihr k\u00f6nnt die Glut nicht lindern,", "tokens": ["ihr", "k\u00f6nnt", "die", "Glut", "nicht", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die grimmig euch verdorrt!", "tokens": ["die", "grim\u00b7mig", "euch", "ver\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gebt acht, die Stunden schleichen,", "tokens": ["Gebt", "acht", ",", "die", "Stun\u00b7den", "schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Morgensonne strahlt:", "tokens": ["die", "Mor\u00b7gen\u00b7son\u00b7ne", "strahlt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gebt acht, ich seh' ein Zeichen,", "tokens": ["Gebt", "acht", ",", "ich", "seh'", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da werden noch mit Streichen", "tokens": ["da", "wer\u00b7den", "noch", "mit", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die Striche euch bezahlt!", "tokens": ["die", "Stri\u00b7che", "euch", "be\u00b7zahlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So n\u00e4hrt ihr selbst die Flamme,", "tokens": ["So", "n\u00e4hrt", "ihr", "selbst", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die selber euch verzehrt:", "tokens": ["die", "sel\u00b7ber", "euch", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon knistert es am Stamme \u2013", "tokens": ["Schon", "knis\u00b7tert", "es", "am", "Stam\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O da\u00df euch Gott verdamme!", "tokens": ["O", "da\u00df", "euch", "Gott", "ver\u00b7dam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr seid kein Mitleid wert.", "tokens": ["Ihr", "seid", "kein", "Mit\u00b7leid", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}