{"dta.poem.19343": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fcrst lie\u00df seinem Sohn verfertigen ein Schild,", "tokens": ["Ein", "F\u00fcrst", "lie\u00df", "sei\u00b7nem", "Sohn", "ver\u00b7fer\u00b7ti\u00b7gen", "ein", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier Felder von Azur, in jedem Feld ein Bild.", "tokens": ["Vier", "Fel\u00b7der", "von", "A\u00b7zur", ",", "in", "je\u00b7dem", "Feld", "ein", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NE", "$,", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und jedem Sinnbild war ein Sinnspruch beigegeben,", "tokens": ["Und", "je\u00b7dem", "Sinn\u00b7bild", "war", "ein", "Sinns\u00b7pruch", "bei\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch rings ums Ganze stand: Nach diesem sollst du leben.", "tokens": ["Doch", "rings", "ums", "Gan\u00b7ze", "stand", ":", "Nach", "die\u00b7sem", "sollst", "du", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$.", "APPR", "PDAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im ersten Felde war ein Hirsch von Gold, dazu", "tokens": ["Im", "ers\u00b7ten", "Fel\u00b7de", "war", "ein", "Hirsch", "von", "Gold", ",", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schrift von Diamant: die G\u00f6tter f\u00fcrchte du.", "tokens": ["Die", "Schrift", "von", "Di\u00b7a\u00b7mant", ":", "die", "G\u00f6t\u00b7ter", "f\u00fcrch\u00b7te", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im andern Feld ein Storch von Silber, und dazu", "tokens": ["Im", "an\u00b7dern", "Feld", "ein", "Storch", "von", "Sil\u00b7ber", ",", "und", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Inschrift von Rubin: die Eltern ehre du.", "tokens": ["Die", "In\u00b7schrift", "von", "Ru\u00b7bin", ":", "die", "El\u00b7tern", "eh\u00b7re", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Im dritten Feld, von Erz die Schildkr\u00f6t', und dazu", "tokens": ["Im", "drit\u00b7ten", "Feld", ",", "von", "Erz", "die", "Schild\u00b7kr\u00f6t'", ",", "und", "da\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schrift von Karneol: dein Haus bestelle du.", "tokens": ["Die", "Schrift", "von", "Kar\u00b7ne\u00b7ol", ":", "dein", "Haus", "be\u00b7stel\u00b7le", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im letzten Feld, von Stahl ein Delfin, und dazu", "tokens": ["Im", "letz\u00b7ten", "Feld", ",", "von", "Stahl", "ein", "Del\u00b7fin", ",", "und", "da\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schrift von Perlensaat: den Freunden diene du.", "tokens": ["Die", "Schrift", "von", "Per\u00b7len\u00b7saat", ":", "den", "Freun\u00b7den", "die\u00b7ne", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Warum ist G\u00f6tterfurcht vom Hirsch gemeint? Er zittert", "tokens": ["Wa\u00b7rum", "ist", "G\u00f6t\u00b7ter\u00b7furcht", "vom", "Hirsch", "ge\u00b7meint", "?", "Er", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "NN", "APPRART", "NN", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Walde, wenn ob ihm der Himmel hochgewittert.", "tokens": ["Im", "Wal\u00b7de", ",", "wenn", "ob", "ihm", "der", "Him\u00b7mel", "hoch\u00b7ge\u00b7wit\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wodurch ist Elternlieb' im Storch erkl\u00e4rt? Der junge", "tokens": ["Wo\u00b7durch", "ist", "El\u00b7tern\u00b7lieb'", "im", "Storch", "er\u00b7kl\u00e4rt", "?", "Der", "jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "NN", "APPRART", "NN", "VVPP", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt die gealterten mit seiner Fl\u00fcgel Schwunge.", "tokens": ["Tr\u00e4gt", "die", "ge\u00b7al\u00b7ter\u00b7ten", "mit", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "Schwun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie zeigt die Schildkr\u00f6t' Hausbestellung an? Sie tr\u00e4gt", "tokens": ["Wie", "zeigt", "die", "Schild\u00b7kr\u00f6t'", "Haus\u00b7be\u00b7stel\u00b7lung", "an", "?", "Sie", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fest auf dem R\u00fccken eins, das ihr kein Stein zerschl\u00e4gt.", "tokens": ["Fest", "auf", "dem", "R\u00fc\u00b7cken", "eins", ",", "das", "ihr", "kein", "Stein", "zer\u00b7schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PIS", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Womit thut Freundesdienst der Delfin kund? Er k\u00fcndet", "tokens": ["Wo\u00b7mit", "thut", "Freun\u00b7des\u00b7dienst", "der", "Del\u00b7fin", "kund", "?", "Er", "k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sturm, und bleibt im Sturm den Schiffenden verb\u00fcndet.", "tokens": ["Den", "Sturm", ",", "und", "bleibt", "im", "Sturm", "den", "Schif\u00b7fen\u00b7den", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Es ist ihm nicht genug, da\u00df er gewarnet h\u00e4tte;", "tokens": ["Es", "ist", "ihm", "nicht", "ge\u00b7nug", ",", "da\u00df", "er", "ge\u00b7war\u00b7net", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er m\u00fcht sich auch, da\u00df er umsonst gewarnte rette.", "tokens": ["Er", "m\u00fcht", "sich", "auch", ",", "da\u00df", "er", "um\u00b7sonst", "ge\u00b7warn\u00b7te", "ret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}