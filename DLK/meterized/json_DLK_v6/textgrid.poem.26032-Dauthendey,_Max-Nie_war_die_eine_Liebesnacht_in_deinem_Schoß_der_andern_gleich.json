{"textgrid.poem.26032": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nie war die eine Liebesnacht in deinem Scho\u00df der andern gleich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie war die eine Liebesnacht", "tokens": ["Nie", "war", "die", "ei\u00b7ne", "Lie\u00b7bes\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem Scho\u00df der andern gleich,", "tokens": ["In", "dei\u00b7nem", "Scho\u00df", "der", "an\u00b7dern", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Leib ist ein Septembermond", "tokens": ["Dein", "Leib", "ist", "ein", "Sep\u00b7tem\u00b7ber\u00b7mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "An immer neuen Fr\u00fcchten reich.", "tokens": ["An", "im\u00b7mer", "neu\u00b7en", "Fr\u00fcch\u00b7ten", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Br\u00fcste sind ein Traubenpaar,", "tokens": ["Die", "Br\u00fcs\u00b7te", "sind", "ein", "Trau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drinnen pocht der junge Wein,", "tokens": ["Und", "drin\u00b7nen", "pocht", "der", "jun\u00b7ge", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Augen sind ein Himmelstor", "tokens": ["Die", "Au\u00b7gen", "sind", "ein", "Him\u00b7mels\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lassen meine W\u00fcnsche ein.", "tokens": ["Und", "las\u00b7sen", "mei\u00b7ne", "W\u00fcn\u00b7sche", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}