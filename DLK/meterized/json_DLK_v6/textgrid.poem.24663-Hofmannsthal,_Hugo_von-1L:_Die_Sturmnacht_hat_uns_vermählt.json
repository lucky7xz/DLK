{"textgrid.poem.24663": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sturmnacht hat uns verm\u00e4hlt", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sturmnacht hat uns verm\u00e4hlt", "tokens": ["Die", "Sturm\u00b7nacht", "hat", "uns", "ver\u00b7m\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Brausen und Toben und Bangen:", "tokens": ["In", "Brau\u00b7sen", "und", "To\u00b7ben", "und", "Ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was unsre Seelen sich lange verhehlt,", "tokens": ["Was", "uns\u00b7re", "See\u00b7len", "sich", "lan\u00b7ge", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da ists uns aufgegangen.", "tokens": ["Da", "ists", "uns", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich las so tief in deinem Blick", "tokens": ["Ich", "las", "so", "tief", "in", "dei\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Strahl vom Wetterleuchten:", "tokens": ["Beim", "Strahl", "vom", "Wet\u00b7ter\u00b7leuch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich las darin mein flammend Gl\u00fcck,", "tokens": ["Ich", "las", "da\u00b7rin", "mein", "flam\u00b7mend", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Glanz, dem feuchten.", "tokens": ["In", "sei\u00b7nem", "Glanz", ",", "dem", "feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es warf der Wind dein duftges Haar", "tokens": ["Es", "warf", "der", "Wind", "dein", "duft\u00b7ges", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir spielend um Stirn und Wangen,", "tokens": ["Mir", "spie\u00b7lend", "um", "Stirn", "und", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es fl\u00fcsterte lockend die Wellenschar", "tokens": ["Es", "fl\u00fcs\u00b7ter\u00b7te", "lo\u00b7ckend", "die", "Wel\u00b7len\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von hei\u00dfem tiefem Verlangen.", "tokens": ["Von", "hei\u00b7\u00dfem", "tie\u00b7fem", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Lippen waren sich so nah,", "tokens": ["Die", "Lip\u00b7pen", "wa\u00b7ren", "sich", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hielt dich fest umschlungen;", "tokens": ["Ich", "hielt", "dich", "fest", "um\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Werben und dein stammelnd Ja,", "tokens": ["Mein", "Wer\u00b7ben", "und", "dein", "stam\u00b7melnd", "Ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hat der Wind verschlungen ...", "tokens": ["Die", "hat", "der", "Wind", "ver\u00b7schlun\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}