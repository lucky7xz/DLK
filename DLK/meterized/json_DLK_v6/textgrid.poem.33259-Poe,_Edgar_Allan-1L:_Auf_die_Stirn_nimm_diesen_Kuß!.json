{"textgrid.poem.33259": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf die Stirn nimm diesen Ku\u00df!", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die Stirn nimm diesen Ku\u00df!", "tokens": ["Auf", "die", "Stirn", "nimm", "die\u00b7sen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIMP", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und da ich nun scheiden mu\u00df,", "tokens": ["Und", "da", "ich", "nun", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So bekenne ich zum Schlu\u00df", "tokens": ["So", "be\u00b7ken\u00b7ne", "ich", "zum", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies noch: Unrecht habt ihr kaum,", "tokens": ["Dies", "noch", ":", "Un\u00b7recht", "habt", "ihr", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$.", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ihr meint, ich lebte Traum;", "tokens": ["Die", "ihr", "meint", ",", "ich", "leb\u00b7te", "Traum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch, wenn Hoffnung j\u00e4h enflohn", "tokens": ["Doch", ",", "wenn", "Hoff\u00b7nung", "j\u00e4h", "en\u00b7flohn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In Tag, in Nacht, in Vision", "tokens": ["In", "Tag", ",", "in", "Nacht", ",", "in", "Vi\u00b7si\u00b7on"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oder anderm Sinn und Wort \u2013", "tokens": ["O\u00b7der", "an\u00b7derm", "Sinn", "und", "Wort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist sie darum weniger ", "tokens": ["Ist", "sie", "da\u00b7rum", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PAV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schaun und Scheinen ist nur Schaum,", "tokens": ["Schaun", "und", "Schei\u00b7nen", "ist", "nur", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nichts als Traum in einem Traum!", "tokens": ["Nichts", "als", "Traum", "in", "ei\u00b7nem", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mitten in dem Wogenbrand", "tokens": ["Mit\u00b7ten", "in", "dem", "Wo\u00b7gen\u00b7brand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh' ich an gequ\u00e4ltem Strand,", "tokens": ["Steh'", "ich", "an", "ge\u00b7qu\u00e4l\u00b7tem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich halte in der Hand", "tokens": ["Und", "ich", "hal\u00b7te", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6rner von dem goldnen Sand \u2013", "tokens": ["K\u00f6r\u00b7ner", "von", "dem", "gold\u00b7nen", "Sand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenig, dennoch ach, sie rinnen", "tokens": ["We\u00b7nig", ",", "den\u00b7noch", "ach", ",", "sie", "rin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Finger mir von hinnen \u2013", "tokens": ["Durch", "die", "Fin\u00b7ger", "mir", "von", "hin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinen mu\u00df ich, weinend sinnen!", "tokens": ["Wei\u00b7nen", "mu\u00df", "ich", ",", "wei\u00b7nend", "sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, kann ich nicht fester fassen,", "tokens": ["Ach", ",", "kann", "ich", "nicht", "fes\u00b7ter", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um sie nicht hinwegzulassen?", "tokens": ["Um", "sie", "nicht", "hin\u00b7weg\u00b7zu\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach, kann ich nicht ", "tokens": ["Ach", ",", "kann", "ich", "nicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Halten vor der Woge Wut?", "tokens": ["Hal\u00b7ten", "vor", "der", "Wo\u00b7ge", "Wut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Nichts als Traum in einem Traum?", "tokens": ["Nichts", "als", "Traum", "in", "ei\u00b7nem", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}