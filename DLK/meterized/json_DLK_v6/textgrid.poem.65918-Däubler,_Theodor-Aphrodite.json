{"textgrid.poem.65918": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aphrodite", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus tausend Br\u00fcsten aufgesch\u00e4umten Meeres", "tokens": ["Aus", "tau\u00b7send", "Br\u00fcs\u00b7ten", "auf\u00b7ge\u00b7sch\u00e4um\u00b7ten", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erperlt der Aphrodite seltnes Licht:", "tokens": ["Er\u00b7perlt", "der", "A\u00b7phro\u00b7di\u00b7te", "selt\u00b7nes", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schwebt am Mittag schattenlos im Licht,", "tokens": ["Sie", "schwebt", "am", "Mit\u00b7tag", "schat\u00b7ten\u00b7los", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du sahst noch nie \u2013 gegeistert \u2013 ein so hehres", "tokens": ["Du", "sahst", "noch", "nie", "\u2013", "ge\u00b7geis\u00b7tert", "\u2013", "ein", "so", "heh\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "VVPP", "$(", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gebild, das dir vom Weibe spricht: begehr es!", "tokens": ["Ge\u00b7bild", ",", "das", "dir", "vom", "Wei\u00b7be", "spricht", ":", "be\u00b7gehr", "es", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Liebe, f\u00fcr und gegen dich in Pflicht,", "tokens": ["O", "Lie\u00b7be", ",", "f\u00fcr", "und", "ge\u00b7gen", "dich", "in", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "KON", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfahr ich Milde, find ich mein Gericht?", "tokens": ["Er\u00b7fahr", "ich", "Mil\u00b7de", ",", "find", "ich", "mein", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df von Ewigkeit: uns nun ein schweres", "tokens": ["Ich", "wei\u00df", "von", "E\u00b7wig\u00b7keit", ":", "uns", "nun", "ein", "schwe\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geheimbewalten altershoher Ehen.", "tokens": ["Ge\u00b7heim\u00b7be\u00b7wal\u00b7ten", "al\u00b7ter\u00b7sho\u00b7her", "E\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihnen kommt der Himmel zu uns her:", "tokens": ["Aus", "ih\u00b7nen", "kommt", "der", "Him\u00b7mel", "zu", "uns", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Frommen blo\u00df geb\u00fchrt ein Leib als Lehen;", "tokens": ["Den", "From\u00b7men", "blo\u00df", "ge\u00b7b\u00fchrt", "ein", "Leib", "als", "Le\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nach Schicksalssternen funkle dein Begehr!", "tokens": ["Nach", "Schick\u00b7salss\u00b7ter\u00b7nen", "funk\u00b7le", "dein", "Be\u00b7gehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von Scham des Blutes la\u00dft euch keusch beweben,", "tokens": ["Von", "Scham", "des", "Blu\u00b7tes", "la\u00dft", "euch", "keusch", "be\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleibt bla\u00df! Das Auge blaut auf reinem Meer.", "tokens": ["Bleibt", "bla\u00df", "!", "Das", "Au\u00b7ge", "blaut", "auf", "rei\u00b7nem", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}