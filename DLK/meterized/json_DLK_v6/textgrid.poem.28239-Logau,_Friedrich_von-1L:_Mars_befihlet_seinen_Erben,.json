{"textgrid.poem.28239": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mars befihlet seinen Erben,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mars befihlet seinen Erben,", "tokens": ["Mars", "be\u00b7fih\u00b7let", "sei\u00b7nen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann er endlich werde sterben,", "tokens": ["Wann", "er", "end\u00b7lich", "wer\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solt man, wann man ihn begr\u00fcbe,", "tokens": ["Solt", "man", ",", "wann", "man", "ihn", "be\u00b7gr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machen, da\u00df nichts \u00fcbrig bliebe,", "tokens": ["Ma\u00b7chen", ",", "da\u00df", "nichts", "\u00fcb\u00b7rig", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil sich doch von seiner Habe", "tokens": ["Weil", "sich", "doch", "von", "sei\u00b7ner", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwerlich erster Erbe labe.", "tokens": ["Schwer\u00b7lich", "ers\u00b7ter", "Er\u00b7be", "la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}