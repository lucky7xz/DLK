{"dta.poem.18084": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Zustande der deutschen  \n Poesie bey Ankunft Martin Opitzens.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ist nun die Natur, wo sind die grossen Sinnen,", "tokens": ["Wo", "ist", "nun", "die", "Na\u00b7tur", ",", "wo", "sind", "die", "gros\u00b7sen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit derer Hoheit ihr zuvor ersch\u00f6pffen k\u00f6nnen", "tokens": ["Mit", "de\u00b7rer", "Ho\u00b7heit", "ihr", "zu\u00b7vor", "er\u00b7sch\u00f6pf\u00b7fen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "PPER", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Grund der Wissenschaft? Wo ist der Weisheit Zier,", "tokens": ["Den", "Grund", "der", "Wis\u00b7sen\u00b7schaft", "?", "Wo", "ist", "der", "Weis\u00b7heit", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit der ihr, hoher Geist, giengt vielen andern f\u00fcr?", "tokens": ["Mit", "der", "ihr", ",", "ho\u00b7her", "Geist", ",", "giengt", "vie\u00b7len", "an\u00b7dern", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PIAT", "PIS", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist der Circkel dann, mit welchem ihr der Sternen,", "tokens": ["Wo", "ist", "der", "Cir\u00b7ckel", "dann", ",", "mit", "wel\u00b7chem", "ihr", "der", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Himmels Eigenschafft gepfleget zu erlernen?", "tokens": ["Und", "Him\u00b7mels", "Ei\u00b7gen\u00b7schafft", "ge\u00b7pfle\u00b7get", "zu", "er\u00b7ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}