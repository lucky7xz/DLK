{"textgrid.poem.42707": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sucht immerhin der Helden Spur", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sucht immerhin der Helden Spur", "tokens": ["Sucht", "im\u00b7mer\u00b7hin", "der", "Hel\u00b7den", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ufer des Skamanders!", "tokens": ["Am", "U\u00b7fer", "des", "Ska\u00b7man\u00b7ders", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Preist, wie ihr wollet, die Bravur", "tokens": ["Preist", ",", "wie", "ihr", "wol\u00b7let", ",", "die", "Bra\u00b7vur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Krieger Alexanders!", "tokens": ["Der", "Krie\u00b7ger", "A\u00b7lex\u00b7an\u00b7ders", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verk\u00fcndiget aus vollem Hals", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7get", "aus", "vol\u00b7lem", "Hals"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Ruhm der Truppen Hannibals!", "tokens": ["Den", "Ruhm", "der", "Trup\u00b7pen", "Han\u00b7ni\u00b7bals", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lobe mir das zahme Heer,", "tokens": ["Ich", "lo\u00b7be", "mir", "das", "zah\u00b7me", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das hier, vom biedern St\u00e4dter", "tokens": ["Das", "hier", ",", "vom", "bie\u00b7dern", "St\u00e4d\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gem\u00e4stet, seit den Staat nicht mehr", "tokens": ["Ge\u00b7m\u00e4s\u00b7tet", ",", "seit", "den", "Staat", "nicht", "mehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Faustrecht unsrer V\u00e4ter", "tokens": ["Das", "Faus\u00b7trecht", "uns\u00b7rer", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Krieg bedroht, der Ruhe pflegt,", "tokens": ["Mit", "Krieg", "be\u00b7droht", ",", "der", "Ru\u00b7he", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald Holz kliebt, und bald S\u00e4nften tr\u00e4gt.", "tokens": ["Bald", "Holz", "kliebt", ",", "und", "bald", "S\u00e4nf\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "KON", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Vorzeit Kriegsvolk war brutal,", "tokens": ["Der", "Vor\u00b7zeit", "Kriegs\u00b7volk", "war", "bru\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und konnte nichts, als morden.", "tokens": ["Und", "konn\u00b7te", "nichts", ",", "als", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch dieses Heer ist kein Gemahl", "tokens": ["Durch", "die\u00b7ses", "Heer", "ist", "kein", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Je kinderlos geworden:", "tokens": ["Je", "kin\u00b7der\u00b7los", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es hilft vielmehr dem Ehmann oft", "tokens": ["Es", "hilft", "viel\u00b7mehr", "dem", "Eh\u00b7mann", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Kindern, die er nie gehofft.", "tokens": ["Zu", "Kin\u00b7dern", ",", "die", "er", "nie", "ge\u00b7hofft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Roms Krieger reitzten oft die Wuth", "tokens": ["Roms", "Krie\u00b7ger", "reitz\u00b7ten", "oft", "die", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volks zu blut'gem Hader.", "tokens": ["Des", "Volks", "zu", "blut'\u00b7gem", "Ha\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier str\u00f6mte noch kein B\u00fcrgerblut,", "tokens": ["Hier", "str\u00f6m\u00b7te", "noch", "kein", "B\u00fcr\u00b7ger\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als durch die Hand der Bader:", "tokens": ["Als", "durch", "die", "Hand", "der", "Ba\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein g\u00e4her L\u00e4rm, kein Aufstand jagt", "tokens": ["Kein", "g\u00e4\u00b7her", "L\u00e4rm", ",", "kein", "Auf\u00b7stand", "jagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Domherrn aus dem Bett der Magd.", "tokens": ["Den", "Dom\u00b7herrn", "aus", "dem", "Bett", "der", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O w\u00e4re man der Mordbegier", "tokens": ["O", "w\u00e4\u00b7re", "man", "der", "Mord\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verw\u00e4gner Eisenfresser", "tokens": ["Ver\u00b7w\u00e4g\u00b7ner", "Ei\u00b7sen\u00b7fres\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch \u00fcberall so gram, als hier!", "tokens": ["Doch", "\u00fc\u00b7be\u00b7rall", "so", "gram", ",", "als", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gienge wahrlich besser:", "tokens": ["Es", "gien\u00b7ge", "wahr\u00b7lich", "bes\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Froh w\u00fcrde sich Jahr aus Jahr ein", "tokens": ["Froh", "w\u00fcr\u00b7de", "sich", "Jahr", "aus", "Jahr", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PRF", "NN", "APPR", "NN", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die ganze Welt des Friedens freun.", "tokens": ["Die", "gan\u00b7ze", "Welt", "des", "Frie\u00b7dens", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}