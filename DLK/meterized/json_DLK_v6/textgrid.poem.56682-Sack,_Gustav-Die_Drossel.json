{"textgrid.poem.56682": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Drossel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich das Pack zusammenballt!", "tokens": ["Wie", "sich", "das", "Pack", "zu\u00b7sam\u00b7men\u00b7ballt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Indes die Trambahn schrillt und gellt,", "tokens": ["In\u00b7des", "die", "Tram\u00b7bahn", "schrillt", "und", "gellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Musik l\u00e4rmt, die Peitsche knallt,", "tokens": ["die", "Mu\u00b7sik", "l\u00e4rmt", ",", "die", "Peit\u00b7sche", "knallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wie ein Hund das Auto bellt,", "tokens": ["und", "wie", "ein", "Hund", "das", "Au\u00b7to", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "h\u00f6rt keiner sie, die unentwegt", "tokens": ["h\u00f6rt", "kei\u00b7ner", "sie", ",", "die", "un\u00b7ent\u00b7wegt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von einem Dach ihr Fl\u00f6tenlied", "tokens": ["von", "ei\u00b7nem", "Dach", "ihr", "Fl\u00f6\u00b7ten\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vollt\u00f6nend in die L\u00fcfte tr\u00e4gt. \u2013", "tokens": ["voll\u00b7t\u00f6\u00b7nend", "in", "die", "L\u00fcf\u00b7te", "tr\u00e4gt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich das schwitzend, br\u00fcllend m\u00fcht,", "tokens": ["Wie", "sich", "das", "schwit\u00b7zend", ",", "br\u00fcl\u00b7lend", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "wie sich das stier und stumpf vermischt,", "tokens": ["wie", "sich", "das", "stier", "und", "stumpf", "ver\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das str\u00f6mt wie ein verschmutzter Bach,", "tokens": ["das", "str\u00f6mt", "wie", "ein", "ver\u00b7schmutz\u00b7ter", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Abwasserhub und Gassengischt,", "tokens": ["Ab\u00b7was\u00b7ser\u00b7hub", "und", "Gas\u00b7sen\u00b7gischt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "indes von jenem Giebeldach", "tokens": ["in\u00b7des", "von", "je\u00b7nem", "Gie\u00b7bel\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "hoch \u00fcber Kehricht, Staub und Wust", "tokens": ["hoch", "\u00fc\u00b7ber", "Keh\u00b7richt", ",", "Staub", "und", "Wust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des kleinen Gl\u00fccks Melancholie", "tokens": ["des", "klei\u00b7nen", "Gl\u00fccks", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "harmlos und selig unbewu\u00dft", "tokens": ["harm\u00b7los", "und", "se\u00b7lig", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ausstr\u00f6mt aus diesem kleinen Vieh,", "tokens": ["aus\u00b7str\u00f6mt", "aus", "die\u00b7sem", "klei\u00b7nen", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "das hei\u00df und frech und elegant", "tokens": ["das", "hei\u00df", "und", "frech", "und", "e\u00b7le\u00b7gant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sein schwarzes Konterfei poussiert \u2013", "tokens": ["sein", "schwar\u00b7zes", "Kon\u00b7ter\u00b7fei", "pous\u00b7siert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie ich dich hasse, feiner Fant,", "tokens": ["wie", "ich", "dich", "has\u00b7se", ",", "fei\u00b7ner", "Fant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der nichts verlor, der nichts verliert,", "tokens": ["der", "nichts", "ver\u00b7lor", ",", "der", "nichts", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "der nur ein Ding ist, das man spielt,", "tokens": ["der", "nur", "ein", "Ding", "ist", ",", "das", "man", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das nur der Fr\u00fchling musiziert,", "tokens": ["das", "nur", "der", "Fr\u00fch\u00b7ling", "mu\u00b7si\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das sich nicht kennt, das sich nicht f\u00fchlt,", "tokens": ["das", "sich", "nicht", "kennt", ",", "das", "sich", "nicht", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKNEG", "VVFIN", "$,", "PRELS", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das nichts verlor und nichts verliert.", "tokens": ["das", "nichts", "ver\u00b7lor", "und", "nichts", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}