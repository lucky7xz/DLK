{"dta.poem.5696": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Last uns mit Achtsamkeit Sein G\u00f6ttlichs Werck be-", "tokens": ["Last", "uns", "mit", "Acht\u00b7sam\u00b7keit", "Sein", "G\u00f6tt\u00b7lichs", "Werck", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "NN", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "dencken!", "tokens": ["den\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nur h\u00fcte man sich doch, damit ein Flatter-Licht", "tokens": ["Nur", "h\u00fc\u00b7te", "man", "sich", "doch", ",", "da\u00b7mit", "ein", "Flat\u00b7ter\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von falscher Phantasey uns nicht vom Weg ablencken", "tokens": ["Von", "fal\u00b7scher", "Phan\u00b7ta\u00b7sey", "uns", "nicht", "vom", "Weg", "ab\u00b7len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und uns verf\u00fchren mag. Er tr\u00fcget warlich nicht,", "tokens": ["Und", "uns", "ver\u00b7f\u00fch\u00b7ren", "mag.", "Er", "tr\u00fc\u00b7get", "war\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er giebet auch nicht zu, da\u00df die betrogen seyn,", "tokens": ["Er", "gie\u00b7bet", "auch", "nicht", "zu", ",", "da\u00df", "die", "be\u00b7tro\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "ART", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die durch Vernunfft den Brauch der Sinne wol regieren.", "tokens": ["Die", "durch", "Ver\u00b7nunfft", "den", "Brauch", "der", "Sin\u00b7ne", "wol", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es mag sich denn so Furcht als Jrrthum frey verliehren!", "tokens": ["Es", "mag", "sich", "denn", "so", "Furcht", "als", "Jrr\u00b7thum", "frey", "ver\u00b7lieh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "NN", "KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}