{"textgrid.poem.25087": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ringsum nun wird es stille,", "tokens": ["Ring\u00b7sum", "nun", "wird", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Indes der Tag versinkt", "tokens": ["In\u00b7des", "der", "Tag", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und froh im Gras die Grille", "tokens": ["Und", "froh", "im", "Gras", "die", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Tau der D\u00e4mmrung trinkt.", "tokens": ["Den", "Tau", "der", "D\u00e4mm\u00b7rung", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aufsteigt die Nacht im Westen,", "tokens": ["Auf\u00b7steigt", "die", "Nacht", "im", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie atmet h\u00f6rbar kaum", "tokens": ["Sie", "at\u00b7met", "h\u00f6r\u00b7bar", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wiegt von Ast zu Aesten", "tokens": ["Und", "wiegt", "von", "Ast", "zu", "A\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Den Wald in Schlaf und Traum.", "tokens": ["Den", "Wald", "in", "Schlaf", "und", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den V\u00f6geln, wie sie br\u00fcten,", "tokens": ["Den", "V\u00f6\u00b7geln", ",", "wie", "sie", "br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt sie die Augen zu", "tokens": ["Dr\u00fcckt", "sie", "die", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lullt im Thal die Bl\u00fcten,", "tokens": ["Und", "lullt", "im", "Thal", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Aehren all in Ruh.", "tokens": ["Die", "A\u00b7eh\u00b7ren", "all", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, Mutter Nacht, und lege", "tokens": ["Komm", ",", "Mut\u00b7ter", "Nacht", ",", "und", "le\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Hand aufs Herz mir mild,", "tokens": ["Die", "Hand", "aufs", "Herz", "mir", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie die wilden Schl\u00e4ge", "tokens": ["Da\u00df", "sie", "die", "wil\u00b7den", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Ruhelosen stillt!", "tokens": ["Dem", "Ru\u00b7he\u00b7lo\u00b7sen", "stillt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}