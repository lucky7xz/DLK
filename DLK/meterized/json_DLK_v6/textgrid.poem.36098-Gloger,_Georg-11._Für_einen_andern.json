{"textgrid.poem.36098": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "11. F\u00fcr einen andern", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, keinen guten Vers hastu von mir zu hoffen,", "tokens": ["Freund", ",", "kei\u00b7nen", "gu\u00b7ten", "Vers", "has\u00b7tu", "von", "mir", "zu", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn meine Poesie ist gestern mir ersoffen,", "tokens": ["Denn", "mei\u00b7ne", "Poe\u00b7sie", "ist", "ge\u00b7stern", "mir", "er\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So da\u00df ich dir gar nichts zu Ehren schreiben kan;", "tokens": ["So", "da\u00df", "ich", "dir", "gar", "nichts", "zu", "Eh\u00b7ren", "schrei\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "ADV", "PIS", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd was hier steht, das hat ihr Fladdergeist getan.", "tokens": ["Vnd", "was", "hier", "steht", ",", "das", "hat", "ihr", "Flad\u00b7der\u00b7geist", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}