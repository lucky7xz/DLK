", "measure": "dactylic.tri"}, "line.4": {"text": "Verg\u00f6nne, da\u00df ich dich hinweise,", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", ",", "da\u00df", "ich", "dich", "hin\u00b7wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Babilon giengest du.", "tokens": ["Nach", "Ba\u00b7bi\u00b7lon", "gien\u00b7gest", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Es war wohl ein innerlich Sehen", "tokens": ["Es", "war", "wohl", "ein", "in\u00b7ner\u00b7lich", "Se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein innerlich Auferstehen,", "tokens": ["Ein", "in\u00b7ner\u00b7lich", "Auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In mir selber stieg sie herauf", "tokens": ["In", "mir", "sel\u00b7ber", "stieg", "sie", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Leben das waren die Wehen", "tokens": ["Das", "Le\u00b7ben", "das", "wa\u00b7ren", "die", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VAFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das sie geb\u00e4rend gekrei\u00dft.", "tokens": ["Das", "sie", "ge\u00b7b\u00e4\u00b7rend", "ge\u00b7krei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.21": {"line.1": {"text": "Was ich verloren, begraben,", "tokens": ["Was", "ich", "ver\u00b7lo\u00b7ren", ",", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was alles ich um es zu haben", "tokens": ["Was", "al\u00b7les", "ich", "um", "es", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PPER", "APPR", "PPER", "PTKZU", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit hei\u00dfer Sehnsucht gesucht", "tokens": ["Mit", "hei\u00b7\u00dfer", "Sehn\u00b7sucht", "ge\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das sollte mich innerlich laben", "tokens": ["Das", "soll\u00b7te", "mich", "in\u00b7ner\u00b7lich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "In unverbotener Frucht.", "tokens": ["In", "un\u00b7ver\u00b7bo\u00b7te\u00b7ner", "Frucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.22": {"line.1": {"text": "Die Schimmer, die Lichter, die Farben,", "tokens": ["Die", "Schim\u00b7mer", ",", "die", "Lich\u00b7ter", ",", "die", "Far\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Sehnsucht goldene Garben,", "tokens": ["Der", "Sehn\u00b7sucht", "gol\u00b7de\u00b7ne", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Duft die Sonne der Tau", "tokens": ["Der", "Duft", "die", "Son\u00b7ne", "der", "Tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die einzeln erblindet mir starben,", "tokens": ["Die", "ein\u00b7zeln", "er\u00b7blin\u00b7det", "mir", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Gott gr\u00fc\u00df dich mein geistlicher Pfau.", "tokens": ["Gott", "gr\u00fc\u00df", "dich", "mein", "geist\u00b7li\u00b7cher", "Pfau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.23": {"line.1": {"text": "Und alles was je ich gewesen", "tokens": ["Und", "al\u00b7les", "was", "je", "ich", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PWS", "ADV", "PPER", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Konnt' ihr in der Seele ich lesen,", "tokens": ["Konnt'", "ihr", "in", "der", "See\u00b7le", "ich", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Konnt' vor ihr in Tr\u00e4nen vergehn,", "tokens": ["Konnt'", "vor", "ihr", "in", "Tr\u00e4\u00b7nen", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Konnt' vor in Reue genesen,", "tokens": ["Konnt'", "vor", "in", "Reu\u00b7e", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und unschuldig dann auferstehn.", "tokens": ["Und", "un\u00b7schul\u00b7dig", "dann", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Ich komme um dich zu heilen,", "tokens": ["Ich", "kom\u00b7me", "um", "dich", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Herr wohl tausend Meilen,", "tokens": ["Der", "Herr", "wohl", "tau\u00b7send", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu brechen mein Brot mit dir,", "tokens": ["Zu", "bre\u00b7chen", "mein", "Brot", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Becher auch mit dir zu teilen,", "tokens": ["Den", "Be\u00b7cher", "auch", "mit", "dir", "zu", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PT