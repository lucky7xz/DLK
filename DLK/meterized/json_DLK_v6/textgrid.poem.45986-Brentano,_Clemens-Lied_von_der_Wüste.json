{"textgrid.poem.45986": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Lied von der W\u00fcste", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin durch die W\u00fcste gezogen", "tokens": ["Ich", "bin", "durch", "die", "W\u00fcs\u00b7te", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des gl\u00fchenden Sandes Wogen", "tokens": ["Des", "gl\u00fc\u00b7hen\u00b7den", "San\u00b7des", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verbrannten mir den Fu\u00df", "tokens": ["Ver\u00b7brann\u00b7ten", "mir", "den", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Wolken haben gelogen", "tokens": ["Die", "Wol\u00b7ken", "ha\u00b7ben", "ge\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es kam kein Regengu\u00df.", "tokens": ["Es", "kam", "kein", "Re\u00b7gen\u00b7gu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Sonne du trankst im Zorne", "tokens": ["O", "Son\u00b7ne", "du", "trankst", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Wasser aus jeglichem Borne", "tokens": ["Das", "Was\u00b7ser", "aus", "jeg\u00b7li\u00b7chem", "Bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "An dem die Reise ruht,", "tokens": ["An", "dem", "die", "Rei\u00b7se", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich d\u00fcrste, es trinken die Dorne", "tokens": ["Ich", "d\u00fcrs\u00b7te", ",", "es", "trin\u00b7ken", "die", "Dor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mein siedend hei\u00dfes Blut.", "tokens": ["Mein", "sie\u00b7dend", "hei\u00b7\u00dfes", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aus zog ich mit sieben Kamelen,", "tokens": ["Aus", "zog", "ich", "mit", "sie\u00b7ben", "Ka\u00b7me\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Grub Wasser aus ihrer Kehle", "tokens": ["Grub", "Was\u00b7ser", "aus", "ih\u00b7rer", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu retten Weib und Kind,", "tokens": ["Zu", "ret\u00b7ten", "Weib", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sch\u00e4tze an Gold und Juwelen", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "an", "Gold", "und", "Ju\u00b7we\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Begrub im Sande der Wind.", "tokens": ["Be\u00b7grub", "im", "San\u00b7de", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Dann w\u00fchlt' ich mit gl\u00fchendem Schwerde", "tokens": ["Dann", "w\u00fchlt'", "ich", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Kindern ein Grab in die Erde,", "tokens": ["Den", "Kin\u00b7dern", "ein", "Grab", "in", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Grab kein Brunnen ward,", "tokens": ["Das", "Grab", "kein", "Brun\u00b7nen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erw\u00fchlte mir keinen Quell,", "tokens": ["Er\u00b7w\u00fchl\u00b7te", "mir", "kei\u00b7nen", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ob Gott sie wohl finden werde", "tokens": ["Ob", "Gott", "sie", "wohl", "fin\u00b7den", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVINF", "VAFIN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nachts br\u00fcllte die Tigerherde", "tokens": ["Nachts", "br\u00fcll\u00b7te", "die", "Ti\u00b7ger\u00b7her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Sonne brannte so grell.", "tokens": ["Die", "Son\u00b7ne", "brann\u00b7te", "so", "grell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ein Kind, das lag unterm Herzen,", "tokens": ["Ein", "Kind", ",", "das", "lag", "un\u00b7term", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das brach, die Mutter in Schmerzen", "tokens": ["Das", "brach", ",", "die", "Mut\u00b7ter", "in", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gebar es sterbend dem Tod,", "tokens": ["Ge\u00b7bar", "es", "ster\u00b7bend", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es go\u00df gleich gl\u00fchenden Erzen", "tokens": ["Es", "go\u00df", "gleich", "gl\u00fc\u00b7hen\u00b7den", "Er\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Sonne mir Licht in die Not.", "tokens": ["Die", "Son\u00b7ne", "mir", "Licht", "in", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Gern h\u00e4tte ich Tr\u00e4nen getrunken", "tokens": ["Gern", "h\u00e4t\u00b7te", "ich", "Tr\u00e4\u00b7nen", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Augen weinten nur Funken,", "tokens": ["Die", "Au\u00b7gen", "wein\u00b7ten", "nur", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief w\u00fchlt' ich ein Grab in den Sand,", "tokens": ["Tief", "w\u00fchlt'", "ich", "ein", "Grab", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bin jammernd hinein mit gesunken,", "tokens": ["Bin", "jam\u00b7mernd", "hin\u00b7ein", "mit", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "VVPP", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Ach, weil ich kein Wasser fand.", "tokens": ["Ach", ",", "weil", "ich", "kein", "Was\u00b7ser", "fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da ward ich zur wandelnden Leiche", "tokens": ["Da", "ward", "ich", "zur", "wan\u00b7deln\u00b7den", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf da\u00df ich den Brunnen erreiche", "tokens": ["Auf", "da\u00df", "ich", "den", "Brun\u00b7nen", "er\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den letzten auf dieser gl\u00fchender an, [", "tokens": ["Den", "letz\u00b7ten", "auf", "die\u00b7ser", "gl\u00fc\u00b7hen\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "APPR", "PDAT", "ADJA", "PTKVZ", "$,", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wie ich so lechzend hinschleiche", "tokens": ["Und", "wie", "ich", "so", "lech\u00b7zend", "hin\u00b7schlei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da br\u00fcllen die Tiger mich an.", "tokens": ["Da", "br\u00fcl\u00b7len", "die", "Ti\u00b7ger", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Es brannte die gl\u00fchende Schwelle", "tokens": ["Es", "brann\u00b7te", "die", "gl\u00fc\u00b7hen\u00b7de", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Tages, da kam ich zur Stelle,", "tokens": ["Des", "Ta\u00b7ges", ",", "da", "kam", "ich", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Brunnen war drocken und tot,", "tokens": ["Der", "Brun\u00b7nen", "war", "dro\u00b7cken", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da schien bei Mitternacht helle", "tokens": ["Da", "schien", "bei", "Mit\u00b7ter\u00b7nacht", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Mond wie mein Herzblut rot.", "tokens": ["Der", "Mond", "wie", "mein", "Herz\u00b7blut", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Das Ziel, ich f\u00fchlt' es gekommen,", "tokens": ["Das", "Ziel", ",", "ich", "f\u00fchlt'", "es", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die gl\u00fchende Leiter erklommen", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7de", "Lei\u00b7ter", "er\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich schrie zu dem bittern Stern", "tokens": ["Ich", "schrie", "zu", "dem", "bit\u00b7tern", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Herr hat gegeben, genommen", "tokens": ["Der", "Herr", "hat", "ge\u00b7ge\u00b7ben", ",", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Gelobt sei der Wille des Herrn.", "tokens": ["Ge\u00b7lobt", "sei", "der", "Wil\u00b7le", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Der Tod stieg auf aus der W\u00fcste,", "tokens": ["Der", "Tod", "stieg", "auf", "aus", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schauderte, da ich ihn gr\u00fc\u00dfte,", "tokens": ["Und", "schau\u00b7der\u00b7te", ",", "da", "ich", "ihn", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und floh, da rief ich ihm zu,", "tokens": ["Und", "floh", ",", "da", "rief", "ich", "ihm", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da\u00df einer hier sterben m\u00fc\u00dfte,", "tokens": ["Da\u00df", "ei\u00b7ner", "hier", "ster\u00b7ben", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er sprach: nicht sterben kannst du.", "tokens": ["Er", "sprach", ":", "nicht", "ster\u00b7ben", "kannst", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKNEG", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Du kannst nicht sterben nicht leben,", "tokens": ["Du", "kannst", "nicht", "ster\u00b7ben", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die ewige Ruhe nicht erwerben,", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Ru\u00b7he", "nicht", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Durst ist unendlich in dir,", "tokens": ["Der", "Durst", "ist", "un\u00b7end\u00b7lich", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dein Erbteil will ich nicht ererben", "tokens": ["Dein", "Erb\u00b7teil", "will", "ich", "nicht", "er\u00b7er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So sprach er und eilte von mir.", "tokens": ["So", "sprach", "er", "und", "eil\u00b7te", "von", "mir", "."], "toke