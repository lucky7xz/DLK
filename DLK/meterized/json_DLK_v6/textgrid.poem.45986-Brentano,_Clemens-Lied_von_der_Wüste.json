ct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Da kniete ich vor ihr nieder,", "tokens": ["Da", "knie\u00b7te", "ich", "vor", "ihr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie legte ihr tauicht Gefieder", "tokens": ["Sie", "leg\u00b7te", "ihr", "tau\u00b7icht", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wohl k\u00fchl um mein gl\u00fchend Haupt,", "tokens": ["Wohl", "k\u00fchl", "um", "mein", "gl\u00fc\u00b7hend", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sang mir die Pilgerlieder", "tokens": ["Und", "sang", "mir", "die", "Pil\u00b7ger\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da hab' ich geliebt und geglaubt.", "tokens": ["Da", "hab'", "ich", "ge\u00b7liebt", "und", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.18": {"line.1": {"text": "Da sah ich den Himmel wohl offen", "tokens": ["Da", "sah", "ich", "den", "Him\u00b7mel", "wohl", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "lin