{"textgrid.poem.33794": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Alhambra", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlangen br\u00fcten nun und nisten,", "tokens": ["Schlan\u00b7gen", "br\u00fc\u00b7ten", "nun", "und", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo beseelt von hohem Mut", "tokens": ["Wo", "be\u00b7seelt", "von", "ho\u00b7hem", "Mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rache br\u00fctend allen Christen", "tokens": ["Ra\u00b7che", "br\u00fc\u00b7tend", "al\u00b7len", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst des Mauren Stirn geruht.", "tokens": ["Einst", "des", "Mau\u00b7ren", "Stirn", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wo lieblich wie die Sonne", "tokens": ["Und", "wo", "lieb\u00b7lich", "wie", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang sein schwarzgelocktes Kind,", "tokens": ["Sang", "sein", "schwarz\u00b7ge\u00b7lock\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nickt die schwarze Belladonne", "tokens": ["Nickt", "die", "schwar\u00b7ze", "Bel\u00b7la\u00b7don\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcsternd in dem Abendwind.", "tokens": ["Fl\u00fcs\u00b7ternd", "in", "dem", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Araber, ein letzter Schimmer", "tokens": ["A\u00b7ra\u00b7ber", ",", "ein", "letz\u00b7ter", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eurer stolzen Reiche blieb", "tokens": ["Eu\u00b7rer", "stol\u00b7zen", "Rei\u00b7che", "blieb"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am Himmel \u2013 wo noch immer", "tokens": ["Nur", "am", "Him\u00b7mel", "\u2013", "wo", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$(", "PWAV", "ADV", "ADV"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.4": {"text": "Wega gl\u00e4nzt und Algenib!", "tokens": ["We\u00b7ga", "gl\u00e4nzt", "und", "Al\u00b7ge\u00b7nib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}