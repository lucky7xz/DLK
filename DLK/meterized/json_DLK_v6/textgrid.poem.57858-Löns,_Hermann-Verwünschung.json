{"textgrid.poem.57858": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verw\u00fcnschung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir meinen Schatz genommen,", "tokens": ["Du", "hast", "mir", "mei\u00b7nen", "Schatz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So jung und schlank, so jung und schlank,", "tokens": ["So", "jung", "und", "schlank", ",", "so", "jung", "und", "schlank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Daf\u00fcr soll Ungl\u00fcck auf dich kommen,", "tokens": ["Da\u00b7f\u00fcr", "soll", "Un\u00b7gl\u00fcck", "auf", "dich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So breit wie lang, so breit wie lang;", "tokens": ["So", "breit", "wie", "lang", ",", "so", "breit", "wie", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was da lebt in Feuersflamm',", "tokens": ["Was", "da", "lebt", "in", "Feu\u00b7ers\u00b7flamm'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was da klebt am Birkenstamm,", "tokens": ["Was", "da", "klebt", "am", "Bir\u00b7kens\u00b7tamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die Kr\u00f6te tr\u00e4gt im Leib,", "tokens": ["Was", "die", "Kr\u00f6\u00b7te", "tr\u00e4gt", "im", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcnsch' ich dir, du schlechtes Weib.", "tokens": ["W\u00fcn\u00b7sch'", "ich", "dir", ",", "du", "schlech\u00b7tes", "Weib", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Herzen die hast du geschieden,", "tokens": ["Zwei", "Her\u00b7zen", "die", "hast", "du", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So jung und schlank, so jung und schlank,", "tokens": ["So", "jung", "und", "schlank", ",", "so", "jung", "und", "schlank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich nehme dir daf\u00fcr den Frieden,", "tokens": ["Ich", "neh\u00b7me", "dir", "da\u00b7f\u00fcr", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So breit wie lang, so breit wie lang;", "tokens": ["So", "breit", "wie", "lang", ",", "so", "breit", "wie", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was am Kreuzweg geht und steht,", "tokens": ["Was", "am", "Kreuz\u00b7weg", "geht", "und", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was am Galgen winkt und weht,", "tokens": ["Was", "am", "Gal\u00b7gen", "winkt", "und", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die Hexe kocht und braut,", "tokens": ["Was", "die", "He\u00b7xe", "kocht", "und", "braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei dir alles angetraut.", "tokens": ["Sei", "dir", "al\u00b7les", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So lange meine Tr\u00e4nen flie\u00dfen,", "tokens": ["So", "lan\u00b7ge", "mei\u00b7ne", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So jung und schlank, so jung und schlank,", "tokens": ["So", "jung", "und", "schlank", ",", "so", "jung", "und", "schlank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollst du's an Leib und Seele b\u00fc\u00dfen,", "tokens": ["Sollst", "du's", "an", "Leib", "und", "See\u00b7le", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So breit wie lang, so breit wie lang;", "tokens": ["So", "breit", "wie", "lang", ",", "so", "breit", "wie", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ich rief um Mitternacht,", "tokens": ["Was", "ich", "rief", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was zum Kirchhof ich gebracht,", "tokens": ["Was", "zum", "Kirch\u00b7hof", "ich", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ich grub in Mulm und Moos,", "tokens": ["Was", "ich", "grub", "in", "Mulm", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wirst du nun und nimmer los.", "tokens": ["Wirst", "du", "nun", "und", "nim\u00b7mer", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}