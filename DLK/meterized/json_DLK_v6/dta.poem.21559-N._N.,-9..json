{"dta.poem.21559": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Simpeldichter h\u00f6r ich ewig flennen,", "tokens": ["Die", "Sim\u00b7pel\u00b7dich\u00b7ter", "h\u00f6r", "ich", "e\u00b7wig", "flen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie tuten alle in dasselbe Horn", "tokens": ["Sie", "tu\u00b7ten", "al\u00b7le", "in", "das\u00b7sel\u00b7be", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nie packt sie der dreimal heil\u2019ge Zorn,", "tokens": ["Und", "nie", "packt", "sie", "der", "drei\u00b7mal", "heil'\u00b7ge", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie das Elend nur aus B\u00fcchern kennen.", "tokens": ["Weil", "sie", "das", "E\u00b7lend", "nur", "aus", "B\u00fc\u00b7chern", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}