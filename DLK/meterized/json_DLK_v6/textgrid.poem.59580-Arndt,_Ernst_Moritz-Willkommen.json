{"textgrid.poem.59580": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Willkommen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, Freund, am deutschen Strand!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Freund", ",", "am", "deut\u00b7schen", "Strand", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen unter deutschen Eichen!", "tokens": ["Will\u00b7kom\u00b7men", "un\u00b7ter", "deut\u00b7schen", "Ei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen! La\u00df uns Herz und Hand", "tokens": ["Will\u00b7kom\u00b7men", "!", "La\u00df", "uns", "Herz", "und", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum alten Bunde fr\u00f6hlich reichen!", "tokens": ["Zum", "al\u00b7ten", "Bun\u00b7de", "fr\u00f6h\u00b7lich", "rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willkommen! S\u00fc\u00dfer Freudenklang,", "tokens": ["Will\u00b7kom\u00b7men", "!", "S\u00fc\u00b7\u00dfer", "Freu\u00b7den\u00b7klang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du braustest einst an M\u00e4larns Strande,", "tokens": ["Du", "braus\u00b7test", "einst", "an", "M\u00e4\u00b7larns", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sirenen t\u00f6nten drein Gesang,", "tokens": ["Si\u00b7re\u00b7nen", "t\u00f6n\u00b7ten", "drein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und Nymphen schwammen froh zum Lande;", "tokens": ["Und", "Nym\u00b7phen", "schwam\u00b7men", "froh", "zum", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Ulmen tanzten mit uns rund,", "tokens": ["Die", "Ul\u00b7men", "tanz\u00b7ten", "mit", "uns", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle L\u00fcfte spielten Geigen,", "tokens": ["Und", "al\u00b7le", "L\u00fcf\u00b7te", "spiel\u00b7ten", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ganze blaue Sternenrund", "tokens": ["Das", "gan\u00b7ze", "blau\u00b7e", "Ster\u00b7nen\u00b7rund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beriefen wir zu Freudenzeugen.", "tokens": ["Be\u00b7rie\u00b7fen", "wir", "zu", "Freu\u00b7den\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O s\u00fc\u00dfe Zeit, wo flohst du hin?", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Zeit", ",", "wo", "flohst", "du", "hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sitzet heut an jenen Wellen?", "tokens": ["Wer", "sit\u00b7zet", "heut", "an", "je\u00b7nen", "Wel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wem schattet jetzt der Ulmen Gr\u00fcn,", "tokens": ["Wem", "schat\u00b7tet", "jetzt", "der", "Ul\u00b7men", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann Sommersonnen Pfeile schnellen?", "tokens": ["Wann", "Som\u00b7mer\u00b7son\u00b7nen", "Pfei\u00b7le", "schnel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wen ladet jetzt der Birkenhain,", "tokens": ["Wen", "la\u00b7det", "jetzt", "der", "Bir\u00b7ken\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der grauen Eichen dunkles Rauschen", "tokens": ["Der", "grau\u00b7en", "Ei\u00b7chen", "dunk\u00b7les", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu s\u00fc\u00dfen Phantasien ein,", "tokens": ["Zu", "s\u00fc\u00b7\u00dfen", "Phan\u00b7ta\u00b7si\u00b7en", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann Geister durch die Schatten lauschen?", "tokens": ["Wann", "Geis\u00b7ter", "durch", "die", "Schat\u00b7ten", "lau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O rinnet nur, ihr Tr\u00e4nen, rinnt!", "tokens": ["O", "rin\u00b7net", "nur", ",", "ihr", "Tr\u00e4\u00b7nen", ",", "rinnt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Geister ziehen mit den Seelen,", "tokens": ["Die", "Geis\u00b7ter", "zie\u00b7hen", "mit", "den", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da, wo keine Herzen sind,", "tokens": ["Und", "da", ",", "wo", "kei\u00b7ne", "Her\u00b7zen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df die Natur nichts zu erz\u00e4hlen.", "tokens": ["Wei\u00df", "die", "Na\u00b7tur", "nichts", "zu", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So komm und weihe durch dein Herz", "tokens": ["So", "komm", "und", "wei\u00b7he", "durch", "dein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fremde Flur zum Vaterlande!", "tokens": ["Die", "frem\u00b7de", "Flur", "zum", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Leid und Freud', in Lust und Schmerz", "tokens": ["In", "Leid", "und", "Freud'", ",", "in", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir halten fest die alten Bande.", "tokens": ["Wir", "hal\u00b7ten", "fest", "die", "al\u00b7ten", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es drehet Zeit und Welt sich um,", "tokens": ["Es", "dre\u00b7het", "Zeit", "und", "Welt", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menschen und die Sterne wandern;", "tokens": ["Die", "Men\u00b7schen", "und", "die", "Ster\u00b7ne", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier bl\u00fcht uns kein Elysium,", "tokens": ["Hier", "bl\u00fcht", "uns", "kein", "E\u00b7ly\u00b7si\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcck rollt von einem zu dem andern.", "tokens": ["Gl\u00fcck", "rollt", "von", "ei\u00b7nem", "zu", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur eines steht ein Felsenberg,", "tokens": ["Nur", "ei\u00b7nes", "steht", "ein", "Fel\u00b7sen\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nie von seiner St\u00e4tte r\u00fccket,", "tokens": ["Der", "nie", "von", "sei\u00b7ner", "St\u00e4t\u00b7te", "r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz, das nimmer \u00fcberzwerch", "tokens": ["Das", "Herz", ",", "das", "nim\u00b7mer", "\u00fc\u00b7berz\u00b7werch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom graden Pfad der Ehre blicket.", "tokens": ["Vom", "gra\u00b7den", "Pfad", "der", "Eh\u00b7re", "bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Das bleibt der ewig feste Pol,", "tokens": ["Das", "bleibt", "der", "e\u00b7wig", "fes\u00b7te", "Pol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worum die kleinen Dinge rollen:", "tokens": ["Wo\u00b7rum", "die", "klei\u00b7nen", "Din\u00b7ge", "rol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wanket alles leicht und hohl,", "tokens": ["Es", "wan\u00b7ket", "al\u00b7les", "leicht", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht nicht auf festem Grund dein Wollen.", "tokens": ["Steht", "nicht", "auf", "fes\u00b7tem", "Grund", "dein", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Das Gl\u00fcck, das glatt und schl\u00fcpfrig rollt,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "glatt", "und", "schl\u00fcpf\u00b7rig", "rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tauscht in Sekunden seine Pfade,", "tokens": ["Tauscht", "in", "Se\u00b7kun\u00b7den", "sei\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist heute mir, dir morgen hold", "tokens": ["Ist", "heu\u00b7te", "mir", ",", "dir", "mor\u00b7gen", "hold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "$,", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und treibt die Narren rund im Rade.", "tokens": ["Und", "treibt", "die", "Nar\u00b7ren", "rund", "im", "Ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "La\u00df fliehn, was sich nicht halten l\u00e4\u00dft,", "tokens": ["La\u00df", "fliehn", ",", "was", "sich", "nicht", "hal\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den leichten Schmetterling la\u00df schweben", "tokens": ["Den", "leich\u00b7ten", "Schmet\u00b7ter\u00b7ling", "la\u00df", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVIMP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und halte nur dich selber fest:", "tokens": ["Und", "hal\u00b7te", "nur", "dich", "sel\u00b7ber", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du h\u00e4ltst das Schicksal und das Leben.", "tokens": ["Du", "h\u00e4ltst", "das", "Schick\u00b7sal", "und", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Willkommen denn zum Neuen Jahr!", "tokens": ["Will\u00b7kom\u00b7men", "denn", "zum", "Neu\u00b7en", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df uns die Blicke fr\u00f6hlich heben!", "tokens": ["La\u00df", "uns", "die", "Bli\u00b7cke", "fr\u00f6h\u00b7lich", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freundschaft lebe treu und wahr!", "tokens": ["Die", "Freund\u00b7schaft", "le\u00b7be", "treu", "und", "wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freiheit soll am h\u00f6chsten leben!", "tokens": ["Die", "Frei\u00b7heit", "soll", "am", "h\u00f6chs\u00b7ten", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Freiheit ist der Seelen Stahl", "tokens": ["Die", "Frei\u00b7heit", "ist", "der", "See\u00b7len", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ritterliche Wehr der Braven,", "tokens": ["Und", "rit\u00b7ter\u00b7li\u00b7che", "Wehr", "der", "Bra\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter":