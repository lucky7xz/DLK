{"textgrid.poem.59783": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei tapfer! Sei ein Mensch! Du tr\u00e4gst das Zeichen", "tokens": ["Sei", "tap\u00b7fer", "!", "Sei", "ein", "Mensch", "!", "Du", "tr\u00e4gst", "das", "Zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$.", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Gott dir hell gepr\u00e4gt auf hoher Stirne \u2013", "tokens": ["Von", "Gott", "dir", "hell", "ge\u00b7pr\u00e4gt", "auf", "ho\u00b7her", "Stir\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, eben da\u00df ich Mensch bin, jagt die bleichen", "tokens": ["Ja", ",", "e\u00b7ben", "da\u00df", "ich", "Mensch", "bin", ",", "jagt", "die", "blei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "PPER", "NN", "VAFIN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedanken oft mir auf in dem Gehirne.", "tokens": ["Ge\u00b7dan\u00b7ken", "oft", "mir", "auf", "in", "dem", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heut wirbl' ich gleich der Lerche sonnentrunken", "tokens": ["Heut", "wirbl'", "ich", "gleich", "der", "Ler\u00b7che", "son\u00b7nen\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Himmelsliedern fr\u00f6hlich auf zur H\u00f6he,", "tokens": ["Mit", "Him\u00b7mels\u00b7lie\u00b7dern", "fr\u00f6h\u00b7lich", "auf", "zur", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und morgen lieg' ich tief hinabgesunken", "tokens": ["Und", "mor\u00b7gen", "lieg'", "ich", "tief", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00e4chz' aus dumpfem Staub mein Menschenwehe.", "tokens": ["Und", "\u00e4ch\u00b7z'", "aus", "dum\u00b7pfem", "Staub", "mein", "Men\u00b7schen\u00b7we\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O schlimmste Zweiheit, \u00e4ltste Menschenklage!", "tokens": ["O", "schlimms\u00b7te", "Zwei\u00b7heit", ",", "\u00e4lts\u00b7te", "Men\u00b7schen\u00b7kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df nun auch \u00e4ltster Weisheit Spruch dir singen:", "tokens": ["La\u00df", "nun", "auch", "\u00e4lts\u00b7ter", "Weis\u00b7heit", "Spruch", "dir", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADJA", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}