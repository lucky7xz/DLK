{"dta.poem.7022": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOtt! wenn ich den Baum so herrlich prangen sehe,", "tokens": ["Ach", "Gott", "!", "wenn", "ich", "den", "Baum", "so", "herr\u00b7lich", "pran\u00b7gen", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieb, da\u00df es mir zur Lust und Dir zum Ruhm geschehe!", "tokens": ["Gieb", ",", "da\u00df", "es", "mir", "zur", "Lust", "und", "Dir", "zum", "Ruhm", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df meine Seele doch, durch mein ger\u00fchrt Gesicht,", "tokens": ["La\u00df", "mei\u00b7ne", "See\u00b7le", "doch", ",", "durch", "mein", "ge\u00b7r\u00fchrt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "VVPP", "NN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Die Sprache, voll geheimer Lehren,", "tokens": ["Die", "Spra\u00b7che", ",", "voll", "ge\u00b7hei\u00b7mer", "Leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So die Natur best\u00e4ndig mit uns spricht,", "tokens": ["So", "die", "Na\u00b7tur", "be\u00b7st\u00e4n\u00b7dig", "mit", "uns", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dieses Baumes Rede h\u00f6ren:", "tokens": ["Und", "die\u00b7ses", "Bau\u00b7mes", "Re\u00b7de", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}