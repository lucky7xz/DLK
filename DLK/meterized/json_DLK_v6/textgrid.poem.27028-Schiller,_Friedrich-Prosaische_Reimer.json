{"textgrid.poem.27028": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Prosaische Reimer", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieland, wie reich ist dein Geist! Das kann man nun erst empfinden,", "tokens": ["Wie\u00b7land", ",", "wie", "reich", "ist", "dein", "Geist", "!", "Das", "kann", "man", "nun", "erst", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PDS", "VMFIN", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieht man, wie fad und wie leer dein caput mortuum ist.", "tokens": ["Sieht", "man", ",", "wie", "fad", "und", "wie", "leer", "dein", "ca\u00b7put", "mor\u00b7tu\u00b7um", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "PPOSAT", "FM", "FM", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}