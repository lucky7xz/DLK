{"textgrid.poem.47452": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich will bis in die Sterne]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will bis in die Sterne", "tokens": ["Ich", "will", "bis", "in", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fahne der Liebe tragen;", "tokens": ["Die", "Fah\u00b7ne", "der", "Lie\u00b7be", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie soll auf einer Wolke", "tokens": ["Sie", "soll", "auf", "ei\u00b7ner", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob s\u00e4mmtlichen Himmeln ragen.", "tokens": ["Ob", "s\u00e4mmt\u00b7li\u00b7chen", "Him\u00b7meln", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich will im hohen \u00c4ther", "tokens": ["Ich", "will", "im", "ho\u00b7hen", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Anstimmen erhabne Lieder,", "tokens": ["An\u00b7stim\u00b7men", "er\u00b7hab\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will r\u00fchmend eine Pauke", "tokens": ["Will", "r\u00fch\u00b7mend", "ei\u00b7ne", "Pau\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unendlicher Ehre schlagen.", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "Eh\u00b7re", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Orion und Plejade", "tokens": ["O\u00b7rion", "und", "Ple\u00b7ja\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Sie sollen im Tanze kreisen,", "tokens": ["Sie", "sol\u00b7len", "im", "Tan\u00b7ze", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Sohre freudig horchend", "tokens": ["Und", "Soh\u00b7re", "freu\u00b7dig", "hor\u00b7chend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das eigene Spiel vertagen.", "tokens": ["Das", "ei\u00b7ge\u00b7ne", "Spiel", "ver\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Tief unter mir die W\u00fcsten,", "tokens": ["Tief", "un\u00b7ter", "mir", "die", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sandigen, ungeheuern,", "tokens": ["Die", "san\u00b7di\u00b7gen", ",", "un\u00b7ge\u00b7heu\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sollen bl\u00fch'n und gr\u00fcnen", "tokens": ["Sie", "sol\u00b7len", "bl\u00fch'n", "und", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich himmlischen Rosenhagen.", "tokens": ["Gleich", "himm\u00b7li\u00b7schen", "Ro\u00b7sen\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbwarum, Hafis?\u00ab so fragst du.", "tokens": ["\u00bb", "wa\u00b7rum", ",", "Ha\u00b7fis", "?", "\u00ab", "so", "fragst", "du", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "NE", "$.", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie magst du so th\u00f6richt fragen?", "tokens": ["Wie", "magst", "du", "so", "th\u00f6\u00b7richt", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es l\u00e4chelte mir die Freundin,", "tokens": ["Es", "l\u00e4\u00b7chel\u00b7te", "mir", "die", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es endeten alle Klagen.", "tokens": ["Es", "en\u00b7de\u00b7ten", "al\u00b7le", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}