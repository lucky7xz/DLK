{"textgrid.poem.32522": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "IiI. Der Tanzb\u00e4r", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Tanzb\u00e4r war der Kett' entrissen,", "tokens": ["Ein", "Tanz\u00b7b\u00e4r", "war", "der", "Kett'", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam wieder in den Wald zur\u00fcck,", "tokens": ["Kam", "wie\u00b7der", "in", "den", "Wald", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tanzte seiner Schar ein Meisterst\u00fcck", "tokens": ["Und", "tanz\u00b7te", "sei\u00b7ner", "Schar", "ein", "Meis\u00b7ter\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf den gewohnten Hinterf\u00fc\u00dfen.", "tokens": ["Auf", "den", "ge\u00b7wohn\u00b7ten", "Hin\u00b7ter\u00b7f\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbseht, schrie er, das ist Kunst; das lernt man in der Welt.", "tokens": ["\u00bb", "seht", ",", "schrie", "er", ",", "das", "ist", "Kunst", ";", "das", "lernt", "man", "in", "der", "Welt", "."], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "NN", "$.", "PDS", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Tut mir es nach, wenns euch gef\u00e4llt,", "tokens": ["Tut", "mir", "es", "nach", ",", "wenns", "euch", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ihr k\u00f6nnt!\u00ab Geh, brummt ein alter B\u00e4r,", "tokens": ["Und", "wenn", "ihr", "k\u00f6nnt", "!", "\u00ab", "Geh", ",", "brummt", "ein", "al\u00b7ter", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$.", "$(", "NE", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dergleichen Kunst, sie sei so schwer,", "tokens": ["Derg\u00b7lei\u00b7chen", "Kunst", ",", "sie", "sei", "so", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie sei so rar sie sei,", "tokens": ["Sie", "sei", "so", "rar", "sie", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zeigt deinen niedern Geist und deine Sklaverei.", "tokens": ["Zeigt", "dei\u00b7nen", "nie\u00b7dern", "Geist", "und", "dei\u00b7ne", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein gro\u00dfer Hofmann sein,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Hof\u00b7mann", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Mann, dem Schmeichelei und List", "tokens": ["Ein", "Mann", ",", "dem", "Schmei\u00b7che\u00b7lei", "und", "List"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt Witz und Tugend ist;", "tokens": ["Statt", "Witz", "und", "Tu\u00b7gend", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der durch Kabalen steigt, des F\u00fcrsten Gunst erstiehlt,", "tokens": ["Der", "durch", "Ka\u00b7ba\u00b7len", "steigt", ",", "des", "F\u00fcrs\u00b7ten", "Gunst", "er\u00b7stiehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Wort und Schwur als Komplimenten spielt,", "tokens": ["Mit", "Wort", "und", "Schwur", "als", "Kom\u00b7pli\u00b7men\u00b7ten", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein solcher Mann, ein gro\u00dfer Hofmann sein,", "tokens": ["Ein", "sol\u00b7cher", "Mann", ",", "ein", "gro\u00b7\u00dfer", "Hof\u00b7mann", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlie\u00dft das Lob oder Tadel ein?", "tokens": ["Schlie\u00dft", "das", "Lob", "o\u00b7der", "Ta\u00b7del", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}