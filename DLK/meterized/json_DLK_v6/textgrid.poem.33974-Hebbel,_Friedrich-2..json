{"textgrid.poem.33974": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liegt Einer schwer gefangen", "tokens": ["Liegt", "Ei\u00b7ner", "schwer", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In \u00f6der Kerkernacht,", "tokens": ["In", "\u00f6\u00b7der", "Ker\u00b7ker\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So t\u00f6dt' er das Verlangen", "tokens": ["So", "t\u00f6dt'", "er", "das", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Freiheit, wenn's erwacht.", "tokens": ["Nach", "Frei\u00b7heit", ",", "wenn's", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn auch sein ernstes Streben", "tokens": ["Wenn", "auch", "sein", "erns\u00b7tes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zuletzt das Ziel erringt,", "tokens": ["Zu\u00b7letzt", "das", "Ziel", "er\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer giebt ihm Muth und Leben", "tokens": ["Wer", "giebt", "ihm", "Muth", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur\u00fcck, die es verschlingt?", "tokens": ["Zu\u00b7r\u00fcck", ",", "die", "es", "ver\u00b7schlingt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tritt er hinaus in's Freie", "tokens": ["Tritt", "er", "hin\u00b7aus", "in's", "Frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fchlt sich ganz zerst\u00f6rt,", "tokens": ["Und", "f\u00fchlt", "sich", "ganz", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das fr\u00e4gt er sich mit Reue,", "tokens": ["Das", "fr\u00e4gt", "er", "sich", "mit", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warum er sich emp\u00f6rt.", "tokens": ["Wa\u00b7rum", "er", "sich", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und st\u00e4rker, immer st\u00e4rker,", "tokens": ["Und", "st\u00e4r\u00b7ker", ",", "im\u00b7mer", "st\u00e4r\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird er sein eig'ner Feind,", "tokens": ["Wird", "er", "sein", "eig'\u00b7ner", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis ihm zuletzt sein Kerker", "tokens": ["Bis", "ihm", "zu\u00b7letzt", "sein", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als seine Welt erscheint.", "tokens": ["Als", "sei\u00b7ne", "Welt", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie der Gedank' auch brenne,", "tokens": ["Wie", "der", "Ge\u00b7dank'", "auch", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch w\u00fcnsch' ich, menschlich-mild,", "tokens": ["Doch", "w\u00fcn\u00b7sch'", "ich", ",", "menschlich\u00b7mild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Keiner sich erkenne", "tokens": ["Da\u00df", "Kei\u00b7ner", "sich", "er\u00b7ken\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In diesem dunklen Bild.", "tokens": ["In", "die\u00b7sem", "dunk\u00b7len", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die eig'ne Qual wird's d\u00e4mpfen,", "tokens": ["Die", "eig'\u00b7ne", "Qual", "wird's", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ihr es immer wi\u00dft,", "tokens": ["Wenn", "ihr", "es", "im\u00b7mer", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Welch Leben die\u00df mein K\u00e4mpfen", "tokens": ["Welch", "Le\u00b7ben", "die\u00df", "mein", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um eine Grabschrift ist.", "tokens": ["Um", "ei\u00b7ne", "Grab\u00b7schrift", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}