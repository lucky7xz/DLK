{"dta.poem.8356": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Schnekken-Haus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies run dgew\u00f6lbte Haus gef\u00f6rmt von der\nNatur,", "tokens": ["Dies", "run", "dge\u00b7w\u00f6lb\u00b7te", "Haus", "ge\u00b7f\u00f6rmt", "von", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist sehr bequem gemacht vor seine Krea-", "tokens": ["Ist", "sehr", "be\u00b7quem", "ge\u00b7macht", "vor", "sei\u00b7ne", "Krea"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die darin wohnt und lebt; weil sie den", "tokens": ["Die", "da\u00b7rin", "wohnt", "und", "lebt", ";", "weil", "sie", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PAV", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich mit solchen stets genau und fest verbindet.", "tokens": ["Und", "sich", "mit", "sol\u00b7chen", "stets", "ge\u00b7nau", "und", "fest", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist ein sch\u00f6nes Bild an Schnekken zu besehn,", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7nes", "Bild", "an", "Schnek\u00b7ken", "zu", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von solchen die niemahls aus ihren H\u00e4usern", "tokens": ["Von", "sol\u00b7chen", "die", "nie\u00b7mahls", "aus", "ih\u00b7ren", "H\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Mit Sorgfalt das bewahrn, was sie mit Schweis", "tokens": ["Mit", "Sorg\u00b7falt", "das", "be\u00b7wahrn", ",", "was", "sie", "mit", "Schweis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und was aus ihnen selbst gleichsam hervor gedrun-", "tokens": ["Und", "was", "aus", "ih\u00b7nen", "selbst", "gleich\u00b7sam", "her\u00b7vor", "ge\u00b7drun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "ADJD", "PTKVZ", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Schnekke ist ein Bild der n\u00fczen H\u00e4uslig-", "tokens": ["Die", "Schnek\u00b7ke", "ist", "ein", "Bild", "der", "n\u00fc\u00b7zen", "H\u00e4us\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die nie ihr Haus verl\u00e4st, die allemahl bereit", "tokens": ["Die", "nie", "ihr", "Haus", "ver\u00b7l\u00e4st", ",", "die", "al\u00b7le\u00b7mahl", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn etwas dran zerbricht es wieder zu verbes-", "tokens": ["Wenn", "et\u00b7was", "dran", "zer\u00b7bricht", "es", "wie\u00b7der", "zu", "ver\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "VVFIN", "PPER", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die sich stets bem\u00fcht die Wohnung zu ver-", "tokens": ["Und", "die", "sich", "stets", "be\u00b7m\u00fcht", "die", "Woh\u00b7nung", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PRF", "ADV", "VVFIN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie woll steht es um dem, der nach der Schnek-", "tokens": ["Wie", "woll", "steht", "es", "um", "dem", ",", "der", "nach", "der", "Schnek"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sein Eigenthum besch\u00fczt, und der sein Haus be-", "tokens": ["Sein", "Ei\u00b7gen\u00b7thum", "be\u00b7sch\u00fczt", ",", "und", "der", "sein", "Haus", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KON", "ART", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Lernt an den Schnekken dies, wer allenthalben", "tokens": ["Lernt", "an", "den", "Schnek\u00b7ken", "dies", ",", "wer", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PDS", "$,", "PWS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Verliehrt sein Eigenthum, in dem ers nicht be-", "tokens": ["Ver\u00b7liehrt", "sein", "Ei\u00b7gen\u00b7thum", ",", "in", "dem", "ers", "nicht", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "PIS", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}