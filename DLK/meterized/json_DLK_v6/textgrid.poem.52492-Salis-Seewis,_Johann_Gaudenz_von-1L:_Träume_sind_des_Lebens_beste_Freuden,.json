{"textgrid.poem.52492": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4ume sind des Lebens beste Freuden,", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ume sind des Lebens beste Freuden,", "tokens": ["Tr\u00e4u\u00b7me", "sind", "des", "Le\u00b7bens", "bes\u00b7te", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "T\u00e4uschung nur und eitler Flitterschein!", "tokens": ["T\u00e4u\u00b7schung", "nur", "und", "eit\u00b7ler", "Flit\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Drum so will ich mich am Wahne weiden,", "tokens": ["Drum", "so", "will", "ich", "mich", "am", "Wah\u00b7ne", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weder kl\u00fcgeln noch zu weise sein.", "tokens": ["We\u00b7der", "kl\u00fc\u00b7geln", "noch", "zu", "wei\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKA", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Keuscher Liebe heilige Gef\u00fchle", "tokens": ["Keu\u00b7scher", "Lie\u00b7be", "hei\u00b7li\u00b7ge", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Freundschaft festes Seelenband", "tokens": ["Und", "der", "Freund\u00b7schaft", "fes\u00b7tes", "See\u00b7len\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sind des Pilgers Trost in dem Gew\u00fchle", "tokens": ["Sind", "des", "Pil\u00b7gers", "Trost", "in", "dem", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf der Wallfahrt in sein Vaterland.", "tokens": ["Auf", "der", "Wall\u00b7fahrt", "in", "sein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sind es Tr\u00e4ume: O so la\u00dft mich tr\u00e4umen,", "tokens": ["Sind", "es", "Tr\u00e4u\u00b7me", ":", "O", "so", "la\u00dft", "mich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "NE", "ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis im Tode sich mein Auge schlie\u00dft.", "tokens": ["Bis", "im", "To\u00b7de", "sich", "mein", "Au\u00b7ge", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dort erwach' ich, wo bei Lebensb\u00e4umen", "tokens": ["Dort", "er\u00b7wach'", "ich", ",", "wo", "bei", "Le\u00b7bens\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ungetr\u00fcbt der Quell der Freuden flie\u00dft.", "tokens": ["Un\u00b7ge\u00b7tr\u00fcbt", "der", "Quell", "der", "Freu\u00b7den", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}