{"textgrid.poem.26230": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin hat mich ein Traum gebracht?", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin hat mich ein Traum gebracht?", "tokens": ["Wo\u00b7hin", "hat", "mich", "ein", "Traum", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weihnachten nennt ihr diese Nacht!", "tokens": ["Weih\u00b7nach\u00b7ten", "nennt", "ihr", "die\u00b7se", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00d6lpalmen stehen aufgeschlagen", "tokens": ["\u00d6l\u00b7pal\u00b7men", "ste\u00b7hen", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie S\u00e4ulen, die die Sterne tragen.", "tokens": ["Wie", "S\u00e4u\u00b7len", ",", "die", "die", "Ster\u00b7ne", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und W\u00e4rme kommt aus jedem Baum", "tokens": ["Und", "W\u00e4r\u00b7me", "kommt", "aus", "je\u00b7dem", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mond h\u00e4ngt blumenhaft im Raum.", "tokens": ["Der", "Mond", "h\u00e4ngt", "blu\u00b7men\u00b7haft", "im", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Luft durchbebt von Bl\u00fctengasen,", "tokens": ["Die", "Luft", "durch\u00b7bebt", "von", "Bl\u00fc\u00b7ten\u00b7ga\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Fu\u00df geht hin auf weichem Rasen.", "tokens": ["Mein", "Fu\u00df", "geht", "hin", "auf", "wei\u00b7chem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wie ein Geist, dem Schwere fehlt,", "tokens": ["Und", "wie", "ein", "Geist", ",", "dem", "Schwe\u00b7re", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bestaunt mein Herz die Tropenwelt.", "tokens": ["Be\u00b7staunt", "mein", "Herz", "die", "Tro\u00b7pen\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Orchideen dunkel liegen,", "tokens": ["Die", "Or\u00b7chi\u00b7de\u00b7en", "dun\u00b7kel", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umkreist von blanken Feuerfliegen.", "tokens": ["Um\u00b7kreist", "von", "blan\u00b7ken", "Feu\u00b7er\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich taste hin durch diese Nacht,", "tokens": ["Ich", "tas\u00b7te", "hin", "durch", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Heer der Grillen laut bewacht.", "tokens": ["Vom", "Heer", "der", "Gril\u00b7len", "laut", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich suche, kann mich selbst nicht finden", "tokens": ["Ich", "su\u00b7che", ",", "kann", "mich", "selbst", "nicht", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser Weihenacht, der linden.", "tokens": ["In", "die\u00b7ser", "Wei\u00b7he\u00b7nacht", ",", "der", "lin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Laut aus weiter Stille kam.", "tokens": ["Ein", "Laut", "aus", "wei\u00b7ter", "Stil\u00b7le", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir zittern pl\u00f6tzlich Trotz und Gram.", "tokens": ["Mir", "zit\u00b7tern", "pl\u00f6tz\u00b7lich", "Trotz", "und", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es fiel dort hinterm letzten Sterne", "tokens": ["Es", "fiel", "dort", "hin\u00b7term", "letz\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Schlag oder Schu\u00df aus klarer Ferne, \u2013", "tokens": ["Schlag", "o\u00b7der", "Schu\u00df", "aus", "kla\u00b7rer", "Fer\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Es krachte nur ein Palmenblatt,", "tokens": ["Es", "krach\u00b7te", "nur", "ein", "Pal\u00b7men\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das morsch zur Erde st\u00fcrzen tat.", "tokens": ["Das", "morsch", "zur", "Er\u00b7de", "st\u00fcr\u00b7zen", "tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich aber h\u00f6re mehr und schaue", "tokens": ["Ich", "a\u00b7ber", "h\u00f6\u00b7re", "mehr", "und", "schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Felder hin, verschneite, rauhe,", "tokens": ["Auf", "Fel\u00b7der", "hin", ",", "ver\u00b7schnei\u00b7te", ",", "rau\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf wei\u00dfe Wege, eisig glatt.", "tokens": ["Auf", "wei\u00b7\u00dfe", "We\u00b7ge", ",", "ei\u00b7sig", "glatt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond, umraucht, scheint hier nur matt,", "tokens": ["Der", "Mond", ",", "um\u00b7raucht", ",", "scheint", "hier", "nur", "matt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gro\u00dfe schwarze Flecken breiten", "tokens": ["Und", "gro\u00b7\u00dfe", "schwar\u00b7ze", "Fle\u00b7cken", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Schneefeld sich, im himmelweiten.", "tokens": ["Im", "Schnee\u00b7feld", "sich", ",", "im", "him\u00b7mel\u00b7wei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Blut der Br\u00fcder hei\u00df hier flo\u00df.", "tokens": ["Das", "Blut", "der", "Br\u00fc\u00b7der", "hei\u00df", "hier", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Sterbender liegt mir im Scho\u00df.", "tokens": ["Ein", "Ster\u00b7ben\u00b7der", "liegt", "mir", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Er haucht: \u00bbIch melde mich zur Stelle,", "tokens": ["Er", "haucht", ":", "\u00bb", "Ich", "mel\u00b7de", "mich", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Herr Leutnant!\u00ab \u2013 Und Todeshelle", "tokens": ["Herr", "Leut\u00b7nant", "!", "\u00ab", "\u2013", "Und", "To\u00b7des\u00b7hel\u00b7le"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "$(", "$(", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Tritt auf die Stirn. Sein Aug' erstarrt.", "tokens": ["Tritt", "auf", "die", "Stirn", ".", "Sein", "Aug'", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Blick wird wie das Eisfeld hart.", "tokens": ["Sein", "Blick", "wird", "wie", "das", "Eis\u00b7feld", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es ist nur Einer von den Tausend!", "tokens": ["Es", "ist", "nur", "Ei\u00b7ner", "von", "den", "Tau\u00b7send", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Blut schl\u00e4gt mir zur Schl\u00e4fe brausend.", "tokens": ["Mein", "Blut", "schl\u00e4gt", "mir", "zur", "Schl\u00e4\u00b7fe", "brau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl' die Weihenacht vollendet,", "tokens": ["Ich", "f\u00fchl'", "die", "Wei\u00b7he\u00b7nacht", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da so voll Pflicht ein Deutscher endet.", "tokens": ["Da", "so", "voll", "Pflicht", "ein", "Deut\u00b7scher", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}