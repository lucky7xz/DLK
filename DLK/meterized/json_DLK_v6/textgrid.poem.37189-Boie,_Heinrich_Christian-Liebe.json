{"textgrid.poem.37189": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Liebe! Morgenrosen", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", "!", "Mor\u00b7gen\u00b7ro\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Athmen reiner nicht den Duft,", "tokens": ["Ath\u00b7men", "rei\u00b7ner", "nicht", "den", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanfter ihnen liebzukosen", "tokens": ["Sanf\u00b7ter", "ih\u00b7nen", "lieb\u00b7zu\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4chelt Zephyr nicht die Luft.", "tokens": ["F\u00e4\u00b7chelt", "Ze\u00b7phyr", "nicht", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Voller nicht aus krausem Laube", "tokens": ["Vol\u00b7ler", "nicht", "aus", "krau\u00b7sem", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reizt den Durst die Nektartraube,", "tokens": ["Reizt", "den", "Durst", "die", "Nekt\u00b7ar\u00b7trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht so labt der Regen d\u00fcrres Feld,", "tokens": ["Nicht", "so", "labt", "der", "Re\u00b7gen", "d\u00fcr\u00b7res", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Als Ihr Reiz, der mich gefangen h\u00e4lt.", "tokens": ["Als", "Ihr", "Reiz", ",", "der", "mich", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Treuer lenkt des Schiffers Nadel", "tokens": ["Treu\u00b7er", "lenkt", "des", "Schif\u00b7fers", "Na\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht gen Norden seine Fahrt,", "tokens": ["Nicht", "gen", "Nor\u00b7den", "sei\u00b7ne", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fester trotzet Herzensadel", "tokens": ["Fes\u00b7ter", "trot\u00b7zet", "Her\u00b7zen\u00b7sa\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht gefahren jeder Art.", "tokens": ["Nicht", "ge\u00b7fah\u00b7ren", "je\u00b7der", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sichrer fallen nicht und schwellen", "tokens": ["Sich\u00b7rer", "fal\u00b7len", "nicht", "und", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir o Mond die Meereswellen,", "tokens": ["Dir", "o", "Mond", "die", "Mee\u00b7res\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als von Schicksalst\u00fcrmen ungekr\u00e4nkt", "tokens": ["Als", "von", "Schick\u00b7sal\u00b7st\u00fcr\u00b7men", "un\u00b7ge\u00b7kr\u00e4nkt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Nur die Liebe meinen Wandel lenkt.", "tokens": ["Nur", "die", "Lie\u00b7be", "mei\u00b7nen", "Wan\u00b7del", "lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Junger Klee erfreut die L\u00e4mmer,", "tokens": ["Jun\u00b7ger", "Klee", "er\u00b7freut", "die", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bienen s\u00fc\u00dfer Thymian,", "tokens": ["Bie\u00b7nen", "s\u00fc\u00b7\u00dfer", "Thy\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Buchenhains Ged\u00e4mmer", "tokens": ["Durch", "des", "Bu\u00b7chen\u00b7hains", "Ge\u00b7d\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgt ein Hirsch der Hindin Bahn.", "tokens": ["Folgt", "ein", "Hirsch", "der", "Hin\u00b7din", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo des Baches Erlen schatten,", "tokens": ["Wo", "des", "Ba\u00b7ches", "Er\u00b7len", "schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lockt die Nachtigall den Gatten,", "tokens": ["Lockt", "die", "Nach\u00b7ti\u00b7gall", "den", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie gehorchen einem innern Ruf,", "tokens": ["Sie", "ge\u00b7hor\u00b7chen", "ei\u00b7nem", "in\u00b7nern", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ich der Liebe, die Ihr Zauber schuf.", "tokens": ["Ich", "der", "Lie\u00b7be", ",", "die", "Ihr", "Zau\u00b7ber", "schuf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wandelbar in stetem Kreise", "tokens": ["Wan\u00b7del\u00b7bar", "in", "ste\u00b7tem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt der Jahreszeiten Lauf,", "tokens": ["Rollt", "der", "Jah\u00b7res\u00b7zei\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus zergangnem Wintereise", "tokens": ["Aus", "zer\u00b7gang\u00b7nem", "Win\u00b7ter\u00b7ei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchn des Lenzes Glocken auf.", "tokens": ["Bl\u00fchn", "des", "Len\u00b7zes", "Glo\u00b7cken", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der Sommer reift und r\u00f6tet,", "tokens": ["Was", "der", "Som\u00b7mer", "reift", "und", "r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinkt vom falben Herbst get\u00f6dtet;", "tokens": ["Sinkt", "vom", "fal\u00b7ben", "Herbst", "ge\u00b7t\u00f6d\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe ha\u00dft den Wechsel der Natur,", "tokens": ["Lie\u00b7be", "ha\u00dft", "den", "Wech\u00b7sel", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Unverwelklich lacht ihr Fr\u00fchling nur.", "tokens": ["Un\u00b7ver\u00b7wel\u00b7klich", "lacht", "ihr", "Fr\u00fch\u00b7ling", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wie ein S\u00e4useln \u00fcber Halmen", "tokens": ["Wie", "ein", "S\u00e4u\u00b7seln", "\u00fc\u00b7ber", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt die Zeit der Cedern Stolz,", "tokens": ["Beugt", "die", "Zeit", "der", "Ce\u00b7dern", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Marmortempel zu zermalmen", "tokens": ["Mar\u00b7mor\u00b7tem\u00b7pel", "zu", "zer\u00b7mal\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NE", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Droht ihr Zahn gleich d\u00fcrrem Holz.", "tokens": ["Droht", "ihr", "Zahn", "gleich", "d\u00fcr\u00b7rem", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wenn jede Kraft ihr weichet,", "tokens": ["Doch", "wenn", "je\u00b7de", "Kraft", "ihr", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Felsen sie dem Boden gleichet,", "tokens": ["Fel\u00b7sen", "sie", "dem", "Bo\u00b7den", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles unter ihrem Fu\u00dftritt schwankt,", "tokens": ["Al\u00b7les", "un\u00b7ter", "ih\u00b7rem", "Fu\u00df\u00b7tritt", "schwankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Hat selbst ihr doch Liebe nicht gewankt.", "tokens": ["Hat", "selbst", "ihr", "doch", "Lie\u00b7be", "nicht", "ge\u00b7wankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Einzig nur aus diesem Leben", "tokens": ["Ein\u00b7zig", "nur", "aus", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann des Todes linde Hand,", "tokens": ["Kann", "des", "To\u00b7des", "lin\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blutet gleich das Herz, sie heben", "tokens": ["Blu\u00b7tet", "gleich", "das", "Herz", ",", "sie", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ihr be\u00dfres Vaterland.", "tokens": ["In", "ihr", "be\u00df\u00b7res", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo bei Seelen, die hienieden", "tokens": ["Wo", "bei", "See\u00b7len", ",", "die", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lebten liebten litten schieden,", "tokens": ["Leb\u00b7ten", "lieb\u00b7ten", "lit\u00b7ten", "schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie des Erdengl\u00fccks kaum mehr gedenkt", "tokens": ["Sie", "des", "Er\u00b7den\u00b7gl\u00fccks", "kaum", "mehr", "ge\u00b7denkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und kein Jammer unsrer Welt sie kr\u00e4nkt.", "tokens": ["Und", "kein", "Jam\u00b7mer", "uns\u00b7rer", "Welt", "sie", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Liebe wie die Seel' entstammet", "tokens": ["Lie\u00b7be", "wie", "die", "Seel'", "ent\u00b7stam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einem Himmel, Gottes Hauch,", "tokens": ["Ei\u00b7nem", "Him\u00b7mel", ",", "Got\u00b7tes", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Sch\u00f6pfers Odem flammet", "tokens": ["Ei\u00b7nes", "Sch\u00f6p\u00b7fers", "O\u00b7dem", "flam\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Zwillingsschwestern auch.", "tokens": ["In", "den", "Zwil\u00b7lings\u00b7schwes\u00b7tern", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort am Born der Seligkeiten", "tokens": ["Dort", "am", "Born", "der", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Huldigen, wann nun der Zeiten", "tokens": ["Hul\u00b7di\u00b7gen", ",", "wann", "nun", "der", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "---++-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und des Todes lezter Ruf verhallt,", "tokens": ["Und", "des", "To\u00b7des", "lez\u00b7ter", "Ruf", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Reine Geister ihrer Allgewalt.", "tokens": ["Rei\u00b7ne", "Geis\u00b7ter", "ih\u00b7rer", "All\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}