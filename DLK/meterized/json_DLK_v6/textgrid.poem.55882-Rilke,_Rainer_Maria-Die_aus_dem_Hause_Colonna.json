{"textgrid.poem.55882": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die aus dem Hause Colonna", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr fremden M\u00e4nner, die ihr jetzt so still", "tokens": ["Ihr", "frem\u00b7den", "M\u00e4n\u00b7ner", ",", "die", "ihr", "jetzt", "so", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in Bildern steht, ihr sa\u00dfet gut zu Pferde", "tokens": ["in", "Bil\u00b7dern", "steht", ",", "ihr", "sa\u00b7\u00dfet", "gut", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und ungeduldig gingt ihr durch das Haus;", "tokens": ["und", "un\u00b7ge\u00b7dul\u00b7dig", "gingt", "ihr", "durch", "das", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie ein sch\u00f6ner Hund, mit derselben Geb\u00e4rde", "tokens": ["wie", "ein", "sch\u00f6\u00b7ner", "Hund", ",", "mit", "der\u00b7sel\u00b7ben", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "ruhn eure H\u00e4nde jetzt bei euch aus.", "tokens": ["ruhn", "eu\u00b7re", "H\u00e4n\u00b7de", "jetzt", "bei", "euch", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Euer Gesicht ist so voll von Schauen,", "tokens": ["Eu\u00b7er", "Ge\u00b7sicht", "ist", "so", "voll", "von", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "denn die Welt war euch Bild und Bild;", "tokens": ["denn", "die", "Welt", "war", "euch", "Bild", "und", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "aus Waffen, Fahnen, Fr\u00fcchten und Frauen", "tokens": ["aus", "Waf\u00b7fen", ",", "Fah\u00b7nen", ",", "Fr\u00fcch\u00b7ten", "und", "Frau\u00b7en"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "quillt euch dieses gro\u00dfe Vertrauen,", "tokens": ["quillt", "euch", "die\u00b7ses", "gro\u00b7\u00dfe", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "da\u00df alles ", "tokens": ["da\u00df", "al\u00b7les"], "token_info": ["word", "word"], "pos": ["KOUS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Aber damals, als ihr noch zu jung", "tokens": ["A\u00b7ber", "da\u00b7mals", ",", "als", "ihr", "noch", "zu", "jung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKA", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wart, die gro\u00dfen Schlachten zu schlagen,", "tokens": ["wart", ",", "die", "gro\u00b7\u00dfen", "Schlach\u00b7ten", "zu", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "zu jung, um den p\u00e4pstlichen Purpur zu tragen,", "tokens": ["zu", "jung", ",", "um", "den", "p\u00e4pst\u00b7li\u00b7chen", "Pur\u00b7pur", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "nicht immer gl\u00fccklich bei Reiten und Jagen,", "tokens": ["nicht", "im\u00b7mer", "gl\u00fcck\u00b7lich", "bei", "Rei\u00b7ten", "und", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Knaben noch, die sich den Frauen versagen,", "tokens": ["Kna\u00b7ben", "noch", ",", "die", "sich", "den", "Frau\u00b7en", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "habt ihr aus jenen Knabentagen", "tokens": ["habt", "ihr", "aus", "je\u00b7nen", "Kna\u00b7ben\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "keine, nicht eine Erinnerung?", "tokens": ["kei\u00b7ne", ",", "nicht", "ei\u00b7ne", "E\u00b7rin\u00b7ne\u00b7rung", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wi\u00dft ihr nicht mehr, was damals war?", "tokens": ["Wi\u00dft", "ihr", "nicht", "mehr", ",", "was", "da\u00b7mals", "war", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Damals war der Altar", "tokens": ["Da\u00b7mals", "war", "der", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "mit dem Bilde, auf dem Maria gebar,", "tokens": ["mit", "dem", "Bil\u00b7de", ",", "auf", "dem", "Ma\u00b7ria", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NE", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "in dem einsamen Seitenschiff.", "tokens": ["in", "dem", "ein\u00b7sa\u00b7men", "Sei\u00b7ten\u00b7schiff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Euch ergriff", "tokens": ["Euch", "er\u00b7griff"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "eine Blumenranke;", "tokens": ["ei\u00b7ne", "Blu\u00b7men\u00b7ran\u00b7ke", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "der Gedanke,", "tokens": ["der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "da\u00df die Font\u00e4ne allein", "tokens": ["da\u00df", "die", "Fon\u00b7t\u00e4\u00b7ne", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "drau\u00dfen im Garten in Mondenschein", "tokens": ["drau\u00b7\u00dfen", "im", "Gar\u00b7ten", "in", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "ihre Wasser warf,", "tokens": ["ih\u00b7re", "Was\u00b7ser", "warf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "war wie eine Welt.", "tokens": ["war", "wie", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Das Fenster ging bis zu den F\u00fc\u00dfen auf wie eine T\u00fcr;", "tokens": ["Das", "Fens\u00b7ter", "ging", "bis", "zu", "den", "F\u00fc\u00b7\u00dfen", "auf", "wie", "ei\u00b7ne", "T\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "und es war Park mit Wiesen und Wegen:", "tokens": ["und", "es", "war", "Park", "mit", "Wie\u00b7sen", "und", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "seltsam nah und doch so entlegen,", "tokens": ["selt\u00b7sam", "nah", "und", "doch", "so", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "seltsam hell und doch wie verborgen,", "tokens": ["selt\u00b7sam", "hell", "und", "doch", "wie", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "KOKOM", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "und die Brunnen rauschten wie Regen,", "tokens": ["und", "die", "Brun\u00b7nen", "rauschten", "wie", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und es war, als k\u00e4me kein Morgen", "tokens": ["und", "es", "war", ",", "als", "k\u00e4\u00b7me", "kein", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "VVFIN", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "dieser langen Nacht entgegen,", "tokens": ["die\u00b7ser", "lan\u00b7gen", "Nacht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "die mit allen Sternen stand.", "tokens": ["die", "mit", "al\u00b7len", "Ster\u00b7nen", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Damals wuchs euch, Knaben, die Hand,", "tokens": ["Da\u00b7mals", "wuchs", "euch", ",", "Kna\u00b7ben", ",", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "die warm war. (Ihr aber wu\u00dftet es nicht.)", "tokens": ["die", "warm", "war", ".", "(", "Ihr", "a\u00b7ber", "wu\u00df\u00b7tet", "es", "nicht", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$.", "$(", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Damals breitete euer Gesicht sich aus.", "tokens": ["Da\u00b7mals", "brei\u00b7te\u00b7te", "eu\u00b7er", "Ge\u00b7sicht", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}