{"dta.poem.10934": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je mehr ich meines Sch\u00f6pfers Werke,", "tokens": ["Je", "mehr", "ich", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betracht und allenthalben merke:", "tokens": ["Be\u00b7tracht", "und", "al\u00b7len\u00b7thal\u00b7ben", "mer\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Je mehr ergetzt mich ihre Pracht.", "tokens": ["Je", "mehr", "er\u00b7getzt", "mich", "ih\u00b7re", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lobe deun, von Lust ger\u00fchret,", "tokens": ["Ich", "lo\u00b7be", "deun", ",", "von", "Lust", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den, dessen Huld sie so gezieret,", "tokens": ["Den", ",", "des\u00b7sen", "Huld", "sie", "so", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und preise Seine Wunder-Macht.", "tokens": ["Und", "prei\u00b7se", "Sei\u00b7ne", "Wun\u00b7der\u00b7Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}