{"textgrid.poem.26103": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blitzfeuer fuhr senkrecht zur Maiennacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blitzfeuer fuhr senkrecht zur Maiennacht,", "tokens": ["Blitz\u00b7feu\u00b7er", "fuhr", "sen\u00b7krecht", "zur", "Mai\u00b7en\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Als w\u00fcrde die Welt flammend niedergemacht.", "tokens": ["Als", "w\u00fcr\u00b7de", "die", "Welt", "flam\u00b7mend", "nie\u00b7der\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die W\u00e4nde entflohen hell aus der Stub',", "tokens": ["Die", "W\u00e4n\u00b7de", "ent\u00b7flo\u00b7hen", "hell", "aus", "der", "Stub'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis der Blitz sich im dr\u00f6hnenden Keller vergrub.", "tokens": ["Bis", "der", "Blitz", "sich", "im", "dr\u00f6h\u00b7nen\u00b7den", "Kel\u00b7ler", "ver\u00b7grub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Sein Strahl totbleich am Bett hinstrich,", "tokens": ["Sein", "Strahl", "tot\u00b7bleich", "am", "Bett", "hin\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All unser Pulsblut mit ihm entwich.", "tokens": ["All", "un\u00b7ser", "Puls\u00b7blut", "mit", "ihm", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie eine Schmiede erklang das Gemach,", "tokens": ["Wie", "ei\u00b7ne", "Schmie\u00b7de", "er\u00b7klang", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Drin sa\u00dfen wir beide wei\u00dfleuchtend wach", "tokens": ["Drin", "sa\u00b7\u00dfen", "wir", "bei\u00b7de", "wei\u00df\u00b7leuch\u00b7tend", "wach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und horchten der Leidenschaft Schmiedesang nach.", "tokens": ["Und", "horch\u00b7ten", "der", "Lei\u00b7den\u00b7schaft", "Schmie\u00b7de\u00b7sang", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}}}}}