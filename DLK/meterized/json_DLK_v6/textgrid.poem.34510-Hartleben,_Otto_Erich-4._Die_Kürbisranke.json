{"textgrid.poem.34510": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "4. Die K\u00fcrbisranke", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ergrimmte Jona tief im Herzen", "tokens": ["Da", "er\u00b7grimm\u00b7te", "Jo\u00b7na", "tief", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und er betete zu Gott und grollte:", "tokens": ["und", "er", "be\u00b7te\u00b7te", "zu", "Gott", "und", "groll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh, das war's, was ich zu mir gesprochen,", "tokens": ["Sieh", ",", "das", "wa\u00b7r's", ",", "was", "ich", "zu", "mir", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da ich noch in meinem Lande wohnte:", "tokens": ["da", "ich", "noch", "in", "mei\u00b7nem", "Lan\u00b7de", "wohn\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "allzuoft emp\u00f6rte deine G\u00fcte", "tokens": ["all\u00b7zu\u00b7oft", "em\u00b7p\u00f6r\u00b7te", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "mein gerechtes Herz, da ich noch Kind war.", "tokens": ["mein", "ge\u00b7rech\u00b7tes", "Herz", ",", "da", "ich", "noch", "Kind", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Darum h\u00f6rt ich nicht auf deine Stimme,", "tokens": ["Da\u00b7rum", "h\u00f6rt", "ich", "nicht", "auf", "dei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "dachte, Herr, vor dir aufs Meer zu fl\u00fcchten:", "tokens": ["dach\u00b7te", ",", "Herr", ",", "vor", "dir", "aufs", "Meer", "zu", "fl\u00fcch\u00b7ten", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "denn ich weiss, du bist barmherzig, gn\u00e4dig", "tokens": ["denn", "ich", "weiss", ",", "du", "bist", "barm\u00b7her\u00b7zig", ",", "gn\u00e4\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und von grosser, allzulanger G\u00fcte,", "tokens": ["und", "von", "gros\u00b7ser", ",", "all\u00b7zu\u00b7lan\u00b7ger", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "und des \u00dcbels, das du schon verh\u00e4ngtest,", "tokens": ["und", "des", "\u00dc\u00b7bels", ",", "das", "du", "schon", "ver\u00b7h\u00e4ng\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "l\u00e4ssest du dich reun! \u2013 So nimm, o Herr, denn", "tokens": ["l\u00e4s\u00b7sest", "du", "dich", "reun", "!", "\u2013", "So", "nimm", ",", "o", "Herr", ",", "denn"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$.", "$(", "ADV", "VVIMP", "$,", "FM", "NN", "$,", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "meine Seele von mir! Meine Augen", "tokens": ["mei\u00b7ne", "See\u00b7le", "von", "mir", "!", "Mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "wollen diesem Volke deine Gnade", "tokens": ["wol\u00b7len", "die\u00b7sem", "Vol\u00b7ke", "dei\u00b7ne", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "nimmer g\u00f6nnen \u2013 lieber will ich sterben,", "tokens": ["nim\u00b7mer", "g\u00f6n\u00b7nen", "\u2013", "lie\u00b7ber", "will", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "als vor seinen S\u00fcnden weiter leben!", "tokens": ["als", "vor", "sei\u00b7nen", "S\u00fcn\u00b7den", "wei\u00b7ter", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und er wandte sich und ging von dannen.", "tokens": ["Und", "er", "wand\u00b7te", "sich", "und", "ging", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Morgenw\u00e4rts der Stadt, auf einem H\u00fcgel", "tokens": ["Mor\u00b7gen\u00b7w\u00e4rts", "der", "Stadt", ",", "auf", "ei\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hielt er Rast und baute eine H\u00fctte \u2013", "tokens": ["hielt", "er", "Rast", "und", "bau\u00b7te", "ei\u00b7ne", "H\u00fct\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "setzte sich davor und sah hinunter:", "tokens": ["setz\u00b7te", "sich", "da\u00b7vor", "und", "sah", "hin\u00b7un\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was der Stadt wohl widerfahren w\u00fcrde.", "tokens": ["Was", "der", "Stadt", "wohl", "wi\u00b7der\u00b7fah\u00b7ren", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch der Herr verschaffte einen K\u00fcrbis,", "tokens": ["Doch", "der", "Herr", "ver\u00b7schaff\u00b7te", "ei\u00b7nen", "K\u00fcr\u00b7bis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "der wuchs \u00fcber Jona, dass er Schatten", "tokens": ["der", "wuchs", "\u00fc\u00b7ber", "Jo\u00b7na", ",", "dass", "er", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "NE", "$,", "KOUS", "PPER", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "gab ob seinem Haupt und vor der Sonne", "tokens": ["gab", "ob", "sei\u00b7nem", "Haupt", "und", "vor", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "gl\u00fchendem Leid den Scheitel ihm bewahrte.", "tokens": ["gl\u00fc\u00b7hen\u00b7dem", "Leid", "den", "Schei\u00b7tel", "ihm", "be\u00b7wahr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Jona freute sich der grossen Bl\u00e4tter", "tokens": ["Jo\u00b7na", "freu\u00b7te", "sich", "der", "gros\u00b7sen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "und entschlief ersch\u00f6pft in ihrem Schutze.", "tokens": ["und", "ent\u00b7schlief", "er\u00b7sch\u00f6pft", "in", "ih\u00b7rem", "Schut\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als jedoch die Morgenr\u00f6the anbrach,", "tokens": ["Als", "je\u00b7doch", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "an\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "hiess Gott einen Wurm, den K\u00fcrbis stechen,", "tokens": ["hiess", "Gott", "ei\u00b7nen", "Wurm", ",", "den", "K\u00fcr\u00b7bis", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "also, dass er hinsank und verdorrte,", "tokens": ["al\u00b7so", ",", "dass", "er", "hin\u00b7sank", "und", "ver\u00b7dorr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und ein d\u00fcrrer Ost, vom Herrn gesendet,", "tokens": ["und", "ein", "d\u00fcr\u00b7rer", "Ost", ",", "vom", "Herrn", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "riss die welken Bl\u00e4tter bald von hinnen.", "tokens": ["riss", "die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "bald", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da die Sonne vollends nun emporstieg,", "tokens": ["Da", "die", "Son\u00b7ne", "vol\u00b7lends", "nun", "em\u00b7por\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "stach sie Jona auf das Haupt, so dass er", "tokens": ["stach", "sie", "Jo\u00b7na", "auf", "das", "Haupt", ",", "so", "dass", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "APPR", "ART", "NN", "$,", "ADV", "KOUS", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "matt an Leib und Seele ward. Er w\u00fcnschte", "tokens": ["matt", "an", "Leib", "und", "See\u00b7le", "ward", ".", "Er", "w\u00fcnschte"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VAFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "sich den Tod herbei und rief zum Herren:", "tokens": ["sich", "den", "Tod", "her\u00b7bei", "und", "rief", "zum", "Her\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Lass mich l\u00e4nger nicht im Unrecht leben!", "tokens": ["Lass", "mich", "l\u00e4n\u00b7ger", "nicht", "im", "Un\u00b7recht", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Da geschah des Herren Wort zu Jona:", "tokens": ["Da", "ge\u00b7schah", "des", "Her\u00b7ren", "Wort", "zu", "Jo\u00b7na", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Meinst du \u2013 billig z\u00fcrnst du um den K\u00fcrbis,", "tokens": ["Meinst", "du", "\u2013", "bil\u00b7lig", "z\u00fcrnst", "du", "um", "den", "K\u00fcr\u00b7bis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "den du nicht gemacht hast, noch gezogen,", "tokens": ["den", "du", "nicht", "ge\u00b7macht", "hast", ",", "noch", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "der in einer Nacht erwuchs und welkte?", "tokens": ["der", "in", "ei\u00b7ner", "Nacht", "er\u00b7wuchs", "und", "welk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sieh, dich jammert seiner kurzen Bl\u00fcthe \u2013", "tokens": ["Sieh", ",", "dich", "jam\u00b7mert", "sei\u00b7ner", "kur\u00b7zen", "Bl\u00fc\u00b7the", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "weil sie deinem Haupte Schutz erwiesen \u2013", "tokens": ["weil", "sie", "dei\u00b7nem", "Haup\u00b7te", "Schutz", "er\u00b7wie\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und mich sollte Ninives nicht jammern,", "tokens": ["und", "mich", "soll\u00b7te", "Ni\u00b7ni\u00b7ves", "nicht", "jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "solcher grossen Stadt, darinnen mehr denn", "tokens": ["sol\u00b7cher", "gros\u00b7sen", "Stadt", ",", "da\u00b7rin\u00b7nen", "mehr", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "hunderttausend Menschen, die nicht wissen,", "tokens": ["hun\u00b7dert\u00b7tau\u00b7send", "Men\u00b7schen", ",", "die", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "was sie gut und b\u00f6se nennen sollen?", "tokens": ["was", "sie", "gut", "und", "b\u00f6\u00b7se", "nen\u00b7nen", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber Jona gab dem Herrn zur Antwort:", "tokens": ["A\u00b7ber", "Jo\u00b7na", "gab", "dem", "Herrn", "zur", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "meas