{"textgrid.poem.64740": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "368. Das Marienbild", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Wundern, die ich that, bleibt jetzo keine Spur;", "tokens": ["Von", "Wun\u00b7dern", ",", "die", "ich", "that", ",", "bleibt", "jet\u00b7zo", "kei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, die hier andachtsvoll als dein Geschenk mich sehn,", "tokens": ["Doch", ",", "die", "hier", "an\u00b7dachts\u00b7voll", "als", "dein", "Ge\u00b7schenk", "mich", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Inbr\u00fcnstig beten sie auch f\u00fcr dein Wohlergehn;", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "be\u00b7ten", "sie", "auch", "f\u00fcr", "dein", "Woh\u00b7ler\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ist nicht Wunder, nur Natur.", "tokens": ["Das", "ist", "nicht", "Wun\u00b7der", ",", "nur", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}