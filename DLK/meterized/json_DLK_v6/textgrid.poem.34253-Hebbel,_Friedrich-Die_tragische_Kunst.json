{"textgrid.poem.34253": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die tragische Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl soll die Kunst euch stets erfreu'n,", "tokens": ["Wohl", "soll", "die", "Kunst", "euch", "stets", "er\u00b7freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst durch das blut'ge Trauerspiel,", "tokens": ["Selbst", "durch", "das", "blut'\u00b7ge", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur m\u00fc\u00dft ihr nicht das Mittel scheu'n,", "tokens": ["Nur", "m\u00fc\u00dft", "ihr", "nicht", "das", "Mit\u00b7tel", "scheu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch das sie's hier erreicht, dies Ziel.", "tokens": ["Durch", "das", "sie's", "hier", "er\u00b7reicht", ",", "dies", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne lacht euch ohne sie,", "tokens": ["Die", "Son\u00b7ne", "lacht", "euch", "oh\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch ohne sie das Morgenroth,", "tokens": ["Euch", "oh\u00b7ne", "sie", "das", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein der Schmerz erquickt euch nie,", "tokens": ["Al\u00b7lein", "der", "Schmerz", "er\u00b7quickt", "euch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nie der Tod, der bitt're Tod.", "tokens": ["Und", "nie", "der", "Tod", ",", "der", "bitt'\u00b7re", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie n\u00f6thigt Beide, es zu thun,", "tokens": ["Sie", "n\u00f6\u00b7thigt", "Bei\u00b7de", ",", "es", "zu", "thun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie f\u00fchrt sie nah' genug heran,", "tokens": ["Sie", "f\u00fchrt", "sie", "nah'", "ge\u00b7nug", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df keine Kraft in euch mehr ruh'n,", "tokens": ["Da\u00df", "kei\u00b7ne", "Kraft", "in", "euch", "mehr", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df jede sich nur steigern kann;", "tokens": ["Da\u00df", "je\u00b7de", "sich", "nur", "stei\u00b7gern", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie h\u00e4lt sie dennoch fern genug,", "tokens": ["Sie", "h\u00e4lt", "sie", "den\u00b7noch", "fern", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df euch ihr Stachel nicht verletzt,", "tokens": ["Da\u00df", "euch", "ihr", "Sta\u00b7chel", "nicht", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und da\u00df nur, wer schon selbst dem Fluch", "tokens": ["Und", "da\u00df", "nur", ",", "wer", "schon", "selbst", "dem", "Fluch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "$,", "PWS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verfallen ist, sich noch entsetzt.", "tokens": ["Ver\u00b7fal\u00b7len", "ist", ",", "sich", "noch", "ent\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Verkehrt sie denn mit Tod und Schmerz,", "tokens": ["Ver\u00b7kehrt", "sie", "denn", "mit", "Tod", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So thut sie's, stiller Hoffnung voll,", "tokens": ["So", "thut", "sie's", ",", "stil\u00b7ler", "Hoff\u00b7nung", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df eben dadurch euer Herz,", "tokens": ["Da\u00df", "e\u00b7ben", "da\u00b7durch", "eu\u00b7er", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie nie, von Leben schwellen soll,", "tokens": ["Wie", "nie", ",", "von", "Le\u00b7ben", "schwel\u00b7len", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und das ein einziger Genu\u00df,", "tokens": ["Und", "das", "ein", "ein\u00b7zi\u00b7ger", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Wie keine Lust ihn euch gew\u00e4hrt,", "tokens": ["Wie", "kei\u00b7ne", "Lust", "ihn", "euch", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Euch Seel' und Sinn erfrischen mu\u00df,", "tokens": ["Euch", "Seel'", "und", "Sinn", "er\u00b7fri\u00b7schen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn sie das Grauen selbst verkl\u00e4rt.", "tokens": ["Wenn", "sie", "das", "Grau\u00b7en", "selbst", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}