{"textgrid.poem.42917": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Madonnengesichter", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer zu ertragen", "tokens": ["Schwer", "zu", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist Dummheit, wenn sie verschlagen", "tokens": ["Ist", "Dumm\u00b7heit", ",", "wenn", "sie", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist oder sich \u00fcberhebt.", "tokens": ["Ist", "o\u00b7der", "sich", "\u00fc\u00b7ber\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "PRF", "VVPP", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Aber im Grunde der Dummheit lebt", "tokens": ["A\u00b7ber", "im", "Grun\u00b7de", "der", "Dumm\u00b7heit", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Das wehrlos Naive.", "tokens": ["Das", "wehr\u00b7los", "Nai\u00b7ve", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Dummheit sch\u00f6ne Tiefe", "tokens": ["Der", "Dumm\u00b7heit", "sch\u00f6\u00b7ne", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist kein Loch.", "tokens": ["Ist", "kein", "Loch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hat sie doch", "tokens": ["Hat", "sie", "doch"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Keinen richtigen Rand", "tokens": ["Kei\u00b7nen", "rich\u00b7ti\u00b7gen", "Rand"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie etwa L\u00f6cher in Strumpf, Fl\u00f6te, Sand.", "tokens": ["Wie", "et\u00b7wa", "L\u00f6\u00b7cher", "in", "Strumpf", ",", "Fl\u00f6\u00b7te", ",", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Huren, sich einsam zur Weihnacht berauschend;", "tokens": ["Hu\u00b7ren", ",", "sich", "ein\u00b7sam", "zur", "Weih\u00b7nacht", "be\u00b7rau\u00b7schend", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wasserm\u00e4dchen, den G\u00e4sten lauschend;", "tokens": ["Was\u00b7ser\u00b7m\u00e4d\u00b7chen", ",", "den", "G\u00e4s\u00b7ten", "lau\u00b7schend", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "M\u00e4gde, die wartend vorm Haus stehn,", "tokens": ["M\u00e4g\u00b7de", ",", "die", "war\u00b7tend", "vorm", "Haus", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "K\u00f6nnen ergreifend sch\u00f6n aussehn.", "tokens": ["K\u00f6n\u00b7nen", "er\u00b7grei\u00b7fend", "sch\u00f6n", "aus\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADJD", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Je mehr Verzicht", "tokens": ["Je", "mehr", "Ver\u00b7zicht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus der Dummheit spricht,", "tokens": ["Aus", "der", "Dumm\u00b7heit", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Desto tiefer neigt,", "tokens": ["Des\u00b7to", "tie\u00b7fer", "neigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Desto h\u00f6her steigt", "tokens": ["Des\u00b7to", "h\u00f6\u00b7her", "steigt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sie. \u2013 Warum zagte der Dichter", "tokens": ["Sie", ".", "\u2013", "Wa\u00b7rum", "zag\u00b7te", "der", "Dich\u00b7ter"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$.", "$(", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vor dem Titel \u00bbMadonnengesichter\u00ab?", "tokens": ["Vor", "dem", "Ti\u00b7tel", "\u00bb", "Ma\u00b7don\u00b7nen\u00b7ge\u00b7sich\u00b7ter", "\u00ab", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "NN", "$(", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}