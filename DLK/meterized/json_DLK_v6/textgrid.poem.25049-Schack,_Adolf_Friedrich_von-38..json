{"textgrid.poem.25049": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn m\u00fcd' du von der Liebe Wonnen,", "tokens": ["Wenn", "m\u00fcd'", "du", "von", "der", "Lie\u00b7be", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanft dich Schlummer \u00fcberflie\u00dft,", "tokens": ["Und", "sanft", "dich", "Schlum\u00b7mer", "\u00fc\u00b7berf\u00b7lie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fcckt f\u00fchl' ich dein warmes Leben", "tokens": ["Ent\u00b7z\u00fcckt", "f\u00fchl'", "ich", "dein", "war\u00b7mes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An meins in jedem Tropfen beben,", "tokens": ["An", "meins", "in", "je\u00b7dem", "Trop\u00b7fen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der, durch die Adern hingeronnen,", "tokens": ["Der", ",", "durch", "die", "A\u00b7dern", "hin\u00b7ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In leichter Wallung sich ergie\u00dft!", "tokens": ["In", "leich\u00b7ter", "Wal\u00b7lung", "sich", "er\u00b7gie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Auges blaue Strahlenkreise", "tokens": ["Des", "Au\u00b7ges", "blau\u00b7e", "Strah\u00b7len\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbirgt die Wimper meinem Blick;", "tokens": ["Ver\u00b7birgt", "die", "Wim\u00b7per", "mei\u00b7nem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch d\u00e4mmernd durch die zarte H\u00fclle", "tokens": ["Doch", "d\u00e4m\u00b7mernd", "durch", "die", "zar\u00b7te", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Mondglanz quillt des Lichtes F\u00fclle,", "tokens": ["Wie", "Mond\u00b7glanz", "quillt", "des", "Lich\u00b7tes", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und deine Lippen murmeln leise", "tokens": ["Und", "dei\u00b7ne", "Lip\u00b7pen", "mur\u00b7meln", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Traume noch von unserm Gl\u00fcck.", "tokens": ["Im", "Trau\u00b7me", "noch", "von", "un\u00b7serm", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}