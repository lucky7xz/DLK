{"textgrid.poem.41236": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort, wo geh\u00fcllt in ewige Nebelnacht,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo geh\u00fcllt in ewige Nebelnacht,", "tokens": ["Dort", ",", "wo", "ge\u00b7h\u00fcllt", "in", "e\u00b7wi\u00b7ge", "Ne\u00b7bel\u00b7nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umbrauset von Orkanen, der Nordpol starrt,", "tokens": ["Um\u00b7brau\u00b7set", "von", "Or\u00b7ka\u00b7nen", ",", "der", "Nord\u00b7pol", "starrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort w\u00f6lbt sich, unter Eisgebirgen,", "tokens": ["Dort", "w\u00f6lbt", "sich", ",", "un\u00b7ter", "Eis\u00b7ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweigend und hehr der Aeonen", "tokens": ["Schwei\u00b7gend", "und", "hehr", "der", "A\u00b7e\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Der Wohnsitz grauer S\u00f6hne der alten Zeit.", "tokens": ["Der", "Wohn\u00b7sitz", "grau\u00b7er", "S\u00f6h\u00b7ne", "der", "al\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf neun und sechzig St\u00fchlen", "tokens": ["Auf", "neun", "und", "sech\u00b7zig", "St\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Seher, den der Morgend\u00e4mm'rung", "tokens": ["Dem", "Se\u00b7her", ",", "den", "der", "Mor\u00b7gen\u00b7d\u00e4m\u00b7m'\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weihe zum gro\u00dfen Gesicht' emporhub \u2013", "tokens": ["Wei\u00b7he", "zum", "gro\u00b7\u00dfen", "Ge\u00b7sicht'", "em\u00b7por\u00b7hub", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Auf neun und sechzig St\u00fchlen da schlummern sie,", "tokens": ["Auf", "neun", "und", "sech\u00b7zig", "St\u00fch\u00b7len", "da", "schlum\u00b7mern", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Greise hundert Winter, und jedesmal,", "tokens": ["Die", "Grei\u00b7se", "hun\u00b7dert", "Win\u00b7ter", ",", "und", "je\u00b7des\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach hundert Wintern, weicht ihr Schlummer", "tokens": ["Nach", "hun\u00b7dert", "Win\u00b7tern", ",", "weicht", "ihr", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor des entriegelten Thores Krachen.", "tokens": ["Vor", "des", "ent\u00b7rie\u00b7gel\u00b7ten", "Tho\u00b7res", "Kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Denn sind nun hundert Winter vorbeigefloh'n,", "tokens": ["Denn", "sind", "nun", "hun\u00b7dert", "Win\u00b7ter", "vor\u00b7bei\u00b7ge\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Dann mehrt der aufgewachten Aeonen Zahl", "tokens": ["Dann", "mehrt", "der", "auf\u00b7ge\u00b7wach\u00b7ten", "A\u00b7e\u00b7o\u00b7nen", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein neuer Bruder, schwebt zur Halle", "tokens": ["Ein", "neu\u00b7er", "Bru\u00b7der", ",", "schwebt", "zur", "Hal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thatenbelastet und m\u00fcde nieder.", "tokens": ["Tha\u00b7ten\u00b7be\u00b7las\u00b7tet", "und", "m\u00fc\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die\u00df war nun eben. Berstend erscholl das Thor,", "tokens": ["Die\u00df", "war", "nun", "e\u00b7ben", ".", "Bers\u00b7tend", "er\u00b7scholl", "das", "Thor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$.", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Fl\u00fcgel Aufschlug st\u00fcrmte die Nebel weg,", "tokens": ["Der", "Fl\u00fc\u00b7gel", "Auf\u00b7schlug", "st\u00fcrm\u00b7te", "die", "Ne\u00b7bel", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich sah den neuen greisen Bruder", "tokens": ["Ich", "sah", "den", "neu\u00b7en", "grei\u00b7sen", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feierlich in die Versammlung sinken.", "tokens": ["Fei\u00b7er\u00b7lich", "in", "die", "Ver\u00b7samm\u00b7lung", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ein gro\u00df Verneigen, ernster Erwartung voll,", "tokens": ["Ein", "gro\u00df", "Ver\u00b7nei\u00b7gen", ",", "erns\u00b7ter", "Er\u00b7war\u00b7tung", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von neun und sechszig St\u00fchlen. Der Bruder (glaubt", "tokens": ["Von", "neun", "und", "sechs\u00b7zig", "St\u00fch\u00b7len", ".", "Der", "Bru\u00b7der", "(", "glaubt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "CARD", "KON", "CARD", "NN", "$.", "ART", "NN", "$(", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem H\u00f6rer, den der Morgend\u00e4mmerung", "tokens": ["Dem", "H\u00f6\u00b7rer", ",", "den", "der", "Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weihe das Ohr, wie das Auge sch\u00e4rfte)", "tokens": ["Wei\u00b7he", "das", "Ohr", ",", "wie", "das", "Au\u00b7ge", "sch\u00e4rf\u00b7te", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Der Bruder brach das Schweigen: \u00bbdas, was euch selbst", "tokens": ["Der", "Bru\u00b7der", "brach", "das", "Schwei\u00b7gen", ":", "\u00bb", "das", ",", "was", "euch", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "$(", "PDS", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das gro\u00dfe Rad der Erdereignisse", "tokens": ["Das", "gro\u00b7\u00dfe", "Rad", "der", "Er\u00b7de\u00b7reig\u00b7nis\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor\u00fcberw\u00e4lzte, greise Br\u00fcder!", "tokens": ["Vor\u00b7\u00fc\u00b7berw\u00b7\u00e4lz\u00b7te", ",", "grei\u00b7se", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darf euch der j\u00fcngste nicht erst erz\u00e4hlen.", "tokens": ["Darf", "euch", "der", "j\u00fcngs\u00b7te", "nicht", "erst", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nur, was ihr nicht saht, was mir beschieden war", "tokens": ["Nur", ",", "was", "ihr", "nicht", "saht", ",", "was", "mir", "be\u00b7schie\u00b7den", "war"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Auf jenem weiten Schauplatz' allein zu sehn", "tokens": ["Auf", "je\u00b7nem", "wei\u00b7ten", "Schau\u00b7platz'", "al\u00b7lein", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch meiner hundert Winter Umschwung,", "tokens": ["Durch", "mei\u00b7ner", "hun\u00b7dert", "Win\u00b7ter", "Um\u00b7schwung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt ihr vernehmen Aeonen, horchet!", "tokens": ["Sollt", "ihr", "ver\u00b7neh\u00b7men", "A\u00b7e\u00b7o\u00b7nen", ",", "hor\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Zwei gro\u00dfe Frauen sah ich, Theresien,", "tokens": ["Zwei", "gro\u00b7\u00dfe", "Frau\u00b7en", "sah", "ich", ",", "The\u00b7re\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Katharinen.", "tokens": ["Und", "Ka\u00b7tha\u00b7ri\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An Weisheit, Macht und Thatenruhme", "tokens": ["An", "Weis\u00b7heit", ",", "Macht", "und", "Tha\u00b7ten\u00b7ruh\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00f6\u00dfere M\u00e4nner auf Herrscherst\u00fchlen.", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7re", "M\u00e4n\u00b7ner", "auf", "Herr\u00b7scher\u00b7st\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Nur Eines Mannes alles umfassenden,", "tokens": ["Nur", "Ei\u00b7nes", "Man\u00b7nes", "al\u00b7les", "um\u00b7fas\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich selber alles schuldigen Heldengeist,", "tokens": ["Sich", "sel\u00b7ber", "al\u00b7les", "schul\u00b7di\u00b7gen", "Hel\u00b7den\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der vor der Spree stolzen Ufern", "tokens": ["Der", "vor", "der", "Spree", "stol\u00b7zen", "U\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gl\u00e4nzte,", "tokens": ["Gl\u00e4nz\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Ein f\u00fcnfter Welttheil,", "tokens": ["Ein", "f\u00fcnf\u00b7ter", "Welt\u00b7theil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sieht Wimpeln wehen, h\u00f6rt des Gesch\u00fctzes Knall.", "tokens": ["Sieht", "Wim\u00b7peln", "we\u00b7hen", ",", "h\u00f6rt", "des", "Ge\u00b7sch\u00fct\u00b7zes", "Knall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein k\u00fchner Britte Cook \u2013 o klaget!", "tokens": ["Ein", "k\u00fch\u00b7ner", "Brit\u00b7te", "Cook", "\u2013", "o", "kla\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fand und bego\u00df ihn mit eig'nem Blute.", "tokens": ["Fand", "und", "be\u00b7go\u00df", "ihn", "mit", "eig'\u00b7nem", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Schon m\u00fcde des Gehorsams entrissen sich", "tokens": ["Schon", "m\u00fc\u00b7de", "des", "Ge\u00b7hor\u00b7sams", "ent\u00b7ris\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mutter England Pflanzer Amerikas.", "tokens": ["Der", "Mut\u00b7ter", "En\u00b7gland", "Pflan\u00b7zer", "A\u00b7me\u00b7ri\u00b7kas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Viel\u00e4stig ragt der neuen Eiche", "tokens": ["Vie\u00b7l\u00e4s\u00b7tig", "ragt", "der", "neu\u00b7en", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wipfel nun auf, und beschattet Meere.", "tokens": ["Wip\u00b7fel", "nun", "auf", ",", "und", "be\u00b7schat\u00b7tet", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Ein Gott geweihter, jeglicher Menschenart", "tokens": ["Ein", "Gott", "ge\u00b7weih\u00b7ter", ",", "jeg\u00b7li\u00b7cher", "Men\u00b7schen\u00b7art"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "PIAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch alle Zonen fr\u00f6hnender M\u00e4nnerbund", "tokens": ["Durch", "al\u00b7le", "Zo\u00b7nen", "fr\u00f6h\u00b7nen\u00b7der", "M\u00e4n\u00b7ner\u00b7bund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erlag den R\u00e4nken, ward zerrissen,", "tokens": ["Er\u00b7lag", "den", "R\u00e4