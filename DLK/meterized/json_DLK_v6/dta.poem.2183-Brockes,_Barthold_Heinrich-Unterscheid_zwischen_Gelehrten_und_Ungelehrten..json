{"dta.poem.2183": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheid zwischen Gelehrten  \n und Ungelehrten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ungelehrten sind Gesch\u00f6pfe, die, ob sie gleich da-", "tokens": ["Die", "Un\u00b7ge\u00b7lehr\u00b7ten", "sind", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "die", ",", "ob", "sie", "gleich", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "zu beschieden,", "tokens": ["zu", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Den Sch\u00f6pfer, aus der Welt, zu kennen, dennoch, mit sich", "tokens": ["Den", "Sch\u00f6p\u00b7fer", ",", "aus", "der", "Welt", ",", "zu", "ken\u00b7nen", ",", "den\u00b7noch", ",", "mit", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$,", "ADV", "$,", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "allein zufrieden,", "tokens": ["al\u00b7lein", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sich mit der Absicht nicht befassen.", "tokens": ["Sich", "mit", "der", "Ab\u00b7sicht", "nicht", "be\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gelehrte sind hingegen solche, die so viel andre Dinge", "tokens": ["Ge\u00b7lehr\u00b7te", "sind", "hin\u00b7ge\u00b7gen", "sol\u00b7che", ",", "die", "so", "viel", "and\u00b7re", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "$,", "PRELS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "lernen,", "tokens": ["ler\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wodurch sie von den wahren Pflichten sich noch um de-", "tokens": ["Wo\u00b7durch", "sie", "von", "den", "wah\u00b7ren", "Pflich\u00b7ten", "sich", "noch", "um", "de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "PRF", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "sto mehr entfernen:", "tokens": ["sto", "mehr", "ent\u00b7fer\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ist es denn wohl bewundernswerth, wenn sie es v\u00f6llig", "tokens": ["Ist", "es", "denn", "wohl", "be\u00b7wun\u00b7derns\u00b7werth", ",", "wenn", "sie", "es", "v\u00f6l\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "unterlassen?", "tokens": ["un\u00b7ter\u00b7las\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}