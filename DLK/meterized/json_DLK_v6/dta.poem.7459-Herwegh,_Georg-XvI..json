{"dta.poem.7459": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann oft stundenlang am Strome stehen,", "tokens": ["Ich", "kann", "oft", "stun\u00b7den\u00b7lang", "am", "Stro\u00b7me", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich entflohen aus der Menschen Bann;", "tokens": ["Wenn", "ich", "ent\u00b7flo\u00b7hen", "aus", "der", "Men\u00b7schen", "Bann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er plaudert hier, wie ein erfahrner Mann,", "tokens": ["Er", "plau\u00b7dert", "hier", ",", "wie", "ein", "er\u00b7fahr\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der in der Welt sich t\u00fcchtig umgesehen.", "tokens": ["Der", "in", "der", "Welt", "sich", "t\u00fcch\u00b7tig", "um\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da schildert er mir seiner Jugend Wehen,", "tokens": ["Da", "schil\u00b7dert", "er", "mir", "sei\u00b7ner", "Ju\u00b7gend", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie er den Weg durch Klippen erst gewann,", "tokens": ["Wie", "er", "den", "Weg", "durch", "Klip\u00b7pen", "erst", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ermattet drauf im Sande schier verrann,", "tokens": ["Er\u00b7mat\u00b7tet", "drauf", "im", "San\u00b7de", "schier", "ver\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes Wort f\u00fchl' ich zum Herzen gehen.", "tokens": ["Und", "je\u00b7des", "Wort", "f\u00fchl'", "ich", "zum", "Her\u00b7zen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie wallt er doch so sicher seine Bahn!", "tokens": ["Wie", "wallt", "er", "doch", "so", "si\u00b7cher", "sei\u00b7ne", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei allem Pl\u00e4nkeln, Hin- und Wiederstreifen", "tokens": ["Bei", "al\u00b7lem", "Pl\u00e4n\u00b7keln", ",", "Hin", "und", "Wie\u00b7der\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergi\u00dft er nie: \u201eIch mu\u00df zum Ocean!\u201c", "tokens": ["Ver\u00b7gi\u00dft", "er", "nie", ":", "\u201e", "Ich", "mu\u00df", "zum", "O\u00b7cean", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "VMFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, Seele, nur willst in der Irre schweifen?", "tokens": ["Du", ",", "See\u00b7le", ",", "nur", "willst", "in", "der", "Ir\u00b7re", "schwei\u00b7fen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O tritt, ein Kind, doch zur Natur heran,", "tokens": ["O", "tritt", ",", "ein", "Kind", ",", "doch", "zur", "Na\u00b7tur", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lern' die Weisheit aus den Wassern greifen!", "tokens": ["Und", "lern'", "die", "Weis\u00b7heit", "aus", "den", "Was\u00b7sern", "grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}