{"dta.poem.23487": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Gottes hand des Himels Gna-", "tokens": ["Von", "Got\u00b7tes", "hand", "des", "Hi\u00b7mels", "Gna"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den-cron", "tokens": ["den\u00b7cron"], "token_info": ["word"], "pos": ["XY"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Gotsforcht jm zuwege\u0303 bringet/", "tokens": ["Die", "Gots\u00b7forcht", "jm", "zu\u00b7weg\u1ebd", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich wai\u00df wie sie sich zu des H\u00f6chste\u0303 thron", "tokens": ["Ich", "wai\u00df", "wie", "sie", "sich", "zu", "des", "H\u00f6chst\u1ebd", "thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "PRF", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit kr\u00e4ftigem eyfer aufschwinget.", "tokens": ["Mit", "kr\u00e4f\u00b7ti\u00b7gem", "ey\u00b7fer", "auf\u00b7schwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sie deren lob billich in meinem mund/", "tokens": ["Sie", "de\u00b7ren", "lob", "bil\u00b7lich", "in", "mei\u00b7nem", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die ist allein aller Tugenten grund/", "tokens": ["Die", "ist", "al\u00b7lein", "al\u00b7ler", "Tu\u00b7gen\u00b7ten", "grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ohn welchen Sie zu boden fallen/", "tokens": ["Ohn", "wel\u00b7chen", "Sie", "zu", "bo\u00b7den", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie ist der Engeln wolgefallen;", "tokens": ["Sie", "ist", "der", "En\u00b7geln", "wol\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie ist der Sehlen starck es band/", "tokens": ["Sie", "ist", "der", "Seh\u00b7len", "starck", "es", "band", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ohn welches sich der mensche\u0303 lieb vernichtet;", "tokens": ["Ohn", "wel\u00b7ches", "sich", "der", "men\u00b7sch\u1ebd", "lieb", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Vnd nur durch Jhr m\u00e4chtige hand", "tokens": ["Vnd", "nur", "durch", "Ihr", "m\u00e4ch\u00b7ti\u00b7ge", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Werden die St\u00e4t vn\u0303 L\u00e4nder aufgerichtet:", "tokens": ["Wer\u00b7den", "die", "St\u00e4t", "v\u00f1", "L\u00e4n\u00b7der", "auf\u00b7ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie gibt Jhm einen solchen muht/", "tokens": ["Sie", "gibt", "Jhm", "ei\u00b7nen", "sol\u00b7chen", "muht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "PIAT", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das Er niemand verdam\u0303et oder hasset/", "tokens": ["Das", "Er", "nie\u00b7mand", "ver\u00b7dam\u0303et", "o\u00b7der", "has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Vnd sich allein auf Got verlasset/", "tokens": ["Vnd", "sich", "al\u00b7lein", "auf", "Got", "ver\u00b7las\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Fromb f\u00fcr sich selbs/ vn\u0303 f\u00fcr jederman gut.", "tokens": ["Fromb", "f\u00fcr", "sich", "selbs", "/", "v\u00f1", "f\u00fcr", "je\u00b7der\u00b7man", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "ADV", "$(", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}}}}