{"textgrid.poem.51271": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fragst du umb die Stund, und umb den Augenblick,", "tokens": ["Was", "fragst", "du", "umb", "die", "Stund", ",", "und", "umb", "den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja umb des Himmels H\u00f6h' und um der Erden Breite:", "tokens": ["Ja", "umb", "des", "Him\u00b7mels", "H\u00f6h'", "und", "um", "der", "Er\u00b7den", "Brei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was sol der Gleichstrich dir? Der Sonnen-Wende Seite:", "tokens": ["Was", "sol", "der", "Gleich\u00b7strich", "dir", "?", "Der", "Son\u00b7nen\u00b7Wen\u00b7de", "Sei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sternen Wechsel Stand, das R\u00e4dlein vom Gel\u00fcck.", "tokens": ["Der", "Ster\u00b7nen", "Wech\u00b7sel", "Stand", ",", "das", "R\u00e4d\u00b7lein", "vom", "Ge\u00b7l\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Umbsonst quartierest du in die zw\u00f6lff Ha\u00fcser ein", "tokens": ["Um\u00b7bsonst", "quar\u00b7tie\u00b7rest", "du", "in", "die", "zw\u00f6lff", "Ha\u00fc\u00b7ser", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "CARD", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Herrscher der Geburt: Umbsonst suchst du die Wercke,", "tokens": ["Die", "Herr\u00b7scher", "der", "Ge\u00b7burt", ":", "Um\u00b7bsonst", "suchst", "du", "die", "Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Zeichen Fall und Gang, der Lichter Schw\u00e4ch und St\u00e4rcke:", "tokens": ["Der", "Zei\u00b7chen", "Fall", "und", "Gang", ",", "der", "Lich\u00b7ter", "Schw\u00e4ch", "und", "St\u00e4r\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihren dritt- und vierdt- und sechsten Angelschein.", "tokens": ["Und", "ih\u00b7ren", "drit\u00b7t", "und", "vierdt", "und", "sechs\u00b7ten", "An\u00b7gel\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "TRUNC", "KON", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wann den Gebuhrts Stamm du darauff von Jahr auff Jahr", "tokens": ["Wann", "den", "Ge\u00b7buhrts", "Stamm", "du", "dar\u00b7auff", "von", "Jahr", "auff", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "PAV", "APPR", "NN", "APPR", "NN"], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Himmel nachgewaltzt, und iede Stellung funden,", "tokens": ["Dem", "Him\u00b7mel", "nach\u00b7ge\u00b7waltzt", ",", "und", "ie\u00b7de", "Stel\u00b7lung", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hast du denn davon? O Mensch, kaum einer Stunden:", "tokens": ["Was", "hast", "du", "denn", "da\u00b7von", "?", "O", "Mensch", ",", "kaum", "ei\u00b7ner", "Stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKVZ", "$.", "NE", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja zwischen dem ein Blick kaum ist und seiner Baar.", "tokens": ["Ja", "zwi\u00b7schen", "dem", "ein", "Blick", "kaum", "ist", "und", "sei\u00b7ner", "Baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "ART", "NN", "ADV", "VAFIN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich wil ein andre Kunst, die aus dem ersten Blick", "tokens": ["Ich", "wil", "ein", "and\u00b7re", "Kunst", ",", "die", "aus", "dem", "ers\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem Leben fleu\u00dft, dich lehrn, die heist: Wol sterben.", "tokens": ["In", "die\u00b7sem", "Le\u00b7ben", "fleu\u00dft", ",", "dich", "lehrn", ",", "die", "heist", ":", "Wol", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "$.", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Dis Thema nihm in acht: Wo du nicht wilst verterben,", "tokens": ["Dis", "The\u00b7ma", "nihm", "in", "acht", ":", "Wo", "du", "nicht", "wilst", "ver\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "PWAV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In jenem findest du den Tod, in diesem Gl\u00fcck.", "tokens": ["In", "je\u00b7nem", "fin\u00b7dest", "du", "den", "Tod", ",", "in", "die\u00b7sem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}