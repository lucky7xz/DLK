{"textgrid.poem.33110": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Laura's Preise schlug Petrarch die Leier,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Laura's Preise schlug Petrarch die Leier,", "tokens": ["Zu", "Lau\u00b7ra's", "Prei\u00b7se", "schlug", "Pe\u00b7trarch", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und all sein Denken war nur eine Feier", "tokens": ["Und", "all", "sein", "Den\u00b7ken", "war", "nur", "ei\u00b7ne", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Doch strebt' er nie nach irdischem Besitze,", "tokens": ["Doch", "strebt'", "er", "nie", "nach", "ir\u00b7di\u00b7schem", "Be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie war die G\u00f6ttin auf dem Wolkensitze,", "tokens": ["Sie", "war", "die", "G\u00f6t\u00b7tin", "auf", "dem", "Wol\u00b7ken\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu der kein Sterblicher die W\u00fcnsche hebt.", "tokens": ["Zu", "der", "kein", "Sterb\u00b7li\u00b7cher", "die", "W\u00fcn\u00b7sche", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So str\u00f6mte Tasso aus der Liebe Quelle", "tokens": ["So", "str\u00f6m\u00b7te", "Tas\u00b7so", "aus", "der", "Lie\u00b7be", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichtkunst heil'ge, segensreiche Welle,", "tokens": ["Der", "Dicht\u00b7kunst", "heil'\u00b7ge", ",", "se\u00b7gens\u00b7rei\u00b7che", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Leonore sang sein Genius.", "tokens": ["F\u00fcr", "Le\u00b7o\u00b7no\u00b7re", "sang", "sein", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "An ihrem Geiste durst' er sich entz\u00fcnden,", "tokens": ["An", "ih\u00b7rem", "Geis\u00b7te", "dur\u00b7st'", "er", "sich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Er war ihr nahe; ihre Blicke k\u00fcnden", "tokens": ["Er", "war", "ihr", "na\u00b7he", ";", "ih\u00b7re", "Bli\u00b7cke", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihm manchen tiefen, ahnungsvollen Gru\u00df.", "tokens": ["Ihm", "man\u00b7chen", "tie\u00b7fen", ",", "ah\u00b7nungs\u00b7vol\u00b7len", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sinkt er nieder vor dem hohen Weibe,", "tokens": ["Da", "sinkt", "er", "nie\u00b7der", "vor", "dem", "ho\u00b7hen", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wird des Stolzes, wird der Rohheit Scheibe,", "tokens": ["Und", "wird", "des", "Stol\u00b7zes", ",", "wird", "der", "Roh\u00b7heit", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wohl mag ich staunend auf Petrarcha sehen,", "tokens": ["Wohl", "mag", "ich", "stau\u00b7nend", "auf", "Pe\u00b7trar\u00b7cha", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Liebess\u00e4nger, frei von ird'schen Wehen \u2013", "tokens": ["Den", "Lie\u00b7bes\u00b7s\u00e4n\u00b7ger", ",", "frei", "von", "ird'\u00b7schen", "We\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Tasso aber fliegt mein ganzes Herz!", "tokens": ["Zu", "Tas\u00b7so", "a\u00b7ber", "fliegt", "mein", "gan\u00b7zes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}