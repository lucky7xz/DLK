{"textgrid.poem.48756": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "43. \u00dcber den ungew\u00f6hnlichen Sturm, nachdem die Freudensch\u00fcsse von beiden Theilen geschehen waren", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie, Eol, was, Neptun? Gef\u00e4llt euch Jovis Schlu\u00df,", "tokens": ["Wie", ",", "E\u00b7ol", ",", "was", ",", "Nep\u00b7tun", "?", "Ge\u00b7f\u00e4llt", "euch", "Jo\u00b7vis", "Schlu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "PRELS", "$,", "NN", "$.", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "der Rat der G\u00f6tter nicht, den n\u00e4chst Merkur verfa\u00dfte,", "tokens": ["der", "Rat", "der", "G\u00f6t\u00b7ter", "nicht", ",", "den", "n\u00e4chst", "Mer\u00b7kur", "ver\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$,", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ihr selbst mit unterschriebt im ewigen Palaste?", "tokens": ["ihr", "selbst", "mit", "un\u00b7ter\u00b7schriebt", "im", "e\u00b7wi\u00b7gen", "Pa\u00b7las\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was, Eol, wie, Neptun? Was sol denn dieser Gu\u00df?", "tokens": ["Was", ",", "E\u00b7ol", ",", "wie", ",", "Nep\u00b7tun", "?", "Was", "sol", "denn", "die\u00b7ser", "Gu\u00df", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "PWAV", "$,", "NN", "$.", "PWS", "VMFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Donner, dieser Sturm, der den erschreckten Flu\u00df", "tokens": ["Der", "Don\u00b7ner", ",", "die\u00b7ser", "Sturm", ",", "der", "den", "er\u00b7schreck\u00b7ten", "Flu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von Grund aus reizet auf, da\u00df er so schnell erbla\u00dfte?", "tokens": ["von", "Grund", "aus", "rei\u00b7zet", "auf", ",", "da\u00df", "er", "so", "schnell", "er\u00b7bla\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Grimm des Wetters rast und pfeifet um die Maste,", "tokens": ["Der", "Grimm", "des", "Wet\u00b7ters", "rast", "und", "pfei\u00b7fet", "um", "die", "Mas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es setzet furchtsam ein der Anker seinen Fu\u00df.", "tokens": ["es", "set\u00b7zet", "furcht\u00b7sam", "ein", "der", "An\u00b7ker", "sei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was aber ist mir das? Wo k\u00f6mmt der lichte Strahl,", "tokens": ["Was", "a\u00b7ber", "ist", "mir", "das", "?", "Wo", "k\u00f6mmt", "der", "lich\u00b7te", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "PDS", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die schnelle Stille her, die Jupiters Gemahl", "tokens": ["die", "schnel\u00b7le", "Stil\u00b7le", "her", ",", "die", "Ju\u00b7pi\u00b7ters", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "von Ost in Westen f\u00fchrt? Ich wunder mich des Wunders.", "tokens": ["von", "Ost", "in", "Wes\u00b7ten", "f\u00fchrt", "?", "Ich", "wun\u00b7der", "mich", "des", "Wun\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "VVFIN", "$.", "PPER", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wird euer Grimm gestraft, ist euch der Eifer leid?", "tokens": ["Wird", "eu\u00b7er", "Grimm", "ge\u00b7straft", ",", "ist", "euch", "der", "Ei\u00b7fer", "leid", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NE", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedeutet es f\u00fcr uns gut' oder b\u00f6se Zeit?", "tokens": ["Be\u00b7deu\u00b7tet", "es", "f\u00fcr", "uns", "gut'", "o\u00b7der", "b\u00f6\u00b7se", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sage, was man wil, ich denke was besonders.", "tokens": ["Man", "sa\u00b7ge", ",", "was", "man", "wil", ",", "ich", "den\u00b7ke", "was", "be\u00b7son\u00b7ders", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$,", "PPER", "VVFIN", "PWS", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}