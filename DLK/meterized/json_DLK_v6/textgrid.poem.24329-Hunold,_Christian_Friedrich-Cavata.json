{"textgrid.poem.24329": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Cavata", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr/ dein Gesalbter stirbt/", "tokens": ["Herr", "/", "dein", "Ge\u00b7salb\u00b7ter", "stirbt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der dich unendlich liebet/", "tokens": ["Der", "dich", "un\u00b7end\u00b7lich", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns lauter Heil erwirbt/", "tokens": ["Uns", "lau\u00b7ter", "Heil", "er\u00b7wirbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und nie/ als itzt betr\u00fcbet.", "tokens": ["Und", "nie", "/", "als", "itzt", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein gantzes Thun war K\u00f6niglich.", "tokens": ["Sein", "gant\u00b7zes", "Thun", "war", "K\u00f6\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir m\u00fcssen seine Gnade prei\u00dfen.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "sei\u00b7ne", "Gna\u00b7de", "prei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Uns wird allzeit unsch\u00e4tzbar hei\u00dfen", "tokens": ["Uns", "wird", "all\u00b7zeit", "un\u00b7sch\u00e4tz\u00b7bar", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVINF"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Der theure Nahme Friederich.", "tokens": ["Der", "theu\u00b7re", "Nah\u00b7me", "Frie\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Klage dringt nur in den Himmel ein/", "tokens": ["Die", "Kla\u00b7ge", "dringt", "nur", "in", "den", "Him\u00b7mel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Friederich hat m\u00fcssen sterblich seyn.", "tokens": ["Da\u00df", "Frie\u00b7de\u00b7rich", "hat", "m\u00fcs\u00b7sen", "sterb\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch Herr/ weil deine Hand", "tokens": ["Doch", "Herr", "/", "weil", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch Friederichs Gem\u00fcthe/", "tokens": ["Durch", "Frie\u00b7de\u00b7richs", "Ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unendlich viele G\u00fcte", "tokens": ["Un\u00b7end\u00b7lich", "vie\u00b7le", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns gn\u00e4dig zugewandt:", "tokens": ["Uns", "gn\u00e4\u00b7dig", "zu\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So tragen wir Gedult.", "tokens": ["So", "tra\u00b7gen", "wir", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es fliessen tausend milde Z\u00e4hren/", "tokens": ["Es", "flies\u00b7sen", "tau\u00b7send", "mil\u00b7de", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein danckbar Hertz dir zu gew\u00e4hren", "tokens": ["Ein", "dan\u00b7ck\u00b7bar", "Hertz", "dir", "zu", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Vor dieses grossen K\u00f6nigs Huld.", "tokens": ["Vor", "die\u00b7ses", "gros\u00b7sen", "K\u00f6\u00b7nigs", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nimm solches Opffer gn\u00e4dig an/", "tokens": ["Nimm", "sol\u00b7ches", "Opf\u00b7fer", "gn\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sende von der Sternen Bahn", "tokens": ["Und", "sen\u00b7de", "von", "der", "Ster\u00b7nen", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Vaters Gl\u00fcck gedoppelt auf den Sohn/", "tokens": ["Des", "Va\u00b7ters", "Gl\u00fcck", "ge\u00b7dop\u00b7pelt", "auf", "den", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und seegne Friedrich Wilhelms Thron.", "tokens": ["Und", "seeg\u00b7ne", "Fried\u00b7rich", "Wil\u00b7helms", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}