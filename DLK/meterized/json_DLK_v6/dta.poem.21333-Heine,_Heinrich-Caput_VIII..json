{"dta.poem.21333": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Pferde wedelten mit dem Schwanz", "tokens": ["Die", "Pfer\u00b7de", "we\u00b7del\u00b7ten", "mit", "dem", "Schwanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So traulich wie alte Bekannte,", "tokens": ["So", "trau\u00b7lich", "wie", "al\u00b7te", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ihre Mistk\u00fcchlein d\u00fcnkten mir sch\u00f6n", "tokens": ["Und", "ih\u00b7re", "Mist\u00b7k\u00fcch\u00b7lein", "d\u00fcnk\u00b7ten", "mir", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie die Aepfel der Atalante!", "tokens": ["Wie", "die", "A\u00b7e\u00b7pfel", "der", "A\u00b7tal\u00b7an\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wir fuhren durch M\u00fchlheim. Die Stadt ist nett,", "tokens": ["Wir", "fuh\u00b7ren", "durch", "M\u00fchl\u00b7heim", ".", "Die", "Stadt", "ist", "nett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Menschen still und flei\u00dfig.", "tokens": ["Die", "Men\u00b7schen", "still", "und", "flei\u00b7\u00dfig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "War dort zuletzt im Monat May", "tokens": ["War", "dort", "zu\u00b7letzt", "im", "Mo\u00b7nat", "May"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jahres Ein und dreyzig.", "tokens": ["Des", "Jah\u00b7res", "Ein", "und", "drey\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "KON", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Damals stand alles im Bl\u00fcthenschmuck", "tokens": ["Da\u00b7mals", "stand", "al\u00b7les", "im", "Bl\u00fc\u00b7then\u00b7schmuck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und die Sonnenlichter lachten,", "tokens": ["Und", "die", "Son\u00b7nen\u00b7lich\u00b7ter", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die V\u00f6gel sangen sehnsuchtvoll,", "tokens": ["Die", "V\u00f6\u00b7gel", "san\u00b7gen", "sehn\u00b7sucht\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Menschen hofften und dachten \u2013", "tokens": ["Und", "die", "Men\u00b7schen", "hoff\u00b7ten", "und", "dach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie dachten: \u201eDie magere Ritterschaft", "tokens": ["Sie", "dach\u00b7ten", ":", "\u201e", "Die", "ma\u00b7ge\u00b7re", "Rit\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wird bald von hinnen reisen,", "tokens": ["Wird", "bald", "von", "hin\u00b7nen", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der Abschiedstrunk wird ihnen kredenzt", "tokens": ["Und", "der", "Ab\u00b7schieds\u00b7trunk", "wird", "ih\u00b7nen", "kre\u00b7denzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aus langen Flaschen von Eisen!", "tokens": ["Aus", "lan\u00b7gen", "Fla\u00b7schen", "von", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}