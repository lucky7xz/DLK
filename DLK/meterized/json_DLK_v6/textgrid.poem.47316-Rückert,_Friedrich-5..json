{"textgrid.poem.47316": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df die Welt in deinen goldnen Str\u00f6men baden, ew'ges Licht!", "tokens": ["La\u00df", "die", "Welt", "in", "dei\u00b7nen", "gold\u00b7nen", "Str\u00f6\u00b7men", "ba\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Speise Geister an der Tafel deiner Gnaden, ew'ges Licht!", "tokens": ["Spei\u00b7se", "Geis\u00b7ter", "an", "der", "Ta\u00b7fel", "dei\u00b7ner", "Gna\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie das Meer in weiten Kreisen um das Land, so flutet dein", "tokens": ["Wie", "das", "Meer", "in", "wei\u00b7ten", "Krei\u00b7sen", "um", "das", "Land", ",", "so", "flu\u00b7tet", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "\u00c4ther um die Welt in weitern Glanzgestaden, ew'ges Licht!", "tokens": ["\u00c4\u00b7ther", "um", "die", "Welt", "in", "wei\u00b7tern", "Glanz\u00b7ge\u00b7sta\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nicht die Sonne dich, die Sonnen zeugest du; in deinem Strahl", "tokens": ["Nicht", "die", "Son\u00b7ne", "dich", ",", "die", "Son\u00b7nen", "zeu\u00b7gest", "du", ";", "in", "dei\u00b7nem", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Tanzen sie, als wie in ihrem Strahle Maden, ew'ges Licht!", "tokens": ["Tan\u00b7zen", "sie", ",", "als", "wie", "in", "ih\u00b7rem", "Strah\u00b7le", "Ma\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "KOKOM", "APPR", "PPOSAT", "NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Nicht der Himmel kann dich fassen, und zur Erde steigest du,", "tokens": ["Nicht", "der", "Him\u00b7mel", "kann", "dich", "fas\u00b7sen", ",", "und", "zur", "Er\u00b7de", "stei\u00b7gest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,", "KON", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Opfer z\u00fcndend unter allen Breitegraden, ew'ges Licht!", "tokens": ["Op\u00b7fer", "z\u00fcn\u00b7dend", "un\u00b7ter", "al\u00b7len", "Brei\u00b7te\u00b7gra\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PIAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Zu dem Meru, zum Olympos, wie zum Sinai herab", "tokens": ["Zu", "dem", "Me\u00b7ru", ",", "zum", "O\u00b7lym\u00b7pos", ",", "wie", "zum", "Si\u00b7nai", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "ADV"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Senkst du hell durch Wolkenschichten einen Faden, ew'ges Licht!", "tokens": ["Senkst", "du", "hell", "durch", "Wol\u00b7ken\u00b7schich\u00b7ten", "ei\u00b7nen", "Fa\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Ab von dir ins Dunkel wendet ihren Pfad die Welt, doch du", "tokens": ["Ab", "von", "dir", "ins", "Dun\u00b7kel", "wen\u00b7det", "ih\u00b7ren", "Pfad", "die", "Welt", ",", "doch", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPR", "PPER", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,", "KON", "PPER"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Str\u00f6mst entgegen aus dem Dunkel ihren Pfaden, ew'ges Licht!", "tokens": ["Str\u00f6mst", "ent\u00b7ge\u00b7gen", "aus", "dem", "Dun\u00b7kel", "ih\u00b7ren", "Pfa\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Auch auf krummen Stra\u00dfen lenkest du den Wahn zur\u00fcck zu dir;", "tokens": ["Auch", "auf", "krum\u00b7men", "Stra\u00b7\u00dfen", "len\u00b7kest", "du", "den", "Wahn", "zu\u00b7r\u00fcck", "zu", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Aber la\u00df zu dir mich wandeln die geraden, ew'ges Licht!", "tokens": ["A\u00b7ber", "la\u00df", "zu", "dir", "mich", "wan\u00b7deln", "die", "ge\u00b7ra\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPER", "PRF", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Wo vor dir sollt' ich mich bergen? Sollt' ich auf zum Himmel fliehn,", "tokens": ["Wo", "vor", "dir", "sollt'", "ich", "mich", "ber\u00b7gen", "?", "Sollt'", "ich", "auf", "zum", "Him\u00b7mel", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$.", "VMFIN", "PPER", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Wo mir funkeln deine lichten Myriaden, ew'ges Licht;", "tokens": ["Wo", "mir", "fun\u00b7keln", "dei\u00b7ne", "lich\u00b7ten", "My\u00b7ria\u00b7den", ",", "ew'\u00b7ges", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Wo vor dir sollt' ich mich decken? Flieh' ich in die Erdennacht?", "tokens": ["Wo", "vor", "dir", "sollt'", "ich", "mich", "de\u00b7cken", "?", "Flieh'", "ich", "in", "die", "Er\u00b7den\u00b7nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Golden brichst du durch des Schachtes dumpfe Schwaden, ew'ges Licht!", "tokens": ["Gol\u00b7den", "brichst", "du", "durch", "des", "Schach\u00b7tes", "dump\u00b7fe", "Schwa\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Ja, dies Herz auf keine Weise kann sich deinem Dienst entziehn,", "tokens": ["Ja", ",", "dies", "Herz", "auf", "kei\u00b7ne", "Wei\u00b7se", "kann", "sich", "dei\u00b7nem", "Dienst", "ent\u00b7ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "NN", "APPR", "PIAT", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Seit du mir dein goldnes Joch hast aufgeladen, ew'ges Licht!", "tokens": ["Seit", "du", "mir", "dein", "gold\u00b7nes", "Joch", "hast", "auf\u00b7ge\u00b7la\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Du mit Strahlen hell besaitend Abendsternes Lautenspiel,", "tokens": ["Du", "mit", "Strah\u00b7len", "hell", "be\u00b7sai\u00b7tend", "A\u00b7bends\u00b7ter\u00b7nes", "Lau\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "VVPP", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Stimmest auch die schrill'ge Leier der Cikaden, ew'ges Licht!", "tokens": ["Stim\u00b7mest", "auch", "die", "schrill'\u00b7ge", "Lei\u00b7er", "der", "Ci\u00b7ka\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Auch in meiner T\u00f6ne Fugen, allgeschmeid'ges, schmiege dich!", "tokens": ["Auch", "in", "mei\u00b7ner", "T\u00f6\u00b7ne", "Fu\u00b7gen", ",", "all\u00b7ge\u00b7schmei\u00b7d'\u00b7ges", ",", "schmie\u00b7ge", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,", "ADJA", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.24": {"text": "Lasse dem Juwel nicht seine Fassung schaden, ew'ges Licht!", "tokens": ["Las\u00b7se", "dem", "Ju\u00b7wel", "nicht", "sei\u00b7ne", "Fas\u00b7sung", "scha\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Gleichwie deine Sonnenstrahle sende meine Lieder aus,", "tokens": ["Gleich\u00b7wie", "dei\u00b7ne", "Son\u00b7nen\u00b7strah\u00b7le", "sen\u00b7de", "mei\u00b7ne", "Lie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Alle Welt zu deinen Festen einzuladen, ew'ges Licht!", "tokens": ["Al\u00b7le", "Welt", "zu", "dei\u00b7nen", "Fes\u00b7ten", "ein\u00b7zu\u00b7la\u00b7den", ",", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVIZU", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}