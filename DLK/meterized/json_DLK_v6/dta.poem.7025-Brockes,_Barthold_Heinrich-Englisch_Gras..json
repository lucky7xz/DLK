{"dta.poem.7025": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Englisch Gras.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewundere mein Hertz, wie mancherley", "tokens": ["Be\u00b7wun\u00b7de\u00b7re", "mein", "Hertz", ",", "wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Art, nur blo\u00df von Grase, sey.", "tokens": ["Die", "Art", ",", "nur", "blo\u00df", "von", "Gra\u00b7se", ",", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiedner andern zu geschweigen;", "tokens": ["Ver\u00b7schied\u00b7ner", "an\u00b7dern", "zu", "ge\u00b7schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So r\u00fchrt ein Gras, so Englisch wird genannt,", "tokens": ["So", "r\u00fchrt", "ein", "Gras", ",", "so", "Eng\u00b7lisch", "wird", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "(weil Engelland es uns zum ersten zugesandt,)", "tokens": ["(", "weil", "En\u00b7gel\u00b7land", "es", "uns", "zum", "ers\u00b7ten", "zu\u00b7ge\u00b7sandt", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "NE", "PPER", "PRF", "APPRART", "ADJA", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit grossem Recht durchs Auge den Verstand.", "tokens": ["Mit", "gros\u00b7sem", "Recht", "durchs", "Au\u00b7ge", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es ist von diesem Gras ein jedes Blat,", "tokens": ["Es", "ist", "von", "die\u00b7sem", "Gras", "ein", "je\u00b7des", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht wie das Gras sonst, d\u00fcnn\u2019 und platt;", "tokens": ["Nicht", "wie", "das", "Gras", "sonst", ",", "d\u00fcnn'", "und", "platt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nein, es ist dicker, und so fein,", "tokens": ["Nein", ",", "es", "ist", "di\u00b7cker", ",", "und", "so", "fein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Da\u00df fast nichts zierlicher kan seyn.", "tokens": ["Da\u00df", "fast", "nichts", "zier\u00b7li\u00b7cher", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADJA", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die sch\u00f6ne Bluhme, die es tr\u00e4get,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Bluh\u00b7me", ",", "die", "es", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verdienet, da\u00df man sie beschauet und erweget,", "tokens": ["Ver\u00b7die\u00b7net", ",", "da\u00df", "man", "sie", "be\u00b7schau\u00b7et", "und", "er\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein\u2019 jegliche besteht aus vielen,", "tokens": ["Ein'", "jeg\u00b7li\u00b7che", "be\u00b7steht", "aus", "vie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In welchen ", "tokens": ["In", "wel\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "F\u00fcnf Saamen-K\u00f6rnerchen erblicket man in ihr,", "tokens": ["F\u00fcnf", "Saa\u00b7men\u00b7K\u00f6r\u00b7ner\u00b7chen", "er\u00b7bli\u00b7cket", "man", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In einer s\u00fcssen Farben Zier", "tokens": ["In", "ei\u00b7ner", "s\u00fcs\u00b7sen", "Far\u00b7ben", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vom sch\u00f6nsten Seladon, die auf ein bleiches gr\u00fcn", "tokens": ["Vom", "sch\u00f6ns\u00b7ten", "Se\u00b7la\u00b7don", ",", "die", "auf", "ein", "blei\u00b7ches", "gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich aber, wenn sie reiffen, ziehn.", "tokens": ["Sich", "a\u00b7ber", ",", "wenn", "sie", "reif\u00b7fen", ",", "ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Stengel ist, verwunderlich formieret,", "tokens": ["Der", "Sten\u00b7gel", "ist", ",", "ver\u00b7wun\u00b7der\u00b7lich", "for\u00b7mie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mit zarten Spitzen ausgezieret,", "tokens": ["Mit", "zar\u00b7ten", "Spit\u00b7zen", "aus\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die fast unz\u00e4hlig sind, woran man eine Haut,", "tokens": ["Die", "fast", "un\u00b7z\u00e4h\u00b7lig", "sind", ",", "wo\u00b7ran", "man", "ei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die, welches fremd und seltsam l\u00e4sst,", "tokens": ["Die", ",", "wel\u00b7ches", "fremd", "und", "selt\u00b7sam", "l\u00e4sst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "An ihrem Stengel gar nicht fest,", "tokens": ["An", "ih\u00b7rem", "Sten\u00b7gel", "gar", "nicht", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Indem der obre Theil des Stengels in ihr stecket,", "tokens": ["In\u00b7dem", "der", "ob\u00b7re", "Theil", "des", "Sten\u00b7gels", "in", "ihr", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als wie vom Futteral bedecket.", "tokens": ["Als", "wie", "vom", "Fut\u00b7te\u00b7ral", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "In dieser fremd- und seltsamen Figur", "tokens": ["In", "die\u00b7ser", "frem\u00b7d", "und", "selt\u00b7sa\u00b7men", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Bewundr\u2019 ich abermahl die spielende Natur,", "tokens": ["Be\u00b7wundr'", "ich", "a\u00b7ber\u00b7mahl", "die", "spie\u00b7len\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und finde, wie sich jedes Ding bem\u00fche,", "tokens": ["Und", "fin\u00b7de", ",", "wie", "sich", "je\u00b7des", "Ding", "be\u00b7m\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Da\u00df es durch Aenderung die Augen auf sich ziehe.", "tokens": ["Da\u00df", "es", "durch", "A\u00b7en\u00b7de\u00b7rung", "die", "Au\u00b7gen", "auf", "sich", "zie\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.30": {"text": "Der Zweck, wie leichtlich zu ergr\u00fcnden,", "tokens": ["Der", "Zweck", ",", "wie", "leicht\u00b7lich", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ist, da\u00df wir auch allhier die Macht des Sch\u00f6pfers finden,", "tokens": ["Ist", ",", "da\u00df", "wir", "auch", "all\u00b7hier", "die", "Macht", "des", "Sch\u00f6p\u00b7fers", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Damit ein solcher Schatz von sch\u00f6nen Seltenheiten", "tokens": ["Da\u00b7mit", "ein", "sol\u00b7cher", "Schatz", "von", "sch\u00f6\u00b7nen", "Sel\u00b7ten\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Uns m\u00f6chte n\u00e4her zu Jhn leiten.", "tokens": ["Uns", "m\u00f6ch\u00b7te", "n\u00e4\u00b7her", "zu", "Jhn", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}