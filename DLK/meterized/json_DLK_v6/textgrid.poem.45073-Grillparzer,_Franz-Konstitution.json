{"textgrid.poem.45073": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Konstitution", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh ich einen so lange entwickeln", "tokens": ["Seh", "ich", "ei\u00b7nen", "so", "lan\u00b7ge", "ent\u00b7wi\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADV", "ADV", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und wenden die T\u00fcte hin und her,", "tokens": ["Und", "wen\u00b7den", "die", "T\u00fc\u00b7te", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rt auf die Erwartung mich ferner zu prickeln,", "tokens": ["H\u00f6rt", "auf", "die", "Er\u00b7war\u00b7tung", "mich", "fer\u00b7ner", "zu", "pri\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich wette darauf: das Papier ist leer.", "tokens": ["Ich", "wet\u00b7te", "da\u00b7rauf", ":", "das", "Pa\u00b7pier", "ist", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}