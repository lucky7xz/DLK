{"textgrid.poem.24630": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgte Gedancken \u00fcber den Gebuhrts-Tag seines Herrn Vaters", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem wir bey der Last der schweren Krieges-Zeiten/", "tokens": ["In", "dem", "wir", "bey", "der", "Last", "der", "schwe\u00b7ren", "Krie\u00b7ge\u00b7sZei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Annoch begl\u00fccket sind/ da\u00df unser Saal-", "tokens": ["An\u00b7noch", "be\u00b7gl\u00fc\u00b7cket", "sind", "/", "da\u00df", "un\u00b7ser", "Saa\u00b7l"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$(", "KOUS", "PPOSAT", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die s\u00fc\u00dfe Ruhe kr\u00f6nt/ und wir den Fr\u00f6hligkeiten", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Ru\u00b7he", "kr\u00f6nt", "/", "und", "wir", "den", "Fr\u00f6h\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach aller Arbeit auch verm\u00f6gen nach zugehn:", "tokens": ["Nach", "al\u00b7ler", "Ar\u00b7beit", "auch", "ver\u00b7m\u00f6\u00b7gen", "nach", "zu\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So will ich diesen Tag zur gr\u00f6sten Freude weihen/", "tokens": ["So", "will", "ich", "die\u00b7sen", "Tag", "zur", "gr\u00f6s\u00b7ten", "Freu\u00b7de", "wei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn heute mu\u00df mein Gl\u00fcck gedoppelt sch\u00f6n gedeihen.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "mein", "Gl\u00fcck", "ge\u00b7dop\u00b7pelt", "sch\u00f6n", "ge\u00b7dei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "VVPP", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Herr Vater/ dessen Gunst ich als ein Sohn mu\u00df ehren/", "tokens": ["Herr", "Va\u00b7ter", "/", "des\u00b7sen", "Gunst", "ich", "als", "ein", "Sohn", "mu\u00df", "eh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PRELAT", "NN", "PPER", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der durch des Himmels-Huld den besten Vater hat/", "tokens": ["Der", "durch", "des", "Him\u00b7mels\u00b7Huld", "den", "bes\u00b7ten", "Va\u00b7ter", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich hoff/ er werde mir nicht mein Vergn\u00fcgen wehren/", "tokens": ["Ich", "hoff", "/", "er", "wer\u00b7de", "mir", "nicht", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "weh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er gebe meiner Lust in Halle heute statt.", "tokens": ["Er", "ge\u00b7be", "mei\u00b7ner", "Lust", "in", "Hal\u00b7le", "heu\u00b7te", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn/ da\u00df ich mich allhier kan untern ", "tokens": ["Denn", "/", "da\u00df", "ich", "mich", "all\u00b7hier", "kan", "un\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4st eben Gott durch ihn und durch sein Wohl geschehen.", "tokens": ["L\u00e4st", "e\u00b7ben", "Gott", "durch", "ihn", "und", "durch", "sein", "Wohl", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Itzt scheinet ihm ein Fest/ das/ wie es ihm das Leben/", "tokens": ["Itzt", "schei\u00b7net", "ihm", "ein", "Fest", "/", "das", "/", "wie", "es", "ihm", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "$(", "PWAV", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bi\u00df auf diesen Tag Gl\u00fcck und Gesundheit giebt", "tokens": ["Und", "bi\u00df", "auf", "die\u00b7sen", "Tag", "Gl\u00fcck", "und", "Ge\u00b7sund\u00b7heit", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mir auch das Leben hat und alles Heil gegeben/", "tokens": ["Mir", "auch", "das", "Le\u00b7ben", "hat", "und", "al\u00b7les", "Heil", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAFIN", "KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das meine Seele so/ als wie sein Leben liebt.", "tokens": ["Das", "mei\u00b7ne", "See\u00b7le", "so", "/", "als", "wie", "sein", "Le\u00b7ben", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "$(", "KOUS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sonne mu\u00df der Welt zu ihrem Lichte dienen/", "tokens": ["Die", "Son\u00b7ne", "mu\u00df", "der", "Welt", "zu", "ih\u00b7rem", "Lich\u00b7te", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So macht sein Lebens-licht/ da\u00df mir der Tag geschienen.", "tokens": ["So", "macht", "sein", "Le\u00b7bens\u00b7licht", "/", "da\u00df", "mir", "der", "Tag", "ge\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Blumen geht es wohl/ weil sie der G\u00e4rtner wartet/", "tokens": ["Den", "Blu\u00b7men", "geht", "es", "wohl", "/", "weil", "sie", "der", "G\u00e4rt\u00b7ner", "war\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zweige wachsen sch\u00f6n/ dieweil er Baum noch gr\u00fcnt.", "tokens": ["Die", "Zwei\u00b7ge", "wach\u00b7sen", "sch\u00f6n", "/", "die\u00b7weil", "er", "Baum", "noch", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$(", "KOUS", "PPER", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bl\u00fchet auch ein Sohn/ der nach dem Vater artet", "tokens": ["So", "bl\u00fc\u00b7het", "auch", "ein", "Sohn", "/", "der", "nach", "dem", "Va\u00b7ter", "ar\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem seiner Eltern Bild zum steten Muster dient/", "tokens": ["Dem", "sei\u00b7ner", "El\u00b7tern", "Bild", "zum", "ste\u00b7ten", "Mus\u00b7ter", "dient", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem ihre G\u00fctigkeit/ die immer auf ihn dencket/", "tokens": ["Dem", "ih\u00b7re", "G\u00fc\u00b7tig\u00b7keit", "/", "die", "im\u00b7mer", "auf", "ihn", "den\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$(", "ART", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch edles Auferziehn das andre Leben schencket.", "tokens": ["Durch", "ed\u00b7les", "Auf\u00b7er\u00b7ziehn", "das", "and\u00b7re", "Le\u00b7ben", "schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kunst/ Klugheit/ Tugenden/ Gelehrsamkeit und Waffen/", "tokens": ["Kunst", "/", "Klug\u00b7heit", "/", "Tu\u00b7gen\u00b7den", "/", "Ge\u00b7lehr\u00b7sam\u00b7keit", "und", "Waf\u00b7fen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehn \u00fcber die Gebuhrt/ und bi\u00df zum Sternen-Dach.", "tokens": ["Gehn", "\u00fc\u00b7ber", "die", "Ge\u00b7buhrt", "/", "und", "bi\u00df", "zum", "Ster\u00b7nen\u00b7Dach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KON", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan ich mir was davon durch muntern Flei\u00df verschaffen/", "tokens": ["Kan", "ich", "mir", "was", "da\u00b7von", "durch", "mun\u00b7tern", "Flei\u00df", "ver\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "PAV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leb ich dem Befehl des theuren Vaters nach.", "tokens": ["So", "leb", "ich", "dem", "Be\u00b7fehl", "des", "theu\u00b7ren", "Va\u00b7ters", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von allen/ was ich nur allhier kan gutes fassen/", "tokens": ["Von", "al\u00b7len", "/", "was", "ich", "nur", "all\u00b7hier", "kan", "gu\u00b7tes", "fas\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "PWS", "PPER", "ADV", "ADV", "VMFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist ihm/ nechst Gott der Ruhm/ Herr Vater blo\u00df gelassen.", "tokens": ["Ist", "ihm", "/", "nechst", "Gott", "der", "Ruhm", "/", "Herr", "Va\u00b7ter", "blo\u00df", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "VVFIN", "NN", "ART", "NN", "$(", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum soll mein Hertze nichts Vergn\u00fcgen sprechen.", "tokens": ["Drum", "soll", "mein", "Hert\u00b7ze", "nichts", "Ver\u00b7gn\u00fc\u00b7gen", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "VVFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es kehrt die Freudigkeit gedoppelt bey mir ein.", "tokens": ["Es", "kehrt", "die", "Freu\u00b7dig\u00b7keit", "ge\u00b7dop\u00b7pelt", "bey", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Unmuht soll bey mir die Sinnen brechen/", "tokens": ["Kein", "Un\u00b7muht", "soll", "bey", "mir", "die", "Sin\u00b7nen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und dieser Tag ein Tag von lauter Wonne seyn/", "tokens": ["Und", "die\u00b7ser", "Tag", "ein", "Tag", "von", "lau\u00b7ter", "Won\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "APPR", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn diesem geht es wohl der mir zur Lust erkohren/", "tokens": ["Denn", "die\u00b7sem", "geht", "es", "wohl", "der", "mir", "zur", "Lust", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "ART", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch den mein Gl\u00fcck und Heil anheute neu gebohren.", "tokens": ["Durch", "den", "mein", "Gl\u00fcck", "und", "Heil", "an\u00b7heu\u00b7te", "neu", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch meine Freude ruht am meisten im Gem\u00fchte/", "tokens": ["Doch", "mei\u00b7ne", "Freu\u00b7de", "ruht", "am", "meis\u00b7ten", "im", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "PIS", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist mehr Tugendhafft als frey und Welt gesinnt.", "tokens": ["Und", "ist", "mehr", "Tu\u00b7gend\u00b7hafft", "als", "frey", "und", "Welt", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "KOKOM", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich preise die an mir erwiesne Vater-G\u00fcte/", "tokens": ["Ich", "prei\u00b7se", "die", "an", "mir", "er\u00b7wies\u00b7ne", "Va\u00b7ter\u00b7G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPER", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Womit mich Gott durch ihn Herr Vater lieb gewinnt/", "tokens": ["Wo\u00b7mit", "mich", "Gott", "durch", "ihn", "Herr", "Va\u00b7ter", "lieb", "ge\u00b7winnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "PPER", "NN", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es soll ein treuer Wunsch 