{"dta.poem.246": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als seine Sch\u00f6ne schlieffe.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ach schlafft ihr angenehme Augen/", "tokens": ["Ach", "schlafft", "ihr", "an\u00b7ge\u00b7neh\u00b7me", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Lichter dieser kleinen Welt/", "tokens": ["Ihr", "Lich\u00b7ter", "die\u00b7ser", "klei\u00b7nen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm ", "tokens": ["Komm"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und zeig ihr/ wie mein Hertz sie liebt.", "tokens": ["Und", "zeig", "ihr", "/", "wie", "mein", "Hertz", "sie", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOKOM", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will dann der Schlaff die Augen r\u00e4umen/", "tokens": ["Will", "dann", "der", "Schlaff", "die", "Au\u00b7gen", "r\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So dann es holde Blicke giebt/", "tokens": ["So", "dann", "es", "hol\u00b7de", "Bli\u00b7cke", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch gr\u00f6sser Gluht daraus zu saugen/", "tokens": ["Noch", "gr\u00f6s\u00b7ser", "Gluht", "da\u00b7raus", "zu", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die mich in steten Flammen h\u00e4lt.", "tokens": ["Die", "mich", "in", "ste\u00b7ten", "Flam\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ach schlafft ihr angenehmen Augen/", "tokens": ["Ach", "schlafft", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr Lichter dieser kleinen Welt.", "tokens": ["Ihr", "Lich\u00b7ter", "die\u00b7ser", "klei\u00b7nen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn wenn ihr schon in Ruhe liegt/", "tokens": ["Denn", "wenn", "ihr", "schon", "in", "Ru\u00b7he", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schl\u00e4fft doch eure Sch\u00f6nheit nicht/", "tokens": ["So", "schl\u00e4fft", "doch", "eu\u00b7re", "Sch\u00f6n\u00b7heit", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen auf den Wangen/", "tokens": ["Die", "Ro\u00b7sen", "auf", "den", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In holder Zierde prangen:", "tokens": ["In", "hol\u00b7der", "Zier\u00b7de", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Liljen-Brust", "tokens": ["Die", "Lil\u00b7jen\u00b7Brust"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Geist vergn\u00fcgt/", "tokens": ["Den", "Geist", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn sie mit Lust", "tokens": ["Wenn", "sie", "mit", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Aus ihren Schrancken bricht.", "tokens": ["Aus", "ih\u00b7ren", "Schran\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Eure Ruhe holde Sch\u00f6ne", "tokens": ["Eu\u00b7re", "Ru\u00b7he", "hol\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df mir eine Unruh seyn.", "tokens": ["Mu\u00df", "mir", "ei\u00b7ne", "Un\u00b7ruh", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die vielen Lieblichkeiten/", "tokens": ["Denn", "die", "vie\u00b7len", "Lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich schlaffend auch bestreiten/", "tokens": ["Die", "mich", "schlaf\u00b7fend", "auch", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmen meine Sinnen ein.", "tokens": ["Neh\u00b7men", "mei\u00b7ne", "Sin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Ruhe holde Sch\u00f6ne/", "tokens": ["Eu\u00b7re", "Ru\u00b7he", "hol\u00b7de", "Sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df mir eine Unruh seyn.", "tokens": ["Mu\u00df", "mir", "ei\u00b7ne", "Un\u00b7ruh", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}