{"textgrid.poem.33941": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Blume und Duft", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Fr\u00fchlings Heiligthume,", "tokens": ["In", "Fr\u00fch\u00b7lings", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dir ein Duft an's Tiefste r\u00fchrt,", "tokens": ["Wenn", "dir", "ein", "Duft", "an's", "Tiefs\u00b7te", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da suche nicht die Blume,", "tokens": ["Da", "su\u00b7che", "nicht", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihn ein Hauch entf\u00fchrt.", "tokens": ["Der", "ihn", "ein", "Hauch", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Duft l\u00e4\u00dft Ew'ges ahnen,", "tokens": ["Der", "Duft", "l\u00e4\u00dft", "Ew'\u00b7ges", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von unbegr\u00e4nztem Leben voll;", "tokens": ["Von", "un\u00b7be\u00b7gr\u00e4nz\u00b7tem", "Le\u00b7ben", "voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blume kann nur mahnen,", "tokens": ["Die", "Blu\u00b7me", "kann", "nur", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie schnell sie welken soll.", "tokens": ["Wie", "schnell", "sie", "wel\u00b7ken", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}