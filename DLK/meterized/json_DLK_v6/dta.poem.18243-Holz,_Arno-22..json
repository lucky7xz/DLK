{"dta.poem.18243": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Bist so s\u00fc\u00df wie zarte Frauenhuld,", "tokens": ["Bist", "so", "s\u00fc\u00df", "wie", "zar\u00b7te", "Frau\u00b7en\u00b7huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn dein tiefgeheimer Zauber labend", "tokens": ["Wenn", "dein", "tief\u00b7ge\u00b7hei\u00b7mer", "Zau\u00b7ber", "la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mich in wunderholde Tr\u00e4ume lullt.", "tokens": ["Mich", "in", "wun\u00b7der\u00b7hol\u00b7de", "Tr\u00e4u\u00b7me", "lullt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bin ich singend \u00fcber Land gezogen", "tokens": ["Bin", "ich", "sin\u00b7gend", "\u00fc\u00b7ber", "Land", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wohl den ganzen Tag im Sonnenschein", "tokens": ["Wohl", "den", "gan\u00b7zen", "Tag", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und nun schreit ich durch den Thoresbogen", "tokens": ["Und", "nun", "schreit", "ich", "durch", "den", "Tho\u00b7res\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In die altersgraue Stadt hinein.", "tokens": ["In", "die", "al\u00b7ters\u00b7grau\u00b7e", "Stadt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Von den holzgeschnitzten Giebelspitzen", "tokens": ["Von", "den", "holz\u00b7ge\u00b7schnitz\u00b7ten", "Gie\u00b7bel\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich schon l\u00e4ngst der letzte Schimmer stahl,", "tokens": ["Sich", "schon", "l\u00e4ngst", "der", "letz\u00b7te", "Schim\u00b7mer", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur die hohen Kirchenkreuze blitzen", "tokens": ["Nur", "die", "ho\u00b7hen", "Kir\u00b7chen\u00b7kreu\u00b7ze", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Golden noch im sp\u00e4ten Abendstrahl.", "tokens": ["Gol\u00b7den", "noch", "im", "sp\u00e4\u00b7ten", "A\u00b7bend\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kinder auf den Treppensteinen hocken,", "tokens": ["Kin\u00b7der", "auf", "den", "Trep\u00b7pen\u00b7stei\u00b7nen", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Spielen Haschen oder Blindekuh,", "tokens": ["Spie\u00b7len", "Ha\u00b7schen", "o\u00b7der", "Blin\u00b7de\u00b7kuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und dazwischen l\u00e4uten fromm die Glocken", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "l\u00e4u\u00b7ten", "fromm", "die", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ADJD", "ART", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Von den Th\u00fcrmen Feierabendruh.", "tokens": ["Von", "den", "Th\u00fcr\u00b7men", "Fei\u00b7er\u00b7a\u00b7ben\u00b7druh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wer sich abgem\u00fcht in Tagesschw\u00fcle,", "tokens": ["Wer", "sich", "ab\u00b7ge\u00b7m\u00fcht", "in", "Ta\u00b7ges\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruht im Schoo\u00dfe seiner Lieben aus;", "tokens": ["Ruht", "im", "Schoo\u00b7\u00dfe", "sei\u00b7ner", "Lie\u00b7ben", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herzerquickend duftet ihm die K\u00fchle,", "tokens": ["Herz\u00b7er\u00b7qui\u00b7ckend", "duf\u00b7tet", "ihm", "die", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein frischgepfl\u00fcckter Blumenstrau\u00df.", "tokens": ["Wie", "ein", "frischge\u00b7pfl\u00fcck\u00b7ter", "Blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rollt kein Wagen mehr, es schl\u00e4gt kein Hammer,", "tokens": ["Rollt", "kein", "Wa\u00b7gen", "mehr", ",", "es", "schl\u00e4gt", "kein", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn der Werkeltag ist l\u00e4ngst verrauscht;", "tokens": ["Denn", "der", "Wer\u00b7kel\u00b7tag", "ist", "l\u00e4ngst", "ver\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "L\u00e4mpchen knistert schon in stiller Kammer,", "tokens": ["L\u00e4mp\u00b7chen", "knis\u00b7tert", "schon", "in", "stil\u00b7ler", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Drin der Nestling Mutters M\u00e4rchen lauscht.", "tokens": ["Drin", "der", "Nest\u00b7ling", "Mut\u00b7ters", "M\u00e4r\u00b7chen", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Immer stiller wird es auf den Gassen,", "tokens": ["Im\u00b7mer", "stil\u00b7ler", "wird", "es", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer heimlicher die D\u00e4mmrung winkt,", "tokens": ["Im\u00b7mer", "heim\u00b7li\u00b7cher", "die", "D\u00e4mm\u00b7rung", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis das Giebeldach die silberblassen,", "tokens": ["Bis", "das", "Gie\u00b7bel\u00b7dach", "die", "sil\u00b7ber\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mondgewebten Flimmerstrahlen trinkt.", "tokens": ["Mond\u00b7ge\u00b7web\u00b7ten", "Flim\u00b7mer\u00b7strah\u00b7len", "trinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo in marktumpflanzten Lindenb\u00e4umen", "tokens": ["Wo", "in", "mark\u00b7tum\u00b7pflanz\u00b7ten", "Lin\u00b7den\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Funkenw\u00fcrmchen hin und wieder fliegt,", "tokens": ["Fun\u00b7ken\u00b7w\u00fcrm\u00b7chen", "hin", "und", "wie\u00b7der", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wandeln Liebende in s\u00fc\u00dfen Tr\u00e4umen,", "tokens": ["Wan\u00b7deln", "Lie\u00b7ben\u00b7de", "in", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Hand in Hand und Arm in Arm geschmiegt.", "tokens": ["Hand", "in", "Hand", "und", "Arm", "in", "Arm", "ge\u00b7schmiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mit den alten, halbverwaschnen Runnen", "tokens": ["Mit", "den", "al\u00b7ten", ",", "halb\u00b7ver\u00b7waschnen", "Run\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und dem steingehaunen Reckenbild", "tokens": ["Und", "dem", "stein\u00b7ge\u00b7hau\u00b7nen", "Re\u00b7cken\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Steht am Rathhauseck der Rolandsbrunnen,", "tokens": ["Steht", "am", "Rath\u00b7hau\u00b7seck", "der", "Ro\u00b7lands\u00b7brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der aus hundert R\u00f6hren t\u00f6nend quillt.", "tokens": ["Der", "aus", "hun\u00b7dert", "R\u00f6h\u00b7ren", "t\u00f6\u00b7nend", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Auf bemoostem Rande sitz' ich nieder,", "tokens": ["Auf", "be\u00b7moos\u00b7tem", "Ran\u00b7de", "sitz'", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ich schaue in die Fluthenpracht,", "tokens": ["Und", "ich", "schau\u00b7e", "in", "die", "Flut\u00b7hen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und ich lausche auf die Wiegenlieder,", "tokens": ["Und", "ich", "lau\u00b7sche", "auf", "die", "Wie\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bis mein Herz zur guten Ruh gebracht.", "tokens": ["Bis", "mein", "Herz", "zur", "gu\u00b7ten", "Ruh", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und da h\u00f6r ich, wie auf leisen Sohlen", "tokens": ["Und", "da", "h\u00f6r", "ich", ",", "wie", "auf", "lei\u00b7sen", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blonde Engel durch die Gassen gehn,", "tokens": ["Blon\u00b7de", "En\u00b7gel", "durch", "die", "Gas\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich blinzle ab und zu verstohlen,", "tokens": ["Und", "ich", "blinz\u00b7le", "ab", "und", "zu", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um die blonden Engel auch zu sehn.", "tokens": ["Um", "die", "blon\u00b7den", "En\u00b7gel", "auch", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O du lieber, linder Sommerabend,", "tokens": ["O", "du", "lie\u00b7ber", ",", "lin\u00b7der", "Som\u00b7mer\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bist so s\u00fc\u00df wie zarte Frauenhuld,", "tokens": ["Bist", "so", "s\u00fc\u00df", "wie", "zar\u00b7te", "Frau\u00b7en\u00b7huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn dein tiefgeheimer Zauber labend", "tokens": ["Wenn", "dein", "tief\u00b7ge\u00b7hei\u00b7mer", "Zau\u00b7ber", "la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mich in wunderholde Tr\u00e4ume lullt!", "tokens": ["Mich", "in", "wun\u00b7der\u00b7hol\u00b7de", "Tr\u00e4u\u00b7me", "lullt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}