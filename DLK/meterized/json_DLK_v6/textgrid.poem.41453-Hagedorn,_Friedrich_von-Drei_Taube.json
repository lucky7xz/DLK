{"textgrid.poem.41453": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Drei Taube", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es haben oft zugleich der Leser und der Dichter,", "tokens": ["Es", "ha\u00b7ben", "oft", "zu\u00b7gleich", "der", "Le\u00b7ser", "und", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch der Criticus kein zuverl\u00e4ssig Ohr.", "tokens": ["Und", "auch", "der", "Cri\u00b7ti\u00b7cus", "kein", "zu\u00b7ver\u00b7l\u00e4s\u00b7sig", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NE", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lud vor einen tauben Richter", "tokens": ["So", "lud", "vor", "ei\u00b7nen", "tau\u00b7ben", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Tauber einen Tauben vor.", "tokens": ["Ein", "Tau\u00b7ber", "ei\u00b7nen", "Tau\u00b7ben", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kl\u00e4ger sagt': Auf meinem Felde", "tokens": ["Der", "Kl\u00e4\u00b7ger", "sagt'", ":", "Auf", "mei\u00b7nem", "Fel\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat er dem Wilde nachgehetzt.", "tokens": ["Hat", "er", "dem", "Wil\u00b7de", "nach\u00b7ge\u00b7hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beklagter: Nein; von seinem Gelde", "tokens": ["Be\u00b7klag\u00b7ter", ":", "Nein", ";", "von", "sei\u00b7nem", "Gel\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PTKANT", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "War l\u00e4ngst das Drittheil abgesetzt.", "tokens": ["War", "l\u00e4ngst", "das", "Dritt\u00b7heil", "ab\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Richter sprach: Das Recht der Ehen", "tokens": ["Der", "Rich\u00b7ter", "sprach", ":", "Das", "Recht", "der", "E\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bleibt heilig, alt und allgemein.", "tokens": ["Bleibt", "hei\u00b7lig", ",", "alt", "und", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es soll die Heirath vor sich gehen,", "tokens": ["Es", "soll", "die", "Hei\u00b7rath", "vor", "sich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und ich will bei der Hochzeit sein!", "tokens": ["Und", "ich", "will", "bei", "der", "Hoch\u00b7zeit", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}}}}