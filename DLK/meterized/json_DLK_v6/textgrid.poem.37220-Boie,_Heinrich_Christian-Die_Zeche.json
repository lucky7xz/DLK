{"textgrid.poem.37220": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeche", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob laut an uns der Gr\u00e4mler Zunft", "tokens": ["Ob", "laut", "an", "uns", "der", "Gr\u00e4m\u00b7ler", "Zunft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entbehrte Freuden r\u00e4che,", "tokens": ["Ent\u00b7behr\u00b7te", "Freu\u00b7den", "r\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und viel von Weisheit spreche", "tokens": ["Und", "viel", "von", "Weis\u00b7heit", "spre\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alles meisternde Vernunft,", "tokens": ["Die", "al\u00b7les", "meis\u00b7tern\u00b7de", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch juble bis zur Wiederkunft", "tokens": ["Doch", "jub\u00b7le", "bis", "zur", "Wie\u00b7der\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Tages unsre Zeche!", "tokens": ["Des", "Ta\u00b7ges", "uns\u00b7re", "Ze\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n Vernunft auch schwatzen kann,", "tokens": ["Wie", "sch\u00f6n", "Ver\u00b7nunft", "auch", "schwat\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wei\u00df sie von Vergn\u00fcgen?", "tokens": ["Was", "wei\u00df", "sie", "von", "Ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um unsers uns betr\u00fcgen", "tokens": ["Um", "un\u00b7sers", "uns", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll wahrlich kein verschrobner Mann.", "tokens": ["Soll", "wahr\u00b7lich", "kein", "ver\u00b7schrob\u00b7ner", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum sezt das Glas nur wieder an", "tokens": ["Drum", "sezt", "das", "Glas", "nur", "wie\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lerts in vollen Z\u00fcgen!", "tokens": ["Und", "lerts", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}