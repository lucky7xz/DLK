{"dta.poem.3867": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen/ der durch Diebstahl  \n verungl\u00fcckt worden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Vo haben denn verdammte H\u00e4nde", "tokens": ["Vo", "ha\u00b7ben", "denn", "ver\u00b7damm\u00b7te", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir deine sonst so festen W\u00e4nde", "tokens": ["Dir", "dei\u00b7ne", "sonst", "so", "fes\u00b7ten", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In der nur letzt vergangnen Nacht", "tokens": ["In", "der", "nur", "letzt", "ver\u00b7gang\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem Stahl durchbohrt gemacht,", "tokens": ["Mit", "ih\u00b7rem", "Stahl", "durch\u00b7bohrt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und m\u00fcssen dir, wer folt es glauben,", "tokens": ["Und", "m\u00fcs\u00b7sen", "dir", ",", "wer", "folt", "es", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein sch\u00f6n und halb Verm\u00f6gen rauben.", "tokens": ["Dein", "sch\u00f6n", "und", "halb", "Ver\u00b7m\u00f6\u00b7gen", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die falsch und freche R\u00e4uber-Schaar,", "tokens": ["Die", "falsch", "und", "fre\u00b7che", "R\u00e4u\u00b7ber\u00b7Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So Mord- und Blutbegierig war,", "tokens": ["So", "Mord", "und", "Blut\u00b7be\u00b7gie\u00b7rig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da sie die Pr\u00fcgel liegen lassen,", "tokens": ["Da", "sie", "die", "Pr\u00fc\u00b7gel", "lie\u00b7gen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Um desto k\u00fchner aufzupassen,", "tokens": ["Um", "des\u00b7to", "k\u00fch\u00b7ner", "auf\u00b7zu\u00b7pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Damit, wenn etwa jemand k\u00e4m,", "tokens": ["Da\u00b7mit", ",", "wenn", "et\u00b7wa", "je\u00b7mand", "k\u00e4m", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Man ihm das arme Leben n\u00e4hm.", "tokens": ["Man", "ihm", "das", "ar\u00b7me", "Le\u00b7ben", "n\u00e4hm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Verflucht, verdammt und diebisch Wesen!", "tokens": ["Ver\u00b7flucht", ",", "ver\u00b7dammt", "und", "die\u00b7bisch", "We\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So bist du in Gefahr gewesen,", "tokens": ["So", "bist", "du", "in", "Ge\u00b7fahr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das Leben hast du zum Gewinn,", "tokens": ["Das", "Le\u00b7ben", "hast", "du", "zum", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Allein das Deine ist dahin.", "tokens": ["Al\u00b7lein", "das", "Dei\u00b7ne", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSAT", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Freund! sey bey dem Verlust gelassen,", "tokens": ["Freund", "!", "sey", "bey", "dem", "Ver\u00b7lust", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Freund! suche dich dabey zu fassen.", "tokens": ["Freund", "!", "su\u00b7che", "dich", "da\u00b7bey", "zu", "fas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PRF", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Verliehrst du gleich dein halbes Gut,", "tokens": ["Ver\u00b7liehrst", "du", "gleich", "dein", "hal\u00b7bes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Behalte deinen frohen Muth,", "tokens": ["Be\u00b7hal\u00b7te", "dei\u00b7nen", "fro\u00b7hen", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du wirst mit innigsten Vergn\u00fcgen", "tokens": ["Du", "wirst", "mit", "in\u00b7nigs\u00b7ten", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Noch alles doppelt wieder kriegen.", "tokens": ["Noch", "al\u00b7les", "dop\u00b7pelt", "wie\u00b7der", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wir wollen nur gelassen seyn.", "tokens": ["Wir", "wol\u00b7len", "nur", "ge\u00b7las\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Kommt bald ein solcher R\u00e4uber ein,", "tokens": ["Kommt", "bald", "ein", "sol\u00b7cher", "R\u00e4u\u00b7ber", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Denn wird er seine Schuld zu b\u00fcssen,", "tokens": ["Denn", "wird", "er", "sei\u00b7ne", "Schuld", "zu", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Herr Hamans Kirchhof leiden m\u00fcssen,", "tokens": ["Herr", "Ha\u00b7mans", "Kirch\u00b7hof", "lei\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und di\u00df nach wohlverdienter Schuld.", "tokens": ["Und", "di\u00df", "nach", "wohl\u00b7ver\u00b7dien\u00b7ter", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Freund! aber deiner fernern Huld", "tokens": ["Freund", "!", "a\u00b7ber", "dei\u00b7ner", "fer\u00b7nern", "Huld"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "W\u00fcnscht sich, da Zeil und Schrifft zu Ende,", "tokens": ["W\u00fcnscht", "sich", ",", "da", "Zeil", "und", "Schrifft", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ein treu und redlicher Studente.", "tokens": ["Ein", "treu", "und", "red\u00b7li\u00b7cher", "Stu\u00b7den\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}