{"textgrid.poem.62624": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Fratscher- und Weltzische Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast bey schlechter Zeit,", "tokens": ["Du", "hast", "bey", "schlech\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo hast du hingedacht? geliebet und gefreyt.", "tokens": ["Wo", "hast", "du", "hin\u00b7ge\u00b7dacht", "?", "ge\u00b7lie\u00b7bet", "und", "ge\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "$.", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Zeiten sind ja schlecht, die Sitten sind verdorben.", "tokens": ["Die", "Zei\u00b7ten", "sind", "ja", "schlecht", ",", "die", "Sit\u00b7ten", "sind", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Treu und Redlichkeit ist g\u00e4nzlich ausgestorben.", "tokens": ["Die", "Treu", "und", "Red\u00b7lich\u00b7keit", "ist", "g\u00e4nz\u00b7lich", "aus\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Heucheley florirt; weil nur Betrug und List,", "tokens": ["Die", "Heu\u00b7che\u00b7ley", "flo\u00b7rirt", ";", "weil", "nur", "Be\u00b7trug", "und", "List", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wind, Prahlsucht, Eigennutz im gr\u00f6\u00dften Schwange ist.", "tokens": ["Wind", ",", "Prahl\u00b7sucht", ",", "Ei\u00b7gen\u00b7nutz", "im", "gr\u00f6\u00df\u00b7ten", "Schwan\u00b7ge", "ist", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Dem ist der Kopf voll Dunst, der Mund voll artger Sachen,", "tokens": ["Dem", "ist", "der", "Kopf", "voll", "Dunst", ",", "der", "Mund", "voll", "art\u00b7ger", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der schl\u00e4ft sich Mittags satt, um in der Nacht zu wachen.", "tokens": ["Der", "schl\u00e4ft", "sich", "Mit\u00b7tags", "satt", ",", "um", "in", "der", "Nacht", "zu", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$,", "KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der will beym Monden-Schein Botanisiren gehn;", "tokens": ["Der", "will", "beym", "Mon\u00b7den\u00b7Schein", "Bo\u00b7ta\u00b7ni\u00b7si\u00b7ren", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nach der Sternen Heer bey Sonnen-Strahlen sehn.", "tokens": ["Und", "nach", "der", "Ster\u00b7nen", "Heer", "bey", "Son\u00b7nen\u00b7Strah\u00b7len", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie bunt und wunderlich gehts in der Welt jetzunder!", "tokens": ["Wie", "bunt", "und", "wun\u00b7der\u00b7lich", "gehts", "in", "der", "Welt", "je\u00b7tzun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Man h\u00f6rt, und sp\u00fchrt, und sieht fast t\u00e4glich neue Wunder.", "tokens": ["Man", "h\u00f6rt", ",", "und", "sp\u00fchrt", ",", "und", "sieht", "fast", "t\u00e4g\u00b7lich", "neu\u00b7e", "Wun\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Betracht den Handel-Stand, den Land und See vermehrt,", "tokens": ["Be\u00b7tracht", "den", "Han\u00b7del\u00b7Stand", ",", "den", "Land", "und", "See", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit was vor Sorg und M\u00fch ist dieser nicht beschwert?", "tokens": ["Mit", "was", "vor", "Sorg", "und", "M\u00fch", "ist", "die\u00b7ser", "nicht", "be\u00b7schwert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "NN", "VAFIN", "PDS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was vor Gefahr und Last, was vor Verdr\u00fc\u00dflichkeiten", "tokens": ["Was", "vor", "Ge\u00b7fahr", "und", "Last", ",", "was", "vor", "Ver\u00b7dr\u00fc\u00df\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sind nicht mit ihm verkn\u00fcpft, und m\u00fcssen ihn begleiten;", "tokens": ["Sind", "nicht", "mit", "ihm", "ver\u00b7kn\u00fcpft", ",", "und", "m\u00fcs\u00b7sen", "ihn", "be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie wird die Redlichkeit mit guten K\u00e4ufern rar?", "tokens": ["Wie", "wird", "die", "Red\u00b7lich\u00b7keit", "mit", "gu\u00b7ten", "K\u00e4u\u00b7fern", "rar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wer bezahlt, wie sonst, die sch\u00f6nen Waaren baar?", "tokens": ["Und", "wer", "be\u00b7zahlt", ",", "wie", "sonst", ",", "die", "sch\u00f6\u00b7nen", "Waa\u00b7ren", "baar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So gehts, man borgt vielmehr. Mahnt man hernach die Schuldt", "tokens": ["So", "gehts", ",", "man", "borgt", "viel\u00b7mehr", ".", "Mahnt", "man", "her\u00b7nach", "die", "Schuldt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "$.", "VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "So heists: man m\u00f6chte sich in etwas noch gedulden.", "tokens": ["So", "heists", ":", "man", "m\u00f6ch\u00b7te", "sich", "in", "et\u00b7was", "noch", "ge\u00b7dul\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PIS", "VMFIN", "PRF", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hilft nach geraumer Zeit die H\u00f6flichkeit nicht mehr,", "tokens": ["Hilft", "nach", "ge\u00b7rau\u00b7mer", "Zeit", "die", "H\u00f6f\u00b7lich\u00b7keit", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und man dringt auf das Geld; da l\u00e4stert man gar sehr.", "tokens": ["Und", "man", "dringt", "auf", "das", "Geld", ";", "da", "l\u00e4s\u00b7tert", "man", "gar", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Man leget sich auf List, und durch Betr\u00fcgereyen", "tokens": ["Man", "le\u00b7get", "sich", "auf", "List", ",", "und", "durch", "Be\u00b7tr\u00fc\u00b7ge\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NE", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Sucht man sich von der Schuld und Zahlung zu befreyen.", "tokens": ["Sucht", "man", "sich", "von", "der", "Schuld", "und", "Zah\u00b7lung", "zu", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie manche Schuld wird nicht ins Conto-Buch gebracht,", "tokens": ["Wie", "man\u00b7che", "Schuld", "wird", "nicht", "ins", "Con\u00b7to\u00b7Buch", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Schaden ist sehr gro\u00df; der Schuldner geht und lacht.", "tokens": ["Der", "Scha\u00b7den", "ist", "sehr", "gro\u00df", ";", "der", "Schuld\u00b7ner", "geht", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So klug ein Kaufmann ist, (durch Schaden wird man kl\u00fcger)", "tokens": ["So", "klug", "ein", "Kauf\u00b7mann", "ist", ",", "(", "durch", "Scha\u00b7den", "wird", "man", "kl\u00fc\u00b7ger", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "$(", "APPR", "NN", "VAFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So weis er dennoch nicht von Frommen, vom Betr\u00fcger", "tokens": ["So", "weis", "er", "den\u00b7noch", "nicht", "von", "From\u00b7men", ",", "vom", "Be\u00b7tr\u00fc\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "PPER", "ADV", "PTKNEG", "APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den Unterschied zu sehn: Weil sich der letztre stellt,", "tokens": ["Den", "Un\u00b7ter\u00b7schied", "zu", "sehn", ":", "Weil", "sich", "der", "letz\u00b7tre", "stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "KOUS", "PRF", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Er sey der Christlichste und Redlichste der Welt.", "tokens": ["Er", "sey", "der", "Christ\u00b7lichs\u00b7te", "und", "Red\u00b7lichs\u00b7te", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach eine schlimme Zeit ist jetzt vorm Handels-Orden!", "tokens": ["Ach", "ei\u00b7ne", "schlim\u00b7me", "Zeit", "ist", "jetzt", "vorm", "Han\u00b7dels\u00b7Or\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich sage jetzo nicht, was dort in S\u00fcd und Norden", "tokens": ["Ich", "sa\u00b7ge", "jet\u00b7zo", "nicht", ",", "was", "dort", "in", "S\u00fcd", "und", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bisher die Kaufmannschaft erlitten und gesehn;", "tokens": ["Bis\u00b7her", "die", "Kauf\u00b7mann\u00b7schaft", "er\u00b7lit\u00b7ten", "und", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Man frage Deutschland nur; was ist nicht j\u00fcngst geschehn?", "tokens": ["Man", "fra\u00b7ge", "Deutschland", "nur", ";", "was", "ist", "nicht", "j\u00fcngst", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "ADV", "$.", "PWS", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Wie manches sch\u00f6nes Gl\u00fcck ist durch Ver\u00e4nderungen", "tokens": ["Wie", "man\u00b7ches", "sch\u00f6\u00b7nes", "Gl\u00fcck", "ist", "durch", "Ver\u00b7\u00e4n\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der M\u00fcnzen hier und da der Handlung mi\u00dfgelungen?", "tokens": ["Der", "M\u00fcn\u00b7zen", "hier", "und", "da", "der", "Hand\u00b7lung", "mi\u00df\u00b7ge\u00b7lun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie hat man sie dadurch gedr\u00fccket und beschwert?", "tokens": ["Wie", "hat", "man", "sie", "da\u00b7durch", "ge\u00b7dr\u00fc\u00b7cket", "und", "be\u00b7schwert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PPER", "PAV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hat nicht das gute Geld zugleich mit aufgeh\u00f6rt?", "tokens": ["Hat", "nicht", "das", "gu\u00b7te", "Geld", "zu\u00b7gleich", "mit", "auf\u00b7ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "ADV", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Man tr\u00f6stete sich zwar, da\u00df doch der Lage B\u00fcrde", "tokens": ["Man", "tr\u00f6s\u00b7te\u00b7te", "sich", "zwar", ",", "da\u00df", "doch", "der", "La\u00b7ge", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sich durch die Aenderung nunmehr verlieren w\u00fcrde;", "tokens": ["Sich", "durch", "die", "A\u00b7en\u00b7de\u00b7rung", "nun\u00b7mehr", "ver\u00b7lie\u00b7ren", "w\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Allein der Trost ist hin, es bleibt doch, wie es ist,", "tokens": ["Al\u00b7lein", "der", "Trost", "ist", "hin", ",", "es", "bleibt", "doch", ",", "wie", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ob man gleich an Procent fast zweymahl zehn vermi\u00dft!", "tokens": ["Ob", "man", "gleich", "an", "Pro\u00b7cent", "fast", "zwey\u00b7mahl", "zehn", "ver\u00b7mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "ADV", "ADV", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die\u00df ist dir, ", "tokens": ["Die\u00df", "ist", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und gleichwohl steckst du dich in Haus- und Wirtschafts Sorgen.", "tokens": ["Und", "gleich\u00b7wohl", "steckst", "du", "dich", "in", "Haus", "und", "Wirt\u00b7schafts", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "TRUNC", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du weist wies jetzo geht; und du hast doch gefreyt.", "tokens": ["Du", "weist", "wies", "jet\u00b7zo", "geht", ";", "und", "du", "hast", "doch", "ge\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "VVFIN", "$.", "KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch mehr, ein Handels-Mann wird dadurch mehr gekr\u00e4nket,", "tokens": ["Noch", "mehr", ",", "ein", "Han\u00b7dels\u00b7Mann", "wird", "da\u00b7durch", "mehr", "ge\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "VAFIN", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Weil manches M\u00e4dgen sich oft spr\u00f6de gnug bedenket,", "tokens": ["Weil", "man\u00b7ches", "M\u00e4d\u00b7gen", "sich", "oft", "spr\u00f6\u00b7de", "gnug", "be\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob sie ihn nehmen will. Da heists der mich will freyn,", "tokens": ["Ob", "sie", "ihn", "neh\u00b7men", "will", ".", "Da", "heists", "der", "mich", "will", "freyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$.", "ADV", "VVFIN", "ART", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mu\u00df graduirt, galant und sch\u00f6n gekleidet seyn.", "tokens": ["Mu\u00df", "gra\u00b7du\u00b7irt", ",", "ga\u00b7lant", "und", "sch\u00f6n", "ge\u00b7klei\u00b7det", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "$,", "ADJD", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O! stellt euch doch gelehrt, schneidt auf, was ihr nur k\u00f6nnet,", "tokens": ["O", "!", "stellt", "euch", "doch", "ge\u00b7lehrt", ",", "schneidt", "auf", ",", "was", "ihr", "nur", "k\u00f6n\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Borgt, machts so bunt ihrs wi\u00dft, da\u00df man euch herrlich nennet", "tokens": ["Borgt", ",", "machts", "so", "bunt", "ihrs", "wi\u00dft", ",", "da\u00df", "man", "euch", "herr\u00b7lich", "nen\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADJD", "NN", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und folglich gl\u00fccklich liebt. Geht! zeigt euch hier und dort!", "tokens": ["Und", "folg\u00b7lich", "gl\u00fcck\u00b7lich", "liebt", ".", "Geht", "!", "zeigt", "euch", "hier", "und", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sucht euch Gesellschaft aus! geht an publicen Ort,", "tokens": ["Sucht", "euch", "Ge\u00b7sell\u00b7schaft", "aus", "!", "geht", "an", "pub\u00b7li\u00b7cen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Und schwatzt und sprecht alda von hochgelehrten Sachen,", "tokens": ["Und", "schwatzt", "und", "sprecht", "al\u00b7da", "von", "hoch\u00b7ge\u00b7lehr\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sucht grosse, wichtige Processe weis zu machen.", "tokens": ["Sucht", "gros\u00b7se", ",", "wich\u00b7ti\u00b7ge", "Pro\u00b7ces\u00b7se", "weis", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zieht kluge M\u00e4nner durch, verh\u00f6hnet sie, und sprecht", "tokens": ["Zieht", "klu\u00b7ge", "M\u00e4n\u00b7ner", "durch", ",", "ver\u00b7h\u00f6h\u00b7net", "sie", ",", "und", "sprecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von Leuten von Verstand und Tugend schn\u00f6d und schlecht;", "tokens": ["Von", "Leu\u00b7ten", "von", "Ver\u00b7stand", "und", "Tu\u00b7gend", "schn\u00f6d", "und", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So k\u00f6nnt ihr gl\u00fccklich seyn. Seht! solche Freyer werden", "tokens": ["So", "k\u00f6nnt", "ihr", "gl\u00fcck\u00b7lich", "seyn", ".", "Seht", "!", "sol\u00b7che", "Frey\u00b7er", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$.", "VVFIN", "$.", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von M\u00e4dgen auserwehlt: Ob sie schon auf der Erden", "tokens": ["Von", "M\u00e4d\u00b7gen", "au\u00b7ser\u00b7wehlt", ":", "Ob", "sie", "schon", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich und der Republic gar wenig n\u00fctzlich sind.", "tokens": ["Sich", "und", "der", "Re\u00b7pub\u00b7lic", "gar", "we\u00b7nig", "n\u00fctz\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So liebt man etwas Ehr und einen Mund voll Wind.", "tokens": ["So", "liebt", "man", "et\u00b7was", "Ehr", "und", "ei\u00b7nen", "Mund", "voll", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jedoch man findet auch noch Leute andrer Sinnen,", "tokens": ["Je\u00b7doch", "man", "fin\u00b7det", "auch", "noch", "Leu\u00b7te", "an\u00b7drer", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "ADV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die auch den Handelstand besonders liebgewinnen,", "tokens": ["Die", "auch", "den", "Han\u00b7del\u00b7stand", "be\u00b7son\u00b7ders", "lieb\u00b7ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie sch\u00e4tzen ihn sehr hoch, und sehn den Nutzen an,", "tokens": ["Sie", "sch\u00e4t\u00b7zen", "ihn", "sehr", "hoch", ",", "und", "sehn", "den", "Nut\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Stadt und Land von ihm vortreflich haben kan.", "tokens": ["Den", "Stadt", "und", "Land", "von", "ihm", "vor\u00b7tre\u00b7flich", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Wie manche hohe Macht sucht ihn vor allen Dingen", "tokens": ["Wie", "man\u00b7che", "ho\u00b7he", "Macht", "sucht", "ihn", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mit Kosten, Flei\u00df und M\u00fch in vollen Flor zu bringen?", "tokens": ["Mit", "Kos\u00b7ten", ",", "Flei\u00df", "und", "M\u00fch", "in", "vol\u00b7len", "Flor", "zu", "brin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie legen diesem Stand nicht wenig Ehr bey,", "tokens": ["Sie", "le\u00b7gen", "die\u00b7sem", "Stand", "nicht", "we\u00b7nig", "Ehr", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PTKNEG", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Und zeigen dadurch an, wie edel dieser sey.", "tokens": ["Und", "zei\u00b7gen", "da\u00b7durch", "an", ",", "wie", "e\u00b7del", "die\u00b7ser", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKVZ", "$,", "PWAV", "ADJD", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Halbgelehrter wu\u00dft in einem Heyraths-Schreiben", "tokens": ["Ein", "Halb\u00b7ge\u00b7lehr\u00b7ter", "wu\u00dft", "in", "ei\u00b7nem", "Hey\u00b7raths\u00b7Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An die vermeinte Braut ihn herrlich zu beschreiben.", "tokens": ["An", "die", "ver\u00b7mein\u00b7te", "Braut", "ihn", "herr\u00b7lich", "zu", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er stellte sinnreich vor, er hab zur Handlung Lust,", "tokens": ["Er", "stell\u00b7te", "sinn\u00b7reich", "vor", ",", "er", "hab", "zur", "Hand\u00b7lung", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PPER", "VAFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie sey nunmehr sein Trost und Labsaal seiner Brust.", "tokens": ["Sie", "sey", "nun\u00b7mehr", "sein", "Trost", "und", "Lab\u00b7saal", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wolte seiner Braut dadurch die Zeit verbessern,", "tokens": ["Er", "wol\u00b7te", "sei\u00b7ner", "Braut", "da\u00b7durch", "die", "Zeit", "ver\u00b7bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und um ein merkliches ihr Capital vergr\u00f6ssern.", "tokens": ["Und", "um", "ein", "merk\u00b7li\u00b7ches", "ihr", "Ca\u00b7pi\u00b7tal", "ver\u00b7gr\u00f6s\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er f\u00fchrte zum Bewei\u00df, da\u00df seine Rede wahr,", "tokens": ["Er", "f\u00fchr\u00b7te", "zum", "Be\u00b7wei\u00df", ",", "da\u00df", "sei\u00b7ne", "Re\u00b7de", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gar viel in Erfurt an, die jetzt ganz offenbar", "tokens": ["Gar", "viel", "in", "Er\u00b7furt", "an", ",", "die", "jetzt", "ganz", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NE", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In vollem Flore stehn, und wenig Anfang hatten.", "tokens": ["In", "vol\u00b7lem", "Flo\u00b7re", "stehn", ",", "und", "we\u00b7nig", "An\u00b7fang", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch ein Exempel kam ihm die\u00dffals wohl zu statten,", "tokens": ["Noch", "ein", "Ex\u00b7em\u00b7pel", "kam", "ihm", "die\u00df\u00b7fals", "wohl", "zu", "stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Er schrieb von einem Mann, der mir zwar nicht bekannt,", "tokens": ["Er", "schrieb", "von", "ei\u00b7nem", "Mann", ",", "der", "mir", "zwar", "nicht", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie der ein reiches Kind vom Dorf und Bauren-Stand.", "tokens": ["Wie", "der", "ein", "rei\u00b7ches", "Kind", "vom", "Dorf", "und", "Bau\u00b7ren\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zur Braut sich auserwehlt, und sey nun in dem Orden", "tokens": ["Zur", "Braut", "sich", "au\u00b7ser\u00b7wehlt", ",", "und", "sey", "nun", "in", "dem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "VVPP", "$,", "KON", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Kaufmannschaft, durch sie, ein grosser Kauffmann worden.", "tokens": ["Der", "Kauf\u00b7mann\u00b7schaft", ",", "durch", "sie", ",", "ein", "gros\u00b7ser", "Kauf\u00b7fmann", "wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Tugend, welche dich belebet und beseelt,", "tokens": ["Die", "Tu\u00b7gend", ",", "wel\u00b7che", "dich", "be\u00b7le\u00b7bet", "und", "be\u00b7seelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hie\u00df dich ein ", "tokens": ["Hie\u00df", "dich", "ein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So kan dich deine Wahl auch nimmermehr betr\u00fcben.", "tokens": ["So", "kan", "dich", "dei\u00b7ne", "Wahl", "auch", "nim\u00b7mer\u00b7mehr", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So rein Dich deine Braut auch liebet und verehrt;", "tokens": ["So", "rein", "Dich", "dei\u00b7ne", "Braut", "auch", "lie\u00b7bet", "und", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So w\u00fcrde ihre Lust doch w\u00fcrklich stark vermehrt,", "tokens": ["So", "w\u00fcr\u00b7de", "ih\u00b7re", "Lust", "doch", "w\u00fcrk\u00b7lich", "stark", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sie den sch\u00f6nen Brief in ihre Hand bek\u00e4me,", "tokens": ["Wenn", "sie", "den", "sch\u00f6\u00b7nen", "Brief", "in", "ih\u00b7re", "Hand", "be\u00b7k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dessen Inhalt recht durchl\u00e4s und selbst vern\u00e4hme.", "tokens": ["Und", "des\u00b7sen", "In\u00b7halt", "recht", "durch\u00b7l\u00e4s", "und", "selbst", "ver\u00b7n\u00e4h\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADJD", "ADJA", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Indessen, da dir nun das Gl\u00fcck ", "tokens": ["In\u00b7des\u00b7sen", ",", "da", "dir", "nun", "das", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut ", "tokens": ["Heut"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So geht mein Wunsch dahin: ", "tokens": ["So", "geht", "mein", "Wunsch", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchr stets die Hand des Gl\u00fccks auf einer Rosen-Weyde,", "tokens": ["F\u00fchr", "stets", "die", "Hand", "des", "Gl\u00fccks", "auf", "ei\u00b7ner", "Ro\u00b7sen\u00b7Wey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit das Sprichwort gilt: Wer nach der Tugend tracht,", "tokens": ["Da\u00b7mit", "das", "Sprich\u00b7wort", "gilt", ":", "Wer", "nach", "der", "Tu\u00b7gend", "tracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$.", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Derselbe wird vom Gl\u00fcck und Seegen reich gemacht.", "tokens": ["Der\u00b7sel\u00b7be", "wird", "vom", "Gl\u00fcck", "und", "See\u00b7gen", "reich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es m\u00f6g der Wind des Gl\u00fccks stets eure M\u00fchle treiben,", "tokens": ["Es", "m\u00f6g", "der", "Wind", "des", "Gl\u00fccks", "stets", "eu\u00b7re", "M\u00fch\u00b7le", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird das M\u00fchlwerk gut benebst dem Handel bleiben.", "tokens": ["So", "wird", "das", "M\u00fchl\u00b7werk", "gut", "be\u00b7nebst", "dem", "Han\u00b7del", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}