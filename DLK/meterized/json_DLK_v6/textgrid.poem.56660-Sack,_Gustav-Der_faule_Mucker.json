{"textgrid.poem.56660": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der faule Mucker", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen schl\u00e4gt, als gei\u00dfelten", "tokens": ["Der", "Re\u00b7gen", "schl\u00e4gt", ",", "als", "gei\u00b7\u00dfel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "des grenzenlosen Himmels wilde Winde", "tokens": ["des", "gren\u00b7zen\u00b7lo\u00b7sen", "Him\u00b7mels", "wil\u00b7de", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den alten Mucker jagend hoch", "tokens": ["den", "al\u00b7ten", "Mu\u00b7cker", "ja\u00b7gend", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und mei\u00dfelten", "tokens": ["und", "mei\u00b7\u00dfel\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "aus Schnee und Schmutz und Stubendunst", "tokens": ["aus", "Schnee", "und", "Schmutz", "und", "Stu\u00b7ben\u00b7dunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "des Sommers nackte Glut heraus.", "tokens": ["des", "Som\u00b7mers", "nack\u00b7te", "Glut", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das gie\u00dft und bl\u00e4st und faucht und raucht,", "tokens": ["Das", "gie\u00dft", "und", "bl\u00e4st", "und", "faucht", "und", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bis \u00fcber Nacht", "tokens": ["bis", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "des Fr\u00fchlings H\u00fclle f\u00e4llt", "tokens": ["des", "Fr\u00fch\u00b7lings", "H\u00fcl\u00b7le", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und \u2013 breit und ungeschlacht", "tokens": ["und", "\u2013", "breit", "und", "un\u00b7ge\u00b7schlacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "fiebert der Sommer durch die Welt!", "tokens": ["fie\u00b7bert", "der", "Som\u00b7mer", "durch", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}