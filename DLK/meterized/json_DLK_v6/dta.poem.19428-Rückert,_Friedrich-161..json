{"dta.poem.19428": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "161.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Todten saget man: er ist zu Gott gegangen;", "tokens": ["Vom", "Tod\u00b7ten", "sa\u00b7get", "man", ":", "er", "ist", "zu", "Gott", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob zum Ewigen k\u00f6nnt' Endliches gelangen!", "tokens": ["Al\u00b7sob", "zum", "E\u00b7wi\u00b7gen", "k\u00f6nnt'", "End\u00b7li\u00b7ches", "ge\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alsob k\u00f6nnt' Endliches vom Ew'gen ferne seyn!", "tokens": ["Al\u00b7sob", "k\u00f6nnt'", "End\u00b7li\u00b7ches", "vom", "Ew'\u00b7gen", "fer\u00b7ne", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "APPRART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist, das ist, wo auch es ist, in Gott allein.", "tokens": ["Was", "ist", ",", "das", "ist", ",", "wo", "auch", "es", "ist", ",", "in", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PDS", "VAFIN", "$,", "PWAV", "ADV", "PPER", "VAFIN", "$,", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast in Gott gelebt, und bist in Gott geschieden,", "tokens": ["Du", "hast", "in", "Gott", "ge\u00b7lebt", ",", "und", "bist", "in", "Gott", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,", "KON", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bist geblieben, wo du warst, in Gottes Frieden.", "tokens": ["Und", "bist", "ge\u00b7blie\u00b7ben", ",", "wo", "du", "warst", ",", "in", "Got\u00b7tes", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "PWAV", "PPER", "VAFIN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ist die Seligkeit, zu der nicht wird gelangen", "tokens": ["Das", "ist", "die", "Se\u00b7lig\u00b7keit", ",", "zu", "der", "nicht", "wird", "ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PTKNEG", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele dort, in der sie hier nicht angefangen.", "tokens": ["Die", "See\u00b7le", "dort", ",", "in", "der", "sie", "hier", "nicht", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das ist die Seligkeit, die dort sich wird entfalten", "tokens": ["Das", "ist", "die", "Se\u00b7lig\u00b7keit", ",", "die", "dort", "sich", "wird", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jeder Seele, die sie hier im Keim enthalten.", "tokens": ["In", "je\u00b7der", "See\u00b7le", ",", "die", "sie", "hier", "im", "Keim", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie unentwickelt auch, wie eingewickelt sei", "tokens": ["Wie", "un\u00b7ent\u00b7wi\u00b7ckelt", "auch", ",", "wie", "ein\u00b7ge\u00b7wi\u00b7ckelt", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "$,", "PWAV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmelskeim, der Hauch des Himmels macht ihn frei.", "tokens": ["Der", "Him\u00b7mel\u00b7ske\u00b7im", ",", "der", "Hauch", "des", "Him\u00b7mels", "macht", "ihn", "frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die F\u00fclle tritt hervor, die H\u00fclle mu\u00df verwesen,", "tokens": ["Die", "F\u00fcl\u00b7le", "tritt", "her\u00b7vor", ",", "die", "H\u00fcl\u00b7le", "mu\u00df", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleich im Wandel bleibt die Wesenheit der Wesen.", "tokens": ["Und", "gleich", "im", "Wan\u00b7del", "bleibt", "die", "We\u00b7sen\u00b7heit", "der", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}