{"textgrid.poem.46087": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[da oben im G\u00e4rtchen]", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da oben im G\u00e4rtchen,", "tokens": ["Da", "o\u00b7ben", "im", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da wehet der Wind,", "tokens": ["Da", "we\u00b7het", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da sitzet Maria", "tokens": ["Da", "sit\u00b7zet", "Ma\u00b7ria"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und wieget ihr Kind,", "tokens": ["Und", "wie\u00b7get", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie wiegt es mit ihrer schneewei\u00dfen Hand,", "tokens": ["Sie", "wiegt", "es", "mit", "ih\u00b7rer", "schnee\u00b7wei\u00b7\u00dfen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und brauchet dazu gar kein Wiegenband.", "tokens": ["Und", "brau\u00b7chet", "da\u00b7zu", "gar", "kein", "Wie\u00b7gen\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will mich zur lieben Maria vermieten,", "tokens": ["Ich", "will", "mich", "zur", "lie\u00b7ben", "Ma\u00b7ria", "ver\u00b7mie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Will helfen ihr Kindlein recht flei\u00dfig zu wiegen,", "tokens": ["Will", "hel\u00b7fen", "ihr", "Kin\u00b7dlein", "recht", "flei\u00b7\u00dfig", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Da f\u00fchrt sie mich auch in ihr K\u00e4mmerlein ein,", "tokens": ["Da", "f\u00fchrt", "sie", "mich", "auch", "in", "ihr", "K\u00e4m\u00b7mer\u00b7lein", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+++-+--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "Da singen die lieben Engelein fein,", "tokens": ["Da", "sin\u00b7gen", "die", "lie\u00b7ben", "En\u00b7ge\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Da singen wir alle das Gloria,", "tokens": ["Da", "sin\u00b7gen", "wir", "al\u00b7le", "das", "Glo\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Das Gloria, Lieb Frau Maria!", "tokens": ["Das", "Glo\u00b7ria", ",", "Lieb", "Frau", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}