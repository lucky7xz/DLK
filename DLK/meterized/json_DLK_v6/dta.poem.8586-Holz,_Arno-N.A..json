{"dta.poem.8586": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Purpurne Fische", "tokens": ["Pur\u00b7pur\u00b7ne", "Fi\u00b7sche"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "schwimmen durch mein dunkles Wasser,", "tokens": ["schwim\u00b7men", "durch", "mein", "dunk\u00b7les", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "weisse Lotosblumen", "tokens": ["weis\u00b7se", "Lo\u00b7tos\u00b7blu\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "bl\u00fchn.", "tokens": ["bl\u00fchn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Immer neue Tempelkr\u00e4nze", "tokens": ["Im\u00b7mer", "neu\u00b7e", "Tem\u00b7pel\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bauten um mich die frommen V\u00f6lker.", "tokens": ["bau\u00b7ten", "um", "mich", "die", "from\u00b7men", "V\u00f6l\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Millionen Lippen d\u00fcrsten nach mir.", "tokens": ["Mil\u00b7lion\u00b7en", "Lip\u00b7pen", "d\u00fcrs\u00b7ten", "nach", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Langsam,", "tokens": ["Lang\u00b7sam", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "jedes Jahrhundert einen Tropfen hoch,", "tokens": ["je\u00b7des", "Jahr\u00b7hun\u00b7dert", "ei\u00b7nen", "Trop\u00b7fen", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "schwillt", "tokens": ["schwillt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "meine Flut.", "tokens": ["mei\u00b7ne", "Flut", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ueber bunte Porphyrtreppen sp\u00fcl ich um gr\u00fcne S\u00e4ulen.", "tokens": ["Ue\u00b7ber", "bun\u00b7te", "Por\u00b7phyr\u00b7trep\u00b7pen", "sp\u00fcl", "ich", "um", "gr\u00fc\u00b7ne", "S\u00e4u\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Tausend Kuppeln", "tokens": ["Tau\u00b7send", "Kup\u00b7peln"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "glitzern aus meinem Grund!", "tokens": ["glit\u00b7zern", "aus", "mei\u00b7nem", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}