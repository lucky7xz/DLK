{"dta.poem.19363": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wirkt Gerechtigkeit, es wirkt die Lieb' ein Band;", "tokens": ["Es", "wirkt", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "es", "wirkt", "die", "Lieb'", "ein", "Band", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wirken beide sch\u00f6n verbunden Hand in Hand!", "tokens": ["Wie", "wir\u00b7ken", "bei\u00b7de", "sch\u00f6n", "ver\u00b7bun\u00b7den", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum Gerechtigkeit, warum tr\u00e4gt Liebe Binden", "tokens": ["Wa\u00b7rum", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "wa\u00b7rum", "tr\u00e4gt", "Lie\u00b7be", "Bin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ums Aug'? um f\u00fcr der Welt Ungleichheit zu erblinden.", "tokens": ["Ums", "Aug'", "?", "um", "f\u00fcr", "der", "Welt", "Un\u00b7gleich\u00b7heit", "zu", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$.", "KOUI", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was die Gerechtigkeit h\u00e4lt \u00e4u\u00dferlich im Bund,", "tokens": ["Was", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "h\u00e4lt", "\u00e4u\u00b7\u00dfer\u00b7lich", "im", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt nur, weil innerlich die Liebe legt den Grund.", "tokens": ["H\u00e4lt", "nur", ",", "weil", "in\u00b7ner\u00b7lich", "die", "Lie\u00b7be", "legt", "den", "Grund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar was Gerechtigkeit verbindet, ist verbindlich,", "tokens": ["Zwar", "was", "Ge\u00b7rech\u00b7tig\u00b7keit", "ver\u00b7bin\u00b7det", ",", "ist", "ver\u00b7bind\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur Verbindlichkeit der Lieb' un\u00fcberwindlich.", "tokens": ["Doch", "nur", "Ver\u00b7bind\u00b7lich\u00b7keit", "der", "Lieb'", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn nicht Gerechtigkeit mit Liebe sich verb\u00e4nde,", "tokens": ["Wenn", "nicht", "Ge\u00b7rech\u00b7tig\u00b7keit", "mit", "Lie\u00b7be", "sich", "ver\u00b7b\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "APPR", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer w\u00e4re so gerecht, der im Gericht best\u00e4nde?", "tokens": ["Wer", "w\u00e4\u00b7re", "so", "ge\u00b7recht", ",", "der", "im", "Ge\u00b7richt", "be\u00b7st\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nur wo Gerechtigkeit und Liebe sind verb\u00fcndet,", "tokens": ["Nur", "wo", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Lie\u00b7be", "sind", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Menschenschuld ges\u00fchnt, und ird'scher Sinn ents\u00fcndet.", "tokens": ["Ist", "Men\u00b7schen\u00b7schuld", "ge\u00b7s\u00fchnt", ",", "und", "ird'\u00b7scher", "Sinn", "ent\u00b7s\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}