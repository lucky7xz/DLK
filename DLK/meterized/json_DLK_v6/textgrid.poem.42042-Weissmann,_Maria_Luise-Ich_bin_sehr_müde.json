{"textgrid.poem.42042": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ich bin sehr m\u00fcde", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Fenster lehnt sich weit in den Abend hinaus,", "tokens": ["Mein", "Fens\u00b7ter", "lehnt", "sich", "weit", "in", "den", "A\u00b7bend", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Wolken stehen \u00fcber den D\u00e4chern, ein Blumenstrau\u00df,", "tokens": ["Die", "Wol\u00b7ken", "ste\u00b7hen", "\u00fc\u00b7ber", "den", "D\u00e4\u00b7chern", ",", "ein", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Luft streichelt mich und ist sanft und voll gro\u00dfer G\u00fcte.", "tokens": ["Die", "Luft", "strei\u00b7chelt", "mich", "und", "ist", "sanft", "und", "voll", "gro\u00b7\u00dfer", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VAFIN", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Ich aber halte die H\u00e4nde gefaltet, denn ich bin m\u00fcde,", "tokens": ["Ich", "a\u00b7ber", "hal\u00b7te", "die", "H\u00e4n\u00b7de", "ge\u00b7fal\u00b7tet", ",", "denn", "ich", "bin", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und h\u00f6re verwundert auf das beschwingte Schreiten", "tokens": ["Und", "h\u00f6\u00b7re", "ver\u00b7wun\u00b7dert", "auf", "das", "be\u00b7schwing\u00b7te", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der Menschen, die auf der Stra\u00dfe vor\u00fcbergleiten,", "tokens": ["Der", "Men\u00b7schen", ",", "die", "auf", "der", "Stra\u00b7\u00dfe", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "So sehr sind ihnen heute die Glieder leicht.", "tokens": ["So", "sehr", "sind", "ih\u00b7nen", "heu\u00b7te", "die", "Glie\u00b7der", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nur ich liege, schwergebettet in meine M\u00fcde.", "tokens": ["Nur", "ich", "lie\u00b7ge", ",", "schwer\u00b7ge\u00b7bet\u00b7tet", "in", "mei\u00b7ne", "M\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Manchmal h\u00f6re ich einen Schritt, der Deinem gleicht,", "tokens": ["Manch\u00b7mal", "h\u00f6\u00b7re", "ich", "ei\u00b7nen", "Schritt", ",", "der", "Dei\u00b7nem", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Dann bin ich, Geliebter, wie die Musik der Schritte leicht", "tokens": ["Dann", "bin", "ich", ",", "Ge\u00b7lieb\u00b7ter", ",", "wie", "die", "Mu\u00b7sik", "der", "Schrit\u00b7te", "leicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und wie die Wolken \u00fcber den D\u00e4chern silberne Bl\u00fcte.", "tokens": ["Und", "wie", "die", "Wol\u00b7ken", "\u00fc\u00b7ber", "den", "D\u00e4\u00b7chern", "sil\u00b7ber\u00b7ne", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}