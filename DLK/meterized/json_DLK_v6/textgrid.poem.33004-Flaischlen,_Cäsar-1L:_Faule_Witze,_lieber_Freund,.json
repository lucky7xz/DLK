{"textgrid.poem.33004": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Faule Witze, lieber Freund,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Faule Witze, lieber Freund,", "tokens": ["Fau\u00b7le", "Wit\u00b7ze", ",", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "kann ein jeder Klempnermeister machen,", "tokens": ["kann", "ein", "je\u00b7der", "Klemp\u00b7ner\u00b7meis\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "faule Witze, \u00fcber die dann", "tokens": ["fau\u00b7le", "Wit\u00b7ze", ",", "\u00fc\u00b7ber", "die", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "andre Klempnermeister wieder lachen ...", "tokens": ["and\u00b7re", "Klemp\u00b7ner\u00b7meis\u00b7ter", "wie\u00b7der", "la\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Immer freilich, mein ich, k\u00f6nn man", "tokens": ["Im\u00b7mer", "frei\u00b7lich", ",", "mein", "ich", ",", "k\u00f6nn", "man"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "PPER", "$,", "VMFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "derart geistreich sich nicht fassen,", "tokens": ["der\u00b7art", "geist\u00b7reich", "sich", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und zuweilen, mein ich, k\u00f6nn man", "tokens": ["und", "zu\u00b7wei\u00b7len", ",", "mein", "ich", ",", "k\u00f6nn", "man"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "PPOSAT", "PPER", "$,", "VMFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ruhig auch was gelten lassen!", "tokens": ["ru\u00b7hig", "auch", "was", "gel\u00b7ten", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PIS", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}