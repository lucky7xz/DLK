{"dta.poem.1151": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "* 37. Der sch\u00f6ne riechende Mund.*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dein Gesicht und Ohr vergn\u00fcgt bey dieser Sch\u00f6nen/ ", "tokens": ["Ist", "dein", "Ge\u00b7sicht", "und", "Ohr", "ver\u00b7gn\u00fcgt", "bey", "die\u00b7ser", "Sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kanstu mit Lust den Ku\u00df der Wangen wieder k\u00e4un/", "tokens": ["Kans\u00b7tu", "mit", "Lust", "den", "Ku\u00df", "der", "Wan\u00b7gen", "wie\u00b7der", "k\u00e4un", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch weicher Glieder Schnee die k\u00fchne Hand erfreun/", "tokens": ["Durch", "wei\u00b7cher", "Glie\u00b7der", "Schnee", "die", "k\u00fch\u00b7ne", "Hand", "er\u00b7freun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was wiltu ihren Mund und Athem viel verh\u00f6nen?", "tokens": ["Was", "wil\u00b7tu", "ih\u00b7ren", "Mund", "und", "A\u00b7them", "viel", "ver\u00b7h\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sol sie den geilen Koth der fremden Katz entlehnen/", "tokens": ["Sol", "sie", "den", "gei\u00b7len", "Koth", "der", "frem\u00b7den", "Katz", "ent\u00b7leh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was nach dem Marder schmeckt/ in Haar und Kleider streun?", "tokens": ["Was", "nach", "dem", "Mar\u00b7der", "schmeckt", "/", "in", "Haar", "und", "Klei\u00b7der", "streun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wiltu Gewinn und Lust der Nase wegen scheun?", "tokens": ["Wil\u00b7tu", "Ge\u00b7winn", "und", "Lust", "der", "Na\u00b7se", "we\u00b7gen", "scheun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Geruch/ der schw\u00e4chste Sinn/ darff Liebe nicht bekr\u00f6nen.", "tokens": ["Ge\u00b7ruch", "/", "der", "schw\u00e4chs\u00b7te", "Sinn", "/", "darff", "Lie\u00b7be", "nicht", "be\u00b7kr\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "$(", "VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Rosen S\u00fc\u00dfigkeit beleidigt den ", "tokens": ["Der", "Ro\u00b7sen", "S\u00fc\u00b7\u00dfig\u00b7keit", "be\u00b7lei\u00b7digt", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Mit Aepffeln aus dem Stall erquickt sich ", "tokens": ["Mit", "A\u00b7e\u00b7pf\u00b7feln", "aus", "dem", "Stall", "er\u00b7quickt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dein eckelnd Haubt verschm\u00e4ht/ was andre nach sich zeucht.", "tokens": ["Dein", "ec\u00b7kelnd", "Haubt", "ver\u00b7schm\u00e4ht", "/", "was", "and\u00b7re", "nach", "sich", "zeucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$(", "PWS", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was Ambra schw\u00e4cht/ wird offt durch Asa frisch gemacht/", "tokens": ["Was", "A\u00b7mbra", "schw\u00e4cht", "/", "wird", "offt", "durch", "A\u00b7sa", "frisch", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$(", "VAFIN", "ADV", "APPR", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird Wild und K\u00f6nigskron\u2019 um den Geruch veracht?", "tokens": ["Wird", "Wild", "und", "K\u00f6\u00b7nigs\u00b7kron'", "um", "den", "Ge\u00b7ruch", "ver\u00b7acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Am besten reucht die Frau/ die nach nichts fremden reucht.", "tokens": ["Am", "bes\u00b7ten", "reucht", "die", "Frau", "/", "die", "nach", "nichts", "frem\u00b7den", "reucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "NN", "$(", "ART", "APPR", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}