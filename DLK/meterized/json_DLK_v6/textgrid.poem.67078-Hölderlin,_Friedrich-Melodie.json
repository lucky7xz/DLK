{"textgrid.poem.67078": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lyda, siehe! zauberisch umwunden", "tokens": ["Ly\u00b7da", ",", "sie\u00b7he", "!", "zau\u00b7be\u00b7risch", "um\u00b7wun\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVIMP", "$.", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4lt das All der Liebe Sch\u00f6pferhand,", "tokens": ["H\u00e4lt", "das", "All", "der", "Lie\u00b7be", "Sch\u00f6p\u00b7fer\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Erd und Himmel wandeln treu verbunden,", "tokens": ["Erd", "und", "Him\u00b7mel", "wan\u00b7deln", "treu", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "L\u00fcftchen s\u00e4useln, Donner rollen nieder \u2013", "tokens": ["L\u00fcft\u00b7chen", "s\u00e4u\u00b7seln", ",", "Don\u00b7ner", "rol\u00b7len", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Staune, Liebe! staun und freue dich!", "tokens": ["Stau\u00b7ne", ",", "Lie\u00b7be", "!", "staun", "und", "freu\u00b7e", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Seelen finden sich im Donner wieder,", "tokens": ["See\u00b7len", "fin\u00b7den", "sich", "im", "Don\u00b7ner", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Seelen kennen in dem L\u00fcftchen sich.", "tokens": ["See\u00b7len", "ken\u00b7nen", "in", "dem", "L\u00fcft\u00b7chen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Am Gestr\u00e4uche lullt in Liebestr\u00e4ume", "tokens": ["Am", "Ge\u00b7str\u00e4u\u00b7che", "lullt", "in", "Lie\u00b7be\u00b7str\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfe Trunkenheit das M\u00e4dchen ein,", "tokens": ["S\u00fc\u00b7\u00dfe", "Trun\u00b7ken\u00b7heit", "das", "M\u00e4d\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Haucht der Fr\u00fchling durch die Bl\u00fctenb\u00e4ume,", "tokens": ["Haucht", "der", "Fr\u00fch\u00b7ling", "durch", "die", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Summen Abendsang die K\u00e4ferlein;", "tokens": ["Sum\u00b7men", "A\u00b7bend\u00b7sang", "die", "K\u00e4\u00b7fer\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Helden springen von der Schlummerst\u00e4tte,", "tokens": ["Hel\u00b7den", "sprin\u00b7gen", "von", "der", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gr\u00fc\u00dft sie br\u00fcderlich der Nachtorkan;", "tokens": ["Gr\u00fc\u00dft", "sie", "br\u00fc\u00b7der\u00b7lich", "der", "Nach\u00b7tor\u00b7kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hinzuschmettern die Tyrannenkette,", "tokens": ["Hin\u00b7zu\u00b7schmet\u00b7tern", "die", "Ty\u00b7ran\u00b7nen\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wallen sie die traute Schreckenbahn.", "tokens": ["Wal\u00b7len", "sie", "die", "trau\u00b7te", "Schre\u00b7cken\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wo der Totenkranz am Grabe fl\u00fcstert,", "tokens": ["Wo", "der", "To\u00b7ten\u00b7kranz", "am", "Gra\u00b7be", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Wurm in schwarzen Wunden nagt,", "tokens": ["Wo", "der", "Wurm", "in", "schwar\u00b7zen", "Wun\u00b7den", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, vom grauen Felsenstrauch umd\u00fcstert,", "tokens": ["Wo", ",", "vom", "grau\u00b7en", "Fel\u00b7sen\u00b7strauch", "um\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Heide hin der Rabe klagt,", "tokens": ["Durch", "die", "Hei\u00b7de", "hin", "der", "Ra\u00b7be", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo die Lerch im Tale froher Lieder,", "tokens": ["Wo", "die", "Lerch", "im", "Ta\u00b7le", "fro\u00b7her", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Pl\u00e4tschernd die Forell im Bache tanzt,", "tokens": ["Pl\u00e4t\u00b7schernd", "die", "Fo\u00b7rell", "im", "Ba\u00b7che", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "T\u00f6nt die Seele Sympathieen wieder,", "tokens": ["T\u00f6nt", "die", "See\u00b7le", "Sym\u00b7pa\u00b7thi\u00b7een", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Von der Liebe Zauber eingepflanzt.", "tokens": ["Von", "der", "Lie\u00b7be", "Zau\u00b7ber", "ein\u00b7ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wo des Geiers Schrei des Raubs sich freuet,", "tokens": ["Wo", "des", "Gei\u00b7ers", "Schrei", "des", "Raubs", "sich", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Aar dem Felsennest entbraust,", "tokens": ["Wo", "der", "Aar", "dem", "Fel\u00b7sen\u00b7nest", "ent\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo Gem\u00e4uer \u00e4chzend niederdr\u00e4uet,", "tokens": ["Wo", "Ge\u00b7m\u00e4u\u00b7er", "\u00e4ch\u00b7zend", "nie\u00b7der\u00b7dr\u00e4u\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo der Wintersturm in Tr\u00fcmmern saust,", "tokens": ["Wo", "der", "Win\u00b7ter\u00b7sturm", "in", "Tr\u00fcm\u00b7mern", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo die Woge, vom Orkan bezwungen,", "tokens": ["Wo", "die", "Wo\u00b7ge", ",", "vom", "Or\u00b7kan", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wieder auf zum schwarzen Himmel tost,", "tokens": ["Wie\u00b7der", "auf", "zum", "schwar\u00b7zen", "Him\u00b7mel", "tost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Trinkt das Riesenherz Begeisterungen,", "tokens": ["Trinkt", "das", "Rie\u00b7sen\u00b7herz", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Von den Schmeichelt\u00f6nen liebgekost.", "tokens": ["Von", "den", "Schmei\u00b7chel\u00b7t\u00f6\u00b7nen", "lieb\u00b7ge\u00b7kost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Felsen zwingt zu trauten Mitgef\u00fchlen", "tokens": ["Fel\u00b7sen", "zwingt", "zu", "trau\u00b7ten", "Mit\u00b7ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tausendstimmiger Naturgesang,", "tokens": ["Tau\u00b7sends\u00b7tim\u00b7mi\u00b7ger", "Na\u00b7tur\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber s\u00fc\u00dfer t\u00f6nt von Saitenspielen", "tokens": ["A\u00b7ber", "s\u00fc\u00b7\u00dfer", "t\u00f6nt", "von", "Sai\u00b7ten\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Allgewaltiger ihr Zauberklang;", "tokens": ["All\u00b7ge\u00b7wal\u00b7ti\u00b7ger", "ihr", "Zau\u00b7ber\u00b7klang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Rascher pocht im angestammten Triebe,", "tokens": ["Ra\u00b7scher", "pocht", "im", "an\u00b7ge\u00b7stamm\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bang und s\u00fc\u00dfe, wie der jungen Braut,", "tokens": ["Bang", "und", "s\u00fc\u00b7\u00dfe", ",", "wie", "der", "jun\u00b7gen", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Jeder Aderschlag, in trunkner Liebe", "tokens": ["Je\u00b7der", "A\u00b7der\u00b7schlag", ",", "in", "trunk\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Findt das Herz den br\u00fcderlichen Laut.", "tokens": ["Findt", "das", "Herz", "den", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aus des Jammerers erstarrtem Blicke", "tokens": ["Aus", "des", "Jam\u00b7me\u00b7rers", "er\u00b7starr\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Locket Labetr\u00e4nen Fl\u00f6tenton,", "tokens": ["Lo\u00b7cket", "La\u00b7be\u00b7tr\u00e4\u00b7nen", "Fl\u00f6\u00b7ten\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Im Gedr\u00e4nge schwarzer Mi\u00dfgeschicke", "tokens": ["Im", "Ge\u00b7dr\u00e4n\u00b7ge", "schwar\u00b7zer", "Mi\u00df\u00b7ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schafft die Schlachttrommete Siegeslohn,", "tokens": ["Schafft", "die", "Schlacht\u00b7trom\u00b7me\u00b7te", "Sie\u00b7ges\u00b7lohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie der St\u00fcrme Macht im Rosenstrauche,", "tokens": ["Wie", "der", "St\u00fcr\u00b7me", "Macht", "im", "Ro\u00b7sen\u00b7strau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rei\u00dft dahin der Saiten Ungest\u00fcm,", "tokens": ["Rei\u00dft", "da\u00b7hin", "der", "Sai\u00b7ten", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Kosend huldiget dem Liebeshauche", "tokens": ["Ko\u00b7send", "hul\u00b7di\u00b7get", "dem", "Lie\u00b7bes\u00b7hau\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sanfter Melodie der Rache Grimm.", "tokens": ["Sanf\u00b7ter", "Me\u00b7lo\u00b7die", "der", "Ra\u00b7che", "Grimm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Reizender ergl\u00fcht der Wangen Rose,", "tokens": ["Rei\u00b7zen\u00b7der", "er\u00b7gl\u00fcht", "der", "Wan\u00b7gen", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Flammenatem haucht der Purpurmund,", "tokens": ["Flam\u00b7me\u00b7na\u00b7tem", "haucht", "der", "Pur\u00b7pur\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hingebannt bei lispelndem Gekose", "tokens": ["Hin\u00b7ge\u00b7bannt", "bei", "lis\u00b7peln\u00b7dem", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schw\u00f6rt die Liebe den Verm\u00e4hlungsbund;", "tokens": ["Schw\u00f6rt", "die", "Lie\u00b7be", "den", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7bund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Niegesungne k\u00f6nigliche Lieder", "tokens": ["Nie\u00b7ge\u00b7sung\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sprossen in des S\u00e4ngers Brust empor,", "tokens": ["Spros\u00b7sen", "in", "des", "S\u00e4n\u00b7gers", "Brust", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Stolzer schwebt des Hochgesangs Gefied