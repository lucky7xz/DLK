{"textgrid.poem.38872": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann sich die Pflanz' entfaltet aus dem Keime,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann sich die Pflanz' entfaltet aus dem Keime,", "tokens": ["Wann", "sich", "die", "Pflanz'", "ent\u00b7fal\u00b7tet", "aus", "dem", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Fr\u00fchlingsl\u00fcfte liebliche Genossen,", "tokens": ["Sind", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "lieb\u00b7li\u00b7che", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kommt goldner Sonnenschein herabgeflossen,", "tokens": ["Kommt", "gold\u00b7ner", "Son\u00b7nen\u00b7schein", "her\u00b7ab\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie gr\u00fcnt und w\u00e4chst, empfindet s\u00fc\u00dfe Tr\u00e4ume.", "tokens": ["Sie", "gr\u00fcnt", "und", "w\u00e4chst", ",", "emp\u00b7fin\u00b7det", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald regt sie sich, in Aengsten, da\u00df sie s\u00e4ume,", "tokens": ["Bald", "regt", "sie", "sich", ",", "in", "A\u00b7engs\u00b7ten", ",", "da\u00df", "sie", "s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Luft, Sonne, Wasser, die sie sch\u00f6n genossen,", "tokens": ["Luft", ",", "Son\u00b7ne", ",", "Was\u00b7ser", ",", "die", "sie", "sch\u00f6n", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Macht quellend Leben und den Kelch erschlossen;", "tokens": ["Macht", "quel\u00b7lend", "Le\u00b7ben", "und", "den", "Kelch", "er\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun ist es Nacht, sie schaut die Sternenr\u00e4ume.", "tokens": ["Nun", "ist", "es", "Nacht", ",", "sie", "schaut", "die", "Ster\u00b7nen\u00b7r\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da f\u00fchlt sie Liebe, und den stillen L\u00fcften", "tokens": ["Da", "f\u00fchlt", "sie", "Lie\u00b7be", ",", "und", "den", "stil\u00b7len", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Giebt sie, von tiefer Inbrunst angesogen,", "tokens": ["Giebt", "sie", ",", "von", "tie\u00b7fer", "In\u00b7brunst", "an\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Blumengeist und stirbt in s\u00fc\u00dfen D\u00fcften.", "tokens": ["Den", "Blu\u00b7men\u00b7geist", "und", "stirbt", "in", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wurdest du zum Himmel hingezogen,", "tokens": ["So", "wur\u00b7dest", "du", "zum", "Him\u00b7mel", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sanft in Musik schiedst du in Freundesarmen,", "tokens": ["Sanft", "in", "Mu\u00b7sik", "schiedst", "du", "in", "Freun\u00b7des\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fr\u00fchling wich, und Klagen ziemt uns Armen.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wich", ",", "und", "Kla\u00b7gen", "ziemt", "uns", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}