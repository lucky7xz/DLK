{"dta.poem.2934": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Einzelheiten mu\u00dft du nie soviel erlauben,", "tokens": ["Den", "Ein\u00b7zel\u00b7hei\u00b7ten", "mu\u00dft", "du", "nie", "so\u00b7viel", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sichern Grundbegriff des Ganzen dir zu rauben.", "tokens": ["Den", "si\u00b7chern", "Grund\u00b7be\u00b7griff", "des", "Gan\u00b7zen", "dir", "zu", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Im Ganzen nimm die Welt, die gro\u00df' und jede kleine,", "tokens": ["Im", "Gan\u00b7zen", "nimm", "die", "Welt", ",", "die", "gro\u00df'", "und", "je\u00b7de", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIMP", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Ganzen das Gem\u00fct des Freundes, wie das deine.", "tokens": ["Im", "Gan\u00b7zen", "das", "Ge\u00b7m\u00fct", "des", "Freun\u00b7des", ",", "wie", "das", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sowie du Launen hast, so hat die Welt sie auch,", "tokens": ["So\u00b7wie", "du", "Lau\u00b7nen", "hast", ",", "so", "hat", "die", "Welt", "sie", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VAFIN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch die Freundschaft sch\u00fcrt kein Feuer ohne Rauch.", "tokens": ["Und", "auch", "die", "Freund\u00b7schaft", "sch\u00fcrt", "kein", "Feu\u00b7er", "oh\u00b7ne", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weh dir, wenn dich verstimmt, was auftaucht und verschwimmt,", "tokens": ["Weh", "dir", ",", "wenn", "dich", "ver\u00b7stimmt", ",", "was", "auf\u00b7taucht", "und", "ver\u00b7schwimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Gef\u00fchl von dem, was dableibt, dir benimmt.", "tokens": ["Und", "das", "Ge\u00b7f\u00fchl", "von", "dem", ",", "was", "dab\u00b7leibt", ",", "dir", "be\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "$,", "PWS", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du f\u00fchlst die heilge Glut, halt ihr den Rauch zu gut,", "tokens": ["Du", "f\u00fchlst", "die", "heil\u00b7ge", "Glut", ",", "halt", "ihr", "den", "Rauch", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werd' \u00fcber Freund und Welt und dich nicht ungemut!", "tokens": ["Werd'", "\u00fc\u00b7ber", "Freund", "und", "Welt", "und", "dich", "nicht", "un\u00b7ge\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "KON", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du kannst durch Liebeskraft einmal die Beiden kl\u00e4ren,", "tokens": ["Du", "kannst", "durch", "Lie\u00b7bes\u00b7kraft", "ein\u00b7mal", "die", "Bei\u00b7den", "kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie ein andermal dir gleichen Dienst gew\u00e4hren.", "tokens": ["Da\u00df", "sie", "ein", "an\u00b7der\u00b7mal", "dir", "glei\u00b7chen", "Dienst", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Welt ist gut, der Freund ist gut, und gut bist du;", "tokens": ["Die", "Welt", "ist", "gut", ",", "der", "Freund", "ist", "gut", ",", "und", "gut", "bist", "du", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn ihr b\u00f6se scheint, gib es dem Schein nicht zu.", "tokens": ["Und", "wenn", "ihr", "b\u00f6\u00b7se", "scheint", ",", "gib", "es", "dem", "Schein", "nicht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}