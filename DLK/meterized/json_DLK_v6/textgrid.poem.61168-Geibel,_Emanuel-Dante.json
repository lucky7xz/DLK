{"textgrid.poem.61168": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Dante", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam durch Veronas Gassen wandelt' einst der gro\u00dfe Dante,", "tokens": ["Ein\u00b7sam", "durch", "Ve\u00b7ro\u00b7nas", "Gas\u00b7sen", "wan\u00b7delt'", "einst", "der", "gro\u00b7\u00dfe", "Dan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jener Florentiner Dichter, den sein Vaterland verbannte.", "tokens": ["Je\u00b7ner", "Flo\u00b7ren\u00b7ti\u00b7ner", "Dich\u00b7ter", ",", "den", "sein", "Va\u00b7ter\u00b7land", "ver\u00b7bann\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da vernahm er, wie ein M\u00e4dchen, das ihn sah vor\u00fcberschreiten,", "tokens": ["Da", "ver\u00b7nahm", "er", ",", "wie", "ein", "M\u00e4d\u00b7chen", ",", "das", "ihn", "sah", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Also sprach zur j\u00fcngern Schwester, welche sa\u00df an ihrer Seiten:", "tokens": ["Al\u00b7so", "sprach", "zur", "j\u00fcn\u00b7gern", "Schwes\u00b7ter", ",", "wel\u00b7che", "sa\u00df", "an", "ih\u00b7rer", "Sei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "\u00bbsiehe, das ist jener Dante, der zur H\u00f6ll' hinabgestiegen,", "tokens": ["\u00bb", "sie\u00b7he", ",", "das", "ist", "je\u00b7ner", "Dan\u00b7te", ",", "der", "zur", "H\u00f6ll'", "hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PDS", "VAFIN", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Merke nur, wie Zorn und Schwermut auf der d\u00fcstern Stirn ihm liegen!", "tokens": ["Mer\u00b7ke", "nur", ",", "wie", "Zorn", "und", "Schwer\u00b7mut", "auf", "der", "d\u00fcs\u00b7tern", "Stirn", "ihm", "lie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Denn in jener Stadt der Qualen mu\u00dft' er solche Dinge schauen,", "tokens": ["Denn", "in", "je\u00b7ner", "Stadt", "der", "Qua\u00b7len", "mu\u00dft'", "er", "sol\u00b7che", "Din\u00b7ge", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df zu l\u00e4cheln nimmer wieder er vermag vor innerm Grauen.\u00ab", "tokens": ["Da\u00df", "zu", "l\u00e4\u00b7cheln", "nim\u00b7mer", "wie\u00b7der", "er", "ver\u00b7mag", "vor", "in\u00b7nerm", "Grau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PTKZU", "VVFIN", "ADV", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Aber Dante, der es h\u00f6rte, wandte sich und brach sein Schweigen:", "tokens": ["A\u00b7ber", "Dan\u00b7te", ",", "der", "es", "h\u00f6r\u00b7te", ",", "wand\u00b7te", "sich", "und", "brach", "sein", "Schwei\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "\u00bbum das L\u00e4cheln zu verlernen, braucht's nicht dort hinabzusteigen.", "tokens": ["\u00bb", "um", "das", "L\u00e4\u00b7cheln", "zu", "ver\u00b7ler\u00b7nen", ",", "braucht's", "nicht", "dort", "hin\u00b7ab\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Allen Schmerz, den ich gesungen, all die Qualen, Greul und Wunden", "tokens": ["Al\u00b7len", "Schmerz", ",", "den", "ich", "ge\u00b7sun\u00b7gen", ",", "all", "die", "Qua\u00b7len", ",", "Greul", "und", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PIAT", "ART", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Hab' ich schon auf dieser Erden, hab' ich in Florenz gefunden.\u00ab", "tokens": ["Hab'", "ich", "schon", "auf", "die\u00b7ser", "Er\u00b7den", ",", "hab'", "ich", "in", "Flo\u00b7renz", "ge\u00b7fun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "VAFIN", "PPER", "APPR", "NE", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}