{"textgrid.poem.41848": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, Rose, die die Theure mir gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, Rose, die die Theure mir gegeben,", "tokens": ["Du", ",", "Ro\u00b7se", ",", "die", "die", "Theu\u00b7re", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, die so hold an ihrer Brust gebl\u00fcht,", "tokens": ["Du", ",", "die", "so", "hold", "an", "ih\u00b7rer", "Brust", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum verwelkt so bald das warme Leben,", "tokens": ["Wa\u00b7rum", "ver\u00b7welkt", "so", "bald", "das", "war\u00b7me", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das jugendlich in deinem Kelch gegl\u00fcht?", "tokens": ["Das", "ju\u00b7gend\u00b7lich", "in", "dei\u00b7nem", "Kelch", "ge\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, als dein Glanz noch ihren Busen schm\u00fcckte,", "tokens": ["Ach", ",", "als", "dein", "Glanz", "noch", "ih\u00b7ren", "Bu\u00b7sen", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihrem Blick der milde Sonnenschein,", "tokens": ["Aus", "ih\u00b7rem", "Blick", "der", "mil\u00b7de", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihrer Brust der Athem dich erquickte,", "tokens": ["Aus", "ih\u00b7rer", "Brust", "der", "A\u00b7them", "dich", "er\u00b7quick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da prangtest du im fr\u00f6hlichen Gedeihn.", "tokens": ["Da", "prang\u00b7test", "du", "im", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch sah ich bald der Wange Roth erblassen,", "tokens": ["Doch", "sah", "ich", "bald", "der", "Wan\u00b7ge", "Roth", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schmerzlich ward dein frischer Reiz getr\u00fcbt,", "tokens": ["Und", "schmerz\u00b7lich", "ward", "dein", "fri\u00b7scher", "Reiz", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als du den Kreis der Freundlichen verlassen,", "tokens": ["Als", "du", "den", "Kreis", "der", "Freund\u00b7li\u00b7chen", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die du so still, so sehnsuchtsvoll geliebt.", "tokens": ["Die", "du", "so", "still", ",", "so", "sehn\u00b7suchts\u00b7voll", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So schied auch jeder Schmuck aus meinem Leben,", "tokens": ["So", "schied", "auch", "je\u00b7der", "Schmuck", "aus", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Todt ist die Lust, der Hoffnung milder Glanz:", "tokens": ["Todt", "ist", "die", "Lust", ",", "der", "Hoff\u00b7nung", "mil\u00b7der", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir kann der Lenz nur welke Blumen geben;", "tokens": ["Mir", "kann", "der", "Lenz", "nur", "wel\u00b7ke", "Blu\u00b7men", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Grabe ziemt kein frisch entbl\u00fchter Kranz.", "tokens": ["Dem", "Gra\u00b7be", "ziemt", "kein", "frisch", "ent\u00b7bl\u00fch\u00b7ter", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch dr\u00e4ngt noch stets aus deiner bleichen H\u00fclle,", "tokens": ["Doch", "dr\u00e4ngt", "noch", "stets", "aus", "dei\u00b7ner", "blei\u00b7chen", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn auch der Reiz des Lebens sich verlor,", "tokens": ["Wenn", "auch", "der", "Reiz", "des", "Le\u00b7bens", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein leiser Traum der hingewelkten F\u00fclle,", "tokens": ["Ein", "lei\u00b7ser", "Traum", "der", "hin\u00b7ge\u00b7welk\u00b7ten", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der linde Duft lebendig sich hervor.", "tokens": ["Der", "lin\u00b7de", "Duft", "le\u00b7ben\u00b7dig", "sich", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So labt auch mich, dem jedes Gl\u00fcck entschwunden,", "tokens": ["So", "labt", "auch", "mich", ",", "dem", "je\u00b7des", "Gl\u00fcck", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie D\u00e4mmrungshauch der Wehmuth zarter Duft,", "tokens": ["Wie", "D\u00e4mm\u00b7rungs\u00b7hauch", "der", "Weh\u00b7muth", "zar\u00b7ter", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und freundlich steigt das Bild der hellern Stunden", "tokens": ["Und", "freund\u00b7lich", "steigt", "das", "Bild", "der", "hel\u00b7lern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Abendglanz aus seiner fr\u00fchen Gruft.", "tokens": ["Im", "A\u00b7bend\u00b7glanz", "aus", "sei\u00b7ner", "fr\u00fc\u00b7hen", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ach, alles, alles Sch\u00f6ne flieht von hinnen,", "tokens": ["Ach", ",", "al\u00b7les", ",", "al\u00b7les", "Sch\u00f6\u00b7ne", "flieht", "von", "hin\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PIAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ferne Stern der Hoffnung sinkt hinab,", "tokens": ["Der", "fer\u00b7ne", "Stern", "der", "Hoff\u00b7nung", "sinkt", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der reinste Traum der Sehnsucht mu\u00df zerrinnen,", "tokens": ["Der", "reins\u00b7te", "Traum", "der", "Sehn\u00b7sucht", "mu\u00df", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kurz bl\u00fcht das Gl\u00fcck, und ewig ist das Grab.", "tokens": ["Kurz", "bl\u00fcht", "das", "Gl\u00fcck", ",", "und", "e\u00b7wig", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch nimmer welkt die heil'ge Lust am Sch\u00f6nen;", "tokens": ["Doch", "nim\u00b7mer", "welkt", "die", "heil'\u00b7ge", "Lust", "am", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch strebt die Lieb' empor im d\u00fcstern Gram;", "tokens": ["Hoch", "strebt", "die", "Lieb'", "em\u00b7por", "im", "d\u00fcs\u00b7tern", "Gram", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nur Liebe kann ihr eignes Leid vers\u00f6hnen", "tokens": ["Nur", "Lie\u00b7be", "kann", "ihr", "eig\u00b7nes", "Leid", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst ersetzen, was die Welt ihr nahm.", "tokens": ["Und", "selbst", "er\u00b7set\u00b7zen", ",", "was", "die", "Welt", "ihr", "nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}