{"textgrid.poem.37351": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem's in der Unterwelt zu still,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem's in der Unterwelt zu still,", "tokens": ["Wem's", "in", "der", "Un\u00b7ter\u00b7welt", "zu", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer oberhalb erscheinen will,", "tokens": ["Wer", "o\u00b7ber\u00b7halb", "er\u00b7schei\u00b7nen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der baut sich, je nach seiner Weise,", "tokens": ["Der", "baut", "sich", ",", "je", "nach", "sei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sichtbarliches Wohngeh\u00e4use.", "tokens": ["Ein", "sicht\u00b7bar\u00b7li\u00b7ches", "Wohn\u00b7ge\u00b7h\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist ein blinder Architekt,", "tokens": ["Er", "ist", "ein", "blin\u00b7der", "Ar\u00b7chi\u00b7tekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der selbst nicht wei\u00df, was er bezweckt.", "tokens": ["Der", "selbst", "nicht", "wei\u00df", ",", "was", "er", "be\u00b7zweckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dennoch verfertigt er genau", "tokens": ["Den\u00b7noch", "ver\u00b7fer\u00b7tigt", "er", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich kunstvoll seinen Leibesbau,", "tokens": ["Sich", "kunst\u00b7voll", "sei\u00b7nen", "Lei\u00b7bes\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sollte mal was dran passieren,", "tokens": ["Und", "soll\u00b7te", "mal", "was", "dran", "pas\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PWS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kann er's verputzen und verschmieren,", "tokens": ["Kann", "er's", "ver\u00b7put\u00b7zen", "und", "ver\u00b7schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und ist er etwa gar ein solch", "tokens": ["Und", "ist", "er", "et\u00b7wa", "gar", "ein", "solch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Geschicktes Tierlein wie der Molch,", "tokens": ["Ge\u00b7schick\u00b7tes", "Tier\u00b7lein", "wie", "der", "Molch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann ist ihm alles einerlei,", "tokens": ["Dann", "ist", "ihm", "al\u00b7les", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und w\u00e4r's ein Bein, er macht es neu.", "tokens": ["Und", "w\u00e4r's", "ein", "Bein", ",", "er", "macht", "es", "neu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nur schad, da\u00df, was so froh begr\u00fcndet,", "tokens": ["Nur", "schad", ",", "da\u00df", ",", "was", "so", "froh", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So traurig mit der Zeit verschwindet,", "tokens": ["So", "trau\u00b7rig", "mit", "der", "Zeit", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wie schlie\u00dflich jeder Bau hienieden,", "tokens": ["Wie", "schlie\u00df\u00b7lich", "je\u00b7der", "Bau", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sogar die stolzen Pyramiden.", "tokens": ["So\u00b7gar", "die", "stol\u00b7zen", "Py\u00b7ra\u00b7mi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}