{"dta.poem.7579": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jungen staun' ich an, die sich so jung geberden,", "tokens": ["Die", "Jun\u00b7gen", "staun'", "ich", "an", ",", "die", "sich", "so", "jung", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als f\u00fcrchteten sie nicht, noch hofftens, alt zu werden.", "tokens": ["Als", "f\u00fcrch\u00b7te\u00b7ten", "sie", "nicht", ",", "noch", "hoff\u00b7tens", ",", "alt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADV", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir aber wurden alt, und werden es, und sehn", "tokens": ["Wir", "a\u00b7ber", "wur\u00b7den", "alt", ",", "und", "wer\u00b7den", "es", ",", "und", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "PPER", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Gott auch dies Geschlecht von Jungen noch vergehn.", "tokens": ["Mit", "Gott", "auch", "dies", "Ge\u00b7schlecht", "von", "Jun\u00b7gen", "noch", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PDS", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was wollen sie, da\u00df sich die einen so titanisch", "tokens": ["Was", "wol\u00b7len", "sie", ",", "da\u00df", "sich", "die", "ei\u00b7nen", "so", "ti\u00b7ta\u00b7nisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "$,", "KOUS", "PRF", "ART", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anstellen, andere geberden gar satanisch?", "tokens": ["An\u00b7stel\u00b7len", ",", "an\u00b7de\u00b7re", "ge\u00b7ber\u00b7den", "gar", "sa\u00b7ta\u00b7nisch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die einen wollen vom gest\u00fcrmten Himmel prallen,", "tokens": ["Die", "ei\u00b7nen", "wol\u00b7len", "vom", "ge\u00b7st\u00fcrm\u00b7ten", "Him\u00b7mel", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern sind der H\u00f6ll' in ihrer Brust verfallen.", "tokens": ["Die", "an\u00b7dern", "sind", "der", "H\u00f6ll'", "in", "ih\u00b7rer", "Brust", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}