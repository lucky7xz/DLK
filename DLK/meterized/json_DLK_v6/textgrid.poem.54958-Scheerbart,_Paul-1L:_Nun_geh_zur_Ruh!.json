{"textgrid.poem.54958": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun geh zur Ruh!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun geh zur Ruh!", "tokens": ["Nun", "geh", "zur", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es ist schon sp\u00e4t,", "tokens": ["Es", "ist", "schon", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun tr\u00e4ume deinen Traum,", "tokens": ["Nun", "tr\u00e4u\u00b7me", "dei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt ist gut,", "tokens": ["Die", "Welt", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Nacht ist kurz.", "tokens": ["Die", "Nacht", "ist", "kurz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun tr\u00e4ume deinen Traum", "tokens": ["Nun", "tr\u00e4u\u00b7me", "dei\u00b7nen", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Liebeslust", "tokens": ["Von", "Lie\u00b7bes\u00b7lust"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und Seligkeit", "tokens": ["Und", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und freundlichen guten Augen", "tokens": ["Und", "freund\u00b7li\u00b7chen", "gu\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Tr\u00e4ume! Tr\u00e4ume", "tokens": ["Tr\u00e4u\u00b7me", "!", "Tr\u00e4u\u00b7me"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Von allen denen,", "tokens": ["Von", "al\u00b7len", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Die du liebst,", "tokens": ["Die", "du", "liebst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Damit sie dich", "tokens": ["Da\u00b7mit", "sie", "dich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Auch lieben \u2013", "tokens": ["Auch", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}}}}}