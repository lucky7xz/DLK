{"dta.poem.4275": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ArIA .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Landmann, uns zum Besten,", "tokens": ["Wenn", "der", "Land\u00b7mann", ",", "uns", "zum", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oben in der Scheune Festen", "tokens": ["O\u00b7ben", "in", "der", "Scheu\u00b7ne", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Getrayd\u2019 in Ordnung legt,", "tokens": ["Das", "Ge\u00b7trayd'", "in", "Ord\u00b7nung", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein billigs Herz bewegt,", "tokens": ["Wird", "ein", "bil\u00b7ligs", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey der Handlung, zu bedenken:", "tokens": ["Bey", "der", "Hand\u00b7lung", ",", "zu", "be\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df nur Dem, Der alle Welt", "tokens": ["Da\u00df", "nur", "Dem", ",", "Der", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDS", "$,", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In so guter Ordnung h\u00e4lt,", "tokens": ["In", "so", "gu\u00b7ter", "Ord\u00b7nung", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Billig ein GOtt Lob zu schenken,", "tokens": ["Bil\u00b7lig", "ein", "Gott", "Lob", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Da wir, was Gott wollen g\u00f6nnen,", "tokens": ["Da", "wir", ",", "was", "Gott", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So geschickt versichern k\u00f6nnen.", "tokens": ["So", "ge\u00b7schickt", "ver\u00b7si\u00b7chern", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heut\u2019 hab\u2019 ich die\u00df, mit Lust, geseh\u2019n,", "tokens": ["Heut'", "hab'", "ich", "die\u00df", ",", "mit", "Lust", ",", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "$,", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mu\u00df, mit gleicher Lust, gesteh\u2019n:", "tokens": ["Und", "mu\u00df", ",", "mit", "glei\u00b7cher", "Lust", ",", "ge\u00b7steh'n", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich, so wie es sich geb\u00fchret,", "tokens": ["Da\u00df", "mich", ",", "so", "wie", "es", "sich", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "KOKOM", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Segen, den man eingef\u00fchret,", "tokens": ["Der", "Se\u00b7gen", ",", "den", "man", "ein\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erfreut, und inniglich ger\u00fchret.", "tokens": ["Er\u00b7freut", ",", "und", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die\u00df annoch um desto mehr,", "tokens": ["Und", "die\u00df", "an\u00b7noch", "um", "des\u00b7to", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "ADV", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dieweil schon eine Zeitlang her", "tokens": ["Die\u00b7weil", "schon", "ei\u00b7ne", "Zeit\u00b7lang", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Luft, die stets von Regen schwehr,", "tokens": ["Die", "Luft", ",", "die", "stets", "von", "Re\u00b7gen", "schwehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Uns lange Hindrung angedr\u00e4uet.", "tokens": ["Uns", "lan\u00b7ge", "Hin\u00b7drung", "an\u00b7ge\u00b7dr\u00e4u\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Daher f\u00fcr diesen sch\u00f6nen Tag,", "tokens": ["Da\u00b7her", "f\u00fcr", "die\u00b7sen", "sch\u00f6\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der uns mit W\u00e4rm\u2019 und Licht erfreuet,", "tokens": ["Der", "uns", "mit", "W\u00e4rm'", "und", "Licht", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Man GOtt nicht gnugsam danken mag.", "tokens": ["Man", "Gott", "nicht", "gnug\u00b7sam", "dan\u00b7ken", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIS", "NN", "PTKNEG", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich danke, grosser Sch\u00f6pfer, Dir,", "tokens": ["Ich", "dan\u00b7ke", ",", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", ",", "Dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Du, wie vor, auch itzo mir", "tokens": ["Da\u00df", "Du", ",", "wie", "vor", ",", "auch", "it\u00b7zo", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PTKVZ", "$,", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Huld und Gnade wollen g\u00f6nnen,", "tokens": ["Die", "Huld", "und", "Gna\u00b7de", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich, zum letztenmahl, den Segen", "tokens": ["Da\u00df", "ich", ",", "zum", "letz\u00b7ten\u00b7mahl", ",", "den", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hab\u2019 erndten, in die Scheuren legen,", "tokens": ["Hab'", "ernd\u00b7ten", ",", "in", "die", "Scheu\u00b7ren", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich desselben freuen k\u00f6nnen.", "tokens": ["Und", "mich", "des\u00b7sel\u00b7ben", "freu\u00b7en", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, la\u00df das, was ich eingenommen,", "tokens": ["Ach", ",", "la\u00df", "das", ",", "was", "ich", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir ferner doch zu gute kommen!", "tokens": ["Mir", "fer\u00b7ner", "doch", "zu", "gu\u00b7te", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, la\u00df mich dieses oft ermessen,", "tokens": ["Ach", ",", "la\u00df", "mich", "die\u00b7ses", "oft", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PDS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Deiner Wohlthat nie vergessen!", "tokens": ["Und", "Dei\u00b7ner", "Wohlt\u00b7hat", "nie", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}