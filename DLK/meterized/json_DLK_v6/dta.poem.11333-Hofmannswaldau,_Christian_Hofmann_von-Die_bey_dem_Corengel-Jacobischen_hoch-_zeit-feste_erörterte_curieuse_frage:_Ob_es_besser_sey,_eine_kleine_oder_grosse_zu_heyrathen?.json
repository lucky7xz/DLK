{"dta.poem.11333": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bey dem Corengel-Jacobischen hoch-  \n zeit-feste er\u00f6rterte curieuse frage:  \n  Ob es besser sey, eine kleine oder grosse zu  \n heyrathen?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das frauenzimmer theilt sich in zwey classen ein,", "tokens": ["Das", "frau\u00b7en\u00b7zim\u00b7mer", "theilt", "sich", "in", "zwey", "clas\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist in seiner art gro\u00df oder etwas klein:", "tokens": ["Und", "ist", "in", "sei\u00b7ner", "art", "gro\u00df", "o\u00b7der", "et\u00b7was", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wenn ein freyers-mann sich eines soll erwehlen,", "tokens": ["Doch", "wenn", "ein", "freyer\u00b7smann", "sich", "ei\u00b7nes", "soll", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "So pflegt er meistentheils beym schlusse zu verfehlen.", "tokens": ["So", "pflegt", "er", "meis\u00b7ten\u00b7theils", "beym", "schlus\u00b7se", "zu", "ver\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn beyde scheinen uns vielfachen lobes werth;", "tokens": ["Denn", "bey\u00b7de", "schei\u00b7nen", "uns", "viel\u00b7fa\u00b7chen", "lo\u00b7bes", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An beyden ist auch was, so unsern geist beschwert.", "tokens": ["An", "bey\u00b7den", "ist", "auch", "was", ",", "so", "un\u00b7sern", "geist", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "PIS", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die grossen schicken sich, die stuben auszuzieren,", "tokens": ["Die", "gros\u00b7sen", "schi\u00b7cken", "sich", ",", "die", "stu\u00b7ben", "aus\u00b7zu\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "$,", "PRELS", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und in dem bette wird man sie nicht leicht verliehren.", "tokens": ["Und", "in", "dem", "bet\u00b7te", "wird", "man", "sie", "nicht", "leicht", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PIS", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn uns, ich wei\u00df nicht was, zu einem kusse zwingt,", "tokens": ["Wenn", "uns", ",", "ich", "wei\u00df", "nicht", "was", ",", "zu", "ei\u00b7nem", "kus\u00b7se", "zwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und der verwehnte mund zu fremden lippen dringt;", "tokens": ["Und", "der", "ver\u00b7wehn\u00b7te", "mund", "zu", "frem\u00b7den", "lip\u00b7pen", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kan ein grosses kind sich gar zu artig schicken:", "tokens": ["So", "kan", "ein", "gros\u00b7ses", "kind", "sich", "gar", "zu", "ar\u00b7tig", "schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man darff sich nicht so sehr, wie bey den kleinen, b\u00fccken:", "tokens": ["Man", "darff", "sich", "nicht", "so", "sehr", ",", "wie", "bey", "den", "klei\u00b7nen", ",", "b\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "PTKNEG", "ADV", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der puckel wird nicht krumm: das angesetzte knie", "tokens": ["Der", "pu\u00b7ckel", "wird", "nicht", "krumm", ":", "das", "an\u00b7ge\u00b7setz\u00b7te", "knie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unser schwacher leib empfindet wenig m\u00fch.", "tokens": ["Und", "un\u00b7ser", "schwa\u00b7cher", "leib", "emp\u00b7fin\u00b7det", "we\u00b7nig", "m\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es hei\u00dft, man soll das weib zu der geh\u00fclfin nehmen;", "tokens": ["Es", "hei\u00dft", ",", "man", "soll", "das", "weib", "zu", "der", "ge\u00b7h\u00fcl\u00b7fin", "neh\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo aber kan sich wohl ein kleines kind bequemen,", "tokens": ["Wo", "a\u00b7ber", "kan", "sich", "wohl", "ein", "klei\u00b7nes", "kind", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "ADV", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In n\u00f6then beyzustehn? Sieht doch ein grosses haus", "tokens": ["In", "n\u00f6\u00b7then", "bey\u00b7zu\u00b7stehn", "?", "Sieht", "doch", "ein", "gros\u00b7ses", "haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVINF", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nicht nur gemeiniglich von aussen pr\u00e4chtig aus;", "tokens": ["Nicht", "nur", "ge\u00b7mei\u00b7nig\u00b7lich", "von", "aus\u00b7sen", "pr\u00e4ch\u00b7tig", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "NE", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es hat auch innerlich die k\u00f6stlichsten gem\u00e4cher:", "tokens": ["Es", "hat", "auch", "in\u00b7ner\u00b7lich", "die", "k\u00f6st\u00b7lichs\u00b7ten", "ge\u00b7m\u00e4\u00b7cher", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auf gleiche weise legt die tugend ihre f\u00e4cher", "tokens": ["Auf", "glei\u00b7che", "wei\u00b7se", "legt", "die", "tu\u00b7gend", "ih\u00b7re", "f\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bey grossen seelen an. Wer sich durch gold ergetzt,", "tokens": ["Bey", "gros\u00b7sen", "see\u00b7len", "an", ".", "Wer", "sich", "durch", "gold", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PWS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ein halb quentgen schon f\u00fcr seine freude sch\u00e4tzt;", "tokens": ["Und", "ein", "halb", "quent\u00b7gen", "schon", "f\u00fcr", "sei\u00b7ne", "freu\u00b7de", "sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den wird ein gantzes pfund gewi\u00df noch mehr erfreuen:", "tokens": ["Den", "wird", "ein", "gant\u00b7zes", "pfund", "ge\u00b7wi\u00df", "noch", "mehr", "er\u00b7freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Di\u00df mu\u00df ich ebenfalls von weibern prophezeyen.", "tokens": ["Di\u00df", "mu\u00df", "ich", "e\u00b7ben\u00b7falls", "von", "wei\u00b7bern", "pro\u00b7phe\u00b7ze\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Setzt ein subtiler mund und eine zarte hand", "tokens": ["Setzt", "ein", "sub\u00b7ti\u00b7ler", "mund", "und", "ei\u00b7ne", "zar\u00b7te", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Die armen sterblichen schon in den \u00e4rgsten brand;", "tokens": ["Die", "ar\u00b7men", "sterb\u00b7li\u00b7chen", "schon", "in", "den", "\u00e4rgs\u00b7ten", "brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.27": {"text": "Was wird denn allbereit zur selben zeit geschehen,", "tokens": ["Was", "wird", "denn", "all\u00b7be\u00b7reit", "zur", "sel\u00b7ben", "zeit", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo sie was k\u00f6stliches an starcken gliedern sehen?", "tokens": ["Wo", "sie", "was", "k\u00f6st\u00b7li\u00b7ches", "an", "star\u00b7cken", "glie\u00b7dern", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJA", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nichts desto weniger bleibt dieses doch mein schlu\u00df:", "tokens": ["Nichts", "des\u00b7to", "we\u00b7ni\u00b7ger", "bleibt", "die\u00b7ses", "doch", "mein", "schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "VVFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df man ein kleines weib am ehsten suchen mu\u00df.", "tokens": ["Da\u00df", "man", "ein", "klei\u00b7nes", "weib", "am", "ehs\u00b7ten", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die ist viel artiger: Wir k\u00f6nnens aus den wercken", "tokens": ["Die", "ist", "viel", "ar\u00b7ti\u00b7ger", ":", "Wir", "k\u00f6n\u00b7nens", "aus", "den", "wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "$.", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der g\u00fctigen natur mehr als zu deutlich mercken:", "tokens": ["Der", "g\u00fc\u00b7ti\u00b7gen", "na\u00b7tur", "mehr", "als", "zu", "deut\u00b7lich", "mer\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "KOKOM", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wenn diese bey der welt was gutes schaffen will,", "tokens": ["Wenn", "die\u00b7se", "bey", "der", "welt", "was", "gu\u00b7tes", "schaf\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "ART", "NN", "PWS", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So achtet sie das maas der gr\u00f6sse nicht gar viel:", "tokens": ["So", "ach\u00b7tet", "sie", "das", "maas", "der", "gr\u00f6s\u00b7se", "nicht", "gar", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie l\u00e4\u00dft aus zinn und bley die gr\u00f6sten klumpen werden;", "tokens": ["Sie", "l\u00e4\u00dft", "aus", "zinn", "und", "bley", "die", "gr\u00f6s\u00b7ten", "klum\u00b7pen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Doch das beliebte gold, das beste marck der erden", "tokens": ["Doch", "das", "be\u00b7lieb\u00b7te", "gold", ",", "das", "bes\u00b7te", "marck", "der", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sieht, wenn es zu uns kommt, gemeiniglich gantz klein:", "tokens": ["Sieht", ",", "wenn", "es", "zu", "uns", "kommt", ",", "ge\u00b7mei\u00b7nig\u00b7lich", "gantz", "klein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der beste diamant verlangt vor seinen schein", "tokens": ["Der", "bes\u00b7te", "di\u00b7a\u00b7mant", "ver\u00b7langt", "vor", "sei\u00b7nen", "schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein grosses fundament: Die allersch\u00f6nste sterne", "tokens": ["Kein", "gros\u00b7ses", "fun\u00b7da\u00b7ment", ":", "Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Stehn von der unter-welt nur blo\u00df darum so ferne,", "tokens": ["Stehn", "von", "der", "un\u00b7ter\u00b7welt", "nur", "blo\u00df", "da\u00b7rum", "so", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADV", "PAV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Weil jede ereatur viel artigkeit erweist,", "tokens": ["Weil", "je\u00b7de", "e\u00b7rea\u00b7