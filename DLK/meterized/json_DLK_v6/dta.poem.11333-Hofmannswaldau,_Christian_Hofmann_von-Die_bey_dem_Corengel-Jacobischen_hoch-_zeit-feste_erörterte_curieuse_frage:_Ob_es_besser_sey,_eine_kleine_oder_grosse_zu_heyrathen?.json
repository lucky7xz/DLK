 "line.46": {"text": "So mit der positur zehn meilen fast erf\u00fcllt.", "tokens": ["So", "mit", "der", "po\u00b7si\u00b7tur", "zehn", "mei\u00b7len", "fast", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "CARD", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die allerkleinste uhr, so man im schiebsack tr\u00e4get,", "tokens": ["Die", "al\u00b7ler\u00b7kleins\u00b7te", "uhr", ",", "so", "man", "im", "schie\u00b7bsack", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ist kostbarer als die, so denen bauren schl\u00e4get.", "tokens": ["Ist", "kost\u00b7ba\u00b7rer", "als", "die", ",", "so", "de\u00b7nen", "bau\u00b7ren", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "$,", "ADV", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der kleinste mode-hut von Caen und Caudebec", "tokens": ["Der", "kleins\u00b7te", "mo\u00b7de\u00b7hut", "von", "Caen", "und", "Cau\u00b7de\u00b7bec"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Nimmt unserm gelde mehr, als grosse filtze, weg.", "tokens": ["Nimmt", "un\u00b7serm", "gel\u00b7de", "mehr", ",", "als", "gros\u00b7se", "filt\u00b7ze", ",", "weg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": [