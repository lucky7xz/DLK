{"textgrid.poem.26111": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ziehen Wetterwolken auf in dunkelnden Zeilen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ziehen Wetterwolken auf in dunkelnden Zeilen,", "tokens": ["Es", "zie\u00b7hen", "Wet\u00b7ter\u00b7wol\u00b7ken", "auf", "in", "dun\u00b7keln\u00b7den", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit gro\u00dfen Gesten sich die Wolken in den Himmel teilen.", "tokens": ["Mit", "gro\u00b7\u00dfen", "Ges\u00b7ten", "sich", "die", "Wol\u00b7ken", "in", "den", "Him\u00b7mel", "tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie von zerschlagenen Scheiben schauen", "tokens": ["Wie", "von", "zer\u00b7schla\u00b7ge\u00b7nen", "Schei\u00b7ben", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Himmel die blauen Scherben und grauen.", "tokens": ["Vom", "Him\u00b7mel", "die", "blau\u00b7en", "Scher\u00b7ben", "und", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "KON", "ADJA", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aufgeworfen steigt ein Acker zum Erdrand hin,", "tokens": ["Auf\u00b7ge\u00b7wor\u00b7fen", "steigt", "ein", "A\u00b7cker", "zum", "Er\u00b7drand", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Obstb\u00e4ume sind mit bl\u00fchenden Scheitern darin.", "tokens": ["Obst\u00b7b\u00e4u\u00b7me", "sind", "mit", "bl\u00fc\u00b7hen\u00b7den", "Schei\u00b7tern", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Acker und Baumger\u00fcste stehen unter des Himmels Ruinen,", "tokens": ["A\u00b7cker", "und", "Baum\u00b7ge\u00b7r\u00fcs\u00b7te", "ste\u00b7hen", "un\u00b7ter", "des", "Him\u00b7mels", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Unter den Wolkenfetzen mit den Leidenschaftsmienen,", "tokens": ["Un\u00b7ter", "den", "Wol\u00b7ken\u00b7fet\u00b7zen", "mit", "den", "Lei\u00b7den\u00b7schafts\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Die alle wie zerbrochene Tafeln den Griffeln der Blitze dienen.", "tokens": ["Die", "al\u00b7le", "wie", "zer\u00b7bro\u00b7che\u00b7ne", "Ta\u00b7feln", "den", "Grif\u00b7feln", "der", "Blit\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+--+-+-", "measure": "iambic.septa.relaxed"}}}}}