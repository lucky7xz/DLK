{"textgrid.poem.53515": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwer ein wenig mich kennt,", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer ein wenig mich kennt,", "tokens": ["\u00bb", "wer", "ein", "we\u00b7nig", "mich", "kennt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wei\u00df, ich bin abstinent,", "tokens": ["Wei\u00df", ",", "ich", "bin", "ab\u00b7sti\u00b7nent", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von fr\u00fchmorgens bis abends um neun!", "tokens": ["Von", "fr\u00fch\u00b7mor\u00b7gens", "bis", "a\u00b7bends", "um", "neun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht ein Weib existiert,", "tokens": ["Nicht", "ein", "Weib", "e\u00b7xis\u00b7tiert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das mich \u2013\u00ab", "tokens": ["Das", "mich", "\u2013", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "$(", "$("], "meter": "-+", "measure": "iambic.single"}}}}}