{"textgrid.poem.45889": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich gr\u00fc\u00df' dich, zarte sch\u00f6ne Fraue]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fc\u00df' dich, zarte sch\u00f6ne Fraue,", "tokens": ["Ich", "gr\u00fc\u00df'", "dich", ",", "zar\u00b7te", "sch\u00f6\u00b7ne", "Frau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und biet' dir freundlich gute Nacht,", "tokens": ["Und", "biet'", "dir", "freund\u00b7lich", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis da\u00df der ew'ge Tag im Taue", "tokens": ["Bis", "da\u00df", "der", "ew'\u00b7ge", "Tag", "im", "Tau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deinem K\u00e4mmerlein erwacht.", "tokens": ["Vor", "dei\u00b7nem", "K\u00e4m\u00b7mer\u00b7lein", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein heil'ger Engel soll zur Seiten", "tokens": ["Ein", "heil'\u00b7ger", "En\u00b7gel", "soll", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deinem Bettlein wachend stehn,", "tokens": ["An", "dei\u00b7nem", "Bet\u00b7tlein", "wa\u00b7chend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den goldnen Fl\u00fcgel ob dir spreiten", "tokens": ["Den", "gold\u00b7nen", "Fl\u00fc\u00b7gel", "ob", "dir", "sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwere Tr\u00e4ume von dir wehn.", "tokens": ["Und", "schwe\u00b7re", "Tr\u00e4u\u00b7me", "von", "dir", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df sie sanft erwache", "tokens": ["Da\u00df", "sie", "sanft", "er\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus ihres Schlummers Ruh',", "tokens": ["Aus", "ih\u00b7res", "Schlum\u00b7mers", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Morgenstern, der scheine", "tokens": ["Der", "Mor\u00b7gens\u00b7tern", ",", "der", "schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr recht mit Liebe zu.", "tokens": ["Ihr", "recht", "mit", "Lie\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie schlafe, sie wache,", "tokens": ["Sie", "schla\u00b7fe", ",", "sie", "wa\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie stehe, sie gehe,", "tokens": ["Sie", "ste\u00b7he", ",", "sie", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Fraue meine,", "tokens": ["Die", "Frau\u00b7e", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Oder was sie tu'.", "tokens": ["O\u00b7der", "was", "sie", "tu'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ich gr\u00fc\u00df' vor aller Bl\u00fct' die Rose,", "tokens": ["Ich", "gr\u00fc\u00df'", "vor", "al\u00b7ler", "Bl\u00fct'", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die an dem Abendhimmel bl\u00fcht,", "tokens": ["Die", "an", "dem", "A\u00b7bend\u00b7him\u00b7mel", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Herz ergie\u00dft sich dir im Scho\u00dfe,", "tokens": ["Ihr", "Herz", "er\u00b7gie\u00dft", "sich", "dir", "im", "Scho\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie zur Erde niedergl\u00fcht.", "tokens": ["Wenn", "sie", "zur", "Er\u00b7de", "nie\u00b7der\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich gr\u00fc\u00df' dich, klarer Abendsterne,", "tokens": ["Ich", "gr\u00fc\u00df'", "dich", ",", "kla\u00b7rer", "A\u00b7bends\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du brennest auf dem Haupte mein.", "tokens": ["Du", "bren\u00b7nest", "auf", "dem", "Haup\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei ihr, bei ihr so w\u00e4r' ich gerne", "tokens": ["Bei", "ihr", ",", "bei", "ihr", "so", "w\u00e4r'", "ich", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem engen K\u00e4mmerlein.", "tokens": ["In", "ih\u00b7rem", "en\u00b7gen", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df ein Engel bringe", "tokens": ["Da\u00df", "ein", "En\u00b7gel", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der Zarten meinen Gru\u00df,", "tokens": ["Der", "Zar\u00b7ten", "mei\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leis wie im Maienscheine", "tokens": ["Leis", "wie", "im", "Mai\u00b7en\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Honigblumen Ku\u00df.", "tokens": ["Der", "Ho\u00b7nig\u00b7blu\u00b7men", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie bete, sie singe,", "tokens": ["Sie", "be\u00b7te", ",", "sie", "sin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da\u00df eile die Weile,", "tokens": ["Da\u00df", "ei\u00b7le", "die", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da ich alleine", "tokens": ["Da", "ich", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ohne sie sein mu\u00df.", "tokens": ["Oh\u00b7ne", "sie", "sein", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAINF", "VMFIN", "$."], "meter": "+--++", "measure": "adoneus"}}}}}