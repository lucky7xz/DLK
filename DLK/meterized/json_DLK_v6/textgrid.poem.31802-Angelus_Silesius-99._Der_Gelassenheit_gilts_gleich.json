{"textgrid.poem.31802": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "99. Der Gelassenheit gilts gleich", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lasse mich Gott ganz; will er mir Leiden machen,", "tokens": ["Ich", "las\u00b7se", "mich", "Gott", "ganz", ";", "will", "er", "mir", "Lei\u00b7den", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "$.", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "So will ich ihm sowohl als ob den Freuden lachen.", "tokens": ["So", "will", "ich", "ihm", "so\u00b7wohl", "als", "ob", "den", "Freu\u00b7den", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "KOKOM", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}