{"textgrid.poem.58814": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "33. Marias Werk", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Martha, du verwirrest dich", "tokens": ["Ach", "Mar\u00b7tha", ",", "du", "ver\u00b7wir\u00b7rest", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch vieles Wirken innerlich;", "tokens": ["Durch", "vie\u00b7les", "Wir\u00b7ken", "in\u00b7ner\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Eins ist not: merk in der Stille,", "tokens": ["Nur", "Eins", "ist", "not", ":", "merk", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADJD", "$.", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df Gott dir nah und was sein Wille!", "tokens": ["Da\u00df", "Gott", "dir", "nah", "und", "was", "sein", "Wil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "KON", "PWS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}