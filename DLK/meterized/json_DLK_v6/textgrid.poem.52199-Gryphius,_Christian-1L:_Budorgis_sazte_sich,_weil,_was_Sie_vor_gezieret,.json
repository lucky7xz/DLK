 "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Und in das sch\u00e4rfste Recht viel Honig eingefl\u00f6st.", "tokens": ["Und", "in", "das", "sch\u00e4rfs\u00b7te", "Recht", "viel", "Ho\u00b7nig", "ein\u00b7ge\u00b7fl\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Ach Tugend! sol dein Haus, dein Tempel izt verwesen?", "tokens": ["Ach", "Tu\u00b7gend", "!", "sol", "dein", "Haus", ",", "dein", "Tem\u00b7pel", "izt", "ver\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "O Zufall! der ein Schwerd durch meine Br\u00fcste st\u00f6st.", "tokens": ["O", "Zu\u00b7fall", "!", "der", "ein", "Schwerd", "durch", "mei\u00b7ne", "Br\u00fcs\u00b7te", "st\u00f6st", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Pflegt das Verh\u00e4ngnis denn so wunderlich zu spielen?", "tokens": ["Pflegt", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "denn", "so", "wun\u00b7der\u00b7lich", "zu", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Kan Klett' und Hagedorn des Winters Grimm bestehn?", "tokens": ["Kan", "Klett'", "und", "Ha\u00b7ge\u00b7dorn", "des"