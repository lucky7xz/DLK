: ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ach nie gesparter Flei\u00df! der t\u00e4glich unverdrossen", "tokens": ["Ach", "nie", "ge\u00b7spar\u00b7ter", "Flei\u00df", "!", "der", "t\u00e4g\u00b7lich", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADJA", "NN", "$.", "ART", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Vor meine Ruh gesorgt: Gelehrt- und kluger Mund!", "tokens": ["Vor", "mei\u00b7ne", "Ruh", "ge\u00b7sorgt", ":", "Ge\u00b7lehr\u00b7t", "und", "klu\u00b7ger", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.67": {"text": "Aus dem der Grichen Schertz, der R\u00f6mer Ernst geflossen!", "tokens": ["Aus", "dem", "der", "Gri\u00b7chen", "Schertz", ",", "der", "R\u00f6\u00b7mer", "Ernst", "ge\u00b7flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Der vor so manchen Ri\u00df des Vaterlandes stund.", "tokens": ["Der", "vor", "so", "man\u00b7chen", "Ri\u00df", "des", "Va\u00b7ter\u00b7lan\u00b7des", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Standhafte Redligkeit! die keine List, kein Dr\u00e4uen,", "tokens": ["Stand\u00b7haf