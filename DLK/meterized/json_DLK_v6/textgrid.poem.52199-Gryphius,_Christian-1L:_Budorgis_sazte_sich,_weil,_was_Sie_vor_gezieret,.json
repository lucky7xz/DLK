{"textgrid.poem.52199": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Budorgis sazte sich, weil, was Sie vor gezieret,", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Budorgis sazte sich, weil, was Sie vor gezieret,", "tokens": ["Bu\u00b7dor\u00b7gis", "saz\u00b7te", "sich", ",", "weil", ",", "was", "Sie", "vor", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$,", "KOUS", "$,", "PWS", "PPER", "APPR", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit ihrem ATLAS fiel, in blassen Todten-Graus.", "tokens": ["Mit", "ih\u00b7rem", "At\u00b7LAS", "fiel", ",", "in", "blas\u00b7sen", "Tod\u00b7ten\u00b7Graus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und starrt', als w\u00e4re sie vom Donner-Strahl ger\u00fchret,", "tokens": ["Und", "starrt'", ",", "als", "w\u00e4\u00b7re", "sie", "vom", "Don\u00b7ner\u00b7Strahl", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Brach aber doch zulezt in diese Seufzer aus:", "tokens": ["Brach", "a\u00b7ber", "doch", "zu\u00b7lezt", "in", "die\u00b7se", "Seuf\u00b7zer", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erz\u00fcrnter Himmel komm', und sch\u00fctte deine Blizen", "tokens": ["Er\u00b7z\u00fcrn\u00b7ter", "Him\u00b7mel", "komm'", ",", "und", "sch\u00fct\u00b7te", "dei\u00b7ne", "Bli\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf die gekr\u00e4nckte Schos! Komm, h\u00e4uffe Streich auf Streich,", "tokens": ["Auf", "die", "ge\u00b7kr\u00e4nck\u00b7te", "Schos", "!", "Komm", ",", "h\u00e4uf\u00b7fe", "Streich", "auf", "Streich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mache meinen Thron, der auf den Ceder-St\u00fctzen,", "tokens": ["Und", "ma\u00b7che", "mei\u00b7nen", "Thron", ",", "der", "auf", "den", "Ce\u00b7der\u00b7St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die du zerschmettert, stund, der Gr\u00e4ber Asche gleich!", "tokens": ["Die", "du", "zer\u00b7schmet\u00b7tert", ",", "stund", ",", "der", "Gr\u00e4\u00b7ber", "A\u00b7sche", "gleich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "$,", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich hab' es l\u00e4ngst verdient. Ich tadel deine Schl\u00fcsse", "tokens": ["Ich", "hab'", "es", "l\u00e4ngst", "ver\u00b7dient", ".", "Ich", "ta\u00b7del", "dei\u00b7ne", "Schl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ernste Strafen nicht, ich ehre deinen Schlag:", "tokens": ["Und", "erns\u00b7te", "Stra\u00b7fen", "nicht", ",", "ich", "eh\u00b7re", "dei\u00b7nen", "Schlag", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur steh mir dieses zu, da\u00df ich die Hertzens-Risse,", "tokens": ["Nur", "steh", "mir", "die\u00b7ses", "zu", ",", "da\u00df", "ich", "die", "Hert\u00b7zens\u00b7Ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den kl\u00e4glichen Verlust genung beweinen mag.", "tokens": ["Den", "kl\u00e4g\u00b7li\u00b7chen", "Ver\u00b7lust", "ge\u00b7nung", "be\u00b7wei\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ihr T\u00f6chter Schlesiens bejammert meine Schmertzen,", "tokens": ["Ihr", "T\u00f6ch\u00b7ter", "Schle\u00b7si\u00b7ens", "be\u00b7jam\u00b7mert", "mei\u00b7ne", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schaut eurer F\u00fcrstin Angst nicht sonder Thr\u00e4nen an,", "tokens": ["Schaut", "eu\u00b7rer", "F\u00fcrs\u00b7tin", "Angst", "nicht", "son\u00b7der", "Thr\u00e4\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PTKNEG", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nehmt meine Seelen-Quaal, mein Leiden, recht zu Hertzen", "tokens": ["Nehmt", "mei\u00b7ne", "See\u00b7len\u00b7Quaal", ",", "mein", "Lei\u00b7den", ",", "recht", "zu", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und sprecht: Budorgis, ach! es ist umb dich gethan!", "tokens": ["Und", "sprecht", ":", "Bu\u00b7dor\u00b7gis", ",", "ach", "!", "es", "ist", "umb", "dich", "ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "ITJ", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Lorber-Baum verdorrt, dein Rosen-Stock verbl\u00fchet,", "tokens": ["Dein", "Lor\u00b7ber\u00b7Baum", "ver\u00b7dorrt", ",", "dein", "Ro\u00b7sen\u00b7Stock", "ver\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Ancker bricht entzwey, dein Schutz-Gestirn vergeht,", "tokens": ["Dein", "An\u00b7cker", "bricht", "ent\u00b7zwey", ",", "dein", "Schutz\u00b7Ge\u00b7stirn", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein st\u00e4rckster Pfeiler f\u00e4llt. Wer in die ferne siehet,", "tokens": ["Dein", "st\u00e4rcks\u00b7ter", "Pfei\u00b7ler", "f\u00e4llt", ".", "Wer", "in", "die", "fer\u00b7ne", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "PWS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sieht wol, da\u00df \u00fcber dir ein schwer Gewiter steht.", "tokens": ["Sieht", "wol", ",", "da\u00df", "\u00fc\u00b7ber", "dir", "ein", "schwer", "Ge\u00b7wi\u00b7ter", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach Schwestern! Freylich ist des H\u00f6chsten Zorn-Gerichte", "tokens": ["Ach", "Schwes\u00b7tern", "!", "Frey\u00b7lich", "ist", "des", "H\u00f6chs\u00b7ten", "Zorn\u00b7Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf \u00fcberh\u00e4ufte Pein und neues Weh bedacht:", "tokens": ["Auf", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7te", "Pein", "und", "neu\u00b7es", "Weh", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als meine Sonne schien, war alles klar und lichte,", "tokens": ["Als", "mei\u00b7ne", "Son\u00b7ne", "schien", ",", "war", "al\u00b7les", "klar", "und", "lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf ihren Untergang folgt eine d\u00fcstre Nacht.", "tokens": ["Auf", "ih\u00b7ren", "Un\u00b7ter\u00b7gang", "folgt", "ei\u00b7ne", "d\u00fcst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach Sonne! die zu fr\u00fch, zu pl\u00f6tzlich hingegangen,", "tokens": ["Ach", "Son\u00b7ne", "!", "die", "zu", "fr\u00fch", ",", "zu", "pl\u00f6tz\u00b7lich", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie herrlich zierte mich dein angenehmer Glantz!", "tokens": ["Wie", "herr\u00b7lich", "zier\u00b7te", "mich", "dein", "an\u00b7ge\u00b7neh\u00b7mer", "Glantz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich konte mit dem Kern der Wissenschafften prangen,", "tokens": ["Ich", "kon\u00b7te", "mit", "dem", "Kern", "der", "Wis\u00b7sen\u00b7schaff\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und Frembde liessen mir den sch\u00f6nen Sieges-Krantz.", "tokens": ["Und", "Fremb\u00b7de", "lies\u00b7sen", "mir", "den", "sch\u00f6\u00b7nen", "Sie\u00b7ges\u00b7Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Guarini schwieg vor meines Pindus singen,", "tokens": ["Der", "Gu\u00b7a\u00b7ri\u00b7ni", "schwieg", "vor", "mei\u00b7nes", "Pin\u00b7dus", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sein Sch\u00e4fer schm\u00fcckte sich mit unsrer Landes-Tracht:", "tokens": ["Sein", "Sch\u00e4\u00b7fer", "schm\u00fcck\u00b7te", "sich", "mit", "uns\u00b7rer", "Lan\u00b7des\u00b7Tracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Marin vermochte nicht sich gegen uns zu schwingen,", "tokens": ["Ma\u00b7rin", "ver\u00b7moch\u00b7te", "nicht", "sich", "ge\u00b7gen", "uns", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie h\u00f6nisch er vorhin ein deutsches Lied verlacht:", "tokens": ["Wie", "h\u00f6\u00b7nisch", "er", "vor\u00b7hin", "ein", "deut\u00b7sches", "Lied", "ver\u00b7lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Erhub Venedig gleich des Loredano Sinnen,", "tokens": ["Er\u00b7hub", "Ve\u00b7ne\u00b7dig", "gleich", "des", "Lo\u00b7re\u00b7da\u00b7no", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NE", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.34": {"text": "Erlangte Gratian vom Tagus ein Altar:", "tokens": ["Er\u00b7lang\u00b7te", "Gra\u00b7ti\u00b7an", "vom", "Ta\u00b7gus", "ein", "Al\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Ph\u00f6bus zeigte schon den deutschen Pierinnen,", "tokens": ["Mein", "Ph\u00f6\u00b7bus", "zeig\u00b7te", "schon", "den", "deut\u00b7schen", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Was mehr als Gratian und Loredano war.", "tokens": ["Was", "mehr", "als", "Gra\u00b7ti\u00b7an", "und", "Lo\u00b7re\u00b7da\u00b7no", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "KOKOM", "NE", "KON", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Britten Lichter sind vor seinem Licht' erblichen,", "tokens": ["Der", "Brit\u00b7ten", "Lich\u00b7ter", "sind", "vor", "sei\u00b7nem", "Licht'", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das kluge Franckreich warf ein neidisch Aug' auf mich:", "tokens": ["Das", "klu\u00b7ge", "Fran\u00b7ck\u00b7reich", "warf", "ein", "nei\u00b7disch", "Aug'", "auf", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Rom hat die Segel oft vor mir und Ihm gestrichen,", "tokens": ["Rom", "hat", "die", "Se\u00b7gel", "oft", "vor", "mir", "und", "Ihm", "ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Von Deutschland sag' ich nichts, weil Opitz selber mich.", "tokens": ["Von", "Deutschland", "sag'", "ich", "nichts", ",", "weil", "O\u00b7pitz", "sel\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PIS", "$,", "KOUS", "NE", "ADV", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "Ach F\u00fcrst der Poesi! Ach Brunqvell edler K\u00fcnste!", "tokens": ["Ach", "F\u00fcrst", "der", "Poe\u00b7si", "!", "Ach", "Br\u00b7un\u00b7qvell", "ed\u00b7ler", "K\u00fcns\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word",