{"textgrid.poem.61394": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Windsbraut", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Fr\u00fchling kommen ins Land,", "tokens": ["Nun", "ist", "der", "Fr\u00fch\u00b7ling", "kom\u00b7men", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So wonnig geht sein Hauch;", "tokens": ["So", "won\u00b7nig", "geht", "sein", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schl\u00e4gt die junge Nachtigall", "tokens": ["Es", "schl\u00e4gt", "die", "jun\u00b7ge", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im bl\u00fchenden Fliederstrauch.", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Flie\u00b7der\u00b7strauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie schl\u00e4gt so s\u00fc\u00df, sie singt so tr\u00fcb", "tokens": ["Sie", "schl\u00e4gt", "so", "s\u00fc\u00df", ",", "sie", "singt", "so", "tr\u00fcb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von gro\u00dfer Liebesmacht;", "tokens": ["Von", "gro\u00b7\u00dfer", "Lie\u00b7bes\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Spiegel steht das Burgfr\u00e4ulein", "tokens": ["Am", "Spie\u00b7gel", "steht", "das", "Burg\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und str\u00e4hlt ihr Haar und lacht.", "tokens": ["Und", "str\u00e4hlt", "ihr", "Haar", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da tritt ihr Bruder dar zu ihr:", "tokens": ["Da", "tritt", "ihr", "Bru\u00b7der", "dar", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Schwester Kunigund',", "tokens": ["\u00bb", "o", "Schwes\u00b7ter", "Ku\u00b7ni\u00b7gund'", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzeih' dir Gott das Lachen", "tokens": ["Ver\u00b7zeih'", "dir", "Gott", "das", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von deinem roten Mund!", "tokens": ["Von", "dei\u00b7nem", "ro\u00b7ten", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verzeih' dir Gott dein arges Spiel", "tokens": ["Ver\u00b7zeih'", "dir", "Gott", "dein", "ar\u00b7ges", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deinen harten Sinn!", "tokens": ["Und", "dei\u00b7nen", "har\u00b7ten", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat dich solche Kunst gelehrt,", "tokens": ["Wer", "hat", "dich", "sol\u00b7che", "Kunst", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du stolze Zauberin?", "tokens": ["Du", "stol\u00b7ze", "Zau\u00b7be\u00b7rin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du f\u00e4ngst mir Ritter und Edelknecht", "tokens": ["Du", "f\u00e4ngst", "mir", "Rit\u00b7ter", "und", "E\u00b7del\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit deiner Augen Schein;", "tokens": ["Mit", "dei\u00b7ner", "Au\u00b7gen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du singst ihr Herz in Liebesglut,", "tokens": ["Du", "singst", "ihr", "Herz", "in", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deins bleibt kalt wie Stein.", "tokens": ["Und", "deins", "bleibt", "kalt", "wie", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Schwester, wer mit Flammen spielt,", "tokens": ["O", "Schwes\u00b7ter", ",", "wer", "mit", "Flam\u00b7men", "spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00f6sch' auch, wo es brennt;", "tokens": ["Der", "l\u00f6sch'", "auch", ",", "wo", "es", "brennt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Locken und dein H\u00f6hnen,", "tokens": ["Dein", "Lo\u00b7cken", "und", "dein", "H\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nimmt kein gutes End'.\u00ab", "tokens": ["Das", "nimmt", "kein", "gu\u00b7tes", "End'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Fr\u00e4ulein sch\u00fcttelt ihr goldnes Haar:", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "sch\u00fct\u00b7telt", "ihr", "gold\u00b7nes", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdu sprichst nicht nach Geb\u00fchr.", "tokens": ["\u00bb", "du", "sprichst", "nicht", "nach", "Ge\u00b7b\u00fchr", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gl\u00e4nzt mein Aug', und bl\u00fcht mein Mund,", "tokens": ["Und", "gl\u00e4nzt", "mein", "Aug'", ",", "und", "bl\u00fcht", "mein", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sag' an, kann ich daf\u00fcr?", "tokens": ["Sag'", "an", ",", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VMFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was schiert mich all die Liebesglut", "tokens": ["Was", "schiert", "mich", "all", "die", "Lie\u00b7bes\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ritter und Edelknecht?", "tokens": ["Von", "Rit\u00b7ter", "und", "E\u00b7del\u00b7knecht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df sie verderben und sterben!", "tokens": ["La\u00df", "sie", "ver\u00b7der\u00b7ben", "und", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie sind mir viel zu schlecht.", "tokens": ["Sie", "sind", "mir", "viel", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "La\u00df sie verderben und sterben!", "tokens": ["La\u00df", "sie", "ver\u00b7der\u00b7ben", "und", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eh' sie mich lehren frein,", "tokens": ["Eh'", "sie", "mich", "leh\u00b7ren", "frein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind, der Wind, das K\u00f6nigskind,", "tokens": ["Der", "Wind", ",", "der", "Wind", ",", "das", "K\u00f6\u00b7nigs\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll eh' mein Buhle sein.\u00ab", "tokens": ["Soll", "eh'", "mein", "Buh\u00b7le", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "KOUS", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zu Nacht das Fr\u00e4ulein schlief im Saal;", "tokens": ["Zu", "Nacht", "das", "Fr\u00e4u\u00b7lein", "schlief", "im", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hatt' einen schweren Traum.", "tokens": ["Sie", "hatt'", "ei\u00b7nen", "schwe\u00b7ren", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr war's, sie fl\u00f6g' ein Vogel", "tokens": ["Ihr", "wa\u00b7r's", ",", "sie", "fl\u00f6g'", "ein", "Vo\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im bodenlosen Raum.", "tokens": ["Im", "bo\u00b7den\u00b7lo\u00b7sen", "Raum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie flog und hatte nicht Rast, es ging", "tokens": ["Sie", "flog", "und", "hat\u00b7te", "nicht", "Rast", ",", "es", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Sausen hinterher,", "tokens": ["Ein", "Sau\u00b7sen", "hin\u00b7ter\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber ihr die leere Luft", "tokens": ["Hoch", "\u00fc\u00b7ber", "ihr", "die", "lee\u00b7re", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unter ihr das Meer.", "tokens": ["Und", "un\u00b7ter", "ihr", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und pl\u00f6tzlich ward es totenstill,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "ward", "es", "to\u00b7tens\u00b7till", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Fl\u00fcgel war wie Blei:", "tokens": ["Ihr", "Fl\u00fc\u00b7gel", "war", "wie", "Blei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinunter st\u00fcrzt sie j\u00e4hlings \u2013", "tokens": ["Hin\u00b7un\u00b7ter", "st\u00fcrzt", "sie", "j\u00e4h\u00b7lings", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wacht sie auf im Schrei.", "tokens": ["Da", "wacht", "sie", "auf", "im", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da horch, was klirrt und klingt im Saal?", "tokens": ["Da", "horch", ",", "was", "klirrt", "und", "klingt", "im", "Saal", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PWS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fenster springen auf \u2013", "tokens": ["Die", "Fens\u00b7ter", "sprin\u00b7gen", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie das Sausen dort im Traum,", "tokens": ["So", "wie", "das", "Sau\u00b7sen", "dort", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So flie\u00dft's an ihr herauf.", "tokens": ["So", "flie\u00dft's", "an", "ihr", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Des Lagers Decken l\u00fcften sich,", "tokens": ["Des", "La\u00b7gers", "De\u00b7cken", "l\u00fcf\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wei\u00df nicht, wie's geschehn;", "tokens": ["Sie", "wei\u00df", "nicht", ",", "wie's", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr faltig Nachtkleid flattert,", "tokens": ["Ihr", "fal\u00b7tig", "Nacht\u00b7kleid", "flat\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihre goldnen Locken wehn.", "tokens": ["Ih\u00b7re", "gold\u00b7nen", "Lo\u00b7cken", "wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Es k\u00fc\u00dft sie was so k\u00fchle,", "tokens": ["Es", "k\u00fc\u00dft", "sie", "was", "so", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihr das Blut gerinnt;", "tokens