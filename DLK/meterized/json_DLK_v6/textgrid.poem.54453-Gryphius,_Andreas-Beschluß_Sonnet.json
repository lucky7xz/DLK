{"textgrid.poem.54453": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Beschlu\u00df Sonnet", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr hatt Ich vor der Faust; doch wer thar hier was schreib\u1ebd", "tokens": ["Mehr", "hatt", "Ich", "vor", "der", "Faust", ";", "doch", "wer", "thar", "hier", "was", "schreib\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "KON", "PWS", "VVFIN", "ADV", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "We\u00f1 eine Fraw dem Man ein Leinen Tuch versagt", "tokens": ["We\u00f1", "ei\u00b7ne", "Fraw", "dem", "Man", "ein", "Lei\u00b7nen", "Tuch", "ver\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "PIS", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins Grab/ vnd wenn sie kaum den andern hat/ der Magd", "tokens": ["Ins", "Grab", "/", "vnd", "wenn", "sie", "kaum", "den", "an\u00b7dern", "hat", "/", "der", "Magd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$(", "KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$(", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beut hundert Thaler an/ da\u00df sie Ihn helff auffreiben", "tokens": ["Beut", "hun\u00b7dert", "Tha\u00b7ler", "an", "/", "da\u00df", "sie", "Ihn", "helff", "auf\u00b7frei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "CARD", "NN", "PTKVZ", "$(", "KOUS", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch L\u00fcgen oder ja durch Zeugn\u00fc\u00df Eyl zutreiben", "tokens": ["Durch", "L\u00fc\u00b7gen", "o\u00b7der", "ja", "durch", "Zeug\u00b7n\u00fc\u00df", "Eyl", "zu\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Hencker in sein Schwerd; v\u00f1 hier kein Armut klagt", "tokens": ["Dem", "Hen\u00b7cker", "in", "sein", "Schwerd", ";", "v\u00f1", "hier", "kein", "Ar\u00b7mut", "klagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "ADV", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die doch wol sonst f\u00fcr Gott nicht einen Kreutzer wagt;", "tokens": ["Die", "doch", "wol", "sonst", "f\u00fcr", "Gott", "nicht", "ei\u00b7nen", "Kreut\u00b7zer", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "APPR", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wil sie doch/ da\u00df Ihr thun sol vngetadelt bleiben.", "tokens": ["Wil", "sie", "doch", "/", "da\u00df", "Ihr", "thun", "sol", "vn\u00b7ge\u00b7ta\u00b7delt", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "ADJD", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "So geht es hin vnd her; man s\u00fcndigt frey hinein/", "tokens": ["So", "geht", "es", "hin", "vnd", "her", ";", "man", "s\u00fcn\u00b7digt", "frey", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$.", "PIS", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gantz ohne schew vnd scham/ vnnd sol doch niemand sein/", "tokens": ["Gantz", "oh\u00b7ne", "schew", "vnd", "scham", "/", "vnnd", "sol", "doch", "nie\u00b7mand", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "KON", "ADJD", "$(", "KON", "VMFIN", "ADV", "PIS", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der di\u00df was jeder thut/ d\u00f6rfft offentlich erzehlen/", "tokens": ["Der", "di\u00df", "was", "je\u00b7der", "thut", "/", "d\u00f6rfft", "of\u00b7fent\u00b7lich", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "PRELS", "PIS", "VVFIN", "$(", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn Warheit schmertzt vnd reist; doch kombt gar offt an Tag/", "tokens": ["Denn", "War\u00b7heit", "schmertzt", "vnd", "reist", ";", "doch", "kombt", "gar", "offt", "an", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df, was nach vieler Sinn gar tieff verborgen lag/", "tokens": ["Di\u00df", ",", "was", "nach", "vie\u00b7ler", "Sinn", "gar", "tieff", "ver\u00b7bor\u00b7gen", "lag", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIAT", "NN", "ADV", "ADJD", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnnd mu\u00df der Warheit nie/ Lufft/ Red/ vnd Freyheit fehlen.", "tokens": ["Vnnd", "mu\u00df", "der", "War\u00b7heit", "nie", "/", "Lufft", "/", "Red", "/", "vnd", "Frey\u00b7heit", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}