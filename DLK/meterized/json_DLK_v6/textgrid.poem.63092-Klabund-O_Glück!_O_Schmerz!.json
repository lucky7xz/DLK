{"textgrid.poem.63092": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "O Gl\u00fcck! O Schmerz!", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gl\u00fcck, so in den Tag hineinzuspr\u00fchn,", "tokens": ["O", "Gl\u00fcck", ",", "so", "in", "den", "Tag", "hin\u00b7ein\u00b7zu\u00b7spr\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich lasse mich bald hier- bald dorthin gl\u00fchn", "tokens": ["Ich", "las\u00b7se", "mich", "bald", "hier", "bald", "dor\u00b7thin", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "TRUNC", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von einem M\u00e4dchenblick, von einer Hand,", "tokens": ["Von", "ei\u00b7nem", "M\u00e4d\u00b7chen\u00b7blick", ",", "von", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die, wei\u00df nicht wie, die meine fand", "tokens": ["Die", ",", "wei\u00df", "nicht", "wie", ",", "die", "mei\u00b7ne", "fand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "VVFIN", "PTKNEG", "KOKOM", "$,", "PRELS", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich nun einen Augenblick umspannt,", "tokens": ["Und", "mich", "nun", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vielleicht auch zwei, vielleicht auch eine Nacht...", "tokens": ["Viel\u00b7leicht", "auch", "zwei", ",", "viel\u00b7leicht", "auch", "ei\u00b7ne", "Nacht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "$,", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Schmerz, wenn schmerzlich dann die Fr\u00fch erwacht!", "tokens": ["O", "Schmerz", ",", "wenn", "schmerz\u00b7lich", "dann", "die", "Fr\u00fch", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADJD", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Zimmer ist so bla\u00df, die Luft so kalt,", "tokens": ["Das", "Zim\u00b7mer", "ist", "so", "bla\u00df", ",", "die", "Luft", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Herz so m\u00fcde \u2013 und das Weib so alt.", "tokens": ["Das", "Herz", "so", "m\u00fc\u00b7de", "\u2013", "und", "das", "Weib", "so", "alt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$(", "KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und jene Hand, die Licht in Nacht gebl\u00f6\u00dft,", "tokens": ["Und", "je\u00b7ne", "Hand", ",", "die", "Licht", "in", "Nacht", "ge\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "H\u00e4ngt steif am Bettrand, irgendleidbeschwert,", "tokens": ["H\u00e4ngt", "steif", "am", "Bet\u00b7trand", ",", "ir\u00b7gend\u00b7leid\u00b7be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ist nur gefa\u00dft noch, nicht begehrt,", "tokens": ["Ist", "nur", "ge\u00b7fa\u00dft", "noch", ",", "nicht", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ADV", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hat mutlos sich und stumm und wie ein wei\u00dfer Traum von uns gel\u00f6st.", "tokens": ["Hat", "mut\u00b7los", "sich", "und", "stumm", "und", "wie", "ein", "wei\u00b7\u00dfer", "Traum", "von", "uns", "ge\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "KON", "ADJD", "KON", "PWAV", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}