{"textgrid.poem.54234": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Engel,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel,", "tokens": ["Mein", "En\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da\u00df dein Kiel so lange Zeit geschwiegen,", "tokens": ["Da\u00df", "dein", "Kiel", "so", "lan\u00b7ge", "Zeit", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zeigt, da\u00df er meiner Schrifft den Stillstand auferlegt,", "tokens": ["Zeigt", ",", "da\u00df", "er", "mei\u00b7ner", "Schrifft", "den", "Still\u00b7stand", "auf\u00b7er\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum mu\u00df ich h\u00f6chst-betr\u00fcbt nebst meiner Muse liegen,", "tokens": ["Drum", "mu\u00df", "ich", "h\u00f6chst\u00b7be\u00b7tr\u00fcbt", "nebst", "mei\u00b7ner", "Mu\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch bleibt dein Befehl mir allzeit eingepr\u00e4gt.", "tokens": ["Je\u00b7doch", "bleibt", "dein", "Be\u00b7fehl", "mir", "all\u00b7zeit", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht ich will den Parna\u00df, nur er will mich, verlassen,", "tokens": ["Nicht", "ich", "will", "den", "Par\u00b7na\u00df", ",", "nur", "er", "will", "mich", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PPER", "VMFIN", "ART", "NN", "$,", "ADV", "PPER", "VMFIN", "PPER", "$,", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Mein schwartz umzognes Hertz wei\u00df keinen andern Schlu\u00df,", "tokens": ["Mein", "schwartz", "um\u00b7zog\u00b7nes", "Hertz", "wei\u00df", "kei\u00b7nen", "an\u00b7dern", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch wird sich niemand gern nach eignen Wollen hassen,", "tokens": ["Doch", "wird", "sich", "nie\u00b7mand", "gern", "nach", "eig\u00b7nen", "Wol\u00b7len", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn was mein Aug erblickt, erkenn ich mit Verdru\u00df.", "tokens": ["Denn", "was", "mein", "Aug", "er\u00b7blickt", ",", "er\u00b7kenn", "ich", "mit", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Himmel kl\u00e4hrt sich aus, und wird dir nicht mehr dr\u00e4uen,", "tokens": ["Dein", "Him\u00b7mel", "kl\u00e4hrt", "sich", "aus", ",", "und", "wird", "dir", "nicht", "mehr", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich seh von ferne schon die heitern Wolcken stehn,", "tokens": ["Ich", "seh", "von", "fer\u00b7ne", "schon", "die", "hei\u00b7tern", "Wol\u00b7cken", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die wollen deinen Geist und dein Gem\u00fcth erfreuen,", "tokens": ["Die", "wol\u00b7len", "dei\u00b7nen", "Geist", "und", "dein", "Ge\u00b7m\u00fcth", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df du hinf\u00fchro kanst auf Nelck und Rosen gehn.", "tokens": ["Da\u00df", "du", "hin\u00b7f\u00fch\u00b7ro", "kanst", "auf", "Nelck", "und", "Ro\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vielleicht wirst du von mir ein wahres Urtheil f\u00e4llen,", "tokens": ["Viel\u00b7leicht", "wirst", "du", "von", "mir", "ein", "wah\u00b7res", "Ur\u00b7theil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein Schreiben, sag ich selbst, hilfft mir zu keiner Kunst,", "tokens": ["Dein", "Schrei\u00b7ben", ",", "sag", "ich", "selbst", ",", "hilfft", "mir", "zu", "kei\u00b7ner", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Sinnen k\u00f6nnen sich nicht, wie sie wollen, stellen,", "tokens": ["Die", "Sin\u00b7nen", "k\u00f6n\u00b7nen", "sich", "nicht", ",", "wie", "sie", "wol\u00b7len", ",", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und meine Wissenschafft bleibt nur ein finstrer Dunst.", "tokens": ["Und", "mei\u00b7ne", "Wis\u00b7sen\u00b7schafft", "bleibt", "nur", "ein", "finst\u00b7rer", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wahr ists, der Schlag ist hart, der deine Brust betroffen,", "tokens": ["Wahr", "ists", ",", "der", "Schlag", "ist", "hart", ",", "der", "dei\u00b7ne", "Brust", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zweyfach, erst deinen Schatz, und denn dein liebes Kind;", "tokens": ["Zwey\u00b7fach", ",", "erst", "dei\u00b7nen", "Schatz", ",", "und", "denn", "dein", "lie\u00b7bes", "Kind", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPOSAT", "NN", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vielleicht l\u00e4st dich das Gl\u00fcck bald im Gefolge hoffen", "tokens": ["Viel\u00b7leicht", "l\u00e4st", "dich", "das", "Gl\u00fcck", "bald", "im", "Ge\u00b7fol\u00b7ge", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df sich ein treues Hertz mit deinen Geist verbind.", "tokens": ["Da\u00df", "sich", "ein", "treu\u00b7es", "Hertz", "mit", "dei\u00b7nen", "Geist", "ver\u00b7bind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch zieht das Schicksal dich nicht von der Kunst zur\u00fccke,", "tokens": ["Doch", "zieht", "das", "Schick\u00b7sal", "dich", "nicht", "von", "der", "Kunst", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die edle Poesie bleibt stets dein Zeitvertreib,", "tokens": ["Die", "ed\u00b7le", "Poe\u00b7sie", "bleibt", "stets", "dein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Mein Echo schicket dir viel Theil genommne Blicke,", "tokens": ["Mein", "E\u00b7cho", "schi\u00b7cket", "dir", "viel", "Theil", "ge\u00b7nomm\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Melpomene klagt mit, als wie ein anders Weib.", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "klagt", "mit", ",", "als", "wie", "ein", "an\u00b7ders", "Weib", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Wir wollen beyde nicht den Schmertzen widerlegen,", "tokens": ["Wir", "wol\u00b7len", "bey\u00b7de", "nicht", "den", "Schmert\u00b7zen", "wi\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der vor dein Mutter Hertz aufs z\u00e4rtlichste durchzieht,", "tokens": ["Der", "vor", "dein", "Mut\u00b7ter", "Hertz", "aufs", "z\u00e4rt\u00b7lichs\u00b7te", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nicht minder wollen wir denselben neu erregen,", "tokens": ["Nicht", "min\u00b7der", "wol\u00b7len", "wir", "den\u00b7sel\u00b7ben", "neu", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "PDS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hier\u00fcber ist mein Kiel zu tr\u00f6sten blo\u00df bem\u00fcht.", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "ist", "mein", "Kiel", "zu", "tr\u00f6s\u00b7ten", "blo\u00df", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wer kan wohl den Orcan am Firmament bezwingen?", "tokens": ["Wer", "kan", "wohl", "den", "Or\u00b7can", "am", "Fir\u00b7ma\u00b7ment", "be\u00b7zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "F\u00fcrwahr des Mannes Tod verursacht meine Pein,", "tokens": ["F\u00fcr\u00b7wahr", "des", "Man\u00b7nes", "Tod", "ver\u00b7ur\u00b7sacht", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auch darf mein Trauer-Thon nicht Freuden-Lieder singen,", "tokens": ["Auch", "darf", "mein", "Trau\u00b7e\u00b7rThon", "nicht", "Freu\u00b7den\u00b7Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und keine Panace wird k\u00fcnfftig vor mich seyn.", "tokens": ["Und", "kei\u00b7ne", "Pa\u00b7na\u00b7ce", "wird", "k\u00fcnff\u00b7tig", "vor", "mich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Drum kan ich sonder Zug ans Reimen nicht gedencken,", "tokens": ["Drum", "kan", "ich", "son\u00b7der", "Zug", "ans", "Rei\u00b7men", "nicht", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich wei\u00df du findest hier, so Mus' als Einfall schlecht.", "tokens": ["Ich", "wei\u00df", "du", "fin\u00b7dest", "hier", ",", "so", "Mus'", "als", "Ein\u00b7fall", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "$,", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der F\u00fcrst befahl es mir, ihn also zu beschencken,", "tokens": ["Der", "F\u00fcrst", "be\u00b7fahl", "es", "mir", ",", "ihn", "al\u00b7so", "zu", "be\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zwar ist mein Vers vor ihn, wie auch vor dich, nicht recht.", "tokens": ["Zwar", "ist", "mein", "Vers", "vor", "ihn", ",", "wie", "auch", "vor", "dich", ",", "nicht", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "PWAV", "ADV", "APPR", "PPER", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mein Epheu soll sich nicht zu dir als Cedern wagen,", "tokens": ["Mein", "E\u00b7pheu", "soll", "sich", "nicht", "zu", "dir", "als", "Ce\u00b7dern", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "PTKNEG", "APPR", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dieweil er gegen dir mehr als zu niedrig scheint.", "tokens": ["Die\u00b7weil", "er", "ge\u00b7gen", "dir", "mehr", "als", "zu", "nied\u00b7rig", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIAT", "KOKOM", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So darffstu weiter nicht nach keiner Ursach fragen,", "tokens": ["So", "darffs\u00b7tu", "wei\u00b7ter", "nicht", "nach", "kei\u00b7ner", "Ur\u00b7sach", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Weil meine Parodie es wie das Hertze meynt.", "tokens": ["Weil", "mei\u00b7ne", "Pa\u00b7ro\u00b7die", "es", "wie", "das", "Hert\u00b7ze", "meynt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "KOKOM", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Vielleicht l\u00e4st sich dein Geist von meiner Einfalt schrecken,", "tokens": ["Viel\u00b7leicht", "l\u00e4st", "sich", "dein", "Geist", "von", "mei\u00b7ner", "Ein\u00b7falt", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da sie dir ehmahls nur ein schlecht Post Scriptum schreibt,", "tokens": ["Da", "sie", "dir", "eh\u00b7mahls", "nur", "ein", "schlecht", "Post", "Scrip\u00b7tum", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ART", "ADJD", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Du aber ruffest jetzt, ich soll die Feder strecken,", "tokens": ["Du", "a\u00b7ber", "ruf\u00b7fest", "jetzt", ",", "ich", "soll", "die", "Fe\u00b7der", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und mein Gehorsam schafft, da\u00df es bey Reimen bleibt.", "tokens": ["Und", "mein", "Ge\u00b7hor\u00b7sam", "schafft", ",", "da\u00df", "es", "bey", "Rei\u00b7men", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Dein Geist und Kiel macht mir die Lieb ins Hertze dringen,", "tokens": ["Dein", "Geist", "und", "Kiel", "macht", "mir", "die", "Lieb", "ins", "Hert\u00b7ze", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NE", "VVFIN", "PPER", "ART", "NN", "APPRART", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Drauf wag ichs, Schwester, auch mit doch erschrocknen Sinn,", "tokens": ["Drauf", "wag", "ichs", ",", "Schwes\u00b7ter", ",", "auch", "mit", "doch", "er\u00b7schrock\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "$,", "NN", "$,", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Das Reimen l\u00e4st sich schwer, doch nicht die Liebe, zwingen,", "tokens": ["Das", "Rei\u00b7men", "l\u00e4st", "sich", "schwer", ",", "doch", "nicht", "die", "Lie\u00b7be", ",", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,", "ADV", "PTKNEG", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Di\u00df, glaub ich, zeigt dir satt, da\u00df ich dein eigen bin.", "tokens": ["Di\u00df", ",", "glaub", "ich", ",", "zeigt", "dir", "satt", ",", "da\u00df", "ich", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Dein gantz gelehrter Brief kan Mensch und G\u00f6ttern weisen,", "tokens": ["Dein", "gantz", "ge\u00b7lehr\u00b7ter", "Brief", "kan", "Mensch", "und", "G\u00f6t\u00b7tern", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wie selbiger mehr als Apellens Pinsel hei\u00dft.", "tokens": ["Wie", "sel\u00b7bi\u00b7ger", "mehr", "als", "A\u00b7pel\u00b7lens", "Pin\u00b7sel", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "KOKOM", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Du kanst mich in der That nicht angenehmer speisen,", "tokens": ["Du", "kanst", "mich", "in", "der", "That", "nicht", "an\u00b7ge\u00b7neh\u00b7mer", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Als wenn mich deine Gunst nicht gantz zur\u00fccke wei\u00dft.", "tokens": ["Als", "wenn", "mich", "dei\u00b7ne", "Gunst", "nicht", "gantz", "zu\u00b7r\u00fc\u00b7cke", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Apollo darff nicht mehr vor dich den Lorber winden,", "tokens": ["A\u00b7pol\u00b7lo", "darff", "nicht", "mehr", "vor", "dich", "den", "Lor\u00b7ber", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADV", "APPR", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Weil er dasselbige vor langer Zeit gethan,", "tokens": ["Weil", "er", "das\u00b7sel\u00b7bi\u00b7ge", "vor", "lan\u00b7ger", "Zeit", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Er kan mit Rechte nichts so ungemeines finden,", "tokens": ["Er", "kan", "mit", "Rech\u00b7te", "nichts", "so", "un\u00b7ge\u00b7mei\u00b7nes", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PIS", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Da man der Dauer nach dich Engeln gleichen kan.", "tokens": ["Da", "man", "der", "Dau\u00b7er", "nach", "dich", "En\u00b7geln", "glei\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Minerva l\u00e4st gewi\u00df Buch und die Waffen fallen,", "tokens": ["Mi\u00b7ner\u00b7va", "l\u00e4st", "ge\u00b7wi\u00df", "Buch", "und", "die", "Waf\u00b7fen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.59": {"text": "Und wird durch deinen Ruhm gantz aus sich selbst gebracht,", "tokens": ["Und", "wird", "durch", "dei\u00b7nen", "Ruhm", "gantz", "aus", "sich", "selbst", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Sie, Ph\u00f6bus und Mercur, l\u00e4st von dir weit erschallen,", "tokens": ["Sie", ",", "Ph\u00f6\u00b7bus", "und", "Mer\u00b7cur", ",", "l\u00e4st", "von", "dir", "weit", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "KON", "NE", "$,", "VVFIN", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.61": {"text": "Was dein belesner Geist vor nette Verse macht.", "tokens": ["Was", "dein", "be\u00b7les\u00b7ner", "Geist", "vor", "net\u00b7te", "Ver\u00b7se", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die Musen wollen dich selbst ihre Schwester nennen,", "tokens": ["Die", "Mu\u00b7sen", "wol\u00b7len", "dich", "selbst", "ih\u00b7re", "Schwes\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Weil jedes sehr bem\u00fcht von deinem Werthe schreibt.", "tokens": ["Weil", "je\u00b7des", "sehr", "be\u00b7m\u00fcht", "von", "dei\u00b7nem", "Wert\u00b7he", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wer dieses, glaub ich, h\u00f6rt, wird deinen Geist erkennen,", "tokens": ["Wer", "die\u00b7ses", ",", "glaub", "ich", ",", "h\u00f6rt", ",", "wird", "dei\u00b7nen", "Geist", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Und da\u00df der Vorzug dir vor allen neunen bleibt.", "tokens": ["Und", "da\u00df", "der", "Vor\u00b7zug", "dir", "vor", "al\u00b7len", "neu\u00b7nen", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Man wird von deinen Geist und allen seltnen Gaben,", "tokens": ["Man", "wird", "von", "dei\u00b7nen", "Geist", "und", "al\u00b7len", "selt\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Wie trefflich selbge sind an der Gelehrsamkeit,", "tokens": ["Wie", "treff\u00b7lich", "selb\u00b7ge", "sind", "an", "der", "Ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.68": {"text": "Wohl schwerlich in der Welt, noch deines gleichen haben,", "tokens": ["Wohl", "schwer\u00b7lich", "in", "der", "Welt", ",", "noch", "dei\u00b7nes", "glei\u00b7chen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "PPOSAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Drum schreibt von deinen Ruhm die Fama weit und breit.", "tokens": ["Drum", "schreibt", "von", "dei\u00b7nen", "Ruhm", "die", "Fa\u00b7ma", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Besonders will ich dich, wie billig, hoch verehren,", "tokens": ["Be\u00b7son\u00b7ders", "will", "ich", "dich", ",", "wie", "bil\u00b7lig", ",", "hoch", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$,", "PWAV", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Die G\u00f6tter sollen dir zu Heerd und Opfer streun,", "tokens": ["Die", "G\u00f6t\u00b7ter", "sol\u00b7len", "dir", "zu", "Heerd", "und", "Op\u00b7fer", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Sie m\u00f6gen so, wie ich, dein trefflich seyn vermehren,", "tokens": ["Sie", "m\u00f6\u00b7gen", "so", ",", "wie", "ich", ",", "dein", "treff\u00b7lich", "seyn", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "PPER", "$,", "PPOSAT", "ADJD", "VAINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Und sich zu deinen Dienst nach Wunsch und Willen weyhn.", "tokens": ["Und", "sich", "zu", "dei\u00b7nen", "Dienst", "nach", "Wunsch", "und", "Wil\u00b7len", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Selbst Flora bringt den Schatz von ihren Blumen-Betten,", "tokens": ["Selbst", "Flo\u00b7ra", "bringt", "den", "Schatz", "von", "ih\u00b7ren", "Blu\u00b7men\u00b7Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und sagt, das soll vor dich ein sanfftes K\u00fcssen seyn.", "tokens": ["Und", "sagt", ",", "das", "soll", "vor", "dich", "ein", "sanff\u00b7tes", "K\u00fcs\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "VMFIN", "APPR", "PRF", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Fortuna wolle dich von Sorg und Kummer retten,", "tokens": ["For\u00b7tu\u00b7na", "wol\u00b7le", "dich", "von", "Sorg", "und", "Kum\u00b7mer", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Und kr\u00f6hne dir dein Haupt nach ausgestandner Pein.", "tokens": ["Und", "kr\u00f6h\u00b7ne", "dir", "dein", "Haupt", "nach", "aus\u00b7ge\u00b7stand\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Di\u00df werd ich Freuden-voll von deinen H\u00e4nden lesen,", "tokens": ["Di\u00df", "werd", "ich", "Freu\u00b7den\u00b7voll", "von", "dei\u00b7nen", "H\u00e4n\u00b7den", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Und trifft denn dieses ein, so wirst du mir gestehn,", "tokens": ["Und", "trifft", "denn", "die\u00b7ses", "ein", ",", "so", "wirst", "du", "mir", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "PDS", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Ich w\u00e4re so, wie du, auch ein Prophet gewesen,", "tokens": ["Ich", "w\u00e4\u00b7re", "so", ",", "wie", "du", ",", "auch", "ein", "Pro\u00b7phet", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Und h\u00e4tte, Jano gleich, dein Wohl voraus gesehn.", "tokens": ["Und", "h\u00e4t\u00b7te", ",", "Ja\u00b7no", "gleich", ",", "dein", "Wohl", "vo\u00b7raus", "ge\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "NE", "ADV", "$,", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Hier will ich jetzt den Brief, doch nicht die Liebe, schliessen,", "tokens": ["Hier", "will", "ich", "jetzt", "den", "Brief", ",", "doch", "nicht", "die", "Lie\u00b7be", ",", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Pr\u00e4g selbe gegen mich auch in dein Hertz hinein;", "tokens": ["Pr\u00e4g", "sel\u00b7be", "ge\u00b7gen", "mich", "auch", "in", "dein", "Hertz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "So wird stets dieser Schlu\u00df aus meiner Feder fliessen,", "tokens": ["So", "wird", "stets", "die\u00b7ser", "Schlu\u00df", "aus", "mei\u00b7ner", "Fe\u00b7der", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.