{"textgrid.poem.64281": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Bismarck und die deutsche Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Redek\u00fcnstler war er, nein!", "tokens": ["Kein", "Re\u00b7de\u00b7k\u00fcnst\u00b7ler", "war", "er", ",", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch bis in Mark und Knochen", "tokens": ["Und", "den\u00b7noch", "bis", "in", "Mark", "und", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drang seine Rede schwertgleich ein,", "tokens": ["Drang", "sei\u00b7ne", "Re\u00b7de", "schwert\u00b7gleich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie \u00bb", "tokens": ["Weil", "sie", "\u00bb"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und seine Feinde nie verzeihn,", "tokens": ["Und", "sei\u00b7ne", "Fein\u00b7de", "nie", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er zu ihnen \u00bb", "tokens": ["Da\u00df", "er", "zu", "ih\u00b7nen", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}