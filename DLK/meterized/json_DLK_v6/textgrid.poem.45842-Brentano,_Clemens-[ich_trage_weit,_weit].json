{"textgrid.poem.45842": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich trage weit, weit]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trage weit, weit", "tokens": ["Ich", "tra\u00b7ge", "weit", ",", "weit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Her\u00fcber ein Leid,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "ein", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich soll es verk\u00fcnden", "tokens": ["Ich", "soll", "es", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und kann doch die Worte nicht finden.", "tokens": ["Und", "kann", "doch", "die", "Wor\u00b7te", "nicht", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Das Leid ist so gro\u00df,", "tokens": ["Das", "Leid", "ist", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Im eigenen Scho\u00df", "tokens": ["Im", "ei\u00b7ge\u00b7nen", "Scho\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "L\u00e4\u00dft sich's nicht bewahren,", "tokens": ["L\u00e4\u00dft", "sich's", "nicht", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Drum sollst Du es, Freundin! erfahren.", "tokens": ["Drum", "sollst", "Du", "es", ",", "Freun\u00b7din", "!", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "$,", "NN", "$.", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und kannst Du die Sprache nicht finden:", "tokens": ["Und", "kannst", "Du", "die", "Spra\u00b7che", "nicht", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So wird sich's verk\u00fcnden,", "tokens": ["So", "wird", "sich's", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So kommt es zu Tage,", "tokens": ["So", "kommt", "es", "zu", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In eigener Sprache.", "tokens": ["In", "ei\u00b7ge\u00b7ner", "Spra\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Es hat es die Freundin erfahren,", "tokens": ["Es", "hat", "es", "die", "Freun\u00b7din", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Will still es bewahren;", "tokens": ["Will", "still", "es", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und freundlich Dein Leben,", "tokens": ["Und", "freund\u00b7lich", "Dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Troste erheben.", "tokens": ["Im", "Tros\u00b7te", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Unerkanntes stilles Leben,", "tokens": ["Un\u00b7er\u00b7kann\u00b7tes", "stil\u00b7les", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat Dir heimlich Nachricht geben,", "tokens": ["Hat", "Dir", "heim\u00b7lich", "Nach\u00b7richt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich komme schon zu sp\u00e4t.", "tokens": ["Und", "ich", "kom\u00b7me", "schon", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahndung hat mich \u00fcbereilet,", "tokens": ["Ahn\u00b7dung", "hat", "mich", "\u00fc\u00b7be\u00b7rei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Dir z\u00e4rter mitgeteilet,", "tokens": ["Und", "Dir", "z\u00e4r\u00b7ter", "mit\u00b7ge\u00b7tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie nun im Himmel geht.", "tokens": ["Da\u00df", "sie", "nun", "im", "Him\u00b7mel", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! sie sprach in letzten Stunden,", "tokens": ["Ach", "!", "sie", "sprach", "in", "letz\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon von D\u00e4mmerung umwunden,", "tokens": ["Schon", "von", "D\u00e4m\u00b7me\u00b7rung", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe Worte leis von Dir.", "tokens": ["Lie\u00b7be", "Wor\u00b7te", "leis", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sich Deiner nie entw\u00f6hnet,", "tokens": ["Hat", "sich", "Dei\u00b7ner", "nie", "ent\u00b7w\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heimlich oft nach Dir gesehnet;", "tokens": ["Heim\u00b7lich", "oft", "nach", "Dir", "ge\u00b7seh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sprach in dem Tod zu mir:", "tokens": ["Und", "sprach", "in", "dem", "Tod", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "M\u00f6ge sich ein neues Leben", "tokens": ["M\u00f6\u00b7ge", "sich", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwiefach sch\u00f6ner um Dich weben;", "tokens": ["Zwie\u00b7fach", "sch\u00f6\u00b7ner", "um", "Dich", "we\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hemme Deiner Tr\u00e4nen Lauf,", "tokens": ["Hem\u00b7me", "Dei\u00b7ner", "Tr\u00e4\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehe, schlie\u00dfe neue Bande.", "tokens": ["Ge\u00b7he", ",", "schlie\u00b7\u00dfe", "neu\u00b7e", "Ban\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Suche meine unbekannte,", "tokens": ["Su\u00b7che", "mei\u00b7ne", "un\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir verlorne Schwester auf.", "tokens": ["Mir", "ver\u00b7lor\u00b7ne", "Schwes\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Teile, was Du mir geteilet,", "tokens": ["Tei\u00b7le", ",", "was", "Du", "mir", "ge\u00b7tei\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr, die noch im Leben weilet,", "tokens": ["Ihr", ",", "die", "noch", "im", "Le\u00b7ben", "wei\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bilde ihr ein freundlich Gl\u00fcck;", "tokens": ["Bil\u00b7de", "ihr", "ein", "freund\u00b7lich", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich schaue dann hernieder,", "tokens": ["Und", "ich", "schau\u00b7e", "dann", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehne aus dem Himmel wieder", "tokens": ["Seh\u00b7ne", "aus", "dem", "Him\u00b7mel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf die Erde mich zur\u00fcck.", "tokens": ["Auf", "die", "Er\u00b7de", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach! ich hab' ihn wohl verstanden,", "tokens": ["Ach", "!", "ich", "hab'", "ihn", "wohl", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner T\u00f6ne stillen Sinn;", "tokens": ["Dei\u00b7ner", "T\u00f6\u00b7ne", "stil\u00b7len", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6n' in Silben sich verbanden,", "tokens": ["T\u00f6n'", "in", "Sil\u00b7ben", "sich", "ver\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Worte klangen zu mir hin.", "tokens": ["Wor\u00b7te", "klan\u00b7gen", "zu", "mir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmerz und Trost hat freundlich sich gereiht,", "tokens": ["Schmerz", "und", "Trost", "hat", "freund\u00b7lich", "sich", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zogen einig durch die Dunkelheit.", "tokens": ["Zo\u00b7gen", "ei\u00b7nig", "durch", "die", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Hell in Deiner Brust erwachte", "tokens": ["Hell", "in", "Dei\u00b7ner", "Brust", "er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Rede dunkler Blick,", "tokens": ["Mei\u00b7ner", "Re\u00b7de", "dunk\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gabst in Worten, was ich dachte,", "tokens": ["Gabst", "in", "Wor\u00b7ten", ",", "was", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zartes Echo! mir zur\u00fcck.", "tokens": ["Zar\u00b7tes", "E\u00b7cho", "!", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmerz und Trost so traulich sich umschlang,", "tokens": ["Schmerz", "und", "Trost", "so", "trau\u00b7lich", "sich", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "In der T\u00f6ne r\u00fchrendem Gesang.", "tokens": ["In", "der", "T\u00f6\u00b7ne", "r\u00fch\u00b7ren\u00b7dem", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Alle Lichter sind versunken,", "tokens": ["Al\u00b7le", "Lich\u00b7ter", "sind", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer wird die Liebe wach,", "tokens": ["Nim\u00b7mer", "wird", "die", "Lie\u00b7be", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist im dunklen Blick ertrunken;", "tokens": ["Ist", "im", "dunk\u00b7len", "Blick", "er\u00b7trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ihr Aug' im Tode brach,", "tokens": ["Als", "ihr", "Aug'", "im", "To\u00b7de", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Trost mit ihr von dannen ging,", "tokens": ["Al\u00b7ler", "Trost", "mit", "ihr", "von", "dan\u00b7nen", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schmerz allein mit Wehe mich umfing.", "tokens": ["Schmerz", "al\u00b7lein", "mit", "We\u00b7he", "mich", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Sieh, ich bin schon weit gegangen,", "tokens": ["Sieh", ",", "ich", "bin", "schon", "weit", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann der Ferne nie entgehn;", "tokens": ["Kann", "der", "Fer\u00b7ne", "nie", "ent\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann die N\u00e4he nie erlangen,", "tokens": ["Kann", "die", "N\u00e4\u00b7he", "nie", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df sie immer ferne sehn.", "tokens": ["Mu\u00df", "sie", "im\u00b7mer", "fer\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmerz auf Erden immer mit mir zieht,", "tokens": ["Schmerz", "auf", "Er\u00b7den", "im\u00b7mer", "mit", "mir", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Trost am Himmel auf den Wolken flieht.", "tokens": ["Trost", "am", "Him\u00b7mel", "auf", "den", "Wol\u00b7ken", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Werde ich doch bald gesunden,", "tokens": ["Wer\u00b7de", "ich", "doch", "bald", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon die Trauer von mir weicht;", "tokens": ["Schon", "die", "Trau\u00b7er", "von", "mir", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe ich sie doch gefunden,", "tokens": ["Ha\u00b7be", "ich", "sie", "doch", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die an mildem Reiz ihr gleicht.", "tokens": ["Die", "an", "mil\u00b7dem", "Reiz", "ihr", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor der Stimme \u00c4hnlichkeit ist schon", "tokens": ["Vor", "der", "Stim\u00b7me", "\u00c4hn\u00b7lich\u00b7keit", "ist", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Aller Schmerz aus meinem Sinn entflohn.", "tokens": ["Al\u00b7ler", "Schmerz", "aus", "mei\u00b7nem", "Sinn", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Und sie sehnet sich dann wieder", "tokens": ["Und", "sie", "seh\u00b7net", "sich", "dann", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Himmelsschein zur\u00fccke,", "tokens": ["Aus", "dem", "Him\u00b7mels\u00b7schein", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Sterne, ihre Blicke", "tokens": ["Al\u00b7le", "Ster\u00b7ne", ",", "ih\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehen auf den Freund hernieder.", "tokens": ["Se\u00b7hen", "auf", "den", "Freund", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Alle Dunkelheit dann fliehet,", "tokens": ["Al\u00b7le", "Dun\u00b7kel\u00b7heit", "dann", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Lichter froh Get\u00fcmmel,", "tokens": ["Und", "der", "Lich\u00b7ter", "froh", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "In dem tiefen blauen Himmel,", "tokens": ["In", "dem", "tie\u00b7fen", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein ganzer Fr\u00fchling bl\u00fchet.", "tokens": ["Wie", "ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", "bl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und der Morgen wird ein K\u00fcssen,", "tokens": ["Und", "der", "Mor\u00b7gen", "wird", "ein", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mittag wird ein s\u00fc\u00df Umfangen;", "tokens": ["Mit\u00b7tag", "wird", "ein", "s\u00fc\u00df", "Um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abendrot ein still Verlangen,", "tokens": ["A\u00b7ben\u00b7drot", "ein", "still", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Nacht werd' ich vermissen.", "tokens": ["Nur", "die", "Nacht", "werd'", "ich", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Die N\u00e4chte sind verschwunden,", "tokens": ["Die", "N\u00e4ch\u00b7te", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die K\u00fcsse sind dahin.", "tokens": ["Die", "K\u00fcs\u00b7se", "sind", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sind die dunkeln Stunden,", "tok