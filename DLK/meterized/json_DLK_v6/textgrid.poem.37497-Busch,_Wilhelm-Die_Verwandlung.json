{"textgrid.poem.37497": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Verwandlung", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gute Schwester Anna spricht", "tokens": ["Die", "gu\u00b7te", "Schwes\u00b7ter", "An\u00b7na", "spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Bruder Karl: \u00bbAch, nasche nicht!\u00ab", "tokens": ["Zu", "Bru\u00b7der", "Karl", ":", "\u00bb", "Ach", ",", "na\u00b7sche", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NE", "$.", "$(", "ITJ", "$,", "ADJA", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch der will immer weiter lecken,", "tokens": ["Doch", "der", "will", "im\u00b7mer", "wei\u00b7ter", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da kommt die Mutter mit dem Stecken.", "tokens": ["Da", "kommt", "die", "Mut\u00b7ter", "mit", "dem", "Ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er l\u00e4uft bis vor das Hexenhaus,", "tokens": ["Er", "l\u00e4uft", "bis", "vor", "das", "He\u00b7xen\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da baumelt eine Wurst heraus.", "tokens": ["Da", "bau\u00b7melt", "ei\u00b7ne", "Wurst", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwipp! f\u00e4ngt ihn mit der Angel schlau", "tokens": ["Schwipp", "!", "f\u00e4ngt", "ihn", "mit", "der", "An\u00b7gel", "schlau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte, b\u00f6se Hexenfrau.", "tokens": ["Die", "al\u00b7te", ",", "b\u00f6\u00b7se", "He\u00b7xen\u00b7frau", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem Karl ist sonderbar zumute,", "tokens": ["Dem", "Karl", "ist", "son\u00b7der\u00b7bar", "zu\u00b7mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Hexe schwingt die Zauberrute", "tokens": ["Die", "He\u00b7xe", "schwingt", "die", "Zau\u00b7ber\u00b7ru\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und macht durch ihre Hexerein", "tokens": ["Und", "macht", "durch", "ih\u00b7re", "He\u00b7xe\u00b7rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus Karl ein kleines Quiekeschwein.", "tokens": ["Aus", "Karl", "ein", "klei\u00b7nes", "Qui\u00b7e\u00b7ke\u00b7schwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Schon f\u00e4ngt der Hexe b\u00f6ser Mann", "tokens": ["Schon", "f\u00e4ngt", "der", "He\u00b7xe", "b\u00f6\u00b7ser", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Messer scharf zu schleifen an.", "tokens": ["Das", "Mes\u00b7ser", "scharf", "zu", "schlei\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da findet das treue Schwesterlein", "tokens": ["Da", "fin\u00b7det", "das", "treu\u00b7e", "Schwes\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wunderblume mit lichtem Schein.", "tokens": ["Die", "Wun\u00b7der\u00b7blu\u00b7me", "mit", "lich\u00b7tem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Und eben als die B\u00f6sen trachten,", "tokens": ["Und", "e\u00b7ben", "als", "die", "B\u00f6\u00b7sen", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Quiekeschwein sich abzuschlachten,", "tokens": ["Das", "Qui\u00b7e\u00b7ke\u00b7schwein", "sich", "ab\u00b7zu\u00b7schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Da tritt herein das \u00c4nnchen. \u2013 Das Schwein quiekt und rennt;", "tokens": ["Da", "tritt", "her\u00b7ein", "das", "\u00c4nn\u00b7chen", ".", "\u2013", "Das", "Schwein", "qui\u00b7ekt", "und", "rennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Hexe f\u00e4llt ins Messer, der b\u00f6se Mann verbrennt.", "tokens": ["Die", "He\u00b7xe", "f\u00e4llt", "ins", "Mes\u00b7ser", ",", "der", "b\u00f6\u00b7se", "Mann", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Und Bruder Karl verliert auch bald", "tokens": ["Und", "Bru\u00b7der", "Karl", "ver\u00b7liert", "auch", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die traurig-schweinerne Gestalt:", "tokens": ["Die", "trau\u00b7rig\u00b7schwei\u00b7ner\u00b7ne", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da ist er froh", "tokens": ["Da", "ist", "er", "froh"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und spricht: \u00bbNie mach' ich's wieder so!\u00ab", "tokens": ["Und", "spricht", ":", "\u00bb", "Nie", "mach'", "ich's", "wie\u00b7der", "so", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PIS", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}