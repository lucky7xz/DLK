{"textgrid.poem.67519": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sagt, Gebieter der Erde,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt, Gebieter der Erde,", "tokens": ["Sagt", ",", "Ge\u00b7bie\u00b7ter", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Warum eilet Ihr so, mit unsrer kleinen", "tokens": ["Wa\u00b7rum", "ei\u00b7let", "Ihr", "so", ",", "mit", "uns\u00b7rer", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gabe, Gedankenfreiheit,", "tokens": ["Ga\u00b7be", ",", "Ge\u00b7dan\u00b7ken\u00b7frei\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Euren eigenen Schatz, die Macht der V\u00f6lker,", "tokens": ["Eu\u00b7ren", "ei\u00b7ge\u00b7nen", "Schatz", ",", "die", "Macht", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Schm\u00e4hlicher hinzurichten?", "tokens": ["Schm\u00e4h\u00b7li\u00b7cher", "hin\u00b7zu\u00b7rich\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der sein inneres Herz, der Wahrheit Tempel,", "tokens": ["Der", "sein", "in\u00b7ne\u00b7res", "Herz", ",", "der", "Wahr\u00b7heit", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sonst mit Freude des J\u00fcnglings", "tokens": ["Sonst", "mit", "Freu\u00b7de", "des", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Aufschlo\u00df, murmelt anjetzt geheimni\u00dfbr\u00fctend,", "tokens": ["Auf\u00b7schlo\u00df", ",", "mur\u00b7melt", "an\u00b7jetzt", "ge\u00b7heim\u00b7ni\u00df\u00b7br\u00fc\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Scheut, die Sonne zu nennen,", "tokens": ["Scheut", ",", "die", "Son\u00b7ne", "zu", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Und verschmachtet im Gram, wenn Ihr am hellen", "tokens": ["Und", "ver\u00b7schmach\u00b7tet", "im", "Gram", ",", "wenn", "Ihr", "am", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Morgen tief in der Nacht seid.", "tokens": ["Mor\u00b7gen", "tief", "in", "der", "Nacht", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Ist's, im Dunkel zu wandeln, G\u00f6tterfreude?", "tokens": ["Ist's", ",", "im", "Dun\u00b7kel", "zu", "wan\u00b7deln", ",", "G\u00f6t\u00b7ter\u00b7freu\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$,", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Oder spaltet ein Lichtmeer", "tokens": ["O\u00b7der", "spal\u00b7tet", "ein", "Licht\u00b7meer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Nicht das Seidegespinnst? In Lykomedes'", "tokens": ["Nicht", "das", "Sei\u00b7de\u00b7ge\u00b7spinnst", "?", "In", "Ly\u00b7ko\u00b7me\u00b7des'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$.", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Kammer verrieth Achilles", "tokens": ["Kam\u00b7mer", "ver\u00b7rieth", "A\u00b7chil\u00b7les"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.16": {"text": "Sich dem Forschenden doch und ging vor Troja.", "tokens": ["Sich", "dem", "For\u00b7schen\u00b7den", "doch", "und", "ging", "vor", "Tro\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}