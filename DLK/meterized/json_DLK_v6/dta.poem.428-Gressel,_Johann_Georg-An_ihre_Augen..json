{"dta.poem.428": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Augen strahlet mich mit sanfften Blicken an/", "tokens": ["Ihr", "Au\u00b7gen", "strah\u00b7let", "mich", "mit", "sanff\u00b7ten", "Bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erquicket meinen Geist/ der sonst nicht leben kan/", "tokens": ["Er\u00b7quic\u00b7ket", "mei\u00b7nen", "Geist", "/", "der", "sonst", "nicht", "le\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ART", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Labt ihn mit holdem Schein/ vertreibt den bittern Kummer/", "tokens": ["Labt", "ihn", "mit", "hol\u00b7dem", "Schein", "/", "ver\u00b7treibt", "den", "bit\u00b7tern", "Kum\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erweckt ihn aus dem Schlaff/ verst\u00f6hrt des Gl\u00fcckes-Schlum\u0303er/", "tokens": ["Er\u00b7weckt", "ihn", "aus", "dem", "Schlaff", "/", "ver\u00b7st\u00f6hrt", "des", "Gl\u00fc\u00b7ckes\u00b7Schlum\u0303er", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch Nein! bestralt mich nicht; es mehrt nur meinen Schmertz", "tokens": ["Doch", "Nein", "!", "be\u00b7stralt", "mich", "nicht", ";", "es", "mehrt", "nur", "mei\u00b7nen", "Schmertz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Genie\u00df ich gleich von euch begehrte Lust und Schertz;", "tokens": ["Ge\u00b7nie\u00df", "ich", "gleich", "von", "euch", "be\u00b7gehr\u00b7te", "Lust", "und", "Schertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Chlorinde weichet nicht von ihren steiffen Sinnen/", "tokens": ["Chlo\u00b7rin\u00b7de", "wei\u00b7chet", "nicht", "von", "ih\u00b7ren", "steif\u00b7fen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Flehn noch Dr\u00e4uen kan ihr hartes Hertz gewinnen.", "tokens": ["Kein", "Flehn", "noch", "Dr\u00e4u\u00b7en", "kan", "ihr", "har\u00b7tes", "Hertz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}