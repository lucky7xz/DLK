{"dta.poem.21363": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein feiner Regen prickelt herab,", "tokens": ["Ein", "fei\u00b7ner", "Re\u00b7gen", "pri\u00b7ckelt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Eiskalt, wie N\u00e4hnadelspitzen.", "tokens": ["Eis\u00b7kalt", ",", "wie", "N\u00e4h\u00b7na\u00b7del\u00b7spit\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$."], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Pferde bewegen traurig den Schwanz,", "tokens": ["Die", "Pfer\u00b7de", "be\u00b7we\u00b7gen", "trau\u00b7rig", "den", "Schwanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie waten im Koth und schwitzen.", "tokens": ["Sie", "wa\u00b7ten", "im", "Koth", "und", "schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}