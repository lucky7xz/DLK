{"dta.poem.11594": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Eberhard W\u00e4chter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "In seine hohen W\u00e4nde eingeschlossen,", "tokens": ["In", "sei\u00b7ne", "ho\u00b7hen", "W\u00e4n\u00b7de", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit traurig sch\u00f6nen Geistern im Verkehr,", "tokens": ["Mit", "trau\u00b7rig", "sch\u00f6\u00b7nen", "Geis\u00b7tern", "im", "Ver\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gest\u00e4rkt am reinen Athem des Homer,", "tokens": ["Ge\u00b7st\u00e4rkt", "am", "rei\u00b7nen", "A\u00b7them", "des", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Goldgew\u00f6lken Attikas umflossen:", "tokens": ["Von", "Gold\u00b7ge\u00b7w\u00f6l\u00b7ken", "At\u00b7ti\u00b7kas", "um\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So stets vor seinen T\u00fcchern unverdrossen,", "tokens": ["So", "stets", "vor", "sei\u00b7nen", "T\u00fc\u00b7chern", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern von dem Markt der K\u00fcnste, sizet er;", "tokens": ["Fern", "von", "dem", "Markt", "der", "K\u00fcns\u00b7te", ",", "si\u00b7zet", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Kein Neid verlezt, kein Lob berauscht ihn mehr,", "tokens": ["Kein", "Neid", "ver\u00b7lezt", ",", "kein", "Lob", "be\u00b7rauscht", "ihn", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm bl\u00fcht ein Kranz bei herrlichern Genossen.", "tokens": ["Ihm", "bl\u00fcht", "ein", "Kranz", "bei", "herr\u00b7li\u00b7chern", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O kommt und schaut ein selig K\u00fcnstlerleben!", "tokens": ["O", "kommt", "und", "schaut", "ein", "se\u00b7lig", "K\u00fcnst\u00b7ler\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besuchet ihn am abendlichen Herd,", "tokens": ["Be\u00b7su\u00b7chet", "ihn", "am", "a\u00b7bend\u00b7li\u00b7chen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn diese Stirne sich der Wunderschwingen", "tokens": ["Wenn", "die\u00b7se", "Stir\u00b7ne", "sich", "der", "Wun\u00b7der\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Genius erwehrend, nun soeben", "tokens": ["Des", "Ge\u00b7nius", "er\u00b7weh\u00b7rend", ",", "nun", "soe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich munter zu dem Alltagskreise kehrt,", "tokens": ["Sich", "mun\u00b7ter", "zu", "dem", "All\u00b7tags\u00b7krei\u00b7se", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Weib und Kinder scherzend um ihn schlingen.", "tokens": ["Den", "Weib", "und", "Kin\u00b7der", "scher\u00b7zend", "um", "ihn", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}