{"textgrid.poem.44179": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[hochwohlgeborne Frau, es kommt ein schlechtes Blat]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochwohlgeborne Frau, es kommt ein schlechtes Blat", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bor\u00b7ne", "Frau", ",", "es", "kommt", "ein", "schlech\u00b7tes", "Blat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einer k\u00fchnen Faust zu Dero werthen H\u00e4nden.", "tokens": ["Von", "ei\u00b7ner", "k\u00fch\u00b7nen", "Faust", "zu", "De\u00b7ro", "wert\u00b7hen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PDS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Geist, so Dero Gunst bey sich verehret hat,", "tokens": ["Mein", "Geist", ",", "so", "De\u00b7ro", "Gunst", "bey", "sich", "ver\u00b7eh\u00b7ret", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PDS", "NN", "APPR", "PRF", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will die Verschwiegenheit mit diesem Feste enden;", "tokens": ["Will", "die", "Ver\u00b7schwie\u00b7gen\u00b7heit", "mit", "die\u00b7sem", "Fes\u00b7te", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er bricht vor Freudigkeit in diese Worte aus:", "tokens": ["Er", "bricht", "vor", "Freu\u00b7dig\u00b7keit", "in", "die\u00b7se", "Wor\u00b7te", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Welch Gl\u00fccke cr\u00f6nt dich jezt, hochadeliches Haus!", "tokens": ["Welch", "Gl\u00fc\u00b7cke", "cr\u00f6nt", "dich", "jezt", ",", "hoc\u00b7ha\u00b7de\u00b7li\u00b7ches", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vergn\u00fcgung zieht bey dir mit vollen Seegeln ein,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "zieht", "bey", "dir", "mit", "vol\u00b7len", "See\u00b7geln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein hohes Nahmenslicht begl\u00fcckt die frohen Stunden,", "tokens": ["Ein", "ho\u00b7hes", "Nah\u00b7mens\u00b7licht", "be\u00b7gl\u00fcckt", "die", "fro\u00b7hen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel will dich selbst mit seiner Huld erfreun,", "tokens": ["Der", "Him\u00b7mel", "will", "dich", "selbst", "mit", "sei\u00b7ner", "Huld", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat im Herbste sich der Sommer eingefunden;", "tokens": ["Es", "hat", "im", "Herbs\u00b7te", "sich", "der", "Som\u00b7mer", "ein\u00b7ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ist, der dich, o Tag, nach W\u00fcrdigkeit besingt", "tokens": ["Wer", "ist", ",", "der", "dich", ",", "o", "Tag", ",", "nach", "W\u00fcr\u00b7dig\u00b7keit", "be\u00b7singt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dir, wie sichs geb\u00fchrt, ein Freudenopfer bringt?", "tokens": ["Und", "dir", ",", "wie", "sichs", "ge\u00b7b\u00fchrt", ",", "ein", "Freu\u00b7den\u00b7op\u00b7fer", "bringt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAV", "PIS", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist ein schlechtes Lied, ich mu\u00df es selbst gestehn,", "tokens": ["Hier", "ist", "ein", "schlech\u00b7tes", "Lied", ",", "ich", "mu\u00df", "es", "selbst", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Verse flie\u00dfen nicht, die Feder ist verdorben;", "tokens": ["Die", "Ver\u00b7se", "flie\u00b7\u00dfen", "nicht", ",", "die", "Fe\u00b7der", "ist", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich Armer m\u00fche mich, die Sonne anzusehn,", "tokens": ["Ich", "Ar\u00b7mer", "m\u00fc\u00b7he", "mich", ",", "die", "Son\u00b7ne", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da doch vor gro\u00dfem Glanz mein Auge fast erstorben:", "tokens": ["Da", "doch", "vor", "gro\u00b7\u00dfem", "Glanz", "mein", "Au\u00b7ge", "fast", "ers\u00b7tor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Adler sollte hier, nicht schlechte Tauben, seyn,", "tokens": ["Ein", "Ad\u00b7ler", "soll\u00b7te", "hier", ",", "nicht", "schlech\u00b7te", "Tau\u00b7ben", ",", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,", "PTKNEG", "ADJA", "NN", "$,", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sonst blendet sie gar leicht ein mehr als heller Schein.", "tokens": ["Sonst", "blen\u00b7det", "sie", "gar", "leicht", "ein", "mehr", "als", "hel\u00b7ler", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch, hohe G\u00f6nnerin, vor der sich jeder b\u00fcckt,", "tokens": ["Doch", ",", "ho\u00b7he", "G\u00f6n\u00b7ne\u00b7rin", ",", "vor", "der", "sich", "je\u00b7der", "b\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "APPR", "PRELS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der so vollkommne Gunst wie ich bereits geno\u00dfen,", "tokens": ["Der", "so", "voll\u00b7komm\u00b7ne", "Gunst", "wie", "ich", "be\u00b7reits", "ge\u00b7no\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KOKOM", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie nehmen g\u00fctigst an, was meine Feder schickt,", "tokens": ["Sie", "neh\u00b7men", "g\u00fc\u00b7tigst", "an", ",", "was", "mei\u00b7ne", "Fe\u00b7der", "schickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da mir von Dero Hand so manches zugeflo\u00dfen.", "tokens": ["Da", "mir", "von", "De\u00b7ro", "Hand", "so", "man\u00b7ches", "zu\u00b7ge\u00b7flo\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDS", "NN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird gleich Ihr kluger Sinn durch diesen Reim verlezt,", "tokens": ["Wird", "gleich", "Ihr", "klu\u00b7ger", "Sinn", "durch", "die\u00b7sen", "Reim", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird der Fehler doch durch meinen Wuntsch ersezt.", "tokens": ["So", "wird", "der", "Feh\u00b7ler", "doch", "durch", "mei\u00b7nen", "Wunt\u00b7sch", "er\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Es fordert meine Pflicht, die Wohlgewogenheit,", "tokens": ["Es", "for\u00b7dert", "mei\u00b7ne", "Pflicht", ",", "die", "Wohl\u00b7ge\u00b7wo\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit Sie mich bedacht, mit Dancken zu erkennen,", "tokens": ["Wo\u00b7mit", "Sie", "mich", "be\u00b7dacht", ",", "mit", "Dan\u00b7cken", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr ein jeder fast die schn\u00f6de D\u00fcrftigkeit", "tokens": ["Je", "mehr", "ein", "je\u00b7der", "fast", "die", "schn\u00f6\u00b7de", "D\u00fcrf\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gro\u00df Verhindern\u00fc\u00df der Tugend pflegt zu nennen;", "tokens": ["Ein", "gro\u00df", "Ver\u00b7hin\u00b7der\u00b7n\u00fc\u00df", "der", "Tu\u00b7gend", "pflegt", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bringt nun Studirenden der Mangel Qual und Pein,", "tokens": ["Bringt", "nun", "Stu\u00b7di\u00b7ren\u00b7den", "der", "Man\u00b7gel", "Qual", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mu\u00df mir selber auch ein harter Ansto\u00df seyn.", "tokens": ["So", "mu\u00df", "mir", "sel\u00b7ber", "auch", "ein", "har\u00b7ter", "An\u00b7sto\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich ehre diesen Tag des Himmels festen Schlu\u00df,", "tokens": ["Ich", "eh\u00b7re", "die\u00b7sen", "Tag", "des", "Him\u00b7mels", "fes\u00b7ten", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den mein schlechter Kopf die freyen K\u00fcnste liebet;", "tokens": ["Durch", "den", "mein", "schlech\u00b7ter", "Kopf", "die", "frey\u00b7en", "K\u00fcns\u00b7te", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach aber, da\u00df ich selbst dabey beklagen mu\u00df,", "tokens": ["Ach", "a\u00b7ber", ",", "da\u00df", "ich", "selbst", "da\u00b7bey", "be\u00b7kla\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "KOUS", "PPER", "ADV", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er mir zwar den Trieb, doch karge Mittel giebet.", "tokens": ["Da\u00df", "er", "mir", "zwar", "den", "Trieb", ",", "doch", "kar\u00b7ge", "Mit\u00b7tel", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie wird nicht alle Kunst und Wei\u00dfheit oft verlezt,", "tokens": ["Wie", "wird", "nicht", "al\u00b7le", "Kunst", "und", "Wei\u00df\u00b7heit", "oft", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "PIAT", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo sich das schwere Joch, der Mangel, hat gesezt!", "tokens": ["Wo", "sich", "das", "schwe\u00b7re", "Joch", ",", "der", "Man\u00b7gel", ",", "hat", "ge\u00b7sezt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was aber klagt mein Sinn den treuen Himmel an?", "tokens": ["Was", "a\u00b7ber", "klagt", "mein", "Sinn", "den", "treu\u00b7en", "Him\u00b7mel", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er zeigt mir ja bereits geneigte Freudenblicke.", "tokens": ["Er", "zeigt", "mir", "ja", "be\u00b7reits", "ge\u00b7neig\u00b7te", "Freu\u00b7den\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht, was des H\u00f6chsten Huld und de\u00dfen Sorgfalt kan:", "tokens": ["Seht", ",", "was", "des", "H\u00f6chs\u00b7ten", "Huld", "und", "de\u00b7\u00dfen", "Sorg\u00b7falt", "kan", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die von der D\u00fcrftigkeit mir angelegten Stricke", "tokens": ["Die", "von", "der", "D\u00fcrf\u00b7tig\u00b7keit", "mir", "an\u00b7ge\u00b7leg\u00b7ten", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rei\u00dft eine hohe Hand in Hofnung fast entzwey", "tokens": ["Rei\u00dft", "ei\u00