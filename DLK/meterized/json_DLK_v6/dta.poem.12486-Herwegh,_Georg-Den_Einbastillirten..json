{"dta.poem.12486": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Den Einbastillirten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das war ein Sprengen aller Bande", "tokens": ["Das", "war", "ein", "Spren\u00b7gen", "al\u00b7ler", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch die Welt ein froher Klang!", "tokens": ["Und", "durch", "die", "Welt", "ein", "fro\u00b7her", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch \u00fcber'm Rhein am Frankenstrande", "tokens": ["Doch", "\u00fc\u00b7ber'm", "Rhein", "am", "Fran\u00b7ken\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entschlief der Vogel, der da sang.", "tokens": ["Ent\u00b7schlief", "der", "Vo\u00b7gel", ",", "der", "da", "sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Kr\u00e4mer h\u00e4lt dort Aehrenlese,", "tokens": ["Ein", "Kr\u00e4\u00b7mer", "h\u00e4lt", "dort", "A\u00b7eh\u00b7ren\u00b7le\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im Staube knirscht ein tapfres Heer:", "tokens": ["Im", "Stau\u00b7be", "knirscht", "ein", "tapf\u00b7res", "Heer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist das alte Land nicht mehr,", "tokens": ["Das", "ist", "das", "al\u00b7te", "Land", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Vaterland der Marseillaise!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "der", "Mar\u00b7seil\u00b7laise", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verstopfet ihr des Ruhmes Quelle,", "tokens": ["Ver\u00b7stop\u00b7fet", "ihr", "des", "Ruh\u00b7mes", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die doch noch M\u00e4nner euch gebar,", "tokens": ["Die", "doch", "noch", "M\u00e4n\u00b7ner", "euch", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ein Regiment der Elle", "tokens": ["Da\u00b7mit", "ein", "Re\u00b7gi\u00b7ment", "der", "El\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bude wandle zum Altar?", "tokens": ["Die", "Bu\u00b7de", "wand\u00b7le", "zum", "Al\u00b7tar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr macht aus eurer Trikolore", "tokens": ["Ihr", "macht", "aus", "eu\u00b7rer", "Tri\u00b7ko\u00b7lo\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schillerndes Kam\u00e4leon,", "tokens": ["Ein", "schil\u00b7lern\u00b7des", "Ka\u00b7m\u00e4\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Frankreichs Krone, bittrer Hohn!", "tokens": ["Und", "Fran\u00b7kreichs", "Kro\u00b7ne", ",", "bit\u00b7trer", "Hohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sitzt fest auf einem Midas-Ohre.", "tokens": ["Sitzt", "fest", "auf", "ei\u00b7nem", "Mi\u00b7das\u00b7Oh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr seid gebunden und gekettet,", "tokens": ["Ihr", "seid", "ge\u00b7bun\u00b7den", "und", "ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wilden Thieren eingehegt;", "tokens": ["Gleich", "wil\u00b7den", "Thie\u00b7ren", "ein\u00b7ge\u00b7hegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O glaubt die Freiheit nicht gerettet,", "tokens": ["O", "glaubt", "die", "Frei\u00b7heit", "nicht", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn euer Aar die Fl\u00fcgel schl\u00e4gt.", "tokens": ["Wenn", "eu\u00b7er", "Aar", "die", "Fl\u00fc\u00b7gel", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr euch ist ", "tokens": ["F\u00fcr", "euch", "ist"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Im eignen Hause zeigt den Muth:", "tokens": ["Im", "eig\u00b7nen", "Hau\u00b7se", "zeigt", "den", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stillt eurer eignen Wunde Blut,", "tokens": ["Stillt", "eu\u00b7rer", "eig\u00b7nen", "Wun\u00b7de", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir wollen ", "tokens": ["Wir", "wol\u00b7len"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Drei Tage hoher Himmelswonne,", "tokens": ["Drei", "Ta\u00b7ge", "ho\u00b7her", "Him\u00b7mels\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da in die Lilien schlug der Blitz \u2014", "tokens": ["Da", "in", "die", "Li\u00b7li\u00b7en", "schlug", "der", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Verge\u00dft doch die Dezember-Sonne", "tokens": ["Ver\u00b7ge\u00dft", "doch", "die", "De\u00b7zem\u00b7ber\u00b7Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von eures Kaisers Austerlitz!", "tokens": ["Von", "eu\u00b7res", "Kai\u00b7sers", "Aus\u00b7ter\u00b7litz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}