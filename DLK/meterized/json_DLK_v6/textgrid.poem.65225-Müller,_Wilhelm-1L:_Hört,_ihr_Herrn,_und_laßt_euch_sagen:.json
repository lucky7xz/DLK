{"textgrid.poem.65225": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt, ihr Herrn, und la\u00dft euch sagen:", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, ihr Herrn, und la\u00dft euch sagen:", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "und", "la\u00dft", "euch", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Uhr hat Zehn geschlagen,", "tokens": ["Weil", "die", "Uhr", "hat", "Zehn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "CARD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns unsrer Rausche Zahl", "tokens": ["La\u00dft", "uns", "uns\u00b7rer", "Rau\u00b7sche", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberschlagen auch einmal.", "tokens": ["\u00dc\u00b7bersc\u00b7hla\u00b7gen", "auch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will das Jahr, in dem wir leben,", "tokens": ["Will", "das", "Jahr", ",", "in", "dem", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht die volle Zahl dir geben,", "tokens": ["Nicht", "die", "vol\u00b7le", "Zahl", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Trink' den zehnten heute dir,", "tokens": ["Trink'", "den", "zehn\u00b7ten", "heu\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und du bist so gut, wie wir.", "tokens": ["Und", "du", "bist", "so", "gut", ",", "wie", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, ihr Herrn, und la\u00dft euch sagen:", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "und", "la\u00dft", "euch", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Uhr hat Elf geschlagen,", "tokens": ["Weil", "die", "Uhr", "hat", "Elf", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt doch an den Elferwein,", "tokens": ["Denkt", "doch", "an", "den", "El\u00b7fer\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und schenkt keinen schlechtern ein.", "tokens": ["Und", "schenkt", "kei\u00b7nen", "schlech\u00b7tern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der edle deutsche Elfer", "tokens": ["Denn", "der", "ed\u00b7le", "deut\u00b7sche", "El\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der wahre Seelenhelfer.", "tokens": ["Ist", "der", "wah\u00b7re", "See\u00b7len\u00b7hel\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Elf! ihr Herrn, der W\u00e4chter spricht:", "tokens": ["Elf", "!", "ihr", "Herrn", ",", "der", "W\u00e4ch\u00b7ter", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6ret und verz\u00e4hlt euch nicht!", "tokens": ["H\u00f6\u00b7ret", "und", "ver\u00b7z\u00e4hlt", "euch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt, ihr Herrn, und la\u00dft euch sagen:", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "und", "la\u00dft", "euch", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Uhr hat Zw\u00f6lf geschlagen,", "tokens": ["Weil", "die", "Uhr", "hat", "Zw\u00f6lf", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "CARD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zur Neige geht der Tag,", "tokens": ["Und", "zur", "Nei\u00b7ge", "geht", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht auf euren Tischen nach,", "tokens": ["Seht", "auf", "eu\u00b7ren", "Ti\u00b7schen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob sich hier und da nicht zeigen", "tokens": ["Ob", "sich", "hier", "und", "da", "nicht", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "KON", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Volle Flaschen oder Neigen.", "tokens": ["Vol\u00b7le", "Fla\u00b7schen", "o\u00b7der", "Nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle m\u00fcssen sein geleert,", "tokens": ["Al\u00b7le", "m\u00fcs\u00b7sen", "sein", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh' der W\u00e4chter wiederkehrt.", "tokens": ["Eh'", "der", "W\u00e4ch\u00b7ter", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt, ihr Herrn, und la\u00dft euch sagen:", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "und", "la\u00dft", "euch", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Uhr hat Eins geschlagen,", "tokens": ["Weil", "die", "Uhr", "hat", "Eins", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der neue Tag beginnt,", "tokens": ["Und", "der", "neu\u00b7e", "Tag", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Holet neuen Wein geschwind,", "tokens": ["Ho\u00b7let", "neu\u00b7en", "Wein", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erw\u00e4hlt euch einen Andern,", "tokens": ["Und", "er\u00b7w\u00e4hlt", "euch", "ei\u00b7nen", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Horn umher zu wandern.", "tokens": ["Mit", "dem", "Horn", "um\u00b7her", "zu", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Guten Morgen! Guten Tag!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", "!", "Gu\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Uhr geht immer nach.", "tokens": ["Mei\u00b7ne", "Uhr", "geht", "im\u00b7mer", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}