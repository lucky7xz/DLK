{"textgrid.poem.31536": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist der Feind erlegt / der H\u00f6ll' ihr Macht geraubet;", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist der Feind erlegt / der H\u00f6ll' ihr Macht geraubet;", "tokens": ["Es", "ist", "der", "Feind", "er\u00b7legt", "/", "der", "H\u00f6ll'", "ihr", "Macht", "ge\u00b7rau\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$(", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Schlangen Haupt zerknirscht / Gesetz' und Schrifft erf\u00fcllt /", "tokens": ["der", "Schlan\u00b7gen", "Haupt", "zer\u00b7knirscht", "/", "Ge\u00b7setz'", "und", "Schrifft", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewissens Anklag' ist / auch Gottes Zorn / gestillt /", "tokens": ["Ge\u00b7wis\u00b7sens", "An\u00b7klag'", "ist", "/", "auch", "Got\u00b7tes", "Zorn", "/", "ge\u00b7stillt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "$(", "ADV", "NN", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit diesem Helden-Streich das H\u00f6llen-Reich betaubet /", "tokens": ["mit", "die\u00b7sem", "Hel\u00b7den\u00b7Streich", "das", "H\u00f6l\u00b7len\u00b7Reich", "be\u00b7tau\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "den Armen Seelen auch der Himmels-Trost erlaubet.", "tokens": ["den", "Ar\u00b7men", "See\u00b7len", "auch", "der", "Him\u00b7mels\u00b7Trost", "er\u00b7lau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vmsonst der H\u00f6llisch Drach nun auf die Frommen br\u00fcllt:", "tokens": ["Vm\u00b7sonst", "der", "H\u00f6l\u00b7lisch", "Drach", "nun", "auf", "die", "From\u00b7men", "br\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PAV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "in meinem Sieges-Fahn sie herrlich sind verh\u00fcllt.", "tokens": ["in", "mei\u00b7nem", "Sie\u00b7ges\u00b7Fahn", "sie", "herr\u00b7lich", "sind", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00f6ll / Teuffel / S\u00fcnd' und Tod / schadt nichts dem der f\u00e4st glaubet.", "tokens": ["H\u00f6ll", "/", "Teuf\u00b7fel", "/", "S\u00fcnd'", "und", "Tod", "/", "schadt", "nichts", "dem", "der", "f\u00e4st", "glau\u00b7bet", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "KON", "NN", "$(", "VVFIN", "PIS", "ART", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ganz' Erl\u00f6sungs-Werk ist v\u00f6llig nun vollbracht;", "tokens": ["Da\u00df", "ganz'", "Er\u00b7l\u00f6\u00b7sungs\u00b7Werk", "ist", "v\u00f6l\u00b7lig", "nun", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df Opffer / so ich bin / auf Ewig schon geschlachtet.", "tokens": ["da\u00df", "Opf\u00b7fer", "/", "so", "ich", "bin", "/", "auf", "E\u00b7wig", "schon", "ge\u00b7schlach\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ADV", "PPER", "VAFIN", "$(", "APPR", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich hab' es alles wol / allein / und gar / gemacht:", "tokens": ["Ich", "hab'", "es", "al\u00b7les", "wol", "/", "al\u00b7lein", "/", "und", "gar", "/", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "$(", "ADV", "$(", "KON", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wer weiter Opffern will / mein v\u00f6lligkeit verachtet.", "tokens": ["wer", "wei\u00b7ter", "Opf\u00b7fern", "will", "/", "mein", "v\u00f6l\u00b7lig\u00b7keit", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VMFIN", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun alles ist durch mich / was euch erl\u00f6st / verricht:", "tokens": ["Nun", "al\u00b7les", "ist", "durch", "mich", "/", "was", "euch", "er\u00b7l\u00f6st", "/", "ver\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "APPR", "PPER", "$(", "PWS", "PPER", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "drum lasst eur selb-Verdienst / seit mir allein verpflicht!", "tokens": ["drum", "lasst", "eur", "selb\u00b7\u00b7Ver\u00b7dienst", "/", "seit", "mir", "al\u00b7lein", "ver\u00b7pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}