{"textgrid.poem.38763": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer kennt der Sehnenden,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kennt der Sehnenden,", "tokens": ["Wer", "kennt", "der", "Seh\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Thr\u00e4nenden", "tokens": ["Thr\u00e4\u00b7nen\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Freud'vollen Schmerz?", "tokens": ["Freu\u00b7d'\u00b7vol\u00b7len", "Schmerz", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ein bangender Scherz", "tokens": ["Ein", "ban\u00b7gen\u00b7der", "Scherz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Spielt Freiheit ringend,", "tokens": ["Spielt", "Frei\u00b7heit", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In Seufzern klingend", "tokens": ["In", "Seuf\u00b7zern", "klin\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Durch's bebende Herz.", "tokens": ["Durch's", "be\u00b7ben\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.8": {"text": "Ich kann mich nicht fassen,", "tokens": ["Ich", "kann", "mich", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mich d\u00fcnket verlassen,", "tokens": ["Mich", "d\u00fcn\u00b7ket", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Versto\u00dfen zu seyn;", "tokens": ["Ver\u00b7sto\u00b7\u00dfen", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Nur Lieb' hat empfunden", "tokens": ["Nur", "Lieb'", "hat", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Wie innig verbunden", "tokens": ["Wie", "in\u00b7nig", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Die Wonnen und Wunden", "tokens": ["Die", "Won\u00b7nen", "und", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Im sel'gen Verein.", "tokens": ["Im", "sel'\u00b7gen", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}