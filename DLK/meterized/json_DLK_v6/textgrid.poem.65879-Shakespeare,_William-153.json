{"textgrid.poem.65879": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "153", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cupido warf die Fackel hin, und schlief;", "tokens": ["Cu\u00b7pi\u00b7do", "warf", "die", "Fa\u00b7ckel", "hin", ",", "und", "schlief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4gdlein der Diana stahl den Fang,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "der", "Di\u00b7a\u00b7na", "stahl", "den", "Fang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und taucht der Liebe Feuerzunder tief", "tokens": ["Und", "taucht", "der", "Lie\u00b7be", "Feu\u00b7er\u00b7zun\u00b7der", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einen kalten Quell, der dort entsprang.", "tokens": ["In", "ei\u00b7nen", "kal\u00b7ten", "Quell", ",", "der", "dort", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Alsbald durchdrang vom heil'gen Brand die Wellen", "tokens": ["Als\u00b7bald", "durch\u00b7drang", "vom", "heil'\u00b7gen", "Brand", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr alle Zeit lebendig rege Glut,", "tokens": ["F\u00fcr", "al\u00b7le", "Zeit", "le\u00b7ben\u00b7dig", "re\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ward ein siedend Bad, in schlimmen F\u00e4llen", "tokens": ["Und", "ward", "ein", "sie\u00b7dend", "Bad", ",", "in", "schlim\u00b7men", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Menschen letzte H\u00fclf' und h\u00f6chstes Gut.", "tokens": ["Der", "Men\u00b7schen", "letz\u00b7te", "H\u00fcl\u00b7f'", "und", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch \u2013 die an Liebchens Blick frisch angefachte Kerze", "tokens": ["Doch", "\u2013", "die", "an", "Lieb\u00b7chens", "Blick", "frisch", "an\u00b7ge\u00b7fach\u00b7te", "Ker\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "ART", "APPR", "NE", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hielt mir aufs Herz der Knabe zum Versuch;", "tokens": ["Hielt", "mir", "aufs", "Herz", "der", "Kna\u00b7be", "zum", "Ver\u00b7such", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df ich, erkrankend von dem hei\u00dfen Schmerze,", "tokens": ["Da\u00df", "ich", ",", "er\u00b7kran\u00b7kend", "von", "dem", "hei\u00b7\u00dfen", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein tr\u00fcber Gast, mich nach dem Bade trug.", "tokens": ["Ein", "tr\u00fc\u00b7ber", "Gast", ",", "mich", "nach", "dem", "Ba\u00b7de", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch half mir's nicht: Die B\u00e4der, die ", "tokens": ["Doch", "half", "mir's", "nicht", ":", "Die", "B\u00e4\u00b7der", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "NE", "PTKNEG", "$.", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sind Amors Feuerquellen, Liebchens Augen.", "tokens": ["Sind", "A\u00b7mors", "Feu\u00b7er\u00b7quel\u00b7len", ",", "Lieb\u00b7chens", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}