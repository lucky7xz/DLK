{"dta.poem.21518": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das verlorene Paradies.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft:", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ward ich von s\u00fc\u00dfem Taumel trunken,", "tokens": ["Da", "ward", "ich", "von", "s\u00fc\u00b7\u00dfem", "Tau\u00b7mel", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und als ob es Frau Venus selber w\u00e4r\u2019,", "tokens": ["Und", "als", "ob", "es", "Frau", "Ve\u00b7nus", "sel\u00b7ber", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "KOUS", "PPER", "NN", "NE", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bin ich ihr an die wildwogenden Br\u00fcste gesunken \u2026", "tokens": ["Bin", "ich", "ihr", "an", "die", "wild\u00b7wo\u00b7gen\u00b7den", "Br\u00fcs\u00b7te", "ge\u00b7sun\u00b7ken", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft,", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihre reifrothen Lippen auf den meinen erbl\u00fchten \u2014", "tokens": ["Ih\u00b7re", "rei\u00b7fro\u00b7then", "Lip\u00b7pen", "auf", "den", "mei\u00b7nen", "er\u00b7bl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "PPOSAT", "ADJA", "$("], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Da verga\u00df ich die harte Noth und den Tod", "tokens": ["Da", "ver\u00b7ga\u00df", "ich", "die", "har\u00b7te", "Noth", "und", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und meiner Mutter liebfrommes Beh\u00fcten \u2026", "tokens": ["Und", "mei\u00b7ner", "Mut\u00b7ter", "lieb\u00b7from\u00b7mes", "Be\u00b7h\u00fc\u00b7ten", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft \u2014", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war\u2019s mir, als qu\u00f6llen Flammenb\u00e4che", "tokens": ["Da", "wa\u00b7r's", "mir", ",", "als", "qu\u00f6l\u00b7len", "Flam\u00b7men\u00b7b\u00e4\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie der H\u00f6lle Sengstrom durch meinen Leib,", "tokens": ["Wie", "der", "H\u00f6l\u00b7le", "Sengs\u00b7trom", "durch", "mei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Als ob bacchantische Brunst mir den Sch\u00e4del zerbreche . .", "tokens": ["Als", "ob", "bac\u00b7chan\u00b7ti\u00b7sche", "Brunst", "mir", "den", "Sch\u00e4\u00b7del", "zer\u00b7bre\u00b7che", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOUS", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$.", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft \u2014", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schluchzend lag ich vor ihr im Staube \u2014", "tokens": ["Schluch\u00b7zend", "lag", "ich", "vor", "ihr", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "APPRART", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Da war\u2019s mir, als st\u00fcrbe der Gott in mir,", "tokens": ["Da", "wa\u00b7r's", "mir", ",", "als", "st\u00fcr\u00b7be", "der", "Gott", "in", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als st\u00fcrb\u2019 an s\u00fcndlose Lieb\u2019 mir der Glaube \u2026", "tokens": ["Als", "st\u00fcrb'", "an", "s\u00fcnd\u00b7lo\u00b7se", "Lieb'", "mir", "der", "Glau\u00b7be", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft,", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wu\u00dft ich, da\u00df ich die Seele verloren \u2014", "tokens": ["Da", "wu\u00dft", "ich", ",", "da\u00df", "ich", "die", "See\u00b7le", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da wu\u00dft ich, da\u00df ich dem Sch\u00e4cher gleich,", "tokens": ["Da", "wu\u00dft", "ich", ",", "da\u00df", "ich", "dem", "Sch\u00e4\u00b7cher", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meine Seele der H\u00f6lle zugeschworen! \u2026", "tokens": ["Mei\u00b7ne", "See\u00b7le", "der", "H\u00f6l\u00b7le", "zu\u00b7ge\u00b7schwo\u00b7ren", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Es hat die Dirne mich gek\u00fc\u00dft \u2014", "tokens": ["Es", "hat", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl trink ich in ihren Armen Wonne \u2014 \u2014", "tokens": ["Wohl", "trink", "ich", "in", "ih\u00b7ren", "Ar\u00b7men", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In meinem Herzen aber ist Finsterni\u00df,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "a\u00b7ber", "ist", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und verdorrt ist mir des Gl\u00fcckes Bronne! \u2026", "tokens": ["Und", "ver\u00b7dorrt", "ist", "mir", "des", "Gl\u00fc\u00b7ckes", "Bron\u00b7ne", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Verdorrt ist mir der lebendige Muth,", "tokens": ["Ver\u00b7dorrt", "ist", "mir", "der", "le\u00b7ben\u00b7di\u00b7ge", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "F\u00fcr meine Br\u00fcder die Gasse zu bahnen, \u2014", "tokens": ["F\u00fcr", "mei\u00b7ne", "Br\u00fc\u00b7der", "die", "Gas\u00b7se", "zu", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zerbrochen hab\u2019 ich die blitzende Wehr,", "tokens": ["Zer\u00b7bro\u00b7chen", "hab'", "ich", "die", "blit\u00b7zen\u00b7de", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zerbrochen die wurfzerfetzten Fahnen . . . .", "tokens": ["Zer\u00b7bro\u00b7chen", "die", "wurf\u00b7zer\u00b7fetz\u00b7ten", "Fah\u00b7nen", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Seitdem die Dirne mich gek\u00fc\u00dft", "tokens": ["Seit\u00b7dem", "die", "Dir\u00b7ne", "mich", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ich nur ", "tokens": ["Kann", "ich", "nur"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In Br\u00fcnsten umklamm\u2019re ich den wei\u00dfen Leib", "tokens": ["In", "Br\u00fcns\u00b7ten", "um\u00b7klam\u00b7m'\u00b7re", "ich", "den", "wei\u00b7\u00dfen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und k\u00fcsse sie \u2014 und der Rest ist Schweigen . . . . . .", "tokens": ["Und", "k\u00fcs\u00b7se", "sie", "und", "der", "Rest", "ist", "Schwei\u00b7gen", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KON", "ART", "NN", "VAFIN", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}