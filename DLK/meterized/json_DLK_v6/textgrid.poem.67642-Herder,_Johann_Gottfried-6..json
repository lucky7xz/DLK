, "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weltgeist, Tyrann!", "tokens": ["Welt\u00b7geist", ",", "Ty\u00b7rann", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$."], "meter": "++-+", "measure": "iambic.di"}}}}}