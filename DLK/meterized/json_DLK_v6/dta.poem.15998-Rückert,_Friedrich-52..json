{"dta.poem.15998": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die leichtste Kunst f\u00fcr dich ist, F\u00fcrst, geliebt zu werden;", "tokens": ["Die", "leichts\u00b7te", "Kunst", "f\u00fcr", "dich", "ist", ",", "F\u00fcrst", ",", "ge\u00b7liebt", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VAFIN", "$,", "NN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur liebreich brauchst du dich, nur menschlich zu geberden.", "tokens": ["Nur", "lieb\u00b7reich", "brauchst", "du", "dich", ",", "nur", "menschlich", "zu", "ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Viel schwerer f\u00e4llt es euch, da\u00df ihr verha\u00dft euch macht;", "tokens": ["Viel", "schwe\u00b7rer", "f\u00e4llt", "es", "euch", ",", "da\u00df", "ihr", "ver\u00b7ha\u00dft", "euch", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch in dieser Kunst habt ihrs so weit gebracht.", "tokens": ["Und", "doch", "in", "die\u00b7ser", "Kunst", "habt", "ihrs", "so", "weit", "ge\u00b7bracht", "."],