{"textgrid.poem.36569": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein M\u00e4dchen, sch\u00f6ner ist", "tokens": ["Ich", "wei\u00df", "ein", "M\u00e4d\u00b7chen", ",", "sch\u00f6\u00b7ner", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein M\u00e4dchen auf der Welt!", "tokens": ["Kein", "M\u00e4d\u00b7chen", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, der du nie verzaubert bist,", "tokens": ["Du", ",", "der", "du", "nie", "ver\u00b7zau\u00b7bert", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, Weiser, oder Held!", "tokens": ["Du", ",", "Wei\u00b7ser", ",", "o\u00b7der", "Held", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du solltest nur mit einem Blick,", "tokens": ["Du", "soll\u00b7test", "nur", "mit", "ei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem nur, es sehn!", "tokens": ["Mit", "ei\u00b7nem", "nur", ",", "es", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem\u00fctig w\u00fcrdest du zur\u00fcck", "tokens": ["De\u00b7m\u00fc\u00b7tig", "w\u00fcr\u00b7dest", "du", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Mut und Weisheit gehn.", "tokens": ["Zu", "Mut", "und", "Weis\u00b7heit", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hineingezogen in ihr Netz", "tokens": ["Hin\u00b7ein\u00b7ge\u00b7zo\u00b7gen", "in", "ihr", "Netz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nheit, l\u00e4gst du da!", "tokens": ["Der", "Sch\u00f6n\u00b7heit", ",", "l\u00e4gst", "du", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Reich, ihr Zepter, ihr Gesetz", "tokens": ["Ihr", "Reich", ",", "ihr", "Zep\u00b7ter", ",", "ihr", "Ge\u00b7setz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkennend, l\u00e4gst du da!", "tokens": ["Er\u00b7ken\u00b7nend", ",", "l\u00e4gst", "du", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Welch eine Stimme! wie so s\u00fc\u00df!", "tokens": ["Welch", "ei\u00b7ne", "Stim\u00b7me", "!", "wie", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst sei es, oder Scherz,", "tokens": ["Ernst", "sei", "es", ",", "o\u00b7der", "Scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie s\u00e4ng', und spr\u00e4ch' ein Paradies", "tokens": ["Sie", "s\u00e4ng'", ",", "und", "spr\u00e4ch'", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst Gellerten in's Herz!", "tokens": ["Selbst", "Gel\u00b7ler\u00b7ten", "in's", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ihr Auge? Solche Heiterkeit", "tokens": ["Ihr", "Au\u00b7ge", "?", "Sol\u00b7che", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im weiblichen Gesicht", "tokens": ["Im", "weib\u00b7li\u00b7chen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fand ich auf Erden weit und breit,", "tokens": ["Fand", "ich", "auf", "Er\u00b7den", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4nd' ich im Himmel nicht!", "tokens": ["F\u00e4nd'", "ich", "im", "Him\u00b7mel", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr L\u00e4cheln macht das Dunkle hell!", "tokens": ["Ihr", "L\u00e4\u00b7cheln", "macht", "das", "Dunk\u00b7le", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Engel w\u00fcrde froh,", "tokens": ["Ein", "En\u00b7gel", "w\u00fcr\u00b7de", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6nnt' er es sehn! Kein Raphael,", "tokens": ["K\u00f6nnt'", "er", "es", "sehn", "!", "Kein", "Ra\u00b7phael", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "PIAT", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Kein \u00d6ser malt es so!", "tokens": ["Kein", "\u00d6\u00b7ser", "malt", "es", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Busen? Tugend stirbt davon!", "tokens": ["Ihr", "Bu\u00b7sen", "?", "Tu\u00b7gend", "stirbt", "da\u00b7von", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wundersch\u00f6n ist er!", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "ist", "er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Nicht Zeus und nicht Anakreon", "tokens": ["Nicht", "Zeus", "und", "nicht", "A\u00b7nak\u00b7re\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "KON", "PTKNEG", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sah einen niedlicher!", "tokens": ["Sah", "ei\u00b7nen", "nied\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "O welche Rosen, welch ein Reiz", "tokens": ["O", "wel\u00b7che", "Ro\u00b7sen", ",", "welch", "ein", "Reiz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie abzubrechen! Komm!", "tokens": ["Sie", "ab\u00b7zu\u00b7bre\u00b7chen", "!", "Komm", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Freund, genug f\u00fcr deinen Geiz,", "tokens": ["O", "Freund", ",", "ge\u00b7nug", "f\u00fcr", "dei\u00b7nen", "Geiz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O w\u00e4rst du nicht zu fromm!", "tokens": ["O", "w\u00e4rst", "du", "nicht", "zu", "fromm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr tiefes Gr\u00fcbchen in dem Kinn!", "tokens": ["Ihr", "tie\u00b7fes", "Gr\u00fcb\u00b7chen", "in", "dem", "Kinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sch\u00f6nes Blut! Ihr Scho\u00df!", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Blut", "!", "Ihr", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Wuchs! Ihr Gang! O Zauberin!", "tokens": ["Ihr", "Wuchs", "!", "Ihr", "Gang", "!", "O", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O G\u00f6ttin! la\u00df mich los!", "tokens": ["O", "G\u00f6t\u00b7tin", "!", "la\u00df", "mich", "los", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}