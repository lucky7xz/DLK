{"textgrid.poem.64500": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "124. An die Karten", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Bl\u00e4tter, wo in bunten Z\u00fcgen", "tokens": ["Ihr", "Bl\u00e4t\u00b7ter", ",", "wo", "in", "bun\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gewinnsucht Wohl und Weh erblickt,", "tokens": ["Ge\u00b7winn\u00b7sucht", "Wohl", "und", "Weh", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr gabt entfernt mir ein Vergn\u00fcgen,", "tokens": ["Ihr", "gabt", "ent\u00b7fernt", "mir", "ein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ward durch euch kein Spieler noch entz\u00fcckt.", "tokens": ["So", "ward", "durch", "euch", "kein", "Spie\u00b7ler", "noch", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch nahm - auf wiederholt Befehlen,", "tokens": ["Euch", "nahm", "auf", "wie\u00b7der\u00b7holt", "Be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie, die nur aus Gehorsam spielt,", "tokens": ["Sie", ",", "die", "nur", "aus", "Ge\u00b7hor\u00b7sam", "spielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu lebhaft, Tr\u00fcmpfe nachzuz\u00e4hlen,", "tokens": ["Zu", "leb\u00b7haft", ",", "Tr\u00fcmp\u00b7fe", "nach\u00b7zu\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beym Gil Blas lacht, in Athalien", "tokens": ["Beym", "Gil", "Blas", "lacht", ",", "in", "A\u00b7tha\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und, seufzend indem sie euch fa\u00dfte,", "tokens": ["Und", ",", "seuf\u00b7zend", "in\u00b7dem", "sie", "euch", "fa\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sprach sie: K\u00e4m' Phaon doch herein,", "tokens": ["Sprach", "sie", ":", "K\u00e4m'", "Phaon", "doch", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Mich, wenn ich ihn auch noch mehr ha\u00dfte,", "tokens": ["Mich", ",", "wenn", "ich", "ihn", "auch", "noch", "mehr", "ha\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verha\u00dftes Spiel, von dir nur zu befreyn!", "tokens": ["Ver\u00b7ha\u00df\u00b7tes", "Spiel", ",", "von", "dir", "nur", "zu", "be\u00b7freyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}