{"textgrid.poem.40087": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treiben? \u2013 Gut!", "tokens": ["Trei\u00b7ben", "?", "\u2013", "Gut", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "nur: keine Gesetze!", "tokens": ["nur", ":", "kei\u00b7ne", "Ge\u00b7set\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich bin die Flut,", "tokens": ["Ich", "bin", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ihr \u2013 seid die Kl\u00f6tze.", "tokens": ["ihr", "\u2013", "seid", "die", "Kl\u00f6t\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}