{"textgrid.poem.34588": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "11. Abri\u00df eines gemeinen Schulmannes", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt zeucht ein Hencker auf, der ehrlich steupt und hauet,", "tokens": ["Itzt", "zeucht", "ein", "Hen\u00b7cker", "auf", ",", "der", "ehr\u00b7lich", "steupt", "und", "hau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bergmann, der allein auf alte Gr\u00fcnde bauet,", "tokens": ["Ein", "Berg\u00b7mann", ",", "der", "al\u00b7lein", "auf", "al\u00b7te", "Gr\u00fcn\u00b7de", "bau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Feind von allem dem, so nicht nach Grichen reucht,", "tokens": ["Ein", "Feind", "von", "al\u00b7lem", "dem", ",", "so", "nicht", "nach", "Gri\u00b7chen", "reucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "ART", "$,", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Fuchs, der in sein Loch auf recht Lateinisch kreucht.", "tokens": ["Ein", "Fuchs", ",", "der", "in", "sein", "Loch", "auf", "recht", "La\u00b7tei\u00b7nisch", "kreucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "APPR", "PPOSAT", "NN", "APPR", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein K\u00f6nig, wo er lehrt, ein Scheusaal auf den Gassen,", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "wo", "er", "lehrt", ",", "ein", "Scheu\u00b7saal", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Atlas, der noch mehr, als Atlas, wil umfassen,", "tokens": ["Ein", "At\u00b7las", ",", "der", "noch", "mehr", ",", "als", "At\u00b7las", ",", "wil", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "ADV", "$,", "KOUS", "NE", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Buhler, der zugleich neun alte M\u00e4gde liebt,", "tokens": ["Ein", "Buh\u00b7ler", ",", "der", "zu\u00b7gleich", "neun", "al\u00b7te", "M\u00e4g\u00b7de", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Kaufmann, der sein Geld f\u00fcr alte Lumpen giebt.", "tokens": ["Ein", "Kauf\u00b7mann", ",", "der", "sein", "Geld", "f\u00fcr", "al\u00b7te", "Lum\u00b7pen", "giebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Cicero, wenn er auf seinem Neste schwebet,", "tokens": ["Ein", "Ci\u00b7ce\u00b7ro", ",", "wenn", "er", "auf", "sei\u00b7nem", "Nes\u00b7te", "schwe\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein rechter Tacitus, wenn er bey fremden lebet,", "tokens": ["Ein", "rech\u00b7ter", "Ta\u00b7ci\u00b7tus", ",", "wenn", "er", "bey", "frem\u00b7den", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "KOUS", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein G\u00e4rtner, wo der Mensch an statt der Propfer ist,", "tokens": ["Ein", "G\u00e4rt\u00b7ner", ",", "wo", "der", "Mensch", "an", "statt", "der", "Prop\u00b7fer", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Reise-Mann, so ihm die Fl\u00fcgel hat erkiest,", "tokens": ["Ein", "Rei\u00b7se\u00b7Mann", ",", "so", "ihm", "die", "Fl\u00fc\u00b7gel", "hat", "er\u00b7kiest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Held, wo ABC mu\u00df zu Soldaten werden,", "tokens": ["Ein", "Held", ",", "wo", "AbC", "mu\u00df", "zu", "Sol\u00b7da\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NE", "VMFIN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ein Igel, wenn er z\u00fcrnt, ein Affe von Geberden,", "tokens": ["Ein", "I\u00b7gel", ",", "wenn", "er", "z\u00fcrnt", ",", "ein", "Af\u00b7fe", "von", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ein schwartzgef\u00e4rbtes Ziel, den Sch\u00fctzen vorgesetzt,", "tokens": ["Ein", "schwartz\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Ziel", ",", "den", "Sch\u00fct\u00b7zen", "vor\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Fechter, der allzeit das Hintertheil verletzt.", "tokens": ["Ein", "Fech\u00b7ter", ",", "der", "all\u00b7zeit", "das", "Hin\u00b7ter\u00b7theil", "ver\u00b7letzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein lebendiges Buch, besudelt eingebunden,", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ges", "Buch", ",", "be\u00b7su\u00b7delt", "ein\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Bergmann, der sein Geld hat durch die Ruthe funden,", "tokens": ["Ein", "Berg\u00b7mann", ",", "der", "sein", "Geld", "hat", "durch", "die", "Ru\u00b7the", "fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein gro\u00df Com\u00f6diant, der die Personen f\u00fchrt,", "tokens": ["Ein", "gro\u00df", "Co\u00b7m\u00f6\u00b7di\u00b7ant", ",", "der", "die", "Per\u00b7so\u00b7nen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.20": {"text": "Durch die der weite Platz der Erden wird geziert.", "tokens": ["Durch", "die", "der", "wei\u00b7te", "Platz", "der", "Er\u00b7den", "wird", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er lebet ohne Ruh mit Reymen bey dem Tische,", "tokens": ["Er", "le\u00b7bet", "oh\u00b7ne", "Ruh", "mit", "Rey\u00b7men", "bey", "dem", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Pa\u00dft eine Sylbe nicht, so macht er ein Gezische,", "tokens": ["Pa\u00dft", "ei\u00b7ne", "Syl\u00b7be", "nicht", ",", "so", "macht", "er", "ein", "Ge\u00b7zi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Verachtet Spei\u00df und Tranck, verstellet Nas' und Mund,", "tokens": ["Ver\u00b7ach\u00b7tet", "Spei\u00df", "und", "Tranck", ",", "ver\u00b7stel\u00b7let", "Nas'", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und f\u00fchret ein Gesicht als ein erz\u00fcrnter Hund,", "tokens": ["Und", "f\u00fch\u00b7ret", "ein", "Ge\u00b7sicht", "als", "ein", "er\u00b7z\u00fcrn\u00b7ter", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der auf den Jungen liegt. Reist Morpheus ihn darnieder,", "tokens": ["Der", "auf", "den", "Jun\u00b7gen", "liegt", ".", "Reist", "Mor\u00b7pheus", "ihn", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "NN", "NE", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So f\u00fchret ihn der Traum auf seine Tages-Lieder;", "tokens": ["So", "f\u00fch\u00b7ret", "ihn", "der", "Traum", "auf", "sei\u00b7ne", "Ta\u00b7ges\u00b7Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bald schreyt er Barbara; bald mu\u00df Celarent her,", "tokens": ["Bald", "schreyt", "er", "Bar\u00b7ba\u00b7ra", ";", "bald", "mu\u00df", "Ce\u00b7la\u00b7rent", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$.", "ADV", "VMFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.28": {"text": "Bald rufft er Tytiro; bald leufft er \u00fcber Meer,", "tokens": ["Bald", "rufft", "er", "Ty\u00b7ti\u00b7ro", ";", "bald", "leufft", "er", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "F\u00fchrt Aristotelen und tausend alte Griechen", "tokens": ["F\u00fchrt", "A\u00b7ris\u00b7to\u00b7te\u00b7len", "und", "tau\u00b7send", "al\u00b7te", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Um seine Feder her; bald schmei\u00dft er um die Z\u00fcchen,", "tokens": ["Um", "sei\u00b7ne", "Fe\u00b7der", "her", ";", "bald", "schmei\u00dft", "er", "um", "die", "Z\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Trifft seiner Frauen Mund, und dessen Hintertheil,", "tokens": ["Trifft", "sei\u00b7ner", "Frau\u00b7en", "Mund", ",", "und", "des\u00b7sen", "Hin\u00b7ter\u00b7theil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,", "KON", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der nechst aus Schelmerey ihm einen langen Keil", "tokens": ["Der", "nechst", "aus", "Schel\u00b7me\u00b7rey", "ihm", "ei\u00b7nen", "lan\u00b7gen", "Keil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Durch seinen Sessel schlug. Bald f\u00e4ngt ihm an zutr\u00e4umen,", "tokens": ["Durch", "sei\u00b7nen", "Ses\u00b7sel", "schlug", ".", "Bald", "f\u00e4ngt", "ihm", "an", "zu\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie er das Ungemach der Schule m\u00f6cht vers\u00e4umen,", "tokens": ["Wie", "er", "das", "Un\u00b7ge\u00b7mach", "der", "Schu\u00b7le", "m\u00f6cht", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Greift auf die Hosen zu, und kehrt sich mit der Hand,", "tokens": ["Greift", "auf", "die", "Ho\u00b7sen", "zu", ",", "und", "kehrt", "sich", "mit", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Streicht Speichel um den Schlaff, und druckt sein bestes Pfand,", "tokens": ["Streicht", "Spei\u00b7chel", "um", "den", "Schlaff", ",", "und", "druckt", "sein", "bes\u00b7tes", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein alt Vocabel-Buch mit den beschmierten Armen,", "tokens": ["Ein", "alt", "Vo\u00b7ca\u00b7bel\u00b7Buch", "mit", "den", "be\u00b7schmier\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "In Hoffnung von der Kunst desselben zu erwarmen,", "tokens": ["In", "Hoff\u00b7nung", "von", "der", "Kunst", "des\u00b7sel\u00b7ben", "zu", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "L\u00e4uft sobald schnarchende in sein Regierungs-Hau\u00df,", "tokens": ["L\u00e4uft", "so\u00b7bald", "schnar\u00b7chen\u00b7de", "in", "sein", "Re\u00b7gie\u00b7rungs\u00b7Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Und trinckt vor b\u00f6se Luft ein Gla\u00df Gebrantes aus.", "tokens": ["Und", "trinckt", "vor", "b\u00f6\u00b7se", "Luft", "ein", "Gla\u00df", "Ge\u00b7bran\u00b7tes", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Trit er auf seinen Thron, so mu\u00df ihm Cato weichen,", "tokens": ["Trit", "er", "auf", "sei\u00b7nen", "Thron", ",", "so", "mu\u00df", "ihm", "Ca\u00b7to", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So ist an Tyranney ihm Nero nicht zugleichen;", "tokens": ["So", "ist", "an", "Ty\u00b7ran\u00b7ney", "ihm", "Ne\u00b7ro", "nicht", "zu\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.43": {"text": "Da streicht er, rauft und schl\u00e4gt, bi\u00df seine Stirne schwitzt,", "tokens": ["Da", "streicht", "er", ",", "rauft", "und", "schl\u00e4gt", ",", "bi\u00df", "sei\u00b7ne", "Stir\u00b7ne", "schwitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Bi\u00df das vertraute Volck auf b\u00f6sem Leder sitzt,", "tokens": ["Bi\u00df", "das", "ver\u00b7trau\u00b7te", "Volck", "auf", "b\u00f6\u00b7sem", "Le\u00b7der", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und seine Hand beklagt. Drum bleibt er ungeliebet,", "tokens": ["Und", "sei\u00b7ne", "Hand", "be\u00b7klagt", ".", "Drum", "bleibt", "er", "un\u00b7ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$.", "PAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "H\u00f6rt, wie das Schulgeschrey ihm b\u00f6se Nahmen giebet,", "tokens": ["H\u00f6rt", ",", "wie", "das", "Schul\u00b7ge\u00b7schrey", "ihm", "b\u00f6\u00b7se", "Nah\u00b7men", "gie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und schilt ihn \u00f6ffentlich f\u00fcr einen solchen Mann,", "tokens": ["Und", "schilt", "ihn", "\u00f6f\u00b7fent\u00b7lich", "f\u00fcr", "ei\u00b7nen", "sol\u00b7chen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.48": {"text": "Der andre f\u00fchren wil und selbst nicht sehen kan.", "tokens": ["Der", "and\u00b7re", "f\u00fch\u00b7ren", "wil", "und", "selbst", "nicht", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der Schwindel dreuet ihm mit einer Todten-Bahre,", "tokens": ["Der", "Schwin\u00b7del", "dreu\u00b7et", "ihm", "mit", "ei\u00b7ner", "Tod\u00b7ten\u00b7Bah\u00b7re", ","], "token_info": [