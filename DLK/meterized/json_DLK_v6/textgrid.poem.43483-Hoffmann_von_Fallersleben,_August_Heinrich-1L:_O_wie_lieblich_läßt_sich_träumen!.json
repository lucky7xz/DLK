{"textgrid.poem.43483": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie lieblich l\u00e4\u00dft sich tr\u00e4umen!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie lieblich l\u00e4\u00dft sich tr\u00e4umen!", "tokens": ["O", "wie", "lieb\u00b7lich", "l\u00e4\u00dft", "sich", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieder wehen aus den L\u00fcften,", "tokens": ["Lie\u00b7der", "we\u00b7hen", "aus", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieder s\u00e4useln aus den B\u00e4umen,", "tokens": ["Lie\u00b7der", "s\u00e4u\u00b7seln", "aus", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Blumen s\u00fc\u00dfen D\u00fcften.", "tokens": ["Aus", "der", "Blu\u00b7men", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Aber Vetter Michel bleibet", "tokens": ["A\u00b7ber", "Vet\u00b7ter", "Mi\u00b7chel", "blei\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur bei Raff's Naturgeschichte,", "tokens": ["Nur", "bei", "Raff's", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Holt sich daraus Stoff und schreibet", "tokens": ["Holt", "sich", "da\u00b7raus", "Stoff", "und", "schrei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PAV", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gro\u00dfe deutsche Prachtgedichte.", "tokens": ["Gro\u00b7\u00dfe", "deut\u00b7sche", "Pracht\u00b7ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie fr\u00f6hlich l\u00e4\u00dft sich singen,", "tokens": ["O", "wie", "fr\u00f6h\u00b7lich", "l\u00e4\u00dft", "sich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Nachtigallen schlagen,", "tokens": ["Wenn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trost dem deutschen Herzen bringen", "tokens": ["Trost", "dem", "deut\u00b7schen", "Her\u00b7zen", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Fr\u00fchlings lichten Tagen!", "tokens": ["In", "des", "Fr\u00fch\u00b7lings", "lich\u00b7ten", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vetter Michel will nur singen,", "tokens": ["Vet\u00b7ter", "Mi\u00b7chel", "will", "nur", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Elephanten machen,", "tokens": ["Was", "die", "E\u00b7le\u00b7phan\u00b7ten", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Leu'n und Tiger springen", "tokens": ["Wie", "die", "Leu'n", "und", "Ti\u00b7ger", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Paviane lachen.", "tokens": ["Und", "die", "Pa\u00b7vi\u00b7a\u00b7ne", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vetter Michel hat's errungen,", "tokens": ["Vet\u00b7ter", "Mi\u00b7chel", "hat's", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat aus Raff's Naturgeschichte", "tokens": ["Hat", "aus", "Raff's", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen neuen Stoff versungen", "tokens": ["Ei\u00b7nen", "neu\u00b7en", "Stoff", "ver\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem sch\u00f6nsten Prachtgedichte.", "tokens": ["Zu", "dem", "sch\u00f6ns\u00b7ten", "Pracht\u00b7ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Frau ist hochentz\u00fccket,", "tokens": ["Sei\u00b7ne", "Frau", "ist", "ho\u00b7chent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie eilet fort nach Hause,", "tokens": ["Und", "sie", "ei\u00b7let", "fort", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kehret wieder und begl\u00fccket", "tokens": ["Keh\u00b7ret", "wie\u00b7der", "und", "be\u00b7gl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihn mit einem seltnen Schmause.", "tokens": ["Ihn", "mit", "ei\u00b7nem", "selt\u00b7nen", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil er solches konnte schreiben,", "tokens": ["Weil", "er", "sol\u00b7ches", "konn\u00b7te", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt sie ihrem Auserkohr'nen,", "tokens": ["Bringt", "sie", "ih\u00b7rem", "Aus\u00b7er\u00b7kohr'\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erstlich Pumpernickelscheiben,", "tokens": ["Erst\u00b7lich", "Pum\u00b7per\u00b7ni\u00b7ckel\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann noch Bier vom ausgegohr'nen,", "tokens": ["Dann", "noch", "Bier", "vom", "aus\u00b7geg\u00b7ohr'\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie lassen die Gedichte", "tokens": ["Und", "sie", "las\u00b7sen", "die", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben hoch viel tausendmale,", "tokens": ["Le\u00b7ben", "hoch", "viel", "tau\u00b7send\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Segnen Raff's Naturgeschichte", "tokens": ["Seg\u00b7nen", "Raff's", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Thierweltideale.", "tokens": ["Und", "die", "Thier\u00b7wel\u00b7ti\u00b7de\u00b7a\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}