{"dta.poem.7693": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Bl\u00e4tter, die so fest j\u00fcngst sa\u00dfen an den Stielen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", ",", "die", "so", "fest", "j\u00fcngst", "sa\u00b7\u00dfen", "an", "den", "Stie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich dachte da\u00df sie nicht vor einem Monat fielen.", "tokens": ["Ich", "dach\u00b7te", "da\u00df", "sie", "nicht", "vor", "ei\u00b7nem", "Mo\u00b7nat", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Frisch, hofft' ich, sollten sie tief in den Winter dauern;", "tokens": ["Frisch", ",", "hofft'", "ich", ",", "soll\u00b7ten", "sie", "tief", "in", "den", "Win\u00b7ter", "dau\u00b7ern", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf einmal rieseln sie herab in bangen Schauern.", "tokens": ["Auf", "ein\u00b7mal", "rie\u00b7seln", "sie", "her\u00b7ab", "in", "ban\u00b7gen", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Sturm hat sie geknickt, kein Frost hat sie verletzt;", "tokens": ["Kein", "Sturm", "hat", "sie", "ge\u00b7knickt", ",", "kein", "Frost", "hat", "sie", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hat sich in der Luft, im Baumsaft was zersetzt?", "tokens": ["Was", "hat", "sich", "in", "der", "Luft", ",", "im", "Baum\u00b7saft", "was", "zer\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wodurch verkommen sind sie so auf einmal nur?", "tokens": ["Wo\u00b7durch", "ver\u00b7kom\u00b7men", "sind", "sie", "so", "auf", "ein\u00b7mal", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "ADV", "APPR", "ADV", "ADV", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie starben, Greisen gleich, am Nachla\u00df der Natur.", "tokens": ["Sie", "star\u00b7ben", ",", "Grei\u00b7sen", "gleich", ",", "am", "Nachl\u00b7a\u00df", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}