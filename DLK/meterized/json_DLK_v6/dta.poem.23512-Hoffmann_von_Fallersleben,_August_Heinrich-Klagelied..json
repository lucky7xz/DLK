{"dta.poem.23512": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann einst die Flaschen gr\u00f6\u00dfer werden,", "tokens": ["Wann", "einst", "die", "Fla\u00b7schen", "gr\u00f6\u00b7\u00dfer", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann einst wohlfeiler wird der Wein,", "tokens": ["Wann", "einst", "wohl\u00b7fei\u00b7ler", "wird", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann findet sich vielleicht auf Erden", "tokens": ["Dann", "fin\u00b7det", "sich", "viel\u00b7leicht", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die goldne Zeit noch einmal ein.", "tokens": ["Die", "gold\u00b7ne", "Zeit", "noch", "ein\u00b7mal", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nicht f\u00fcr uns! uns ist geboten", "tokens": ["Doch", "nicht", "f\u00fcr", "uns", "!", "uns", "ist", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In allen Dingen N\u00fcchternheit \u2014", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "N\u00fcch\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die goldne Zeit geh\u00f6rt den Todten,", "tokens": ["Die", "gold\u00b7ne", "Zeit", "ge\u00b7h\u00f6rt", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und uns nur die papierne Zeit.", "tokens": ["Und", "uns", "nur", "die", "pa\u00b7pier\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! kleiner werden unsre Flaschen,", "tokens": ["Ach", "!", "klei\u00b7ner", "wer\u00b7den", "uns\u00b7re", "Fla\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00e4glich theurer wird der Wein,", "tokens": ["Und", "t\u00e4g\u00b7lich", "theu\u00b7rer", "wird", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leerer wird's in unsern Taschen \u2014", "tokens": ["Und", "lee\u00b7rer", "wird's", "in", "un\u00b7sern", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar keine Zeit wird bald mehr sein.", "tokens": ["Gar", "kei\u00b7ne", "Zeit", "wird", "bald", "mehr", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}