{"textgrid.poem.31666": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein beherztes dapfers Herz ist dem Gl\u00fcck nicht untergeben:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein beherztes dapfers Herz ist dem Gl\u00fcck nicht untergeben:", "tokens": ["Ein", "be\u00b7herz\u00b7tes", "dap\u00b7fers", "Herz", "ist", "dem", "Gl\u00fcck", "nicht", "un\u00b7ter\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "kan sich auch / von ihm gest\u00fcrzt / durch Gro\u00dfm\u00fctigkeit erheben.", "tokens": ["kan", "sich", "auch", "/", "von", "ihm", "ge\u00b7st\u00fcrzt", "/", "durch", "Gro\u00df\u00b7m\u00fc\u00b7tig\u00b7keit", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "$(", "APPR", "PPER", "VVPP", "$(", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Marx Coriolanens Elend / bracht ihn in der Feinde Gunst.", "tokens": ["Marx", "Co\u00b7ri\u00b7o\u00b7la\u00b7nens", "E\u00b7lend", "/", "bracht", "ihn", "in", "der", "Fein\u00b7de", "Gunst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Also ist / zum Ehr-erlangen / Vngl\u00fccks Sieg die n\u00e4chste Kunst.", "tokens": ["Al\u00b7so", "ist", "/", "zum", "Ehr\u00b7er\u00