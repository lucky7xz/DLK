{"textgrid.poem.57034": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmstr\u00f6m liebt es, Tiere nachzuahmen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m liebt es, Tiere nachzuahmen,", "tokens": ["Palm\u00b7str\u00f6m", "liebt", "es", ",", "Tie\u00b7re", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und erzieht zwei junge Schneider", "tokens": ["und", "er\u00b7zieht", "zwei", "jun\u00b7ge", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "CARD", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lediglich auf Tierkost\u00fcme.", "tokens": ["le\u00b7di\u00b7glich", "auf", "Tier\u00b7kos\u00b7t\u00fc\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So z.B. hockt er gern als Rabe", "tokens": ["So", "z.", "B.", "hockt", "er", "gern", "als", "Ra\u00b7be"], "token_info": ["word", "abbreviation", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auf dem oberen Aste einer Eiche", "tokens": ["auf", "dem", "o\u00b7be\u00b7ren", "As\u00b7te", "ei\u00b7ner", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "und beobachtet den Himmel.", "tokens": ["und", "be\u00b7ob\u00b7ach\u00b7tet", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "H\u00e4ufig auch als Bernhardiner", "tokens": ["H\u00e4u\u00b7fig", "auch", "als", "Bern\u00b7har\u00b7di\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "legt er zottigen Kopf auf tapfere Pfoten,", "tokens": ["legt", "er", "zot\u00b7ti\u00b7gen", "Kopf", "auf", "tap\u00b7fe\u00b7re", "Pfo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "bellt im Schlaf und tr\u00e4umt gerettete Wanderer.", "tokens": ["bellt", "im", "Schlaf", "und", "tr\u00e4umt", "ge\u00b7ret\u00b7te\u00b7te", "Wan\u00b7de\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Oder spinnt ein Netz in seinem Garten", "tokens": ["O\u00b7der", "spinnt", "ein", "Netz", "in", "sei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus Spagat und sitzt als eine Spinne", "tokens": ["aus", "Spa\u00b7gat", "und", "sitzt", "als", "ei\u00b7ne", "Spin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "tagelang in dessen Mitte.", "tokens": ["ta\u00b7ge\u00b7lang", "in", "des\u00b7sen", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRELAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder schwimmt, ein glotzge\u00e4ugter Karpfen,", "tokens": ["O\u00b7der", "schwimmt", ",", "ein", "glotz\u00b7ge\u00b7\u00e4ug\u00b7ter", "Karp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "rund um die Font\u00e4ne seines Teiches", "tokens": ["rund", "um", "die", "Fon\u00b7t\u00e4\u00b7ne", "sei\u00b7nes", "Tei\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und erlaubt den Kindern ihn zu f\u00fcttern.", "tokens": ["und", "er\u00b7laubt", "den", "Kin\u00b7dern", "ihn", "zu", "f\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Oder h\u00e4ngt sich im Kost\u00fcm des Storches", "tokens": ["O\u00b7der", "h\u00e4ngt", "sich", "im", "Kos\u00b7t\u00fcm", "des", "Stor\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "unter eines Luftschiffs Gondel", "tokens": ["un\u00b7ter", "ei\u00b7nes", "Luft\u00b7schiffs", "Gon\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und verreist so nach \u00c4gypten.", "tokens": ["und", "ver\u00b7reist", "so", "nach", "\u00c4\u00b7gyp\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}