{"textgrid.poem.62010": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[o sieh, wie sich's t\u00fcrmt]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sieh, wie sich's t\u00fcrmt,", "tokens": ["O", "sieh", ",", "wie", "sich's", "t\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Welle sich b\u00e4umt,", "tokens": ["Die", "Wel\u00b7le", "sich", "b\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Ufer ergr\u00fcnt,", "tokens": ["Das", "U\u00b7fer", "er\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Von Blumen bes\u00e4umt.", "tokens": ["Von", "Blu\u00b7men", "be\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Es n\u00e4h'rt sich das Boot,", "tokens": ["Es", "n\u00e4h'rt", "sich", "das", "Boot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Woge sie sch\u00e4umt,", "tokens": ["Die", "Wo\u00b7ge", "sie", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das M\u00e4gdlein da drin,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "da", "drin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das M\u00e4gdlein es tr\u00e4umt!", "tokens": ["Das", "M\u00e4gd\u00b7lein", "es", "tr\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und \u00fcber dem Haupt,", "tokens": ["Und", "\u00fc\u00b7ber", "dem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Am Himmel aufw\u00e4rts,", "tokens": ["Am", "Him\u00b7mel", "auf\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein Zeichen von Gold,", "tokens": ["Ein", "Zei\u00b7chen", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein flammendes Herz!", "tokens": ["Ein", "flam\u00b7men\u00b7des", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}