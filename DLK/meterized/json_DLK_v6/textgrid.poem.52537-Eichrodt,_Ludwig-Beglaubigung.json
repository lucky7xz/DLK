{"textgrid.poem.52537": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Beglaubigung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich, das wei\u00dft du;", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "das", "wei\u00dft", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob du mich wieder liebst?", "tokens": ["Ob", "du", "mich", "wie\u00b7der", "liebst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit klopfendem Herzen las ichs", "tokens": ["Mit", "klop\u00b7fen\u00b7dem", "Her\u00b7zen", "las", "ichs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Im Briefe, den du schriebst.", "tokens": ["Im", "Brie\u00b7fe", ",", "den", "du", "schriebst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch erst wenn in die Arme", "tokens": ["Doch", "erst", "wenn", "in", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bald dich schlie\u00dfen kann,", "tokens": ["Ich", "bald", "dich", "schlie\u00b7\u00dfen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich k\u00fcssen und dich herzen,", "tokens": ["Dich", "k\u00fcs\u00b7sen", "und", "dich", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab ich den Glauben dran.", "tokens": ["Hab", "ich", "den", "Glau\u00b7ben", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}