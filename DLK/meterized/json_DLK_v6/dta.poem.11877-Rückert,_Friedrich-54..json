{"dta.poem.11877": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil du der ganzen Welt nicht kannst als Herrscher walten,", "tokens": ["Weil", "du", "der", "gan\u00b7zen", "Welt", "nicht", "kannst", "als", "Herr\u00b7scher", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VMFIN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib ganz sie auf! schlimm ist von ihr ein Theil behalten.", "tokens": ["Gib", "ganz", "sie", "auf", "!", "schlimm", "ist", "von", "ihr", "ein", "Theil", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PTKVZ", "$.", "ADJD", "VAFIN", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Tode mu\u00dft du es, thu's, weil du kannst, im Leben;", "tokens": ["Im", "To\u00b7de", "mu\u00dft", "du", "es", ",", "thu's", ",", "weil", "du", "kannst", ",", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "$,", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gib auf die falsche Welt, eh sie dich auf wird geben.", "tokens": ["Gib", "auf", "die", "fal\u00b7sche", "Welt", ",", "eh", "sie", "dich", "auf", "wird", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Statt der Dem\u00fcthigung gezwungener Entsagung", "tokens": ["Statt", "der", "De\u00b7m\u00fct\u00b7hi\u00b7gung", "ge\u00b7zwun\u00b7ge\u00b7ner", "Ent\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+--++--+---+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sei dein das Hochgef\u00fchl freiwilliger Entschlagung.", "tokens": ["Sei", "dein", "das", "Hoch\u00b7ge\u00b7f\u00fchl", "frei\u00b7wil\u00b7li\u00b7ger", "Ent\u00b7schla\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}