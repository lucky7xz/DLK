{"textgrid.poem.46775": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[weil ich sie dir leider]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbweil ich sie dir leider", "tokens": ["\u00bb", "weil", "ich", "sie", "dir", "lei\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "PPER", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Selbst nicht bringen kann,", "tokens": ["Selbst", "nicht", "brin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bring' ich, lieber Mann,", "tokens": ["Bring'", "ich", ",", "lie\u00b7ber", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hier, was noch von beider", "tokens": ["Hier", ",", "was", "noch", "von", "bei\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "ADV", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Untergang entrann,", "tokens": ["Un\u00b7ter\u00b7gang", "ent\u00b7rann", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dir aufs Zimmer bring' ich ihre Kleider.\u00ab", "tokens": ["Dir", "aufs", "Zim\u00b7mer", "bring'", "ich", "ih\u00b7re", "Klei\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, der Tod, der Scheider", "tokens": ["Ja", ",", "der", "Tod", ",", "der", "Schei\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwischen Seel' und Leib,", "tokens": ["Zwi\u00b7schen", "Seel'", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hat, o liebes Weib", "tokens": ["Hat", ",", "o", "lie\u00b7bes", "Weib"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "FM", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bei dem Scheiden beider", "tokens": ["Bei", "dem", "Schei\u00b7den", "bei\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Diesen Leidvertreib", "tokens": ["Die\u00b7sen", "Leid\u00b7ver\u00b7treib"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Uns doch lassen m\u00fcssen, ihre Kleider.", "tokens": ["Uns", "doch", "las\u00b7sen", "m\u00fcs\u00b7sen", ",", "ih\u00b7re", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMINF", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Seh' ich ihre Kleider,", "tokens": ["Seh'", "ich", "ih\u00b7re", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die sie abgelegt,", "tokens": ["Die", "sie", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mir im Herzen regt", "tokens": ["Mir", "im", "Her\u00b7zen", "regt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sich das Bildni\u00df beider", "tokens": ["Sich", "das", "Bild\u00b7ni\u00df", "bei\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lebender bewegt;", "tokens": ["Le\u00b7ben\u00b7der", "be\u00b7wegt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Doch dir regt der Anblick Thr\u00e4nen leider.", "tokens": ["Doch", "dir", "regt", "der", "An\u00b7blick", "Thr\u00e4\u00b7nen", "lei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Darum nimm die Kleider", "tokens": ["Da\u00b7rum", "nimm", "die", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wieder liebes Weib,", "tokens": ["Wie\u00b7der", "lie\u00b7bes", "Weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und bewahret bleib',", "tokens": ["Und", "be\u00b7wah\u00b7ret", "bleib'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Als Erinnrung beider,", "tokens": ["Als", "E\u00b7rinn\u00b7rung", "bei\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dieser Leib vom Leib,", "tokens": ["Die\u00b7ser", "Leib", "vom", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Fern dem Blick der Sp\u00f6tter und der Neider!", "tokens": ["Fern", "dem", "Blick", "der", "Sp\u00f6t\u00b7ter", "und", "der", "Nei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}