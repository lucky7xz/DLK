{"textgrid.poem.64319": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "39. Rundgesang", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00f6hlich t\u00f6nt der Becher Klang", "tokens": ["Fr\u00f6h\u00b7lich", "t\u00f6nt", "der", "Be\u00b7cher", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im vertrauten Kreise!", "tokens": ["Im", "ver\u00b7trau\u00b7ten", "Krei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lieblich schallt ein Rundgesang", "tokens": ["Lieb\u00b7lich", "schallt", "ein", "Rund\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der V\u00e4ter Weise!", "tokens": ["Nach", "der", "V\u00e4\u00b7ter", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freunde, freut euch alle!", "tokens": ["Freun\u00b7de", ",", "freut", "euch", "al\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Freunde, trinket alle!", "tokens": ["Freun\u00b7de", ",", "trin\u00b7ket", "al\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Singt mit lautem Schalle:", "tokens": ["Singt", "mit", "lau\u00b7tem", "Schal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Traute Br\u00fcder, schenket ein!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sto\u00dfet an, und trinkt den Wein!", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "und", "trinkt", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Winde diese Blumen mir", "tokens": ["Win\u00b7de", "die\u00b7se", "Blu\u00b7men", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Haar; ich winde", "tokens": ["In", "das", "Haar", ";", "ich", "win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Epheu um den Becher dir,", "tokens": ["E\u00b7pheu", "um", "den", "Be\u00b7cher", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Freundliche Selinde!", "tokens": ["Freund\u00b7li\u00b7che", "Se\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "La\u00df den Becher rauschen,", "tokens": ["La\u00df", "den", "Be\u00b7cher", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn die M\u00e4gdlein lauschen,", "tokens": ["Wenn", "die", "M\u00e4gd\u00b7lein", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ob wir K\u00fcsse tauschen!", "tokens": ["Ob", "wir", "K\u00fcs\u00b7se", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Traute Br\u00fcder, schenket ein!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sto\u00dfet an, und trinkt den Wein!", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "und", "trinkt", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du dort, schenke m\u00e4\u00dfig ein!", "tokens": ["Du", "dort", ",", "schen\u00b7ke", "m\u00e4\u00b7\u00dfig", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn Erfahrung lehret,", "tokens": ["Denn", "Er\u00b7fah\u00b7rung", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Scherz und Freude scheucht der Wein,", "tokens": ["Scherz", "und", "Freu\u00b7de", "scheucht", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er uns beth\u00f6ret.", "tokens": ["Wenn", "er", "uns", "be\u00b7th\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, sie fliehn erschrocken", "tokens": ["Ach", ",", "sie", "fliehn", "er\u00b7schro\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Aus zerst\u00f6rten Locken", "tokens": ["Aus", "zer\u00b7st\u00f6r\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von geworfnen Brocken!", "tokens": ["Von", "ge\u00b7worf\u00b7nen", "Bro\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Traute Br\u00fcder, schenket ein!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sto\u00dfet an, und trinkt den Wein!", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "und", "trinkt", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer mit Gegenliebe liebt,", "tokens": ["Wer", "mit", "Ge\u00b7gen\u00b7lie\u00b7be", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freue sich von Herzen!", "tokens": ["Freu\u00b7e", "sich", "von", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Wen sein M\u00e4dchen noch betr\u00fcbt,", "tokens": ["Wen", "sein", "M\u00e4d\u00b7chen", "noch", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffe Trost nach Schmerzen!", "tokens": ["Hof\u00b7fe", "Trost", "nach", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freund, beim Rosenbecher", "tokens": ["Freund", ",", "beim", "Ro\u00b7sen\u00b7be\u00b7cher"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Leert vielleicht dein R\u00e4cher", "tokens": ["Leert", "viel\u00b7leicht", "dein", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Amor seinen K\u00f6cher.", "tokens": ["A\u00b7mor", "sei\u00b7nen", "K\u00f6\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Traute Br\u00fcder, schenket ein!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sto\u00dfet an, und trinkt den Wein!", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "und", "trinkt", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Neue Freuden gehn mir auf!", "tokens": ["Neu\u00b7e", "Freu\u00b7den", "gehn", "mir", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4tter wird die Stirne!", "tokens": ["Gl\u00e4t\u00b7ter", "wird", "die", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leicht wird meines Blutes Lauf,", "tokens": ["Leicht", "wird", "mei\u00b7nes", "Blu\u00b7tes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heller mein Gehirne!", "tokens": ["Hel\u00b7ler", "mein", "Ge\u00b7hir\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seht, die Gl\u00e4ser blinken!", "tokens": ["Seht", ",", "die", "Gl\u00e4\u00b7ser", "blin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Selbst die M\u00e4gdlein winken,", "tokens": ["Selbst", "die", "M\u00e4gd\u00b7lein", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Noch einmal zu trinken!", "tokens": ["Noch", "ein\u00b7mal", "zu", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Traute Br\u00fcder, schenket ein!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sto\u00dfet an, und trinkt den Wein!", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "und", "trinkt", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}