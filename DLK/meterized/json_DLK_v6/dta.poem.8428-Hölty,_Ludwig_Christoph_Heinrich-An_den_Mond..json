{"dta.poem.8428": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durch dieses Buchengr\u00fcn,", "tokens": ["Durch", "die\u00b7ses", "Bu\u00b7chen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo Fantasein und Traumgestalten immer", "tokens": ["Wo", "Fan\u00b7ta\u00b7sein", "und", "Traum\u00b7ge\u00b7stal\u00b7ten", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Vor mir vor\u00fcberfliehn!", "tokens": ["Vor", "mir", "vor\u00b7\u00fc\u00b7berf\u00b7liehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Enth\u00fclle dich, dass ich die St\u00e4tte finde,", "tokens": ["Ent\u00b7h\u00fcl\u00b7le", "dich", ",", "dass", "ich", "die", "St\u00e4t\u00b7te", "fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo oft mein M\u00e4dchen sass,", "tokens": ["Wo", "oft", "mein", "M\u00e4d\u00b7chen", "sass", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oft, im Wehn des Buchbaums und der Linde,", "tokens": ["Und", "oft", ",", "im", "Wehn", "des", "Buch\u00b7baums", "und", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der goldnen Stadt vergass!", "tokens": ["Der", "gold\u00b7nen", "Stadt", "ver\u00b7gass", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Enth\u00fclle dich, dass ich des Strauchs mich freue,", "tokens": ["Ent\u00b7h\u00fcl\u00b7le", "dich", ",", "dass", "ich", "des", "Strauchs", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der K\u00fchlung ihr gerauscht,", "tokens": ["Der", "K\u00fch\u00b7lung", "ihr", "ge\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und einen Kranz auf jeden Anger streue,", "tokens": ["Und", "ei\u00b7nen", "Kranz", "auf", "je\u00b7den", "An\u00b7ger", "streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sie den Bach belauscht!", "tokens": ["Wo", "sie", "den", "Bach", "be\u00b7lauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann, lieber Mond, dann nim den Schleier wieder,", "tokens": ["Dann", ",", "lie\u00b7ber", "Mond", ",", "dann", "nim", "den", "Schlei\u00b7er", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "$,", "ADV", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und traur' um deinen Freund,", "tokens": ["Und", "traur'", "um", "dei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weine durch den Wolkenflor hernieder,", "tokens": ["Und", "wei\u00b7ne", "durch", "den", "Wol\u00b7ken\u00b7flor", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dein Verlassner weint!", "tokens": ["Wie", "dein", "Ver\u00b7lass\u00b7ner", "weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}