{"textgrid.poem.38264": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh aus, mein Herz, und suche Freud", "tokens": ["Geh", "aus", ",", "mein", "Herz", ",", "und", "su\u00b7che", "Freud"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser lieben Sommerzeit,", "tokens": ["In", "die\u00b7ser", "lie\u00b7ben", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An deines Gottes Gaben;", "tokens": ["An", "dei\u00b7nes", "Got\u00b7tes", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schau an der sch\u00f6nen G\u00e4rten Zier,", "tokens": ["Schau", "an", "der", "sch\u00f6\u00b7nen", "G\u00e4r\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und siehe, wie sie mir und dir", "tokens": ["Und", "sie\u00b7he", ",", "wie", "sie", "mir", "und", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "PWAV", "PPER", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich ausgeschm\u00fccket haben.", "tokens": ["Sich", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4ume stehen voller Laub,", "tokens": ["Die", "B\u00e4u\u00b7me", "ste\u00b7hen", "vol\u00b7ler", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Erdreich decket seinen Staub", "tokens": ["Das", "Er\u00b7dreich", "de\u00b7cket", "sei\u00b7nen", "Staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit einem gr\u00fcnen Kleide.", "tokens": ["Mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Narcissen und die Tulipan,", "tokens": ["Nar\u00b7cis\u00b7sen", "und", "die", "Tu\u00b7li\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ziehen sich viel sch\u00f6ner an,", "tokens": ["Die", "zie\u00b7hen", "sich", "viel", "sch\u00f6\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Salamonis Seide.", "tokens": ["Als", "Sa\u00b7la\u00b7mo\u00b7nis", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Lerche schwingt sich in die Luft,", "tokens": ["Die", "Ler\u00b7che", "schwingt", "sich", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das T\u00e4ubchen fleucht aus seiner Kluft,", "tokens": ["Das", "T\u00e4ub\u00b7chen", "fleucht", "aus", "sei\u00b7ner", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht sich in die W\u00e4lder.", "tokens": ["Und", "macht", "sich", "in", "die", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hochgelobte Nachtigall", "tokens": ["Die", "hoch\u00b7ge\u00b7lob\u00b7te", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erg\u00f6tzt und f\u00fcllt mit ihrem Schall", "tokens": ["Er\u00b7g\u00f6tzt", "und", "f\u00fcllt", "mit", "ih\u00b7rem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Berg, H\u00fcgel, Thal und Felder.", "tokens": ["Berg", ",", "H\u00fc\u00b7gel", ",", "Thal", "und", "Fel\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Glucke f\u00fchrt ihr K\u00fcchlein aus,", "tokens": ["Die", "Glu\u00b7cke", "f\u00fchrt", "ihr", "K\u00fcch\u00b7lein", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Storch baut und bewohnt sein Haus,", "tokens": ["Der", "Storch", "baut", "und", "be\u00b7wohnt", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schw\u00e4lblein spei\u00dft die Jungen;", "tokens": ["Das", "Schw\u00e4lb\u00b7lein", "spei\u00dft", "die", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schnelle Hirsch, das leichte Reh", "tokens": ["Der", "schnel\u00b7le", "Hirsch", ",", "das", "leich\u00b7te", "Reh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist froh, und kommt aus seiner H\u00f6h,", "tokens": ["Ist", "froh", ",", "und", "kommt", "aus", "sei\u00b7ner", "H\u00f6h", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins tiefe Gras gesprungen.", "tokens": ["Ins", "tie\u00b7fe", "Gras", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die B\u00e4chlein rauschen in dem Sand,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "rau\u00b7schen", "in", "dem", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mahlen sich in ihrem Rand", "tokens": ["Und", "mah\u00b7len", "sich", "in", "ih\u00b7rem", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schattenreichen Myrthen;", "tokens": ["Mit", "schat\u00b7ten\u00b7rei\u00b7chen", "Myr\u00b7then", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wiesen liegen hart dabei,", "tokens": ["Die", "Wie\u00b7sen", "lie\u00b7gen", "hart", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und klingen ganz von Lustgeschrey", "tokens": ["Und", "klin\u00b7gen", "ganz", "von", "Lust\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schaaf und ihrer Hirten.", "tokens": ["Der", "Schaaf", "und", "ih\u00b7rer", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die unverdro\u00dfne Bienenschaar", "tokens": ["Die", "un\u00b7ver\u00b7dro\u00df\u00b7ne", "Bie\u00b7nen\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fleucht hin und her, sucht hier und dar", "tokens": ["Fleucht", "hin", "und", "her", ",", "sucht", "hier", "und", "dar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr edle Honigspeise;", "tokens": ["Ihr", "ed\u00b7le", "Ho\u00b7nigs\u00b7pei\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des s\u00fc\u00dfen Weinstocks starker Saft", "tokens": ["Des", "s\u00fc\u00b7\u00dfen", "Wein\u00b7stocks", "star\u00b7ker", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bringt t\u00e4glich neue St\u00e4rk und Kraft", "tokens": ["Bringt", "t\u00e4g\u00b7lich", "neu\u00b7e", "St\u00e4rk", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinem schwachen Reise.", "tokens": ["In", "sei\u00b7nem", "schwa\u00b7chen", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich selber kann und mag nicht ruhn,", "tokens": ["Ich", "sel\u00b7ber", "kann", "und", "mag", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des grossen Gottes grosses Thun", "tokens": ["Des", "gros\u00b7sen", "Got\u00b7tes", "gros\u00b7ses", "Thun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweckt mir alle Sinnen;", "tokens": ["Er\u00b7weckt", "mir", "al\u00b7le", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich singe mit, wenn alles singt,", "tokens": ["Ich", "sin\u00b7ge", "mit", ",", "wenn", "al\u00b7les", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lasse, was dem H\u00f6chsten klingt,", "tokens": ["Und", "las\u00b7se", ",", "was", "dem", "H\u00f6chs\u00b7ten", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus meinem Herzen rinnen.", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, denk ich, bist du hier so sch\u00f6n,", "tokens": ["Ach", ",", "denk", "ich", ",", "bist", "du", "hier", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4ssest uns so lieblich gehn,", "tokens": ["Und", "l\u00e4s\u00b7sest", "uns", "so", "lieb\u00b7lich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dieser armen Erden;", "tokens": ["Auf", "die\u00b7ser", "ar\u00b7men", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was will doch wohl nach dieser Welt", "tokens": ["Was", "will", "doch", "wohl", "nach", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort in dem festen Himmelszelt", "tokens": ["Dort", "in", "dem", "fes\u00b7ten", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und g\u00fcldnem Schlosse werden.", "tokens": ["Und", "g\u00fcld\u00b7nem", "Schlos\u00b7se", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O w\u00e4r ich da! o st\u00fcnd ich schon,", "tokens": ["O", "w\u00e4r", "ich", "da", "!", "o", "st\u00fcnd", "ich", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$.", "FM", "KON", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach s\u00fc\u00dfer Gott vor deinem Thron,", "tokens": ["Ach", "s\u00fc\u00b7\u00dfer", "Gott", "vor", "dei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00fcge meine Palmen;", "tokens": ["Und", "tr\u00fc\u00b7ge", "mei\u00b7ne", "Pal\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wollt ich nach der Engel Weis", "tokens": ["So", "wollt", "ich", "nach", "der", "En\u00b7gel", "Weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erh\u00f6hen deines Namens Preis", "tokens": ["Er\u00b7h\u00f6\u00b7hen", "dei\u00b7nes", "Na\u00b7mens", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tausend sch\u00f6nen Psalmen.", "tokens": ["Mit", "tau\u00b7send", "sch\u00f6\u00b7nen", "Psal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}