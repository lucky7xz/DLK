{"textgrid.poem.44494": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ru\u00dfland", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, du Land der eisgen Steppen,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "du", "Land", "der", "eis\u00b7gen", "Step\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deinen V\u00f6lkern rauh und starr und roh,", "tokens": ["Mit", "dei\u00b7nen", "V\u00f6l\u00b7kern", "rauh", "und", "starr", "und", "roh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sie die Unschuld zum Polarkreis schleppen,", "tokens": ["Wo", "sie", "die", "Un\u00b7schuld", "zum", "Po\u00b7lar\u00b7kreis", "schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo noch Gewalt des \u00dcberma\u00dfes froh.", "tokens": ["Wo", "noch", "Ge\u00b7walt", "des", "\u00dc\u00b7berm\u00b7a\u00b7\u00dfes", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl wei\u00df ich, was du drohst: du drohst mit Banden,", "tokens": ["Wohl", "wei\u00df", "ich", ",", "was", "du", "drohst", ":", "du", "drohst", "mit", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl wei\u00df ich, was du willst: du willst die Welt,", "tokens": ["Wohl", "wei\u00df", "ich", ",", "was", "du", "willst", ":", "du", "willst", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch Heil mit dir und deinen Landen.", "tokens": ["Und", "den\u00b7noch", "Heil", "mit", "dir", "und", "dei\u00b7nen", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Greif zu! schlag los! Zertr\u00fcmmre, was dich h\u00e4lt.", "tokens": ["Greif", "zu", "!", "schlag", "los", "!", "Zer\u00b7tr\u00fcmm\u00b7re", ",", "was", "dich", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "NE", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort, wo des Bospors \u00e4therblaue Wogen", "tokens": ["Dort", ",", "wo", "des", "Bos\u00b7pors", "\u00e4t\u00b7her\u00b7blau\u00b7e", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Durch reiche Ernten fluten ohne Saat,", "tokens": ["Durch", "rei\u00b7che", "Ern\u00b7ten", "flu\u00b7ten", "oh\u00b7ne", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sich des \u00dcberflusses Horn gebogen", "tokens": ["Wo", "sich", "des", "\u00dc\u00b7berf\u00b7lus\u00b7ses", "Horn", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinlegt um Konstantinus' goldne Stadt,", "tokens": ["Hin\u00b7legt", "um", "Kon\u00b7stan\u00b7ti\u00b7nus'", "gold\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Dort setz dich hin in deiner Tierheit F\u00fclle,", "tokens": ["Dort", "setz", "dich", "hin", "in", "dei\u00b7ner", "Tier\u00b7heit", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frohlockend ob der sp\u00e4tgelungnen Tat,", "tokens": ["Froh\u00b7lo\u00b7ckend", "ob", "der", "sp\u00e4t\u00b7ge\u00b7lung\u00b7nen", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fletsche deiner Z\u00e4hne Reihn und br\u00fclle,", "tokens": ["Und", "flet\u00b7sche", "dei\u00b7ner", "Z\u00e4h\u00b7ne", "Reihn", "und", "br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sooft ein St\u00f6rer sich dem Raube naht.", "tokens": ["Sooft", "ein", "St\u00f6\u00b7rer", "sich", "dem", "Rau\u00b7be", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht, da\u00df ich Gutes dir und Frohes g\u00f6nnte,", "tokens": ["Nicht", ",", "da\u00df", "ich", "Gu\u00b7tes", "dir", "und", "Fro\u00b7hes", "g\u00f6nn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "NN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hasse deiner R\u00e4uberh\u00e4nde Brauch", "tokens": ["Ich", "has\u00b7se", "dei\u00b7ner", "R\u00e4u\u00b7ber\u00b7h\u00e4n\u00b7de", "Brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00f6chte dich vernichten, wenn ich k\u00f6nnte,", "tokens": ["Und", "m\u00f6ch\u00b7te", "dich", "ver\u00b7nich\u00b7ten", ",", "wenn", "ich", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6nnt deine Gegner ich vernichten auch.", "tokens": ["K\u00f6nnt", "dei\u00b7ne", "Geg\u00b7ner", "ich", "ver\u00b7nich\u00b7ten", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn sie, dir Widersacher und Genossen,", "tokens": ["Denn", "sie", ",", "dir", "Wi\u00b7der\u00b7sa\u00b7cher", "und", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gleiche Lust mit gleichem Kitzel neckt,", "tokens": ["Die", "glei\u00b7che", "Lust", "mit", "glei\u00b7chem", "Kit\u00b7zel", "neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Kleinen Qu\u00e4ler, wie du Scherg im Gro\u00dfen,", "tokens": ["Im", "Klei\u00b7nen", "Qu\u00e4\u00b7ler", ",", "wie", "du", "Scherg", "im", "Gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie brauchen einen M\u00e4chtgen, der sie schreckt.", "tokens": ["Sie", "brau\u00b7chen", "ei\u00b7nen", "M\u00e4cht\u00b7gen", ",", "der", "sie", "schreckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Als noch der Mann, dem nun die gr\u00fcnen Gr\u00e4ser", "tokens": ["Als", "noch", "der", "Mann", ",", "dem", "nun", "die", "gr\u00fc\u00b7nen", "Gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Helena die Schlummerst\u00e4tte baun,", "tokens": ["Auf", "He\u00b7le\u00b7na", "die", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als er noch stand, der Allmacht Zornverweser,", "tokens": ["Als", "er", "noch", "stand", ",", "der", "All\u00b7macht", "Zorn\u00b7ver\u00b7we\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da waren sie wie L\u00e4mmer anzuschaun.", "tokens": ["Da", "wa\u00b7ren", "sie", "wie", "L\u00e4m\u00b7mer", "an\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da krochen sie um des Gewaltgen F\u00fc\u00dfe,", "tokens": ["Da", "kro\u00b7chen", "sie", "um", "des", "Ge\u00b7walt\u00b7gen", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da lechzten sie nach Freiheit und nach Licht,", "tokens": ["Da", "lechz\u00b7ten", "sie", "nach", "Frei\u00b7heit", "und", "nach", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da boten sie der Menschheit Freundesgr\u00fc\u00dfe,", "tokens": ["Da", "bo\u00b7ten", "sie", "der", "Menschheit", "Freun\u00b7des\u00b7gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Nicht nur das Recht, auch g\u00f6ttlich schien die Pflicht.", "tokens": ["Nicht", "nur", "das", "Recht", ",", "auch", "g\u00f6tt\u00b7lich", "schien", "die", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch als erf\u00fcllt das Ma\u00df von Gottes Zorne", "tokens": ["Doch", "als", "er\u00b7f\u00fcllt", "das", "Ma\u00df", "von", "Got\u00b7tes", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVPP", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Titan, nicht ihnen, Gott verfiel,", "tokens": ["Und", "der", "Ti\u00b7tan", ",", "nicht", "ih\u00b7nen", ",", "Gott", "ver\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PTKNEG", "PPER", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verga\u00dfen sie das heilig laut Beschworne", "tokens": ["Ver\u00b7ga\u00b7\u00dfen", "sie", "das", "hei\u00b7lig", "laut", "Be\u00b7schwor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und setzten gleiche Frevel sich zum Ziel.", "tokens": ["Und", "setz\u00b7ten", "glei\u00b7che", "Fre\u00b7vel", "sich", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Not vorbei, war auch vorbei das Beten,", "tokens": ["Die", "Not", "vor\u00b7bei", ",", "war", "auch", "vor\u00b7bei", "das", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Regenmantel wich der warmen Zeit,", "tokens": ["Der", "Re\u00b7gen\u00b7man\u00b7tel", "wich", "der", "war\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Zwerge lockts, des Riesen Spur zu treten,", "tokens": ["Die", "Zwer\u00b7ge", "lockts", ",", "des", "Rie\u00b7sen", "Spur", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War klein die Kraft, war das Gewissen weit.", "tokens": ["War", "klein", "die", "Kraft", ",", "war", "das", "Ge\u00b7wis\u00b7sen", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und, Pfennige der umgem\u00fcnzten Krone", "tokens": ["Und", ",", "Pfen\u00b7ni\u00b7ge", "der", "um\u00b7ge\u00b7m\u00fcnz\u00b7ten", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bezahlten sie in gleichem Wuchergeld,", "tokens": ["Be\u00b7zahl\u00b7ten", "sie", "in", "glei\u00b7chem", "Wu\u00b7cher\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Zutraun ward des Treubruchs Spott zum Lohne,", "tokens": ["Dem", "Zu\u00b7traun", "ward", "des", "Treu\u00b7bruchs", "Spott", "zum", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal dunkelts in der lichten Welt", "tokens": ["Noch", "ein\u00b7mal", "dun\u00b7kelts", "in", "der", "lich\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und nachten wirds, wenn nicht der Schreck vom neuen", "tokens": ["Und", "nach\u00b7ten", "wirds", ",", "wenn", "nicht", "der", "Schreck", "vom", "neu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VAFIN", "$,", "KOUS", "PTKNEG", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Drohenden sie zu Bedrohten schafft.", "tokens": ["Aus", "Dro\u00b7hen\u00b7den", "sie", "zu", "Be\u00b7droh\u00b7ten", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohlan d