{"textgrid.poem.63817": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In der Barke", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei fr\u00f6hliche Leutchen,", "tokens": ["Zwei", "fr\u00f6h\u00b7li\u00b7che", "Leut\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht lang noch verm\u00e4hlt.", "tokens": ["Nicht", "lang", "noch", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er hat sich zum Br\u00e4utchen", "tokens": ["Er", "hat", "sich", "zum", "Br\u00e4ut\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Sch\u00f6nste gew\u00e4hlt.", "tokens": ["Die", "Sch\u00f6ns\u00b7te", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ihr sonniger Nachen", "tokens": ["Ihr", "son\u00b7ni\u00b7ger", "Na\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Wellen durchfliegt.", "tokens": ["Die", "Wel\u00b7len", "durch\u00b7fliegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Augen ihr lachen,", "tokens": ["Die", "Au\u00b7gen", "ihr", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dicht an ihn geschmiegt.", "tokens": ["Dicht", "an", "ihn", "ge\u00b7schmiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Mit schwellendem Segel", "tokens": ["Mit", "schwel\u00b7len\u00b7dem", "Se\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den gleitenden Kahn", "tokens": ["Den", "glei\u00b7ten\u00b7den", "Kahn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Umflattern die V\u00f6gel", "tokens": ["Um\u00b7flat\u00b7tern", "die", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf leuchtender Bahn.", "tokens": ["Auf", "leuch\u00b7ten\u00b7der", "Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Sie wirft, um zu locken", "tokens": ["Sie", "wirft", ",", "um", "zu", "lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die gierige Brut,", "tokens": ["Die", "gie\u00b7ri\u00b7ge", "Brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wei\u00dfschimmernde Brocken", "tokens": ["Wei\u00df\u00b7schim\u00b7mern\u00b7de", "Bro\u00b7cken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hinaus in die Flut.", "tokens": ["Hin\u00b7aus", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Das Herzchen, von reiner", "tokens": ["Das", "Herz\u00b7chen", ",", "von", "rei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gl\u00fcckseligkeit voll,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "voll", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Verlangt, da\u00df nicht einer", "tokens": ["Ver\u00b7langt", ",", "da\u00df", "nicht", "ei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PTKNEG", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Leer ausgehn soll.", "tokens": ["Leer", "aus\u00b7gehn", "soll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und wie sie sich zanken", "tokens": ["Und", "wie", "sie", "sich", "zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und tauchen zum Grund,", "tokens": ["Und", "tau\u00b7chen", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von s\u00fc\u00dfen Gedanken", "tokens": ["Von", "s\u00fc\u00b7\u00dfen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verkl\u00e4rt sich ihr Mund.", "tokens": ["Ver\u00b7kl\u00e4rt", "sich", "ihr", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Sie fl\u00fcstert so munter", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "so", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Liebsten empor.", "tokens": ["Zum", "Liebs\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der beugt sich hinunter", "tokens": ["Der", "beugt", "sich", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und k\u00fc\u00dft ihr das Ohr.", "tokens": ["Und", "k\u00fc\u00dft", "ihr", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Der Alte, der Ferge", "tokens": ["Der", "Al\u00b7te", ",", "der", "Fer\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit braunem Gesicht,", "tokens": ["Mit", "brau\u00b7nem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Blickt still in die Berge,", "tokens": ["Blickt", "still", "in", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als merkt' er es nicht.", "tokens": ["Als", "merkt'", "er", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Er denkt wohl: O blieben", "tokens": ["Er", "denkt", "wohl", ":", "O", "blie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "NE", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie lang in Sal\u00f2!", "tokens": ["Sie", "lang", "in", "Sa\u00b7l\u00f2", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie macht junges Lieben", "tokens": ["Wie", "macht", "jun\u00b7ges", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "So k\u00f6niglich froh.", "tokens": ["So", "k\u00f6\u00b7nig\u00b7lich", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Vor Kosen und K\u00fcssen", "tokens": ["Vor", "Ko\u00b7sen", "und", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Er w\u00fcnscht nicht einmal", "tokens": ["Er", "w\u00fcnscht", "nicht", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Die Taxe zu wissen,", "tokens": ["Die", "Ta\u00b7xe", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der junge Gemahl.", "tokens": ["Der", "jun\u00b7ge", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Mir kommt es gelegen;", "tokens": ["Mir", "kommt", "es", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Denn landen wir an,", "tokens": ["Denn", "lan\u00b7den", "wir", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie K\u00f6nige pflegen,", "tokens": ["Wie", "K\u00f6\u00b7ni\u00b7ge", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bezahlt er mich dann.", "tokens": ["Be\u00b7zahlt", "er", "mich", "dann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}