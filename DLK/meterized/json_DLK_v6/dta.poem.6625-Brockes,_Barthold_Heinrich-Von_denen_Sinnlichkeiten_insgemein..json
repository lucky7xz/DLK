{"dta.poem.6625": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da man der Pflantzen Bau nicht gnug bewundern kan;", "tokens": ["Da", "man", "der", "Pflant\u00b7zen", "Bau", "nicht", "gnug", "be\u00b7wun\u00b7dern", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was f\u00fcr ein GOETTLJCHS Werck trifft man nicht an,", "tokens": ["Was", "f\u00fcr", "ein", "GoETTLJCHS", "Werck", "trifft", "man", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "In solcher Werck\u2019 Erschaffung, welche leben", "tokens": ["In", "sol\u00b7cher", "Werck", "Er\u00b7schaf\u00b7fung", ",", "wel\u00b7che", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und welche wir, sich r\u00fchren, athmen, wachen,", "tokens": ["Und", "wel\u00b7che", "wir", ",", "sich", "r\u00fch\u00b7ren", ",", "ath\u00b7men", ",", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "$,", "PRF", "VVINF", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch schlaffen, in der Fluth, auch in den L\u00fcfften schweben,", "tokens": ["Auch", "schlaf\u00b7fen", ",", "in", "der", "Fluth", ",", "auch", "in", "den", "L\u00fcff\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auf der Erden rennen sehn,", "tokens": ["Und", "auf", "der", "Er\u00b7den", "ren\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und welche man durch so viel Wunder-Sachen,", "tokens": ["Und", "wel\u00b7che", "man", "durch", "so", "viel", "Wun\u00b7der\u00b7Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PIS", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die eintzig aus dem Trieb\u2019 entstehn,", "tokens": ["Die", "eint\u00b7zig", "aus", "dem", "Trieb'", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So gar der menschlichen Vernnunft will \u00e4hnlich machen.", "tokens": ["So", "gar", "der", "menschli\u00b7chen", "Vern\u00b7nunft", "will", "\u00e4hn\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}