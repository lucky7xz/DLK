{"textgrid.poem.28415": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du stellst dich fremd, o Herr, als k\u00fcntestu nichts sagen", "tokens": ["Du", "stellst", "dich", "fremd", ",", "o", "Herr", ",", "als", "k\u00fcn\u00b7tes\u00b7tu", "nichts", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,", "KOUS", "NE", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem, was sich begibt bey uns und unsren Tagen;", "tokens": ["Von", "dem", ",", "was", "sich", "be\u00b7gibt", "bey", "uns", "und", "un\u00b7sren", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PRF", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum \u00f6ffn uns Schrifft und Hertz, auff da\u00df wir seyn bereit,", "tokens": ["Drum", "\u00f6ffn", "uns", "Schrifft", "und", "Hertz", ",", "auff", "da\u00df", "wir", "seyn", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "APPR", "KOUS", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Leiden einzugehn in deine Herrligkeit.", "tokens": ["Durch", "Lei\u00b7den", "ein\u00b7zu\u00b7gehn", "in", "dei\u00b7ne", "Herr\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}