{"dta.poem.16159": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "213.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weisen lehren dich, so schwierig als Entsagung", "tokens": ["Die", "Wei\u00b7sen", "leh\u00b7ren", "dich", ",", "so", "schwie\u00b7rig", "als", "Ent\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des W\u00fcnschenswerthen sei des Widrigen Ertragung.", "tokens": ["Des", "W\u00fcn\u00b7schens\u00b7wert\u00b7hen", "sei", "des", "Wid\u00b7ri\u00b7gen", "Er\u00b7tra\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber darf es dir wol im Vertrauen sagen:", "tokens": ["Ich", "a\u00b7ber", "darf", "es", "dir", "wol", "im", "Ver\u00b7trau\u00b7en", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem Sinn hab' ich nie entsagt und nie ertragen.", "tokens": ["In", "dem", "Sinn", "hab'", "ich", "nie", "ent\u00b7sagt", "und", "nie", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "KON", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Was ich gegeben hin, was ich auf mich genommen,", "tokens": ["Was", "ich", "ge\u00b7ge\u00b7ben", "hin", ",", "was", "ich", "auf", "mich", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "PTKVZ", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kann nicht sagen, schwer sei es mir angekommen.", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ",", "schwer", "sei", "es", "mir", "an\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}