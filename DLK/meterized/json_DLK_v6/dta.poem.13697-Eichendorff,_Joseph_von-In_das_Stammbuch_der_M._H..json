{"dta.poem.13697": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch der M. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ist hell der Himmel, heiter alle Wellen", "tokens": ["Ist", "hell", "der", "Him\u00b7mel", ",", "hei\u00b7ter", "al\u00b7le", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betritt der Schiffer wieder seine Wogen,", "tokens": ["Be\u00b7tritt", "der", "Schif\u00b7fer", "wie\u00b7der", "sei\u00b7ne", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor\u00fcber Wald und Berge schnell geflogen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "Wald", "und", "Ber\u00b7ge", "schnell", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er mu\u00df, wohin die vollen Segel schwellen.", "tokens": ["Er", "mu\u00df", ",", "wo\u00b7hin", "die", "vol\u00b7len", "Se\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In Duft versinken bald all' liebe Stellen,", "tokens": ["In", "Duft", "ver\u00b7sin\u00b7ken", "bald", "all'", "lie\u00b7be", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Cypressen nur noch ragen aus den Wogen,", "tokens": ["Cyp\u00b7res\u00b7sen", "nur", "noch", "ra\u00b7gen", "aus", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Her\u00fcber kommt manch s\u00fc\u00dfer Laut geflogen,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "kommt", "manch", "s\u00fc\u00b7\u00dfer", "Laut", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es trinkt das Meer der Klagen sanfte Quellen.", "tokens": ["Es", "trinkt", "das", "Meer", "der", "Kla\u00b7gen", "sanf\u00b7te", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nichts weilt. &#8212; Doch zaubern Treue und Verlangen,", "tokens": ["Nichts", "weilt", ".", "&#8212;", "Doch", "zau\u00b7bern", "Treu\u00b7e", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "$(", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da mu\u00df sich bl\u00fch'nder alte Zeit erneuern,", "tokens": ["Da", "mu\u00df", "sich", "bl\u00fch'n\u00b7der", "al\u00b7te", "Zeit", "er\u00b7neu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Oeffnet die Ferne drauf die Wunderlichtung,", "tokens": ["Oeff\u00b7net", "die", "Fer\u00b7ne", "drauf", "die", "Wun\u00b7der\u00b7lich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Ruht Dein Bild drin bekr\u00e4nzt in heil'ger Dichtung. &#8212;", "tokens": ["Ruht", "Dein", "Bild", "drin", "be\u00b7kr\u00e4nzt", "in", "heil'\u00b7ger", "Dich\u00b7tung", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Fern la\u00df' den Freund nach Ost und West nur steuern,", "tokens": ["Fern", "la\u00df'", "den", "Freund", "nach", "Ost", "und", "West", "nur", "steu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "ART", "NN", "APPR", "NE", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Frei scheint er wohl &#8212; Du h\u00e4ltst ihn doch gefangen!", "tokens": ["Frei", "scheint", "er", "wohl", "&#8212;", "Du", "h\u00e4ltst", "ihn", "doch", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}