{"textgrid.poem.31638": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "10. Erkennet doch da\u00df der Herr seine Heiligen wunderlich f\u00fchrt", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Meer und Jordans-Fluten ungenetzt hindurch geleit /", "tokens": ["Durch", "die", "Meer", "und", "Jordans\u00b7Flu\u00b7ten", "un\u00b7ge\u00b7netzt", "hin\u00b7durch", "ge\u00b7leit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ADJD", "PAV", "NN", "$("], "meter": "+-+-++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "und auf Feuer-Ro\u00df- und Wagen bringen in die Ewigkeit /", "tokens": ["und", "auf", "Feu\u00b7er\u00b7Ro\u00df", "und", "Wa\u00b7gen", "brin\u00b7gen", "in", "die", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "TRUNC", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "ist zwar wunderlich gef\u00fchrt; doch das gr\u00f6ste Wunder-wei\u00dfen", "tokens": ["ist", "zwar", "wun\u00b7der\u00b7lich", "ge\u00b7f\u00fchrt", ";", "doch", "das", "gr\u00f6s\u00b7te", "Wun\u00b7der\u00b7wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "ist / im Cre\u00fctz ein Lied gemacht / wie man Gott in Freud w\u00f6ll prei\u00dfen /", "tokens": ["ist", "/", "im", "Cre\u00fctz", "ein", "Lied", "ge\u00b7macht", "/", "wie", "man", "Gott", "in", "Freud", "w\u00f6ll", "prei\u00b7\u00dfen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "APPRART", "NN", "ART", "NN", "VVPP", "$(", "PWAV", "PIS", "NN", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dieses kan der Geist der Freuden: wann uns seine H\u00fclff einfliest /", "tokens": ["Die\u00b7ses", "kan", "der", "Geist", "der", "Freu\u00b7den", ":", "wann", "uns", "sei\u00b7ne", "H\u00fclff", "ein\u00b7fliest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ART", "NN", "$.", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "macht sie / da\u00df man Sonn' im Regen / H\u00f6nig in der Gall / geniest.", "tokens": ["macht", "sie", "/", "da\u00df", "man", "Sonn'", "im", "Re\u00b7gen", "/", "H\u00f6\u00b7nig", "in", "der", "Gall", "/", "ge\u00b7niest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PIS", "NN", "APPRART", "NN", "$(", "NN", "APPR", "ART", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}