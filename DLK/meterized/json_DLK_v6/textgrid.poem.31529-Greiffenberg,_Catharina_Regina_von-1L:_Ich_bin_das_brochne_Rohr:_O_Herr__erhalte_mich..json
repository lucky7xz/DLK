{"textgrid.poem.31529": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin das brochne Rohr: O Herr / erhalte mich.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin das brochne Rohr: O Herr / erhalte mich.", "tokens": ["Ich", "bin", "das", "broch\u00b7ne", "Rohr", ":", "O", "Herr", "/", "er\u00b7hal\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "NE", "NN", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerbrich mich nur nicht gar / erbarme dich der Schwachen.", "tokens": ["Zer\u00b7brich", "mich", "nur", "nicht", "gar", "/", "er\u00b7bar\u00b7me", "dich", "der", "Schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein leichter ansto\u00df-Wind kan mich bald wanken machen.", "tokens": ["Ein", "leich\u00b7ter", "an\u00b7sto\u00df\u00b7Wind", "kan", "mich", "bald", "wan\u00b7ken", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In mir ist keine Krafft: Ach binde mich an dich!", "tokens": ["In", "mir", "ist", "kei\u00b7ne", "Krafft", ":", "Ach", "bin\u00b7de", "mich", "an", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "$.", "ITJ", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "es lasse / was dein Geist einbl\u00e4st / vernemen sich!", "tokens": ["es", "las\u00b7se", "/", "was", "dein", "Geist", "ein\u00b7bl\u00e4st", "/", "ver\u00b7ne\u00b7men", "sich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "es schallen aus dem Mund / die eingegebnen Sachen!", "tokens": ["es", "schal\u00b7len", "aus", "dem", "Mund", "/", "die", "ein\u00b7ge\u00b7geb\u00b7nen", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ach halt' es an dein Herz / da dir die Knecht' einstachen:", "tokens": ["ach", "halt'", "es", "an", "dein", "Herz", "/", "da", "dir", "die", "Knecht'", "ein\u00b7sta\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so wird dein Blut / seim Mark / recht Herzergetzbarlich.", "tokens": ["so", "wird", "dein", "Blut", "/", "seim", "Mark", "/", "recht", "Her\u00b7zer\u00b7getz\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$(", "APPRART", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df mich die R\u00f6hren seyn / nach deinem hohen Willen /", "tokens": ["La\u00df", "mich", "die", "R\u00f6h\u00b7ren", "seyn", "/", "nach", "dei\u00b7nem", "ho\u00b7hen", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VAINF", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "durch die dein' Ehr und Lob das Land pflegt anzuf\u00fcllen /", "tokens": ["durch", "die", "dein'", "Ehr", "und", "Lob", "das", "Land", "pflegt", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und wider Prei\u00df und Dank durch sie gen Himmel steigt:", "tokens": ["und", "wi\u00b7der", "Prei\u00df", "und", "Dank", "durch", "sie", "gen", "Him\u00b7mel", "steigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der Segen werd' herab / der Ruhm hinauf / geneigt.", "tokens": ["der", "Se\u00b7gen", "werd'", "her\u00b7ab", "/", "der", "Ruhm", "hin\u00b7auf", "/", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "PTKVZ", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nur w\u00fcrdig mich / mein Gott / dein Gnad in mich zu giessen.", "tokens": ["Nur", "w\u00fcr\u00b7dig", "mich", "/", "mein", "Gott", "/", "dein", "Gnad", "in", "mich", "zu", "gies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ob ich schon solche Ehr mu\u00df Leben-lassend b\u00fcssen.", "tokens": ["Ob", "ich", "schon", "sol\u00b7che", "Ehr", "mu\u00df", "Le\u00b7ben\u00b7las\u00b7send", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}