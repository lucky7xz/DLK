{"dta.poem.10431": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich steh' auf des Berges Spitze,", "tokens": ["Ich", "steh'", "auf", "des", "Ber\u00b7ges", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und werde sentimental.", "tokens": ["Und", "wer\u00b7de", "sen\u00b7ti\u00b7men\u00b7tal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201ewenn ich ein V\u00f6glein w\u00e4re!\u201c", "tokens": ["\u201e", "wenn", "ich", "ein", "V\u00f6\u00b7glein", "w\u00e4\u00b7re", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seufz' ich viel tausendmal.", "tokens": ["Seuf\u00b7z'", "ich", "viel", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich eine Schwalbe w\u00e4re,", "tokens": ["Wenn", "ich", "ei\u00b7ne", "Schwal\u00b7be", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So fl\u00f6g' ich zu dir, mein Kind,", "tokens": ["So", "fl\u00f6g'", "ich", "zu", "dir", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und baute mir mein Nestchen", "tokens": ["Und", "bau\u00b7te", "mir", "mein", "Nest\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo deine Fenster sind.", "tokens": ["Wo", "dei\u00b7ne", "Fens\u00b7ter", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich eine Nachtigall w\u00e4re,", "tokens": ["Wenn", "ich", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So fl\u00f6g' ich zu dir, mein Kind,", "tokens": ["So", "fl\u00f6g'", "ich", "zu", "dir", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und s\u00e4nge dir Nachts meine Lieder", "tokens": ["Und", "s\u00e4n\u00b7ge", "dir", "Nachts", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Herab von der gr\u00fcnen Lind'.", "tokens": ["Her\u00b7ab", "von", "der", "gr\u00fc\u00b7nen", "Lind'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn ich ein Gimpel w\u00e4re,", "tokens": ["Wenn", "ich", "ein", "Gim\u00b7pel", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fl\u00f6g' ich gleich an dein Herz;", "tokens": ["So", "fl\u00f6g'", "ich", "gleich", "an", "dein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du bist ja hold den Gimpeln,", "tokens": ["Du", "bist", "ja", "hold", "den", "Gim\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und heilest Gimpelschmerz.", "tokens": ["Und", "hei\u00b7lest", "Gim\u00b7pel\u00b7schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}