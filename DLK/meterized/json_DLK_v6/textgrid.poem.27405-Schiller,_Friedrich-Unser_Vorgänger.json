{"textgrid.poem.27405": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unser Vorg\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Martial, wenn ihr's nicht wi\u00dft, bewirtete einst so die R\u00f6mer;", "tokens": ["Mar\u00b7ti\u00b7al", ",", "wenn", "ih\u00b7r's", "nicht", "wi\u00dft", ",", "be\u00b7wir\u00b7te\u00b7te", "einst", "so", "die", "R\u00f6\u00b7mer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+----+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Viel mehr geben wir nicht \u2013 aber die Meinung ist gut.", "tokens": ["Viel", "mehr", "ge\u00b7ben", "wir", "nicht", "\u2013", "a\u00b7ber", "die", "Mei\u00b7nung", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "$(", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}}}}