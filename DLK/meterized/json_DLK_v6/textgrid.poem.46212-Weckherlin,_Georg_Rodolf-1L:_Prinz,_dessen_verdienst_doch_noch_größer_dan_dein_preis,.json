{"textgrid.poem.46212": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Prinz, dessen verdienst doch noch gr\u00f6\u00dfer dan dein preis,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinz, dessen verdienst doch noch gr\u00f6\u00dfer dan dein preis,", "tokens": ["Prinz", ",", "des\u00b7sen", "ver\u00b7dienst", "doch", "noch", "gr\u00f6\u00b7\u00dfer", "dan", "dein", "preis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "ADV", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "wiewol dein wahres lob die himmel selbs kaum gr\u00e4nzen,", "tokens": ["wie\u00b7wol", "dein", "wah\u00b7res", "lob", "die", "him\u00b7mel", "selbs", "kaum", "gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "fahr fort, o gro\u00dfer held, und unserm feind beweis,", "tokens": ["fahr", "fort", ",", "o", "gro\u00b7\u00dfer", "held", ",", "und", "un\u00b7serm", "feind", "be\u00b7weis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df die blitz deines schwerts mehr dan des adlers gl\u00e4nzen.", "tokens": ["da\u00df", "die", "blitz", "dei\u00b7nes", "schwerts", "mehr", "dan", "des", "ad\u00b7lers", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jemehr der dolle feind auf alle grobe weis", "tokens": ["Je\u00b7mehr", "der", "dol\u00b7le", "feind", "auf", "al\u00b7le", "gro\u00b7be", "weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "ADJA", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "will seinen sig und pracht durch unsern fall erg\u00e4nzen,", "tokens": ["will", "sei\u00b7nen", "sig", "und", "pracht", "durch", "un\u00b7sern", "fall", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "jemehr lehr du ihn, held, gerecht, stark, from und weis,", "tokens": ["je\u00b7mehr", "lehr", "du", "ihn", ",", "held", ",", "ge\u00b7recht", ",", "stark", ",", "from", "und", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "da\u00df nur dein haupt allein wert seiner lorberkr\u00e4nzen.", "tokens": ["da\u00df", "nur", "dein", "haupt", "al\u00b7lein", "wert", "sei\u00b7ner", "lor\u00b7ber\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jedoch weil unsre forcht, wie dein mut, billich gro\u00df,", "tokens": ["Je\u00b7doch", "weil", "uns\u00b7re", "forcht", ",", "wie", "dein", "mut", ",", "bil\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so h\u00f6r auch unsern rat, dich und uns zu bewahren,", "tokens": ["so", "h\u00f6r", "auch", "un\u00b7sern", "rat", ",", "dich", "und", "uns", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$,", "PPER", "KON", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und f\u00f6rcht mit uns dein herz, vil zu gro\u00df, vil zu blo\u00df;", "tokens": ["und", "f\u00f6rcht", "mit", "uns", "dein", "herz", ",", "vil", "zu", "gro\u00df", ",", "vil", "zu", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,", "ADV", "PTKA", "ADJD", "$,", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Dan wer erkennet nicht, wan du stets mit gefahren", "tokens": ["Dan", "wer", "er\u00b7ken\u00b7net", "nicht", ",", "wan", "du", "stets", "mit", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "APPR", "VVPP"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "erquickest deinen mut, da\u00df du an mut gleichlos", "tokens": ["er\u00b7qui\u00b7ckest", "dei\u00b7nen", "mut", ",", "da\u00df", "du", "an", "mut", "gleich\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "kanst keinen gr\u00f6\u00dfern feind, dan dein herz selbs, erfahren.", "tokens": ["kanst", "kei\u00b7nen", "gr\u00f6\u00b7\u00dfern", "feind", ",", "dan", "dein", "herz", "selbs", ",", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}