{"dta.poem.10826": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Betrachtung der Gr\u00f6sse  \n GOttes und unsre Nichtigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich Sch\u00f6pfer deine Gr\u00f6\u00dfe", "tokens": ["Wenn", "ich", "Sch\u00f6p\u00b7fer", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur in dem Gedanken messe;", "tokens": ["Nur", "in", "dem", "Ge\u00b7dan\u00b7ken", "mes\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So erblindet der Verstand;", "tokens": ["So", "er\u00b7blin\u00b7det", "der", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich kan keinen Maasstab finden,", "tokens": ["Ich", "kan", "kei\u00b7nen", "Maas\u00b7stab", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Tieffen zu ergr\u00fcnden,", "tokens": ["Dei\u00b7ne", "Tief\u00b7fen", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch dadurch wird mir bekandt;", "tokens": ["Doch", "da\u00b7durch", "wird", "mir", "be\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da\u00df du bist ein solcher Geist,", "tokens": ["Da\u00df", "du", "bist", "ein", "sol\u00b7cher", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der da unerme\u00dflich heist.", "tokens": ["Der", "da", "un\u00b7er\u00b7me\u00df\u00b7lich", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich deine Macht erwege", "tokens": ["Wenn", "ich", "dei\u00b7ne", "Macht", "er\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mir vor die Augen lege", "tokens": ["Und", "mir", "vor", "die", "Au\u00b7gen", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Was durch deinen Wink entsteht,", "tokens": ["Was", "durch", "dei\u00b7nen", "Wink", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sich diese Welt-Maschine", "tokens": ["Wie", "sich", "die\u00b7se", "Welt\u00b7Ma\u00b7schi\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die weite Sternen B\u00fchne,", "tokens": ["Und", "die", "wei\u00b7te", "Ster\u00b7nen", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blos durch deinen Willen dreht;", "tokens": ["Blos", "durch", "dei\u00b7nen", "Wil\u00b7len", "dreht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So f\u00e4llt mir dadurch gleich ein:", "tokens": ["So", "f\u00e4llt", "mir", "da\u00b7durch", "gleich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ADV", "PTKVZ", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Denk ich nach wie das entstanden,", "tokens": ["Denk", "ich", "nach", "wie", "das", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "KOKOM", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was im Weltraum ist vorhanden;", "tokens": ["Was", "im", "Welt\u00b7raum", "ist", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So begreif ich dieses nicht,", "tokens": ["So", "be\u00b7greif", "ich", "die\u00b7ses", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dabei kan ich wieder merken,", "tokens": ["Da\u00b7bei", "kan", "ich", "wie\u00b7der", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mir dieses k\u00f6nn best\u00e4rken,", "tokens": ["Da\u00df", "mir", "die\u00b7ses", "k\u00f6nn", "be\u00b7st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Seelen schwaches Licht,", "tokens": ["Wie", "der", "See\u00b7len", "schwa\u00b7ches", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Verstand nicht sehen kan,", "tokens": ["Der", "Ver\u00b7stand", "nicht", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was der Sch\u00f6pfer hat gethan.", "tokens": ["Was", "der", "Sch\u00f6p\u00b7fer", "hat", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich versp\u00fcre in Gedanken,", "tokens": ["Ich", "ver\u00b7sp\u00fc\u00b7re", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gar eng gesezt die Schranken", "tokens": ["Wie", "gar", "eng", "ge\u00b7sezt", "die", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich \u00fcbersteigen kan;", "tokens": ["Die", "ich", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So viel kan ich nur erkennen:", "tokens": ["So", "viel", "kan", "ich", "nur", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist gros, ich nichts zu nennen,", "tokens": ["Du", "bist", "gros", ",", "ich", "nichts", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gib nur, da\u00df ich denke dran,", "tokens": ["Gib", "nur", ",", "da\u00df", "ich", "den\u00b7ke", "dran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du bist ein solcher Geist,", "tokens": ["Da\u00df", "du", "bist", "ein", "sol\u00b7cher", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der uns unermeslich heist.", "tokens": ["Der", "uns", "un\u00b7er\u00b7mes\u00b7lich", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gib da\u00df ich mich stets bestrebe,", "tokens": ["Gib", "da\u00df", "ich", "mich", "stets", "be\u00b7stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich mich zu dir erhebe,", "tokens": ["Wenn", "ich", "mich", "zu", "dir", "er\u00b7he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur auf mich herab zu sehn;", "tokens": ["Nur", "auf", "mich", "her\u00b7ab", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So werd ich gar bald ermessen,", "tokens": ["So", "werd", "ich", "gar", "bald", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das kein Mensch des Sch\u00f6pfers Gr\u00f6ssen,", "tokens": ["Das", "kein", "Mensch", "des", "Sch\u00f6p\u00b7fers", "Gr\u00f6s\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die unendlich, k\u00f6nn verstehn.", "tokens": ["Die", "un\u00b7end\u00b7lich", ",", "k\u00f6nn", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VMFIN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "So erkenn ich: GOtt allein,", "tokens": ["So", "er\u00b7kenn", "ich", ":", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist nur gros; wir alle klein.", "tokens": ["Ist", "nur", "gros", ";", "wir", "al\u00b7le", "klein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "PPER", "PIAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}