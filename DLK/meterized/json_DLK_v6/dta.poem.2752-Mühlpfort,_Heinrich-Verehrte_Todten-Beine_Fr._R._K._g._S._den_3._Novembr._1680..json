{"dta.poem.2752": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verehrte Todten-Beine/  \n  Fr. R. K. g. S. den 3. Novembr.  1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbtster Freund/ der Schnee/ so ihn schon l\u00e4ngst befal-\nlen/", "tokens": ["Be\u00b7tr\u00b7\u00fcbts\u00b7ter", "Freund", "/", "der", "Schnee", "/", "so", "ihn", "schon", "l\u00e4ngst", "be\u00b7fa\u00b7l", "len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ART", "NN", "$(", "ADV", "PPER", "ADV", "ADV", "TRUNC", "VVINF", "$("], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zerschmeltzt von heissem Leyd in eine gr\u00f6\u00dfre Fluth.", "tokens": ["Zer\u00b7schmeltzt", "von", "heis\u00b7sem", "Leyd", "in", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich seh\u2019 im Thr\u00e4nen-Meer/ die Augen wie Corallen/", "tokens": ["Ich", "seh'", "im", "Thr\u00e4\u00b7nen\u00b7Meer", "/", "die", "Au\u00b7gen", "wie", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "ART", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie den Wellen gleich schl\u00e4gt das best\u00fcrmte Blut.", "tokens": ["Und", "wie", "den", "Wel\u00b7len", "gleich", "schl\u00e4gt", "das", "be\u00b7st\u00fcrm\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df ist der letzte Sto\u00df so durch die Seele dringet/", "tokens": ["Di\u00df", "ist", "der", "letz\u00b7te", "Sto\u00df", "so", "durch", "die", "See\u00b7le", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df scheiden greisst ins Hertz und kreischet die Gebein\u2019;", "tokens": ["Di\u00df", "schei\u00b7den", "greisst", "ins", "Hertz", "und", "krei\u00b7schet", "die", "Ge\u00b7bein'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo meine Feder nun nicht sattsam Trost beybringet/", "tokens": ["Wo", "mei\u00b7ne", "Fe\u00b7der", "nun", "nicht", "satt\u00b7sam", "Trost", "bey\u00b7brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So r\u00e4um\u2019 ich seinem Schmertz das Vor-Recht billich ein.", "tokens": ["So", "r\u00e4um'", "ich", "sei\u00b7nem", "Schmertz", "das", "Vor\u00b7Recht", "bil\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df wol was er klagt: Bey so verlebten Tagen/", "tokens": ["Ich", "wei\u00df", "wol", "was", "er", "klagt", ":", "Bey", "so", "ver\u00b7leb\u00b7ten", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$.", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Da jeder Tritt und Schritt sich zu dem Grabe naht/", "tokens": ["Da", "je\u00b7der", "Tritt", "und", "Schritt", "sich", "zu", "dem", "Gra\u00b7be", "naht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die liebste Pflegerin sehn auff der Baare tragen/", "tokens": ["Die", "liebs\u00b7te", "Pfle\u00b7ge\u00b7rin", "sehn", "auff", "der", "Baa\u00b7re", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was erndtet man da ein/ als eine Thr\u00e4nen-Saat?", "tokens": ["Was", "ernd\u00b7tet", "man", "da", "ein", "/", "als", "ei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7Saat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "ART", "$(", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Traurt doch der Erden-Krei\u00df wenn ihm die Sonn\u2019 entsincket/", "tokens": ["Traurt", "doch", "der", "Er\u00b7den\u00b7Krei\u00df", "wenn", "ihm", "die", "Sonn'", "ent\u00b7sin\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Nacht so ohne Stern und Flammen ist betr\u00fcbt;", "tokens": ["Die", "Nacht", "so", "oh\u00b7ne", "Stern", "und", "Flam\u00b7men", "ist", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man schau Auroren an die Thau als Thr\u00e4nen trincket/", "tokens": ["Man", "schau", "Au\u00b7ro\u00b7ren", "an", "die", "Thau", "als", "Thr\u00e4\u00b7nen", "trin\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "NN", "APPR", "ART", "NN", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bi\u00df Ph\u00f6bus g\u00fcldner Mund den ersten Ku\u00df ihr gibt;", "tokens": ["Bi\u00df", "Ph\u00f6\u00b7bus", "g\u00fcld\u00b7ner", "Mund", "den", "ers\u00b7ten", "Ku\u00df", "ihr", "gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und solte nicht ein Mann/ die Sonne seines Hertzen", "tokens": ["Und", "sol\u00b7te", "nicht", "ein", "Mann", "/", "die", "Son\u00b7ne", "sei\u00b7nes", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Leit-Stern seiner Ruh betr\u00fcbt sehn unter gehn?", "tokens": ["Den", "Leit\u00b7S\u00b7tern", "sei\u00b7ner", "Ruh", "be\u00b7tr\u00fcbt", "sehn", "un\u00b7ter", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mu\u00df er nicht einsam da in Myrrhen-bittern Schmertzen", "tokens": ["Mu\u00df", "er", "nicht", "ein\u00b7sam", "da", "in", "Myr\u00b7rhen\u00b7bit\u00b7tern", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und tieffer Finstern\u00fc\u00df verschrenckt elende stehn?", "tokens": ["Und", "tief\u00b7fer", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ver\u00b7schrenckt", "e\u00b7len\u00b7de", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie seufftzt ein Krancker nicht/ wenn er den Artzt mu\u00df missen;", "tokens": ["Wie", "seufftzt", "ein", "Kran\u00b7cker", "nicht", "/", "wenn", "er", "den", "Artzt", "mu\u00df", "mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ach mehr als Artzt und Cur f\u00e4llt jetzt/ mein Freund/ dahin.", "tokens": ["Ach", "mehr", "als", "Artzt", "und", "Cur", "f\u00e4llt", "jetzt", "/", "mein", "Freund", "/", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "ADV", "KOUS", "NN", "KON", "NN", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Grund in seinem Hau\u00df ist g\u00e4ntzlich eingerissen/", "tokens": ["Der", "Grund", "in", "sei\u00b7nem", "Hau\u00df", "ist", "g\u00e4ntz\u00b7lich", "ein\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Ehren-voller Grei\u00df/ wo soll er jetzt hinfliehn?", "tokens": ["Mein", "Eh\u00b7ren\u00b7vol\u00b7ler", "Grei\u00df", "/", "wo", "soll", "er", "jetzt", "hin\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Viel die durch Krieg und Brand das Ungl\u00fcck weggejaget/", "tokens": ["Viel", "die", "durch", "Krieg", "und", "Brand", "das", "Un\u00b7gl\u00fcck", "weg\u00b7ge\u00b7ja\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nahm offt ein frembder Ort in sichre Wohnung ein:", "tokens": ["Nahm", "offt", "ein", "fremb\u00b7der", "Ort", "in", "sich\u00b7re", "Woh\u00b7nung", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er h\u00f6re mit Gedult was jetzt mein Mund f\u00fcrsaget/", "tokens": ["Er", "h\u00f6\u00b7re", "mit", "Ge\u00b7dult", "was", "jetzt", "mein", "Mund", "f\u00fcr\u00b7sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "&q;Die beste Wohnung ist der Liebsten Leichen-Stein.", "tokens": ["&", "q", ";", "Die", "bes\u00b7te", "Woh\u00b7nung", "ist", "der", "Liebs\u00b7ten", "Lei\u00b7chen\u00b7Stein", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$.", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "Es scheint zwar ungereimt lebendig wollen sterben/", "tokens": ["Es", "scheint", "zwar", "un\u00b7ge\u00b7reimt", "le\u00b7ben\u00b7dig", "wol\u00b7len", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df/ wer tr\u00f6sten soll/ nur blo\u00df zum Grabe f\u00fchrt;", "tokens": ["Und", "da\u00df", "/", "wer", "tr\u00f6s\u00b7ten", "soll", "/", "nur", "blo\u00df", "zum", "Gra\u00b7be", "f\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "PWS", "VVINF", "VMFIN", "$(", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Allein ein Todten-Kopff kan Christen nicht entf\u00e4rben/", "tokens": ["Al\u00b7lein", "ein", "Tod\u00b7ten\u00b7Kopff", "kan", "Chris\u00b7ten", "nicht", "ent\u00b7f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Di\u00df ist der letzte Schmuck der alle C\u00f6rper ziert.", "tokens": ["Di\u00df", "ist", "der", "letz\u00b7te", "Schmuck", "der", "al\u00b7le", "C\u00f6r\u00b7per", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Jugend/ weil sie bl\u00fcht/ sucht ihre Zier im Spiegel/", "tokens": ["Die", "Ju\u00b7gend", "/", "weil", "sie", "bl\u00fcht", "/", "sucht", "ih\u00b7re", "Zier", "im", "Spie\u00b7gel", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Knochen geben uns das Sicht-Gla\u00df jener Lust/", "tokens": ["Die", "Kno\u00b7chen", "ge\u00b7ben", "uns", "das", "Sicht\u00b7Gla\u00df", "je\u00b7ner", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und sind der Ewigkeit unwiederrufflich Sigel/", "tokens": ["Und", "sind", "der", "E\u00b7wig\u00b7keit", "un\u00b7wie\u00b7der\u00b7ruff\u00b7lich", "Si\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Solt\u2019 einem Alten nicht di\u00df Kleinod seyn bewust?", "tokens": ["Solt'", "ei\u00b7nem", "Al\u00b7ten", "nicht", "di\u00df", "Klei\u00b7nod", "seyn", "be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "PDS", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und ob manch frecher Mensch die Todten-Kisten hasset/", "tokens": ["Und", "ob", "manch", "fre\u00b7cher", "Mensch", "die", "Tod\u00b7ten\u00b7Kis\u00b7ten", "has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.38": {"text": "Und Leichen noch vielmehr als die Gespenster scheut,", "tokens": ["Und", "Lei\u00b7chen", "noch", "viel\u00b7mehr", "als", "die", "Ge\u00b7spens\u00b7ter", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "So hat wol unverhofft der Tod ihn schon umbfasset/", "tokens": ["So", "hat", "wol", "un\u00b7ver\u00b7hofft", "der", "Tod", "ihn", "schon", "umb\u00b7fas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und in dem ersten Trotz des Lebens abgemeyht.", "tokens": ["Und", "in",