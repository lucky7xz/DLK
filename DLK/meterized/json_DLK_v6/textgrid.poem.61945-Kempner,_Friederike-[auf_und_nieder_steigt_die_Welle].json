{"textgrid.poem.61945": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[auf und nieder steigt die Welle]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf und nieder steigt die Welle,", "tokens": ["Auf", "und", "nie\u00b7der", "steigt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf und nieder steigt die Nacht,", "tokens": ["Auf", "und", "nie\u00b7der", "steigt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Sterne Glanz und Pracht", "tokens": ["Und", "der", "Ster\u00b7ne", "Glanz", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wechseln mit des Tages Helle.", "tokens": ["Wech\u00b7seln", "mit", "des", "Ta\u00b7ges", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ew'ger Wechsel, Nacht und Helle,", "tokens": ["Ew'\u00b7ger", "Wech\u00b7sel", ",", "Nacht", "und", "Hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcne Matten, dunkler Schacht,", "tokens": ["Gr\u00fc\u00b7ne", "Mat\u00b7ten", ",", "dunk\u00b7ler", "Schacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprich, was siehst Du auf der Wacht,", "tokens": ["Sprich", ",", "was", "siehst", "Du", "auf", "der", "Wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigend auf die festen W\u00e4lle?", "tokens": ["Stei\u00b7gend", "auf", "die", "fes\u00b7ten", "W\u00e4l\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Moder, Moder aus den Gr\u00fcften,", "tokens": ["Mo\u00b7der", ",", "Mo\u00b7der", "aus", "den", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumend\u00fcfte in den L\u00fcften,", "tokens": ["Blu\u00b7men\u00b7d\u00fcf\u00b7te", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchen Geist, ach, schwergepr\u00fcften.", "tokens": ["Man\u00b7chen", "Geist", ",", "ach", ",", "schwer\u00b7ge\u00b7pr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ITJ", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Welt, die Alles preist,", "tokens": ["Ei\u00b7ne", "Welt", ",", "die", "Al\u00b7les", "preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da Glanz und Schimmer hei\u00dft,", "tokens": ["Was", "da", "Glanz", "und", "Schim\u00b7mer", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das B\u00f6se vorw\u00e4rts rei\u00dft! \u2013", "tokens": ["Und", "das", "B\u00f6\u00b7se", "vor\u00b7w\u00e4rts", "rei\u00dft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}