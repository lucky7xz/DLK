{"textgrid.poem.43665": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schmetterlings Sterbelied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbleb' wohl, mein Vater Sonnenschein!", "tokens": ["\u00bb", "leb'", "wohl", ",", "mein", "Va\u00b7ter", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du, meine Mutter Bl\u00fcthenduft!", "tokens": ["Du", ",", "mei\u00b7ne", "Mut\u00b7ter", "Bl\u00fc\u00b7then\u00b7duft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Schwestern all' und Br\u00fcderlein", "tokens": ["Ihr", "Schwes\u00b7tern", "all'", "und", "Br\u00fc\u00b7derl\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PIAT", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im s\u00fc\u00dfen Hauch der Himmelsluft!", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Hauch", "der", "Him\u00b7mel\u00b7sluft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schwebte gern mit euch umher", "tokens": ["Ich", "schweb\u00b7te", "gern", "mit", "euch", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Wald und Wiese, Au und Feld;", "tokens": ["In", "Wald", "und", "Wie\u00b7se", ",", "Au", "und", "Feld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie war mein Herz von Sorgen schwer,", "tokens": ["Nie", "war", "mein", "Herz", "von", "Sor\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ungern verlass' ich diese Welt.\u00ab", "tokens": ["Un\u00b7gern", "ver\u00b7lass'", "ich", "die\u00b7se", "Welt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PDAT", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "So sang der m\u00fcde Schmetterling,", "tokens": ["So", "sang", "der", "m\u00fc\u00b7de", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sang er sich sein Sterbelied.", "tokens": ["So", "sang", "er", "sich", "sein", "Ster\u00b7be\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum als er an zu leben fing,", "tokens": ["Kaum", "als", "er", "an", "zu", "le\u00b7ben", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War hin sein Leben und er schied.", "tokens": ["War", "hin", "sein", "Le\u00b7ben", "und", "er", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}