{"dta.poem.14980": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Mit unverf\u00e4lschtem Blut!", "tokens": ["Mit", "un\u00b7ver\u00b7f\u00e4lschtem", "Blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Feierstunde heilig Wehn", "tokens": ["Der", "Fei\u00b7er\u00b7stun\u00b7de", "hei\u00b7lig", "Wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwellt meinen jungen Muth.", "tokens": ["Schwellt", "mei\u00b7nen", "jun\u00b7gen", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es treibt mich rasch zum Liede fort,", "tokens": ["Es", "treibt", "mich", "rasch", "zum", "Lie\u00b7de", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Harfensturm hinaus,", "tokens": ["Zum", "Har\u00b7fen\u00b7sturm", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im Herzen lebt ein k\u00fchnes Wort, \u2014", "tokens": ["Im", "Her\u00b7zen", "lebt", "ein", "k\u00fch\u00b7nes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was gilt's, ich sprech' es aus.", "tokens": ["Was", "gilt's", ",", "ich", "sprech'", "es", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist schlimm, die Welt ist karg,", "tokens": ["Die", "Zeit", "ist", "schlimm", ",", "die", "Welt", "ist", "karg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Besten weggerafft,", "tokens": ["Die", "Bes\u00b7ten", "weg\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Erde wird ein gro\u00dfer Sarg", "tokens": ["Die", "Er\u00b7de", "wird", "ein", "gro\u00b7\u00dfer", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit und der Kraft.", "tokens": ["Der", "Frei\u00b7heit", "und", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch Muth! \u2014 Wenn auch die Tyranney,", "tokens": ["Doch", "Muth", "!", "Wenn", "auch", "die", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "$(", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die deutsche Flur zertrat,", "tokens": ["Die", "deut\u00b7sche", "Flur", "zer\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In vielen Herzen, still und treu,", "tokens": ["In", "vie\u00b7len", "Her\u00b7zen", ",", "still", "und", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Keimt noch des Guten Saat.", "tokens": ["Keimt", "noch", "des", "Gu\u00b7ten", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Versch\u00fcchtert durch den blut'gen Ruhm", "tokens": ["Ver\u00b7sch\u00fcch\u00b7tert", "durch", "den", "blut'\u00b7gen", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch der Schlachten Gl\u00fcck,", "tokens": ["Und", "durch", "der", "Schlach\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flohn zu der Seele Heiligthum,", "tokens": ["Flohn", "zu", "der", "See\u00b7le", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00fcnste scheu zur\u00fcck.", "tokens": ["Die", "K\u00fcns\u00b7te", "scheu", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sind auch die Th\u00e4ler jetzt verwai\u00dft,", "tokens": ["Sind", "auch", "die", "Th\u00e4\u00b7ler", "jetzt", "ver\u00b7wai\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sonst ihr Tempel war,", "tokens": ["Wo", "sonst", "ihr", "Tem\u00b7pel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es bleibt doch jeder reine Geist", "tokens": ["Es", "bleibt", "doch", "je\u00b7der", "rei\u00b7ne", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr ewiger Altar.", "tokens": ["Ihr", "e\u00b7wi\u00b7ger", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Freundestreu und Wahrheit gilt", "tokens": ["Und", "Freun\u00b7de\u00b7streu", "und", "Wahr\u00b7heit", "gilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eine heil'ge Pflicht.", "tokens": ["Noch", "ei\u00b7ne", "heil'\u00b7ge", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh, wie der Giesbach brausend schwillt! \u2014", "tokens": ["Sieh", ",", "wie", "der", "Gies\u00b7bach", "brau\u00b7send", "schwillt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du rufst; mich schreckt er nicht.", "tokens": ["Du", "rufst", ";", "mich", "schreckt", "er", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00e4g es vor mir wolkenweit", "tokens": ["Und", "l\u00e4g", "es", "vor", "mir", "wol\u00b7ken\u00b7weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sternhoch \u00fcber mir,", "tokens": ["Und", "stern\u00b7hoch", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Beim Gott! ich halte meinen Eid!", "tokens": ["Beim", "Gott", "!", "ich", "hal\u00b7te", "mei\u00b7nen", "Eid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schlag ein! ich folge dir!", "tokens": ["Schlag", "ein", "!", "ich", "fol\u00b7ge", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Frauenunschuld, Frauenlieb,", "tokens": ["Und", "Frau\u00b7en\u00b7un\u00b7schuld", ",", "Frau\u00b7en\u00b7lieb", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht noch als h\u00f6chstes Gut,", "tokens": ["Steht", "noch", "als", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo deutscher Ahnen Sitte blieb,", "tokens": ["Wo", "deut\u00b7scher", "Ah\u00b7nen", "Sit\u00b7te", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deutscher J\u00fcnglings-Muth.", "tokens": ["Und", "deut\u00b7scher", "J\u00fcng\u00b7lings\u00b7Muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch trifft den Frevler heil'ger Bann,", "tokens": ["Noch", "trifft", "den", "Frev\u00b7ler", "heil'\u00b7ger", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der diesen Zauber st\u00f6rt;", "tokens": ["Der", "die\u00b7sen", "Zau\u00b7ber", "st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer f\u00fcr sein Lieb' nicht sterben kann,", "tokens": ["Wer", "f\u00fcr", "sein", "Lieb'", "nicht", "ster\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist keines Kusses werth.", "tokens": ["Ist", "kei\u00b7nes", "Kus\u00b7ses", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch du hast noch nicht ausgeflammt", "tokens": ["Auch", "du", "hast", "noch", "nicht", "aus\u00b7ge\u00b7flammt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du heil'ge Religion!", "tokens": ["Du", "heil'\u00b7ge", "Re\u00b7li\u00b7gi\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was von der ew'gen Liebe stammt,", "tokens": ["Was", "von", "der", "ew'\u00b7gen", "Lie\u00b7be", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist zeitlich nicht entflohn.", "tokens": ["Ist", "zeit\u00b7lich", "nicht", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Blut w\u00e4scht die Alt\u00e4re rein,", "tokens": ["Das", "Blut", "w\u00e4scht", "die", "Al\u00b7t\u00e4\u00b7re", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wir entheiligt sehn.", "tokens": ["Die", "wir", "en\u00b7thei\u00b7ligt", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Kreuze schl\u00e4gt man frevelnd ein,", "tokens": ["Die", "Kreu\u00b7ze", "schl\u00e4gt", "man", "fre\u00b7velnd", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch bleibt der Glaube stehn.", "tokens": ["Doch", "bleibt", "der", "Glau\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und noch regt sich mit Adlers Schwung", "tokens": ["Und", "noch", "regt", "sich", "mit", "Ad\u00b7lers", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der vaterl\u00e4nd'sche Geist", "tokens": ["Der", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7sche", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und noch lebt die Begeisterung,", "tokens": ["Und", "noch", "lebt", "die", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die alle Ketten rei\u00dft.", "tokens": ["Die", "al\u00b7le", "Ket\u00b7ten", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie wir hier zusammenstehn", "tokens": ["Und", "wie", "wir", "hier", "zu\u00b7sam\u00b7men\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Lust und Lieb getaucht,", "tokens": ["In", "Lust", "und", "Lieb", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wollen wir uns wieder sehn", "tokens": ["So", "wol\u00b7len", "wir", "uns", "wie\u00b7der", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn's von den Bergen raucht.", "tokens": ["Wenn's", "von", "den", "Ber\u00b7gen", "raucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Dann frisch Gesellen, Kraft und Muth,", "tokens": ["Dann", "frisch", "Ge\u00b7sel\u00b7len", ",", "Kraft", "und", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag der Rache k\u00f6mmt,", "tokens": ["Der", "Tag", "der", "Ra\u00b7che", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir sie mit dem eignen Blut", "tokens": ["Bis", "wir", "sie", "mit", "dem", "eig\u00b7nen", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Boden weggeschwemmt. \u2014", "tokens": ["Vom", "Bo\u00b7den", "weg\u00b7ge\u00b7schwemmt", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und du im freien Morgenroth", "tokens": ["Und", "du", "im", "frei\u00b7en", "Mor\u00b7gen\u00b7roth"], "