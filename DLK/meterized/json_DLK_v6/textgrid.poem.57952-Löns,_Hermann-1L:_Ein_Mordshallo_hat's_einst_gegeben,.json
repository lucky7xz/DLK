{"textgrid.poem.57952": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Mordshallo hat's einst gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mordshallo hat's einst gegeben,", "tokens": ["Ein", "Mords\u00b7hal\u00b7lo", "hat's", "einst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als C\u00e4sar nahm den Rubikon,", "tokens": ["Als", "C\u00e4\u00b7sar", "nahm", "den", "Ru\u00b7bi\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei uns gibt's andre \u00dcberschritte,", "tokens": ["Bei", "uns", "gibt's", "and\u00b7re", "\u00dc\u00b7bersc\u00b7hrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch wir sagen keinen Ton.", "tokens": ["Je\u00b7doch", "wir", "sa\u00b7gen", "kei\u00b7nen", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob Rathaus oder etwas andres,", "tokens": ["Ob", "Rat\u00b7haus", "o\u00b7der", "et\u00b7was", "and\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie kommt man aus mit dem Etat,", "tokens": ["Nie", "kommt", "man", "aus", "mit", "dem", "E\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuweilen scheint's, als w\u00fcrd' es langen,", "tokens": ["Zu\u00b7wei\u00b7len", "scheint's", ",", "als", "w\u00fcrd'", "es", "lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hinterher ist alles da.", "tokens": ["Doch", "hin\u00b7ter\u00b7her", "ist", "al\u00b7les", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die neue Rennbahn hat's bewiesen;", "tokens": ["Die", "neu\u00b7e", "Renn\u00b7bahn", "hat's", "be\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Nachtrag? Kinder, welch Malheur!", "tokens": ["Kein", "Nach\u00b7trag", "?", "Kin\u00b7der", ",", "welch", "Mal\u00b7heur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nein, es stimmt, denn als man nachsah,", "tokens": ["Doch", "nein", ",", "es", "stimmt", ",", "denn", "als", "man", "nach\u00b7sah", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "$,", "KON", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da waren's hunderttausend mehr.", "tokens": ["Da", "wa\u00b7ren's", "hun\u00b7dert\u00b7tau\u00b7send", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Stadthall'nbau sehn drum entgegen", "tokens": ["Dem", "Stadt\u00b7hall'\u00b7nbau", "sehn", "drum", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir mit Vertrauensv\u00f6lligkeit;", "tokens": ["Wir", "mit", "Ver\u00b7trau\u00b7ens\u00b7v\u00f6l\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist erst der Bauetat bewilligt,", "tokens": ["Ist", "erst", "der", "Bau\u00b7e\u00b7tat", "be\u00b7wil\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachtrag find't sich mit der Zeit.", "tokens": ["Der", "Nach\u00b7trag", "find't", "sich", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}