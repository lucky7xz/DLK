{"textgrid.poem.33014": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mach, ", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mach, ", "tokens": ["Mach", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "nur sorg, da\u00df es in ", "tokens": ["nur", "sorg", ",", "da\u00df", "es", "in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "als Ganzes, Volles dir gelingt,", "tokens": ["als", "Gan\u00b7zes", ",", "Vol\u00b7les", "dir", "ge\u00b7lingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und da\u00df nichts Fremdes dazwischen klingt!", "tokens": ["und", "da\u00df", "nichts", "Frem\u00b7des", "da\u00b7zwi\u00b7schen", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Man nenn's dann gut, man nenn es schlecht ...", "tokens": ["Man", "nenn's", "dann", "gut", ",", "man", "nenn", "es", "schlecht", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "es habe ruhig jeder recht,", "tokens": ["es", "ha\u00b7be", "ru\u00b7hig", "je\u00b7der", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und wer da lachen will, soll lachen ...", "tokens": ["und", "wer", "da", "la\u00b7chen", "will", ",", "soll", "la\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVINF", "VMFIN", "$,", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Witze sind \u00fcber alles zu machen!", "tokens": ["Wit\u00b7ze", "sind", "\u00fc\u00b7ber", "al\u00b7les", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "die einzige Frage, die da gilt,", "tokens": ["die", "ein\u00b7zi\u00b7ge", "Fra\u00b7ge", ",", "die", "da", "gilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "ob einer lobt nun oder schilt,", "tokens": ["ob", "ei\u00b7ner", "lobt", "nun", "o\u00b7der", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "die einzige Frage ist: gabst du ein Eig'nes!?", "tokens": ["die", "ein\u00b7zi\u00b7ge", "Fra\u00b7ge", "ist", ":", "gabst", "du", "ein", "Eig'\u00b7nes", "!?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}