{"dta.poem.21479": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Omphale .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Augen wie Kohlen so gl\u00fchend und gro\u00df", "tokens": ["Zwei", "Au\u00b7gen", "wie", "Koh\u00b7len", "so", "gl\u00fc\u00b7hend", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KOKOM", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durch\u2019s Zimmer, das dunkele, irren;", "tokens": ["Durch's", "Zim\u00b7mer", ",", "das", "dun\u00b7ke\u00b7le", ",", "ir\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "$,", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Man h\u00f6rt nur ein seltsam eint\u00f6nig Ger\u00e4usch,", "tokens": ["Man", "h\u00f6rt", "nur", "ein", "selt\u00b7sam", "ein\u00b7t\u00f6\u00b7nig", "Ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Schnurren und Rollen und Schwirren.", "tokens": ["Ein", "Schnur\u00b7ren", "und", "Rol\u00b7len", "und", "Schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Bisweilen ein St\u00f6hnen so tief und schwer,", "tokens": ["Bis\u00b7wei\u00b7len", "ein", "St\u00f6h\u00b7nen", "so", "tief", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dann wieder das ew\u2019ge Geschnurre;", "tokens": ["Dann", "wie\u00b7der", "das", "ew'\u00b7ge", "Ge\u00b7schnur\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bisweilen auch ist es, als kl\u00e4ng\u2019 im Gemach", "tokens": ["Bis\u00b7wei\u00b7len", "auch", "ist", "es", ",", "als", "kl\u00e4ng'", "im", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "$,", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Eines riesigen Hundes Geknurre . . . . .", "tokens": ["Ei\u00b7nes", "rie\u00b7si\u00b7gen", "Hun\u00b7des", "Ge\u00b7knur\u00b7re", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$.", "$.", "$.", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da nahet die Sclavin im bunten Gewand,", "tokens": ["Da", "na\u00b7het", "die", "Scla\u00b7vin", "im", "bun\u00b7ten", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zu erleuchten das \u00fcppige Dunkel:", "tokens": ["Zu", "er\u00b7leuch\u00b7ten", "das", "\u00fcp\u00b7pi\u00b7ge", "Dun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und es str\u00f6met die Ampel vom Deckengeb\u00e4lk", "tokens": ["Und", "es", "str\u00f6\u00b7met", "die", "Am\u00b7pel", "vom", "De\u00b7cken\u00b7ge\u00b7b\u00e4lk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Hernieder ein duftend Gefunkel.", "tokens": ["Her\u00b7nie\u00b7der", "ein", "duf\u00b7tend", "Ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Nicht achtet die Alte des H\u00fcnen, der dort", "tokens": ["Nicht", "ach\u00b7tet", "die", "Al\u00b7te", "des", "H\u00fc\u00b7nen", ",", "der", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Versenkt ist in gr\u00fcbelndes Sinnen,", "tokens": ["Ver\u00b7senkt", "ist", "in", "gr\u00fc\u00b7beln\u00b7des", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dess\u2019 eherner Fu\u00df nur beweget das Rad,", "tokens": ["Dess'", "e\u00b7her\u00b7ner", "Fu\u00df", "nur", "be\u00b7we\u00b7get", "das", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Um schm\u00e4hlich erniedert zu spinnen.", "tokens": ["Um", "schm\u00e4h\u00b7lich", "er\u00b7nie\u00b7dert", "zu", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Wohl h\u00e4ngt ihm ein weibisches Frauengewand", "tokens": ["Wohl", "h\u00e4ngt", "ihm", "ein", "wei\u00b7bi\u00b7sches", "Frau\u00b7en\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nachl\u00e4ssig und schleppend hernieder,", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "und", "schlep\u00b7pend", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch jegliches Regen des Leibes enth\u00fcllt", "tokens": ["Doch", "jeg\u00b7li\u00b7ches", "Re\u00b7gen", "des", "Lei\u00b7bes", "ent\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Formen der ehernen Glieder.", "tokens": ["Die", "For\u00b7men", "der", "e\u00b7her\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Doch ha, nun schwebet sie selber herein,", "tokens": ["Doch", "ha", ",", "nun", "schwe\u00b7bet", "sie", "sel\u00b7ber", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die lydische Amazone,", "tokens": ["Die", "ly\u00b7di\u00b7sche", "A\u00b7ma\u00b7zo\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Goldspangen auf nacktem und rosigem Arm,", "tokens": ["Gold\u00b7span\u00b7gen", "auf", "nack\u00b7tem", "und", "ro\u00b7si\u00b7gem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auf br\u00e4unlichen Locken die Krone.", "tokens": ["Auf", "br\u00e4un\u00b7li\u00b7chen", "Lo\u00b7cken", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Ein h\u00f6hnisches L\u00e4cheln umspielet den Mund,", "tokens": ["Ein", "h\u00f6h\u00b7ni\u00b7sches", "L\u00e4\u00b7cheln", "um\u00b7spie\u00b7let", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schier lechzend die Augen erstrahlen,", "tokens": ["Schier", "lech\u00b7zend", "die", "Au\u00b7gen", "er\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Als k\u00f6nnten sie nimmer ges\u00e4ttigt beschau\u2019n", "tokens": ["Als", "k\u00f6nn\u00b7ten", "sie", "nim\u00b7mer", "ge\u00b7s\u00e4t\u00b7tigt", "be\u00b7schau'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "VVPP", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Des Helden unendliche Qualen.", "tokens": ["Des", "Hel\u00b7den", "un\u00b7end\u00b7li\u00b7che", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Sie lagert sich nieder auf schwellendem Pf\u00fchl", "tokens": ["Sie", "la\u00b7gert", "sich", "nie\u00b7der", "auf", "schwel\u00b7len\u00b7dem", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und blicket herab zu dem Recken;", "tokens": ["Und", "bli\u00b7cket", "her\u00b7ab", "zu", "dem", "Re\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u201enun komm, mein H\u00fcndchen, nun darfst du die Hand", "tokens": ["\u201e", "nun", "komm", ",", "mein", "H\u00fcnd\u00b7chen", ",", "nun", "darfst", "du", "die", "Hand"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Herrin gehorsam belecken!\u201c", "tokens": ["Der", "Her\u00b7rin", "ge\u00b7hor\u00b7sam", "be\u00b7le\u00b7cken", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Und das H\u00fcndchen erhebt sich und schmieget sich hin", "tokens": ["Und", "das", "H\u00fcnd\u00b7chen", "er\u00b7hebt", "sich", "und", "schmie\u00b7get", "sich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "APZR"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Zu den F\u00fc\u00dfen des \u00fcppigen Weibes,", "tokens": ["Zu", "den", "F\u00fc\u00b7\u00dfen", "des", "\u00fcp\u00b7pi\u00b7gen", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und k\u00fc\u00dft ihr die Hand und ein Beben durchzieht", "tokens": ["Und", "k\u00fc\u00dft", "ihr", "die", "Hand", "und", "ein", "Be\u00b7ben", "durch\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Glieder des \u00fcppigen Leibes.", "tokens": ["Die", "Glie\u00b7der", "des", "\u00fcp\u00b7pi\u00b7gen", "Lei\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Und sie wirft sich ein Fell um den blendenden Hals,", "tokens": ["Und", "sie", "wirft", "sich", "ein", "Fell", "um", "den", "blen\u00b7den\u00b7den", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Um die Brust, um die Schulter, die nackten,", "tokens": ["Um", "die", "Brust", ",", "um", "die", "Schul\u00b7ter", ",", "die", "nack\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Das Fell des neme", "tokens": ["Das", "Fell", "des", "ne\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zwei H\u00e4nde zerdr\u00fcckten und packten.", "tokens": ["Zwei", "H\u00e4n\u00b7de", "zer\u00b7dr\u00fcck\u00b7ten", "und", "pack\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Dann greift nach der Keule die zierliche Hand,", "tokens": ["Dann", "greift", "nach", "der", "Keu\u00b7le", "die", "zier\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u201eei, ", "tokens": ["\u201e", "ei", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und die Sieger doch immer die Sch\u00f6nen?\u201c", "tokens": ["Und", "die", "Sie\u00b7ger", "doch", "im\u00b7mer", "die", "Sch\u00f6\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.12": {"line.1": {"text": "\u201each, Omphale, ja, die Gewaltigsten sind", "tokens": ["\u201e", "ach", ",", "Om\u00b7pha\u00b7le", ",", "ja", ",", "die", "Ge\u00b7wal\u00b7tigs\u00b7ten", "sind"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PTKANT", "$,", "ART", "NN", "VAFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und die Sieger wohl immer die Sch\u00f6nen,", "tokens": ["Und", "die", "Sie\u00b7ger", "wohl", "im\u00b7mer", "die", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Doch st\u00e4rker bed\u00fcnkt mich die G\u00f6ttin zu sein,", "tokens": ["Doch", "st\u00e4r\u00b7ker", "be\u00b7d\u00fcnkt", "mich", "die", "G\u00f6t\u00b7tin", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die uns lehret das ", "tokens": ["Die", "uns", "leh\u00b7ret", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "\u201eo schweig, mein Trauter, und k\u00fc\u00df mir den Mund,", "tokens": ["\u201e", "o", "schweig", ",", "mein", "Trau\u00b7ter", ",", "und", "k\u00fc\u00df", "mir", "den", "Mund", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJD", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ruh\u2019 aus bei mir von dem weibischen Dienst", "tokens": ["Ruh'", "aus", "bei", "mir", "von", "dem", "w