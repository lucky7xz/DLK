{"textgrid.poem.43034": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00fcrkin", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise Abendwinde necken", "tokens": ["Lei\u00b7se", "Ab\u00b7end\u00b7win\u00b7de", "ne\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Buhlerisch den Myrthenhain,", "tokens": ["Buh\u00b7le\u00b7risch", "den", "Myr\u00b7then\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bergen sich in Lorbeerhecken,", "tokens": ["Ber\u00b7gen", "sich", "in", "Lor\u00b7beer\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiegen dort die Bl\u00fcthen ein;", "tokens": ["Wie\u00b7gen", "dort", "die", "Bl\u00fc\u00b7then", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flattern weiter dann zum Meere,", "tokens": ["Flat\u00b7tern", "wei\u00b7ter", "dann", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das in einer wilden Nacht", "tokens": ["Das", "in", "ei\u00b7ner", "wil\u00b7den", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott als eine Liebesz\u00e4hre", "tokens": ["Gott", "als", "ei\u00b7ne", "Lie\u00b7bes\u00b7z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einst der Erde gleichgemacht.", "tokens": ["Einst", "der", "Er\u00b7de", "gleich\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mild umgaukeln bunte Lichter", "tokens": ["Mild", "um\u00b7gau\u00b7keln", "bun\u00b7te", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon des Abends goldnes Thor;", "tokens": ["Schon", "des", "A\u00b7bends", "gold\u00b7nes", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigend aus dem Dorf der Richter", "tokens": ["Schwei\u00b7gend", "aus", "dem", "Dorf", "der", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt ein stolzes Weib hervor;", "tokens": ["Tritt", "ein", "stol\u00b7zes", "Weib", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auf \u00f6der Felsenklippe,", "tokens": ["Und", "auf", "\u00f6\u00b7der", "Fel\u00b7sen\u00b7klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche nach den Wogen fa\u00dft,", "tokens": ["Wel\u00b7che", "nach", "den", "Wo\u00b7gen", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4lt sie \u2013 Seufzer auf der Lippe \u2013", "tokens": ["H\u00e4lt", "sie", "\u2013", "Seuf\u00b7zer", "auf", "der", "Lip\u00b7pe", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine kurze Sclavenrast.", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "Scla\u00b7ven\u00b7rast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bblass' die Liebe schnell erblassen,", "tokens": ["\u00bb", "lass'", "die", "Lie\u00b7be", "schnell", "er\u00b7blas\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Du, Frankensohn gen\u00e4hrt!", "tokens": ["Die", "Du", ",", "Fran\u00b7ken\u00b7sohn", "ge\u00b7n\u00e4hrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen mu\u00df ich Dich verlassen,", "tokens": ["Mor\u00b7gen", "mu\u00df", "ich", "Dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil der Sultan mein begehrt.\u00ab", "tokens": ["Weil", "der", "Sul\u00b7tan", "mein", "be\u00b7gehrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also t\u00f6nen ihre Worte", "tokens": ["Al\u00b7so", "t\u00f6\u00b7nen", "ih\u00b7re", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wund hervor aus wunder Brust;", "tokens": ["Wund", "her\u00b7vor", "aus", "wun\u00b7der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn der Herr der hohen Pforte", "tokens": ["Denn", "der", "Herr", "der", "ho\u00b7hen", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt nur schn\u00f6de Sinnenlust. \u2013", "tokens": ["Kennt", "nur", "schn\u00f6\u00b7de", "Sin\u00b7nen\u00b7lust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh! da bricht durch Wolkenschleier", "tokens": ["Sieh", "!", "da", "bricht", "durch", "Wol\u00b7ken\u00b7schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell des Mondes Silberlicht,", "tokens": ["Hell", "des", "Mon\u00b7des", "Sil\u00b7ber\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Stambul in stummer Feier", "tokens": ["Und", "Stam\u00b7bul", "in", "stum\u00b7mer", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zeigt sich ihrem Angesicht.", "tokens": ["Zeigt", "sich", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh! im Vordergrunde schimmert", "tokens": ["Weh", "!", "im", "Vor\u00b7der\u00b7grun\u00b7de", "schim\u00b7mert"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "H\u00f6rst Du, wie das Meer jetzt wimmert,", "tokens": ["H\u00f6rst", "Du", ",", "wie", "das", "Meer", "jetzt", "wim\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das ein edles Weib verschlingt? \u2013", "tokens": ["Das", "ein", "ed\u00b7les", "Weib", "ver\u00b7schlingt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Willst Du ihren Tod beklagen,", "tokens": ["Willst", "Du", "ih\u00b7ren", "Tod", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft Du trauern allerw\u00e4rts;", "tokens": ["Mu\u00dft", "Du", "trau\u00b7ern", "al\u00b7ler\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wo immer Herzen schlagen,", "tokens": ["Denn", "wo", "im\u00b7mer", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Foltert sie derselbe Schmerz,", "tokens": ["Fol\u00b7tert", "sie", "der\u00b7sel\u00b7be", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist das Heiligste ge\u00e4chtet,", "tokens": ["Ist", "das", "Hei\u00b7ligs\u00b7te", "ge\u00e4ch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wird der Satzung nur gefr\u00f6hnt;", "tokens": ["Wird", "der", "Sat\u00b7zung", "nur", "ge\u00b7fr\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder Pulsschlag ist geknechtet,", "tokens": ["Je\u00b7der", "Puls\u00b7schlag", "ist", "ge\u00b7knech\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedes freie Weib geh\u00f6hnt! \u2013", "tokens": ["Je\u00b7des", "frei\u00b7e", "Weib", "ge\u00b7h\u00f6hnt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}