{"textgrid.poem.33722": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Schutzgeist unterband mir Goldsandalen,", "tokens": ["Kein", "Schutz\u00b7geist", "un\u00b7ter\u00b7band", "mir", "Gold\u00b7san\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An meiner Wiege stand mein Widerstreiter,", "tokens": ["An", "mei\u00b7ner", "Wie\u00b7ge", "stand", "mein", "Wi\u00b7der\u00b7strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Taten schritt nicht einen Schritt ich weiter,", "tokens": ["Zu", "Ta\u00b7ten", "schritt", "nicht", "ei\u00b7nen", "Schritt", "ich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo nicht Zuf\u00e4lle den Erfolg mir stahlen.", "tokens": ["Wo", "nicht", "Zu\u00b7f\u00e4l\u00b7le", "den", "Er\u00b7folg", "mir", "stah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum freudelosen Sieg nach tausend Qualen", "tokens": ["Zum", "freu\u00b7de\u00b7lo\u00b7sen", "Sieg", "nach", "tau\u00b7send", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Macht' ich die Bahn mit meinem Blut nur breiter;", "tokens": ["Macht'", "ich", "die", "Bahn", "mit", "mei\u00b7nem", "Blut", "nur", "brei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie, nie beging ich unumschr\u00e4nkt und heiter", "tokens": ["Nie", ",", "nie", "be\u00b7ging", "ich", "un\u00b7um\u00b7schr\u00e4nkt", "und", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gro\u00dfen, meines Lebens Kaiserwahlen.", "tokens": ["Die", "gro\u00b7\u00dfen", ",", "mei\u00b7nes", "Le\u00b7bens", "Kai\u00b7ser\u00b7wah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Streben alles blieb ein fruchtlos rauhes", "tokens": ["Mein", "Stre\u00b7ben", "al\u00b7les", "blieb", "ein", "frucht\u00b7los", "rau\u00b7hes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PIS", "VVFIN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00fcrmen ewig neuer Widerst\u00e4nde,", "tokens": ["Be\u00b7st\u00fcr\u00b7men", "e\u00b7wig", "neu\u00b7er", "Wi\u00b7der\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kampf mit S\u00e4ulen eines Felsenbaues.", "tokens": ["Ein", "Kampf", "mit", "S\u00e4u\u00b7len", "ei\u00b7nes", "Fel\u00b7sen\u00b7bau\u00b7es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fcr meinen Durst, f\u00fcr meine Fieberbr\u00e4nde", "tokens": ["F\u00fcr", "mei\u00b7nen", "Durst", ",", "f\u00fcr", "mei\u00b7ne", "Fie\u00b7ber\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fiel nie das Manna jenes Seelentaues,", "tokens": ["Fiel", "nie", "das", "Man\u00b7na", "je\u00b7nes", "See\u00b7len\u00b7tau\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Trostes, da\u00df ein Herz mit mir empf\u00e4nde.", "tokens": ["Des", "Tros\u00b7tes", ",", "da\u00df", "ein", "Herz", "mit", "mir", "em\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}