{"dta.poem.23676": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Pesne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Maler, male meine Freunde!", "tokens": ["Ma\u00b7ler", ",", "ma\u00b7le", "mei\u00b7ne", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleist soll, mitten unter Helden,", "tokens": ["Kleist", "soll", ",", "mit\u00b7ten", "un\u00b7ter", "Hel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das Lob der Gottheit sinnen.", "tokens": ["Auf", "das", "Lob", "der", "Got\u00b7theit", "sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mal ihn unter tausend Blumen;", "tokens": ["Mal", "ihn", "un\u00b7ter", "tau\u00b7send", "Blu\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mal ihn, da\u00df er an dem Himmel,", "tokens": ["Mal", "ihn", ",", "da\u00df", "er", "an", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Regenbogen vor sich siehet!", "tokens": ["Re\u00b7gen\u00b7bo\u00b7gen", "vor", "sich", "sie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Adler soll dem wilden Menzel", "tokens": ["Ad\u00b7ler", "soll", "dem", "wil\u00b7den", "Men\u00b7zel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem krummen Schwerdte drohen,", "tokens": ["Mit", "dem", "krum\u00b7men", "Schwerd\u00b7te", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zugleich den Maasstab halten.", "tokens": ["Und", "zu\u00b7gleich", "den", "Maas\u00b7stab", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Donop soll satirisch l&#228;cheln.", "tokens": ["Do\u00b7nop", "soll", "sa\u00b7ti\u00b7risch", "l", "&#228;", "cheln", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "VMFIN", "ADJD", "NN", "$(", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Seidlitz soll der Braut entfliehen,", "tokens": ["Seid\u00b7litz", "soll", "der", "Braut", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Die ihm seine Mutter bringet.", "tokens": ["Die", "ihm", "sei\u00b7ne", "Mut\u00b7ter", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Venus soll, mit ofnen Armen,", "tokens": ["Ve\u00b7nus", "soll", ",", "mit", "of\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihm vergn\u00fcgt entgegen eilen,", "tokens": ["Ihm", "ver\u00b7gn\u00fcgt", "ent\u00b7ge\u00b7gen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und, Adonis an der Seite,", "tokens": ["Und", ",", "A\u00b7do\u00b7nis", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Soll den Pfeil, der ihn erobert,", "tokens": ["Soll", "den", "Pfeil", ",", "der", "ihn", "er\u00b7o\u00b7bert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Einem Plutus sp\u00f6ttisch zeigen.", "tokens": ["Ei\u00b7nem", "Plu\u00b7tus", "sp\u00f6t\u00b7tisch", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": ".... soll der Tugend folgen,", "tokens": ["....", "soll", "der", "Tu\u00b7gend", "fol\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Die ihm himmlisch freundlich winket.", "tokens": ["Die", "ihm", "himm\u00b7lisch", "freund\u00b7lich", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Kannst du wol die Tugend malen?", "tokens": ["Kannst", "du", "wol", "die", "Tu\u00b7gend", "ma\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Male sie wie seine Schwester.", "tokens": ["Ma\u00b7le", "sie", "wie", "sei\u00b7ne", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Fromm soll reife Weitzen&#228;hren", "tokens": ["Fromm", "soll", "rei\u00b7fe", "Weit\u00b7zen", "&#228;", "hren"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "VMFIN", "ADJA", "NN", "$(", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Um das Haupt der Ceres winden.", "tokens": ["Um", "das", "Haupt", "der", "Ce\u00b7res", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Lamprecht soll, umringt von Lastern,", "tokens": ["Lam\u00b7precht", "soll", ",", "um\u00b7ringt", "von", "Las\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "G\u00fctig mit den Lastern streiten.", "tokens": ["G\u00fc\u00b7tig", "mit", "den", "Las\u00b7tern", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Mal um ihn die Laster he\u00dflich!", "tokens": ["Mal", "um", "ihn", "die", "Las\u00b7ter", "he\u00df\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Male sie, da\u00df man sich f\u00fcrchtet,", "tokens": ["Ma\u00b7le", "sie", ",", "da\u00df", "man", "sich", "f\u00fcrch\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.28": {"text": "Wie Lucan die Hexen malet!", "tokens": ["Wie", "Lu\u00b7can", "die", "He\u00b7xen", "ma\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Naumann soll, mit starren Augen,", "tokens": ["Nau\u00b7mann", "soll", ",", "mit", "star\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Einen Liebesgott betrachten,", "tokens": ["Ei\u00b7nen", "Lie\u00b7bes\u00b7gott", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Der ihn wiederum betrachtet;", "tokens": ["Der", "ihn", "wie\u00b7de\u00b7rum", "be\u00b7trach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Gieb auch beiden Pfeil und Bogen,", "tokens": ["Gieb", "auch", "bei\u00b7den", "Pfeil", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df sie auf einander zielen.", "tokens": ["Da\u00df", "sie", "auf", "ein\u00b7an\u00b7der", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Sulzer soll, am sch\u00f6nsten Morgen,", "tokens": ["Sul\u00b7zer", "soll", ",", "am", "sch\u00f6ns\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Auf der sch\u00f6nsten Aue schleichen.", "tokens": ["Auf", "der", "sch\u00f6ns\u00b7ten", "Au\u00b7e", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "La\u00df uns sehn, wie er sich freuet,", "tokens": ["La\u00df", "uns", "sehn", ",", "wie", "er", "sich", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Wenn er neue Blumen findet,", "tokens": ["Wenn", "er", "neu\u00b7e", "Blu\u00b7men", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wie er, wenn ein Freund erscheinet,", "tokens": ["Wie", "er", ",", "wenn", "ein", "Freund", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Auch die Blumen gleich verl\u00e4sset,", "tokens": ["Auch", "die", "Blu\u00b7men", "gleich", "ver\u00b7l\u00e4s\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und dem Freund entgegen eilet.", "tokens": ["Und", "dem", "Freund", "ent\u00b7ge\u00b7gen", "ei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Uz, wie la\u00df ich dich doch malen?", "tokens": ["Uz", ",", "wie", "la\u00df", "ich", "dich", "doch", "ma\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVIMP", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Siehst du nicht dem Wachsbild \u00e4hnlich,", "tokens": ["Siehst", "du", "nicht", "dem", "Wachs\u00b7bild", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Das Anakreon bestellte?", "tokens": ["Das", "A\u00b7nak\u00b7re\u00b7on", "be\u00b7stell\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.44": {"text": "Maler, mal ihn nach dem Bilde:", "tokens": ["Ma\u00b7ler", ",", "mal", "ihn", "nach", "dem", "Bil\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Mal ihn, hinter Rosenb\u00fcschen,", "tokens": ["Mal", "ihn", ",", "hin\u00b7ter", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "An dem Ufer eines Teiches.", "tokens": ["An", "dem", "U\u00b7fer", "ei\u00b7nes", "Tei\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "La\u00df ihn lauschen, la\u00df ihn sehen,", "tokens": ["La\u00df", "ihn", "lau\u00b7schen", ",", "la\u00df", "ihn", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wie sich eine Venus badet.", "tokens": ["Wie", "sich", "ei\u00b7ne", "Ve\u00b7nus", "ba\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Maler, dis sind meine Freunde.", "tokens": ["Ma\u00b7ler", ",", "dis", "sind", "mei\u00b7ne", "Freun\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Male mich, da\u00df ich sie k\u00fcsse,", "tokens": ["Ma\u00b7le", "mich", ",", "da\u00df", "ich", "sie", "k\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Und dann male ", "tokens": ["Und", "dann", "ma\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "An der Seite seines ", "tokens": ["An", "der", "Sei\u00b7te", "sei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.53": {"text": "An der Hand des besten Priesters,", "tokens": ["An", "der", "Hand", "des", "bes\u00b7ten", "Pries\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Da\u00df er meine Freunde siehet.", "tokens": ["Da\u00df", "er", "mei\u00b7ne", "Freun\u00b7de", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Wenn du meinen Vater malest,", "tokens": ["Wenn", "du", "mei\u00b7nen", "Va\u00b7ter", "ma\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Must du, mit beseelten Z\u00fcgen,", "tokens": ["Must", "du", ",", "mit", "be\u00b7seel\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Seine Redlichkeit bezeichnen.", "tokens": ["Sei\u00b7ne", "Red\u00b7lich\u00b7keit", "be\u00b7zeich\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Denn es soll sein wehrtes Bildnis,", "tokens": ["Denn", "es", "soll", "sein", "wehr\u00b7tes", "Bild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Wenn ichs meinen Freunden zeige,", "tokens": ["Wenn", "ichs", "mei\u00b7nen", "Freun\u00b7den", "zei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Mich und sie zur Tugend reitzen.", "tokens": ["Mich", "und", "sie", "zur", "Tu\u00b7gend", "reit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Maler, nun kannst du mir danken,", "tokens": ["Ma\u00b7ler", ",", "nun", "kannst", "du", "mir", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Wenn die Bilder treuer M\u00e4nner", "tokens": ["Wenn", "die", "Bil\u00b7der", "treu\u00b7er", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Deinem Pinsel Ehre bringen.", "tokens": ["Dei\u00b7nem", "Pin\u00b7sel", "Eh\u00b7re", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Sollen Bilder treuer Sch\u00f6nen,", "tokens": ["Sol\u00b7len", "Bil\u00b7der", "treu\u00b7er", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Deinem Pinsel Ehre bringen;", "tokens": ["Dei\u00b7nem", "Pin\u00b7sel", "Eh\u00b7re", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "O so mal auch ihre M\u00e4dchen.", "tokens": ["O", "so", "mal", "auch", "ih\u00b7re", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Geh, und frage meine Freunde:", "tokens": ["Geh", ",", "und", "fra\u00b7ge", "mei\u00b7ne", "Freun\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Sagt, wo habt ihr eure M\u00e4dchen?", "tokens": ["Sagt", ",", "wo", "habt", "ihr", "eu\u00b7re", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}