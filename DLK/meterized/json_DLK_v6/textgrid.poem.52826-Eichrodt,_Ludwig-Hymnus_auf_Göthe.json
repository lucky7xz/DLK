{"textgrid.poem.52826": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus auf G\u00f6the", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es preisen alle Zungen", "tokens": ["Es", "prei\u00b7sen", "al\u00b7le", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Namen G\u00f6the laut,", "tokens": ["Den", "Na\u00b7men", "G\u00f6\u00b7the", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Alten und die Jungen", "tokens": ["Die", "Al\u00b7ten", "und", "die", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind noch von ihm erbaut.", "tokens": ["Sind", "noch", "von", "ihm", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum sag' auch ich, nicht bl\u00f6the:", "tokens": ["D'\u00b7rum", "sag'", "auch", "ich", ",", "nicht", "bl\u00f6t\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es war ihm nichts zu schwierig,", "tokens": ["Es", "war", "ihm", "nichts", "zu", "schwie\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er dichtet es geschwind,", "tokens": ["Er", "dich\u00b7tet", "es", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Trauerspiel und Lyrik", "tokens": ["In", "Trau\u00b7er\u00b7spiel", "und", "Ly\u00b7rik"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat er den Preis verdient.", "tokens": ["Hat", "er", "den", "Preis", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum, ob er sich's verb\u00f6the:", "tokens": ["D'\u00b7rum", ",", "ob", "er", "sich's", "ver\u00b7b\u00f6t\u00b7he", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "An seinem Werther h\u00e4rmt man", "tokens": ["An", "sei\u00b7nem", "Wert\u00b7her", "h\u00e4rmt", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich seiner Zeit wie jetzt,", "tokens": ["Sich", "sei\u00b7ner", "Zeit", "wie", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "KOKOM", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das deutsche Blut erw\u00e4rmt man", "tokens": ["Das", "deut\u00b7sche", "Blut", "er\u00b7w\u00e4rmt", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Berlichinger G\u00f6tz.", "tokens": ["Am", "Ber\u00b7li\u00b7chin\u00b7ger", "G\u00f6tz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum rufet fr\u00fch und sp\u00f6the!", "tokens": ["D'\u00b7rum", "ru\u00b7fet", "fr\u00fch", "und", "sp\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wen sollte nicht ermahnen", "tokens": ["Wen", "soll\u00b7te", "nicht", "er\u00b7mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gottvergessne Faust,", "tokens": ["Der", "gott\u00b7ver\u00b7gess\u00b7ne", "Faust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der auf des Lasters Bahnen", "tokens": ["Der", "auf", "des", "Las\u00b7ters", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der H\u00f6lle zugesaust.", "tokens": ["Der", "H\u00f6l\u00b7le", "zu\u00b7ge\u00b7saust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum lodre die Rak\u00f6the:", "tokens": ["D'\u00b7rum", "lod\u00b7re", "die", "Ra\u00b7k\u00f6\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In seinen Elegieen", "tokens": ["In", "sei\u00b7nen", "E\u00b7le\u00b7gi\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat er sich nicht geniert,", "tokens": ["Hat", "er", "sich", "nicht", "ge\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man hat's ihm gern verziehen,", "tokens": ["Man", "hat's", "ihm", "gern", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man sich alterirt.", "tokens": ["Da\u00df", "man", "sich", "al\u00b7ter\u00b7irt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum thu auch ich nicht spr\u00f6the:", "tokens": ["D'\u00b7rum", "thu", "auch", "ich", "nicht", "spr\u00f6\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als alter Musenpriester", "tokens": ["Als", "al\u00b7ter", "Mu\u00b7sen\u00b7pries\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trieb er Chinesisch noch,", "tokens": ["Trieb", "er", "Chi\u00b7ne\u00b7sisch", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und war er gleich Minister,", "tokens": ["Und", "war", "er", "gleich", "Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So nahm er Zeit sich doch.", "tokens": ["So", "nahm", "er", "Zeit", "sich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum riefen seine R\u00e4the:", "tokens": ["D'\u00b7rum", "rie\u00b7fen", "sei\u00b7ne", "R\u00e4\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er hat gemalt, gezeichnet,", "tokens": ["Er", "hat", "ge\u00b7malt", ",", "ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott und die Welt studirt,", "tokens": ["Gott", "und", "die", "Welt", "stu\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sonst sich angeeignet,", "tokens": ["Und", "sonst", "sich", "an\u00b7ge\u00b7eig\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was einen Menschen ziert.", "tokens": ["Was", "ei\u00b7nen", "Men\u00b7schen", "ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum, blies er auch nicht Fl\u00f6the:", "tokens": ["D'\u00b7rum", ",", "blies", "er", "auch", "nicht", "Fl\u00f6\u00b7the", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er schrieb den Wilhelm Meister,", "tokens": ["Er", "schrieb", "den", "Wil\u00b7helm", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Buch wie andre mehr,", "tokens": ["Ein", "Buch", "wie", "and\u00b7re", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das gute Herz beweist er", "tokens": ["Das", "gu\u00b7te", "Herz", "be\u00b7weist", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Hermann sehr.", "tokens": ["In", "sei\u00b7nem", "Her\u00b7mann", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum, g\u00e4b' es auch Em\u00f6the:", "tokens": ["D'\u00b7rum", ",", "g\u00e4b'", "es", "auch", "E\u00b7m\u00f6t\u00b7he", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "ADV", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Einsamer Schlaf und Wanzen,", "tokens": ["Ein\u00b7sa\u00b7mer", "Schlaf", "und", "Wan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "War ihm, er sagt's, verha\u00dft,", "tokens": ["War", "ihm", ",", "er", "sagt's", ",", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch hat er stets die Pflanzen", "tokens": ["Auch", "hat", "er", "stets", "die", "Pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gem\u00fcthvoll angefa\u00dft.", "tokens": ["Ge\u00b7m\u00fcth\u00b7voll", "an\u00b7ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum rufe Hans und Gr\u00f6the", "tokens": ["D'\u00b7rum", "ru\u00b7fe", "Hans", "und", "Gr\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Selbst an der Farbenlehre", "tokens": ["Selbst", "an", "der", "Far\u00b7ben\u00b7leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schrieb dieser gro\u00dfe Mann,", "tokens": ["Schrieb", "die\u00b7ser", "gro\u00b7\u00dfe", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und das ist doch so schwere,", "tokens": ["Und", "das", "ist", "doch", "so", "schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man nur staunen kann.", "tokens": ["Da\u00df", "man", "nur", "stau\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum, ob man mich auch t\u00f6dte:", "tokens": ["D'\u00b7rum", ",", "ob", "man", "mich", "auch", "t\u00f6d\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gepriesen sei der G\u00f6the!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "G\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bettina, die so kindlich,", "tokens": ["Bet\u00b7ti\u00b7na", ",", "die", "so", "kind\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sprach ihn als Freundin an;", "tokens": ["Sprach", "ihn", "als", "Freun\u00b7din", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch sagt er Vieles m\u00fcndlich", "tokens": ["Auch", "sagt", "er", "Vie\u00b7les", "m\u00fcnd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", 