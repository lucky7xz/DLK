{"textgrid.poem.40260": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weich k\u00fc\u00dft die Zweige", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weich k\u00fc\u00dft die Zweige", "tokens": ["Weich", "k\u00fc\u00dft", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der wei\u00dfe Mond;", "tokens": ["der", "wei\u00b7\u00dfe", "Mond", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ein Fl\u00fcstern wohnt", "tokens": ["ein", "Fl\u00fcs\u00b7tern", "wohnt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "im Laub, als neige,", "tokens": ["im", "Laub", ",", "als", "nei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "als schweige sich der Hain zur Ruh \u2013", "tokens": ["als", "schwei\u00b7ge", "sich", "der", "Hain", "zur", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PRF", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebte du.", "tokens": ["Ge\u00b7lieb\u00b7te", "du", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Weiher ruht, und", "tokens": ["Der", "Wei\u00b7her", "ruht", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Weide schimmert;", "tokens": ["die", "Wei\u00b7de", "schim\u00b7mert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "ihr Schatten flimmert", "tokens": ["ihr", "Schat\u00b7ten", "flim\u00b7mert"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "in seiner Flut, und", "tokens": ["in", "sei\u00b7ner", "Flut", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "der Wind weint in den B\u00e4umen \u2013", "tokens": ["der", "Wind", "weint", "in", "den", "B\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wir tr\u00e4umen ... tr\u00e4umen.", "tokens": ["wir", "tr\u00e4u\u00b7men", "...", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Weiten leuchten", "tokens": ["Die", "Wei\u00b7ten", "leuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Beruhigung;", "tokens": ["Be\u00b7ru\u00b7hi\u00b7gung", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "die Niederung", "tokens": ["die", "Nie\u00b7de\u00b7rung"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "hebt bleich den feuchten", "tokens": ["hebt", "bleich", "den", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schleier hin zum Himmelssaum \u2013", "tokens": ["Schlei\u00b7er", "hin", "zum", "Him\u00b7mels\u00b7saum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "oh hin \u2013 o Traum ...", "tokens": ["oh", "hin", "\u2013", "o", "Traum", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$(", "FM", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}