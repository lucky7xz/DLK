{"dta.poem.8837": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "In der mich nichts belastete,", "tokens": ["In", "der", "mich", "nichts", "be\u00b7las\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der ich noch so wohlgemuth,", "tokens": ["In", "der", "ich", "noch", "so", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Tisch der Ruhe gastete!", "tokens": ["Am", "Tisch", "der", "Ru\u00b7he", "gas\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der ich nicht nach falscher Gunst", "tokens": ["In", "der", "ich", "nicht", "nach", "fal\u00b7scher", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit eil'gen Schritten hastete!", "tokens": ["Mit", "eil'\u00b7gen", "Schrit\u00b7ten", "has\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du flohst, es rette mich das Gl\u00fcck,", "tokens": ["Du", "flohst", ",", "es", "ret\u00b7te", "mich", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da's wei\u00df, wie lang ich fastete,", "tokens": ["Da's", "wei\u00df", ",", "wie", "lang", "ich", "fas\u00b7te\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie lang' ich keine sch\u00f6ne Hand", "tokens": ["Wie", "lang'", "ich", "kei\u00b7ne", "sch\u00f6\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit meiner Hand betastete!", "tokens": ["Mit", "mei\u00b7ner", "Hand", "be\u00b7tas\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}