{"dta.poem.2391": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mein' Augen allzeit von sich giessen/", "tokens": ["Mein'", "Au\u00b7gen", "all\u00b7zeit", "von", "sich", "gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des bittren Leidens herben Bach/", "tokens": ["Des", "bit\u00b7tren", "Lei\u00b7dens", "her\u00b7ben", "Bach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wangen stets mit ", "tokens": ["Die", "Wan\u00b7gen", "stets", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Mein mattes ", "tokens": ["Mein", "mat\u00b7tes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wenn es O Schikkung an dich d\u00e4nkt.", "tokens": ["Wenn", "es", "O", "Schik\u00b7kung", "an", "dich", "d\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Beweinet doch mein grosses Leiden/", "tokens": ["Be\u00b7wei\u00b7net", "doch", "mein", "gros\u00b7ses", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Beklaget meine Noht ihr Haiden/", "tokens": ["Be\u00b7kla\u00b7get", "mei\u00b7ne", "Noht", "ihr", "Hai\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und du/ du stille Felderruh/", "tokens": ["Und", "du", "/", "du", "stil\u00b7le", "Fel\u00b7der\u00b7ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil meine G\u00f6tlin ", "tokens": ["Weil", "mei\u00b7ne", "G\u00f6t\u00b7lin"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mich armen gar nicht lieben wil.", "tokens": ["Mich", "ar\u00b7men", "gar", "nicht", "lie\u00b7ben", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleubts nur im ", "tokens": ["Gleubts", "nur", "im"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPRART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Jhr stumme Beum\u2019 all\u2019 ingesamt/", "tokens": ["Ihr", "stum\u00b7me", "Beum'", "all'", "in\u00b7ge\u00b7samt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Angst mu\u00df eure Zahl nachgehen/", "tokens": ["Der", "Angst", "mu\u00df", "eu\u00b7re", "Zahl", "nach\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In die ich schmertzlich bin verdammt/", "tokens": ["In", "die", "ich", "schmertz\u00b7lich", "bin", "ver\u00b7dammt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel mehr sind Ahrten meiner Pein/", "tokens": ["Viel", "mehr", "sind", "Ahr\u00b7ten", "mei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}