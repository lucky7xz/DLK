{"textgrid.poem.23997": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwischen Hetzen und Hasten,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Hetzen und Hasten,", "tokens": ["Zwi\u00b7schen", "Het\u00b7zen", "und", "Has\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In L\u00e4rmen und Lasten,", "tokens": ["In", "L\u00e4r\u00b7men", "und", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von Zeit zu Zeit", "tokens": ["Von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mag gerne ich rasten", "tokens": ["Mag", "ger\u00b7ne", "ich", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In Nachdenklichkeit.", "tokens": ["In", "Nach\u00b7denk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fliege, fliege, mein Denken, zur\u00fcck,", "tokens": ["Flie\u00b7ge", ",", "flie\u00b7ge", ",", "mein", "Den\u00b7ken", ",", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Suche, suche: in heimlichen Ecken", "tokens": ["Su\u00b7che", ",", "su\u00b7che", ":", "in", "heim\u00b7li\u00b7chen", "E\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "D\u00e4mmerbrauner Vergangenheit", "tokens": ["D\u00e4m\u00b7mer\u00b7brau\u00b7ner", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mag wohl von verklungenem Gl\u00fcck", "tokens": ["Mag", "wohl", "von", "ver\u00b7klun\u00b7ge\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Blinkend ein Bl\u00e4ttchen stecken.", "tokens": ["Blin\u00b7kend", "ein", "Bl\u00e4tt\u00b7chen", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und ich suche in meinem Andenkenkasten.", "tokens": ["Und", "ich", "su\u00b7che", "in", "mei\u00b7nem", "An\u00b7den\u00b7ken\u00b7kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zwischen B\u00e4ndern und Briefen,", "tokens": ["Zwi\u00b7schen", "B\u00e4n\u00b7dern", "und", "Brie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die lange schliefen,", "tokens": ["Die", "lan\u00b7ge", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus trockenen Blumen und blassen Schleifen", "tokens": ["Aus", "tro\u00b7cke\u00b7nen", "Blu\u00b7men", "und", "blas\u00b7sen", "Schlei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Will ich mir was Liebes greifen.", "tokens": ["Will", "ich", "mir", "was", "Lie\u00b7bes", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da fand einen Zettel ich, bleistiftbeschrieben,", "tokens": ["Da", "fand", "ei\u00b7nen", "Zet\u00b7tel", "ich", ",", "blei\u00b7stift\u00b7be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der hat mir die W\u00e4rme ins Herz getrieben.", "tokens": ["Der", "hat", "mir", "die", "W\u00e4r\u00b7me", "ins", "Herz", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Was stand denn da?", "tokens": ["Was", "stand", "denn", "da", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Von meiner Hand:", "tokens": ["Von", "mei\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "I mag Di gern leid'n; Du: Magst Du mi aa?,", "tokens": ["I", "mag", "Di", "gern", "lei\u00b7d'n", ";", "Du", ":", "Magst", "Du", "mi", "aa", "?", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["XY", "VMFIN", "NE", "ADV", "VVINF", "$.", "PPER", "$.", "VMFIN", "PPER", "FM", "FM", "$.", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "In schm\u00e4chtigen Z\u00fcgen darunter stand:", "tokens": ["In", "schm\u00e4ch\u00b7ti\u00b7gen", "Z\u00fc\u00b7gen", "da\u00b7run\u00b7ter", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "In L\u00e4rm und Last,", "tokens": ["In", "L\u00e4rm", "und", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In zager Zeit", "tokens": ["In", "za\u00b7ger", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "War mir ein Gast", "tokens": ["War", "mir", "ein", "Gast"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Aus Gl\u00fcckseligkeit", "tokens": ["Aus", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dies kleine Ja der Vergangenheit.", "tokens": ["Dies", "klei\u00b7ne", "Ja", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}