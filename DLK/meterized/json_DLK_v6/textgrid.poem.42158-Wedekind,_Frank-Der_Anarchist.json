{"textgrid.poem.42158": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Anarchist", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reicht mir in der Todesstunde", "tokens": ["Reicht", "mir", "in", "der", "To\u00b7dess\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht in Gnaden den Pokal!", "tokens": ["Nicht", "in", "Gna\u00b7den", "den", "Po\u00b7kal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Weibes hei\u00dfem Munde", "tokens": ["Von", "des", "Wei\u00b7bes", "hei\u00b7\u00dfem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mich trinken noch einmal!", "tokens": ["La\u00dft", "mich", "trin\u00b7ken", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6gt ihr sinnlos euch berauschen,", "tokens": ["M\u00f6gt", "ihr", "sinn\u00b7los", "euch", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein Blut zerrinnt im Sand.", "tokens": ["Wenn", "mein", "Blut", "zer\u00b7rinnt", "im", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Ku\u00df mag sie nicht tauschen,", "tokens": ["Mei\u00b7nen", "Ku\u00df", "mag", "sie", "nicht", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht f\u00fcr Brot aus Henkershand.", "tokens": ["Nicht", "f\u00fcr", "Brot", "aus", "Hen\u00b7ker\u00b7shand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen Sohn wird sie geb\u00e4ren,", "tokens": ["Ei\u00b7nen", "Sohn", "wird", "sie", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem mein Kreuz im Herzen steht,", "tokens": ["Dem", "mein", "Kreuz", "im", "Her\u00b7zen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr seiner Mutter Z\u00e4hren", "tokens": ["Der", "f\u00fcr", "sei\u00b7ner", "Mut\u00b7ter", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eurer Kinder H\u00e4upter m\u00e4ht.", "tokens": ["Eu\u00b7rer", "Kin\u00b7der", "H\u00e4up\u00b7ter", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}