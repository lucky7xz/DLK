{"textgrid.poem.31633": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesu! schweb' in meinen Sinnen /", "tokens": ["Je\u00b7su", "!", "schweb'", "in", "mei\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie die Erd' im Luffte-Krei\u00df.", "tokens": ["wie", "die", "Erd'", "im", "Luff\u00b7te\u00b7Krei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Segne meiner Wert beginnen.", "tokens": ["Seg\u00b7ne", "mei\u00b7ner", "Wert", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schenk mir / dir zu Prei\u00df / den Prei\u00df.", "tokens": ["Schenk", "mir", "/", "dir", "zu", "Prei\u00df", "/", "den", "Prei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "APPR", "NN", "$(", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey mein Mittel-Punct und Ziel /", "tokens": ["Sey", "mein", "Mit\u00b7tel\u00b7Punct", "und", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df (ich mache was ich will / )", "tokens": ["da\u00df", "(", "ich", "ma\u00b7che", "was", "ich", "will", "/", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$(", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "mu\u00df mit dir mein Herz anheben /", "tokens": ["mu\u00df", "mit", "dir", "mein", "Herz", "an\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mit dir enden auch / mein Leben!", "tokens": ["mit", "dir", "en\u00b7den", "auch", "/", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}