{"dta.poem.2162": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungl\u00fccklicher Zustand eines  \n Atheisten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Unterscheid, der zwischen uns, (die wir ein", "tokens": ["Der", "Un\u00b7ter\u00b7scheid", ",", "der", "zwi\u00b7schen", "uns", ",", "(", "die", "wir", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "$,", "$(", "PRELS", "PPER", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "g\u00f6ttliches Regieren", "tokens": ["g\u00f6tt\u00b7li\u00b7ches", "Re\u00b7gie\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In allen Dingen feste gl\u00e4uben,) und etwan einer Seel\u2019", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "fes\u00b7te", "gl\u00e4u\u00b7ben", ",", ")", "und", "et\u00b7wan", "ei\u00b7ner", "Seel'"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "VVINF", "$,", "$(", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zu finden,", "tokens": ["zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die eines solchen Trosts beraubt, ist ziemlich deutlich", "tokens": ["Die", "ei\u00b7nes", "sol\u00b7chen", "Trosts", "be\u00b7raubt", ",", "ist", "ziem\u00b7lich", "deut\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "PIAT", "NN", "VVPP", "$,", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zu versp\u00fcren,", "tokens": ["zu", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wenn wir den Zustand eines Menschen, der schwerer", "tokens": ["Wenn", "wir", "den", "Zu\u00b7stand", "ei\u00b7nes", "Men\u00b7schen", ",", "der", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Sorgen voll, ergr\u00fcnden,", "tokens": ["Sor\u00b7gen", "voll", ",", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wenn er, halb schlafend und halb wachend, mit ganz", "tokens": ["Wenn", "er", ",", "halb", "schla\u00b7fend", "und", "halb", "wa\u00b7chend", ",", "mit", "ganz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "ADJD", "KON", "ADJD", "VVPP", "$,", "APPR", "ADV"], "meter": "--+---+---+", "measure": "anapaest.init"}, "line.10": {"text": "benebeltem Gem\u00fcth", "tokens": ["be\u00b7ne\u00b7bel\u00b7tem", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Von Kummer, Gram und Harm umgeben, fast nichts,", "tokens": ["Von", "Kum\u00b7mer", ",", "Gram", "und", "Harm", "um\u00b7ge\u00b7ben", ",", "fast", "nichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,", "ADV", "PIS", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "als schwarze Larven, sieht,", "tokens": ["als", "schwar\u00b7ze", "Lar\u00b7ven", ",", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Nur gr\u00e4mliche Jdeen zeugt, durch deren Schatten gar", "tokens": ["Nur", "gr\u00e4m\u00b7li\u00b7che", "Jdeen", "zeugt", ",", "durch", "de\u00b7ren", "Schat\u00b7ten", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,", "APPR", "PRELAT", "NN", "ADV"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.14": {"text": "kein Licht", "tokens": ["kein", "Licht"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Von einiger erheiternden und trosterf\u00fcllten Hoffnung", "tokens": ["Von", "ei\u00b7ni\u00b7ger", "er\u00b7hei\u00b7tern\u00b7den", "und", "tros\u00b7ter\u00b7f\u00fcll\u00b7ten", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "bricht,", "tokens": ["bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Wenn die ihn in best\u00e4ndiger Verwirrung, als im Zirkel,", "tokens": ["Wenn", "die", "ihn", "in", "be\u00b7st\u00e4n\u00b7di\u00b7ger", "Ver\u00b7wir\u00b7rung", ",", "als", "im", "Zir\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "jagen,", "tokens": ["ja\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Das Hirn mit finstrer Schwermuth f\u00fcllen, uns lauter", "tokens": ["Das", "Hirn", "mit", "finst\u00b7rer", "Schwer\u00b7muth", "f\u00fcl\u00b7len", ",", "uns", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,", "PPER", "PIAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "vorgeseh'ne Plagen", "tokens": ["vor\u00b7ge\u00b7seh'\u00b7ne", "Pla\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Jm Kopf sich gleichsam mahlend w\u00e4lzen. Dergleichen", "tokens": ["Jm", "Kopf", "sich", "gleich\u00b7sam", "mah\u00b7lend", "w\u00e4l\u00b7zen", ".", "Derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PRF", "ADJD", "ADJD", "VVINF", "$.", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Widrigkeiten schwinden,", "tokens": ["Wid\u00b7rig\u00b7kei\u00b7ten", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Wenn wir uns, wenn wir aufgewacht, in einem andern", "tokens": ["Wenn", "wir", "uns", ",", "wenn", "wir", "auf\u00b7ge\u00b7wacht", ",", "in", "ei\u00b7nem", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Stande sehn,", "tokens": ["Stan\u00b7de", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Und den obangef\u00fchrten Trost, da\u00df alle Dinge, die ge-", "tokens": ["Und", "den", "o\u00b7ban\u00b7ge\u00b7f\u00fchr\u00b7ten", "Trost", ",", "da\u00df", "al\u00b7le", "Din\u00b7ge", ",", "die", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "PIAT", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "schehn,", "tokens": ["schehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Von einem Gott regieret werden, mit unsrer Sorgen", "tokens": ["Von", "ei\u00b7nem", "Gott", "re\u00b7gie\u00b7ret", "wer\u00b7den", ",", "mit", "uns\u00b7rer", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VAINF", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Heer verbinden;", "tokens": ["Heer", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Es wird dadurch der finstre Nebel, als wie durch einen", "tokens": ["Es", "wird", "da\u00b7durch", "der", "finst\u00b7re", "Ne\u00b7bel", ",", "als", "wie", "durch", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Glanz, zerstreut;", "tokens": ["Glanz", ",", "zer\u00b7streut", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Man wird dadurch zugleich geschickt, bequemre Mittel", "tokens": ["Man", "wird", "da\u00b7durch", "zu\u00b7gleich", "ge\u00b7schickt", ",", "be\u00b7quem\u00b7re", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "PAV", "ADV", "VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "zu ersinnen,", "tokens": ["zu", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Das Ungl\u00fcck von uns abzuwenden; wodurch wir Linde-", "tokens": ["Das", "Un\u00b7gl\u00fcck", "von", "uns", "ab\u00b7zu\u00b7wen\u00b7den", ";", "wo\u00b7durch", "wir", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVIZU", "$.", "PWAV", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "rung gewinnen,", "tokens": ["rung", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.35": {"text": "Zumal uns dann die holde Hoffnung, die blo\u00df auf Gott", "tokens": ["Zu\u00b7mal", "uns", "dann", "die", "hol\u00b7de", "Hoff\u00b7nung", ",", "die", "blo\u00df", "auf", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "sich fu\u00dft, erfreut.", "tokens": ["sich", "fu\u00dft", ",", "er\u00b7freut", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Da gegentheils ein Atheist in seinem Schwermuthschlum-", "tokens": ["Da", "ge\u00b7gen\u00b7theils", "ein", "A\u00b7theist", "in", "sei\u00b7nem", "Schwer\u00b7muth\u00b7schlum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "mer bleibet,", "tokens": ["mer", "blei\u00b7bet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Und aus der dicken Finsterni\u00df der Schwermuth, weder", "tokens": ["Und", "aus", "der", "di\u00b7cken", "Fins\u00b7ter\u00b7ni\u00df", "der", "Schwer\u00b7muth", ",", "we\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Tag noch Nacht,", "tokens": ["Tag", "noch", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Von einer steten Last gedruckt, wenn er gleich wacht,", "tokens": ["Von", "ei\u00b7ner", "ste\u00b7ten", "Last", "ge\u00b7druckt", ",", "wenn", "er", "gleich", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "doch nicht erwacht,", "tokens": ["doch", "nicht", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Da, statt der Hoffnung, Furcht und Zweifel ihn stets", "tokens": ["Da", ",", "statt", "der", "Hoff\u00b7nung", ",", "Furcht", "und", "Zwei\u00b7fel", "ihn", "stets"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUI", "ART", "NN", "$,", "NN", "KON", "NN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.44": {"text": "im Unmuthskreise treibet.", "tokens": ["im", "Un\u00b7muths\u00b7krei\u00b7se", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}