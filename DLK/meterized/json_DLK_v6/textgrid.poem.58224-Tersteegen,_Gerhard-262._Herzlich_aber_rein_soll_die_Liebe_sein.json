{"textgrid.poem.58224": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "262. Herzlich aber rein soll die Liebe sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df die Br\u00fcder herzlich lieben,", "tokens": ["Man", "mu\u00df", "die", "Br\u00fc\u00b7der", "herz\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch lieb sie rein, in Gott allein,", "tokens": ["Doch", "lieb", "sie", "rein", ",", "in", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADJD", "$,", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst hindert's nur, sonst bringt's Betr\u00fcben,", "tokens": ["Sonst", "hin\u00b7dert's", "nur", ",", "sonst", "bringt's", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-++-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Was g\u00f6ttlich ist, macht ", "tokens": ["Was", "g\u00f6tt\u00b7lich", "ist", ",", "macht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}