{"dta.poem.20532": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem/ der sich vergn\u00fcget/", "tokens": ["Wohl", "dem", "/", "der", "sich", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "PRELS", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und freundlichkeit stets seine freundin nennt/", "tokens": ["Und", "freund\u00b7lich\u00b7keit", "stets", "sei\u00b7ne", "freun\u00b7din", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nach begierd\u2019 und geitz nicht als an ketten lieget/", "tokens": ["Der", "nach", "be\u00b7gierd'", "und", "geitz", "nicht", "als", "an", "ket\u00b7ten", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "VVIMP", "PTKNEG", "KOKOM", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den freunde wohlfahrt nicht als eine nessel brennt:", "tokens": ["Den", "freun\u00b7de", "wohl\u00b7fahrt", "nicht", "als", "ei\u00b7ne", "nes\u00b7sel", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Freud und vergn\u00fcgung kan den wermuth-safft vers\u00fcssen/", "tokens": ["Freud", "und", "ver\u00b7gn\u00fc\u00b7gung", "kan", "den", "wer\u00b7muth\u00b7\u00b7safft", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ART", "NN", "VVPP", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Und traurigkeit verbleibt des todes schulter-k\u00fcssen.", "tokens": ["Und", "trau\u00b7rig\u00b7keit", "ver\u00b7bleibt", "des", "to\u00b7des", "schul\u00b7ter\u00b7k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nichts kan hier ewig w\u00e4hren/", "tokens": ["Nichts", "kan", "hier", "e\u00b7wig", "w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der grause sturm mu\u00df endlich doch vergehn;", "tokens": ["Der", "grau\u00b7se", "sturm", "mu\u00df", "end\u00b7lich", "doch", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des unfalls f\u00e4ssel will der zeiten rost verzehren/", "tokens": ["Des", "un\u00b7falls", "f\u00e4s\u00b7sel", "will", "der", "zei\u00b7ten", "rost", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die morgen-r\u00f6the selbst will aus der nacht entstehn/", "tokens": ["Die", "mor\u00b7gen\u00b7r\u00f6\u00b7the", "selbst", "will", "aus", "der", "nacht", "ent\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den strauch/ darauff man itzt nur dornen kan versp\u00fcren/", "tokens": ["Den", "strauch", "/", "dar\u00b7auff", "man", "itzt", "nur", "dor\u00b7nen", "kan", "ver\u00b7sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PAV", "PIS", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird bald ein rosen-knopff von hundert bl\u00e4ttern zieren.", "tokens": ["Wird", "bald", "ein", "ro\u00b7sen\u00b7knopff", "von", "hun\u00b7dert", "bl\u00e4t\u00b7tern", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "CARD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein auffgeweckt gem\u00fcthe", "tokens": ["Ein", "auff\u00b7ge\u00b7weckt", "ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verzaget nicht/ wenn schwerer donner kracht/", "tokens": ["Ver\u00b7za\u00b7get", "nicht", "/", "wenn", "schwe\u00b7rer", "don\u00b7ner", "kracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "KOUS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es anckert stets getrost auff seines sch\u00f6pffers g\u00fcte/", "tokens": ["Es", "an\u00b7ckert", "stets", "ge\u00b7trost", "auff", "sei\u00b7nes", "sch\u00f6pf\u00b7fers", "g\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mehrmahls ha\u00df zu lust und gifft zu labsal macht:", "tokens": ["Der", "mehr\u00b7mahls", "ha\u00df", "zu", "lust", "und", "gifft", "zu", "lab\u00b7sal", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKZU", "VVFIN", "KON", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein centner ungedult ist kein so kr\u00e4fftig st\u00fccke/", "tokens": ["Ein", "cent\u00b7ner", "un\u00b7ge\u00b7dult", "ist", "kein", "so", "kr\u00e4ff\u00b7tig", "st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df es vertreiben k\u00f6nt\u2019 ein qventgen ungel\u00fccke.", "tokens": ["Da\u00df", "es", "ver\u00b7trei\u00b7ben", "k\u00f6nt'", "ein", "qvent\u00b7gen", "un\u00b7ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sein eigen hertze fressen", "tokens": ["Sein", "ei\u00b7gen", "hert\u00b7ze", "fres\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine kost/ die fleisch und witz verzehrt/", "tokens": ["Ist", "ei\u00b7ne", "kost", "/", "die", "fleisch", "und", "witz", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "ART", "NN", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der hat gantz GOttes macht und menschen pflicht vergessen/", "tokens": ["Der", "hat", "gantz", "Got\u00b7tes", "macht", "und", "men\u00b7schen", "pflicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "VVFIN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sich mit kummer-brod und thr\u00e4nen-wasser nehrt.", "tokens": ["So", "sich", "mit", "kum\u00b7mer\u00b7brod", "und", "thr\u00e4\u00b7nen\u00b7was\u00b7ser", "nehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein leichter fliegen-fu\u00df kan narren traurig machen/", "tokens": ["Ein", "leich\u00b7ter", "flie\u00b7gen\u00b7fu\u00df", "kan", "nar\u00b7ren", "trau\u00b7rig", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ein erhabner geist wird auff den dornen lachen.", "tokens": ["Und", "ein", "er\u00b7hab\u00b7ner", "geist", "wird", "auff", "den", "dor\u00b7nen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der sch\u00f6nheit edles prangen", "tokens": ["Der", "sch\u00f6n\u00b7heit", "ed\u00b7les", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut eifersucht wie schierlings blumen an/", "tokens": ["Schaut", "ei\u00b7fer\u00b7sucht", "wie", "schier\u00b7lings", "blu\u00b7men", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ungez\u00e4hmte lust/ was anders zu erlangen/", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "lust", "/", "was", "an\u00b7ders", "zu", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht/ da\u00df der alte mann nichts recht geniessen kan.", "tokens": ["Macht", "/", "da\u00df", "der", "al\u00b7te", "mann", "nichts", "recht", "ge\u00b7nies\u00b7sen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "ADJA", "NN", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wer sich begierd\u2019 und geitz l\u00e4st hertz und sinne binden/", "tokens": ["Wer", "sich", "be\u00b7gierd'", "und", "geitz", "l\u00e4st", "hertz", "und", "sin\u00b7ne", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "NN", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der wird verdru\u00df und angst bey lust und reichthum finden.", "tokens": ["Der", "wird", "ver\u00b7dru\u00df", "und", "angst", "bey", "lust", "und", "reicht\u00b7hum", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein hertze voller freude", "tokens": ["Ein", "hert\u00b7ze", "vol\u00b7ler", "freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heist scharffes saltz carnari-zucker seyn/", "tokens": ["Heist", "scharf\u00b7fes", "saltz", "car\u00b7na\u00b7ri\u00b7zu\u00b7cker", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein wasser wird zu wein/ sein garn zu weisser seide/", "tokens": ["Sein", "was\u00b7ser", "wird", "zu", "wein", "/", "sein", "garn", "zu", "weis\u00b7ser", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$(", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein blasser monden-blick wird ihm zum sonnen-schein.", "tokens": ["Ein", "blas\u00b7ser", "mon\u00b7den\u00b7blick", "wird", "ihm", "zum", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer sich vergn\u00fcgen kan/ schmeckt nichts als ambra-kuchen/", "tokens": ["Wer", "sich", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/", "schmeckt", "nichts", "als", "am\u00b7bra\u00b7ku\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVINF", "VMFIN", "$(", "VVFIN", "PIS", "KOKOM", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unverg\u00e4nglichkeit bringt lauter marter-wochen.", "tokens": ["Und", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "bringt", "lau\u00b7ter", "mar\u00b7ter\u00b7wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was nutzen sch\u00e4tz\u2019 und g\u00fcter?", "tokens": ["Was", "nut\u00b7zen", "sch\u00e4tz'", "und", "g\u00fc\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hilfft uns doch viel sch\u00f6nheit/ ehr und pracht?", "tokens": ["Was", "hilfft", "uns", "doch", "viel", "sch\u00f6n\u00b7heit", "/", "ehr", "und", "pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$(", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergn\u00fcgung ist allein der reichthum der gem\u00fcther/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "ist", "al\u00b7lein", "der", "reicht\u00b7hum", "der", "ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der bleibet ewig arm/ der stets nach mehrentracht.", "tokens": ["Der", "blei\u00b7bet", "e\u00b7wig", "arm", "/", "der", "stets", "nach", "meh\u00b7ren\u00b7tracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADJD", "$(", "ART", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wem nicht durch unlusts-gifft des geistes kr\u00e4ffte schwinden/", "tokens": ["Wem", "nicht", "durch", "un\u00b7lust\u00b7s\u00b7gifft", "des", "geis\u00b7tes", "kr\u00e4ff\u00b7te", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Der wird sein paradie\u00df auch in der w\u00fcsten finden.", "tokens": ["Der", "wird", "sein", "pa\u00b7ra\u00b7die\u00df", "auch", "in", "der", "w\u00fcs\u00b7ten", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}