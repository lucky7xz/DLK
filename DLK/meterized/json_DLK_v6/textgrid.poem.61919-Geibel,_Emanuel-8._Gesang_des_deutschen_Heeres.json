{"textgrid.poem.61919": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8. Gesang des deutschen Heeres", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schw\u00f6ren wir, getreuen Muts", "tokens": ["So", "schw\u00f6\u00b7ren", "wir", ",", "ge\u00b7treu\u00b7en", "Muts"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kampf und Todeswehen", "tokens": ["In", "Kampf", "und", "To\u00b7des\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf den letzten Tropfen Bluts", "tokens": ["Bis", "auf", "den", "letz\u00b7ten", "Trop\u00b7fen", "Bluts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Aus West und Ost, aus S\u00fcd und Nord,", "tokens": ["Aus", "West", "und", "Ost", ",", "aus", "S\u00fcd", "und", "Nord", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Deutschland hei\u00dft das Losungswort,", "tokens": ["Deutschland", "hei\u00dft", "das", "Lo\u00b7sungs\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hie deutsches Reich f\u00fcr immer!", "tokens": ["Hie", "deut\u00b7sches", "Reich", "f\u00fcr", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir fragen nichts nach Ruhm und Glanz,", "tokens": ["Wir", "fra\u00b7gen", "nichts", "nach", "Ruhm", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sind gar bald verdorben;", "tokens": ["Die", "sind", "gar", "bald", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns hat die Not des Vaterlands,", "tokens": ["Uns", "hat", "die", "Not", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die harte Not geworben.", "tokens": ["Die", "har\u00b7te", "Not", "ge\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Weib und Kind, f\u00fcr Haus und Herd", "tokens": ["F\u00fcr", "Weib", "und", "Kind", ",", "f\u00fcr", "Haus", "und", "Herd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Z\u00fcckten wir das scharfe Schwert,", "tokens": ["Z\u00fcck\u00b7ten", "wir", "das", "schar\u00b7fe", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu siegen oder zu sterben.", "tokens": ["Zu", "sie\u00b7gen", "o\u00b7der", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Komm an denn, Feind, wenn deutsches Mark", "tokens": ["Komm", "an", "denn", ",", "Feind", ",", "wenn", "deut\u00b7sches", "Mark"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "$,", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu sp\u00fcren dich gel\u00fcstet!", "tokens": ["Zu", "sp\u00fc\u00b7ren", "dich", "ge\u00b7l\u00fcs\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hie steht ein Volk in Eintracht stark,", "tokens": ["Hie", "steht", "ein", "Volk", "in", "Ein\u00b7tracht", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Gottes Kraft ger\u00fcstet.", "tokens": ["In", "Got\u00b7tes", "Kraft", "ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schmettre, Kriegsposaunenklang!", "tokens": ["Schmett\u00b7re", ",", "Kriegs\u00b7po\u00b7sau\u00b7nen\u00b7klang", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brause, brause, Schlachtgesang!", "tokens": ["Brau\u00b7se", ",", "brau\u00b7se", ",", "Schlacht\u00b7ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hie deutsches Reich f\u00fcr immer!", "tokens": ["Hie", "deut\u00b7sches", "Reich", "f\u00fcr", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}