{"textgrid.poem.66546": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter all des Tags Gestalten", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter all des Tags Gestalten", "tokens": ["Un\u00b7ter", "all", "des", "Tags", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt wohl eine, die geweiht", "tokens": ["Kommt", "wohl", "ei\u00b7ne", ",", "die", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns ber\u00fchrt mit Zauberwalten,", "tokens": ["Uns", "be\u00b7r\u00fchrt", "mit", "Zau\u00b7ber\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie zu reiner Festlichkeit.", "tokens": ["Wie", "zu", "rei\u00b7ner", "Fest\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So ergr\u00fcnd' ich wohl vergebens,", "tokens": ["So", "er\u00b7gr\u00fcnd'", "ich", "wohl", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was durch dich den Tag vers\u00f6hnt,", "tokens": ["Was", "durch", "dich", "den", "Tag", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das nur f\u00fchl' ich, da\u00df des Lebens", "tokens": ["Das", "nur", "f\u00fchl'", "ich", ",", "da\u00df", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganzer Inhalt sich versch\u00f6nt.", "tokens": ["Gan\u00b7zer", "In\u00b7halt", "sich", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}