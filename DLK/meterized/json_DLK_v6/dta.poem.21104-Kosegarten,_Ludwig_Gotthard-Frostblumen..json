{"dta.poem.21104": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Frostblumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, im Dufte der Fenstern, wie blinket die               ", "tokens": ["Sieh", ",", "im", "Duf\u00b7te", "der", "Fens\u00b7tern", ",", "wie", "blin\u00b7ket", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blume des Frostes!", "tokens": ["Blu\u00b7me", "des", "Fros\u00b7tes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vater Jennern umarmt neckend der fr\u00f6hliche", "tokens": ["Va\u00b7ter", "Jen\u00b7nern", "um\u00b7armt", "ne\u00b7ckend", "der", "fr\u00f6h\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "May.", "tokens": ["May", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So umarme mich einst, Erinnerung sch\u00f6nerer", "tokens": ["So", "um\u00b7ar\u00b7me", "mich", "einst", ",", "E\u00b7rin\u00b7ne\u00b7rung", "sch\u00f6\u00b7ne\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "ADJA"], "meter": "--+-++-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Tage!", "tokens": ["Ta\u00b7ge", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So umschmeichle dereinst kosend den trau-", "tokens": ["So", "um\u00b7schmeich\u00b7le", "de\u00b7reinst", "ko\u00b7send", "den", "trau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "TRUNC"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "renden Greis!", "tokens": ["ren\u00b7den", "Greis", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}}}}}