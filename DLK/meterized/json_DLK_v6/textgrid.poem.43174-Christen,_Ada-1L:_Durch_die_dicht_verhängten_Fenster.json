{"textgrid.poem.43174": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die dicht verh\u00e4ngten Fenster", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die dicht verh\u00e4ngten Fenster", "tokens": ["Durch", "die", "dicht", "ver\u00b7h\u00e4ng\u00b7ten", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt das dumpfe Wagenrollen,", "tokens": ["Dringt", "das", "dump\u00b7fe", "Wa\u00b7gen\u00b7rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verscheucht die Nachtgespenster,", "tokens": ["Und", "ver\u00b7scheucht", "die", "Nacht\u00b7ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Traum mir nahen wollen.", "tokens": ["Die", "im", "Traum", "mir", "na\u00b7hen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "ADJA", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber rauschend durch mein Zimmer", "tokens": ["A\u00b7ber", "rau\u00b7schend", "durch", "mein", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogt ein Meer von wirren T\u00f6nen,", "tokens": ["Wogt", "ein", "Meer", "von", "wir\u00b7ren", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus all' dem Schmerzgewimmer", "tokens": ["Und", "aus", "all'", "dem", "Schmerz\u00b7ge\u00b7wim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "H\u00f6r' ich meine Seele st\u00f6hnen!", "tokens": ["H\u00f6r'", "ich", "mei\u00b7ne", "See\u00b7le", "st\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r' ich meine Seele weinen \u2013", "tokens": ["H\u00f6r'", "ich", "mei\u00b7ne", "See\u00b7le", "wei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht um dieses Leibes Sterben \u2013", "tokens": ["Nicht", "um", "die\u00b7ses", "Lei\u00b7bes", "Ster\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es bangt ihr vor dem kleinen,", "tokens": ["Doch", "es", "bangt", "ihr", "vor", "dem", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcden, einsamen Verderben.", "tokens": ["M\u00fc\u00b7den", ",", "ein\u00b7sa\u00b7men", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}}}}