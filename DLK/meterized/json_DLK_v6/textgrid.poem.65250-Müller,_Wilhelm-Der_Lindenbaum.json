{"textgrid.poem.65250": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Lindenbaum", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Brunnen vor dem Thore", "tokens": ["Am", "Brun\u00b7nen", "vor", "dem", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein Lindenbaum:", "tokens": ["Da", "steht", "ein", "Lin\u00b7den\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich tr\u00e4umt' in seinem Schatten", "tokens": ["Ich", "tr\u00e4umt'", "in", "sei\u00b7nem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So manchen s\u00fc\u00dfen Traum.", "tokens": ["So", "man\u00b7chen", "s\u00fc\u00b7\u00dfen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schnitt in seine Rinde", "tokens": ["Ich", "schnitt", "in", "sei\u00b7ne", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So manches liebe Wort;", "tokens": ["So", "man\u00b7ches", "lie\u00b7be", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zog in Freud' und Leide", "tokens": ["Es", "zog", "in", "Freud'", "und", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihm mich immer fort.", "tokens": ["Zu", "ihm", "mich", "im\u00b7mer", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00dft' auch heute wandern", "tokens": ["Ich", "mu\u00dft'", "auch", "heu\u00b7te", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vorbei in tiefer Nacht,", "tokens": ["Vor\u00b7bei", "in", "tie\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hab' ich noch im Dunkel", "tokens": ["Da", "hab'", "ich", "noch", "im", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen zugemacht.", "tokens": ["Die", "Au\u00b7gen", "zu\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und seine Zweige rauschten,", "tokens": ["Und", "sei\u00b7ne", "Zwei\u00b7ge", "rauschten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als riefen sie mir zu:", "tokens": ["Als", "rie\u00b7fen", "sie", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm her zu mir, Geselle,", "tokens": ["Komm", "her", "zu", "mir", ",", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier findst du deine Ruh'!", "tokens": ["Hier", "findst", "du", "dei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die kalten Winde bliesen", "tokens": ["Die", "kal\u00b7ten", "Win\u00b7de", "blie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir grad' in's Angesicht,", "tokens": ["Mir", "grad'", "in's", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Hut flog mir vom Kopfe,", "tokens": ["Der", "Hut", "flog", "mir", "vom", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wendete mich nicht.", "tokens": ["Ich", "wen\u00b7de\u00b7te", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun bin ich manche Stunde", "tokens": ["Nun", "bin", "ich", "man\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entfernt von jenem Ort,", "tokens": ["Ent\u00b7fernt", "von", "je\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer h\u00f6r' ich's rauschen:", "tokens": ["Und", "im\u00b7mer", "h\u00f6r'", "ich's", "rau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du f\u00e4ndest Ruhe dort!", "tokens": ["Du", "f\u00e4n\u00b7dest", "Ru\u00b7he", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}