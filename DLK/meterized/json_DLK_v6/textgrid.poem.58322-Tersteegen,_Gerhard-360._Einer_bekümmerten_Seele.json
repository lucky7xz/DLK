{"textgrid.poem.58322": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "360. Einer bek\u00fcmmerten Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df fallen, was dich qu\u00e4lt,", "tokens": ["La\u00df", "fal\u00b7len", ",", "was", "dich", "qu\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Klag Jesu, was dir fehlt;", "tokens": ["Klag", "Je\u00b7su", ",", "was", "dir", "fehlt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Helfer ist nicht ferne,", "tokens": ["Dein", "Hel\u00b7fer", "ist", "nicht", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er hilft uns ja so gerne,", "tokens": ["Er", "hilft", "uns", "ja", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df dich nur seiner Hand", "tokens": ["La\u00df", "dich", "nur", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und gib ihm Herz und Willen,", "tokens": ["Und", "gib", "ihm", "Herz", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df seinen Zug dich stillen,", "tokens": ["La\u00df", "sei\u00b7nen", "Zug", "dich", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So brechen alle Band'!", "tokens": ["So", "bre\u00b7chen", "al\u00b7le", "Band'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}