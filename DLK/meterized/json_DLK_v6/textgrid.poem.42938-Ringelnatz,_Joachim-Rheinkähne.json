{"textgrid.poem.42938": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Rheink\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Rhein durchgleiten die gro\u00dfen", "tokens": ["Den", "Rhein", "durch\u00b7glei\u00b7ten", "die", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00e4hne. Breit und flach.", "tokens": ["K\u00e4h\u00b7ne", ".", "Breit", "und", "flach", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es sitzen zwei Badehosen", "tokens": ["Es", "sit\u00b7zen", "zwei", "Ba\u00b7de\u00b7ho\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf dem hintersten Dach.", "tokens": ["Auf", "dem", "hin\u00b7ters\u00b7ten", "Dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "In diesen Hosen stecken", "tokens": ["In", "die\u00b7sen", "Ho\u00b7sen", "ste\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei M\u00e4nner, nackt und braun.", "tokens": ["Zwei", "M\u00e4n\u00b7ner", ",", "nackt", "und", "braun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lieben das Tempo der Schnecken", "tokens": ["Die", "lie\u00b7ben", "das", "Tem\u00b7po", "der", "Schne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schimpfen auf ihre Fraun.", "tokens": ["Und", "schimp\u00b7fen", "auf", "ih\u00b7re", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und mustern die fremden Weiber,", "tokens": ["Und", "mus\u00b7tern", "die", "frem\u00b7den", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die strandl\u00e4ngs promeniern.", "tokens": ["Die", "strand\u00b7l\u00e4ngs", "pro\u00b7me\u00b7ni\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Glauben doch oft nackte Leiber,", "tokens": ["Glau\u00b7ben", "doch", "oft", "nack\u00b7te", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sie an sich imponieren.", "tokens": ["Da\u00df", "sie", "an", "sich", "im\u00b7po\u00b7nie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ausgetretene Schuhe", "tokens": ["Wie", "aus\u00b7ge\u00b7tre\u00b7te\u00b7ne", "Schu\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind diese K\u00e4hne. Hat jeder Kahn", "tokens": ["Sind", "die\u00b7se", "K\u00e4h\u00b7ne", ".", "Hat", "je\u00b7der", "Kahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "$.", "VAFIN", "PIAT", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Solch friedlich h\u00e4usliche Ruhe,", "tokens": ["Solch", "fried\u00b7lich", "h\u00e4us\u00b7li\u00b7che", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat keiner das Getue", "tokens": ["Hat", "kei\u00b7ner", "das", "Ge\u00b7tue"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der preu\u00dfischen Eisenbahn.", "tokens": ["Der", "preu\u00b7\u00dfi\u00b7schen", "Ei\u00b7sen\u00b7bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "In jedem Kinderwagen", "tokens": ["In", "je\u00b7dem", "Kin\u00b7der\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Strande rollt ein Kind.", "tokens": ["Am", "Stran\u00b7de", "rollt", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Keins dieser Kinder wird fragen,", "tokens": ["Keins", "die\u00b7ser", "Kin\u00b7der", "wird", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was Schleppk\u00e4hne sind.", "tokens": ["Was", "Schlepp\u00b7k\u00e4h\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}}}}}