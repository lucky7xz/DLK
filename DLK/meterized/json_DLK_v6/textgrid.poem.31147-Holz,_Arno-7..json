{"textgrid.poem.31147": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sassen in Walhall und tranken,", "tokens": ["Sie", "sas\u00b7sen", "in", "Wal\u00b7hall", "und", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Kukukuhr schlug Eins,", "tokens": ["Die", "Ku\u00b7ku\u00b7kuhr", "schlug", "Eins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Patagonier, Inder und Franken,", "tokens": ["Pa\u00b7ta\u00b7go\u00b7nier", ",", "In\u00b7der", "und", "Fran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Confuzius, Kant und Prinz Heinz.", "tokens": ["Con\u00b7fu\u00b7zius", ",", "Kant", "und", "Prinz", "Heinz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sassen und tranken und Plato", "tokens": ["Sie", "sas\u00b7sen", "und", "tran\u00b7ken", "und", "Pla\u00b7to"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013der Windhund sass neben Silen! \u2013", "tokens": ["\u2013", "der", "Wind\u00b7hund", "sass", "ne\u00b7ben", "Si\u00b7len", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Silentium, rief er, bis Dato", "tokens": ["Si\u00b7len\u00b7ti\u00b7um", ",", "rief", "er", ",", "bis", "Da\u00b7to"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geht nichts mir \u00fcber Athen!", "tokens": ["Geht", "nichts", "mir", "\u00fc\u00b7ber", "A\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Athen mit seiner Athene", "tokens": ["A\u00b7then", "mit", "sei\u00b7ner", "A\u00b7the\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+--+----", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und Phidias, dem griechischen Kiss,", "tokens": ["Und", "Phi\u00b7di\u00b7as", ",", "dem", "grie\u00b7chi\u00b7schen", "Kiss", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Athen und notabene", "tokens": ["A\u00b7then", "und", "no\u00b7ta\u00b7be\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJA"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Seine Akropolis!", "tokens": ["Sei\u00b7ne", "Ak\u00b7ro\u00b7po\u00b7lis", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Virgil zerschlug seinen Humpen", "tokens": ["Vir\u00b7gil", "zer\u00b7schlug", "sei\u00b7nen", "Hum\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und br\u00fcllte: Rom, Hund, Rom!", "tokens": ["Und", "br\u00fcll\u00b7te", ":", "Rom", ",", "Hund", ",", "Rom", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$,", "NN", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch sein Nebenmann liess sich nicht lumpen:", "tokens": ["Auch", "sein", "Ne\u00b7ben\u00b7mann", "liess", "sich", "nicht", "lum\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "O Stadt am Gangastrom!", "tokens": ["O", "Stadt", "am", "Gan\u00b7ga\u00b7strom", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Teut Michel pries keusch Buxtehude", "tokens": ["Teut", "Mi\u00b7chel", "pries", "keusch", "Bux\u00b7te\u00b7hu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und machte dazu: Hem Hem!", "tokens": ["Und", "mach\u00b7te", "da\u00b7zu", ":", "Hem", "Hem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$.", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Salomo, der Jude,", "tokens": ["Und", "Sa\u00b7lo\u00b7mo", ",", "der", "Ju\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pl\u00e4dirte: Jerusalem!", "tokens": ["Pl\u00e4\u00b7dir\u00b7te", ":", "Je\u00b7ru\u00b7sa\u00b7lem", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Napoli vedi e mori!", "tokens": ["Na\u00b7po\u00b7li", "ve\u00b7di", "e", "mo\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein Kerl im Frack hat's geschnalzt,", "tokens": ["Ein", "Kerl", "im", "Frack", "hat's", "ge\u00b7schnalzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bis meuchlings ein frecher Mahori", "tokens": ["Bis", "meuch\u00b7lings", "ein", "fre\u00b7cher", "Ma\u00b7ho\u00b7ri"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihm gr\u00fcndlich die Suppe versalzt.", "tokens": ["Ihm", "gr\u00fcnd\u00b7lich", "die", "Sup\u00b7pe", "ver\u00b7salzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Da erhub sich vom goldenen Stuhle,", "tokens": ["Da", "er\u00b7hub", "sich", "vom", "gol\u00b7de\u00b7nen", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Das Trinkhorn in der Hand,", "tokens": ["Das", "Trink\u00b7horn", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der alte K\u00f6nig von Thule", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "von", "Thu\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und k\u00fcsste sein Burschenband.", "tokens": ["Und", "k\u00fcss\u00b7te", "sein", "Bur\u00b7schen\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Es blitzte sein Schl\u00e4ger im Weine,", "tokens": ["Es", "blitz\u00b7te", "sein", "Schl\u00e4\u00b7ger", "im", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es klang so voll, so weich:", "tokens": ["Es", "klang", "so", "voll", ",", "so", "weich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alt Heidelberg, du Feine,", "tokens": ["Alt", "Hei\u00b7del\u00b7berg", ",", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Stadt an Ehren reich!", "tokens": ["Du", "Stadt", "an", "Eh\u00b7ren", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Alt Heidelberg, du Feine \u2013", "tokens": ["Alt", "Hei\u00b7del\u00b7berg", ",", "du", "Fei\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "PPER", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie das ins Herz ihm schnitt!", "tokens": ["Wie", "das", "ins", "Herz", "ihm", "schnitt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sang es nicht mehr alleine,", "tokens": ["Er", "sang", "es", "nicht", "mehr", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zehntausend sangen es mit!", "tokens": ["Zehn\u00b7tau\u00b7send", "san\u00b7gen", "es", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Es sang es der ganze Chorus,", "tokens": ["Es", "sang", "es", "der", "gan\u00b7ze", "Cho\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Childe Harold brummte: All right!", "tokens": ["Chil\u00b7de", "Ha\u00b7rold", "brumm\u00b7te", ":", "All", "right", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und selbst der K\u00f6nig Porus", "tokens": ["Und", "selbst", "der", "K\u00f6\u00b7nig", "Po\u00b7rus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Rief: Wetter, das Ding hat Schneid!", "tokens": ["Rief", ":", "Wet\u00b7ter", ",", "das", "Ding", "hat", "Schneid", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Derweilen, draussen vorm Thore,", "tokens": ["Der\u00b7wei\u00b7len", ",", "draus\u00b7sen", "vorm", "Tho\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stand lauschend ein deutscher Scholar,", "tokens": ["Stand", "lau\u00b7schend", "ein", "deut\u00b7scher", "Scho\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der eben seiner Lore", "tokens": ["Der", "e\u00b7ben", "sei\u00b7ner", "Lo\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lachend entlaufen war.", "tokens": ["La\u00b7chend", "ent\u00b7lau\u00b7fen", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Der hatte kein W\u00f6rtlein verloren,", "tokens": ["Der", "hat\u00b7te", "kein", "W\u00f6rt\u00b7lein", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der fing einen Sonnenstrahl", "tokens": ["Der", "fing", "ei\u00b7nen", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gab ihm vertr\u00e4umt die Sporen", "tokens": ["Und", "gab", "ihm", "ver\u00b7tr\u00e4umt", "die", "Spo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ritt ins Neckarthal.", "tokens": ["Und", "ritt", "ins", "Ne\u00b7ck\u00b7ar\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Und heute, im Abendscheine,", "tokens": ["Und", "heu\u00b7te", ",", "im", "A\u00b7bend\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jeder Vogel singt es vom Blatt:", "tokens": ["Je\u00b7der", "Vo\u00b7gel", "singt", "es", "vom", "Blatt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Alt Heidelberg, du Feine,", "tokens": ["Alt", "Hei\u00b7del\u00b7berg", ",", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alt Heidelberg, du Stadt!", "tokens": ["Alt", "Hei\u00b7del\u00b7berg", ",", "du", "Stadt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}