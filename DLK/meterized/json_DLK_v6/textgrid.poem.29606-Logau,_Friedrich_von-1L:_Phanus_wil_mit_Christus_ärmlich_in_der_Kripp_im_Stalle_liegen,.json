{"textgrid.poem.29606": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Phanus wil mit Christus \u00e4rmlich in der Kripp im Stalle liegen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phanus wil mit Christus \u00e4rmlich in der Kripp im Stalle liegen,", "tokens": ["Pha\u00b7nus", "wil", "mit", "Chris\u00b7tus", "\u00e4rm\u00b7lich", "in", "der", "Kripp", "im", "Stal\u00b7le", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "NE", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann ein Stern nur wolte kummen, der es also k\u00fcnte f\u00fcgen,", "tokens": ["Wann", "ein", "Stern", "nur", "wol\u00b7te", "kum\u00b7men", ",", "der", "es", "al\u00b7so", "k\u00fcn\u00b7te", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,", "PRELS", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df die Weisen k\u00e4men her und die Sch\u00e4tze legten au\u00df,", "tokens": ["Da\u00df", "die", "Wei\u00b7sen", "k\u00e4\u00b7men", "her", "und", "die", "Sch\u00e4t\u00b7ze", "leg\u00b7ten", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und von Ochsen immer voll und von Eseln sey sein Haus.", "tokens": ["Und", "von", "Och\u00b7sen", "im\u00b7mer", "voll", "und", "von", "E\u00b7seln", "sey", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ADJD", "KON", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}