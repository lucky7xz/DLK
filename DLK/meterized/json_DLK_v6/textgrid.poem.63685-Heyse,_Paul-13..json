{"textgrid.poem.63685": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesegnet ist dies Jahr mit Niederschl\u00e4gen.", "tokens": ["Ge\u00b7seg\u00b7net", "ist", "dies", "Jahr", "mit", "Nie\u00b7der\u00b7schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PDS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaum wurden Weg und Steg im Walde trocken", "tokens": ["Kaum", "wur\u00b7den", "Weg", "und", "Steg", "im", "Wal\u00b7de", "tro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Heuchelsonne will hinaus mich locken,", "tokens": ["Und", "Heu\u00b7chel\u00b7son\u00b7ne", "will", "hin\u00b7aus", "mich", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "APZR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Treibt rasch nach Haus mich ein Gewitterregen.", "tokens": ["Treibt", "rasch", "nach", "Haus", "mich", "ein", "Ge\u00b7wit\u00b7ter\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun, wie Gott will! Ich habe nichts dagegen,", "tokens": ["Nun", ",", "wie", "Gott", "will", "!", "Ich", "ha\u00b7be", "nichts", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "VMFIN", "$.", "PPER", "VAFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch einen Tag im Zimmer zu verhocken,", "tokens": ["Auch", "ei\u00b7nen", "Tag", "im", "Zim\u00b7mer", "zu", "ver\u00b7ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fehlt ein dritter Mann nicht zum Tarocken,", "tokens": ["Und", "fehlt", "ein", "drit\u00b7ter", "Mann", "nicht", "zum", "Ta\u00b7ro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So mag die Sintflut kommen meinetwegen.", "tokens": ["So", "mag", "die", "Sint\u00b7flut", "kom\u00b7men", "mei\u00b7net\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was soll der Mensch, der tagelang vergebens", "tokens": ["Was", "soll", "der", "Mensch", ",", "der", "ta\u00b7ge\u00b7lang", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Sonne seufzte, andres auch beginnen,", "tokens": ["Nach", "Son\u00b7ne", "seufz\u00b7te", ",", "and\u00b7res", "auch", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als mildern durch ein Spiel den Ernst des Lebens?", "tokens": ["Als", "mil\u00b7dern", "durch", "ein", "Spiel", "den", "Ernst", "des", "Le\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was andres l\u00f6st den Druck von seinen Sinnen", "tokens": ["Was", "and\u00b7res", "l\u00f6st", "den", "Druck", "von", "sei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und scheint ihm noch ein w\u00fcrd'ges Ziel des Strebens,", "tokens": ["Und", "scheint", "ihm", "noch", "ein", "w\u00fcrd'\u00b7ges", "Ziel", "des", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als manchmal ein Coeursolo zu gewinnen?", "tokens": ["Als", "manch\u00b7mal", "ein", "Co\u00b7eur\u00b7so\u00b7lo", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}