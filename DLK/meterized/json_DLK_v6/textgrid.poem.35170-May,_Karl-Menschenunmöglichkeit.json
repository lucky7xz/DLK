{"textgrid.poem.35170": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Menschenunm\u00f6glichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm mit, komm mit, und folge mir;", "tokens": ["Komm", "mit", ",", "komm", "mit", ",", "und", "fol\u00b7ge", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchre dich so gern, so gern.", "tokens": ["Ich", "f\u00fch\u00b7re", "dich", "so", "gern", ",", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zeige und erkl\u00e4re dir", "tokens": ["Ich", "zei\u00b7ge", "und", "er\u00b7kl\u00e4\u00b7re", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Welt von Stern zu Stern.", "tokens": ["Die", "gan\u00b7ze", "Welt", "von", "Stern", "zu", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir fangen an beim Anbeginn", "tokens": ["Wir", "fan\u00b7gen", "an", "beim", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6ren auf beim Ende dort;", "tokens": ["Und", "h\u00f6\u00b7ren", "auf", "beim", "En\u00b7de", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir gehen gleich zu Beiden hin,", "tokens": ["Wir", "ge\u00b7hen", "gleich", "zu", "Bei\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Beide find derselbe Ort.", "tokens": ["Denn", "Bei\u00b7de", "find", "der\u00b7sel\u00b7be", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da wir bei dem Anfang schon", "tokens": ["Und", "da", "wir", "bei", "dem", "An\u00b7fang", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ende angekommen sind,", "tokens": ["Am", "En\u00b7de", "an\u00b7ge\u00b7kom\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist die Ewigkeit entflohn", "tokens": ["So", "ist", "die", "E\u00b7wig\u00b7keit", "ent\u00b7flohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie so geschwind, wie so geschwind.", "tokens": ["Wie", "so", "ge\u00b7schwind", ",", "wie", "so", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4hrend dieser Ewigkeit", "tokens": ["Und", "w\u00e4h\u00b7rend", "die\u00b7ser", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich erkl\u00e4rt wieviel, wieviel?", "tokens": ["Hab", "ich", "er\u00b7kl\u00e4rt", "wie\u00b7viel", ",", "wie\u00b7viel", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihr in eurer Spanne Zeit", "tokens": ["Und", "ihr", "in", "eu\u00b7rer", "Span\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treibt ganz genau dasselbe Spiel!", "tokens": ["Treibt", "ganz", "ge\u00b7nau", "das\u00b7sel\u00b7be", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}