{"dta.poem.3275": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Welche Gottheit soll auch mir", "tokens": ["Wel\u00b7che", "Got\u00b7theit", "soll", "auch", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VMFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Wunsch gew\u00e4hren?", "tokens": ["Ei\u00b7nen", "Wunsch", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unentschlossen irr ich hier", "tokens": ["Un\u00b7ent\u00b7schlos\u00b7sen", "irr", "ich", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen den Alt\u00e4ren.", "tokens": ["Zwi\u00b7schen", "den", "Al\u00b7t\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sorgen schw\u00e4rmen rund herum", "tokens": ["Sor\u00b7gen", "schw\u00e4r\u00b7men", "rund", "he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Gott der Sch\u00e4tze;", "tokens": ["Um", "den", "Gott", "der", "Sch\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Ehre Heiligthum", "tokens": ["Und", "der", "Eh\u00b7re", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt voll falscher Netze.", "tokens": ["Liegt", "voll", "fal\u00b7scher", "Net\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "In der Sch\u00f6nheit Schoose liegt", "tokens": ["In", "der", "Sch\u00f6n\u00b7heit", "Schoo\u00b7se", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor, der mit K\u00fcssen", "tokens": ["A\u00b7mor", ",", "der", "mit", "K\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sich an ihren Busen schmiegt,", "tokens": ["Sich", "an", "ih\u00b7ren", "Bu\u00b7sen", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da wir zittern m\u00fcssen.", "tokens": ["Da", "wir", "zit\u00b7tern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Amor soll willkommen seyn:", "tokens": ["A\u00b7mor", "soll", "will\u00b7kom\u00b7men", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch ich will nur lachen;", "tokens": ["Doch", "ich", "will", "nur", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er mu\u00df bey meinem Wein", "tokens": ["Und", "er", "mu\u00df", "bey", "mei\u00b7nem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nicht irre machen.", "tokens": ["Mich", "nicht", "ir\u00b7re", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ruhm und du, gefl\u00fcgelt Gold!", "tokens": ["Ruhm", "und", "du", ",", "ge\u00b7fl\u00fc\u00b7gelt", "Gold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "$,", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich entsag euch beyden.", "tokens": ["Ich", "ent\u00b7sag", "euch", "bey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ihr selbst mich suchen wollt;", "tokens": ["Wenn", "ihr", "selbst", "mich", "su\u00b7chen", "wollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich euch nicht meiden.", "tokens": ["Will", "ich", "euch", "nicht", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}