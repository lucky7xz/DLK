{"dta.poem.3090": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "135.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Freuden greifest du nach allen neuen Bildern", "tokens": ["Mit", "Freu\u00b7den", "grei\u00b7fest", "du", "nach", "al\u00b7len", "neu\u00b7en", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt und der Natur, was sie auch m\u00f6gen schildern,", "tokens": ["Der", "Welt", "und", "der", "Na\u00b7tur", ",", "was", "sie", "auch", "m\u00f6\u00b7gen", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht, um mit Bilderkram dein Zimmer auszuschm\u00fccken,", "tokens": ["Nicht", ",", "um", "mit", "Bil\u00b7der\u00b7kram", "dein", "Zim\u00b7mer", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUI", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sondern um deinen Sinn mit ihnen auszudr\u00fccken.", "tokens": ["Son\u00b7dern", "um", "dei\u00b7nen", "Sinn", "mit", "ih\u00b7nen", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "PPER", "VVIZU", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ob ein Gedanke nun vom Bild sei angeregt,", "tokens": ["Ob", "ein", "Ge\u00b7dan\u00b7ke", "nun", "vom", "Bild", "sei", "an\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob in das Bild ein schon Gedachtes sich gelegt;", "tokens": ["Ob", "in", "das", "Bild", "ein", "schon", "Ge\u00b7dach\u00b7tes", "sich", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADV", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df nur das sch\u00f6ne Spiel der Kunst dich nicht verdrie\u00dfen,", "tokens": ["La\u00df", "nur", "das", "sch\u00f6\u00b7ne", "Spiel", "der", "Kunst", "dich", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein eignes Innres dir in Bildern aufzuschlie\u00dfen!", "tokens": ["Dein", "eig\u00b7nes", "Inn\u00b7res", "dir", "in", "Bil\u00b7dern", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn, wie dein Auge selbst sich sieht im Spiegel nur,", "tokens": ["Denn", ",", "wie", "dein", "Au\u00b7ge", "selbst", "sich", "sieht", "im", "Spie\u00b7gel", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPOSAT", "NN", "ADV", "PRF", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So dein Gem\u00fct allein im Bilde der Natur.", "tokens": ["So", "dein", "Ge\u00b7m\u00fct", "al\u00b7lein", "im", "Bil\u00b7de", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}