{"dta.poem.12038": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht minder haben dich die Ding' als du sie hast;", "tokens": ["Nicht", "min\u00b7der", "ha\u00b7ben", "dich", "die", "Ding'", "als", "du", "sie", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "ART", "NN", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du suchest deine Lust, und findest deine Last.", "tokens": ["Du", "su\u00b7chest", "dei\u00b7ne", "Lust", ",", "und", "fin\u00b7dest", "dei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht nur dein Hab' und Gut, nicht nur dein Weib und Kind,", "tokens": ["Nicht", "nur", "dein", "Hab'", "und", "Gut", ",", "nicht", "nur", "dein", "Weib", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "KON", "ADJD", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Garten, Haus und Hof, dein Esel, Schaf und Rind;", "tokens": ["Dein", "Gar\u00b7ten", ",", "Haus", "und", "Hof", ",", "dein", "E\u00b7sel", ",", "Schaf", "und", "Rind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch deine Wissenschaft und deine Kunst vor allen,", "tokens": ["Auch", "dei\u00b7ne", "Wis\u00b7sen\u00b7schaft", "und", "dei\u00b7ne", "Kunst", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind minder dir da als du ihnen zu Gefallen.", "tokens": ["Sind", "min\u00b7der", "dir", "da", "als", "du", "ih\u00b7nen", "zu", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "KOUS", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Rath' ich deswegen dir vondannen sie zu treiben,", "tokens": ["Ra\u00b7th'", "ich", "des\u00b7we\u00b7gen", "dir", "von\u00b7dan\u00b7nen", "sie", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Da ohn' einander ihr einmal nicht k\u00f6nnet bleiben?", "tokens": ["Da", "ohn'", "ein\u00b7an\u00b7der", "ihr", "ein\u00b7mal", "nicht", "k\u00f6n\u00b7net", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich rathe nur, dich recht mit ihnen abzufinden,", "tokens": ["Ich", "ra\u00b7the", "nur", ",", "dich", "recht", "mit", "ih\u00b7nen", "ab\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "ADV", "APPR", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So den Begriff von Lust und Sorge zu verbinden,", "tokens": ["So", "den", "Be\u00b7griff", "von", "Lust", "und", "Sor\u00b7ge", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df du in ihnen mehr die Lust siehst, weil vorhanden", "tokens": ["Da\u00df", "du", "in", "ih\u00b7nen", "mehr", "die", "Lust", "siehst", ",", "weil", "vor\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie einmal sind, und mehr die Sorge, wenn sie schwanden.", "tokens": ["Sie", "ein\u00b7mal", "sind", ",", "und", "mehr", "die", "Sor\u00b7ge", ",", "wenn", "sie", "schwan\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "KON", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}