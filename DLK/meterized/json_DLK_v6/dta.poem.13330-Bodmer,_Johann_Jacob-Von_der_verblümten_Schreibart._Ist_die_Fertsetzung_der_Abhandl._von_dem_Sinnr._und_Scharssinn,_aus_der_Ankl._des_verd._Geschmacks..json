{"dta.poem.13330": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Jris farbenreicher Krantz,", "tokens": ["Der", "Jris", "far\u00b7ben\u00b7rei\u00b7cher", "Krantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Des hellen Abendsterns so lieblich reiner Glantz,", "tokens": ["Des", "hel\u00b7len", "A\u00b7bends\u00b7terns", "so", "lieb\u00b7lich", "rei\u00b7ner", "Glantz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erquicken kaum so sehr das menschliche Gesicht,", "tokens": ["Er\u00b7qui\u00b7cken", "kaum", "so", "sehr", "das", "menschli\u00b7che", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Als das auf hundert tausend Arten", "tokens": ["Als", "das", "auf", "hun\u00b7dert", "tau\u00b7send", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "APPR", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gef\u00e4rbet und gebrochne Licht", "tokens": ["Ge\u00b7f\u00e4r\u00b7bet", "und", "ge\u00b7broch\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einem bunten Blumen-Garten.", "tokens": ["Von", "ei\u00b7nem", "bun\u00b7ten", "Blu\u00b7men\u00b7Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}