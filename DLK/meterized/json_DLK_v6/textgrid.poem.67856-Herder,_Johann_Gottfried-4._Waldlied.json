{"textgrid.poem.67856": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4. Waldlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrm, st\u00fcrm, du Winterwind!", "tokens": ["St\u00fcrm", ",", "st\u00fcrm", ",", "du", "Win\u00b7ter\u00b7wind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bist doch, wie's Menschen sind,", "tokens": ["Bist", "doch", ",", "wie's", "Men\u00b7schen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein undankbarer mir!", "tokens": ["Kein", "un\u00b7dank\u00b7ba\u00b7rer", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Zahn bei\u00dft grimmig drein;", "tokens": ["Dein", "Zahn", "bei\u00dft", "grim\u00b7mig", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch warum sollt's nicht seyn?", "tokens": ["Doch", "wa\u00b7rum", "sollt's", "nicht", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hab' ich doch nichts mit dir.", "tokens": ["Hab'", "ich", "doch", "nichts", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ChOR:", "tokens": ["ChOR", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Heiho, singt Heiho, im Gr\u00fcnen hier heilig!", "tokens": ["Hei\u00b7ho", ",", "singt", "Hei\u00b7ho", ",", "im", "Gr\u00fc\u00b7nen", "hier", "hei\u00b7lig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NE", "$,", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Die Lieb' ist nur Kurzweil, die Freundschaft nicht treulich!", "tokens": ["Die", "Lieb'", "ist", "nur", "Kurz\u00b7weil", ",", "die", "Freund\u00b7schaft", "nicht", "treu\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Heiho, hier fr\u00f6hlich, dies Leben ist selig!", "tokens": ["Hei\u00b7ho", ",", "hier", "fr\u00f6h\u00b7lich", ",", "dies", "Le\u00b7ben", "ist", "se\u00b7lig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "$,", "PDS", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh durch, du Lufthauch, geh!", "tokens": ["Geh", "durch", ",", "du", "Luft\u00b7hauch", ",", "geh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stichst nimmer doch so weh,", "tokens": ["Stichst", "nim\u00b7mer", "doch", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Hohn f\u00fcr Gutthat sticht.", "tokens": ["Als", "Hohn", "f\u00fcr", "Gut\u00b7that", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du hauchst zwar Wass'r in Eis,", "tokens": ["Du", "hauchst", "zwar", "Wass'r", "in", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ist mir's Paradeis", "tokens": ["Doch", "ist", "mir's", "Pa\u00b7ra\u00b7deis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr: \u00bbFreund, ich kenn ihn nicht!\u00ab", "tokens": ["F\u00fcr", ":", "\u00bb", "Freund", ",", "ich", "kenn", "ihn", "nicht", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$.", "$(", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ChOR:", "tokens": ["ChOR", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Heiho, singt Heiho, im Gr\u00fcnen hier heilig!", "tokens": ["Hei\u00b7ho", ",", "singt", "Hei\u00b7ho", ",", "im", "Gr\u00fc\u00b7nen", "hier", "hei\u00b7lig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NE", "$,", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Die Lieb' ist nur Kurzweil, die Freundschaft nicht treulich!", "tokens": ["Die", "Lieb'", "ist", "nur", "Kurz\u00b7weil", ",", "die", "Freund\u00b7schaft", "nicht", "treu\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Heiho, hier fr\u00f6hlich, dies Leben ist selig!", "tokens": ["Hei\u00b7ho", ",", "hier", "fr\u00f6h\u00b7lich", ",", "dies", "Le\u00b7ben", "ist", "se\u00b7lig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "$,", "PDS", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}