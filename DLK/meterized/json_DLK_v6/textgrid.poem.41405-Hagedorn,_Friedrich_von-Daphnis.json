{"textgrid.poem.41405": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Daphnis", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem H\u00fcgel voller Linden", "tokens": ["An", "ei\u00b7nem", "H\u00fc\u00b7gel", "vol\u00b7ler", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df Amarill, und war bem\u00fcht,", "tokens": ["Sa\u00df", "A\u00b7ma\u00b7rill", ",", "und", "war", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Blumen einen Kranz zu winden,", "tokens": ["Aus", "Blu\u00b7men", "ei\u00b7nen", "Kranz", "zu", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang ein angenehmes Lied.", "tokens": ["Und", "sang", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie, die so manches Herz ger\u00fchret,", "tokens": ["Sie", ",", "die", "so", "man\u00b7ches", "Herz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie, vieler Seufzer einz'ges Ziel,", "tokens": ["Sie", ",", "vie\u00b7ler", "Seuf\u00b7zer", "einz'\u00b7ges", "Ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ward hier vom Daphnis ausgesp\u00fcret,", "tokens": ["Ward", "hier", "vom", "Daph\u00b7nis", "aus\u00b7ge\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der ihr vor allen wohlgefiel.", "tokens": ["Der", "ihr", "vor", "al\u00b7len", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie manches kam ihm jetzt zu statten!", "tokens": ["Wie", "man\u00b7ches", "kam", "ihm", "jetzt", "zu", "stat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lockung stiller Abendzeit,", "tokens": ["Die", "Lo\u00b7ckung", "stil\u00b7ler", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sichrer und verschwiegner Schatten,", "tokens": ["Ein", "sich\u00b7rer", "und", "ver\u00b7schwieg\u00b7ner", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mai, ein Freund der Z\u00e4rtlichkeit,", "tokens": ["Der", "Mai", ",", "ein", "Freund", "der", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Mund und Auge reich an Freuden,", "tokens": ["Ihr", "Mund", "und", "Au\u00b7ge", "reich", "an", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr ihm schon oft verrathner Sinn;", "tokens": ["Ihr", "ihm", "schon", "oft", "ver\u00b7rath\u00b7ner", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein, der Sch\u00e4fer war bescheiden,", "tokens": ["Al\u00b7lein", ",", "der", "Sch\u00e4\u00b7fer", "war", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ging nicht bis zur Sch\u00e4ferin.", "tokens": ["Und", "ging", "nicht", "bis", "zur", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hatte das Ger\u00e4usch vernommen,", "tokens": ["Sie", "hat\u00b7te", "das", "Ge\u00b7r\u00e4usch", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihren Hirten bald entdeckt.", "tokens": ["Und", "ih\u00b7ren", "Hir\u00b7ten", "bald", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lacht', und hie\u00df ihn n\u00e4her kommen,", "tokens": ["Sie", "lacht'", ",", "und", "hie\u00df", "ihn", "n\u00e4\u00b7her", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: Was hast du dich versteckt?", "tokens": ["Und", "sprach", ":", "Was", "hast", "du", "dich", "ver\u00b7steckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4ltst du aus Schalkheit dich verborgen?", "tokens": ["H\u00e4ltst", "du", "aus", "Schalk\u00b7heit", "dich", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df ich vor dir von hinnen fliehn?", "tokens": ["Mu\u00df", "ich", "vor", "dir", "von", "hin\u00b7nen", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du schweigest? Ich will nichts besorgen;", "tokens": ["Du", "schwei\u00b7gest", "?", "Ich", "will", "nichts", "be\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Dich macht die Liebe nicht zu k\u00fchn.", "tokens": ["Dich", "macht", "die", "Lie\u00b7be", "nicht", "zu", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du lernst die Furcht von deinen Schafen:", "tokens": ["Du", "lernst", "die", "Furcht", "von", "dei\u00b7nen", "Scha\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch hast du hier zu ruhen Lust,", "tokens": ["Doch", "hast", "du", "hier", "zu", "ru\u00b7hen", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So darfst du unbek\u00fcmmert schlafen", "tokens": ["So", "darfst", "du", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Arm, an dieser Brust.", "tokens": ["In", "mei\u00b7nem", "Arm", ",", "an", "die\u00b7ser", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird dir Morpheus Tr\u00e4ume senden,", "tokens": ["Es", "wird", "dir", "Mor\u00b7pheus", "Tr\u00e4u\u00b7me", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Scherz und Jugend fr\u00f6hlich macht.", "tokens": ["Die", "Scherz", "und", "Ju\u00b7gend", "fr\u00f6h\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber will den Kranz vollenden,", "tokens": ["Ich", "a\u00b7ber", "will", "den", "Kranz", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn der war dir schon zugedacht.", "tokens": ["Denn", "der", "war", "dir", "schon", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Er dankt, gehorcht, und legt sich nieder,", "tokens": ["Er", "dankt", ",", "ge\u00b7horcht", ",", "und", "legt", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn streichelt ihre sanfte Hand:", "tokens": ["Ihn", "strei\u00b7chelt", "ih\u00b7re", "sanf\u00b7te", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er streckt sich aus, und danket wieder:", "tokens": ["Er", "streckt", "sich", "aus", ",", "und", "dan\u00b7ket", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hirtenstab f\u00e4llt in den Sand.", "tokens": ["Der", "Hir\u00b7ten\u00b7stab", "f\u00e4llt", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Nachdem er sich an sie gelehnet,", "tokens": ["Nach\u00b7dem", "er", "sich", "an", "sie", "ge\u00b7leh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und, sonder Ungemach und Pein,", "tokens": ["Und", ",", "son\u00b7der", "Un\u00b7ge\u00b7mach", "und", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dreimal geseufzt, dreimal geg\u00e4hnet,", "tokens": ["Drei\u00b7mal", "ge\u00b7seufzt", ",", "drei\u00b7mal", "ge\u00b7g\u00e4h\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Schl\u00e4ft Daphnis endlich schnarchend ein.", "tokens": ["Schl\u00e4ft", "Daph\u00b7nis", "end\u00b7lich", "schnar\u00b7chend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie rafft sich auf, um wegzugehen,", "tokens": ["Sie", "rafft", "sich", "auf", ",", "um", "weg\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur sagt sie dieses noch zuletzt:", "tokens": ["Nur", "sagt", "sie", "die\u00b7ses", "noch", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zucht, die ich an dir gesehen,", "tokens": ["Die", "Zucht", ",", "die", "ich", "an", "dir", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird billig von mir hochgesch\u00e4tzt.", "tokens": ["Wird", "bil\u00b7lig", "von", "mir", "hoch\u00b7ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man mu\u00df der Tugend Lob ertheilen:", "tokens": ["Man", "mu\u00df", "der", "Tu\u00b7gend", "Lob", "er\u00b7thei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer schl\u00e4ft so sch\u00f6n, so ehrfurchtvoll?", "tokens": ["Wer", "schl\u00e4ft", "so", "sch\u00f6n", ",", "so", "ehr\u00b7furcht\u00b7voll", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich mu\u00df zu meinen Heerden eilen;", "tokens": ["Ich", "mu\u00df", "zu", "mei\u00b7nen", "Heer\u00b7den", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sittsamer Sch\u00e4fer, schlafe wohl!", "tokens": ["Sitt\u00b7sa\u00b7mer", "Sch\u00e4\u00b7fer", ",", "schla\u00b7fe", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}