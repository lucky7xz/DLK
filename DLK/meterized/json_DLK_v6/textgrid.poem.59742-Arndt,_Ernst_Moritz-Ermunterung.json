{"textgrid.poem.59742": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du sinken, nichts als sinken,", "tokens": ["Willst", "du", "sin\u00b7ken", ",", "nichts", "als", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PIS", "KOKOM", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Armes, krankes Menschenherz?", "tokens": ["Ar\u00b7mes", ",", "kran\u00b7kes", "Men\u00b7schen\u00b7herz", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer nur den Becher trinken,", "tokens": ["Im\u00b7mer", "nur", "den", "Be\u00b7cher", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den dir f\u00fcllet Sorg' und Schmerz?", "tokens": ["Den", "dir", "f\u00fcl\u00b7let", "Sor\u00b7g'", "und", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Immer alles nur in grauen,", "tokens": ["Im\u00b7mer", "al\u00b7les", "nur", "in", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwarzen Erdenfarben sehn?", "tokens": ["Schwar\u00b7zen", "Er\u00b7den\u00b7far\u00b7ben", "sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lerne doch nach oben schauen,", "tokens": ["Ler\u00b7ne", "doch", "nach", "o\u00b7ben", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die heitern Sterne gehn.", "tokens": ["Wo", "die", "hei\u00b7tern", "Ster\u00b7ne", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dahin schau'! Da ist dein Eigen,", "tokens": ["Da\u00b7hin", "schau'", "!", "Da", "ist", "dein", "Ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da dein altes Heimatland;", "tokens": ["Da", "dein", "al\u00b7tes", "Hei\u00b7mat\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dahin schau'! Und lerne steigen", "tokens": ["Da\u00b7hin", "schau'", "!", "Und", "ler\u00b7ne", "stei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "KON", "VVFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Aus dem d\u00fcrren Erdensand,", "tokens": ["Aus", "dem", "d\u00fcr\u00b7ren", "Er\u00b7den\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem tr\u00fcben Nebelstaube \u2013", "tokens": ["Aus", "dem", "tr\u00fc\u00b7ben", "Ne\u00b7bel\u00b7stau\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm den Flug und zittre nicht,", "tokens": ["Nimm", "den", "Flug", "und", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Glaube, was der Christenglaube", "tokens": ["Glau\u00b7be", ",", "was", "der", "Chris\u00b7ten\u00b7glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald zweitausend Jahre spricht.", "tokens": ["Bald", "zweit\u00b7au\u00b7send", "Jah\u00b7re", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da hinauf! Da ist dein Streiter,", "tokens": ["Da", "hin\u00b7auf", "!", "Da", "ist", "dein", "Strei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem Not und Tod zerf\u00e4llt,", "tokens": ["Vor", "dem", "Not", "und", "Tod", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dahin schau'! Und hell und heiter", "tokens": ["Da\u00b7hin", "schau'", "!", "Und", "hell", "und", "hei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "KON", "ADJD", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Bl\u00fcht dir wieder Gottes Welt \u2013", "tokens": ["Bl\u00fcht", "dir", "wie\u00b7der", "Got\u00b7tes", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaue, schau' auf diesen Einen:", "tokens": ["Schau\u00b7e", ",", "schau'", "auf", "die\u00b7sen", "Ei\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer steht der Held bereit,", "tokens": ["Im\u00b7mer", "steht", "der", "Held", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der sein Himmelreich l\u00e4\u00dft scheinen", "tokens": ["Der", "sein", "Him\u00b7mel\u00b7reich", "l\u00e4\u00dft", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dein kurzes Erdenleid.", "tokens": ["Auf", "dein", "kur\u00b7zes", "Er\u00b7den\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, auf diesen Einen, deinen", "tokens": ["Ja", ",", "auf", "die\u00b7sen", "Ei\u00b7nen", ",", "dei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "APPR", "PDAT", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiland, schaue, halte fest", "tokens": ["Hei\u00b7land", ",", "schau\u00b7e", ",", "hal\u00b7te", "fest"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Einen, der die Seinen", "tokens": ["An", "dem", "Ei\u00b7nen", ",", "der", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun und nimmermehr verl\u00e4\u00dft;", "tokens": ["Nun", "und", "nim\u00b7mer\u00b7mehr", "ver\u00b7l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus ihn sollst allein du schauen,", "tokens": ["Aus", "ihn", "sollst", "al\u00b7lein", "du", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der vom Himmel niederkam,", "tokens": ["Der", "vom", "Him\u00b7mel", "nie\u00b7der\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der hinweg des Todes Grauen", "tokens": ["Der", "hin\u00b7weg", "des", "To\u00b7des", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und der H\u00f6lle Schrecken nahm.", "tokens": ["Und", "der", "H\u00f6l\u00b7le", "Schre\u00b7cken", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schaue! Suche! Du wirst finden,", "tokens": ["Schau\u00b7e", "!", "Su\u00b7che", "!", "Du", "wirst", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt, was du gefunden hast,", "tokens": ["Halt", ",", "was", "du", "ge\u00b7fun\u00b7den", "hast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so gib den leichten Winden", "tokens": ["Und", "so", "gib", "den", "leich\u00b7ten", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle schwere Erdenlast", "tokens": ["Al\u00b7le", "schwe\u00b7re", "Er\u00b7den\u00b7last"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutig! Denn der h\u00f6chste Sieger", "tokens": ["Mu\u00b7tig", "!", "Denn", "der", "h\u00f6chs\u00b7te", "Sie\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreitet dir im Streit voran,", "tokens": ["Schrei\u00b7tet", "dir", "im", "Streit", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Losung t\u00f6nt dem Krieger:", "tokens": ["Und", "die", "Lo\u00b7sung", "t\u00f6nt", "dem", "Krie\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}