{"textgrid.poem.26331": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "O, ein Schluck Heimatfrische!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, ein Schluck Heimatfrische! O, ein Schluck k\u00fchle Luft!", "tokens": ["O", ",", "ein", "Schluck", "Hei\u00b7mat\u00b7fri\u00b7sche", "!", "O", ",", "ein", "Schluck", "k\u00fch\u00b7le", "Luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$.", "NE", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ich sehne mich fort vom Gemische", "tokens": ["Ich", "seh\u00b7ne", "mich", "fort", "vom", "Ge\u00b7mi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Aus Schw\u00fcle und giftigem Duft.", "tokens": ["Aus", "Schw\u00fc\u00b7le", "und", "gif\u00b7ti\u00b7gem", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "O, etwas Winterdunkel! O, eine Flocke Schnee!", "tokens": ["O", ",", "et\u00b7was", "Win\u00b7ter\u00b7dun\u00b7kel", "!", "O", ",", "ei\u00b7ne", "Flo\u00b7cke", "Schnee", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "$.", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das immergr\u00fcne Gefunkel", "tokens": ["Das", "im\u00b7mer\u00b7gr\u00fc\u00b7ne", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Palme tut mir weh.", "tokens": ["Der", "Pal\u00b7me", "tut", "mir", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O, ein Paar Augen, stahlblaue,", "tokens": ["O", ",", "ein", "Paar", "Au\u00b7gen", ",", "stahl\u00b7blau\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eine Str\u00e4hne goldblondes Haar,", "tokens": ["Ei\u00b7ne", "Str\u00e4h\u00b7ne", "gold\u00b7blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Darauf ich mein Gl\u00fcck erbaue.", "tokens": ["Da\u00b7rauf", "ich", "mein", "Gl\u00fcck", "er\u00b7bau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}