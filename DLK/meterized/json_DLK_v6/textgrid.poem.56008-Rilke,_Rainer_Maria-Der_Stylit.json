{"textgrid.poem.56008": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Stylit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6lker schlugen \u00fcber ihm zusammen,", "tokens": ["V\u00f6l\u00b7ker", "schlu\u00b7gen", "\u00fc\u00b7ber", "ihm", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die er k\u00fcren durfte und verdammen;", "tokens": ["die", "er", "k\u00fc\u00b7ren", "durf\u00b7te", "und", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und erratend, da\u00df er sich verlor,", "tokens": ["und", "er\u00b7ra\u00b7tend", ",", "da\u00df", "er", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "klomm er aus dem Volksgeruch mit klammen", "tokens": ["klomm", "er", "aus", "dem", "Volks\u00b7ge\u00b7ruch", "mit", "klam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00e4nden einen S\u00e4ulenschaft empor,", "tokens": ["H\u00e4n\u00b7den", "ei\u00b7nen", "S\u00e4u\u00b7len\u00b7schaft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "der noch immer stieg und nichts mehr hob,", "tokens": ["der", "noch", "im\u00b7mer", "stieg", "und", "nichts", "mehr", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "KON", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und begann, allein auf seiner Fl\u00e4che,", "tokens": ["und", "be\u00b7gann", ",", "al\u00b7lein", "auf", "sei\u00b7ner", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ganz von vorne seine eigne Schw\u00e4che", "tokens": ["ganz", "von", "vor\u00b7ne", "sei\u00b7ne", "eig\u00b7ne", "Schw\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "zu vergleichen mit des Herren Lob;", "tokens": ["zu", "ver\u00b7glei\u00b7chen", "mit", "des", "Her\u00b7ren", "Lob", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "und da war kein Ende: er verglich;", "tokens": ["und", "da", "war", "kein", "En\u00b7de", ":", "er", "ver\u00b7glich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$.", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und der Andre wurde immer gr\u00f6\u00dfer.", "tokens": ["und", "der", "And\u00b7re", "wur\u00b7de", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Hirten, Ackerbauer, Fl\u00f6\u00dfer", "tokens": ["Und", "die", "Hir\u00b7ten", ",", "A\u00b7cker\u00b7bau\u00b7er", ",", "Fl\u00f6\u00b7\u00dfer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sahn ihn klein und au\u00dfer sich", "tokens": ["sahn", "ihn", "klein", "und", "au\u00b7\u00dfer", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "APPR", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "immer mit dem ganzen Himmel reden,", "tokens": ["im\u00b7mer", "mit", "dem", "gan\u00b7zen", "Him\u00b7mel", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "eingeregnet manchmal, manchmal licht;", "tokens": ["ein\u00b7ge\u00b7reg\u00b7net", "manch\u00b7mal", ",", "manch\u00b7mal", "licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und sein Heulen st\u00fcrzte sich auf jeden,", "tokens": ["und", "sein", "Heu\u00b7len", "st\u00fcrz\u00b7te", "sich", "auf", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "so als heulte er ihm ins Gesicht.", "tokens": ["so", "als", "heul\u00b7te", "er", "ihm", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Doch er sah seit Jahren nicht,", "tokens": ["Doch", "er", "sah", "seit", "Jah\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "wie der Menge Dr\u00e4ngen und Verlauf", "tokens": ["wie", "der", "Men\u00b7ge", "Dr\u00e4n\u00b7gen", "und", "Ver\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "unten unaufh\u00f6rlich sich erg\u00e4nzte,", "tokens": ["un\u00b7ten", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "sich", "er\u00b7g\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und das Blanke an den F\u00fcrsten gl\u00e4nzte", "tokens": ["und", "das", "Blan\u00b7ke", "an", "den", "F\u00fcrs\u00b7ten", "gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "lange nicht so hoch hinauf.", "tokens": ["lan\u00b7ge", "nicht", "so", "hoch", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber wenn er oben, fast verdammt", "tokens": ["A\u00b7ber", "wenn", "er", "o\u00b7ben", ",", "fast", "ver\u00b7dammt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und von ihrem Widerstand zerschunden,", "tokens": ["und", "von", "ih\u00b7rem", "Wi\u00b7der\u00b7stand", "zer\u00b7schun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "einsam mit verzweifeltem Geschreie", "tokens": ["ein\u00b7sam", "mit", "ver\u00b7zwei\u00b7fel\u00b7tem", "Ge\u00b7schrei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sch\u00fcttelte die t\u00e4glichen D\u00e4monen:", "tokens": ["sch\u00fct\u00b7tel\u00b7te", "die", "t\u00e4g\u00b7li\u00b7chen", "D\u00e4\u00b7mo\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "fielen langsam auf die erste Reihe", "tokens": ["fie\u00b7len", "lang\u00b7sam", "auf", "die", "ers\u00b7te", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "schwer und ungeschickt aus seinen Wunden", "tokens": ["schwer", "und", "un\u00b7ge\u00b7schickt", "aus", "sei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "gro\u00dfe W\u00fcrmer in die offnen Kronen", "tokens": ["gro\u00b7\u00dfe", "W\u00fcr\u00b7mer", "in", "die", "off\u00b7nen", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und vermehrten sich im Samt.", "tokens": ["und", "ver\u00b7mehr\u00b7ten", "sich", "im", "Samt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}