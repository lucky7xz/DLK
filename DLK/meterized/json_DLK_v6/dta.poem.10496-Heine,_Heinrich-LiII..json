{"dta.poem.10496": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Verrieth mein blasses Angesicht", "tokens": ["Ver\u00b7rieth", "mein", "blas\u00b7ses", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir nicht mein Liebeswehe?", "tokens": ["Dir", "nicht", "mein", "Lie\u00b7bes\u00b7we\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und willst du, da\u00df der stolze Mund", "tokens": ["Und", "willst", "du", ",", "da\u00df", "der", "stol\u00b7ze", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Bettelwort gestehe?", "tokens": ["Das", "Bet\u00b7tel\u00b7wort", "ge\u00b7ste\u00b7he", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O, dieser Mund ist gar zu stolz,", "tokens": ["O", ",", "die\u00b7ser", "Mund", "ist", "gar", "zu", "stolz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann nur k\u00fcssen und scherzen;", "tokens": ["Und", "kann", "nur", "k\u00fcs\u00b7sen", "und", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er spr\u00e4che vielleicht ein h\u00f6hnisch Wort,", "tokens": ["Er", "spr\u00e4\u00b7che", "viel\u00b7leicht", "ein", "h\u00f6h\u00b7nisch", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00e4hrend ich sterbe vor Schmerzen.", "tokens": ["W\u00e4h\u00b7rend", "ich", "ster\u00b7be", "vor", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}