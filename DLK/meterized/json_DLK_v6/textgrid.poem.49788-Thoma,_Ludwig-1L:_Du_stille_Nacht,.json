{"textgrid.poem.49788": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du stille Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du stille Nacht,", "tokens": ["Du", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie hast du Freude uns gebracht!", "tokens": ["Wie", "hast", "du", "Freu\u00b7de", "uns", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie war die fremde Kirche nicht", "tokens": ["Wie", "war", "die", "frem\u00b7de", "Kir\u00b7che", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einemmal so hell und licht!", "tokens": ["Mit", "ei\u00b7nem\u00b7mal", "so", "hell", "und", "licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie dehnte sich der Raum so weit!", "tokens": ["Wie", "dehn\u00b7te", "sich", "der", "Raum", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da lag in seiner Herrlichkeit", "tokens": ["Da", "lag", "in", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Vaterland.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nicht wie es sonst vor Augen stand,", "tokens": ["Nicht", "wie", "es", "sonst", "vor", "Au\u00b7gen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ach, nur davon ein kleines St\u00fcck,", "tokens": ["Ach", ",", "nur", "da\u00b7von", "ein", "klei\u00b7nes", "St\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In dem sich uns das Erdengl\u00fcck", "tokens": ["In", "dem", "sich", "uns", "das", "Er\u00b7den\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zusammendr\u00e4ngt. Ein stilles Haus", "tokens": ["Zu\u00b7sam\u00b7men\u00b7dr\u00e4ngt", ".", "Ein", "stil\u00b7les", "Haus"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Im Heimatdorf. Es dringt heraus", "tokens": ["Im", "Hei\u00b7mat\u00b7dorf", ".", "Es", "dringt", "he\u00b7raus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein freundlich warmer Lichterschein.", "tokens": ["Ein", "freund\u00b7lich", "war\u00b7mer", "Lich\u00b7ter\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sie m\u00fcssen wach zur Stunde sein", "tokens": ["Sie", "m\u00fcs\u00b7sen", "wach", "zur", "Stun\u00b7de", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und r\u00fcsten sich zum Kirchengang", "tokens": ["Und", "r\u00fcs\u00b7ten", "sich", "zum", "Kir\u00b7chen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den tief verschneiten Weg entlang.", "tokens": ["Den", "tief", "ver\u00b7schnei\u00b7ten", "Weg", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie man so neben ihnen geht,", "tokens": ["Wie", "man", "so", "ne\u00b7ben", "ih\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In fernster Ferne noch versteht,", "tokens": ["In", "ferns\u00b7ter", "Fer\u00b7ne", "noch", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was einer leis zum andern sagt", "tokens": ["Was", "ei\u00b7ner", "leis", "zum", "an\u00b7dern", "sagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJD", "APPRART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und still nur mit den Augen fragt ...", "tokens": ["Und", "still", "nur", "mit", "den", "Au\u00b7gen", "fragt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Orgel schweigt. Das Lied ist aus,", "tokens": ["Die", "Or\u00b7gel", "schweigt", ".", "Das", "Lied", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Versunken sind nun Dorf und Haus,", "tokens": ["Ver\u00b7sun\u00b7ken", "sind", "nun", "Dorf", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Versunken, was am Heiligen Christ", "tokens": ["Ver\u00b7sun\u00b7ken", ",", "was", "am", "Hei\u00b7li\u00b7gen", "Christ"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "So heimatlich gewesen ist.", "tokens": ["So", "hei\u00b7mat\u00b7lich", "ge\u00b7we\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}