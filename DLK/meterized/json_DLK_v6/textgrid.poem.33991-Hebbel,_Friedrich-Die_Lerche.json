{"textgrid.poem.33991": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam in Ungarn durch ein Thal gefahren,", "tokens": ["Ich", "kam", "in", "Un\u00b7garn", "durch", "ein", "Thal", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von leichten Rossen schnell dahin getragen,", "tokens": ["Von", "leich\u00b7ten", "Ros\u00b7sen", "schnell", "da\u00b7hin", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6rte \u00fcber mir die Lerche schlagen,", "tokens": ["Und", "h\u00f6r\u00b7te", "\u00fc\u00b7ber", "mir", "die", "Ler\u00b7che", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die durch den Aether zog, den bl\u00e4ulich-klaren.", "tokens": ["Die", "durch", "den", "A\u00b7e\u00b7ther", "zog", ",", "den", "bl\u00e4u\u00b7lich\u00b7kla\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Bald aber mu\u00dfte ich erstaunt gewahren,", "tokens": ["Bald", "a\u00b7ber", "mu\u00df\u00b7te", "ich", "er\u00b7staunt", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie zu mir hinab scho\u00df in den Wagen,", "tokens": ["Da\u00df", "sie", "zu", "mir", "hin\u00b7ab", "scho\u00df", "in", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schien mir dies Vertrau'n zugleich ein Zagen", "tokens": ["Doch", "schien", "mir", "dies", "Ver\u00b7trau'n", "zu\u00b7gleich", "ein", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDS", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor einem andern Feind zu offenbaren.", "tokens": ["Vor", "ei\u00b7nem", "an\u00b7dern", "Feind", "zu", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich schaute auf und sah den Habicht hangen,", "tokens": ["Ich", "schau\u00b7te", "auf", "und", "sah", "den", "Ha\u00b7bicht", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nicht gewohnt ist, Schwache zu verschonen,", "tokens": ["Der", "nicht", "ge\u00b7wohnt", "ist", ",", "Schwa\u00b7che", "zu", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "VAFIN", "$,", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie hatte Schutz gesucht auf meinen Knieen;", "tokens": ["Sie", "hat\u00b7te", "Schutz", "ge\u00b7sucht", "auf", "mei\u00b7nen", "Kni\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber dachte: da\u00df das klein're Bangen", "tokens": ["Ich", "a\u00b7ber", "dach\u00b7te", ":", "da\u00df", "das", "klein'\u00b7re", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch dir einfl\u00f6\u00dft, soll sich dir belohnen,", "tokens": ["Der", "Mensch", "dir", "ein\u00b7fl\u00f6\u00dft", ",", "soll", "sich", "dir", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "VMFIN", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lie\u00df sie ungefangen wieder ziehen!", "tokens": ["Und", "lie\u00df", "sie", "un\u00b7ge\u00b7fan\u00b7gen", "wie\u00b7der", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}