{"textgrid.poem.50812": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun stimmet die Harfen und salbet die Geigen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun stimmet die Harfen und salbet die Geigen", "tokens": ["Nun", "stim\u00b7met", "die", "Har\u00b7fen", "und", "sal\u00b7bet", "die", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und gebt euch die H\u00e4ndlein zum himmlischen Reigen,", "tokens": ["Und", "gebt", "euch", "die", "H\u00e4nd\u00b7lein", "zum", "himm\u00b7li\u00b7schen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Weiblein, ein M\u00e4nnlein,", "tokens": ["Ein", "Weib\u00b7lein", ",", "ein", "M\u00e4nn\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein H\u00fchnlein, ein H\u00e4hnlein,", "tokens": ["Ein", "H\u00fchn\u00b7lein", ",", "ein", "H\u00e4hn\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Je zwei und zwei, wie sich's am besten schickt", "tokens": ["Je", "zwei", "und", "zwei", ",", "wie", "sich's", "am", "bes\u00b7ten", "schickt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "KON", "CARD", "$,", "PWAV", "PIS", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und man sich am fr\u00f6mmsten zu Herzen dr\u00fcckt.", "tokens": ["Und", "man", "sich", "am", "fr\u00f6mms\u00b7ten", "zu", "Her\u00b7zen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "APPRART", "ADJA", "APPR", "NN", "VVFIN", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sind alle da? Ei, so verschlie\u00dfet den Himmel,", "tokens": ["Sind", "al\u00b7le", "da", "?", "Ei", ",", "so", "ver\u00b7schlie\u00b7\u00dfet", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$.", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "La\u00dft drau\u00dfen das s\u00fcndige Pack und Gewimmel,", "tokens": ["La\u00dft", "drau\u00b7\u00dfen", "das", "s\u00fcn\u00b7di\u00b7ge", "Pack", "und", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Verberget die Kniffe,", "tokens": ["Ver\u00b7ber\u00b7get", "die", "Knif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die l\u00fcsternen Griffe,", "tokens": ["Die", "l\u00fcs\u00b7ter\u00b7nen", "Grif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wir haben den Geist uns zu Fleische gemacht", "tokens": ["Wir", "ha\u00b7ben", "den", "Geist", "uns", "zu", "Flei\u00b7sche", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und feiern subtil die urewige Nacht!", "tokens": ["Und", "fei\u00b7ern", "sub\u00b7til", "die", "u\u00b7re\u00b7wi\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu wecken die schlaffen, woll\u00fcstigen Gluten,", "tokens": ["Zu", "we\u00b7cken", "die", "schlaf\u00b7fen", ",", "wol\u00b7l\u00fcs\u00b7ti\u00b7gen", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bestreicht uns der Satan den Hintern mit Ruten;", "tokens": ["Be\u00b7streicht", "uns", "der", "Sa\u00b7tan", "den", "Hin\u00b7tern", "mit", "Ru\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die heilige V\u00f6lle", "tokens": ["Die", "hei\u00b7li\u00b7ge", "V\u00f6l\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Durchw\u00fcrze die H\u00f6lle!", "tokens": ["Durch\u00b7w\u00fcr\u00b7ze", "die", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nun l\u00f6schet die Lichter von ungef\u00e4hr;", "tokens": ["Nun", "l\u00f6\u00b7schet", "die", "Lich\u00b7ter", "von", "un\u00b7ge\u00b7f\u00e4hr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das T\u00f6chterlein tanzt mit dem Mission\u00e4r!", "tokens": ["Das", "T\u00f6ch\u00b7ter\u00b7lein", "tanzt", "mit", "dem", "Mis\u00b7si\u00b7o\u00b7n\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "O s\u00fc\u00dfliches Grunzen, o seliges Dunkel,", "tokens": ["O", "s\u00fc\u00df\u00b7li\u00b7ches", "Grun\u00b7zen", ",", "o", "se\u00b7li\u00b7ges", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Begehrliches Suchen und tappend Gemunkel!", "tokens": ["Be\u00b7gehr\u00b7li\u00b7ches", "Su\u00b7chen", "und", "tap\u00b7pend", "Ge\u00b7mun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mich fasset ein Schwindel!", "tokens": ["Mich", "fas\u00b7set", "ein", "Schwin\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bacchantisch Gesindel!", "tokens": ["Bac\u00b7chan\u00b7tisch", "Ge\u00b7sin\u00b7del", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O heilige, himmlische Windbeutelei \u2013", "tokens": ["O", "hei\u00b7li\u00b7ge", ",", "himm\u00b7li\u00b7sche", "Wind\u00b7beu\u00b7te\u00b7lei", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Hinschmelz ich und sied ich im seligsten Brei!", "tokens": ["Hin\u00b7schmelz", "ich", "und", "sied", "ich", "im", "se\u00b7ligs\u00b7ten", "Brei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}