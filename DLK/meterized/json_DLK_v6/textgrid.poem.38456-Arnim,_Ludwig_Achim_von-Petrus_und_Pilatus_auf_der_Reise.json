{"textgrid.poem.38456": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Petrus und Pilatus auf der Reise", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pilatus wollte wandern,", "tokens": ["Pi\u00b7la\u00b7tus", "woll\u00b7te", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von einer Stadt zur andern,", "tokens": ["Von", "ei\u00b7ner", "Stadt", "zur", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Juchheisasa andern.", "tokens": ["Juch\u00b7hei\u00b7sa\u00b7sa", "an\u00b7dern", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sagt Pilatus.", "tokens": ["Sagt", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Jetzt kommen wir vor ein Wirthshaus,", "tokens": ["Jetzt", "kom\u00b7men", "wir", "vor", "ein", "Wirths\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Frau Wirthin schenkt uns Wein heraus,", "tokens": ["Frau", "Wirt\u00b7hin", "schenkt", "uns", "Wein", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Juchheisasa u.s.w.", "tokens": ["Juch\u00b7hei\u00b7sa\u00b7sa", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "KON", "VVIMP", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Sagt Pilatus.", "tokens": ["Sagt", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Womit willst du ihn bezahlen?", "tokens": ["Wo\u00b7mit", "willst", "du", "ihn", "be\u00b7zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ich hab noch einen Thaler,", "tokens": ["Ich", "hab", "noch", "ei\u00b7nen", "Tha\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Juchh. u.s.w.", "tokens": ["Juchh", ".", "u.", "s.", "w."], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "$.", "KON", "VVIMP", "NN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Sagt Pilatus.", "tokens": ["Sagt", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo hast du dann den Thaler bekommen?", "tokens": ["Wo", "hast", "du", "dann", "den", "Tha\u00b7ler", "be\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich hab ihn den Bauern genommen,", "tokens": ["Ich", "hab", "ihn", "den", "Bau\u00b7ern", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Juchh. u.s.w.", "tokens": ["Juchh", ".", "u.", "s.", "w."], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "$.", "KON", "VVIMP", "NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Sprach Pilatus.", "tokens": ["Sprach", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Jetzt hast du keinen Seegen.", "tokens": ["Jetzt", "hast", "du", "kei\u00b7nen", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Daran ist nichts gelegen,", "tokens": ["Da\u00b7ran", "ist", "nichts", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Juchh. u.s.w.", "tokens": ["Juchh", ".", "u.", "s.", "w."], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "$.", "KON", "VVIMP", "NN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Sprach Pilatus.", "tokens": ["Sprach", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Jetzt kommst du nicht in Himmel ein.", "tokens": ["Jetzt", "kommst", "du", "nicht", "in", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "So reit ich auf einem Schimmel hinein.", "tokens": ["So", "reit", "ich", "auf", "ei\u00b7nem", "Schim\u00b7mel", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Juchh. u.s.w.", "tokens": ["Juchh", ".", "u.", "s.", "w."], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "$.", "KON", "VVIMP", "NN"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Sprach Pilatus.", "tokens": ["Sprach", "Pi\u00b7la\u00b7tus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "So f\u00e4llst du herunter und brichst das Bein.", "tokens": ["So", "f\u00e4llst", "du", "her\u00b7un\u00b7ter", "und", "brichst", "das", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Sprach Petrus.", "tokens": ["Sprach", "Pet\u00b7rus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "So rutsch ich auf dem Hintern hinein,", "tokens": ["So", "rutsch", "ich", "auf", "dem", "Hin\u00b7tern", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Juchh. u.s.w.", "tokens": ["Juchh", ".", "u.", "s.", "w."], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "$.", "KON", "VVIMP", "NN"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Sprach Pilatu", "tokens": ["Sprach", "Pi\u00b7la\u00b7tu"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}}}}}