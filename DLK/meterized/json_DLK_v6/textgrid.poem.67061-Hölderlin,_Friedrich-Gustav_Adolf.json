{"textgrid.poem.67061": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gustav Adolf", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, ihr Kinder von Teut!", "tokens": ["Kommt", ",", "ihr", "Kin\u00b7der", "von", "Teut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ihr Kinder von Teut! zum Tale der Schlacht.", "tokens": ["Ihr", "Kin\u00b7der", "von", "Teut", "!", "zum", "Ta\u00b7le", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$.", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Entbl\u00f6\u00dfet die H\u00e4upter, ihr Kinder von Teut!", "tokens": ["Ent\u00b7bl\u00f6\u00b7\u00dfet", "die", "H\u00e4up\u00b7ter", ",", "ihr", "Kin\u00b7der", "von", "Teut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und schauet nieder mit heiligem Blick!", "tokens": ["Und", "schau\u00b7et", "nie\u00b7der", "mit", "hei\u00b7li\u00b7gem", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn hier \u2013 hier starb der Mann,", "tokens": ["Denn", "hier", "\u2013", "hier", "starb", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Taten die Lande sahn,", "tokens": ["Des", "Ta\u00b7ten", "die", "Lan\u00b7de", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und ihren Felsen geboten,", "tokens": ["Und", "ih\u00b7ren", "Fel\u00b7sen", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu beugen die Scheitel den Taten des Manns,", "tokens": ["Zu", "beu\u00b7gen", "die", "Schei\u00b7tel", "den", "Ta\u00b7ten", "des", "Manns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und ihren H\u00fcgeln geboten,", "tokens": ["Und", "ih\u00b7ren", "H\u00fc\u00b7geln", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Zu beugen ihr Haupt den Taten des Manns;", "tokens": ["Zu", "beu\u00b7gen", "ihr", "Haupt", "den", "Ta\u00b7ten", "des", "Manns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des Taten die Meere sahn,", "tokens": ["Des", "Ta\u00b7ten", "die", "Mee\u00b7re", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und Wogen t\u00fcrmten,", "tokens": ["Und", "Wo\u00b7gen", "t\u00fcrm\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Und St\u00fcrme beriefen,", "tokens": ["Und", "St\u00fcr\u00b7me", "be\u00b7rie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Zu donnern ein Lob den Taten des Manns;", "tokens": ["Zu", "don\u00b7nern", "ein", "Lob", "den", "Ta\u00b7ten", "des", "Manns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Entbl\u00f6\u00dfet die H\u00e4upter, ihr Kinder von Teut!", "tokens": ["Ent\u00b7bl\u00f6\u00b7\u00dfet", "die", "H\u00e4up\u00b7ter", ",", "ihr", "Kin\u00b7der", "von", "Teut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.16": {"text": "Denn hier \u2013 hier starb der Mann,", "tokens": ["Denn", "hier", "\u2013", "hier", "starb", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Des Name, wann einst", "tokens": ["Des", "Na\u00b7me", ",", "wann", "einst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Des Ozeans Inseln sich k\u00fcssen,", "tokens": ["Des", "O\u00b7ze\u00b7ans", "In\u00b7seln", "sich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.19": {"text": "Und Kolumbens Welt Lusitanias K\u00fcsten umarmt,", "tokens": ["Und", "Ko\u00b7lum\u00b7bens", "Welt", "Lu\u00b7si\u00b7ta\u00b7ni\u00b7as", "K\u00fcs\u00b7ten", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "NE", "NN", "VVPP", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.20": {"text": "Von fernen V\u00f6lkern gepriesen,", "tokens": ["Von", "fer\u00b7nen", "V\u00f6l\u00b7kern", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Von fremden Zungen genannt,", "tokens": ["Von", "frem\u00b7den", "Zun\u00b7gen", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Am heiligen Denkmal, im Herzen der Edlen", "tokens": ["Am", "hei\u00b7li\u00b7gen", "Denk\u00b7mal", ",", "im", "Her\u00b7zen", "der", "Ed\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.23": {"text": "Noch ewig, wie Gottes Gestirne steht,", "tokens": ["Noch", "e\u00b7wig", ",", "wie", "Got\u00b7tes", "Ge\u00b7stir\u00b7ne", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Entbl\u00f6\u00dfet die H\u00e4upter, ihr Kinder von Teut!", "tokens": ["Ent\u00b7bl\u00f6\u00b7\u00dfet", "die", "H\u00e4up\u00b7ter", ",", "ihr", "Kin\u00b7der", "von", "Teut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "Und schauet nieder mit heiligem Blick!", "tokens": ["Und", "schau\u00b7et", "nie\u00b7der", "mit", "hei\u00b7li\u00b7gem", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Denn hier \u2013 starb \u2013 Gustav.", "tokens": ["Denn", "hier", "\u2013", "starb", "\u2013", "Gus\u00b7tav", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$(", "VVFIN", "$(", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Es l\u00e4rmt' im Tale die Schlacht,", "tokens": ["Es", "l\u00e4rmt'", "im", "Ta\u00b7le", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die Siege zu kr\u00f6nen, die blutige Schlacht,", "tokens": ["Die", "Sie\u00b7ge", "zu", "kr\u00f6\u00b7nen", ",", "die", "blu\u00b7ti\u00b7ge", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und Heldenknie sanken, und Felsenherzen erbebten", "tokens": ["Und", "Hel\u00b7den\u00b7knie", "san\u00b7ken", ",", "und", "Fel\u00b7sen\u00b7her\u00b7zen", "er\u00b7beb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$,", "KON", "NN", "VVINF"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vor Gustav Adolfs Schwert,", "tokens": ["Vor", "Gus\u00b7tav", "A\u00b7dolfs", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Blut der R\u00e4uber flo\u00df,", "tokens": ["Und", "Blut", "der", "R\u00e4u\u00b7ber", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Blut der Witwenm\u00f6rder,", "tokens": ["Und", "Blut", "der", "Wit\u00b7wen\u00b7m\u00f6r\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Blut der Sch\u00e4nder der Freiheit flo\u00df,", "tokens": ["Und", "Blut", "der", "Sch\u00e4n\u00b7der", "der", "Frei\u00b7heit", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und hinan im Blute der R\u00e4uber hinan", "tokens": ["Und", "hi\u00b7nan", "im", "Blu\u00b7te", "der", "R\u00e4u\u00b7ber", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "St\u00fcrzt', als ein Racheblitz des R\u00e4chers,", "tokens": ["St\u00fcrzt'", ",", "als", "ein", "Ra\u00b7che\u00b7blitz", "des", "R\u00e4\u00b7chers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit seinen Treuen Gustav hinan.", "tokens": ["Mit", "sei\u00b7nen", "Treu\u00b7en", "Gus\u00b7tav", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Er gedachte seiner Taten,", "tokens": ["Er", "ge\u00b7dach\u00b7te", "sei\u00b7ner", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da flammte sein Auge von G\u00f6tterlust,", "tokens": ["Da", "flamm\u00b7te", "sein", "Au\u00b7ge", "von", "G\u00f6t\u00b7ter\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Seiner Taten vor Gott,", "tokens": ["Sei\u00b7ner", "Ta\u00b7ten", "vor", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Und Himmelsruhe verkl\u00e4rte sein Angesicht", "tokens": ["Und", "Him\u00b7mels\u00b7ru\u00b7he", "ver\u00b7kl\u00e4r\u00b7te", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und hinan, in seiner Himmelsruhe", "tokens": ["Und", "hi\u00b7nan", ",", "in", "sei\u00b7ner", "Him\u00b7mels\u00b7ru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "St\u00fcrzt' an der Spitze der Treuen Gustav hinan \u2013", "tokens": ["St\u00fcrzt'", "an", "der", "Spit\u00b7ze", "der", "Treu\u00b7en", "Gus\u00b7tav", "hi\u00b7nan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "NE", "PTKVZ", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Doch wehe! unter den Treuen", "tokens": ["Doch", "we\u00b7he", "!", "un\u00b7ter", "den", "Treu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Lauscht' ein Verr\u00e4ter;", "tokens": ["Lauscht'", "ein", "Ver\u00b7r\u00e4\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Er dachte \u2013 der Verr\u00e4ter \u2013 den H\u00f6llengedanken,", "tokens": ["Er", "dach\u00b7te", "\u2013", "der", "Ver\u00b7r\u00e4\u00b7ter", "\u2013", "den", "H\u00f6l\u00b7len\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und \u2013 Gustav \u2013 sank.", "tokens": ["Und", "\u2013", "Gus\u00b7tav", "\u2013", "sank", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ha! Verr\u00e4ter! Verr\u00e4ter!", "tokens": ["Ha", "!", "Ver\u00b7r\u00e4\u00b7ter", "!", "Ver\u00b7r\u00e4\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Da\u00df in der Todesstunde dein Weib dich verdamme,", "tokens": ["Da\u00df", "in", "der", "To\u00b7dess\u00b7tun\u00b7de", "dein", "Weib", "dich", "ver\u00b7dam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wehe! \u00fcber dich rufen deine S\u00f6hne,", "tokens": ["Und", "we\u00b7he", "!", "\u00fc\u00b7ber", "dich", "ru\u00b7fen", "dei\u00b7ne", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und deine Enkel die Tat ins Ohr dir heulen,", "tokens": ["Und", "dei\u00b7ne", "En\u00b7kel", "die", "Tat", "ins", "Ohr", "dir", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bis deine Blicke erstarren im Grauen des Meuchelmords,", "tokens": ["Bis", "dei\u00b7ne", "Bli\u00b7cke", "er\u00b7star\u00b7ren", "im", "Grau\u00b7en", "des", "Meu\u00b7chel\u00b7mords", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und deine Seele flieht vor den Schrecken der Ewigkeit.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "flieht", "vor", "den", "Schre\u00b7cken", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "w