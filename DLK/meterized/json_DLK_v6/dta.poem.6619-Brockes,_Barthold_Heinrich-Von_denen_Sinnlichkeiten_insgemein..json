{"dta.poem.6619": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Zeug den wir geordnet sehn,", "tokens": ["Den", "Zeug", "den", "wir", "ge\u00b7ord\u00b7net", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getheilt in Element, in Himmel und in Sternen,", "tokens": ["Ge\u00b7theilt", "in", "E\u00b7le\u00b7ment", ",", "in", "Him\u00b7mel", "und", "in", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sollt\u2019 ohne Geist man selbe wol verstehn,", "tokens": ["Sollt'", "oh\u00b7ne", "Geist", "man", "sel\u00b7be", "wol", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PIS", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja ihre Gegenwart einst zu bemercken lernen?", "tokens": ["Ja", "ih\u00b7re", "Ge\u00b7gen\u00b7wart", "einst", "zu", "be\u00b7mer\u00b7cken", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verwirret und vermischt mit jenen grossen Hauffen,", "tokens": ["Ver\u00b7wir\u00b7ret", "und", "ver\u00b7mischt", "mit", "je\u00b7nen", "gros\u00b7sen", "Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "W\u00fcrd\u2019 jeder unvermerckt beweget, vorw\u00e4rts lauffen.", "tokens": ["W\u00fcrd'", "je\u00b7der", "un\u00b7ver\u00b7merckt", "be\u00b7we\u00b7get", ",", "vor\u00b7w\u00e4rts", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein Mensch w\u00fcrd\u2019 etwas anders seyn,", "tokens": ["Kein", "Mensch", "w\u00fcrd'", "et\u00b7was", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als eine Bluhm, ein Baum, ein Marmor-Stein.", "tokens": ["Als", "ei\u00b7ne", "Bluhm", ",", "ein", "Baum", ",", "ein", "Mar\u00b7mor\u00b7Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In die Materie w\u00fcrd\u2019 er sich wieder sencken,", "tokens": ["In", "die", "Ma\u00b7te\u00b7rie", "w\u00fcrd'", "er", "sich", "wie\u00b7der", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Ohn einmal da\u00df er leb\u2019 und worden sey, zu dencken;", "tokens": ["Ohn", "ein\u00b7mal", "da\u00df", "er", "leb'", "und", "wor\u00b7den", "sey", ",", "zu", "den\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADV", "KOUS", "PPER", "VVFIN", "KON", "VAPP", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn uns nicht GOTT der SCHOEPFFER wollen", "tokens": ["Wenn", "uns", "nicht", "GoTT", "der", "ScHOE\u00b7PF\u00b7FER", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ART", "NN", "VMFIN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "g\u00f6nnen,", "tokens": ["g\u00f6n\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Den Geist, der f\u00e4hig ist zu dencken, zu erkennen.", "tokens": ["Den", "Geist", ",", "der", "f\u00e4\u00b7hig", "ist", "zu", "den\u00b7cken", ",", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}