{"textgrid.poem.35522": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch meine letzten Tage", "tokens": ["Durch", "mei\u00b7ne", "letz\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein seltsam Leuchten ging \u2013", "tokens": ["Ein", "selt\u00b7sam", "Leuch\u00b7ten", "ging", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie helle Botschaft gro\u00dfer Zeit \u2013", "tokens": ["Wie", "hel\u00b7le", "Bot\u00b7schaft", "gro\u00b7\u00dfer", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie stummer Schicksalswink:", "tokens": ["Wie", "stum\u00b7mer", "Schick\u00b7sals\u00b7wink", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df, da die Stunden herbsten,", "tokens": ["Da\u00df", ",", "da", "die", "Stun\u00b7den", "herbs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Licht auf Abschied sann,", "tokens": ["Das", "Licht", "auf", "Ab\u00b7schied", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sieg in mir vollendet ward,", "tokens": ["Der", "Sieg", "in", "mir", "voll\u00b7en\u00b7det", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um den ich rang so hart, so hart,", "tokens": ["Um", "den", "ich", "rang", "so", "hart", ",", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als Fr\u00fchlingsst\u00fcrme krachten,", "tokens": ["Als", "Fr\u00fch\u00b7lings\u00b7st\u00fcr\u00b7me", "krach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Als Sommern\u00e4chte wachten \u2013", "tokens": ["Als", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "wach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den ich trotz hei\u00dfem Trachten,", "tokens": ["Den", "ich", "trotz", "hei\u00b7\u00dfem", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Doch nimmer, nimmermehr gewann! ...", "tokens": ["Doch", "nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "ge\u00b7wann", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch meine Brust es s\u00e4uselt", "tokens": ["Durch", "mei\u00b7ne", "Brust", "es", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie tagende D\u00e4mmerung \u2013", "tokens": ["Wie", "ta\u00b7gen\u00b7de", "D\u00e4m\u00b7me\u00b7rung", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Und mich ergreift so seltsam s\u00fc\u00df", "tokens": ["Und", "mich", "er\u00b7greift", "so", "selt\u00b7sam", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schweifender Sehnsucht Schwung!", "tokens": ["Schwei\u00b7fen\u00b7der", "Sehn\u00b7sucht", "Schwung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Bald wird es sich erf\u00fcllen \u2013", "tokens": ["Bald", "wird", "es", "sich", "er\u00b7f\u00fcl\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur ", "tokens": ["Zur"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Die letzte B\u00fcrde werf' ich hin \u2013", "tokens": ["Die", "letz\u00b7te", "B\u00fcr\u00b7de", "wer\u00b7f'", "ich", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nur eines d\u00fcnkt mich noch Gewinn \u2013", "tokens": ["Nur", "ei\u00b7nes", "d\u00fcnkt", "mich", "noch", "Ge\u00b7winn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Zweifel unterliegen \u2013", "tokens": ["Die", "Zwei\u00b7fel", "un\u00b7ter\u00b7lie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mag's brechen oder biegen \u2013", "tokens": ["Mag's", "bre\u00b7chen", "o\u00b7der", "bie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}