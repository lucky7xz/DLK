{"textgrid.poem.40312": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6tter der Zeit, ich schm\u00e4hte gestern,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6tter der Zeit, ich schm\u00e4hte gestern,", "tokens": ["G\u00f6t\u00b7ter", "der", "Zeit", ",", "ich", "schm\u00e4h\u00b7te", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "und schm\u00e4hen will ich euch auch heut,", "tokens": ["und", "schm\u00e4\u00b7hen", "will", "ich", "euch", "auch", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "G\u00f6tter der Zeit, euch ewig l\u00e4stern,", "tokens": ["G\u00f6t\u00b7ter", "der", "Zeit", ",", "euch", "e\u00b7wig", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "h\u00f6rt mein lachendes L\u00e4stergel\u00e4ut!", "tokens": ["h\u00f6rt", "mein", "la\u00b7chen\u00b7des", "L\u00e4s\u00b7ter\u00b7ge\u00b7l\u00e4ut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr f\u00fchrt die Macht, ich f\u00fchre Klage,", "tokens": ["Ihr", "f\u00fchrt", "die", "Macht", ",", "ich", "f\u00fch\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich f\u00fchre das Wort in meiner Macht!", "tokens": ["ich", "f\u00fch\u00b7re", "das", "Wort", "in", "mei\u00b7ner", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dreizehn liegt ihr beim Gelage;", "tokens": ["Drei\u00b7zehn", "liegt", "ihr", "beim", "Ge\u00b7la\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "das bedeutet Totenwacht,", "tokens": ["das", "be\u00b7deu\u00b7tet", "To\u00b7ten\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unfall, Hinfall \u2013 singt die Sage.", "tokens": ["Un\u00b7fall", ",", "Hin\u00b7fall", "\u2013", "singt", "die", "Sa\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6tter, nehmt euch bald in Acht!", "tokens": ["G\u00f6t\u00b7ter", ",", "nehmt", "euch", "bald", "in", "Acht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "sehr schnell eilen die lustigen Tage,", "tokens": ["sehr", "schnell", "ei\u00b7len", "die", "lus\u00b7ti\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "G\u00f6tter, G\u00f6tter, und Loke lacht!", "tokens": ["G\u00f6t\u00b7ter", ",", "G\u00f6t\u00b7ter", ",", "und", "Lo\u00b7ke", "lacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ja, ich sa\u00df in j\u00fcngeren Stunden", "tokens": ["Ja", ",", "ich", "sa\u00df", "in", "j\u00fcn\u00b7ge\u00b7ren", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "zu Gast in eurem G\u00f6ttersaal:", "tokens": ["zu", "Gast", "in", "eu\u00b7rem", "G\u00f6t\u00b7ter\u00b7saal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "an dem Strick, den ihr gebunden,", "tokens": ["an", "dem", "Strick", ",", "den", "ihr", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hingeschleift zu euerm Mahl.", "tokens": ["hin\u00b7ge\u00b7schleift", "zu", "eu\u00b7erm", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum: eure eiternden Wunden,", "tokens": ["Da\u00b7rum", ":", "eu\u00b7re", "ei\u00b7tern\u00b7den", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Loke kennt, kennt ihre Zahl!", "tokens": ["Lo\u00b7ke", "kennt", ",", "kennt", "ih\u00b7re", "Zahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ekel f\u00fchlt'ich vor den vollen Gef\u00e4\u00dfen,", "tokens": ["E\u00b7kel", "f\u00fchlt'\u00b7ich", "vor", "den", "vol\u00b7len", "Ge\u00b7f\u00e4\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und euer Wein war ekler noch;", "tokens": ["und", "eu\u00b7er", "Wein", "war", "ek\u00b7ler", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "euer Singsang verdarb mir das Essen,", "tokens": ["eu\u00b7er", "Sings\u00b7ang", "ver\u00b7darb", "mir", "das", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "der fad wie d\u00fcnne Br\u00fche roch.", "tokens": ["der", "fad", "wie", "d\u00fcn\u00b7ne", "Br\u00fc\u00b7he", "roch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum: das k\u00f6nnt ihr Loke nicht vergessen,", "tokens": ["Drum", ":", "das", "k\u00f6nnt", "ihr", "Lo\u00b7ke", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "PDS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "da\u00df er nicht lobkr\u00e4hend vor euch kroch.", "tokens": ["da\u00df", "er", "nicht", "lob\u00b7kr\u00e4\u00b7hend", "vor", "euch", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nein, ich ", "tokens": ["Nein", ",", "ich"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "will nicht singen f\u00fcr euern Fra\u00df;", "tokens": ["will", "nicht", "sin\u00b7gen", "f\u00fcr", "eu\u00b7ern", "Fra\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "nein, ich will euch lieber schm\u00e4hen", "tokens": ["nein", ",", "ich", "will", "euch", "lie\u00b7ber", "schm\u00e4\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mit meinem gro\u00dfen, sch\u00f6nen Ha\u00df!", "tokens": ["mit", "mei\u00b7nem", "gro\u00b7\u00dfen", ",", "sch\u00f6\u00b7nen", "Ha\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Meine Sehnen habt ihr mir zerstochen,", "tokens": ["Mei\u00b7ne", "Seh\u00b7nen", "habt", "ihr", "mir", "zer\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "mich geschmiedet auf dies Gletscherjoch,", "tokens": ["mich", "ge\u00b7schmie\u00b7det", "auf", "dies", "Glet\u00b7scher\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PDS", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "mir die Z\u00e4hne ausgebrochen,", "tokens": ["mir", "die", "Z\u00e4h\u00b7ne", "aus\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aber meine Zunge l\u00e4stert doch!", "tokens": ["a\u00b7ber", "mei\u00b7ne", "Zun\u00b7ge", "l\u00e4s\u00b7tert", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ja: ich habe eure Schmach verraten,", "tokens": ["Ja", ":", "ich", "ha\u00b7be", "eu\u00b7re", "Schmach", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00f6tter \u2013 das war all mein Fehl!", "tokens": ["G\u00f6t\u00b7ter", "\u2013", "das", "war", "all", "mein", "Fehl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eure heiligen Gr\u00e4uelthaten,", "tokens": ["eu\u00b7re", "hei\u00b7li\u00b7gen", "Gr\u00e4u\u00b7elt\u00b7ha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "eurer festen Schl\u00f6sser S\u00fcndenhehl.", "tokens": ["eu\u00b7rer", "fes\u00b7ten", "Schl\u00f6s\u00b7ser", "S\u00fcn\u00b7den\u00b7hehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Drum hei\u00dft Loke der Erste der Hasser,", "tokens": ["Drum", "hei\u00dft", "Lo\u00b7ke", "der", "Ers\u00b7te", "der", "Has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "der L\u00e4sterer Erster in euerm Lied;", "tokens": ["der", "L\u00e4s\u00b7te\u00b7rer", "Ers\u00b7ter", "in", "eu\u00b7erm", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "ja, es ehrt, es ehrt ihn, da\u00df er", "tokens": ["ja", ",", "es", "ehrt", ",", "es", "ehrt", "ihn", ",", "da\u00df", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Verr\u00e4ter verriet!", "tokens": ["Ver\u00b7r\u00e4\u00b7ter", "ver\u00b7riet", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Wenn den Gewaltigen straft der Schwache,", "tokens": ["Wenn", "den", "Ge\u00b7wal\u00b7ti\u00b7gen", "straft", "der", "Schwa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "dann hei\u00dft die Strafe Rachewut.", "tokens": ["dann", "hei\u00dft", "die", "Stra\u00b7fe", "Ra\u00b7che\u00b7wut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei's! Ja, G\u00f6tter: ich \u00fcbte Rache,", "tokens": ["Sei's", "!", "Ja", ",", "G\u00f6t\u00b7ter", ":", "ich", "\u00fcb\u00b7te", "Ra\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKANT", "$,", "NN", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "h\u00f6rt es, Rache \u2013 und r\u00e4chte gut!", "tokens": ["h\u00f6rt", "es", ",", "Ra\u00b7che", "\u2013", "und", "r\u00e4ch\u00b7te", "gut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Habe erbrochen die Bundeslade,", "tokens": ["Ha\u00b7be", "er\u00b7bro\u00b7chen", "die", "Bun\u00b7des\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "habe den Moder ans Licht gekarrt,", "tokens": ["ha\u00b7be", "den", "Mo\u00b7der", "ans", "Licht", "ge\u00b7karrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "euch abgerissen die Maskerade", "tokens": ["euch", "ab\u00b7ge\u00b7ris\u00b7sen", "die", "Mas\u00b7ke\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und eure Nacktheit offenbart.", "tokens": ["und", "eu\u00b7re", "Nackt\u00b7heit", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Habe euern G\u00f6tzendienst verachtet,", "tokens": ["Ha\u00b7be", "eu\u00b7ern", "G\u00f6t\u00b7zen\u00b7dienst", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "von euren Bildern den Flitter geklopft,", "tokens": ["von", "eu\u00b7ren", "Bil\u00b7dern", "den", "Flit\u00b7ter", "ge\u00b7klopft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "habe das goldne Kalb geschlachtet,", "tokens": ["ha\u00b7be", "das", "gold\u00b7ne", "Kalb", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Habe ger\u00e4cht, du alte G\u00f6tterhure,", "tokens": ["Ha\u00b7be", "ge\u00b7r\u00e4cht", ",", "du", "al\u00b7te", "G\u00f6t\u00b7ter\u00b7hu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "ger\u00e4cht all meiner Jugend Weh,", "tokens": ["ge\u