{"dta.poem.7816": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wunderleib/ ist zwar in etwas abge-", "tokens": ["Der", "Wun\u00b7der\u00b7leib", "/", "ist", "zwar", "in", "et\u00b7was", "ab\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VAFIN", "ADV", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die innerliche Zier kan doch kein Pinsel", "tokens": ["Die", "in\u00b7ner\u00b7li\u00b7che", "Zier", "kan", "doch", "kein", "Pin\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie ist mit Stand/ Verstand/ und allem", "tokens": ["Sie", "ist", "mit", "Stand", "/", "Ver\u00b7stand", "/", "und", "al\u00b7lem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$(", "NN", "$(", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie prangt so Tugend-voll/ als reich an", "tokens": ["Sie", "prangt", "so", "Tu\u00b7gen\u00b7dvoll", "/", "als", "reich", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$(", "KOUS", "ADJD", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der Leib ist G\u00f6ttlich-sch\u00f6n: dem Geist auch", "tokens": ["Der", "Leib", "ist", "G\u00f6tt\u00b7lich\u00b7sch\u00f6n", ":", "dem", "Geist", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kurtz: Alls ist sie allein. Kein Lob ihr Lob", "tokens": ["Kurtz", ":", "Alls", "ist", "sie", "al\u00b7lein", ".", "Kein", "Lob", "ihr", "Lob"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "$.", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}