{"textgrid.poem.64326": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr fr\u00f6hlichen J\u00fcnglinge, h\u00f6ret", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr fr\u00f6hlichen J\u00fcnglinge, h\u00f6ret", "tokens": ["Ihr", "fr\u00f6h\u00b7li\u00b7chen", "J\u00fcng\u00b7lin\u00b7ge", ",", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den fr\u00f6hlichen J\u00fcngling, er lehret", "tokens": ["Den", "fr\u00f6h\u00b7li\u00b7chen", "J\u00fcng\u00b7ling", ",", "er", "leh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Euch gl\u00fccklich und weise zu sein.", "tokens": ["Euch", "gl\u00fcck\u00b7lich", "und", "wei\u00b7se", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Heut ist mir im Herzen so helle!", "tokens": ["Heut", "ist", "mir", "im", "Her\u00b7zen", "so", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ich sch\u00f6pfe die Freud' aus der Quelle,", "tokens": ["Ich", "sch\u00f6p\u00b7fe", "die", "Freud'", "aus", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In altem hungarischen Wein!", "tokens": ["In", "al\u00b7tem", "hun\u00b7ga\u00b7ri\u00b7schen", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Auf! wackre Gesellen, und tr\u00e4nket", "tokens": ["Auf", "!", "wack\u00b7re", "Ge\u00b7sel\u00b7len", ",", "und", "tr\u00e4n\u00b7ket"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit Freude die Seelen! Es kr\u00e4nket", "tokens": ["Mit", "Freu\u00b7de", "die", "See\u00b7len", "!", "Es", "kr\u00e4n\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den h\u00f6llischen Drachen das Gl\u00fcck!", "tokens": ["Den", "h\u00f6l\u00b7li\u00b7schen", "Dra\u00b7chen", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch h\u00fctet euch, Br\u00fcder; er lauschet!", "tokens": ["Doch", "h\u00fc\u00b7tet", "euch", ",", "Br\u00fc\u00b7der", ";", "er", "lau\u00b7schet", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und wo sich ein J\u00fcngling berauschet,", "tokens": ["Und", "wo", "sich", "ein", "J\u00fcng\u00b7ling", "be\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Da grinzt er mit schielendem Blick!", "tokens": ["Da", "grinzt", "er", "mit", "schie\u00b7len\u00b7dem", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Oft f\u00fchrt er, bei n\u00e4chtlichen Fackeln,", "tokens": ["Oft", "f\u00fchrt", "er", ",", "bei", "n\u00e4cht\u00b7li\u00b7chen", "Fa\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Reigen der Thoren; sie wackeln", "tokens": ["Die", "Rei\u00b7gen", "der", "Tho\u00b7ren", ";", "sie", "wa\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Frohlockend, und tr\u00e4umen nicht Harm.", "tokens": ["Froh\u00b7lo\u00b7ckend", ",", "und", "tr\u00e4u\u00b7men", "nicht", "Harm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Er f\u00fchrt sie im Taumel des Tanzes;", "tokens": ["Er", "f\u00fchrt", "sie", "im", "Tau\u00b7mel", "des", "Tan\u00b7zes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Noch duften die Blumen des Kranzes,", "tokens": ["Noch", "duf\u00b7ten", "die", "Blu\u00b7men", "des", "Kran\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Schon h\u00e4lt sie die ", "tokens": ["Schon", "h\u00e4lt", "sie", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich warne dich, flatternde Jugend:", "tokens": ["Ich", "war\u00b7ne", "dich", ",", "flat\u00b7tern\u00b7de", "Ju\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Oft grenzet die Freude der Tugend", "tokens": ["Oft", "gren\u00b7zet", "die", "Freu\u00b7de", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "An giftiger Laster Genu\u00df.", "tokens": ["An", "gif\u00b7ti\u00b7ger", "Las\u00b7ter", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So schleichet im freundlichen Schatten", "tokens": ["So", "schlei\u00b7chet", "im", "freund\u00b7li\u00b7chen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Pappel, auf bl\u00fchenden Matten,", "tokens": ["Der", "Pap\u00b7pel", ",", "auf", "bl\u00fc\u00b7hen\u00b7den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Natter, und sticht dich im Fu\u00df.", "tokens": ["Die", "Nat\u00b7ter", ",", "und", "sticht", "dich", "im", "Fu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Drum merke dir, was ich dich lehre:", "tokens": ["Drum", "mer\u00b7ke", "dir", ",", "was", "ich", "dich", "leh\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df dich der Feind nicht beth\u00f6re,", "tokens": ["Auf", "da\u00df", "dich", "der", "Feind", "nicht", "be\u00b7th\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So suche dir heut noch ein Weib!", "tokens": ["So", "su\u00b7che", "dir", "heut", "noch", "ein", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Statt l\u00e4nger zu flattern, erw\u00e4hle", "tokens": ["Statt", "l\u00e4n\u00b7ger", "zu", "flat\u00b7tern", ",", "er\u00b7w\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein M\u00e4dchen mit lieblicher Seele,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "mit", "lieb\u00b7li\u00b7cher", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und eben so lieblichem Leib!", "tokens": ["Und", "e\u00b7ben", "so", "lieb\u00b7li\u00b7chem", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Es halte sich jeder zur Schande,", "tokens": ["Es", "hal\u00b7te", "sich", "je\u00b7der", "zur", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu fliehn die holdseligen Bande,", "tokens": ["Zu", "fliehn", "die", "hold\u00b7se\u00b7li\u00b7gen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Womit uns ein Weibchen umschlingt!", "tokens": ["Wo\u00b7mit", "uns", "ein", "Weib\u00b7chen", "um\u00b7schlingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie f\u00fchrt uns am rosichten B\u00e4ndchen,", "tokens": ["Sie", "f\u00fchrt", "uns", "am", "ro\u00b7sich\u00b7ten", "B\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mit samtnen und kosenden H\u00e4ndchen,", "tokens": ["Mit", "samt\u00b7nen", "und", "ko\u00b7sen\u00b7den", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Bis sie in den Himmel uns bringt!", "tokens": ["Bis", "sie", "in", "den", "Him\u00b7mel", "uns", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "O Wonne, sein Weibchen zu wiegen", "tokens": ["O", "Won\u00b7ne", ",", "sein", "Weib\u00b7chen", "zu", "wie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In Armen der Liebe, zu liegen", "tokens": ["In", "Ar\u00b7men", "der", "Lie\u00b7be", ",", "zu", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Beim Weibchen in s\u00fc\u00dfem Genu\u00df!", "tokens": ["Beim", "Weib\u00b7chen", "in", "s\u00fc\u00b7\u00dfem", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich achte, mit neidenden Blicken,", "tokens": ["Ich", "ach\u00b7te", ",", "mit", "nei\u00b7den\u00b7den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und schmachtendem Geisterentz\u00fccken,", "tokens": ["Und", "schmach\u00b7ten\u00b7dem", "Geis\u00b7te\u00b7rent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Umschweben die Engel den Ku\u00df.", "tokens": ["Um\u00b7schwe\u00b7ben", "die", "En\u00b7gel", "den", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Ich h\u00e4tt' euch noch vieles gelehret;", "tokens": ["Ich", "h\u00e4tt'", "euch", "noch", "vie\u00b7les", "ge\u00b7leh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das M\u00e4dchen hier hat mich gest\u00f6ret,", "tokens": ["Das", "M\u00e4d\u00b7chen", "hier", "hat", "mich", "ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie weckte den Trunknen dort auf.", "tokens": ["Sie", "weck\u00b7te", "den", "Trunk\u00b7nen", "dort", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wart, Braune! Gleich wirst du ihm b\u00fc\u00dfen!", "tokens": ["Wart", ",", "Brau\u00b7ne", "!", "Gleich", "wirst", "du", "ihm", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er straft dich mit duftenden K\u00fcssen,", "tokens": ["Er", "straft", "dich", "mit", "duf\u00b7ten\u00b7den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und hascht dich im wankenden Lauf.", "tokens": ["Und", "hascht", "dich", "im", "wan\u00b7ken\u00b7den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}