{"textgrid.poem.61562": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sobald die Nacht mit dunklem Fl\u00fcgelpaar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald die Nacht mit dunklem Fl\u00fcgelpaar", "tokens": ["So\u00b7bald", "die", "Nacht", "mit", "dunk\u00b7lem", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Erd' umf\u00e4ngt, da\u00df jeder Strahl verbla\u00dft:", "tokens": ["Die", "Erd'", "um\u00b7f\u00e4ngt", ",", "da\u00df", "je\u00b7der", "Strahl", "ver\u00b7bla\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Luft und Meer, im Wald von Ast zu Ast", "tokens": ["In", "Luft", "und", "Meer", ",", "im", "Wald", "von", "Ast", "zu", "Ast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und unterm Dach wird still, was rege war.", "tokens": ["Und", "un\u00b7term", "Dach", "wird", "still", ",", "was", "re\u00b7ge", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADJD", "$,", "PWS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn Schlaf, der durch die Glieder wunderbar", "tokens": ["Denn", "Schlaf", ",", "der", "durch", "die", "Glie\u00b7der", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich ausgie\u00dft, g\u00f6nnet dem Gedanken Rast,", "tokens": ["Sich", "aus\u00b7gie\u00dft", ",", "g\u00f6n\u00b7net", "dem", "Ge\u00b7dan\u00b7ken", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis da\u00df aufs neu den Tag mit seiner Last", "tokens": ["Bis", "da\u00df", "aufs", "neu", "den", "Tag", "mit", "sei\u00b7ner", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aurora weckt im blonden Lockenhaar.", "tokens": ["Au\u00b7ro\u00b7ra", "weckt", "im", "blon\u00b7den", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich Ungl\u00fccksel'ger nur bleib' unerquickt;", "tokens": ["Ich", "Un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "nur", "bleib'", "un\u00b7er\u00b7quickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn Seufzen, feindlich aller Ruhe, schafft", "tokens": ["Denn", "Seuf\u00b7zen", ",", "feind\u00b7lich", "al\u00b7ler", "Ru\u00b7he", ",", "schafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "ADJD", "PIAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Auge schlaflos und mein Herz voll Bangen.", "tokens": ["Mein", "Au\u00b7ge", "schlaf\u00b7los", "und", "mein", "Herz", "voll", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und, gleich dem V\u00f6gelchen im Garn verstrickt,", "tokens": ["Und", ",", "gleich", "dem", "V\u00f6\u00b7gel\u00b7chen", "im", "Garn", "ver\u00b7strickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Je mehr ich suche zu entfliehn der Haft,", "tokens": ["Je", "mehr", "ich", "su\u00b7che", "zu", "ent\u00b7fliehn", "der", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So mehr im Wirrsal find' ich mich gefangen.", "tokens": ["So", "mehr", "im", "Wirr\u00b7sal", "find'", "ich", "mich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}