{"dta.poem.19923": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Lob des Flachses .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl hat Sommer sich zum Kranze", "tokens": ["Wohl", "hat", "Som\u00b7mer", "sich", "zum", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche zarte Bl\u00fcth' gewoben.", "tokens": ["Man\u00b7che", "zar\u00b7te", "Bl\u00fcth'", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, Flachs, dich mildste Pflanze!", "tokens": ["A\u00b7ber", ",", "Flachs", ",", "dich", "milds\u00b7te", "Pflan\u00b7ze", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich doch vor allen loben.", "tokens": ["Mu\u00df", "ich", "doch", "vor", "al\u00b7len", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blauen Himmel ausgestreuet", "tokens": ["Blau\u00b7en", "Him\u00b7mel", "aus\u00b7ge\u00b7streu\u00b7et"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du \u00fcber dunkle Auen,", "tokens": ["Hast", "du", "\u00fc\u00b7ber", "dunk\u00b7le", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine milde Sch\u00f6nheit freuet", "tokens": ["Dei\u00b7ne", "mil\u00b7de", "Sch\u00f6n\u00b7heit", "freu\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gleich zart geschaffne Frauen.", "tokens": ["Die", "gleich", "zart", "ge\u00b7schaff\u00b7ne", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weiches Gr\u00fcn der Stengel zieret,", "tokens": ["Wei\u00b7ches", "Gr\u00fcn", "der", "Sten\u00b7gel", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcthe tr\u00e4gt des Himmels Helle,", "tokens": ["Bl\u00fc\u00b7the", "tr\u00e4gt", "des", "Him\u00b7mels", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leis vom Westhauch anger\u00fchret", "tokens": ["Leis", "vom", "West\u00b7hauch", "an\u00b7ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wogt sie sanft in blauer Welle.", "tokens": ["Wogt", "sie", "sanft", "in", "blau\u00b7er", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist die Bl\u00fcthe Dir entfallen,", "tokens": ["Ist", "die", "Bl\u00fc\u00b7the", "Dir", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht man dich aus dunkler Erden,", "tokens": ["Zieht", "man", "dich", "aus", "dunk\u00b7ler", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darfst nicht mehr im Westhauch wallen", "tokens": ["Darfst", "nicht", "mehr", "im", "West\u00b7hauch", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Must durch Feu'r zu Silber werden.", "tokens": ["Must", "durch", "Feu'r", "zu", "Sil\u00b7ber", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Hand gesch\u00e4ft'ger Frauen", "tokens": ["Und", "die", "Hand", "ge\u00b7sch\u00e4ft'\u00b7ger", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchrt dich unter muntern Scherzen,", "tokens": ["R\u00fchrt", "dich", "un\u00b7ter", "mun\u00b7tern", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klar wie Mondschein anzuschauen", "tokens": ["Klar", "wie", "Mond\u00b7schein", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du theuer ihrem Herzen.", "tokens": ["Bist", "du", "theu\u00b7er", "ih\u00b7rem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In dem blanken M\u00e4dchenzimmer,", "tokens": ["In", "dem", "blan\u00b7ken", "M\u00e4d\u00b7chen\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis ber\u00fchrt von zartem Munde,", "tokens": ["Leis", "be\u00b7r\u00fchrt", "von", "zar\u00b7tem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n verkl\u00e4rt von Sternenschimmer", "tokens": ["Sch\u00f6n", "ver\u00b7kl\u00e4rt", "von", "Ster\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dir manche liebe Stunde.", "tokens": ["Wird", "dir", "man\u00b7che", "lie\u00b7be", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "N\u00e4chtlich in des Landmanns H\u00fctte,", "tokens": ["N\u00e4cht\u00b7lich", "in", "des", "Land\u00b7manns", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ein flammend Holz die Kerze,", "tokens": ["Wo", "ein", "flam\u00b7mend", "Holz", "die", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In viel muntrer M\u00e4gdlein Mitte,", "tokens": ["In", "viel", "mun\u00b7trer", "M\u00e4gd\u00b7lein", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du bey Gesang und Scherze.", "tokens": ["Bist", "du", "bey", "Ge\u00b7sang", "und", "Scher\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Drausen brausen Sturm, Gespenster;", "tokens": ["Drau\u00b7sen", "brau\u00b7sen", "Sturm", ",", "Ge\u00b7spens\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandrer wird der Sorg' entladen,", "tokens": ["Wand\u00b7rer", "wird", "der", "Sor\u00b7g'", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sieht er hinter hellem Fenster", "tokens": ["Sieht", "er", "hin\u00b7ter", "hel\u00b7lem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimisch deinen goldnen Faden.", "tokens": ["Hei\u00b7misch", "dei\u00b7nen", "gold\u00b7nen", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zarten Leib in dich gekleidet", "tokens": ["Zar\u00b7ten", "Leib", "in", "dich", "ge\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt das M\u00e4gdlein zum Altare,", "tokens": ["Tritt", "das", "M\u00e4gd\u00b7lein", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegst ein segnend Kreuz gebreitet", "tokens": ["Liegst", "ein", "seg\u00b7nend", "Kreuz", "ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmernd \u00fcber dunkler Bahre.", "tokens": ["Schim\u00b7mernd", "\u00fc\u00b7ber", "dunk\u00b7ler", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bist des S\u00e4uglings erste H\u00fclle,", "tokens": ["Bist", "des", "S\u00e4ug\u00b7lings", "ers\u00b7te", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielest lind um seine Glieder, \u2014", "tokens": ["Spie\u00b7lest", "lind", "um", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleich in dich geh\u00fcllt und stille", "tokens": ["Bleich", "in", "dich", "ge\u00b7h\u00fcllt", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt der Mensch zur Erde wieder.", "tokens": ["Kehrt", "der", "Mensch", "zur", "Er\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}