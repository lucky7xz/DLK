{"textgrid.poem.34129": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen viel gedruckten Lyricus", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunderlich ist es, gewi\u00df! Auch wird's die Geschichte verzeichnen,", "tokens": ["Wun\u00b7der\u00b7lich", "ist", "es", ",", "ge\u00b7wi\u00df", "!", "Auch", "wird's", "die", "Ge\u00b7schich\u00b7te", "ver\u00b7zeich\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df man so oft dich gedruckt, aber bescheide dich doch!", "tokens": ["Da\u00df", "man", "so", "oft", "dich", "ge\u00b7druckt", ",", "a\u00b7ber", "be\u00b7schei\u00b7de", "dich", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kalk bleibt Kalk, er wird nicht darum von dem Gesunden", "tokens": ["Kalk", "bleibt", "Kalk", ",", "er", "wird", "nicht", "da\u00b7rum", "von", "dem", "Ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "PAV", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Mit gerechnet zum Mehl, weil ihn der Kranke verschlingt.", "tokens": ["Mit", "ge\u00b7rech\u00b7net", "zum", "Mehl", ",", "weil", "ihn", "der", "Kran\u00b7ke", "ver\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}