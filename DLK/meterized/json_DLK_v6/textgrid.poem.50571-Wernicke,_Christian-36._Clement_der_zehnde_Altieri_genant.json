{"textgrid.poem.50571": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "36. Clement der zehnde Altieri genant", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' als zu Rom zum Pabst ", "tokens": ["Eh'", "als", "zu", "Rom", "zum", "Pabst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So wurden diese Vers' auf ihn aus Spott gemacht;", "tokens": ["So", "wur\u00b7den", "die\u00b7se", "Ver\u00b7s'", "auf", "ihn", "aus", "Spott", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und heimlich unter's Volck gebracht;", "tokens": ["Und", "heim\u00b7lich", "un\u00b7ter's", "Volck", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worauf ein Freund von ihm, durch Geld und Gunst bewegt,", "tokens": ["Wo\u00b7rauf", "ein", "Freund", "von", "ihm", ",", "durch", "Geld", "und", "Gunst", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch diese jene wiederlegt:", "tokens": ["Durch", "die\u00b7se", "je\u00b7ne", "wie\u00b7der\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}