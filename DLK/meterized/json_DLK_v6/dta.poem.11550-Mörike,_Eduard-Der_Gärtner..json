{"dta.poem.11550": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "So wei\u00df wie der Schnee,", "tokens": ["So", "wei\u00df", "wie", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die sch\u00f6nste Prinzessin", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Prin\u00b7zes\u00b7sin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Reit't durch die Allee.", "tokens": ["Reit't", "durch", "die", "Al\u00b7lee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der Weg, den das R\u00f6\u00dflein", "tokens": ["Der", "Weg", ",", "den", "das", "R\u00f6\u00df\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hintanzet so hold,", "tokens": ["Hin\u00b7tan\u00b7zet", "so", "hold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Sand, den ich streute,", "tokens": ["Der", "Sand", ",", "den", "ich", "streu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er blinket wie Gold!", "tokens": ["Er", "blin\u00b7ket", "wie", "Gold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du rosenfarbs H\u00fctlein,", "tokens": ["Du", "ro\u00b7sen\u00b7farbs", "H\u00fct\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wohl auf und wohl ab!", "tokens": ["Wohl", "auf", "und", "wohl", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O wirf eine Feder", "tokens": ["O", "wirf", "ei\u00b7ne", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Verstohlen herab!", "tokens": ["Ver\u00b7stoh\u00b7len", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und willst du dagegen", "tokens": ["Und", "willst", "du", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PAV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Eine Bl\u00fcthe von mir,", "tokens": ["Ei\u00b7ne", "Bl\u00fc\u00b7the", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nimm tausend f\u00fcr Eine,", "tokens": ["Nimm", "tau\u00b7send", "f\u00fcr", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "APPR", "ART", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nimm alle daf\u00fcr!", "tokens": ["Nimm", "al\u00b7le", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIS", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}