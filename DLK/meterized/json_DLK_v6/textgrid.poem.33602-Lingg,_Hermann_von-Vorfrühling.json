{"textgrid.poem.33602": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seelenvoll neigt d\u00e4mmernd des Himmels Lichtblau", "tokens": ["See\u00b7len\u00b7voll", "neigt", "d\u00e4m\u00b7mernd", "des", "Him\u00b7mels", "Licht\u00b7blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Sich zur Erdnacht nieder im Blumenkelche;", "tokens": ["Sich", "zur", "Erd\u00b7nacht", "nie\u00b7der", "im", "Blu\u00b7men\u00b7kel\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Laub an Laub, schwertauende Bl\u00e4tter, wie sie", "tokens": ["Laub", "an", "Laub", ",", "schwert\u00b7au\u00b7en\u00b7de", "Bl\u00e4t\u00b7ter", ",", "wie", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN", "$,", "PWAV", "PPER"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Fl\u00fcstern im Schlafe!", "tokens": ["Fl\u00fcs\u00b7tern", "im", "Schla\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Will es Fr\u00fchling werden, und kommt ihr wieder,", "tokens": ["Will", "es", "Fr\u00fch\u00b7ling", "wer\u00b7den", ",", "und", "kommt", "ihr", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihr aus mildern Zonen gesandte Tage,", "tokens": ["Ihr", "aus", "mil\u00b7dern", "Zo\u00b7nen", "ge\u00b7sand\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Von der holden Lerche verk\u00fcndigt, kommt ihr,", "tokens": ["Von", "der", "hol\u00b7den", "Ler\u00b7che", "ver\u00b7k\u00fcn\u00b7digt", ",", "kommt", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kommt ihr doch wieder?", "tokens": ["Kommt", "ihr", "doch", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}