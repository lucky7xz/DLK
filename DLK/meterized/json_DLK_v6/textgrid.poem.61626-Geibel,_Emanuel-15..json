{"textgrid.poem.61626": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beim Mondesuntergange", "tokens": ["Beim", "Mon\u00b7des\u00b7un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergl\u00e4nzt wie Gold das Meer,", "tokens": ["Er\u00b7gl\u00e4nzt", "wie", "Gold", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwarz blickt mit schroffem Hange", "tokens": ["Schwarz", "blickt", "mit", "schrof\u00b7fem", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leukadias Felsen her.", "tokens": ["Leu\u00b7ka\u00b7di\u00b7as", "Fel\u00b7sen", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da taucht mir tief im Sinne", "tokens": ["Da", "taucht", "mir", "tief", "im", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleichwie aus D\u00e4mmerflor", "tokens": ["Gleich\u00b7wie", "aus", "D\u00e4m\u00b7mer\u00b7flor"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Sapphos wilder Minne", "tokens": ["Von", "Sap\u00b7phos", "wil\u00b7der", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alte M\u00e4r empor.", "tokens": ["Die", "al\u00b7te", "M\u00e4r", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Volke der Hellenen", "tokens": ["Dem", "Vol\u00b7ke", "der", "Hel\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sang sie zum erstenmal", "tokens": ["Sang", "sie", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die eifers\u00fccht'gen Tr\u00e4nen", "tokens": ["Die", "ei\u00b7fer\u00b7s\u00fccht'\u00b7gen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlorner Liebesqual.", "tokens": ["Ver\u00b7lor\u00b7ner", "Lie\u00b7bes\u00b7qual", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch leben jene Gluten,", "tokens": ["Noch", "le\u00b7ben", "je\u00b7ne", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die t\u00f6nend sie durchw\u00fchlt,", "tokens": ["Die", "t\u00f6\u00b7nend", "sie", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis sie in diesen Fluten", "tokens": ["Bis", "sie", "in", "die\u00b7sen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr brennend Herz gek\u00fchlt.", "tokens": ["Ihr", "bren\u00b7nend", "Herz", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und oft bei Nacht dort oben,", "tokens": ["Und", "oft", "bei", "Nacht", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn hoch die Wolken gehn,", "tokens": ["Wenn", "hoch", "die", "Wol\u00b7ken", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Haupt vom Kranz umwoben", "tokens": ["Das", "Haupt", "vom", "Kranz", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieht sie der Schiffer stehn.", "tokens": ["Sieht", "sie", "der", "Schif\u00b7fer", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gespenstisch weht ihr Schleier,", "tokens": ["Ge\u00b7spens\u00b7tisch", "weht", "ihr", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und \u00fcberm Wogendrang", "tokens": ["Und", "\u00fc\u00b7berm", "Wo\u00b7gen\u00b7drang"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Winde schwebt zur Leier", "tokens": ["Im", "Win\u00b7de", "schwebt", "zur", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sehns\u00fcchtig ihr Gesang:", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "ihr", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbschon senkt der Mond sich tr\u00fcbe,", "tokens": ["\u00bb", "schon", "senkt", "der", "Mond", "sich", "tr\u00fc\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mitternacht bricht ein;", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "bricht", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz vergeht vor Liebe,", "tokens": ["Mein", "Herz", "ver\u00b7geht", "vor", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weh, ich bin allein!\u00ab", "tokens": ["Und", "weh", ",", "ich", "bin", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}