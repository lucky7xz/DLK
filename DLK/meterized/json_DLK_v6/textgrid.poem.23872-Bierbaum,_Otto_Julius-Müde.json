{"textgrid.poem.23872": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlie\u00df die Th\u00fcre hinter mir,", "tokens": ["Ich", "schlie\u00df", "die", "Th\u00fc\u00b7re", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ohne G\u00e4ste sein;", "tokens": ["Will", "oh\u00b7ne", "G\u00e4s\u00b7te", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab mich selbst verlassen,", "tokens": ["Ich", "hab", "mich", "selbst", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum bin ich so allein.", "tokens": ["Drum", "bin", "ich", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich mache alle L\u00e4den zu,", "tokens": ["Ich", "ma\u00b7che", "al\u00b7le", "L\u00e4\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was soll mir Tag und Licht.", "tokens": ["Was", "soll", "mir", "Tag", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Feuer ist verglommen,", "tokens": ["Das", "Feu\u00b7er", "ist", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne brauch ich nicht.", "tokens": ["Die", "Son\u00b7ne", "brauch", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle gar kein Leben mehr;", "tokens": ["Ich", "f\u00fch\u00b7le", "gar", "kein", "Le\u00b7ben", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe ist vorbei.", "tokens": ["Die", "Lie\u00b7be", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann nicht einmal weinen,", "tokens": ["Ich", "kann", "nicht", "ein\u00b7mal", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus mir ringt sich kein Schrei.", "tokens": ["Aus", "mir", "ringt", "sich", "kein", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ich habe keinen Gott und Freund", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "Gott", "und", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bin so sinnenleer,", "tokens": ["Und", "bin", "so", "sin\u00b7nen\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, wenn das Gl\u00fcck jetzt k\u00e4me,", "tokens": ["Da\u00df", ",", "wenn", "das", "Gl\u00fcck", "jetzt", "k\u00e4\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich f\u00fchlte es nicht mehr.", "tokens": ["Ich", "f\u00fchl\u00b7te", "es", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich schlie\u00df die Th\u00fcre hinter mir,", "tokens": ["Ich", "schlie\u00df", "die", "Th\u00fc\u00b7re", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin nur f\u00fcr den zu Haus,", "tokens": ["Bin", "nur", "f\u00fcr", "den", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "APPR", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von dem es hei\u00dft, er f\u00e4chelt", "tokens": ["Von", "dem", "es", "hei\u00dft", ",", "er", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das letzte Fl\u00e4mmchen aus.", "tokens": ["Das", "letz\u00b7te", "Fl\u00e4mm\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}