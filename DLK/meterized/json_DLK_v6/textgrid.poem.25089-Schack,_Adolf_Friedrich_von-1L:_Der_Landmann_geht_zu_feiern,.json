{"textgrid.poem.25089": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Landmann geht zu feiern,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Landmann geht zu feiern,", "tokens": ["Der", "Land\u00b7mann", "geht", "zu", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Sonnenglut versengt,", "tokens": ["Von", "Son\u00b7nen\u00b7glut", "ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sanft mit seinen Schleiern", "tokens": ["Die", "sanft", "mit", "sei\u00b7nen", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Abend nun verh\u00e4ngt:", "tokens": ["Der", "A\u00b7bend", "nun", "ver\u00b7h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es huscht durch laub'ge Aeste", "tokens": ["Es", "huscht", "durch", "laub'\u00b7ge", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00e4nfling heim zum Neste,", "tokens": ["Der", "H\u00e4nf\u00b7ling", "heim", "zum", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo auf den warmen Eiern", "tokens": ["Wo", "auf", "den", "war\u00b7men", "Ei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Weibchen ihn empf\u00e4ngt.", "tokens": ["Sein", "Weib\u00b7chen", "ihn", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon ruht in s\u00fc\u00dfer Zelle", "tokens": ["Schon", "ruht", "in", "s\u00fc\u00b7\u00dfer", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Biene arbeitsmatt,", "tokens": ["Die", "Bie\u00b7ne", "ar\u00b7beits\u00b7matt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Schlaf streckt die Libelle", "tokens": ["Zum", "Schlaf", "streckt", "die", "Li\u00b7bel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-++-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sich auf das Lindenblatt;", "tokens": ["Sich", "auf", "das", "Lin\u00b7den\u00b7blatt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Dorfe kehrt der M\u00e4her,", "tokens": ["Zum", "Dor\u00b7fe", "kehrt", "der", "M\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nah schon gl\u00e4nzt und n\u00e4her", "tokens": ["Und", "nah", "schon", "gl\u00e4nzt", "und", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das L\u00e4mpchen ihm, das helle,", "tokens": ["Das", "L\u00e4mp\u00b7chen", "ihm", ",", "das", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von seiner Lagerstatt.", "tokens": ["Von", "sei\u00b7ner", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht fehlt die Ankerkette", "tokens": ["Nicht", "fehlt", "die", "An\u00b7ker\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem m\u00fcden Rudersmann,", "tokens": ["Dem", "m\u00fc\u00b7den", "Ru\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Rehe nicht sein Bette", "tokens": ["Dem", "Re\u00b7he", "nicht", "sein", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Buchwald oder Tann,", "tokens": ["In", "Buch\u00b7wald", "o\u00b7der", "Tann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nicht die Schlucht dem Winde,", "tokens": ["Und", "nicht", "die", "Schlucht", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In der er Ruhe finde;", "tokens": ["In", "der", "er", "Ru\u00b7he", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo aber ist die St\u00e4tte,", "tokens": ["Wo", "a\u00b7ber", "ist", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darauf ", "tokens": ["Da\u00b7rauf"], "token_info": ["word"], "pos": ["PAV"], "meter": "+-", "measure": "trochaic.single"}}}}}