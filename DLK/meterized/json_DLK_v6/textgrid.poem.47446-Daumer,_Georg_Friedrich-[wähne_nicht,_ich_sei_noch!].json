{"textgrid.poem.47446": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[w\u00e4hne nicht, ich sei noch!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hne nicht, ich sei noch!", "tokens": ["W\u00e4h\u00b7ne", "nicht", ",", "ich", "sei", "noch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ging ja doch in deiner Liebe Feuer", "tokens": ["Ging", "ja", "doch", "in", "dei\u00b7ner", "Lie\u00b7be", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf in helle Flammen", "tokens": ["Auf", "in", "hel\u00b7le", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Meines Seins entz\u00fcndlich arme Scheuer!", "tokens": ["Mei\u00b7nes", "Seins", "ent\u00b7z\u00fcnd\u00b7lich", "ar\u00b7me", "Scheu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nur ein H\u00e4ufchen Asche", "tokens": ["Nur", "ein", "H\u00e4uf\u00b7chen", "A\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Blieb zur\u00fcck; das st\u00e4ubet in die L\u00fcfte,", "tokens": ["Blieb", "zu\u00b7r\u00fcck", ";", "das", "st\u00e4u\u00b7bet", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sinkt herab und legt sich", "tokens": ["Sinkt", "her\u00b7ab", "und", "legt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PRF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vor die F\u00fc\u00dfe dir als Liebessteuer.", "tokens": ["Vor", "die", "F\u00fc\u00b7\u00dfe", "dir", "als", "Lie\u00b7bess\u00b7teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}