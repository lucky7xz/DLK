{"dta.poem.9748": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem nechsten dient ich vor mit meinem geist und leibe/", "tokens": ["Dem", "nechs\u00b7ten", "dient", "ich", "vor", "mit", "mei\u00b7nem", "geist", "und", "lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu zeiten auch wol gar mit meinem lieben weibe;", "tokens": ["Zu", "zei\u00b7ten", "auch", "wol", "gar", "mit", "mei\u00b7nem", "lie\u00b7ben", "wei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja auch nach meinem tod sind n\u00fctzlich meine sachen/", "tokens": ["Ja", "auch", "nach", "mei\u00b7nem", "tod", "sind", "n\u00fctz\u00b7lich", "mei\u00b7ne", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von meinen h\u00f6rnern kan man feste k\u00e4mme machen.", "tokens": ["Von", "mei\u00b7nen", "h\u00f6r\u00b7nern", "kan", "man", "fes\u00b7te", "k\u00e4m\u00b7me", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VMFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}