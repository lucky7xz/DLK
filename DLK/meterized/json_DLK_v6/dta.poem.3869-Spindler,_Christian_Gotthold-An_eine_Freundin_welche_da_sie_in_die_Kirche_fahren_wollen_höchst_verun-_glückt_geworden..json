{"dta.poem.3869": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "An eine Freundin/ welche/ da sie in die  \n Kirche fahren wollen/ h\u00f6chst verun-  \n gl\u00fcckt geworden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum bricht der frohe Sonntag an,", "tokens": ["Kaum", "bricht", "der", "fro\u00b7he", "Sonn\u00b7tag", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum ist sein helles Licht erschienen,", "tokens": ["Kaum", "ist", "sein", "hel\u00b7les", "Licht", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So suchst du deinem GOtt zu dienen,", "tokens": ["So", "suchst", "du", "dei\u00b7nem", "Gott", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du suchst die rechte Gnaden-Bahn;", "tokens": ["Du", "suchst", "die", "rech\u00b7te", "Gna\u00b7den\u00b7Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich solte nun so Pferd als Wagen", "tokens": ["Dich", "sol\u00b7te", "nun", "so", "Pferd", "als", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu deines GOttes Tempel tragen.", "tokens": ["Zu", "dei\u00b7nes", "Got\u00b7tes", "Tem\u00b7pel", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Freudigkeit suchst du sein Wort,", "tokens": ["Mit", "Freu\u00b7dig\u00b7keit", "suchst", "du", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du f\u00e4hrst mit gr\u00f6ster Sehnsucht fort.", "tokens": ["Du", "f\u00e4hrst", "mit", "gr\u00f6s\u00b7ter", "Sehn\u00b7sucht", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun kamst du in den stillen Hayn,", "tokens": ["Nun", "kamst", "du", "in", "den", "stil\u00b7len", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey sch\u00f6n und angenehmen Wetter,", "tokens": ["Bey", "sch\u00f6n", "und", "an\u00b7ge\u00b7neh\u00b7men", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Zephyr regt die sanfften Bl\u00e4tter,", "tokens": ["Ein", "Ze\u00b7phyr", "regt", "die", "sanff\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was konte wohl vergn\u00fcgters seyn?", "tokens": ["Was", "kon\u00b7te", "wohl", "ver\u00b7gn\u00fcg\u00b7ters", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schwester sasse dir zur Seiten,", "tokens": ["Die", "Schwes\u00b7ter", "sas\u00b7se", "dir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die wolte dich dahin begleiten,", "tokens": ["Die", "wol\u00b7te", "dich", "da\u00b7hin", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr fuhrt in stiller Einsamkeit", "tokens": ["Ihr", "fuhrt", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bey annoch fr\u00fcher Morgen-Zeit.", "tokens": ["Bey", "an\u00b7noch", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie? welch Schicksal reget sich!", "tokens": ["Doch", "wie", "?", "welch", "Schick\u00b7sal", "re\u00b7get", "sich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PWAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pferde fangen an zu springen,", "tokens": ["Die", "Pfer\u00b7de", "fan\u00b7gen", "an", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Berg und Fels mit Macht zu dringen.", "tokens": ["Durch", "Berg", "und", "Fels", "mit", "Macht", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00e4umen beyde f\u00fcrchterlich;", "tokens": ["Sie", "sch\u00e4u\u00b7men", "bey\u00b7de", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wagen h\u00e4nget im Moraste,", "tokens": ["Der", "Wa\u00b7gen", "h\u00e4n\u00b7get", "im", "Mo\u00b7ras\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er f\u00e4nget sich an einem Aste,", "tokens": ["Er", "f\u00e4n\u00b7get", "sich", "an", "ei\u00b7nem", "As\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er f\u00e4lt; das muntre Gaul entweicht,", "tokens": ["Er", "f\u00e4lt", ";", "das", "mun\u00b7tre", "Gaul", "ent\u00b7weicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und euch hat Fall und St\u00fcrtz gebeugt.", "tokens": ["Und", "euch", "hat", "Fall", "und", "St\u00fcrtz", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gerechter GOtt! o welch ein Schmertz!", "tokens": ["Ge\u00b7rech\u00b7ter", "Gott", "!", "o", "welch", "ein", "Schmertz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "FM", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier lagen zwey zerquetschte Leiber,", "tokens": ["Hier", "la\u00b7gen", "zwey", "zer\u00b7quetschte", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zwey Gottesf\u00fcrchtig fromme Weiber,", "tokens": ["Zwey", "Got\u00b7tes\u00b7f\u00fcrch\u00b7tig", "from\u00b7me", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Freundin, ach! dein Schwester-Hertz", "tokens": ["Und", "Freun\u00b7din", ",", "ach", "!", "dein", "Schwes\u00b7ter\u00b7Hertz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "ITJ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df, da man sie sucht aufzuheben,", "tokens": ["Mu\u00df", ",", "da", "man", "sie", "sucht", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den redlich frommen Geist aufgeben,", "tokens": ["Den", "red\u00b7lich", "from\u00b7men", "Geist", "auf\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du lagest noch in deinem Blut,", "tokens": ["Du", "la\u00b7gest", "noch", "in", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man bringt dich in das nechste Gut.", "tokens": ["Man", "bringt", "dich", "in", "das", "nechs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man suchet Rettung in der Noth,", "tokens": ["Man", "su\u00b7chet", "Ret\u00b7tung", "in", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es werden dir die vielen Wunden", "tokens": ["Es", "wer\u00b7den", "dir", "die", "vie\u00b7len", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Sorgfalt durch den Artzt verbunden,", "tokens": ["Mit", "Sorg\u00b7falt", "durch", "den", "Artzt", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch war die fromme Schwester todt.", "tokens": ["Doch", "war", "die", "from\u00b7me", "Schwes\u00b7ter", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Leben hat dir GOtt geschencket,", "tokens": ["Das", "Le\u00b7ben", "hat", "dir", "Gott", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Alleine wenn dein Hertz bedencket", "tokens": ["Al\u00b7lei\u00b7ne", "wenn", "dein", "Hertz", "be\u00b7den\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der frommen Schwester Todes Quaal,", "tokens": ["Der", "from\u00b7men", "Schwes\u00b7ter", "To\u00b7des", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du st\u00fcrbest lieber hundert mahl.", "tokens": ["Du", "st\u00fcr\u00b7best", "lie\u00b7ber", "hun\u00b7dert", "mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun, ", "tokens": ["Nun", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die dich bi\u00dfhero hart geschlagen,", "tokens": ["Die", "dich", "bi\u00df\u00b7he\u00b7ro", "hart", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die helffe dir dein Elend tragen,", "tokens": ["Die", "helf\u00b7fe", "dir", "dein", "E\u00b7lend", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn deine Noth ist GOtt bekannt;", "tokens": ["Denn", "dei\u00b7ne", "Noth", "ist", "Gott", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er thuts; er hilfft dir gantz gewi\u00df,", "tokens": ["Er", "thuts", ";", "er", "hilfft", "dir", "gantz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn fliehet Angst und K\u00fcmmerni\u00df,", "tokens": ["Denn", "flie\u00b7het", "Angst", "und", "K\u00fcm\u00b7mer\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sobald der Freuden-Stern erschienen,", "tokens": ["So\u00b7bald", "der", "Freu\u00b7den\u00b7S\u00b7tern", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Wohl wird auch im Alter gr\u00fcnen.", "tokens": ["Dein", "Wohl", "wird", "auch", "im", "Al\u00b7ter", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}