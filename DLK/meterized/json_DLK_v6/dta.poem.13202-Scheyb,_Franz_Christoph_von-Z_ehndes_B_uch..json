{"dta.poem.13202": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enichts leg\u2019 ich meinem Wiz, nichts meiner Einsicht bey;", "tokens": ["\u201e", "nichts", "leg'", "ich", "mei\u00b7nem", "Wiz", ",", "nichts", "mei\u00b7ner", "Ein\u00b7sicht", "bey", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eich wei\u00df, da\u00df alles nur ein Werck des Himmels sey.", "tokens": ["\u201e", "ich", "wei\u00df", ",", "da\u00df", "al\u00b7les", "nur", "ein", "Werck", "des", "Him\u00b7mels", "sey", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "235\u201dMich hat er zwar erw\u00e4hlt; von ihm bin ich gekr\u00f6net;", "tokens": ["\"", "Mich", "hat", "er", "zwar", "er\u00b7w\u00e4hlt", ";", "von", "ihm", "bin", "ich", "ge\u00b7kr\u00f6\u00b7net", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201emein Alles ist von ihm, und seiner Macht entlehnet.", "tokens": ["\u201e", "mein", "Al\u00b7les", "ist", "von", "ihm", ",", "und", "sei\u00b7ner", "Macht", "ent\u00b7leh\u00b7net", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "PIS", "VAFIN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eeuch aber hat er mir als Wachten beygesezt,", "tokens": ["\u201e", "euch", "a\u00b7ber", "hat", "er", "mir", "als", "Wach\u00b7ten", "bey\u00b7ge\u00b7sezt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VAFIN", "PPER", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund solche F\u00e4higkeit in euer Herz geezt,", "tokens": ["\u201e", "und", "sol\u00b7che", "F\u00e4\u00b7hig\u00b7keit", "in", "eu\u00b7er", "Herz", "geezt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "\u201eda\u00df, weil ihr meinen Thron durch Krieg behaupten mu\u00dftet,", "tokens": ["\u201e", "da\u00df", ",", "weil", "ihr", "mei\u00b7nen", "Thron", "durch", "Krieg", "be\u00b7haup\u00b7ten", "mu\u00df\u00b7tet", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "240\u201dJhr jene Kraft, die GOtt euch gab, zu brauchen wu\u00dftet.", "tokens": ["\"", "Ihr", "je\u00b7ne", "Kraft", ",", "die", "Gott", "euch", "gab", ",", "zu", "brau\u00b7chen", "wu\u00df\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PDAT", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edurch Krieg, sag\u2019 ich, von dem die Vorwelt keine Spuhr", "tokens": ["\u201e", "durch", "Krieg", ",", "sag'", "ich", ",", "von", "dem", "die", "Vor\u00b7welt", "kei\u00b7ne", "Spuhr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "PRELS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eviel weniger die Last und Gr\u00e4\u00dflichkeit erfuhr;", "tokens": ["\u201e", "viel", "we\u00b7ni\u00b7ger", "die", "Last", "und", "Gr\u00e4\u00df\u00b7lich\u00b7keit", "er\u00b7fuhr", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edurch Krieg, mit welchem uns so Freund\u2019 als Feind\u2019 umrungen,", "tokens": ["\u201e", "durch", "Krieg", ",", "mit", "wel\u00b7chem", "uns", "so", "Freund'", "als", "Feind'", "um\u00b7run\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ein dem wir doch mit Gl\u00fcck und Sieg die Fahnen schwungen.", "tokens": ["\u201e", "in", "dem", "wir", "doch", "mit", "Gl\u00fcck", "und", "Sieg", "die", "Fah\u00b7nen", "schwun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "245\u201dMit einem Wort: es ist kein Kriegs-Heer, sondern GOtt", "tokens": ["\"", "Mit", "ei\u00b7nem", "Wort", ":", "es", "ist", "kein", "Kriegs\u00b7Heer", ",", "son\u00b7dern", "Gott"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eder mich durch euch, und euch durch mich von dieser Noth,", "tokens": ["\u201e", "der", "mich", "durch", "euch", ",", "und", "euch", "durch", "mich", "von", "die\u00b7ser", "Noth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PRF", "APPR", "PPER", "$,", "KON", "PPER", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201evon meiner Feinde Groll, von Sturz und Fall gerettet;", "tokens": ["\u201e", "von", "mei\u00b7ner", "Fein\u00b7de", "Groll", ",", "von", "Sturz", "und", "Fall", "ge\u00b7ret\u00b7tet", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eer ists, den ich um Rath und Beystand angebettet.", "tokens": ["\u201e", "er", "ists", ",", "den", "ich", "um", "Rath", "und", "Beys\u00b7tand", "an\u00b7ge\u00b7bet\u00b7tet", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eja! GOtt ists, welcher mich zum Werckzeug ausersehn,", "tokens": ["\u201e", "ja", "!", "Gott", "ists", ",", "wel\u00b7cher", "mich", "zum", "Wer\u00b7ck\u00b7zeug", "au\u00b7ser\u00b7sehn", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "250\u201dDurch welchen Land und Leut' in Heil und Wohlfart stehn.", "tokens": ["\"", "Durch", "wel\u00b7chen", "Land", "und", "Leut'", "in", "Heil", "und", "Wohl\u00b7fart", "stehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eeuch aber hat er mir zu meinem Rath gegeben:", "tokens": ["\u201e", "euch", "a\u00b7ber", "hat", "er", "mir", "zu", "mei\u00b7nem", "Rath", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201edie\u00df ists, wodurch wir nun in der Errettung leben.", "tokens": ["\u201e", "die\u00df", "ists", ",", "wo\u00b7durch", "wir", "nun", "in", "der", "Er\u00b7ret\u00b7tung", "le\u00b7ben", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}