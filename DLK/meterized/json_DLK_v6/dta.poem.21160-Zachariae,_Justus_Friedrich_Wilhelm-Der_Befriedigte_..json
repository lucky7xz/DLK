{"dta.poem.21160": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Befriedigte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt, da die Erde sich verj\u00fcngt,", "tokens": ["Jetzt", ",", "da", "die", "Er\u00b7de", "sich", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Vogel Freude singt;", "tokens": ["Und", "je\u00b7der", "Vo\u00b7gel", "Freu\u00b7de", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt sollt\u2019 ich Brunnenflaschen leeren?", "tokens": ["Jetzt", "sollt'", "ich", "Brun\u00b7nen\u00b7fla\u00b7schen", "lee\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das plaudert mir kein Doktor ein.", "tokens": ["Das", "plau\u00b7dert", "mir", "kein", "Dok\u00b7tor", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebt mir die Flaschen voller Wein!", "tokens": ["Gebt", "mir", "die", "Fla\u00b7schen", "vol\u00b7ler", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das l\u00e4\u00dft sich h\u00f6ren!", "tokens": ["Das", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was Bav in einem Abend schreibt,", "tokens": ["Was", "Bav", "in", "ei\u00b7nem", "A\u00b7bend", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Pflicht und Amt dazu ihn treibt,", "tokens": ["Wenn", "Pflicht", "und", "Amt", "da\u00b7zu", "ihn", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das lasse, wer da will, sich lehren.", "tokens": ["Das", "las\u00b7se", ",", "wer", "da", "will", ",", "sich", "leh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lobe, was, ohn Amt und Pflicht,", "tokens": ["Ich", "lo\u00b7be", ",", "was", ",", "ohn", "Amt", "und", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Damon beym Burgunder spricht.", "tokens": ["Mein", "Da\u00b7mon", "beym", "Bur\u00b7gun\u00b7der", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das l\u00e4\u00dft sich h\u00f6ren!", "tokens": ["Das", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Speront reimt, doch er reimt f\u00fcr sich.", "tokens": ["Spe\u00b7ront", "reimt", ",", "doch", "er", "reimt", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "PPER", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was thut das? Jhr seyd wunderlich;", "tokens": ["Was", "thut", "das", "?", "Ihr", "seyd", "wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan ihm ja kein Mensch verwehren.", "tokens": ["Das", "kan", "ihm", "ja", "kein", "Mensch", "ver\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr euch, ihn zu lesen, scheut.", "tokens": ["Da\u00df", "ihr", "euch", ",", "ihn", "zu", "le\u00b7sen", ",", "scheut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ihr nicht seine Freunde seyd \u2014", "tokens": ["Da\u00df", "ihr", "nicht", "sei\u00b7ne", "Freun\u00b7de", "seyd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das l\u00e4\u00dft sich h\u00f6ren!", "tokens": ["Das", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Man ladet mich in G\u00e4rten ein.", "tokens": ["Man", "la\u00b7det", "mich", "in", "G\u00e4r\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie werden uns willkommen seyn \u2014", "tokens": ["Sie", "wer\u00b7den", "uns", "will\u00b7kom\u00b7men", "seyn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, ich f\u00fcrchte sie zu st\u00f6ren.", "tokens": ["Al\u00b7lein", ",", "ich", "f\u00fcrch\u00b7te", "sie", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist wohl viel Gesellschaft da? \u2014", "tokens": ["Es", "ist", "wohl", "viel", "Ge\u00b7sell\u00b7schaft", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es geht noch. Daphne \u2014 Daphne? Ja!", "tokens": ["Es", "geht", "noch", ".", "Daph\u00b7ne", "Daph\u00b7ne", "?", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "NN", "$(", "NN", "$.", "PTKANT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Das l\u00e4\u00dft sich h\u00f6ren!", "tokens": ["Das", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}