{"textgrid.poem.47530": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nie wallte sie zur Schule]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie wallte sie zur Schule;", "tokens": ["Nie", "wall\u00b7te", "sie", "zur", "Schu\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie f\u00fchrte sie die Spule,", "tokens": ["Nie", "f\u00fchr\u00b7te", "sie", "die", "Spu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die kritzende, der Feder;", "tokens": ["Die", "krit\u00b7zen\u00b7de", ",", "der", "Fe\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nie vor dem Lehrerstuhle", "tokens": ["Nie", "vor", "dem", "Leh\u00b7rer\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aufsagte sie ihr Spr\u00fcchlein,", "tokens": ["Auf\u00b7sag\u00b7te", "sie", "ihr", "Spr\u00fcch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hafisens s\u00fc\u00dfe Buhle;", "tokens": ["Ha\u00b7fi\u00b7sens", "s\u00fc\u00b7\u00dfe", "Buh\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch diese Feine meistert", "tokens": ["Doch", "die\u00b7se", "Fei\u00b7ne", "meis\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Meister all' der Schule.", "tokens": ["Die", "Meis\u00b7ter", "all'", "der", "Schu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}