{"textgrid.poem.48378": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cope schrieb einen Brief an den Kavalier:", "tokens": ["Co\u00b7pe", "schrieb", "ei\u00b7nen", "Brief", "an", "den", "Ka\u00b7va\u00b7lier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u00bbso du Mut hast, komm und fecht' mit mir,", "tokens": ["\u00bb", "so", "du", "Mut", "hast", ",", "komm", "und", "fecht'", "mit", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "NN", "VAFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und bist du nicht in zwei Stunden hier,", "tokens": ["Und", "bist", "du", "nicht", "in", "zwei", "Stun\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "CARD", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So komm' ich fr\u00fch am Morgen.\u00ab", "tokens": ["So", "komm'", "ich", "fr\u00fch", "am", "Mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Prinz Charlie sah hinein in den Brief;", "tokens": ["Prinz", "Char\u00b7lie", "sah", "hin\u00b7ein", "in", "den", "Brief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Er zog sein Schwert und lacht' und rief:", "tokens": ["Er", "zog", "sein", "Schwert", "und", "lacht'", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbund sind deine Gr\u00e4ben noch so tief,", "tokens": ["\u00bb", "und", "sind", "dei\u00b7ne", "Gr\u00e4\u00b7ben", "noch", "so", "tief", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wir kommen fr\u00fch am Morgen.\u00ab", "tokens": ["Wir", "kom\u00b7men", "fr\u00fch", "am", "Mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf, Hochlandsbursche, auf, ins Feld,", "tokens": ["Auf", ",", "Hoch\u00b7lands\u00b7bur\u00b7sche", ",", "auf", ",", "ins", "Feld", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grau-D\u00e4mmrung schon die Nacht erhellt,", "tokens": ["Grau\u00b7D\u00e4mm\u00b7rung", "schon", "die", "Nacht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo John Cope uns hinbestellt,", "tokens": ["Und", "wo", "John", "Co\u00b7pe", "uns", "hin\u00b7be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da stehn wir fr\u00fch am Morgen.", "tokens": ["Da", "stehn", "wir", "fr\u00fch", "am", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie, was? ob Cope noch schlafen mag?", "tokens": ["Wie", ",", "was", "?", "ob", "Co\u00b7pe", "noch", "schla\u00b7fen", "mag", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "$.", "KOUS", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wach auf, es ist schon heller Tag,", "tokens": ["Wach", "auf", ",", "es", "ist", "schon", "hel\u00b7ler", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rst du nicht Pfeif' und Trommelschlag?", "tokens": ["H\u00f6rst", "du", "nicht", "Pfeif'", "und", "Trom\u00b7mel\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir kommen fr\u00fch am Morgen.", "tokens": ["Wir", "kom\u00b7men", "fr\u00fch", "am", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Halt, Cope, was l\u00e4ufst du schon von fern?", "tokens": ["Halt", ",", "Co\u00b7pe", ",", "was", "l\u00e4ufst", "du", "schon", "von", "fern", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir sch\u00fcttelten dir die P\u00e4tschchen gern,", "tokens": ["Wir", "sch\u00fct\u00b7tel\u00b7ten", "dir", "die", "P\u00e4t\u00b7schchen", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun lauf' und gr\u00fc\u00df' uns deinen Herrn", "tokens": ["Nun", "lauf'", "und", "gr\u00fc\u00df'", "uns", "dei\u00b7nen", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und biet' ihm guten Morgen.", "tokens": ["Und", "biet'", "ihm", "gu\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Cope lief bis Leith mit rotem Gesicht;", "tokens": ["Co\u00b7pe", "lief", "bis", "Leith", "mit", "ro\u00b7tem", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u00bbwo sind deine Leute?\u00ab der Sheriff spricht,", "tokens": ["\u00bb", "wo", "sind", "dei\u00b7ne", "Leu\u00b7te", "?", "\u00ab", "der", "She\u00b7riff", "spricht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "\u00bbzum Teufel\u00ab, rief Cope, \u00bbich wei\u00df es nicht,", "tokens": ["\u00bb", "zum", "Teu\u00b7fel", "\u00ab", ",", "rief", "Co\u00b7pe", ",", "\u00bb", "ich", "wei\u00df", "es", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$(", "$,", "VVFIN", "NE", "$,", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah sie zuletzt heut morgen.\u00ab", "tokens": ["Ich", "sah", "sie", "zu\u00b7letzt", "heut", "mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}