{"textgrid.poem.57512": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: So f\u00e4llt denn unser Haupt? So mu\u00df des Landes Ehre,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So f\u00e4llt denn unser Haupt? So mu\u00df des Landes Ehre,", "tokens": ["So", "f\u00e4llt", "denn", "un\u00b7ser", "Haupt", "?", "So", "mu\u00df", "des", "Lan\u00b7des", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Reu\u00dfenstammes Preis gleichwohl in Sarg und Grab?", "tokens": ["Des", "Reu\u00b7\u00dfen\u00b7stam\u00b7mes", "Preis", "gleich\u00b7wohl", "in", "Sarg", "und", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach ja! die Schmerzenspost, die ich erschrocken h\u00f6re,", "tokens": ["Ach", "ja", "!", "die", "Schmer\u00b7zens\u00b7post", ",", "die", "ich", "er\u00b7schro\u00b7cken", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dringt meinen Augen Salz, dem Herzen Seufzer ab.", "tokens": ["Dringt", "mei\u00b7nen", "Au\u00b7gen", "Salz", ",", "dem", "Her\u00b7zen", "Seuf\u00b7zer", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ists; das Trauerblatt, das wir vorl\u00e4ngst gescheuet,", "tokens": ["So", "ists", ";", "das", "Trau\u00b7er\u00b7blatt", ",", "das", "wir", "vor\u00b7l\u00e4ngst", "ge\u00b7scheu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stellt sich nach kurzer Frist vor unsern Augen ein.", "tokens": ["Stellt", "sich", "nach", "kur\u00b7zer", "Frist", "vor", "un\u00b7sern", "Au\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Krankheit, die ihm l\u00e4ngst die lange Nacht gedr\u00e4uet,", "tokens": ["Die", "Krank\u00b7heit", ",", "die", "ihm", "l\u00e4ngst", "die", "lan\u00b7ge", "Nacht", "ge\u00b7dr\u00e4u\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verlacht der Aerzte Kunst und heischt den Leichenstein.", "tokens": ["Ver\u00b7lacht", "der", "A\u00b7erz\u00b7te", "Kunst", "und", "heischt", "den", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ich sah schon ganz erstarrt auf jenes Berges H\u00fcgeln,", "tokens": ["Ich", "sah", "schon", "ganz", "er\u00b7starrt", "auf", "je\u00b7nes", "Ber\u00b7ges", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der unsre Burg bedeckt, des matten Bothen Lauf:", "tokens": ["Der", "uns\u00b7re", "Burg", "be\u00b7deckt", ",", "des", "mat\u00b7ten", "Bo\u00b7then", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Ahndung schreckte schon mit schwarzen Trauersiegeln,", "tokens": ["Die", "Ahn\u00b7dung", "schreck\u00b7te", "schon", "mit", "schwar\u00b7zen", "Trau\u00b7er\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und endlich hub sein Wort den ganzen Zweifel auf.", "tokens": ["Und", "end\u00b7lich", "hub", "sein", "Wort", "den", "gan\u00b7zen", "Zwei\u00b7fel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es hie\u00df: der Herr ist todt! und ein beglaubtes Schreiben", "tokens": ["Es", "hie\u00df", ":", "der", "Herr", "ist", "todt", "!", "und", "ein", "be\u00b7glaub\u00b7tes", "Schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bekr\u00e4ftigte darauf sein tr\u00fcbes Klaggeschrey.", "tokens": ["Be\u00b7kr\u00e4f\u00b7tig\u00b7te", "da\u00b7rauf", "sein", "tr\u00fc\u00b7bes", "Klag\u00b7ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was Wunder, wenn wir da vor Schrecken sprachlos bleiben?", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "wir", "da", "vor", "Schre\u00b7cken", "sprach\u00b7los", "blei\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn unser aller Schmerz ist v\u00f6llig einerley.", "tokens": ["Denn", "un\u00b7ser", "al\u00b7ler", "Schmerz", "ist", "v\u00f6l\u00b7lig", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach! seufzten wir zuletzt: so ist es doch geschehen,", "tokens": ["Ach", "!", "seufz\u00b7ten", "wir", "zu\u00b7letzt", ":", "so", "ist", "es", "doch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was unsre K\u00fcmmerni\u00df bisher besorget hat:", "tokens": ["Was", "uns\u00b7re", "K\u00fcm\u00b7mer\u00b7ni\u00df", "bis\u00b7her", "be\u00b7sor\u00b7get", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So fand des Landes Gram, der treuen Diener Flehen,", "tokens": ["So", "fand", "des", "Lan\u00b7des", "Gram", ",", "der", "treu\u00b7en", "Die\u00b7ner", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey dir, Barmherziger! f\u00fcr die\u00dfmal keine statt?", "tokens": ["Bey", "dir", ",", "Barm\u00b7her\u00b7zi\u00b7ger", "!", "f\u00fcr", "die\u00df\u00b7mal", "kei\u00b7ne", "statt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "APPR", "ADV", "PIAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Warum soll dein Geschick und z\u00fcrnendes Entschlie\u00dfen,", "tokens": ["Wa\u00b7rum", "soll", "dein", "Ge\u00b7schick", "und", "z\u00fcr\u00b7nen\u00b7des", "Ent\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zu ganzer V\u00f6lker Qual so unerbittlich seyn?", "tokens": ["Zu", "gan\u00b7zer", "V\u00f6l\u00b7ker", "Qual", "so", "un\u00b7er\u00b7bitt\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie? stellst du, strenger Arm! wenn unsre Thr\u00e4nen flie\u00dfen,", "tokens": ["Wie", "?", "stellst", "du", ",", "stren\u00b7ger", "Arm", "!", "wenn", "uns\u00b7re", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das angedrohte Leid nicht mehr, wie vormals, ein?", "tokens": ["Das", "an\u00b7ge\u00b7droh\u00b7te", "Leid", "nicht", "mehr", ",", "wie", "vor\u00b7mals", ",", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O nein! der edle Stamm der hochgepriesnen Reu\u00dfen", "tokens": ["O", "nein", "!", "der", "ed\u00b7le", "Stamm", "der", "hoch\u00b7ge\u00b7pri\u00b7es\u00b7nen", "Reu\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Verliert sein \u00e4ltstes Haupt, das eine St\u00fctze war;", "tokens": ["Ver\u00b7liert", "sein", "\u00e4lts\u00b7tes", "Haupt", ",", "das", "ei\u00b7ne", "St\u00fct\u00b7ze", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja, was wir unsern Schmuck und unsre Krone hei\u00dfen,", "tokens": ["Ja", ",", "was", "wir", "un\u00b7sern", "Schmuck", "und", "uns\u00b7re", "Kro\u00b7ne", "hei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Liegt starr, entseelt und kalt auf einer Todtenbaar.", "tokens": ["Liegt", "starr", ",", "ent\u00b7seelt", "und", "kalt", "auf", "ei\u00b7ner", "Tod\u00b7ten\u00b7baar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Den Wissenschaften klug, Erfahrung reif gemacht;", "tokens": ["Den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "klug", ",", "Er\u00b7fah\u00b7rung", "reif", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dem der Geschichte Licht, von mehr als einem Lande,", "tokens": ["Dem", "der", "Ge\u00b7schich\u00b7te", "Licht", ",", "von", "mehr", "als", "ei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "$,", "APPR", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Von jedem Reich der Welt, die Staatskunst beygebracht;", "tokens": ["Von", "je\u00b7dem", "Reich", "der", "Welt", ",", "die", "Staats\u00b7kunst", "bey\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein rathender ", "tokens": ["Ein", "ra\u00b7then\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.34": {"text": "Der \u00fcber alles Gl\u00fcck und Ungl\u00fcck Meister blieb,", "tokens": ["Der", "\u00fc\u00b7ber", "al\u00b7les", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "Meis\u00b7ter", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und jedes Ungemach, mit ungemeinen Kr\u00e4ften", "tokens": ["Und", "je\u00b7des", "Un\u00b7ge\u00b7mach", ",", "mit", "un\u00b7ge\u00b7mei\u00b7nen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und festgesetzter Brust, beherzt zur\u00fccke trieb;", "tokens": ["Und", "fest\u00b7ge\u00b7setz\u00b7ter", "Brust", ",", "be\u00b7herzt", "zu\u00b7r\u00fc\u00b7cke", "trieb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Der sich die Gnade stets zur Richtschnur auserw\u00e4hlt;", "tokens": ["Der", "sich", "die", "Gna\u00b7de", "stets", "zur", "Richt\u00b7schnur", "au\u00b7ser\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja, w\u00e4r es keine Pflicht, die Laster scharf zu lohnen,", "tokens": ["Ja", ",", "w\u00e4r", "es", "kei\u00b7ne", "Pflicht", ",", "die", "Las\u00b7ter", "scharf", "zu", "loh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "PIAT", "NN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So h\u00e4tt er wahrlich gern auch diese losgez\u00e4hlt.", "tokens": ["So", "h\u00e4tt", "er", "wahr\u00b7lich", "gern", "auch", "die\u00b7se", "los\u00b7ge\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So \u00e4chzen Stadt und Land, die Berge sammt den Fl\u00fcssen,", "tokens": ["So", "\u00e4ch\u00b7zen", "Stadt", "und", "Land", ",", "die", "Ber\u00b7ge", "sammt", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So seufzet Hof und Volk und jeder Unterthan;", "tokens": ["So", "seuf\u00b7zet", "Hof", "und", "Volk", "und", "je\u00b7der", "Un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"]