{"dta.poem.8406": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Wilhelm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.56", "nl:0.28", "no:0.14"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "verlassene Wilhelm", "tokens": ["ver\u00b7las\u00b7se\u00b7ne", "Wil\u00b7helm"], "token_info": ["word", "word"], "pos": ["ADJA", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "W\u00fcnschte den Tod, und besuchte nicht mehr den", "tokens": ["W\u00fcnschte", "den", "Tod", ",", "und", "be\u00b7such\u00b7te", "nicht", "mehr", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "ART"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "gefl\u00fcgelten Reigen,", "tokens": ["ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Rei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht das Ostergelag und das Fest der bemaleten", "tokens": ["Nicht", "das", "Os\u00b7ter\u00b7ge\u00b7lag", "und", "das", "Fest", "der", "be\u00b7ma\u00b7le\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "KON", "ART", "NN", "ART", "ADJA"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Eier,", "tokens": ["Ei\u00b7er", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Nicht den gaukelnden Tanz um die Osterflamme des", "tokens": ["Nicht", "den", "gau\u00b7keln\u00b7den", "Tanz", "um", "die", "Os\u00b7ter\u00b7flam\u00b7me", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "H\u00fcgels.", "tokens": ["H\u00fc\u00b7gels", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Einsam war er, und still wie das Grab, und glaubte", "tokens": ["Ein\u00b7sam", "war", "er", ",", "und", "still", "wie", "das", "Grab", ",", "und", "glaub\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "mit jedem", "tokens": ["mit", "je\u00b7dem"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Tritt in die Erde zu sinken. Die Knaben und M\u00e4dchen", "tokens": ["Tritt", "in", "die", "Er\u00b7de", "zu", "sin\u00b7ken", ".", "Die", "Kna\u00b7ben", "und", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "des Dorfes", "tokens": ["des", "Dor\u00b7fes"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Brachen Main, und schm\u00fcckten das Haus und die", "tokens": ["Bra\u00b7chen", "Main", ",", "und", "schm\u00fcck\u00b7ten", "das", "Haus", "und", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "VVFIN", "ART", "NN", "KON", "ART"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "l\u00e4ndliche Diele,", "tokens": ["l\u00e4nd\u00b7li\u00b7che", "Die\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und begr\u00fcssten den heiligen Abend vor Pfingsten", "tokens": ["Und", "be\u00b7gr\u00fcs\u00b7sten", "den", "hei\u00b7li\u00b7gen", "A\u00b7bend", "vor", "Pfings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "mit Liedern.", "tokens": ["mit", "Lie\u00b7dern", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Wilhelm floh das Gew\u00fchl der begl\u00fcckten fr\u00f6hlichen", "tokens": ["Wil\u00b7helm", "floh", "das", "Ge\u00b7w\u00fchl", "der", "be\u00b7gl\u00fcck\u00b7ten", "fr\u00f6h\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "ADJA", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Leute,", "tokens": ["Leu\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Wandelt' \u00fcber den Gottesacker, und ging in die", "tokens": ["Wan\u00b7delt'", "\u00fc\u00b7ber", "den", "Got\u00b7te\u00b7sa\u00b7cker", ",", "und", "ging", "in", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Kirche,", "tokens": ["Kir\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Nahm den Kranz der geliebten Braut von der Wand,", "tokens": ["Nahm", "den", "Kranz", "der", "ge\u00b7lieb\u00b7ten", "Braut", "von", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "und kniete", "tokens": ["und", "knie\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "An dem Altar, und barg das Gesicht in die Blumen", "tokens": ["An", "dem", "Al\u00b7tar", ",", "und", "barg", "das", "Ge\u00b7sicht", "in", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "des Kranzes,", "tokens": ["des", "Kran\u00b7zes", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Flehte weinend zu Gott: O entnim mich der Erde,", "tokens": ["Fleh\u00b7te", "wei\u00b7nend", "zu", "Gott", ":", "O", "ent\u00b7nim", "mich", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$.", "NE", "NE", "PPER", "ART", "NN", "$,"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "mein Vater!", "tokens": ["mein", "Va\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Ruf mich zu meiner Entschlummerten! Doch dein", "tokens": ["Ruf", "mich", "zu", "mei\u00b7ner", "Ent\u00b7schlum\u00b7mer\u00b7ten", "!", "Doch", "dein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$.", "KON", "PPOSAT"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Wille geschehe!", "tokens": ["Wil\u00b7le", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Lispelnd bebte das Gold und die Flitterblumen des", "tokens": ["Lis\u00b7pelnd", "beb\u00b7te", "das", "Gold", "und", "die", "Flit\u00b7ter\u00b7blu\u00b7men", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Kranzes,", "tokens": ["Kran\u00b7zes", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Lieblich rauschten die flatternden B\u00e4nder, wie Bl\u00e4t-", "tokens": ["Lieb\u00b7lich", "rauschten", "die", "flat\u00b7tern\u00b7den", "B\u00e4n\u00b7der", ",", "wie", "Bl\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "ter im Winde,", "tokens": ["ter", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.32": {"text": "Und ein fliegender Lichtglanz flog durch die Fenster", "tokens": ["Und", "ein", "flie\u00b7gen\u00b7der", "Licht\u00b7glanz", "flog", "durch", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.33": {"text": "der Kirche.", "tokens": ["der", "Kir\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Ruhiger wandelte Wilhelm nach Haus. Bald h\u00f6rten", "tokens": ["Ru\u00b7hi\u00b7ger", "wan\u00b7del\u00b7te", "Wil\u00b7helm", "nach", "Haus", ".", "Bald", "h\u00f6r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "NE", "APPR", "NN", "$.", "ADV", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.35": {"text": "die Schwestern", "tokens": ["die", "Schwes\u00b7tern"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Drauf die Todtenuhr in der Kammer pickern, und", "tokens": ["Drauf", "die", "Tod\u00b7te\u00b7nuhr", "in", "der", "Kam\u00b7mer", "pi\u00b7ckern", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,", "KON"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "sahen", "tokens": ["sa\u00b7hen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Auf der Diele den Sarg, und den Pfarrer im Mantel", "tokens": ["Auf", "der", "Die\u00b7le", "den", "Sarg", ",", "und", "den", "Pfar\u00b7rer", "im", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "daneben;", "tokens": ["da\u00b7ne\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.40": {"text": "Und das Leichhuhn schlug an die Kammerfenster,", "tokens": ["Und", "das", "Leich\u00b7huhn", "schlug", "an", "die", "Kam\u00b7mer\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.41": {"text": "und heulte.", "tokens": ["und", "heul\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Wenige Wochen, da starb der verlassene traurige", "tokens": ["We\u00b7ni\u00b7ge", "Wo\u00b7chen", ",", "da", "starb", "der", "ver\u00b7las\u00b7se\u00b7ne", "trau\u00b7ri\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.43": {"text": "Wilhelm,", "tokens": ["Wil\u00b7helm", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Und sein gr\u00fcnendes Grab ragt hart am Grabe des", "tokens": ["Und", "sein", "gr\u00fc\u00b7nen\u00b7des", "Grab", "ragt", "hart", "am", "Gra\u00b7be", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "ART"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.45": {"text": "M\u00e4dchens.", "tokens": ["M\u00e4d\u00b7chens", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}