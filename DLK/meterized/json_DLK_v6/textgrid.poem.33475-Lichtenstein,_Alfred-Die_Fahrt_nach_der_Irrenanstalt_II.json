{"textgrid.poem.33475": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Fahrt nach der Irrenanstalt II", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines M\u00e4dchen hockt mit einem kleinen Bruder", "tokens": ["Ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", "hockt", "mit", "ei\u00b7nem", "klei\u00b7nen", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei einer umgest\u00fcrzten Wassertonne.", "tokens": ["Bei", "ei\u00b7ner", "um\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Was\u00b7ser\u00b7ton\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Fetzen, fressend liegt ein Menschenluder", "tokens": ["In", "Fet\u00b7zen", ",", "fres\u00b7send", "liegt", "ein", "Men\u00b7schen\u00b7lu\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Zigarrenstummel auf der gelben Sonne.", "tokens": ["Wie", "ein", "Zi\u00b7gar\u00b7ren\u00b7stum\u00b7mel", "auf", "der", "gel\u00b7ben", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwei d\u00fcnne Ziegen stehn in weiten gr\u00fcnen R\u00e4umen", "tokens": ["Zwei", "d\u00fcn\u00b7ne", "Zie\u00b7gen", "stehn", "in", "wei\u00b7ten", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Pfl\u00f6cken, deren Strick sich manchmal straffte.", "tokens": ["An", "Pfl\u00f6\u00b7cken", ",", "de\u00b7ren", "Strick", "sich", "manch\u00b7mal", "straff\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unsichtbar hinter ungeheuren B\u00e4umen", "tokens": ["Un\u00b7sicht\u00b7bar", "hin\u00b7ter", "un\u00b7ge\u00b7heu\u00b7ren", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Unglaublich friedlich naht das gro\u00dfe Grauenhafte.", "tokens": ["Un\u00b7glaub\u00b7lich", "fried\u00b7lich", "naht", "das", "gro\u00b7\u00dfe", "Grau\u00b7en\u00b7haf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}