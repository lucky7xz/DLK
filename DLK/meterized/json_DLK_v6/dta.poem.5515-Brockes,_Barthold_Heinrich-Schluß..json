{"dta.poem.5515": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verargt mir's nicht, geliebte Menschen, da\u00df euch von der", "tokens": ["Ver\u00b7argt", "mir's", "nicht", ",", "ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "da\u00df", "euch", "von", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PTKNEG", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so sch\u00f6nen Welt", "tokens": ["so", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So vieles und so oft durch mich wird wiederhohlt und vor-", "tokens": ["So", "vie\u00b7les", "und", "so", "oft", "durch", "mich", "wird", "wie\u00b7der\u00b7hohlt", "und", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "KON", "ADV", "ADV", "APPR", "PPER", "VAFIN", "VVPP", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gestellt.", "tokens": ["ge\u00b7stellt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ich halt\u2019 es theils f\u00fcr meine Pflicht; theils seyd ihr selber", "tokens": ["Ich", "halt'", "es", "theils", "f\u00fcr", "mei\u00b7ne", "Pflicht", ";", "theils", "seyd", "ihr", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schuld daran,", "tokens": ["Schuld", "da\u00b7ran", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da ich (doch manchen ausgenommen) von vielen noch nicht", "tokens": ["Da", "ich", "(", "doch", "man\u00b7chen", "aus\u00b7ge\u00b7nom\u00b7men", ")", "von", "vie\u00b7len", "noch", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "ADV", "PIS", "VVPP", "$(", "APPR", "PIS", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sagen kann,", "tokens": ["sa\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Da\u00df ihr, aus dem gewohnten Schlaf (so doch so n\u00f6thig)", "tokens": ["Da\u00df", "ihr", ",", "aus", "dem", "ge\u00b7wohn\u00b7ten", "Schlaf", "(", "so", "doch", "so", "n\u00f6\u00b7thig", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "aufgewacht,", "tokens": ["auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Des grossen Sch\u00f6pfers grosse Wunder, und in denselben,", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "gros\u00b7se", "Wun\u00b7der", ",", "und", "in", "den\u00b7sel\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "KON", "APPR", "PDS", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "seine Macht,", "tokens": ["sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und Lieb\u2019 und Wei\u00dfheit schmeckt und seht. Daher ich noch", "tokens": ["Und", "Lieb'", "und", "Wei\u00df\u00b7heit", "schmeckt", "und", "seht", ".", "Da\u00b7her", "ich", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$.", "PAV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nicht m\u00fcde werde,", "tokens": ["nicht", "m\u00fc\u00b7de", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Der grossen GOttheit, Lieb\u2019 und Allmacht, im Schmuck des", "tokens": ["Der", "gros\u00b7sen", "Got\u00b7theit", ",", "Lieb'", "und", "All\u00b7macht", ",", "im", "Schmuck", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Himmels und der Erde,", "tokens": ["Him\u00b7mels", "und", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Euch unaufh\u00f6rlich anzupreisen, und dulde, der Gesch\u00f6pfe", "tokens": ["Euch", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "an\u00b7zu\u00b7prei\u00b7sen", ",", "und", "dul\u00b7de", ",", "der", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "VVIZU", "$,", "KON", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Herrn", "tokens": ["Herrn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Zu Ehren, euer Nasen r\u00fcmpfen, und euer h\u00f6nisch Tadeln,", "tokens": ["Zu", "Eh\u00b7ren", ",", "eu\u00b7er", "Na\u00b7sen", "r\u00fcmp\u00b7fen", ",", "und", "eu\u00b7er", "h\u00f6\u00b7nisch", "Ta\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "VVINF", "$,", "KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "gern;", "tokens": ["gern", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Vin auch der Hofnung, da\u00df, da ihr, nur durch Gewohnheit", "tokens": ["Vin", "auch", "der", "Hof\u00b7nung", ",", "da\u00df", ",", "da", "ihr", ",", "nur", "durch", "Ge\u00b7wohn\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "$,", "KOUS", "$,", "KOUS", "PPER", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "schlaft, dennoch,", "tokens": ["schlaft", ",", "den\u00b7noch", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Auch ihr, wo nicht; doch eure Kinder, des Undancks ungl\u00fcck-", "tokens": ["Auch", "ihr", ",", "wo", "nicht", ";", "doch", "eu\u00b7re", "Kin\u00b7der", ",", "des", "Un\u00b7dancks", "un\u00b7gl\u00fc\u00b7ck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PWAV", "PTKNEG", "$.", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "seeligs Joch", "tokens": ["see\u00b7ligs", "Joch"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Dereinst von Halse werffen werdet. Ich hoff es, und ich", "tokens": ["De\u00b7reinst", "von", "Hal\u00b7se", "werf\u00b7fen", "wer\u00b7det", ".", "Ich", "hoff", "es", ",", "und", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVINF", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "glaub' es schier.", "tokens": ["glaub'", "es", "schier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Ja, wenn es nicht geschehen sollte, wie ich jedoch nicht", "tokens": ["Ja", ",", "wenn", "es", "nicht", "ge\u00b7sche\u00b7hen", "soll\u00b7te", ",", "wie", "ich", "je\u00b7doch", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PWAV", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "hoffen will;", "tokens": ["hof\u00b7fen", "will", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "So f\u00fchl ich dennoch solche Lust, und solchen strengen Trieb", "tokens": ["So", "f\u00fchl", "ich", "den\u00b7noch", "sol\u00b7che", "Lust", ",", "und", "sol\u00b7chen", "stren\u00b7gen", "Trieb"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "in mir,", "tokens": ["in", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "Da\u00df ich mich nicht entschliessen kann, von GOttes Wunder-", "tokens": ["Da\u00df", "ich", "mich", "nicht", "ent\u00b7schlies\u00b7sen", "kann", ",", "von", "Got\u00b7tes", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wercken still,", "tokens": ["Wer\u00b7cken", "still", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Und, euch zu willen, stumm zu bleiben. Jetzt da ich aber-", "tokens": ["Und", ",", "euch", "zu", "wil\u00b7len", ",", "stumm", "zu", "blei\u00b7ben", ".", "Jetzt", "da", "ich", "a\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "APPR", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$.", "ADV", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "mahl die Zier,", "tokens": ["mahl", "die", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}}}}}