{"dta.poem.12484": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Amnestie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie l\u00e4cheln! \u2014 doch ihr L\u00e4cheln ist verloren,", "tokens": ["Sie", "l\u00e4\u00b7cheln", "!", "doch", "ihr", "L\u00e4\u00b7cheln", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vergebens ihrer Blicke Sonnenschein;", "tokens": ["Ver\u00b7ge\u00b7bens", "ih\u00b7rer", "Bli\u00b7cke", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ich f\u00fcr F\u00fcrstendonner keine Ohren,", "tokens": ["Wie", "ich", "f\u00fcr", "F\u00fcrs\u00b7ten\u00b7don\u00b7ner", "kei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich kein Herz f\u00fcr ihre Schmeichelei'n.", "tokens": ["Hab'", "ich", "kein", "Herz", "f\u00fcr", "ih\u00b7re", "Schmei\u00b7che\u00b7lei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O seht euch vor, es ist ein falsches Treiben!", "tokens": ["O", "seht", "euch", "vor", ",", "es", "ist", "ein", "fal\u00b7sches", "Trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und diese Gnade \u2014 unser j\u00fcngst Gericht!", "tokens": ["Und", "die\u00b7se", "Gna\u00b7de", "un\u00b7ser", "j\u00fcngst", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir wollen, Br\u00fcder, auf dem Wahlplatz bleiben:", "tokens": ["Wir", "wol\u00b7len", ",", "Br\u00fc\u00b7der", ",", "auf", "dem", "Wahl\u00b7platz", "blei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Garde stirbt, doch sie ergiebt sich nicht!", "tokens": ["Die", "Gar\u00b7de", "stirbt", ",", "doch", "sie", "er\u00b7giebt", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Rosen gilt's die Freiheit zu erdr\u00fccken,", "tokens": ["In", "Ro\u00b7sen", "gilt's", "die", "Frei\u00b7heit", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich in Ketten nicht erdrosseln l\u00e4\u00dft:", "tokens": ["Die", "sich", "in", "Ket\u00b7ten", "nicht", "er\u00b7dros\u00b7seln", "l\u00e4\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O g\u00f6nnt dem Volk, dem P\u00f6bel sein Entz\u00fccken,", "tokens": ["O", "g\u00f6nnt", "dem", "Volk", ",", "dem", "P\u00f6\u00b7bel", "sein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die\u00df falsche, heuchlerische Freudenfest!", "tokens": ["Die\u00df", "fal\u00b7sche", ",", "heuch\u00b7le\u00b7ri\u00b7sche", "Freu\u00b7den\u00b7fest", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihn hungert wohl, er geht nach seinem Brode,", "tokens": ["Ihn", "hun\u00b7gert", "wohl", ",", "er", "geht", "nach", "sei\u00b7nem", "Bro\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das man ihm f\u00fcrder reichlicher verspricht:", "tokens": ["Das", "man", "ihm", "f\u00fcr\u00b7der", "reich\u00b7li\u00b7cher", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Uns d\u00fcrstet. Drum: die\u00df Glas dem freien Tode!", "tokens": ["Uns", "d\u00fcrs\u00b7tet", ".", "Drum", ":", "die\u00df", "Glas", "dem", "frei\u00b7en", "To\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "PAV", "$.", "PDS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Garde stirbt, doch sie ergiebt sich nicht!", "tokens": ["Die", "Gar\u00b7de", "stirbt", ",", "doch", "sie", "er\u00b7giebt", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ei schaut, der K\u00e4fig wird nun aufgeschlossen,", "tokens": ["Ei", "schaut", ",", "der", "K\u00e4\u00b7fig", "wird", "nun", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da l\u00e4ngst der Vogel nicht mehr fliegen kann;", "tokens": ["Da", "l\u00e4ngst", "der", "Vo\u00b7gel", "nicht", "mehr", "flie\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So mancher unsrer alten Kampfgenossen", "tokens": ["So", "man\u00b7cher", "uns\u00b7rer", "al\u00b7ten", "Kampf\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist nun ein m\u00fcder, ein gebrochner Mann!", "tokens": ["Ist", "nun", "ein", "m\u00fc\u00b7der", ",", "ein", "ge\u00b7broch\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00fcbsch sind die Blumen, drin ihr sprecht; nur schade,", "tokens": ["H\u00fcbsch", "sind", "die", "Blu\u00b7men", ",", "drin", "ihr", "sprecht", ";", "nur", "scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$.", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Da\u00df draus der Dorn des Despotismus sticht.", "tokens": ["Da\u00df", "draus", "der", "Dorn", "des", "Des\u00b7po\u00b7tis\u00b7mus", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Recht vor Gott braucht keines K\u00f6nigs Gnade:", "tokens": ["Das", "Recht", "vor", "Gott", "braucht", "kei\u00b7nes", "K\u00f6\u00b7nigs", "Gna\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Garde stirbt, doch sie ergiebt sich nicht!", "tokens": ["Die", "Gar\u00b7de", "stirbt", ",", "doch", "sie", "er\u00b7giebt", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gesch\u00e4ftig dr\u00e4ngt das Volk von nah' und ferne,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "dr\u00e4ngt", "das", "Volk", "von", "nah'", "und", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des F\u00fcrsten H\u00e4nde k\u00fcssend, sich heran:", "tokens": ["Des", "F\u00fcrs\u00b7ten", "H\u00e4n\u00b7de", "k\u00fcs\u00b7send", ",", "sich", "he\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es sei \u2014 wir folgen unserm eignen Sterne,", "tokens": ["Es", "sei", "wir", "fol\u00b7gen", "un\u00b7serm", "eig\u00b7nen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Des Thrones Himmel ist nicht seine Bahn.", "tokens": ["Des", "Thro\u00b7nes", "Him\u00b7mel", "ist", "nicht", "sei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mag sich die Welt im Strahl der Gnade sonnen,", "tokens": ["Mag", "sich", "die", "Welt", "im", "Strahl", "der", "Gna\u00b7de", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich kenn' ein F\u00e4hnlein doch, das weiter ficht;", "tokens": ["Ich", "kenn'", "ein", "F\u00e4hn\u00b7lein", "doch", ",", "das", "wei\u00b7ter", "ficht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Frisch, meine Jugend, frisch den Kampf begonnen!", "tokens": ["Frisch", ",", "mei\u00b7ne", "Ju\u00b7gend", ",", "frisch", "den", "Kampf", "be\u00b7gon\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Garde stirbt, doch sie ergiebt sich nicht!", "tokens": ["Die", "Gar\u00b7de", "stirbt", ",", "doch", "sie", "er\u00b7giebt", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was war denn zu vergessen und vergeben,", "tokens": ["Was", "war", "denn", "zu", "ver\u00b7ges\u00b7sen", "und", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PTKZU", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und welche Todess\u00fcnde zu verzeihn?", "tokens": ["Und", "wel\u00b7che", "To\u00b7des\u00b7s\u00fcn\u00b7de", "zu", "ver\u00b7zeihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach mancher Krone pflegten wir zu streben;", "tokens": ["Nach", "man\u00b7cher", "Kro\u00b7ne", "pfleg\u00b7ten", "wir", "zu", "stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sagt, schenkt man in Euren Kronen Wein?", "tokens": ["Doch", "sagt", ",", "schenkt", "man", "in", "Eu\u00b7ren", "Kro\u00b7nen", "Wein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wir wollten uns so gern mit euch vers\u00f6hnen!", "tokens": ["Wir", "woll\u00b7ten", "uns", "so", "gern", "mit", "euch", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebt Raum der Freiheit, wie dem Tageslicht!", "tokens": ["Gebt", "Raum", "der", "Frei\u00b7heit", ",", "wie", "dem", "Ta\u00b7ges\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr zaudert? \u2014 Gut, so la\u00dft den Schlachtruf t\u00f6nen:", "tokens": ["Ihr", "zau\u00b7dert", "?", "Gut", ",", "so", "la\u00dft", "den", "Schlach\u00b7truf", "t\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die Garde stirbt, doch sie ergiebt sich nicht!", "tokens": ["Die", "Gar\u00b7de", "stirbt", ",", "doch", "sie", "er\u00b7giebt", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So will's die Zeit! sie heischet Feuerzungen,", "tokens": ["So", "will's", "die", "Zeit", "!", "sie", "hei\u00b7schet", "Feu\u00b7er\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Sturm verweht der Liebe sanften Hauch;", "tokens": ["Ihr", "Sturm", "ver\u00b7weht", "der", "Lie\u00b7be", "sanf\u00b7ten", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch was wir f\u00fcr die Freiheit einst errungen,", "tokens": ["Doch", "was", "wir", "f\u00fcr", "die", "Frei\u00b7heit", "einst", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Errangen wir f\u00fcr unsre Liebsten auch.", "tokens": ["Er\u00b7ran\u00b7gen", "wir", "f\u00fcr", "uns\u00b7re", "Liebs\u00b7ten", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Alle jubelnd in die H\u00e4nde schlagen,", "tokens": ["Wenn", "Al\u00b7le", "ju\u00b7belnd", "in", "die", "H\u00e4n\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", 