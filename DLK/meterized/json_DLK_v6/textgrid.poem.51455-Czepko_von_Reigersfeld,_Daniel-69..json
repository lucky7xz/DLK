{"textgrid.poem.51455": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im h\u00f6chsten Wesen lag die Seele zugeschlossen,", "tokens": ["Im", "h\u00f6chs\u00b7ten", "We\u00b7sen", "lag", "die", "See\u00b7le", "zu\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Wunder! Gott und Sie die sind zugleich entsprossen.", "tokens": ["O", "Wun\u00b7der", "!", "Gott", "und", "Sie", "die", "sind", "zu\u00b7gleich", "ent\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "KON", "PPER", "PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}