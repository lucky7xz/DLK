{"textgrid.poem.59724": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fcck auf die Reise! Pilgre fort!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck auf die Reise! Pilgre fort!", "tokens": ["Gl\u00fcck", "auf", "die", "Rei\u00b7se", "!", "Pilg\u00b7re", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lockt dich weg mit Wunderkl\u00e4ngen,", "tokens": ["Es", "lockt", "dich", "weg", "mit", "Wun\u00b7der\u00b7kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die weite Welt, sie lockt dich fort", "tokens": ["Die", "wei\u00b7te", "Welt", ",", "sie", "lockt", "dich", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Vaterhaus, dem stillen, engen.", "tokens": ["Vom", "Va\u00b7ter\u00b7haus", ",", "dem", "stil\u00b7len", ",", "en\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh, Sohn! Viel Sch\u00f6nes wirst du schaun", "tokens": ["Geh", ",", "Sohn", "!", "Viel", "Sch\u00f6\u00b7nes", "wirst", "du", "schaun"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In neuen Toren, neuen Gattern,", "tokens": ["In", "neu\u00b7en", "To\u00b7ren", ",", "neu\u00b7en", "Gat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch bunte Schl\u00f6sser gnug dir baun,", "tokens": ["Auch", "bun\u00b7te", "Schl\u00f6s\u00b7ser", "gnug", "dir", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die spanischen Schl\u00f6ssern gleich zerflattern.", "tokens": ["Die", "spa\u00b7ni\u00b7schen", "Schl\u00f6s\u00b7sern", "gleich", "zer\u00b7flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch horch', vernimm mein letztes Wort:", "tokens": ["Doch", "horch'", ",", "ver\u00b7nimm", "mein", "letz\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen jenen Zauberglocken,", "tokens": ["Von", "al\u00b7len", "je\u00b7nen", "Zau\u00b7ber\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die rastlos weg von Ort zu Ort", "tokens": ["Die", "rast\u00b7los", "weg", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den lebensl\u00fcsternen J\u00fcngling locken,", "tokens": ["Den", "le\u00b7bens\u00b7l\u00fcs\u00b7ter\u00b7nen", "J\u00fcng\u00b7ling", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "T\u00f6nt endlich klagend durch ein Ton", "tokens": ["T\u00f6nt", "end\u00b7lich", "kla\u00b7gend", "durch", "ein", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie aus der Erde fernsten Enden,", "tokens": ["Wie", "aus", "der", "Er\u00b7de", "ferns\u00b7ten", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Klang der Sehnsucht, dir, o Sohn,", "tokens": ["Ein", "Klang", "der", "Sehn\u00b7sucht", ",", "dir", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Eingeweide umzuwenden.", "tokens": ["Die", "Ein\u00b7ge\u00b7wei\u00b7de", "um\u00b7zu\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zur\u00fcck zur Heimat klingt der Klang,", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Hei\u00b7mat", "klingt", "der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck zum engen Vaterhause,", "tokens": ["Zu\u00b7r\u00fcck", "zum", "en\u00b7gen", "Va\u00b7ter\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir wird in weiter Welt so bang,", "tokens": ["Dir", "wird", "in", "wei\u00b7ter", "Welt", "so", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sehnst dich nach der stillen Klause.", "tokens": ["Du", "sehnst", "dich", "nach", "der", "stil\u00b7len", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O bringst du dann dein eignes Herz", "tokens": ["O", "bringst", "du", "dann", "dein", "eig\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mit aus wildem Weltgewimmel,", "tokens": ["Noch", "mit", "aus", "wil\u00b7dem", "Welt\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So war dein Pilgern froher Scherz,", "tokens": ["So", "war", "dein", "Pil\u00b7gern", "fro\u00b7her", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Spiel auf Erden hin zum Himmel.", "tokens": ["Ein", "Spiel", "auf", "Er\u00b7den", "hin", "zum", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}