{"textgrid.poem.67171": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Chiron", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du, Nachdenkliches! das immer mu\u00df", "tokens": ["Wo", "bist", "du", ",", "Nach\u00b7denk\u00b7li\u00b7ches", "!", "das", "im\u00b7mer", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$.", "ART", "ADV", "VMFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur Seite gehn, zu Zeiten, wo bist du, Licht?", "tokens": ["Zur", "Sei\u00b7te", "gehn", ",", "zu", "Zei\u00b7ten", ",", "wo", "bist", "du", ",", "Licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "APPR", "NN", "$,", "PWAV", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl ist das Herz wach, doch mir z\u00fcrnt, mich", "tokens": ["Wohl", "ist", "das", "Herz", "wach", ",", "doch", "mir", "z\u00fcrnt", ",", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hemmt die erstaunende Nacht nun immer", "tokens": ["Hemmt", "die", "er\u00b7stau\u00b7nen\u00b7de", "Nacht", "nun", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst n\u00e4mlich folgt ich Kr\u00e4utern des Walds und lauscht", "tokens": ["Sonst", "n\u00e4m\u00b7lich", "folgt", "ich", "Kr\u00e4u\u00b7tern", "des", "Walds", "und", "lauscht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein weiches Wild am H\u00fcgel; und nie umsonst.", "tokens": ["Ein", "wei\u00b7ches", "Wild", "am", "H\u00fc\u00b7gel", ";", "und", "nie", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$.", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nie t\u00e4uschten, auch nicht einmal deine", "tokens": ["Nie", "t\u00e4uschten", ",", "auch", "nicht", "ein\u00b7mal", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "PTKNEG", "ADV", "PPOSAT"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "V\u00f6gel; denn allzubereit fast kamst du,", "tokens": ["V\u00f6\u00b7gel", ";", "denn", "all\u00b7zu\u00b7be\u00b7reit", "fast", "kamst", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "So F\u00fcllen oder Garten dir labend ward,", "tokens": ["So", "F\u00fcl\u00b7len", "o\u00b7der", "Gar\u00b7ten", "dir", "la\u00b7bend", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ratschlagend, Herzens wegen; wo bist du, Licht?", "tokens": ["Rat\u00b7schla\u00b7gend", ",", "Her\u00b7zens", "we\u00b7gen", ";", "wo", "bist", "du", ",", "Licht", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "APPR", "$.", "PWAV", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Herz ist wieder wach, doch herzlos", "tokens": ["Das", "Herz", "ist", "wie\u00b7der", "wach", ",", "doch", "herz\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "ADV", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zieht die gewaltige Nacht mich immer.", "tokens": ["Zieht", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Nacht", "mich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wars wohl. Und von Krokus und Thymian", "tokens": ["Ich", "wars", "wohl", ".", "Und", "von", "Kro\u00b7kus", "und", "Thy\u00b7mi\u00b7an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "KON", "APPR", "NE", "KON", "NE"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und Korn gab mir die Erde den ersten Strau\u00df.", "tokens": ["Und", "Korn", "gab", "mir", "die", "Er\u00b7de", "den", "ers\u00b7ten", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und bei der Sterne K\u00fchle lernt ich,", "tokens": ["Und", "bei", "der", "Ster\u00b7ne", "K\u00fch\u00b7le", "lernt", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber das Nennbare nur. Und bei mir", "tokens": ["A\u00b7ber", "das", "Nenn\u00b7ba\u00b7re", "nur", ".", "Und", "bei", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$.", "KON", "APPR", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Das wilde Feld entzaubernd, das traurge, zog", "tokens": ["Das", "wil\u00b7de", "Feld", "ent\u00b7zau\u00b7bernd", ",", "das", "traur\u00b7ge", ",", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PDS", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Halbgott, Zevs Knecht, ein, der gerade Mann;", "tokens": ["Der", "Halb\u00b7gott", ",", "Zevs", "Knecht", ",", "ein", ",", "der", "ge\u00b7ra\u00b7de", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NE", "$,", "PTKVZ", "$,", "PRELS", "ADV", "NN", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nun sitz ich still allein, von einer", "tokens": ["Nun", "sitz", "ich", "still", "al\u00b7lein", ",", "von", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ADV", "$,", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stunde zur anderen, und Gestalten", "tokens": ["Stun\u00b7de", "zur", "an\u00b7de\u00b7ren", ",", "und", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "$,", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Aus frischer Erd und Wolken der Liebe schafft,", "tokens": ["Aus", "fri\u00b7scher", "Erd", "und", "Wol\u00b7ken", "der", "Lie\u00b7be", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil Gift ist zwischen uns, mein Gedanke nun;", "tokens": ["Weil", "Gift", "ist", "zwi\u00b7schen", "uns", ",", "mein", "Ge\u00b7dan\u00b7ke", "nun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ferne lausch ich hin, ob nicht ein", "tokens": ["Und", "fer\u00b7ne", "lausch", "ich", "hin", ",", "ob", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPER", "PTKVZ", "$,", "KOUS", "PTKNEG", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freundlicher Retter vielleicht mir komme.", "tokens": ["Freund\u00b7li\u00b7cher", "Ret\u00b7ter", "viel\u00b7leicht", "mir", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dann h\u00f6r ich oft den Wagen des Donnerers", "tokens": ["Dann", "h\u00f6r", "ich", "oft", "den", "Wa\u00b7gen", "des", "Don\u00b7ne\u00b7rers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Mittag, wenn er naht, der bekannteste,", "tokens": ["Am", "Mit\u00b7tag", ",", "wenn", "er", "naht", ",", "der", "be\u00b7kann\u00b7tes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn ihm das Haus bebt und der Boden", "tokens": ["Wenn", "ihm", "das", "Haus", "bebt", "und", "der", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reiniget sich, und die Qual Echo wird.", "tokens": ["Rei\u00b7ni\u00b7get", "sich", ",", "und", "die", "Qual", "E\u00b7cho", "wird", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Den Retter h\u00f6r ich dann in der Nacht, ich h\u00f6r", "tokens": ["Den", "Ret\u00b7ter", "h\u00f6r", "ich", "dann", "in", "der", "Nacht", ",", "ich", "h\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn t\u00f6tend, den Befreier, und drunten voll", "tokens": ["Ihn", "t\u00f6\u00b7tend", ",", "den", "Be\u00b7frei\u00b7er", ",", "und", "drun\u00b7ten", "voll"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "ART", "NN", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von \u00fcppgem Kraut, als in Gesichten,", "tokens": ["Von", "\u00fcpp\u00b7gem", "Kraut", ",", "als", "in", "Ge\u00b7sich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schau ich die Erd, ein gewaltig Feuer;", "tokens": ["Schau", "ich", "die", "Erd", ",", "ein", "ge\u00b7wal\u00b7tig", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Die Tage aber wechseln, wenn einer dann", "tokens": ["Die", "Ta\u00b7ge", "a\u00b7ber", "wech\u00b7seln", ",", "wenn", "ei\u00b7ner", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zusiehet denen, lieblich und b\u00f6s, ein Schmerz,", "tokens": ["Zu\u00b7sie\u00b7het", "de\u00b7nen", ",", "lieb\u00b7lich", "und", "b\u00f6s", ",", "ein", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wenn einer zweigestalt ist, und es", "tokens": ["Wenn", "ei\u00b7ner", "zwei\u00b7ge\u00b7stalt", "ist", ",", "und", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "KON", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kennet kein einziger nicht das Beste;", "tokens": ["Ken\u00b7net", "kein", "ein\u00b7zi\u00b7ger", "nicht", "das", "Bes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Das aber ist der Stachel des Gottes; nie", "tokens": ["Das", "a\u00b7ber", "ist", "der", "Sta\u00b7chel", "des", "Got\u00b7tes", ";", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann einer lieben g\u00f6ttliches Unrecht sonst.", "tokens": ["Kann", "ei\u00b7ner", "lie\u00b7ben", "g\u00f6tt\u00b7li\u00b7ches", "Un\u00b7recht", "sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Einheimisch aber ist der Gott dann", "tokens": ["Ein\u00b7hei\u00b7misch", "a\u00b7ber", "ist", "der", "Gott", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Angesichts da, und die Erd ist anders.", "tokens": ["An\u00b7ge\u00b7sichts", "da", ",", "und", "die", "Erd", "ist", "an\u00b7ders", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Tag! Tag! Nun wieder atmet ihr recht; nun trinkt,", "tokens": ["Tag", "!", "Tag", "!", "Nun", "wie\u00b7der", "at\u00b7met", "ihr", "recht", ";", "nun", "trinkt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr meiner B\u00e4che Weiden! ein Augenlicht,", "tokens": ["Ihr", "mei\u00b7ner", "B\u00e4\u00b7che", "Wei\u00b7den", "!", "ein", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und rechte Stapfen gehn, und als ein", "tokens": ["Und", "rech\u00b7te", "Stap\u00b7fen", "gehn", ",", "und", "als", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,", "KON", "KOUS", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herrscher, mit Sporen, und bei dir selber", "tokens": ["Herr\u00b7scher", ",", "mit", "Spo\u00b7ren", ",", "und", "bei", "dir", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "$,", "KON", "APPR", "PPER", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "\u00d6rtlich, Irrstern des Tages, erscheinest du,", "tokens": ["\u00d6rt\u00b7lich", ",", "Irrs\u00b7tern", "des", "Ta\u00b7ges", ",", "er\u00b7schei\u00b7nest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du auch, o Erde, friedliche Wieg, und du,", "tokens": ["Du", "auch", ",", "o", "Er\u00b7de", ",", "fried\u00b7li\u00b7che", "Wieg", ",", "und", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos"