{"textgrid.poem.31403": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Allheit / derer G\u00fct' unzahlbar mich erquicket!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Allheit / derer G\u00fct' unzahlbar mich erquicket!", "tokens": ["Ach", "All\u00b7heit", "/", "de\u00b7rer", "G\u00fct'", "un\u00b7zahl\u00b7bar", "mich", "er\u00b7quic\u00b7ket", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PDS", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O viel bestrahltes Liecht / O reiches Gnaden Pfand!", "tokens": ["O", "viel", "be\u00b7strahl\u00b7tes", "Liecht", "/", "O", "rei\u00b7ches", "Gna\u00b7den", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$(", "NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nur unerforschlicher / jemehr du bist bekandt!", "tokens": ["nur", "un\u00b7er\u00b7for\u00b7schli\u00b7cher", "/", "je\u00b7mehr", "du", "bist", "be\u00b7kandt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+--+----+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ich werd' erleucht-verblendt / von dir so hell beblicket.", "tokens": ["ich", "werd'", "er\u00b7leucht\u00b7ver\u00b7blendt", "/", "von", "dir", "so", "hell", "be\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die heilig Wunderlust im Geist mich so entr\u00fccket /", "tokens": ["Die", "hei\u00b7lig", "Wun\u00b7der\u00b7lust", "im", "Geist", "mich", "so", "ent\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df ich daf\u00fcr nicht n\u00e4hm den h\u00f6chsten Kronen-Stand.", "tokens": ["da\u00df", "ich", "da\u00b7f\u00fcr", "nicht", "n\u00e4hm", "den", "h\u00f6chs\u00b7ten", "Kro\u00b7nen\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich achte Geld und Welt / vor lauter Sand und Tand /", "tokens": ["Ich", "ach\u00b7te", "Geld", "und", "Welt", "/", "vor", "lau\u00b7ter", "Sand", "und", "Tand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$(", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und bin vor Himmels-Freud ja selbst aus mir verz\u00fccket.", "tokens": ["und", "bin", "vor", "Him\u00b7mels\u00b7Freud", "ja", "selbst", "aus", "mir", "ver\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach da\u00df mein Athem w\u00e4r ein lob-durchs\u00fcsster Wind /", "tokens": ["Ach", "da\u00df", "mein", "A\u00b7them", "w\u00e4r", "ein", "lob\u00b7durch\u00b7s\u00fcss\u00b7ter", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und Sternen-werts auff\u00fchrt die Flammen meiner Liebe!", "tokens": ["und", "Ster\u00b7nen\u00b7werts", "auf\u00b7f\u00fchrt", "die", "Flam\u00b7men", "mei\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "ach da\u00df ich mich vor Lieb wie F\u00f6nix nicht entz\u00fcnd' /", "tokens": ["ach", "da\u00df", "ich", "mich", "vor", "Lieb", "wie", "F\u00f6\u00b7nix", "nicht", "ent\u00b7z\u00fcnd'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "NN", "KOKOM", "NE", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und ganz begl\u00fcckt vergeh in so hoch edlem triebe!", "tokens": ["und", "ganz", "be\u00b7gl\u00fcckt", "ver\u00b7geh", "in", "so", "hoch", "ed\u00b7lem", "trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "APPR", "ADV", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "la\u00df mich durch Dankbarkeit / Gott / deinen Spiegel seyn /", "tokens": ["la\u00df", "mich", "durch", "Dank\u00b7bar\u00b7keit", "/", "Gott", "/", "dei\u00b7nen", "Spie\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$(", "NN", "$(", "PPOSAT", "NN", "VAINF", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "da\u00df widerschein dein Strahl ein Gnaden-lobes-Schein!", "tokens": ["da\u00df", "wi\u00b7der\u00b7schein", "dein", "Strahl", "ein", "Gna\u00b7den\u00b7lo\u00b7bes\u00b7Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}