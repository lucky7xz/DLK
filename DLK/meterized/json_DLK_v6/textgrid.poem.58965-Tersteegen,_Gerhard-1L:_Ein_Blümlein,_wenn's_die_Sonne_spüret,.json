{"textgrid.poem.58965": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bl\u00fcmlein, wenn's die Sonne sp\u00fcret,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bl\u00fcmlein, wenn's die Sonne sp\u00fcret,", "tokens": ["Ein", "Bl\u00fcm\u00b7lein", ",", "wenn's", "die", "Son\u00b7ne", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00f6ffnet stille, sanft und froh;", "tokens": ["Sich", "\u00f6ff\u00b7net", "stil\u00b7le", ",", "sanft", "und", "froh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann Gottes Gnade dich ber\u00fchret,", "tokens": ["Wann", "Got\u00b7tes", "Gna\u00b7de", "dich", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df auch dein Herz bewirken so!", "tokens": ["La\u00df", "auch", "dein", "Herz", "be\u00b7wir\u00b7ken", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}