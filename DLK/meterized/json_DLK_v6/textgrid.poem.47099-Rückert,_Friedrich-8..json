{"textgrid.poem.47099": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Liebe hat mich beschlichen,", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "hat", "mich", "be\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie der Fr\u00fchling die Erde,", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wann der Winter nun ist entwichen,", "tokens": ["Wann", "der", "Win\u00b7ter", "nun", "ist", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kaum merkt sie, da\u00df warm es werde.", "tokens": ["Kaum", "merkt", "sie", ",", "da\u00df", "warm", "es", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber der Sonne heimliche Kraft", "tokens": ["A\u00b7ber", "der", "Son\u00b7ne", "heim\u00b7li\u00b7che", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat schon das Herz ihr ger\u00fchret,", "tokens": ["Hat", "schon", "das", "Herz", "ihr", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Wurzel regt sich der Saft,", "tokens": ["In", "der", "Wur\u00b7zel", "regt", "sich", "der", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Noch ehe der Zweig es sp\u00fcret.", "tokens": ["Noch", "e\u00b7he", "der", "Zweig", "es", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Schnee zerschmilzt, die Wolken zergehn,", "tokens": ["Der", "Schnee", "zer\u00b7schmilzt", ",", "die", "Wol\u00b7ken", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die erste Bl\u00fct' ist entglommen,", "tokens": ["Die", "ers\u00b7te", "Bl\u00fct'", "ist", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann sieht sie in voller Glut sich stehn", "tokens": ["Dann", "sieht", "sie", "in", "vol\u00b7ler", "Glut", "sich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wei\u00df nicht, wie es gekommen.", "tokens": ["Und", "wei\u00df", "nicht", ",", "wie", "es", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}