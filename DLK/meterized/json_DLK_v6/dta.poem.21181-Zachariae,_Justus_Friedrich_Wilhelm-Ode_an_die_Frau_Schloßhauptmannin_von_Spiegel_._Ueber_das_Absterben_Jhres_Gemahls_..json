{"dta.poem.21181": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ode  \n an die Frau Schlo\u00dfhauptmannin  \n  von Spiegel .  \n Ueber das Absterben  \n  Jhres Gemahls .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch seh ich Dich gen Himmel schauen,", "tokens": ["Noch", "seh", "ich", "Dich", "gen", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit thr\u00e4nendem von Angst gebrochnem Blick!", "tokens": ["Mit", "thr\u00e4\u00b7nen\u00b7dem", "von", "Angst", "ge\u00b7broch\u00b7nem", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Du gebeugteste der Frauen,", "tokens": ["O", "Du", "ge\u00b7beug\u00b7tes\u00b7te", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ist nunmehr Dein ganzes irdsches Gl\u00fcck?", "tokens": ["Wo", "ist", "nun\u00b7mehr", "Dein", "gan\u00b7zes", "ird\u00b7sches", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist dahin! \u2014 Als wenn im Wetter", "tokens": ["Es", "ist", "da\u00b7hin", "!", "Als", "wenn", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "$.", "$(", "KOUS", "KOUS", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein schneller Stral vom schwarzen Himmel f\u00e4hrt,", "tokens": ["Ein", "schnel\u00b7ler", "Stral", "vom", "schwar\u00b7zen", "Him\u00b7mel", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Baum entflammt, und Stamm und Bl\u00e4tter", "tokens": ["Den", "Baum", "ent\u00b7flammt", ",", "und", "Stamm", "und", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit wilder Glut im Augenblick verzehrt.", "tokens": ["Mit", "wil\u00b7der", "Glut", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So liegt Dein Spiegel! La\u00df den Klagen", "tokens": ["So", "liegt", "Dein", "Spie\u00b7gel", "!", "La\u00df", "den", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den freyen Larf; zu sehr verdient er sie!", "tokens": ["Den", "frey\u00b7en", "Larf", ";", "zu", "sehr", "ver\u00b7dient", "er", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du siehest ihn zur Gruft getragen", "tokens": ["Du", "sie\u00b7hest", "ihn", "zur", "Gruft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hart geraubt, zu unverhoft, zu fr\u00fch!", "tokens": ["Zu", "hart", "ge\u00b7raubt", ",", "zu", "un\u00b7ver\u00b7hoft", ",", "zu", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$,", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht deiner Z\u00e4hren Strom zu wehren,", "tokens": ["Nicht", "dei\u00b7ner", "Z\u00e4h\u00b7ren", "Strom", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Naht sich zu dir die Muse, selbst gebeugt;", "tokens": ["Naht", "sich", "zu", "dir", "die", "Mu\u00b7se", ",", "selbst", "ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00fcrde weniger Dich ehren,", "tokens": ["Ich", "w\u00fcr\u00b7de", "we\u00b7ni\u00b7ger", "Dich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn weniger Dein Herz sich uns gezeigt.", "tokens": ["Wenn", "we\u00b7ni\u00b7ger", "Dein", "Herz", "sich", "uns", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich selbst, der ich nicht das verlohren,", "tokens": ["Ich", "selbst", ",", "der", "ich", "nicht", "das", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was Du verlierst, ich steh noch stumm und kalt;", "tokens": ["Was", "Du", "ver\u00b7lierst", ",", "ich", "steh", "noch", "stumm", "und", "kalt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir klingt in den erschrocknen Ohren", "tokens": ["Mir", "klingt", "in", "den", "er\u00b7schrock\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein R\u00f6cheln noch; noch seh ich die Gestalt", "tokens": ["Sein", "R\u00f6\u00b7cheln", "noch", ";", "noch", "seh", "ich", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Des Sterbenden. Mu\u00dft ich es sehen,", "tokens": ["Des", "Ster\u00b7ben\u00b7den", ".", "Mu\u00dft", "ich", "es", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "O Theurester, wie dir das Auge brach!", "tokens": ["O", "Theu\u00b7res\u00b7ter", ",", "wie", "dir", "das", "Au\u00b7ge", "brach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sahs; mir blieb der Athem stehen,", "tokens": ["Ich", "sahs", ";", "mir", "blieb", "der", "A\u00b7them", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sprach Gebet, kaum wissend, da\u00df ichs sprach.", "tokens": ["Ich", "sprach", "Ge\u00b7bet", ",", "kaum", "wis\u00b7send", ",", "da\u00df", "ichs", "sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVPP", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So war die edle Seel entwichen!", "tokens": ["So", "war", "die", "ed\u00b7le", "Seel", "ent\u00b7wi\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er lag vor uns, den wir so sehr geliebt,", "tokens": ["Er", "lag", "vor", "uns", ",", "den", "wir", "so", "sehr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein kalter Leichnam, starr, verblichen,", "tokens": ["Ein", "kal\u00b7ter", "Leich\u00b7nam", ",", "starr", ",", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir all um ihn lautweinend und betr\u00fcbt.", "tokens": ["Wir", "all", "um", "ihn", "laut\u00b7wei\u00b7nend", "und", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "APPR", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Tritt her zu seiner fr\u00fchen Bahre", "tokens": ["Tritt", "her", "zu", "sei\u00b7ner", "fr\u00fc\u00b7hen", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leichtsinniger! tritt her, sieh schreckensvoll,", "tokens": ["Leicht\u00b7sin\u00b7ni\u00b7ger", "!", "tritt", "her", ",", "sieh", "schre\u00b7ckens\u00b7voll", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Da\u00df Jugend, so wie graue Haare,", "tokens": ["Da\u00df", "Ju\u00b7gend", ",", "so", "wie", "grau\u00b7e", "Haa\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Todes Schwerdt, gleich grausam, treffen soll.", "tokens": ["Des", "To\u00b7des", "Schwerdt", ",", "gleich", "grau\u00b7sam", ",", "tref\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ADJD", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du fliehst? \u2014 Mit furchtbar weiten Schritten", "tokens": ["Du", "fliehst", "?", "Mit", "furcht\u00b7bar", "wei\u00b7ten", "Schrit\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "ADJD", "ADJA", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hohlt er dich ein; wie eitel ist dein Fliehn!", "tokens": ["Hohlt", "er", "dich", "ein", ";", "wie", "ei\u00b7tel", "ist", "dein", "Fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$.", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Nicht Klagen, Thr\u00e4nen, oder Bitten,", "tokens": ["Nicht", "Kla\u00b7gen", ",", "Thr\u00e4\u00b7nen", ",", "o\u00b7der", "Bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Stand, nicht Pracht, nicht Gold, entfernen ihn.", "tokens": ["Nicht", "Stand", ",", "nicht", "Pracht", ",", "nicht", "Gold", ",", "ent\u00b7fer\u00b7nen", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wenn jemals Thr\u00e4nen ihn ger\u00fchret,", "tokens": ["Wenn", "je\u00b7mals", "Thr\u00e4\u00b7nen", "ihn", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00e4tten ihn die Deinigen ger\u00fchrt,", "tokens": ["So", "h\u00e4t\u00b7ten", "ihn", "die", "Dei\u00b7ni\u00b7gen", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebeugte Frau! Doch er vollf\u00fchret", "tokens": ["Ge\u00b7beug\u00b7te", "Frau", "!", "Doch", "er", "voll\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den schweren Schlag, und ach! er ist vollf\u00fchrt!", "tokens": ["Den", "schwe\u00b7ren", "Schlag", ",", "und", "ach", "!", "er", "ist", "voll\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "XY", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Du, der du seine Pfeile lenkest,", "tokens": ["Du", ",", "der", "du", "sei\u00b7ne", "Pfei\u00b7le", "len\u00b7kest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "O Ewiger! der du auch solchem Schmerz,", "tokens": ["O", "E\u00b7wi\u00b7ger", "!", "der", "du", "auch", "sol\u00b7chem", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$.", "ART", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch solchem Jammer, Kr\u00e4fte schenkest,", "tokens": ["Auch", "sol\u00b7chem", "Jam\u00b7mer", ",", "Kr\u00e4f\u00b7te", "schen\u00b7kest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O schau herab auf Jhr zerri\u00dfnes Herz!", "tokens": ["O", "schau", "her\u00b7ab", "auf", "Ihr", "zer\u00b7ri\u00df\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Zerrissen blutet es \u2014 zerrissen", "tokens": ["Zer\u00b7ris\u00b7sen", "blu\u00b7tet", "es", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von deiner Hand; denn ists nicht deine Hand,", "tokens": ["Von", "dei\u00b7ner", "Hand", ";", "denn", "ists", "nicht", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Jhr das gr\u00f6\u00dfte Gl\u00fcck entrissen,", "tokens": ["Die", "Ihr", "das", "gr\u00f6\u00df\u00b7te", "Gl\u00fcck", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das reinste Gl\u00fcck, das Sterbliche gekannt?", "tokens": ["Das", "reins\u00b7te", "Gl\u00fcck", ",", "das", "Sterb\u00b7li\u00b7che", "ge\u00b7kannt", "?"], "token_info": 