", "tokens": ["Und", "dei\u00b7nen", "ho\u00b7hen", "Wil\u00b7len", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wohlthat auch im Jammer Jhr gesandt.", "tokens": ["Der", "Wohlt\u00b7hat", "auch", "im", "Jam\u00b7mer", "Ihr", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}