{"textgrid.poem.52350": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "68. Radschlagen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbas Anno tw\u00f6lf ick ok m\u00fc\u00dft mit\u00ab,", "tokens": ["\u00bb", "as", "An\u00b7no", "tw\u00f6lf", "ick", "ok", "m\u00fc\u00dft", "mit", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vetellte Leutnant Herr von Schlemm, \u2013", "tokens": ["Ve\u00b7tell\u00b7te", "Leut\u00b7nant", "Herr", "von", "Schlemm", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "APPR", "NE", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un't rookte gruuglich achte em, \u2013", "tokens": ["Un't", "rook\u00b7te", "gruug\u00b7lich", "ach\u00b7te", "em", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADJA", "ADJA", "$,", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00bbdunn hahr ick en Bediente, Namens Schmidt.", "tokens": ["\u00bb", "dunn", "hahr", "ick", "en", "Be\u00b7dien\u00b7te", ",", "Na\u00b7mens", "Schmidt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "NE", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dei Kierl wier niedertr\u00e4chtig stark;", "tokens": ["Dei", "Ki\u00b7erl", "wier", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "stark", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "S\u00f6\u00df Sch\u00e4pel Arwten groote Maat,", "tokens": ["S\u00f6\u00df", "Sch\u00e4\u00b7pel", "Arw\u00b7ten", "groo\u00b7te", "Maat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dat wier v\u00f6r em en bloten Quark!", "tokens": ["Dat", "wier", "v\u00f6r", "em", "en", "blo\u00b7ten", "Quark", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "In't ganze Batalljohn wier kein Soldat,", "tokens": ["In't", "gan\u00b7ze", "Ba\u00b7tall\u00b7john", "wier", "kein", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dei em von sienen Platz hahr r\u00f6gt", "tokens": ["Dei", "em", "von", "sie\u00b7nen", "Platz", "hahr", "r\u00f6gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un em den stiewen Arm hahr b\u00f6gt. \u2013", "tokens": ["Un", "em", "den", "stie\u00b7wen", "Arm", "hahr", "b\u00f6gt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Un das is wahr! Mich soll der Teufel hohlen! \u2013", "tokens": ["Un", "das", "is", "wahr", "!", "Mich", "soll", "der", "Teu\u00b7fel", "hoh\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "FM", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie wieren doamals grar in Polen,", "tokens": ["Wie", "wie\u00b7ren", "doa\u00b7mals", "grar", "in", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Un ick m\u00fc\u00dft up dei schlichtsten Weeg'", "tokens": ["Un", "ick", "m\u00fc\u00dft", "up", "dei", "schlichts\u00b7ten", "Wee\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Von Stadt tau Stadt her\u00fcmme f\u00fchren", "tokens": ["Von", "Stadt", "tau", "Stadt", "he\u00b7r\u00fcm\u00b7me", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "CARD", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Un m\u00fc\u00dft mit gaure W\u00fchr un Schl\u00e4g'", "tokens": ["Un", "m\u00fc\u00dft", "mit", "gau\u00b7re", "W\u00fchr", "un", "Schl\u00e4g'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "ADJA", "NN", "FM", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "'ne Partie Ossen requiriren.", "tokens": ["'ne", "Par\u00b7tie", "Os\u00b7sen", "re\u00b7qui\u00b7ri\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Bi dei Gelegenheit, dunn kehmen wi denn mal", "tokens": ["Bi", "dei", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "dunn", "keh\u00b7men", "wi", "denn", "mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "KOKOM", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In einen Weg, wat einen Mordweg was:", "tokens": ["In", "ei\u00b7nen", "Weg", ",", "wat", "ei\u00b7nen", "Mord\u00b7weg", "was", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "ART", "NN", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dei Wagen g\u00fcng bald up, bald dal", "tokens": ["Dei", "Wa\u00b7gen", "g\u00fcng", "bald", "up", ",", "bald", "dal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "D\u00f6rch deipen Dreck. Knacks! br\u00f6k dei As.", "tokens": ["D\u00f6rch", "dei\u00b7pen", "Dreck", ".", "Knacks", "!", "br\u00f6k", "dei", "As", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$.", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doa seeten wi! Wo s\u00fcll dit waren?", "tokens": ["Doa", "see\u00b7ten", "wi", "!", "Wo", "s\u00fcll", "dit", "wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "PWAV", "NE", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Dat neegste D\u00f6rp wier noch 'ne Miel wiet af.", "tokens": ["Dat", "neegs\u00b7te", "D\u00f6rp", "wier", "noch", "'ne", "Miel", "wiet", "af", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "ADV", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Doch ihre wi uns besannen hahren,", "tokens": ["Doch", "ih\u00b7re", "wi", "uns", "be\u00b7san\u00b7nen", "hah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PWAV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Dunn wier mien Schmidt all von den Wagen 'raf", "tokens": ["Dunn", "wier", "mien", "Schmidt", "all", "von", "den", "Wa\u00b7gen", "'raf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE", "PIAT", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Un kreeg den St\u00fcmmel von dei As tau faaten", "tokens": ["Un", "kreeg", "den", "St\u00fcm\u00b7mel", "von", "dei", "As", "tau", "faa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NE", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Un wull mi nich utstiegen laaten. \u2013", "tokens": ["Un", "wull", "mi", "nich", "ut\u00b7stie\u00b7gen", "laa\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "NE", "PTKNEG", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "\u203aherr Leutnant\u2039, s\u00e4hr 'e, \u203asitten S\u2039 ruhig wi\u00df,", "tokens": ["\u203a", "herr", "Leut\u00b7nant", "\u2039", ",", "s\u00e4hr", "'e", ",", "\u203a", "sit\u00b7ten", "S", "\u2039", "ru\u00b7hig", "wi\u00df", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$(", "$,", "ADJD", "VVFIN", "$,", "XY", "XY", "XY", "$(", "ADJD", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.28": {"text": "Ick faat dei As en En'n lang an,", "tokens": ["Ick", "faat", "dei", "As", "en", "En'n", "lang", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Denn, denk ick, waren wi, wenn't jichtens m\u00e4glich is,", "tokens": ["Denn", ",", "denk", "ick", ",", "wa\u00b7ren", "wi", ",", "wenn't", "jich\u00b7tens", "m\u00e4g\u00b7lich", "is", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "VAFIN", "KOKOM", "$,", "VVFIN", "ADV", "ADJD", "FM", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch endlich kamen an en D\u00f6rp heran.\u2039", "tokens": ["Doch", "end\u00b7lich", "ka\u00b7men", "an", "en", "D\u00f6rp", "he\u00b7ran", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NE", "NE", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Dei Kierl, dei kreeg nu mittewiel", "tokens": ["Dei", "Ki\u00b7erl", ",", "dei", "kreeg", "nu", "mit\u00b7te\u00b7wiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Dei As tau faaten un hei dr\u00f6g sei furt", "tokens": ["Dei", "As", "tau", "faa\u00b7ten", "un", "hei", "dr\u00f6g", "sei", "furt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Bet an den neegsten, l\u00fctten Urt;", "tokens": ["Bet", "an", "den", "neegs\u00b7ten", ",", "l\u00fct\u00b7ten", "Urt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "\u203ane ganze Meile trug sie der Carnaille.", "tokens": ["\u203a", "ne", "gan\u00b7ze", "Mei\u00b7le", "trug", "sie", "der", "Car\u00b7nail\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Die Sach' ist wahr auf gage und auf taille!\u00ab \u2013", "tokens": ["Die", "Sach'", "ist", "wahr", "auf", "ga\u00b7ge", "und", "auf", "tail\u00b7le", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "\u00bbis dat ok wat? Dat kann en Jere\u00ab,", "tokens": ["\u00bb", "is", "dat", "ok", "wat", "?", "Dat", "kann", "en", "Je\u00b7re", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "$.", "NN", "VMFIN", "FM", "FM", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "S\u00e4hr ick, \u00bbick hahr mal Einen, dei heit Schr\u00f6re\u00ab, \u2013", "tokens": ["S\u00e4hr", "ick", ",", "\u00bb", "ick", "hahr", "mal", "Ei\u00b7nen", ",", "dei", "heit", "Schr\u00f6\u00b7re", "\u00ab", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PPER", "VVFIN", "ADV", "ART", "$,", "FM.la", "FM.la", "FM.la", "$(", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Un nu f\u00fcngt ok bi mi tau rocken an, \u2013", "tokens": ["Un", "nu", "f\u00fcngt", "ok", "bi", "mi", "tau", "ro\u00b7cken", "an", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "VVFIN", "FM", "FM", "FM", "FM", "VVFIN", "PTKVZ", "$,", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "\u00bbdat was en Kierl, grar as en Hampelmann,", "tokens": ["\u00bb", "dat", "was", "en", "Ki\u00b7erl", ",", "grar", "as", "en", "Ham\u00b7pel\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "NE", "$,", "FM", "FM", "FM", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.40": {"text": "So'n Kierl is nahrens nich tau finnen,", "tokens": ["So'n", "Ki\u00b7erl", "is", "nah\u00b7rens", "nich", "tau", "fin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Grar as 'ne W\u00e4hr k\u00fcnn hei sich winnen,", "tokens": ["Grar", "as", "'ne", "W\u00e4hr", "k\u00fcnn", "hei", "sich", "win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Un up den Kopp, doa stellt hei sich,", "tokens": ["Un", "up", "den", "Kopp", ",", "doa", "stellt", "hei", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "FM", "FM", "FM", "PRF", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.43": {"text": "Un radschlahn k\u00fcnn hei meisterlich. \u2013", "tokens": ["Un", "rad\u00b7schlahn", "k\u00fcnn", "hei", "meis\u00b7ter\u00b7lich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.44": {"text": "Wi reis'ten ok einmal von Stadt tau Stadt,", "tokens": ["Wi", "reis'\u00b7ten", "ok", "ein\u00b7mal", "von", "Stadt", "tau", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "APPR", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Un unnewegs br\u00f6k uns denn ok dat Rad.", "tokens": ["Un", "un\u00b7ne\u00b7wegs", "br\u00f6k", "uns", "denn", "ok", "dat", "Rad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Na, ick steeg af, bekeek mi nu dei As,", "tokens": ["Na", ",", "ick", "steeg", "af", ",", "be\u00b7keek", "mi", "nu", "dei", "As", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Un as ick seeg, dat dei nich braken was,", "tokens": ["Un", "as", "ick", "seeg", ",", "dat", "dei", "nich", "bra\u00b7ken", "was", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "PIS", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.48": {"text": "So r\u00f6p ick: \u203aSchroere! Kumm mal hea", "tokens": ["So", "r\u00f6p", "ick", ":", "\u203a", "Schroe\u00b7re", "!", "Kumm", "mal", "hea"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "NE", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Un stell Di dichting hia mal v\u00f6a;", "tokens": ["Un", "stell", "Di", "dich\u00b7ting", "hia", "mal", "v\u00f6a", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Hier v\u00f6r dei As! \u2013 Nu dreih Di 'r\u00fcmme \u2013", "tokens": ["Hier", "v\u00f6r", "dei", "As", "!", "\u2013", "Nu", "dreih", "Di", "'r\u00fcm\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "$(", "ADV", "CARD", "NE", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Nu maak Di noch en B\u00e4ten kr\u00fcmme \u2013", "tokens": ["Nu", "maak", "Di", "noch", "en", "B\u00e4\u00b7ten", "kr\u00fcm\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Nu krup herute ut dei Hos'!", "tokens": ["Nu", "krup", "he\u00b7ru\u00b7te", "ut", "dei", "