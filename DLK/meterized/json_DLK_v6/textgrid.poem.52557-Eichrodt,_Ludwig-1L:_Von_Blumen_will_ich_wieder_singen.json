{"textgrid.poem.52557": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Blumen will ich wieder singen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Blumen will ich wieder singen", "tokens": ["Von", "Blu\u00b7men", "will", "ich", "wie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Sonnenschein, vom Mai,", "tokens": ["Vom", "Son\u00b7nen\u00b7schein", ",", "vom", "Mai", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Wanderlust und Becherklingen!", "tokens": ["Von", "Wan\u00b7der\u00b7lust", "und", "Be\u00b7cher\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entweiche, Gr\u00fcbelei!", "tokens": ["Ent\u00b7wei\u00b7che", ",", "Gr\u00fc\u00b7be\u00b7lei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sang und sprach vom Menschenthume", "tokens": ["Ich", "sang", "und", "sprach", "vom", "Men\u00b7schen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gegen Tyrannei,", "tokens": ["Und", "ge\u00b7gen", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unbek\u00fcmmert um die Blume", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "die", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchjammert ich den Mai.", "tokens": ["Durch\u00b7jam\u00b7mert", "ich", "den", "Mai", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sang mit eifernder Geberde \u2013", "tokens": ["Ich", "sang", "mit", "ei\u00b7fern\u00b7der", "Ge\u00b7ber\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Mein Fr\u00fchling, o verzeih!", "tokens": ["Mein", "Fr\u00fch\u00b7ling", ",", "o", "ver\u00b7zeih", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An aller Lust der gr\u00fcnen Erde", "tokens": ["An", "al\u00b7ler", "Lust", "der", "gr\u00fc\u00b7nen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ging ich im Zorn vorbei.", "tokens": ["Ging", "ich", "im", "Zorn", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem Vaterland wollt ich entrinnen,", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "wollt", "ich", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil es nicht stark und frei \u2013", "tokens": ["Weil", "es", "nicht", "stark", "und", "frei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schmeichelt meinen spr\u00f6den Sinnen", "tokens": ["Da", "schmei\u00b7chelt", "mei\u00b7nen", "spr\u00f6\u00b7den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der heimathliche Mai.", "tokens": ["Der", "hei\u00b7math\u00b7li\u00b7che", "Mai", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Der Fr\u00fchling ist mir nachgesprungen", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "mir", "nach\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit hellem Jubelschrei,", "tokens": ["Mit", "hel\u00b7lem", "Ju\u00b7bel\u00b7schrei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit beredten Bl\u00fcthenzungen", "tokens": ["Und", "mit", "be\u00b7red\u00b7ten", "Bl\u00fc\u00b7then\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekehrte mich der Mai.", "tokens": ["Be\u00b7kehr\u00b7te", "mich", "der", "Mai", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}