{"textgrid.poem.60417": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach, wer m\u00f6chte einsam trinken,", "tokens": ["\u00bb", "ach", ",", "wer", "m\u00f6ch\u00b7te", "ein\u00b7sam", "trin\u00b7ken", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Rede, Rundgesang,", "tokens": ["Oh\u00b7ne", "Re\u00b7de", ",", "Rund\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne an die Brust zu sinken", "tokens": ["Oh\u00b7ne", "an", "die", "Brust", "zu", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem Freund im Wonnedrang?\u00ab", "tokens": ["Ei\u00b7nem", "Freund", "im", "Won\u00b7ne\u00b7drang", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich; \u2013 die Freunde sind zu selten;", "tokens": ["Ich", ";", "\u2013", "die", "Freun\u00b7de", "sind", "zu", "sel\u00b7ten", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "$(", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Denken trinkt das Tier,", "tokens": ["Oh\u00b7ne", "Den\u00b7ken", "trinkt", "das", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich lad aus andern Welten", "tokens": ["Und", "ich", "lad", "aus", "an\u00b7dern", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber meine G\u00e4ste mir.", "tokens": ["Lie\u00b7ber", "mei\u00b7ne", "G\u00e4s\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn im Wein Gedanken quellen,", "tokens": ["Wenn", "im", "Wein", "Ge\u00b7dan\u00b7ken", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fchlt ihr mir den Schlamm empor,", "tokens": ["W\u00fchlt", "ihr", "mir", "den", "Schlamm", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie des Ganges heilge Wellen", "tokens": ["Wie", "des", "Gan\u00b7ges", "heil\u00b7ge", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcbt ein Elefantenchor.", "tokens": ["Tr\u00fcbt", "ein", "E\u00b7lef\u00b7an\u00b7ten\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dionys in Vaterarme", "tokens": ["Di\u00b7o\u00b7nys", "in", "Va\u00b7te\u00b7rar\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mild den einzlen Mann empfing,", "tokens": ["Mild", "den", "einz\u00b7len", "Mann", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, gekr\u00e4nket von dem Schwarme,", "tokens": ["Der", ",", "ge\u00b7kr\u00e4n\u00b7ket", "von", "dem", "Schwar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach Eleusis opfern ging.", "tokens": ["Nach", "E\u00b7leu\u00b7sis", "op\u00b7fern", "ging", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}