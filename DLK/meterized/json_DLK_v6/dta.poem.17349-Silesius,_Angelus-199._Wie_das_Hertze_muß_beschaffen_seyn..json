{"dta.poem.17349": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "199.  Wie das Hertze mu\u00df beschaffen  \n seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Christ wo der Ewge GOtt dein Hertz sol nehmen ein/", "tokens": ["Christ", "wo", "der", "Ew\u00b7ge", "Gott", "dein", "Hertz", "sol", "neh\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "ART", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So mu\u00df kein bildn\u00fc\u00df drinn/ al\u00df seines Sohnes seyn.", "tokens": ["So", "mu\u00df", "kein", "bild\u00b7n\u00fc\u00df", "drinn", "/", "al\u00df", "sei\u00b7nes", "Soh\u00b7nes", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "ADV", "$(", "KOUS", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}