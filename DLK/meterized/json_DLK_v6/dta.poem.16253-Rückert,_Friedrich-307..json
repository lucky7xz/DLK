{"dta.poem.16253": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "307.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das G\u00e4hnen, lieber Sohn, es ist zwar unwillk\u00fcrlich,", "tokens": ["Das", "G\u00e4h\u00b7nen", ",", "lie\u00b7ber", "Sohn", ",", "es", "ist", "zwar", "un\u00b7will\u00b7k\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch abgew\u00f6hnen mu\u00dft du dir's als ungeb\u00fcrlich.", "tokens": ["Doch", "ab\u00b7ge\u00b7w\u00f6h\u00b7nen", "mu\u00dft", "du", "dir's", "als", "un\u00b7ge\u00b7b\u00fcr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PIS", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe nie gesehn, da\u00df, wenn du auf den Z\u00e4hnen", "tokens": ["Ich", "ha\u00b7be", "nie", "ge\u00b7sehn", ",", "da\u00df", ",", "wenn", "du", "auf", "den", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Gutes hast zu ", "tokens": ["Was", "Gu\u00b7tes", "hast", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Auch w\u00fcrde dir dadurch des Kauens Kraft entrissen,", "tokens": ["Auch", "w\u00fcr\u00b7de", "dir", "da\u00b7durch", "des", "Kau\u00b7ens", "Kraft", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fallen m\u00f6chte dir aus offnem Mund der Bissen.", "tokens": ["Und", "fal\u00b7len", "m\u00f6ch\u00b7te", "dir", "aus", "off\u00b7nem", "Mund", "der", "Bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Beim Lernen aber ist das G\u00e4hnen gleich erweckt;", "tokens": ["Beim", "Ler\u00b7nen", "a\u00b7ber", "ist", "das", "G\u00e4h\u00b7nen", "gleich", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sehe, da\u00df es dir nicht wie das Essen schmeckt.", "tokens": ["Ich", "se\u00b7he", ",", "da\u00df", "es", "dir", "nicht", "wie", "das", "Es\u00b7sen", "schmeckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn g\u00e4hnend sich der Mund aufthut, schlie\u00dft sich das Ohr", "tokens": ["Wenn", "g\u00e4h\u00b7nend", "sich", "der", "Mund", "auf\u00b7thut", ",", "schlie\u00dft", "sich", "das", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So da\u00df es ungeh\u00f6rt des Lehrers Wort verlor.", "tokens": ["So", "da\u00df", "es", "un\u00b7ge\u00b7h\u00f6rt", "des", "Leh\u00b7rers", "Wort", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn g\u00e4hnend sich der Mund aufthut, gehn zu die Augen,", "tokens": ["Wenn", "g\u00e4h\u00b7nend", "sich", "der", "Mund", "auf\u00b7thut", ",", "gehn", "zu", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie des Buches Schrift nicht aufzufassen taugen.", "tokens": ["Da\u00df", "sie", "des", "Bu\u00b7ches", "Schrift", "nicht", "auf\u00b7zu\u00b7fas\u00b7sen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Des Lernens S\u00fc\u00dfigkeit hast du noch nicht empfunden,", "tokens": ["Des", "Ler\u00b7nens", "S\u00fc\u00b7\u00dfig\u00b7keit", "hast", "du", "noch", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst w\u00e4re dir die Lust zu g\u00e4hnen ganz verschwunden.", "tokens": ["Sonst", "w\u00e4\u00b7re", "dir", "die", "Lust", "zu", "g\u00e4h\u00b7nen", "ganz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKZU", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das Wissen, wi\u00df o Sohn, ist auch ein guter Bissen,", "tokens": ["Das", "Wis\u00b7sen", ",", "wi\u00df", "o", "Sohn", ",", "ist", "auch", "ein", "gu\u00b7ter", "Bis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "FM", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Seelengaumen wird durchs G\u00e4hnen er entrissen.", "tokens": ["Dem", "See\u00b7len\u00b7gau\u00b7men", "wird", "durchs", "G\u00e4h\u00b7nen", "er", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Drum wenn beim Lernen dir ein G\u00e4hnen kommt, so hemm' es,", "tokens": ["Drum", "wenn", "beim", "Ler\u00b7nen", "dir", "ein", "G\u00e4h\u00b7nen", "kommt", ",", "so", "hemm'", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entschlossen mit dem Schlo\u00df der Z\u00e4hne niederklemm' es!", "tokens": ["Ent\u00b7schlos\u00b7sen", "mit", "dem", "Schlo\u00df", "der", "Z\u00e4h\u00b7ne", "nie\u00b7der\u00b7klemm'", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "So hat es dir vorerst den Bissen nicht genommen,", "tokens": ["So", "hat", "es", "dir", "vor\u00b7erst", "den", "Bis\u00b7sen", "nicht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich wird ihm selbst die Lust vergehn zu kommen.", "tokens": ["Und", "end\u00b7lich", "wird", "ihm", "selbst", "die", "Lust", "ver\u00b7gehn", "zu", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}