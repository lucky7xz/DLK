{"textgrid.poem.47077": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum in der Ecke stehn,", "tokens": ["Wa\u00b7rum", "in", "der", "E\u00b7cke", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um mir einen Ku\u00df zu g\u00f6nnen?", "tokens": ["Um", "mir", "ei\u00b7nen", "Ku\u00df", "zu", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df es doch die Leute sehn,", "tokens": ["La\u00df", "es", "doch", "die", "Leu\u00b7te", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sie mir nicht wehren k\u00f6nnen!", "tokens": ["Was", "sie", "mir", "nicht", "weh\u00b7ren", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df den hei\u00dfen Blick der Sonnen", "tokens": ["La\u00df", "den", "hei\u00b7\u00dfen", "Blick", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sehn darein mit Neid und Groll;", "tokens": ["Sehn", "da\u00b7rein", "mit", "Neid", "und", "Groll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Lieb' ist solch ein Bronnen,", "tokens": ["Uns\u00b7re", "Lieb'", "ist", "solch", "ein", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der nicht dran versiegen soll.", "tokens": ["Der", "nicht", "dran", "ver\u00b7sie\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PAV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}