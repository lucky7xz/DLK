{"textgrid.poem.28130": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Biebel, Gottes Wort, ist mein Belieb im Leben;", "tokens": ["Die", "Bie\u00b7bel", ",", "Got\u00b7tes", "Wort", ",", "ist", "mein", "Be\u00b7lieb", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie kan mir Trost in Angst, und Rath in N\u00f6then geben.", "tokens": ["Sie", "kan", "mir", "Trost", "in", "Angst", ",", "und", "Rath", "in", "N\u00f6\u00b7then", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Biebel, Gottes Wort, ist mein Belieb im sterben;", "tokens": ["Die", "Bie\u00b7bel", ",", "Got\u00b7tes", "Wort", ",", "ist", "mein", "Be\u00b7lieb", "im", "ster\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo sie mich weiset hin, da kan ich nicht verterben.", "tokens": ["Wo", "sie", "mich", "wei\u00b7set", "hin", ",", "da", "kan", "ich", "nicht", "ver\u00b7ter\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}