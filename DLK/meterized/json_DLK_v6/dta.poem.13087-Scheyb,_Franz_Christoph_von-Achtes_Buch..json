{"dta.poem.13087": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edurch solche Bildnissen vernimmt die sp\u00e4te Welt", "tokens": ["\u201e", "durch", "sol\u00b7che", "Bild\u00b7nis\u00b7sen", "ver\u00b7nimmt", "die", "sp\u00e4\u00b7te", "Welt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso viel, als wann man ihr des Heldens Ruhm erz\u00e4hlt.", "tokens": ["\u201e", "so", "viel", ",", "als", "wann", "man", "ihr", "des", "Hel\u00b7dens", "Ruhm", "er\u00b7z\u00e4hlt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "KOUS", "PWAV", "PIS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eso wurde dessen Nahm in tausend Jahren klingen,", "tokens": ["\u201e", "so", "wur\u00b7de", "des\u00b7sen", "Nahm", "in", "tau\u00b7send", "Jah\u00b7ren", "klin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PDS", "NN", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso k\u00f6nnte, wie du sagst, ihn dieser Stein besingen;", "tokens": ["\u201e", "so", "k\u00f6nn\u00b7te", ",", "wie", "du", "sagst", ",", "ihn", "die\u00b7ser", "Stein", "be\u00b7sin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "205\u201dNicht anders: dann was zeigt ein Bogen, ein Colo\u00df?", "tokens": ["\"", "Nicht", "an\u00b7ders", ":", "dann", "was", "zeigt", "ein", "Bo\u00b7gen", ",", "ein", "Co\u00b7lo\u00df", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "$.", "ADV", "PWS", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eist er von mir entbl\u00f6\u00dft, so sieht man einen Klo\u00df.", "tokens": ["\u201e", "ist", "er", "von", "mir", "ent\u00b7bl\u00f6\u00dft", ",", "so", "sieht", "man", "ei\u00b7nen", "Klo\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}