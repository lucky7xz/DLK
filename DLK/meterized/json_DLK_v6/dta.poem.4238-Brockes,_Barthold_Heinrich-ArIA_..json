{"dta.poem.4238": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ArIA .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit erstaunendem Ergetzen,", "tokens": ["Mit", "er\u00b7stau\u00b7nen\u00b7dem", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fast mit fr\u00f6hlichem Entsetzen,", "tokens": ["Fast", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh\u2019 ich, mit der Erden Sch\u00e4tzen,", "tokens": ["Seh'", "ich", ",", "mit", "der", "Er\u00b7den", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier aufs neue mich beschenkt.", "tokens": ["Hier", "aufs", "neu\u00b7e", "mich", "be\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein, des Sch\u00f6pfers weise G\u00fcte,", "tokens": ["Mein", ",", "des", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief bewunderndes Gem\u00fchte", "tokens": ["Tief", "be\u00b7wun\u00b7dern\u00b7des", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freut sich, lobet GOtt und denkt:", "tokens": ["Freut", "sich", ",", "lo\u00b7bet", "Gott", "und", "denkt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit erstaunendem rc.", "tokens": ["Mit", "er\u00b7stau\u00b7nen\u00b7dem", "rc", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ist ein Wesen, das uns liebet,", "tokens": ["Ist", "ein", "We\u00b7sen", ",", "das", "uns", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und uns so viel Gutes giebet,", "tokens": ["Und", "uns", "so", "viel", "Gu\u00b7tes", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oder ist es etwan nicht?", "tokens": ["O\u00b7der", "ist", "es", "et\u00b7wan", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es nicht; so ists vern\u00fcnftig,", "tokens": ["Ist", "es", "nicht", ";", "so", "ists", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$.", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr, itzt sowohl als k\u00fcnftig,", "tokens": ["Da\u00df", "ihr", ",", "itzt", "so\u00b7wohl", "als", "k\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "KON", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht beachtet, was geschicht.", "tokens": ["Nicht", "be\u00b7ach\u00b7tet", ",", "was", "ge\u00b7schicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist es aber; so betrachtet,", "tokens": ["Ist", "es", "a\u00b7ber", ";", "so", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie erf\u00fcllt ihr eure Pflicht,", "tokens": ["Wie", "er\u00b7f\u00fcllt", "ihr", "eu\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da ihr Seine Huld nicht achtet?", "tokens": ["Da", "ihr", "Sei\u00b7ne", "Huld", "nicht", "ach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Minder kann f\u00fcrwahr kein Vieh", "tokens": ["Min\u00b7der", "kann", "f\u00fcr\u00b7wahr", "kein", "Vieh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Des empfangnen Guts gedenken,", "tokens": ["Des", "emp\u00b7fang\u00b7nen", "Guts", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Minder Dank dem Geber schenken.", "tokens": ["Min\u00b7der", "Dank", "dem", "Ge\u00b7ber", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Jenes dankt dem Sch\u00f6pfer nie;", "tokens": ["Je\u00b7nes", "dankt", "dem", "Sch\u00f6p\u00b7fer", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ist es aber darum schlimmer,", "tokens": ["Ist", "es", "a\u00b7ber", "da\u00b7rum", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Als ihr stummen Menschen seyd?", "tokens": ["Als", "ihr", "stum\u00b7men", "Men\u00b7schen", "seyd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Gottes Macht und G\u00fctigkeit", "tokens": ["Got\u00b7tes", "Macht", "und", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ehrt auch ihr, im Danken, nimmer.", "tokens": ["Ehrt", "auch", "ihr", ",", "im", "Dan\u00b7ken", ",", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "APPRART", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ist denn, was GOtt hier beschehrt,", "tokens": ["Ist", "denn", ",", "was", "Gott", "hier", "be\u00b7schehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWS", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Jm Getrayde, das euch n\u00e4hrt,", "tokens": ["Jm", "Ge\u00b7tray\u00b7de", ",", "das", "euch", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Etwan nicht der M\u00fche wehrt,", "tokens": ["Et\u00b7wan", "nicht", "der", "M\u00fc\u00b7he", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df ihr solchen GOtt verehrt,", "tokens": ["Da\u00df", "ihr", "sol\u00b7chen", "Gott", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Der doch, blo\u00df aus lauter G\u00fcte,", "tokens": ["Der", "doch", ",", "blo\u00df", "aus", "lau\u00b7ter", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ein durch Jhn vergn\u00fcgt Gem\u00fchte,", "tokens": ["Ein", "durch", "Jhn", "ver\u00b7gn\u00fcgt", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Eure Lust, zum Dienst begehrt?", "tokens": ["Eu\u00b7re", "Lust", ",", "zum", "Dienst", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}