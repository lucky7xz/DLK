{"textgrid.poem.59778": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man schilt mein Deutschland einen Greis,", "tokens": ["Man", "schilt", "mein", "Deutschland", "ei\u00b7nen", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zu kalt und zu verst\u00e4ndig,", "tokens": ["Zu", "kalt", "und", "zu", "ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber schelt': \u00bbEr ist zu hei\u00df,", "tokens": ["Ich", "a\u00b7ber", "schelt'", ":", "\u00bb", "Er", "ist", "zu", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Junge, zu lebendig,", "tokens": ["Der", "Jun\u00b7ge", ",", "zu", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Junge noch, doch hoffnungsvoll", "tokens": ["Ein", "Jun\u00b7ge", "noch", ",", "doch", "hoff\u00b7nungs\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei allen tollen Streichen!", "tokens": ["Bei", "al\u00b7len", "tol\u00b7len", "Strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und grade darum darf und soll", "tokens": ["Und", "gra\u00b7de", "da\u00b7rum", "darf", "und", "soll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "VMFIN", "KON", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Hoffnung mir nicht bleichen.", "tokens": ["Die", "Hoff\u00b7nung", "mir", "nicht", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kann man den wilden Jugendmut,", "tokens": ["Kann", "man", "den", "wil\u00b7den", "Ju\u00b7gend\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der sch\u00e4umt und b\u00e4umt, nur binden,", "tokens": ["Der", "sch\u00e4umt", "und", "b\u00e4umt", ",", "nur", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So wird er sein verlornes Gut,", "tokens": ["So", "wird", "er", "sein", "ver\u00b7lor\u00b7nes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Freiheit, wiederfinden.\u00ab", "tokens": ["Die", "Frei\u00b7heit", ",", "wie\u00b7der\u00b7fin\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}