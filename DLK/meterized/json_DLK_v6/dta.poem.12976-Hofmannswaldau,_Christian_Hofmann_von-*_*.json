{"dta.poem.12976": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft der hohe witz, damit dein ehrgeitz stutzet?", "tokens": ["Was", "hilfft", "der", "ho\u00b7he", "witz", ",", "da\u00b7mit", "dein", "ehr\u00b7geitz", "stut\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesetzt, da\u00df du, o mensch! der kl\u00fcffte grund durch-", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "du", ",", "o", "mensch", "!", "der", "kl\u00fcff\u00b7te", "grund", "durch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "$,", "FM", "FM", "$.", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "rennst,", "tokens": ["rennst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die kr\u00e4ffte der natur, der l\u00e4nder sitten kennst,", "tokens": ["Die", "kr\u00e4ff\u00b7te", "der", "na\u00b7tur", ",", "der", "l\u00e4n\u00b7der", "sit\u00b7ten", "kennst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und alles inne hast, was gro\u00dfen h\u00f6fen nutzet.", "tokens": ["Und", "al\u00b7les", "in\u00b7ne", "hast", ",", "was", "gro\u00b7\u00dfen", "h\u00f6\u00b7fen", "nut\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "VAFIN", "$,", "PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gesetzt: da\u00df dein verstand bi\u00df in den himmel steigt,", "tokens": ["Ge\u00b7setzt", ":", "da\u00df", "dein", "ver\u00b7stand", "bi\u00df", "in", "den", "him\u00b7mel", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KOUS", "PPOSAT", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in der monden-welt die b\u00fcrger kennen lernet;", "tokens": ["Und", "in", "der", "mon\u00b7den\u00b7welt", "die", "b\u00fcr\u00b7ger", "ken\u00b7nen", "ler\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es bleibt die weisheit doch noch weit von dir entfernet,", "tokens": ["Es", "bleibt", "die", "weis\u00b7heit", "doch", "noch", "weit", "von", "dir", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil keine wissenschafft dir deine thorheit zeigt.", "tokens": ["Weil", "kei\u00b7ne", "wis\u00b7sen\u00b7schafft", "dir", "dei\u00b7ne", "thor\u00b7heit", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}