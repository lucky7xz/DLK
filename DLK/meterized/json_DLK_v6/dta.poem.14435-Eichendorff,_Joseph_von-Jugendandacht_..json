{"dta.poem.14435": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00e4fer spricht, wenn er fr\u00fchmorgens weidet:", "tokens": ["Der", "Sch\u00e4\u00b7fer", "spricht", ",", "wenn", "er", "fr\u00fch\u00b7mor\u00b7gens", "wei\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edort dr\u00fcben wohnt Sie hinter Berg' und Fl\u00fcssen!\u201c", "tokens": ["\u201e", "dort", "dr\u00fc\u00b7ben", "wohnt", "Sie", "hin\u00b7ter", "Ber\u00b7g'", "und", "Fl\u00fcs\u00b7sen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch seine Wunden deckt Sie gern mit K\u00fcssen,", "tokens": ["Doch", "sei\u00b7ne", "Wun\u00b7den", "deckt", "Sie", "gern", "mit", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann lauschend Licht am stillen Abend scheidet.", "tokens": ["Wann", "lau\u00b7schend", "Licht", "am", "stil\u00b7len", "A\u00b7bend", "schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "m