{"textgrid.poem.43566": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seemuscheln", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme her vom Meeresstrand,", "tokens": ["Ich", "kom\u00b7me", "her", "vom", "Mee\u00b7res\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fand ich Bl\u00fcmlein allerhand:", "tokens": ["Da", "fand", "ich", "Bl\u00fcm\u00b7lein", "al\u00b7ler\u00b7hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ohne Pfleg' und Sonnenschein", "tokens": ["Die", "oh\u00b7ne", "Pfleg'", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im wei\u00dfen Sande gut gedeih'n,", "tokens": ["Im", "wei\u00b7\u00dfen", "San\u00b7de", "gut", "ge\u00b7deih'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die nicht im Reif und Schnee vergehn,", "tokens": ["Die", "nicht", "im", "Reif", "und", "Schnee", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Regengu\u00df und Sturm bestehn,", "tokens": ["Vor", "Re\u00b7gen\u00b7gu\u00df", "und", "Sturm", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, die da bl\u00fch'n das ganze Jahr", "tokens": ["Ja", ",", "die", "da", "bl\u00fch'n", "das", "gan\u00b7ze", "Jahr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Strande sch\u00f6n und wunderbar.", "tokens": ["Am", "Stran\u00b7de", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbhast du dabei an mich gedacht", "tokens": ["\u00bb", "hast", "du", "da\u00b7bei", "an", "mich", "ge\u00b7dacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "PAV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mir ein Bl\u00fcmlein mitgebracht?\u00ab", "tokens": ["Und", "mir", "ein", "Bl\u00fcm\u00b7lein", "mit\u00b7ge\u00b7bracht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich habe wohl an dich gedacht", "tokens": ["Ich", "ha\u00b7be", "wohl", "an", "dich", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dir manch Bl\u00fcmlein mitgebracht.", "tokens": ["Und", "dir", "manch", "Bl\u00fcm\u00b7lein", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimm diese Muscheln gelb und grau", "tokens": ["Nimm", "die\u00b7se", "Mu\u00b7scheln", "gelb", "und", "grau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rosenroth und himmelblau.", "tokens": ["Und", "ro\u00b7sen\u00b7roth", "und", "him\u00b7mel\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}