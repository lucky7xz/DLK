{"textgrid.poem.38621": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Todtenopfer", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Berge stehn so d\u00fcster,", "tokens": ["Die", "Ber\u00b7ge", "stehn", "so", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Nebeldunst umflort;", "tokens": ["Von", "Ne\u00b7bel\u00b7dunst", "um\u00b7flort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch banges Rohrgefl\u00fcster", "tokens": ["Durch", "ban\u00b7ges", "Rohr\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rinnt schwach das B\u00e4chlein fort;", "tokens": ["Rinnt", "schwach", "das", "B\u00e4ch\u00b7lein", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein fernes Hirtenfeuer,", "tokens": ["Ein", "fer\u00b7nes", "Hir\u00b7ten\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am grauen Fichtenhain,", "tokens": ["Am", "grau\u00b7en", "Fich\u00b7ten\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hellt matt der D\u00e4mmrung Schleyer,", "tokens": ["Hellt", "matt", "der", "D\u00e4mm\u00b7rung", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Leichenfackel-Schein.", "tokens": ["Wie", "Lei\u00b7chen\u00b7fa\u00b7ckel\u00b7Schein", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus Warten und aus Kl\u00fcften", "tokens": ["Aus", "War\u00b7ten", "und", "aus", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fleugt scheu die Eul' empor;", "tokens": ["Fleugt", "scheu", "die", "Eul'", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gehn aus ihren Gr\u00fcften", "tokens": ["Es", "gehn", "aus", "ih\u00b7ren", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Geister leis' hervor;", "tokens": ["Die", "Geis\u00b7ter", "leis'", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Still tanzen, in Ruinen,", "tokens": ["Still", "tan\u00b7zen", ",", "in", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Gnomen und die Fey'n,", "tokens": ["Die", "Gno\u00b7men", "und", "die", "Fey'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Gl\u00fchwurm bleich beschienen,", "tokens": ["Vom", "Gl\u00fch\u00b7wurm", "bleich", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den abendlichen Reih'n.", "tokens": ["Den", "a\u00b7bend\u00b7li\u00b7chen", "Reih'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Seegestad' erl\u00f6schen", "tokens": ["Am", "See\u00b7ge\u00b7stad'", "er\u00b7l\u00f6\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Dorfes L\u00e4mpchen schon;", "tokens": ["Des", "Dor\u00b7fes", "L\u00e4mp\u00b7chen", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Klosters dunklen Eschen", "tokens": ["Des", "Klos\u00b7ters", "dunk\u00b7len", "E\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entlispelt Klageton;", "tokens": ["Ent\u00b7lis\u00b7pelt", "Kla\u00b7ge\u00b7ton", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sterne blinken traurig", "tokens": ["Die", "Ster\u00b7ne", "blin\u00b7ken", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Herbstgew\u00f6lk umgraut;", "tokens": ["Vom", "Herbst\u00b7ge\u00b7w\u00f6lk", "um\u00b7graut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Winde seufzen schaurig", "tokens": ["Die", "Win\u00b7de", "seuf\u00b7zen", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im hohen Farrenkraut.", "tokens": ["Im", "ho\u00b7hen", "Far\u00b7ren\u00b7kraut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Traurenden Gedanken", "tokens": ["Des", "Trau\u00b7ren\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entschweifen bang dem Schoo\u00df", "tokens": ["Ent\u00b7schwei\u00b7fen", "bang", "dem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Alpenwelt, und wanken", "tokens": ["Der", "Al\u00b7pen\u00b7welt", ",", "und", "wan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um ferner Gr\u00e4ber Moos.", "tokens": ["Um", "fer\u00b7ner", "Gr\u00e4\u00b7ber", "Moos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Tief ist die Ruh' der Gr\u00fcfte;", "tokens": ["Tief", "ist", "die", "Ruh'", "der", "Gr\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Morgensonne Licht,", "tokens": ["Der", "Mor\u00b7gen\u00b7son\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Wehn der Fr\u00fchlingsl\u00fcfte,", "tokens": ["Das", "Wehn", "der", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weckt ihre Schlumm'rer nicht.", "tokens": ["Weckt", "ih\u00b7re", "Schlum\u00b7m'\u00b7rer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "O Freunde! deren holde", "tokens": ["O", "Freun\u00b7de", "!", "de\u00b7ren", "hol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestalten, mildumstralt", "tokens": ["Ge\u00b7stal\u00b7ten", ",", "mil\u00b7dums\u00b7tralt"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von blassem Abendgolde,", "tokens": ["Von", "blas\u00b7sem", "A\u00b7bend\u00b7gol\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir die Erinnrung malt:", "tokens": ["Mir", "die", "E\u00b7rinn\u00b7rung", "malt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+----+", "measure": "dactylic.init"}, "line.5": {"text": "F\u00fcnf Kr\u00e4nze von Platanen", "tokens": ["F\u00fcnf", "Kr\u00e4n\u00b7ze", "von", "Pla\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bringt, hier am Felsaltar,", "tokens": ["Bringt", ",", "hier", "am", "Fel\u00b7sal\u00b7tar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sehnsucht euren Manen", "tokens": ["Die", "Sehn\u00b7sucht", "eu\u00b7ren", "Ma\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Todtenopfer dar.", "tokens": ["Zum", "Tod\u00b7ten\u00b7op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}