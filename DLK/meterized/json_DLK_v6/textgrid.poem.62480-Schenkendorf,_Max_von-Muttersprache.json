{"textgrid.poem.62480": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Muttersprache", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Muttersprache, Mutterlaut!", "tokens": ["Mut\u00b7ter\u00b7spra\u00b7che", ",", "Mut\u00b7ter\u00b7laut", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so wonnesam, so traut!", "tokens": ["Wie", "so", "won\u00b7ne\u00b7sam", ",", "so", "traut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erstes Wort, das mir erschallet,", "tokens": ["Ers\u00b7tes", "Wort", ",", "das", "mir", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfes, erstes Liebeswort,", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "ers\u00b7tes", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erster Ton, den ich gelallet,", "tokens": ["Ers\u00b7ter", "Ton", ",", "den", "ich", "ge\u00b7lal\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klingest ewig in mir fort.", "tokens": ["Klin\u00b7gest", "e\u00b7wig", "in", "mir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wie tr\u00fcb ist meinem Sinn,", "tokens": ["Ach", ",", "wie", "tr\u00fcb", "ist", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich in der Fremde bin,", "tokens": ["Wenn", "ich", "in", "der", "Frem\u00b7de", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich fremde Zungen \u00fcben,", "tokens": ["Wenn", "ich", "frem\u00b7de", "Zun\u00b7gen", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremde Worte brauchen mu\u00df,", "tokens": ["Frem\u00b7de", "Wor\u00b7te", "brau\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ich nimmermehr kann lieben,", "tokens": ["Die", "ich", "nim\u00b7mer\u00b7mehr", "kann", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die nicht klingen als ein Gru\u00df!", "tokens": ["Die", "nicht", "klin\u00b7gen", "als", "ein", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprache sch\u00f6n und wunderbar,", "tokens": ["Spra\u00b7che", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wie klingest du so klar!", "tokens": ["Ach", "wie", "klin\u00b7gest", "du", "so", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will noch tiefer mich vertiefen", "tokens": ["Will", "noch", "tie\u00b7fer", "mich", "ver\u00b7tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Reichthum, in die Pracht,", "tokens": ["In", "den", "Reicht\u00b7hum", ",", "in", "die", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist mir's doch, als ob mich riefen", "tokens": ["Ist", "mir's", "doch", ",", "als", "ob", "mich", "rie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "V\u00e4ter aus des Grabes Nacht.", "tokens": ["V\u00e4\u00b7ter", "aus", "des", "Gra\u00b7bes", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Klinge, klinge fort und fort,", "tokens": ["Klin\u00b7ge", ",", "klin\u00b7ge", "fort", "und", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heldensprache, Liebeswort,", "tokens": ["Hel\u00b7den\u00b7spra\u00b7che", ",", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steig' empor aus tiefen Gr\u00fcften,", "tokens": ["Steig'", "em\u00b7por", "aus", "tie\u00b7fen", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ngst verschollnes altes Lied,", "tokens": ["L\u00e4ngst", "ver\u00b7scholl\u00b7nes", "al\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leb' aufs Neu in heil'gen Schriften,", "tokens": ["Leb'", "aufs", "Neu", "in", "heil'\u00b7gen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dir jedes Herz ergl\u00fcht.", "tokens": ["Da\u00df", "dir", "je\u00b7des", "Herz", "er\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ueberall weht Gottes Hauch,", "tokens": ["Ue\u00b7be\u00b7rall", "weht", "Got\u00b7tes", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilig ist wol mancher Brauch.", "tokens": ["Hei\u00b7lig", "ist", "wol", "man\u00b7cher", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber soll ich beten, danken,", "tokens": ["A\u00b7ber", "soll", "ich", "be\u00b7ten", ",", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geb' ich meine Liebe kund,", "tokens": ["Geb'", "ich", "mei\u00b7ne", "Lie\u00b7be", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine seligsten Gedanken,", "tokens": ["Mei\u00b7ne", "se\u00b7ligs\u00b7ten", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Sprech' ich wie der Mutter Mund!", "tokens": ["Sprech'", "ich", "wie", "der", "Mut\u00b7ter", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}