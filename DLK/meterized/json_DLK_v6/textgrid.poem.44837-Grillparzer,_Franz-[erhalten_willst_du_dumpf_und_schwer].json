{"textgrid.poem.44837": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[erhalten willst du dumpf und schwer]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhalten willst du dumpf und schwer,", "tokens": ["Er\u00b7hal\u00b7ten", "willst", "du", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4chst doch alles brausend.", "tokens": ["Und", "w\u00e4chst", "doch", "al\u00b7les", "brau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer hundert hatte, hats nicht mehr,", "tokens": ["Wer", "hun\u00b7dert", "hat\u00b7te", ",", "hats", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "VAFIN", "$,", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwirbt der Nachbar tausend.", "tokens": ["Er\u00b7wirbt", "der", "Nach\u00b7bar", "tau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}