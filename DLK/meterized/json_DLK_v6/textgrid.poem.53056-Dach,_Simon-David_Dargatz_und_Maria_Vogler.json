{"textgrid.poem.53056": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "David Dargatz und Maria Vogler", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00fcnde mit der Erden", "tokens": ["Es", "st\u00fcn\u00b7de", "mit", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann lieben solte werden", "tokens": ["Wann", "lie\u00b7ben", "sol\u00b7te", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVINF", "VMFIN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Menschen abgethan,", "tokens": ["Von", "Men\u00b7schen", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Al\u00df wann der Sonnen wagen", "tokens": ["Al\u00df", "wann", "der", "Son\u00b7nen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem leuchten wolt' entsagen", "tokens": ["Dem", "leuch\u00b7ten", "wolt'", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVINF", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auff seiner Himmels Bahn.", "tokens": ["Auff", "sei\u00b7ner", "Him\u00b7mels", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn ist auch zu ermessen,", "tokens": ["Denn", "ist", "auch", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was t\u00e4glich wird gefressen", "tokens": ["Was", "t\u00e4g\u00b7lich", "wird", "ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Volk durch Krieg vnd Schwerd,", "tokens": ["F\u00fcr", "Volk", "durch", "Krieg", "vnd", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd was hievor noch blieben,", "tokens": ["Vnd", "was", "hie\u00b7vor", "noch", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df sonst wie Rauch verstieben,", "tokens": ["Mu\u00df", "sonst", "wie", "Rauch", "ver\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Pest vnd Brand verheert.", "tokens": ["Durch", "Pest", "vnd", "Brand", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man fehrt in grossen Scharen", "tokens": ["Man", "fehrt", "in", "gros\u00b7sen", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach so viel Tausent Jahren", "tokens": ["Nach", "so", "viel", "Tau\u00b7sent", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Noch durch des Charons Meer,", "tokens": ["Noch", "durch", "des", "Cha\u00b7rons", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch sind da keine Wellen,", "tokens": ["Doch", "sind", "da", "kei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die einen nur zur stellen", "tokens": ["Die", "ei\u00b7nen", "nur", "zur", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "APPRART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hie br\u00e4chten wieder her.", "tokens": ["Hie", "br\u00e4ch\u00b7ten", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Sonne geht zwar nieder,", "tokens": ["Die", "Son\u00b7ne", "geht", "zwar", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6mpt aber t\u00e4glich wieder,", "tokens": ["K\u00f6mpt", "a\u00b7ber", "t\u00e4g\u00b7lich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wald lest seine Pracht,", "tokens": ["Der", "Wald", "lest", "sei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wird er wieder gl\u00e4ntzen,", "tokens": ["Doch", "wird", "er", "wie\u00b7der", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bald im frischen Lentzen", "tokens": ["So", "bald", "im", "fri\u00b7schen", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sonn' Ihn angelacht.", "tokens": ["Die", "Sonn'", "Ihn", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hat sich der Mond verlohren,", "tokens": ["Hat", "sich", "der", "Mond", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird wieder doch gebohren,", "tokens": ["Wird", "wie\u00b7der", "doch", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Meer leufft ab vnd zu,", "tokens": ["Das", "Meer", "leufft", "ab", "vnd", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der aber k\u00f6mpt nicht wieder,", "tokens": ["Der", "a\u00b7ber", "k\u00f6mpt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der sich nur einmahl nieder", "tokens": ["Der", "sich", "nur", "ein\u00b7mahl", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Legt zu der langen Ruh.", "tokens": ["Legt", "zu", "der", "lan\u00b7gen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Welt wer' vntergangen", "tokens": ["Die", "Welt", "wer'", "vn\u00b7ter\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sie kaum angefangen", "tokens": ["Da", "sie", "kaum", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Ihrer Kindheit schon,", "tokens": ["In", "Ih\u00b7rer", "Kind\u00b7heit", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df aber sie noch lebet,", "tokens": ["Da\u00df", "a\u00b7ber", "sie", "noch", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch noch zu leben strebet,", "tokens": ["Auch", "noch", "zu", "le\u00b7ben", "stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ist der Liebe thun.", "tokens": ["Das", "ist", "der", "Lie\u00b7be", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Lieb' allein kan machen", "tokens": ["Die", "Lieb'", "al\u00b7lein", "kan", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was nur von sch\u00f6nen Sachen", "tokens": ["Was", "nur", "von", "sch\u00f6\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird jrgends angeschawt,", "tokens": ["Wird", "jr\u00b7gends", "an\u00b7ge\u00b7schawt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch sie hat m\u00fcssen werden", "tokens": ["Durch", "sie", "hat", "m\u00fcs\u00b7sen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "VMFIN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Fewr, Lufft, Wasser, Erden,", "tokens": ["Das", "Fewr", ",", "Lufft", ",", "Was\u00b7ser", ",", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Himmel auch erbawt.", "tokens": ["Der", "Him\u00b7mel", "auch", "er\u00b7bawt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df sich ein Wald verj\u00fcnget,", "tokens": ["Da\u00df", "sich", "ein", "Wald", "ver\u00b7j\u00fcn\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df hie ein Vogel singet,", "tokens": ["Da\u00df", "hie", "ein", "Vo\u00b7gel", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das dort wird Wild gesp\u00fcrt,", "tokens": ["Das", "dort", "wird", "Wild", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df B\u00e4ume Fr\u00fcchte hegen,", "tokens": ["Da\u00df", "B\u00e4u\u00b7me", "Fr\u00fcch\u00b7te", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gra\u00df w\u00e4chst nach dem Regen,", "tokens": ["Das", "Gra\u00df", "w\u00e4chst", "nach", "dem", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Liebe solches r\u00fchrt.", "tokens": ["Von", "Lie\u00b7be", "sol\u00b7ches", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wann alles die\u00df zusammen", "tokens": ["Wann", "al\u00b7les", "die\u00df", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Hitz' vnd Macht der Flammen", "tokens": ["Durch", "Hitz'", "vnd", "Macht", "der", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird werden Rauch vnd Wind,", "tokens": ["Wird", "wer\u00b7den", "Rauch", "vnd", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird doch die Liebe stehen,", "tokens": ["Wird", "doch", "die", "Lie\u00b7be", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnnd ewig nicht vergehen,", "tokens": ["Vnnd", "e\u00b7wig", "nicht", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weil sie Gott selbst entz\u00fcndt.", "tokens": ["Weil", "sie", "Gott", "selbst", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Er wird durch sie getrieben", "tokens": ["Er", "wird", "durch", "sie", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ewiglich zu lieben,", "tokens": ["Die", "e\u00b7wig\u00b7lich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So er Ihm hat erwehlt,", "tokens": ["So", "er", "Ihm", "hat", "er\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Eh al\u00df die Welt gegr\u00fcndet", "tokens": ["Eh", "al\u00df", "die", "Welt", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mit allem was man findet,", "tokens": ["Mit", "al\u00b7lem", "was", "man", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eh al\u00df man Stunden zehlt'.", "tokens": ["Eh", "al\u00df", "man", "Stun\u00b7den", "zehlt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Al\u00dfdann wird man erkennen,", "tokens": ["Al\u00df\u00b7dann", "wird", "man", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Was wir nur Tr\u00e4wm' jtzt nennen,", "tokens": ["Was", "wir", "nur", "Tr\u00e4wm'", "jtzt", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sehr er vns geliebt,", "tokens": ["Wie", "sehr", "er", "vns", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie er sich vns verbunden,", "tokens": ["Wie", "er", "sich", "vns", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann er durch seine Wunden", "tokens": ["Wann", "er", "durch", "sei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vns Ihm selbst wiedergiebt:", "tokens": ["Vns", "Ihm", "selbst", "wie\u00b7der\u00b7giebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was sey vom Himmel steigen,", "tokens": ["Was", "sey", "vom", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich vor dem Menschen neigen", "tokens": ["Sich", "vor", "dem", "Men\u00b7schen", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den er selbst hat gemacht,", "tokens": ["Den", "er", "selbst", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was sey Frost, Hitz' erleiden,", "tokens": ["Was", "sey", "Frost", ",", "Hitz'", "er\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durst, Hunger, Schmach vnd neiden,", "tokens": ["Durst", ",", "Hun\u00b7ger", ",", "Schmach", "vnd", "nei\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von S\u00fcndern seyn verlacht.", "tokens": ["Von", "S\u00fcn\u00b7dern", "seyn", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Demnach, der sich ergeben", "tokens": ["Dem\u00b7nach", ",", "der", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Liebes Joch zu leben,", "tokens": ["Im", "Lie\u00b7bes", "Joch", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der irret g\u00e4ntzlich nicht,", "tokens": ["Der", "ir\u00b7ret", "g\u00e4ntz\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann er sich nur nicht m\u00fchet,", "tokens": ["Wann", "er", "sich", "nur", "nicht", "m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Joch der Vnzucht ziehet,", "tokens": ["Am", "Joch", "der", "Vn\u00b7zucht", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ausser dieser Pflicht.", "tokens": ["Die", "aus\u00b7ser", "die\u00b7ser", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die b\u00f6se Lust verschwindet,", "tokens": ["Die", "b\u00f6\u00b7se", "Lust", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Ihre stat sich findet", "tokens": ["An", "Ih\u00b7re", "stat", "sich", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Leid, Seelenweh' vnd Schand',", "tokens": ["Leid", ",", "See\u00b7len\u00b7weh'", "vnd", "Schand'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Ehlich Leben bleibet,", "tokens": ["Ein", "Eh\u00b7lich", "Le\u00b7ben", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leid, Trawrigkeit vertreibet,", "tokens": ["Leid", ",", "Traw\u00b7rig\u00b7keit", "ver\u00b7trei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bringt gut Ger\u00fccht' im Land'.", "tokens": ["Bringt", "gut", "Ge\u00b7r\u00fccht'", "im", "Land'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ihr werdet besser wissen", "tokens": ["Ihr", "wer\u00b7det", "bes\u00b7ser", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was der hat zu geniessen,", "tokens": ["Was", "der", "hat", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Herr Br\u00e4utgam, der so lebt,", "tokens": ["Herr", "Br\u00e4ut\u00b7gam", ",", "der", "so", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Ihr in diesem Stande", "tokens": ["Wenn", "Ihr", "in", "die\u00b7sem", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Liebe keuschem Bande", "tokens": ["Der", "Lie\u00b7be", "keu\u00b7schem", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ewr Leben erst anhebt.", "tokens": ["Ewr", "Le\u00b7ben", "erst", "an\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich acht' halb derer Leben,", "tokens": ["Ich", "acht'", "halb", "de\u00b7rer", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PDS", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Die sich nicht weiter geben,", "tokens": ["Die", "sich", "nicht", "wei\u00b7ter", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnnd m\u00fcssen so davon,", "tokens": ["Vnnd", "m\u00fcs\u00b7sen", "so", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der seinen guten Nahmen", "tokens": ["Der", "sei\u00b7nen", "gu\u00b7ten", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hin erbt auff seinen Saamen,", "tokens": ["Hin", "erbt", "auff", "sei\u00b7nen", "Saa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der lebt vnd st\u00fcrb' er schon.", "tokens": ["Der", "lebt", "vnd", "st\u00fcrb'", "er", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}