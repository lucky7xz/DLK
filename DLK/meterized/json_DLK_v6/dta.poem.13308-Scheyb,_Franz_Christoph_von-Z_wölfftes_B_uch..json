{"dta.poem.13308": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso will ich meinen Schlu\u00df nach diesen ", "tokens": ["\u201e", "so", "will", "ich", "mei\u00b7nen", "Schlu\u00df", "nach", "die\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eund sie dadurch noch mehr den Tugenden verpflichten:", "tokens": ["\u201e", "und", "sie", "da\u00b7durch", "noch", "mehr", "den", "Tu\u00b7gen\u00b7den", "ver\u00b7pflich\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "PAV", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eer ist auch schon so viel als richtig und gef\u00e4llt;", "tokens": ["\u201e", "er", "ist", "auch", "schon", "so", "viel", "als", "rich\u00b7tig", "und", "ge\u00b7f\u00e4llt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ADV", "PIAT", "KOKOM", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "290\u201dEs kommt mir nichts mehr vor, so mich im Zweifel h\u00e4lt.", "tokens": ["\"", "Es", "kommt", "mir", "nichts", "mehr", "vor", ",", "so", "mich", "im", "Zwei\u00b7fel", "h\u00e4lt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIS", "ADV", "PTKVZ", "$,", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas auf dem K\u00f6nigs-Thron die Kronen-H\u00e4upter sprechen,", "tokens": ["\u201e", "was", "auf", "dem", "K\u00f6\u00b7nigs\u00b7Thron", "die", "Kro\u00b7nen\u00b7H\u00e4up\u00b7ter", "spre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ekann die Scharffsinnigkeit der Wahrheit niemahls schw\u00e4chen.", "tokens": ["\u201e", "kann", "die", "Scharff\u00b7sin\u00b7nig\u00b7keit", "der", "Wahr\u00b7heit", "nie\u00b7mahls", "schw\u00e4\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewir wissen alles schon aus dem dreyfachen Mund,", "tokens": ["\u201e", "wir", "wis\u00b7sen", "al\u00b7les", "schon", "aus", "dem", "drey\u00b7fa\u00b7chen", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "\u201eso macht die Wahrheit es nur etwas kl\u00e4rer kund.", "tokens": ["\u201e", "so", "macht", "die", "Wahr\u00b7heit", "es", "nur", "et\u00b7was", "kl\u00e4\u00b7rer", "kund", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}