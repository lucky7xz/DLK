{"dta.poem.8694": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Robert und der Invalide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst unser H\u00fcttlein du im Abend schimmern? &#8212;", "tokens": ["Siehst", "un\u00b7ser", "H\u00fctt\u00b7lein", "du", "im", "A\u00b7bend", "schim\u00b7mern", "?", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lacht hinaus in's \u00f6de Heideland,", "tokens": ["Es", "lacht", "hin\u00b7aus", "in's", "\u00f6\u00b7de", "Hei\u00b7de\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APZR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als wohnt' in ihm das Gl\u00fcck, das uns entschwand,", "tokens": ["Als", "wohnt'", "in", "ihm", "das", "Gl\u00fcck", ",", "das", "uns", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht ein finstres Paar von Menschentr\u00fcmmern.", "tokens": ["Und", "nicht", "ein", "finst\u00b7res", "Paar", "von", "Men\u00b7schen\u00b7tr\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus einer andern Zeit, der guten, alten,", "tokens": ["Aus", "ei\u00b7ner", "an\u00b7dern", "Zeit", ",", "der", "gu\u00b7ten", ",", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als noch das Gl\u00fcck geruht in H\u00fcttleins Schoo\u00df,", "tokens": ["Als", "noch", "das", "Gl\u00fcck", "ge\u00b7ruht", "in", "H\u00fctt\u00b7leins", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und reicher Segen das Gefild umflo\u00df,", "tokens": ["Und", "rei\u00b7cher", "Se\u00b7gen", "das", "Ge\u00b7fild", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hat es die heitre Miene sich erhalten.", "tokens": ["Hat", "es", "die", "heit\u00b7re", "Mie\u00b7ne", "sich", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Hier sah man einst in sch\u00f6nen Sommertagen", "tokens": ["Hier", "sah", "man", "einst", "in", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die frohen L\u00e4mmer auf der Weide springen,", "tokens": ["Die", "fro\u00b7hen", "L\u00e4m\u00b7mer", "auf", "der", "Wei\u00b7de", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hier h\u00f6rte man die Hirtenfl\u00f6te klingen", "tokens": ["Hier", "h\u00f6r\u00b7te", "man", "die", "Hir\u00b7ten\u00b7fl\u00f6\u00b7te", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und im Getreide hell die Wachtel schlagen.", "tokens": ["Und", "im", "Ge\u00b7trei\u00b7de", "hell", "die", "Wach\u00b7tel", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hier zog der Pfad durch frische Wiesengr\u00fcnde,", "tokens": ["Hier", "zog", "der", "Pfad", "durch", "fri\u00b7sche", "Wie\u00b7sen\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df Abends er dem fr\u00f6hlichen Gesellen", "tokens": ["Da\u00df", "A\u00b7bends", "er", "dem", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Den schnellsten Weg zu seinem Liebchen k\u00fcnde;", "tokens": ["Den", "schnells\u00b7ten", "Weg", "zu", "sei\u00b7nem", "Lieb\u00b7chen", "k\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nun wiegt kein Saatfeld seine goldnen Wellen,", "tokens": ["Nun", "wiegt", "kein", "Saat\u00b7feld", "sei\u00b7ne", "gold\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und Alles schl\u00e4ft in tiefer Heideruh.", "tokens": ["Und", "Al\u00b7les", "schl\u00e4ft", "in", "tie\u00b7fer", "Hei\u00b7de\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Pfad, der nichts der Liebe mehr zu k\u00fcnden,", "tokens": ["Der", "Pfad", ",", "der", "nichts", "der", "Lie\u00b7be", "mehr", "zu", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Schlo\u00df trauernd seine gr\u00fcnen Lippen zu;", "tokens": ["Schlo\u00df", "trau\u00b7ernd", "sei\u00b7ne", "gr\u00fc\u00b7nen", "Lip\u00b7pen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und rings umher Vergessen und Verschwinden.", "tokens": ["Und", "rings", "um\u00b7her", "Ver\u00b7ges\u00b7sen", "und", "Ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Das H\u00fcttlein nur mit seinem Lindenbaume", "tokens": ["Das", "H\u00fctt\u00b7lein", "nur", "mit", "sei\u00b7nem", "Lin\u00b7den\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ist nicht erwacht aus seinem holden Traume.", "tokens": ["Ist", "nicht", "er\u00b7wacht", "aus", "sei\u00b7nem", "hol\u00b7den", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "\u2014 Ihm gleicht die Erde jenseits uns'rer Heide,", "tokens": ["Ihm", "gleicht", "die", "Er\u00b7de", "jen\u00b7seits", "un\u00b7s'\u00b7rer", "Hei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Ob l\u00e4ngst das Gl\u00fcck aus ihren Armen floh,", "tokens": ["Ob", "l\u00e4ngst", "das", "Gl\u00fcck", "aus", "ih\u00b7ren", "Ar\u00b7men", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die Erde thut, wie einst, noch immer froh,", "tokens": ["Die", "Er\u00b7de", "thut", ",", "wie", "einst", ",", "noch", "im\u00b7mer", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADV", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und schm\u00fcckt sich gerne mit dem Bl\u00fcthenkleide;", "tokens": ["Und", "schm\u00fcckt", "sich", "ger\u00b7ne", "mit", "dem", "Bl\u00fc\u00b7then\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Getreu der alten, schon gedankenlosen", "tokens": ["Ge\u00b7treu", "der", "al\u00b7ten", ",", "schon", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Gewohnheit, tr\u00e4gt sie j\u00e4hrlich ihre Rosen. \u2014", "tokens": ["Ge\u00b7wohn\u00b7heit", ",", "tr\u00e4gt", "sie", "j\u00e4hr\u00b7lich", "ih\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Hab' meine Lust, im H\u00fcttlein dort zu hausen,", "tokens": ["Hab'", "mei\u00b7ne", "Lust", ",", "im", "H\u00fctt\u00b7lein", "dort", "zu", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Es ist so leicht gezimmert, leicht bedacht,", "tokens": ["Es", "ist", "so", "leicht", "ge\u00b7zim\u00b7mert", ",", "leicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Da h\u00f6ren recht wir's, wenn die Winde brausen,", "tokens": ["Da", "h\u00f6\u00b7ren", "recht", "wir's", ",", "wenn", "die", "Win\u00b7de", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und unser Sch\u00e4tzel kommt, die Wetternacht.", "tokens": ["Und", "un\u00b7ser", "Sch\u00e4t\u00b7zel", "kommt", ",", "die", "Wet\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Bin gerne dort in heitern Abendstunden,", "tokens": ["Bin", "ger\u00b7ne", "dort", "in", "hei\u00b7tern", "A\u00b7bends\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn schon der lezte Sonnenstrahl geschwunden,", "tokens": ["Wenn", "schon", "der", "lez\u00b7te", "Son\u00b7nen\u00b7strahl", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wenn hell zu Sternen Sterne sich gesellen,", "tokens": ["Wenn", "hell", "zu", "Ster\u00b7nen", "Ster\u00b7ne", "sich", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und unsre Hunde auf zum Monde bellen,", "tokens": ["Und", "uns\u00b7re", "Hun\u00b7de", "auf", "zum", "Mon\u00b7de", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wenn sich der stille, blasse schleicht heran,", "tokens": ["Wenn", "sich", "der", "stil\u00b7le", ",", "blas\u00b7se", "schleicht", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "$,", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Als wollt' er diebisch unsrer H\u00fctte nah'n,", "tokens": ["Als", "wollt'", "er", "die\u00b7bisch", "uns\u00b7rer", "H\u00fct\u00b7te", "nah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und uns mit seinen leisen Silberh\u00e4nden", "tokens": ["Und", "uns", "mit", "sei\u00b7nen", "lei\u00b7sen", "Sil\u00b7ber\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Den leichten Schlaf durch's Fensterlein entwenden. \u2014", "tokens": ["Den", "leich\u00b7ten", "Schlaf", "durch's", "Fens\u00b7ter\u00b7lein", "ent\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Freund! h\u00f6re doch! wo wandert deine Seele,", "tokens": ["Freund", "!", "h\u00f6\u00b7re", "doch", "!", "wo", "wan\u00b7dert", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Derweil ich hier von H\u00fctt' und Mond erz\u00e4hle?", "tokens": ["Der\u00b7weil", "ich", "hier", "von", "H\u00fctt'", "und", "Mond", "er\u00b7z\u00e4h\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}