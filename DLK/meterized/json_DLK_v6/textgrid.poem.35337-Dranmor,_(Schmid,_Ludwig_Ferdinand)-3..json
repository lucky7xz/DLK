{"textgrid.poem.35337": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dein greises Haupt mit Silberlocken", "tokens": ["Ich", "sah", "dein", "grei\u00b7ses", "Haupt", "mit", "Sil\u00b7berl\u00b7o\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dachte: Nein! du hast nicht wohlgethan;", "tokens": ["Und", "dach\u00b7te", ":", "Nein", "!", "du", "hast", "nicht", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Haus des Irrsinns ist der Vatikan \u2013", "tokens": ["Ein", "Haus", "des", "Irr\u00b7sinns", "ist", "der", "Va\u00b7ti\u00b7kan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wandte mich von dir, bewegt, erschrocken.", "tokens": ["Und", "wand\u00b7te", "mich", "von", "dir", ",", "be\u00b7wegt", ",", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch drau\u00dfen in dem schrankenlosen Dom,", "tokens": ["Doch", "drau\u00b7\u00dfen", "in", "dem", "schran\u00b7ken\u00b7lo\u00b7sen", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erleuchtet von der Sternenkuppel Lichtern,", "tokens": ["Er\u00b7leuch\u00b7tet", "von", "der", "Ster\u00b7nen\u00b7kup\u00b7pel", "Lich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da rief ich, fern von p\u00e4pstlichen Gesichtern:", "tokens": ["Da", "rief", "ich", ",", "fern", "von", "p\u00e4pst\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Urquell des Lebens, hier ist unser Rom!", "tokens": ["Ur\u00b7quell", "des", "Le\u00b7bens", ",", "hier", "ist", "un\u00b7ser", "Rom", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}