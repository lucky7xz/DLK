{"textgrid.poem.41890": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Wolken finster schwellen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Wolken finster schwellen,", "tokens": ["Wie", "die", "Wol\u00b7ken", "fins\u00b7ter", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie ewig weiter wandern!", "tokens": ["Wie", "sie", "e\u00b7wig", "wei\u00b7ter", "wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine hebt sich nach der andern,", "tokens": ["Ei\u00b7ne", "hebt", "sich", "nach", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Himmel fa\u00dft sie nicht.", "tokens": ["Und", "der", "Him\u00b7mel", "fa\u00dft", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will auch oft an klaren Stellen", "tokens": ["Will", "auch", "oft", "an", "kla\u00b7ren", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundlich sich die Sonne zeigen,", "tokens": ["Freund\u00b7lich", "sich", "die", "Son\u00b7ne", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer neue Nebel steigen", "tokens": ["Im\u00b7mer", "neu\u00b7e", "Ne\u00b7bel", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wogend um das holde Licht:", "tokens": ["Wo\u00b7gend", "um", "das", "hol\u00b7de", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Finstres Herz, so willst du immer", "tokens": ["Finst\u00b7res", "Herz", ",", "so", "willst", "du", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Gedanken zu Gedanken", "tokens": ["Von", "Ge\u00b7dan\u00b7ken", "zu", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Traum zu Traume schwanken,", "tokens": ["Und", "von", "Traum", "zu", "Trau\u00b7me", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein aufgeregtes Meer?", "tokens": ["Wie", "ein", "auf\u00b7ge\u00b7reg\u00b7tes", "Meer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lacht dir doch mit hellem Schimmer", "tokens": ["Lacht", "dir", "doch", "mit", "hel\u00b7lem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Sonne still und freundlich;", "tokens": ["Ei\u00b7ne", "Son\u00b7ne", "still", "und", "freund\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprich, was th\u00fcrmen denn so feindlich", "tokens": ["Sprich", ",", "was", "th\u00fcr\u00b7men", "denn", "so", "feind\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine W\u00fcnsche sich umher?", "tokens": ["Dei\u00b7ne", "W\u00fcn\u00b7sche", "sich", "um\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}