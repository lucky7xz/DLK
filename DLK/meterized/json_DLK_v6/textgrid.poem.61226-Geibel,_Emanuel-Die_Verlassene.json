{"textgrid.poem.61226": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Verlassene", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O singt nur, ihr Schwestern, mit fr\u00f6hlichem Mund", "tokens": ["O", "singt", "nur", ",", "ihr", "Schwes\u00b7tern", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und f\u00fchret den Reigen im Lindengrund", "tokens": ["Und", "f\u00fch\u00b7ret", "den", "Rei\u00b7gen", "im", "Lin\u00b7den\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit den Burschen bei Zithern und Geigen! -", "tokens": ["Mit", "den", "Bur\u00b7schen", "bei", "Zi\u00b7thern", "und", "Gei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich aber la\u00dft gehn und schweigen!", "tokens": ["Mich", "a\u00b7ber", "la\u00dft", "gehn", "und", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was blickt ihr mir nach, und was wollt ihr von mir?", "tokens": ["Was", "blickt", "ihr", "mir", "nach", ",", "und", "was", "wollt", "ihr", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KON", "PWS", "VMFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich habe die Freude getragen wie ihr", "tokens": ["Ich", "ha\u00b7be", "die", "Freu\u00b7de", "ge\u00b7tra\u00b7gen", "wie", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "KOKOM", "PPOSAT"], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In der Brust mit Lachen und Scherzen -", "tokens": ["In", "der", "Brust", "mit", "La\u00b7chen", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun trag' ich den Tod im Herzen.", "tokens": ["Nun", "trag'", "ich", "den", "Tod", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Durch alle Wipfel der Lenzhauch geht,", "tokens": ["Durch", "al\u00b7le", "Wip\u00b7fel", "der", "Lenz\u00b7hauch", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin der Baum, der laublos steht;", "tokens": ["Ich", "bin", "der", "Baum", ",", "der", "laub\u00b7los", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wasser rieseln so helle,", "tokens": ["Die", "Was\u00b7ser", "rie\u00b7seln", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich bin die vertrocknete Quelle.", "tokens": ["Ich", "bin", "die", "ver\u00b7trock\u00b7ne\u00b7te", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die Treue, die Treue, darauf ich gebaut,", "tokens": ["Die", "Treu\u00b7e", ",", "die", "Treu\u00b7e", ",", "da\u00b7rauf", "ich", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PAV", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie ist mit dem Schnee vor der Sonne zertaut;", "tokens": ["Sie", "ist", "mit", "dem", "Schnee", "vor", "der", "Son\u00b7ne", "zer\u00b7taut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie Spreu vor dem Winde, so stiebet", "tokens": ["Wie", "Spreu", "vor", "dem", "Win\u00b7de", ",", "so", "stie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Meine Liebe, die ich geliebet.", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "die", "ich", "ge\u00b7lie\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}