{"textgrid.poem.47449": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eine tr\u00f6stlich holde Kunde weigre nicht!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine tr\u00f6stlich holde Kunde weigre nicht!", "tokens": ["Ei\u00b7ne", "tr\u00f6st\u00b7lich", "hol\u00b7de", "Kun\u00b7de", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Eine traute Wonnestunde weigre nicht!", "tokens": ["Ei\u00b7ne", "trau\u00b7te", "Won\u00b7nes\u00b7tun\u00b7de", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Heilbescheer, balsamischen, mein s\u00fc\u00dfer Arzt,", "tokens": ["Heil\u00b7be\u00b7scheer", ",", "bal\u00b7sa\u00b7mi\u00b7schen", ",", "mein", "s\u00fc\u00b7\u00dfer", "Arzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "F\u00fcr so manche b\u00f6se Wunde weigre nicht!", "tokens": ["F\u00fcr", "so", "man\u00b7che", "b\u00f6\u00b7se", "Wun\u00b7de", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Deiner Lippe Kandel, da\u00df von jeglicher", "tokens": ["Dei\u00b7ner", "Lip\u00b7pe", "Kan\u00b7del", ",", "da\u00df", "von", "jeg\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "$,", "KOUS", "APPR", "PIAT"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Thr\u00e4ne mir das Aug' gesunde, weigre nicht!", "tokens": ["Thr\u00e4\u00b7ne", "mir", "das", "Aug'", "ge\u00b7sun\u00b7de", ",", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Der da bis zum Kafe deine Sch\u00f6ne preis't,", "tokens": ["Der", "da", "bis", "zum", "Ka\u00b7fe", "dei\u00b7ne", "Sch\u00f6\u00b7ne", "preis't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Seine Kost dem Liedermunde weigre nicht!", "tokens": ["Sei\u00b7ne", "Kost", "dem", "Lie\u00b7der\u00b7mun\u00b7de", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Jene Brust, aus Lilienschimmer aufgeballt,", "tokens": ["Je\u00b7ne", "Brust", ",", "aus", "Li\u00b7li\u00b7en\u00b7schim\u00b7mer", "auf\u00b7ge\u00b7ballt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schwellend in vollkommner Runde, weigre nicht!", "tokens": ["Schwel\u00b7lend", "in", "voll\u00b7komm\u00b7ner", "Run\u00b7de", ",", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Was mit eifersuchtentflammtem Auge sieht", "tokens": ["Was", "mit", "ei\u00b7fer\u00b7such\u00b7tent\u00b7flamm\u00b7tem", "Au\u00b7ge", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die gestirnte Weltrotunde, weigre nicht!", "tokens": ["Die", "ge\u00b7stirn\u00b7te", "Welt\u00b7ro\u00b7tun\u00b7de", ",", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Was zu vollgehaltiger \u00c4onen Werth", "tokens": ["Was", "zu", "voll\u00b7ge\u00b7hal\u00b7ti\u00b7ger", "\u00c4\u00b7o\u00b7nen", "Werth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Stempelt eine Lustsekunde, weigre nicht!", "tokens": ["Stem\u00b7pelt", "ei\u00b7ne", "Lust\u00b7se\u00b7kun\u00b7de", ",", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Was allein empor zu halten f\u00e4hig ist", "tokens": ["Was", "al\u00b7lein", "em\u00b7por", "zu", "hal\u00b7ten", "f\u00e4\u00b7hig", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PTKVZ", "PTKZU", "VVINF", "ADJD", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ueber'm \u00f6den Grabesschlunde, weigre nicht!", "tokens": ["Ue\u00b7ber'm", "\u00f6\u00b7den", "Gra\u00b7bes\u00b7schlun\u00b7de", ",", "weig\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}