{"dta.poem.19672": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "2.  Epistel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einen freundlichen Gru\u00df,               ", "tokens": ["Ei\u00b7nen", "freund\u00b7li\u00b7chen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der in das Herze soll und mu\u00df;", "tokens": ["Der", "in", "das", "Her\u00b7ze", "soll", "und", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDS", "VVFIN", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gru\u00df liegt begraben,", "tokens": ["Der", "Gru\u00df", "liegt", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zwischen zwey goldenen Buchstaben,", "tokens": ["Zwi\u00b7schen", "zwey", "gol\u00b7de\u00b7nen", "Buch\u00b7sta\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Der eine hei\u00df: Eine Perle fein,", "tokens": ["Der", "ei\u00b7ne", "hei\u00df", ":", "Ei\u00b7ne", "Per\u00b7le", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich kann nicht Herzallerliebste stets bey dir seyn!", "tokens": ["Ich", "kann", "nicht", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "stets", "bey", "dir", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der andre hei\u00df: Sammet und Seiden,", "tokens": ["Der", "and\u00b7re", "hei\u00df", ":", "Sam\u00b7met", "und", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "$.", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Mein Schatz soll andre Junggesellen meiden.", "tokens": ["Mein", "Schatz", "soll", "and\u00b7re", "Jung\u00b7ge\u00b7sel\u00b7len", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich habe einen heimlichen Bothen ausgesandt,", "tokens": ["Ich", "ha\u00b7be", "ei\u00b7nen", "heim\u00b7li\u00b7chen", "Bo\u00b7then", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der dir und mir ist wohlbekannt,", "tokens": ["Der", "dir", "und", "mir", "ist", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das T\u00e4ublein thu ich bitten", "tokens": ["Das", "T\u00e4ub\u00b7lein", "thu", "ich", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Mit tugendlichen Sitten,", "tokens": ["Mit", "tu\u00b7gend\u00b7li\u00b7chen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df es soll mein Bothe seyn", "tokens": ["Da\u00df", "es", "soll", "mein", "Bo\u00b7the", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und sagen zu der Liebsten mein;", "tokens": ["Und", "sa\u00b7gen", "zu", "der", "Liebs\u00b7ten", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich gr\u00fc\u00df sie heimlich in der Still", "tokens": ["Ich", "gr\u00fc\u00df", "sie", "heim\u00b7lich", "in", "der", "Still"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und trau den falschen Zungen nicht viel,", "tokens": ["Und", "trau", "den", "fal\u00b7schen", "Zun\u00b7gen", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Gr\u00fc\u00dfe nur ihr M\u00fcndlein roth und wei\u00df,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "nur", "ihr", "M\u00fcnd\u00b7lein", "roth", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Welches ist gezieret mit ganzem Flei\u00df,", "tokens": ["Wel\u00b7ches", "ist", "ge\u00b7zie\u00b7ret", "mit", "gan\u00b7zem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Gr\u00fc\u00dfe sie durch grasgr\u00fcnen Klee,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "sie", "durch", "gras\u00b7gr\u00fc\u00b7nen", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Nach ihr thut mir mein Herz so weh.", "tokens": ["Nach", "ihr", "thut", "mir", "mein", "Herz", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Ich w\u00fcnsche ihr soviel gute Tage und Augenblick,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "ihr", "so\u00b7viel", "gu\u00b7te", "Ta\u00b7ge", "und", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Als ich des Nachts Sterne am Himmel erblick.", "tokens": ["Als", "ich", "des", "Nachts", "Ster\u00b7ne", "am", "Him\u00b7mel", "er\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ich w\u00fcnsche meiner Herzliebsten ein Haus", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mei\u00b7ner", "Herz\u00b7liebs\u00b7ten", "ein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Mich zu ihr immer ein und aus,", "tokens": ["Mich", "zu", "ihr", "im\u00b7mer", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Von Kristallen eine Th\u00fcr,", "tokens": ["Von", "Kris\u00b7tal\u00b7len", "ei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Und von N\u00e4gelein einen Riegel daf\u00fcr;", "tokens": ["Und", "von", "N\u00e4\u00b7ge\u00b7lein", "ei\u00b7nen", "Rie\u00b7gel", "da\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "PAV", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Von Sammet und Seiden ein Bett,", "tokens": ["Von", "Sam\u00b7met", "und", "Sei\u00b7den", "ein", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.28": {"text": "Das ist ihr zarter Leib wohl werth.", "tokens": ["Das", "ist", "ihr", "zar\u00b7ter", "Leib", "wohl", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wir leben beide auf dieser Erden,", "tokens": ["Wir", "le\u00b7ben", "bei\u00b7de", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Ach, da\u00df sie bald mein eigen m\u00f6cht werden.", "tokens": ["Ach", ",", "da\u00df", "sie", "bald", "mein", "ei\u00b7gen", "m\u00f6cht", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Eh ich meine Herzvielgeliebte wollt lassen,", "tokens": ["Eh", "ich", "mei\u00b7ne", "Herz\u00b7viel\u00b7ge\u00b7lieb\u00b7te", "wollt", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Eh sollt mein Herz ein Pfeil durchsto\u00dfen;", "tokens": ["Eh", "sollt", "mein", "Herz", "ein", "Pfeil", "durch\u00b7sto\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.33": {"text": "Eh ich meine Herzallerliebste wollt meiden,", "tokens": ["Eh", "ich", "mei\u00b7ne", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "wollt", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Eh sollt mein Herz eine S\u00e4ge durchschneiden.", "tokens": ["Eh", "sollt", "mein", "Herz", "ei\u00b7ne", "S\u00e4\u00b7ge", "durch\u00b7schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.35": {"text": "Es kann keiner seyn so behend,", "tokens": ["Es", "kann", "kei\u00b7ner", "seyn", "so", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Der von der Liebe k\u00f6nnt schreiben ein End;", "tokens": ["Der", "von", "der", "Lie\u00b7be", "k\u00f6nnt", "schrei\u00b7ben", "ein", "End", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.37": {"text": "Sie ist mein Morgen und Abendstern,", "tokens": ["Sie", "ist", "mein", "Mor\u00b7gen", "und", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Meine Augen sehn sie allezeit gern;", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "sehn", "sie", "al\u00b7le\u00b7zeit", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Ich sitze beym Trinken oder Essen,", "tokens": ["Ich", "sit\u00b7ze", "beym", "Trin\u00b7ken", "o\u00b7der", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "So kann ich meine Herzallerliebste nicht vergessen;", "tokens": ["So", "kann", "ich", "mei\u00b7ne", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "nicht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Wenn ich sie seh voll Freuden schweben,", "tokens": ["Wenn", "ich", "sie", "seh", "voll", "Freu\u00b7den", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "So freuet sich mein ganzes Leben.", "tokens": ["So", "freu\u00b7et", "sich", "mein", "gan\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Herzallerliebste, ich la\u00df nicht von dir ab,", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", ",", "ich", "la\u00df", "nicht", "von", "dir", "ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.44": {"text": "Bis man mich tr\u00e4get ins k\u00fchle Grab.", "tokens": ["Bis", "man", "mich", "tr\u00e4\u00b7get", "ins", "k\u00fch\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.45": {"text": "Herz in Herz geschlossen,", "tokens": ["Herz", "in", "Herz", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.46": {"text": "Pfeil in Pfeil gesto\u00dfen,", "tokens": ["Pfeil", "in", "Pfeil", "ge\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.47": {"text": "Lieb in Lieb verpflicht,", "tokens": ["Lieb", "in", "Lieb", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.48": {"text": "Herzallerliebste verla\u00df mich nicht;", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "ver\u00b7la\u00df", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.49": {"text": "Denn mein Herz ist ein Diamant,", "tokens": ["Denn", "mein", "Herz", "ist", "ein", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.50": {"text": "Dein und meine Liebe scheidet niemand.", "tokens": ["Dein", "und", "mei\u00b7ne", "Lie\u00b7be", "schei\u00b7det", "nie\u00b7mand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "NN", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Keine Rose, keine Nelke kann bl\u00fchen so sch\u00f6n,", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", ",", "kei\u00b7ne", "Nel\u00b7ke", "kann", "bl\u00fc\u00b7hen", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "Als wenn zwey verliebte Seele beysammen thun stehn.", "tokens"