{"textgrid.poem.35953": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich einsam schleiche", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich einsam schleiche", "tokens": ["Wenn", "ich", "ein\u00b7sam", "schlei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bei des Mondes Schein", "tokens": ["Bei", "des", "Mon\u00b7des", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hin durch Nu\u00dfgestr\u00e4uche", "tokens": ["Hin", "durch", "Nu\u00df\u00b7ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zu dem Buchenhain,", "tokens": ["Zu", "dem", "Bu\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn dann Philomele", "tokens": ["Wenn", "dann", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mir ihr Liedchen singt", "tokens": ["Mir", "ihr", "Lied\u00b7chen", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und in meine Seele", "tokens": ["Und", "in", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Tiefe Sehnsucht dringt:", "tokens": ["Tie\u00b7fe", "Sehn\u00b7sucht", "dringt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O dann t\u00f6nt die Laute", "tokens": ["O", "dann", "t\u00f6nt", "die", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zitternd mir im Arm", "tokens": ["Zit\u00b7ternd", "mir", "im", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PPER", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und es kr\u00e4nzt mit Raute", "tokens": ["Und", "es", "kr\u00e4nzt", "mit", "Rau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mich der Wehmuth Harm,", "tokens": ["Mich", "der", "Weh\u00b7muth", "Harm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Leise rinnen Thr\u00e4nen", "tokens": ["Lei\u00b7se", "rin\u00b7nen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auf die Veilchen hin", "tokens": ["Auf", "die", "Veil\u00b7chen", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und ein banges Sehnen", "tokens": ["Und", "ein", "ban\u00b7ges", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Bebt mir durch den Sinn,", "tokens": ["Bebt", "mir", "durch", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zieht mich nach der Ferne,", "tokens": ["Zieht", "mich", "nach", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo mein Liebling winkt,", "tokens": ["Wo", "mein", "Lieb\u00b7ling", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf zu jenem Sterne,", "tokens": ["Auf", "zu", "je\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der so r\u00f6thlich blinkt \u2013", "tokens": ["Der", "so", "r\u00f6th\u00b7lich", "blinkt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Einzige Vertraute,", "tokens": ["Ein\u00b7zi\u00b7ge", "Ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tr\u00f6sterin im Schmerz,", "tokens": ["Tr\u00f6s\u00b7te\u00b7rin", "im", "Schmerz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "T\u00f6ne du, o Laute,", "tokens": ["T\u00f6\u00b7ne", "du", ",", "o", "Lau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ruhe mir in's Herz!", "tokens": ["Ru\u00b7he", "mir", "in's", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}