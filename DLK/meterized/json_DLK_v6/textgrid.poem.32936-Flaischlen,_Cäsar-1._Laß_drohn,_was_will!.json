{"textgrid.poem.32936": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. La\u00df drohn, was will!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh doch den Wettersturm am Himmel!", "tokens": ["Sieh", "doch", "den", "Wet\u00b7ter\u00b7sturm", "am", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sieh doch die Wolken um die H\u00f6hn!\u00ab", "tokens": ["sieh", "doch", "die", "Wol\u00b7ken", "um", "die", "H\u00f6hn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich aber sag: das geht vor\u00fcber", "tokens": ["Ich", "a\u00b7ber", "sag", ":", "das", "geht", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$.", "PDS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und auf den Abend wird es sch\u00f6n!", "tokens": ["und", "auf", "den", "A\u00b7bend", "wird", "es", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gebt mich nur frei und la\u00dft mich's wagen,", "tokens": ["Gebt", "mich", "nur", "frei", "und", "la\u00dft", "mich's", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein bi\u00dfchen auch mir selbst zu traun!", "tokens": ["ein", "bi\u00df\u00b7chen", "auch", "mir", "selbst", "zu", "traun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was frommt es denn, altjungfer-\u00e4ngstlich", "tokens": ["Was", "frommt", "es", "denn", ",", "alt\u00b7jung\u00b7fer\u00b7\u00e4ngst\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nach jedem Nebel auszuschaun!?", "tokens": ["nach", "je\u00b7dem", "Ne\u00b7bel", "aus\u00b7zu\u00b7schaun", "!?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur frei sein mu\u00df ich! frei und ... ehe", "tokens": ["Nur", "frei", "sein", "mu\u00df", "ich", "!", "frei", "und", "...", "e\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VAINF", "VMFIN", "PPER", "$.", "ADJD", "KON", "$(", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Zorn zum Sieg in mir erlahmt", "tokens": ["der", "Zorn", "zum", "Sieg", "in", "mir", "er\u00b7lahmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und was ich Gro\u00dfes m\u00f6chte, elend", "tokens": ["und", "was", "ich", "Gro\u00b7\u00dfes", "m\u00f6ch\u00b7te", ",", "e\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PPER", "NN", "VMFIN", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in Alltagstr\u00f6del sich verkramt!", "tokens": ["in", "All\u00b7tag\u00b7str\u00f6\u00b7del", "sich", "ver\u00b7kramt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch tr\u00e4gt zu stolzbekr\u00e4nzten Zielen", "tokens": ["Noch", "tr\u00e4gt", "zu", "stolz\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Zie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein jauchzend Hoffen mich empor ...", "tokens": ["ein", "jauch\u00b7zend", "Hof\u00b7fen", "mich", "em\u00b7por", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und bis zu Ende sei gehalten,", "tokens": ["und", "bis", "zu", "En\u00b7de", "sei", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "was meiner Jugend ich beschwor!", "tokens": ["was", "mei\u00b7ner", "Ju\u00b7gend", "ich", "be\u00b7schwor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und grollten rings auch tausend Wetter", "tokens": ["Und", "groll\u00b7ten", "rings", "auch", "tau\u00b7send", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und droht es noch so von den H\u00f6hn ...", "tokens": ["und", "droht", "es", "noch", "so", "von", "den", "H\u00f6hn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "la\u00df drohn, was will! es geht vor\u00fcber", "tokens": ["la\u00df", "drohn", ",", "was", "will", "!", "es", "geht", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PWS", "VMFIN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und auf den Abend wird es sch\u00f6n!", "tokens": ["und", "auf", "den", "A\u00b7bend", "wird", "es", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}