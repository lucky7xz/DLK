{"textgrid.poem.50337": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "53. Tarquinius und Lucretia", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.85", "pl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie ", "tokens": ["So", "wie"], "token_info": ["word", "word"], "pos": ["ADV", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "So b\u00fcsst ", "tokens": ["So", "b\u00fcsst"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Beyd' haben ihren Wunsch: Man schaut an einem Ort", "tokens": ["Beyd'", "ha\u00b7ben", "ih\u00b7ren", "Wunsch", ":", "Man", "schaut", "an", "ei\u00b7nem", "Ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist des einen ", "tokens": ["Es", "ist", "des", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch spricht ein ", "tokens": ["Doch", "spricht", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der bringt ", "tokens": ["Der", "bringt"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die bringt ", "tokens": ["Die", "bringt"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}}}}