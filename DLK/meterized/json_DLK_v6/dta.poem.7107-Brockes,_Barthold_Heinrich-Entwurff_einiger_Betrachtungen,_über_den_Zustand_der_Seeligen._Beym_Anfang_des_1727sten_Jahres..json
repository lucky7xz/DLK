{"dta.poem.7107": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! wenn ich diesemnach solch einen Stand erwege,", "tokens": ["Ach", "!", "wenn", "ich", "die\u00b7sem\u00b7nach", "solch", "ei\u00b7nen", "Stand", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADV", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der Vollenkommenheit,", "tokens": ["Und", "in", "der", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So unaussprechlicher und ew\u2019ger Seeligkeit,", "tokens": ["So", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "und", "ew'\u00b7ger", "See\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Sch\u00f6pffers Liebe, Macht und Weisheit \u00fcberlege;", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", "Lie\u00b7be", ",", "Macht", "und", "Weis\u00b7heit", "\u00fc\u00b7berl\u00b7e\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fchl\u2019 ich bereits auf ird\u2019sche Sinnen,", "tokens": ["F\u00fchl'", "ich", "be\u00b7reits", "auf", "ird'\u00b7sche", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von einer nie gef\u00fchlten Lust,", "tokens": ["Von", "ei\u00b7ner", "nie", "ge\u00b7f\u00fchl\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus jenem Wollust-Meer, verschied\u2019ne Tropffen rinnen.", "tokens": ["Aus", "je\u00b7nem", "Wol\u00b7lust\u00b7Meer", ",", "ver\u00b7schie\u00b7d'\u00b7ne", "Tropf\u00b7fen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Es lechtzt, und wallt, und stehnt die Seel in meiner Brust;", "tokens": ["Es", "lechtzt", ",", "und", "wallt", ",", "und", "stehnt", "die", "Seel", "in", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie schwingt die Fittige, dahin sich zu verf\u00fcgen:", "tokens": ["Sie", "schwingt", "die", "Fit\u00b7ti\u00b7ge", ",", "da\u00b7hin", "sich", "zu", "ver\u00b7f\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein s\u00fcsses Sehnen dehnt in ruhigen Vergn\u00fcgen", "tokens": ["Ein", "s\u00fcs\u00b7ses", "Seh\u00b7nen", "dehnt", "in", "ru\u00b7hi\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mein gantzes Wesen aus: Ein\u2019 aufgekl\u00e4rte Stille", "tokens": ["Mein", "gant\u00b7zes", "We\u00b7sen", "aus", ":", "Ein'", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Umgiebt, durchdringet mich: Von jenem reinen Licht,", "tokens": ["Um\u00b7giebt", ",", "durch\u00b7drin\u00b7get", "mich", ":", "Von", "je\u00b7nem", "rei\u00b7nen", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$.", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das, wie ein reger Blitz, durch Nacht und Nebel bricht,", "tokens": ["Das", ",", "wie", "ein", "re\u00b7ger", "Blitz", ",", "durch", "Nacht", "und", "Ne\u00b7bel", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Trifft mich ein kleiner Strahl: Von jener feel\u2019gen F\u00fclle", "tokens": ["Trifft", "mich", "ein", "klei\u00b7ner", "Strahl", ":", "Von", "je\u00b7ner", "feel'\u00b7gen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Versp\u00fcr\u2019 ich halb entz\u00fcckt, in br\u00fcnstigen Verlangen,", "tokens": ["Ver\u00b7sp\u00fcr'", "ich", "halb", "ent\u00b7z\u00fcckt", ",", "in", "br\u00fcns\u00b7ti\u00b7gen", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schon einen Vorschmack hier. Ach GOTT! la\u00df doch bey", "tokens": ["Schon", "ei\u00b7nen", "Vor\u00b7schmack", "hier", ".", "Ach", "GoTT", "!", "la\u00df", "doch", "bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$.", "NN", "NE", "$.", "VVIMP", "ADV", "APPR"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Zeiten", "tokens": ["Zei\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Mich von dem Dienst der morschen Eitelkeiten,", "tokens": ["Mich", "von", "dem", "Dienst", "der", "mor\u00b7schen", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Voll s\u00fcsser Hoffnungs-Gluht, entfernen!", "tokens": ["Voll", "s\u00fcs\u00b7ser", "Hoff\u00b7nungs\u00b7Gluht", ",", "ent\u00b7fer\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ach! la\u00df mich auf der Welt", "tokens": ["Ach", "!", "la\u00df", "mich", "auf", "der", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "An das, was Dir gef\u00e4llt,", "tokens": ["An", "das", ",", "was", "Dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "An Deiner Wunder Heer, die Du uns vorgestellt,", "tokens": ["An", "Dei\u00b7ner", "Wun\u00b7der", "Heer", ",", "die", "Du", "uns", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mich inniglich vergn\u00fcgen lernen!", "tokens": ["Mich", "in\u00b7nig\u00b7lich", "ver\u00b7gn\u00fc\u00b7gen", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ach! la\u00df mich Deine Weg\u2019 und Deine Wunder kennen,", "tokens": ["Ach", "!", "la\u00df", "mich", "Dei\u00b7ne", "Weg'", "und", "Dei\u00b7ne", "Wun\u00b7der", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und stets in froher Danckbarkeit,", "tokens": ["Und", "stets", "in", "fro\u00b7her", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Derselben Vollenkommenheit,", "tokens": ["Der\u00b7sel\u00b7ben", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und was Du uns schon hier so gn\u00e4dig wollen g\u00f6nnen;", "tokens": ["Und", "was", "Du", "uns", "schon", "hier", "so", "gn\u00e4\u00b7dig", "wol\u00b7len", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "ADV", "ADV", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Betrachten, und in allen Sch\u00e4tzen", "tokens": ["Be\u00b7trach\u00b7ten", ",", "und", "in", "al\u00b7len", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Erden, Dir zur Ehr, mich inniglich ergetzen!", "tokens": ["Der", "Er\u00b7den", ",", "Dir", "zur", "Ehr", ",", "mich", "in\u00b7nig\u00b7lich", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPRART", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So machen wir schon in der Zeit", "tokens": ["So", "ma\u00b7chen", "wir", "schon", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Uns, HERR, zu Deinen Ruhm, zur Seeligkeit bereit.", "tokens": ["Uns", ",", "HeRR", ",", "zu", "Dei\u00b7nen", "Ruhm", ",", "zur", "See\u00b7lig\u00b7keit", "be\u00b7reit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}