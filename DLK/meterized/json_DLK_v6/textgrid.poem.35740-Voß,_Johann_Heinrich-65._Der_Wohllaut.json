{"textgrid.poem.35740": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "65. Der Wohllaut", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Perlenhell von Taugefunkel", "tokens": ["Per\u00b7len\u00b7hell", "von", "Tau\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand dein ganzer Mai, o Flora;", "tokens": ["Stand", "dein", "gan\u00b7zer", "Mai", ",", "o", "Flo\u00b7ra", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell wie Purpur, sank Aurora", "tokens": ["Hell", "wie", "Pur\u00b7pur", ",", "sank", "Au\u00b7ro\u00b7ra"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft in Dunkel", "tokens": ["Sanft", "in", "Dun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zum Ocean.", "tokens": ["Zum", "O\u00b7cean", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Lunas Scheib' in klarer R\u00fcndung", "tokens": ["Lu\u00b7nas", "Scheib'", "in", "kla\u00b7rer", "R\u00fcn\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wallt' aus Silberduft zum \u00c4ther;", "tokens": ["Wallt'", "aus", "Sil\u00b7ber\u00b7duft", "zum", "\u00c4\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wir schauten, voll erh\u00f6hter", "tokens": ["Und", "wir", "schau\u00b7ten", ",", "voll", "er\u00b7h\u00f6h\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Vorempfindung,", "tokens": ["Vor\u00b7emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die stille Bahn.", "tokens": ["Die", "stil\u00b7le", "Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Leis aus lichter Wolke hallen", "tokens": ["Leis", "aus", "lich\u00b7ter", "Wol\u00b7ke", "hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00f6rten wir's, als s\u00e4ng' Amphion,", "tokens": ["H\u00f6r\u00b7ten", "wir's", ",", "als", "s\u00e4ng'", "Am\u00b7phi\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "NE", "NE", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wie, wann l\u00e4chelt Zeus Kronion,", "tokens": ["Wie", ",", "wann", "l\u00e4\u00b7chelt", "Zeus", "Kro\u00b7ni\u00b7on", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VVFIN", "NE", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lenze wallen", "tokens": ["Len\u00b7ze", "wal\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Auf Thal' und H\u00f6h'n.", "tokens": ["Auf", "Thal'", "und", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "NN", "KON", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ihr Romanen, ihr Achaier,", "tokens": ["Ihr", "Ro\u00b7ma\u00b7nen", ",", "ihr", "A\u00b7chai\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr im Glanze sangt harmonisch;", "tokens": ["Ihr", "im", "Glan\u00b7ze", "sangt", "har\u00b7mo\u00b7nisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00e4onidisch und maronisch", "tokens": ["M\u00e4o\u00b7ni\u00b7disch", "und", "ma\u00b7ro\u00b7nisch"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Klang die Leier", "tokens": ["Klang", "die", "Lei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "In lindem Wehn.", "tokens": ["In", "lin\u00b7dem", "Wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbheil! die alte Nacht veraltet", "tokens": ["\u00bb", "heil", "!", "die", "al\u00b7te", "Nacht", "ver\u00b7al\u00b7tet"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(rief's herab) in deutscher Wildnis!", "tokens": ["(", "rie\u00b7f's", "her\u00b7ab", ")", "in", "deut\u00b7scher", "Wild\u00b7nis", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald erbl\u00fcht, nach Hellas' Bildnis", "tokens": ["Bald", "er\u00b7bl\u00fcht", ",", "nach", "Hel\u00b7las'", "Bild\u00b7nis"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Umgestaltet,", "tokens": ["Um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die B\u00e1rbarflur!", "tokens": ["Die", "B\u00e1r\u00b7ba\u00b7rflur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Tr\u00e4ger Unzier Herrschaft endigt,", "tokens": ["Tr\u00e4\u00b7ger", "Un\u00b7zier", "Herr\u00b7schaft", "en\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch Apollon und Ly\u00e4os;", "tokens": ["Durch", "A\u00b7pol\u00b7lon", "und", "Ly\u00b7\u00e4\u00b7os", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Ferne schon am Nord-Rhip\u00e4os", "tokens": ["Fer\u00b7ne", "schon", "am", "Nord\u00b7R\u00b7hip\u00e4os"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wird geb\u00e4ndigt", "tokens": ["Wird", "ge\u00b7b\u00e4n\u00b7digt"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Der B\u00e4r und Ur!", "tokens": ["Der", "B\u00e4r", "und", "Ur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "F\u00fcr Gesetz und Ordnung f\u00fcgsam,", "tokens": ["F\u00fcr", "Ge\u00b7setz", "und", "Ord\u00b7nung", "f\u00fcg\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strebt der franke Geist nach Wahrheit;", "tokens": ["Strebt", "der", "fran\u00b7ke", "Geist", "nach", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Red' in holder Klarheit", "tokens": ["Und", "die", "Red'", "in", "hol\u00b7der", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hallet biegsam", "tokens": ["Hal\u00b7let", "bieg\u00b7sam"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Apollons Hall!", "tokens": ["A\u00b7pol\u00b7lons", "Hall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bald vereint sich Kraft und Sch\u00f6ne", "tokens": ["Bald", "ver\u00b7eint", "sich", "Kraft", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald mit Wohllaut Wohlbewegung;", "tokens": ["Bald", "mit", "Wohl\u00b7laut", "Wohl\u00b7be\u00b7we\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedem Schwung' und jeder Regung", "tokens": ["Je\u00b7dem", "Schwung'", "und", "je\u00b7der", "Re\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Folgt der T\u00f6ne", "tokens": ["Folgt", "der", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Geme\u00dfner Fall!", "tokens": ["Ge\u00b7me\u00df\u00b7ner", "Fall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Bald durch Deutschland t\u00f6nt gewaltig", "tokens": ["Bald", "durch", "Deutschland", "t\u00f6nt", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVFIN", "ADJD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Deine Melodie, Homeros!", "tokens": ["Dei\u00b7ne", "Me\u00b7lo\u00b7die", ",", "Ho\u00b7me\u00b7ros", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bald nicht sparsam bl\u00fcht ein Heros,", "tokens": ["Bald", "nicht", "spar\u00b7sam", "bl\u00fcht", "ein", "He\u00b7ros", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mannigfaltig", "tokens": ["Man\u00b7nig\u00b7fal\u00b7tig"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "An Red' und That!", "tokens": ["An", "Red'", "und", "That", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Deutschen Ton' horcht endlich gerne", "tokens": ["Deut\u00b7schen", "Ton'", "horcht", "end\u00b7lich", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4lsch' und Frank, Ver\u00e4chter weiland!", "tokens": ["W\u00e4l\u00b7sch'", "und", "Frank", ",", "Ver\u00b7\u00e4ch\u00b7ter", "wei\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Auch das stolze Britteneiland", "tokens": ["Auch", "das", "stol\u00b7ze", "Brit\u00b7ten\u00b7ei\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Horch' und lerne", "tokens": ["Horch'", "und", "ler\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Hellenenpfad!", "tokens": ["Hel\u00b7len\u00b7en\u00b7pfad", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wie des Wohlklangs Kind Jona", "tokens": ["Wie", "des", "Wohl\u00b7klangs", "Kind", "Jo\u00b7na"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kraft und Anmut einst get\u00f6net:", "tokens": ["Kraft", "und", "An\u00b7mut", "einst", "ge\u00b7t\u00f6\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also sing' itzt unverh\u00f6hnet,", "tokens": ["Al\u00b7so", "sing'", "itzt", "un\u00b7ver\u00b7h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O Teutona,", "tokens": ["O", "Teu\u00b7to\u00b7na", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dem Helikon!\u00ab \u2013", "tokens": ["Dem", "He\u00b7li\u00b7kon", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Lehr', Apollon, lehr' uns Lieder;", "tokens": ["Lehr'", ",", "A\u00b7pol\u00b7lon", ",", "lehr'", "uns", "Lie\u00b7der", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deines Wohlklangs ward uns wenig!", "tokens": ["Dei\u00b7nes", "Wohl\u00b7klangs", "ward", "uns", "we\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Flehten wir; und siebent\u00f6nig", "tokens": ["Fleh\u00b7ten", "wir", ";", "und", "sie\u00b7ben\u00b7t\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schwebte nieder", "tokens": ["Schweb\u00b7te", "nie\u00b7der"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKVZ"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ein Barbiton.", "tokens": ["Ein", "Bar\u00b7bi\u00b7ton", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}