{"textgrid.poem.46727": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[komm in den Wald, wenn du willst bei uns sein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm in den Wald, wenn du willst bei uns sein,", "tokens": ["Komm", "in", "den", "Wald", ",", "wenn", "du", "willst", "bei", "uns", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00fchl' unsern Odem dir entgegenwehn!", "tokens": ["F\u00fchl'", "un\u00b7sern", "O\u00b7dem", "dir", "ent\u00b7ge\u00b7gen\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Komm in den Wald! wir wollen sein bei dir;", "tokens": ["Komm", "in", "den", "Wald", "!", "wir", "wol\u00b7len", "sein", "bei", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "APPR", "PPER", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "H\u00f6r' unser Fl\u00fcstern dir entgegenflehn!", "tokens": ["H\u00f6r'", "un\u00b7ser", "Fl\u00fcs\u00b7tern", "dir", "ent\u00b7ge\u00b7gen\u00b7flehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir wollen dir in jedem Hauch der Luft,", "tokens": ["Wir", "wol\u00b7len", "dir", "in", "je\u00b7dem", "Hauch", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit jedem Strahl der Sonn' entgegengehn.", "tokens": ["Mit", "je\u00b7dem", "Strahl", "der", "Sonn'", "ent\u00b7ge\u00b7gen\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O siehst du nicht die Blumen im Geb\u00fcsch,", "tokens": ["O", "siehst", "du", "nicht", "die", "Blu\u00b7men", "im", "Ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die dir die feuchten Blick' entgegendrehn?", "tokens": ["Die", "dir", "die", "feuch\u00b7ten", "Blick'", "ent\u00b7ge\u00b7gen\u00b7drehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir werden, wo du siehst im Quell dein Bild,", "tokens": ["Wir", "wer\u00b7den", ",", "wo", "du", "siehst", "im", "Quell", "dein", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus Himmelswolken dir entgegensehn.", "tokens": ["Aus", "Him\u00b7mels\u00b7wol\u00b7ken", "dir", "ent\u00b7ge\u00b7gen\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und denke nicht, da\u00df starr die Felsen dort", "tokens": ["Und", "den\u00b7ke", "nicht", ",", "da\u00df", "starr", "die", "Fel\u00b7sen", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als Scheidewand der Lieb' entgegenstehn,", "tokens": ["Als", "Schei\u00b7de\u00b7wand", "der", "Lieb'", "ent\u00b7ge\u00b7gen\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sie werden, frag' in unserm Namen nur,", "tokens": ["Sie", "wer\u00b7den", ",", "frag'", "in", "un\u00b7serm", "Na\u00b7men", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In unserm Namen dir entgegenen.", "tokens": ["In", "un\u00b7serm", "Na\u00b7men", "dir", "ent\u00b7ge\u00b7ge\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}