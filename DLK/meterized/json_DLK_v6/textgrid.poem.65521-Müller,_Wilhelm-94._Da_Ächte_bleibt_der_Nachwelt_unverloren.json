{"textgrid.poem.65521": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "94. Da \u00c4chte bleibt der Nachwelt unverloren", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie liegt der edle Kern so lange in Spreu und Plunder tief versteckt!", "tokens": ["Wie", "liegt", "der", "ed\u00b7le", "Kern", "so", "lan\u00b7ge", "in", "Spreu", "und", "Plun\u00b7der", "tief", "ver\u00b7steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So bleibt auch oft das Werk des Meisters vom leichten Tand der Zeit bedeckt.", "tokens": ["So", "bleibt", "auch", "oft", "das", "Werk", "des", "Meis\u00b7ters", "vom", "leich\u00b7ten", "Tand", "der", "Zeit", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Mag dr\u00fcber hin das Unkraut wuchern, der starke Keim bricht doch hervor,", "tokens": ["Mag", "dr\u00fc\u00b7ber", "hin", "das", "Un\u00b7kraut", "wu\u00b7chern", ",", "der", "star\u00b7ke", "Keim", "bricht", "doch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ADV", "ART", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und streckt zu einem Wunderbaume sich vor der Nachwelt schnell empor.", "tokens": ["Und", "streckt", "zu", "ei\u00b7nem", "Wun\u00b7der\u00b7bau\u00b7me", "sich", "vor", "der", "Nach\u00b7welt", "schnell", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}