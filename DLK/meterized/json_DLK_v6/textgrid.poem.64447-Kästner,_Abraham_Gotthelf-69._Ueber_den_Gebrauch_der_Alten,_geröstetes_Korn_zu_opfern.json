{"textgrid.poem.64447": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "69. Ueber den Gebrauch der Alten, ger\u00f6stetes Korn zu opfern", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Himmel wand auf Roms Alt\u00e4ren", "tokens": ["Gen", "Him\u00b7mel", "wand", "auf", "Roms", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich heil'ger Dampf gesengter Aehren;", "tokens": ["Sich", "heil'\u00b7ger", "Dampf", "ge\u00b7seng\u00b7ter", "A\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Opfer kennt noch manches Land:", "tokens": ["Das", "Op\u00b7fer", "kennt", "noch", "man\u00b7ches", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stutzer, der Coquetten G\u00f6tzen,", "tokens": ["Der", "Stut\u00b7zer", ",", "der", "Co\u00b7quet\u00b7ten", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem M\u00fc\u00dfiggange, den Geschw\u00e4tzen,", "tokens": ["Dem", "M\u00fc\u00b7\u00dfig\u00b7gan\u00b7ge", ",", "den", "Ge\u00b7schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird auch bey uns Caffee gebrannt.", "tokens": ["Wird", "auch", "bey", "uns", "Caf\u00b7fee", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}