{"textgrid.poem.42983": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Abgesehen von der Profitl\u00fcge", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kurzen Beine der L\u00fcge sind", "tokens": ["Die", "kur\u00b7zen", "Bei\u00b7ne", "der", "L\u00fc\u00b7ge", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch nur etwas Relatives.", "tokens": ["Auch", "nur", "et\u00b7was", "Re\u00b7la\u00b7ti\u00b7ves", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Segler kreuzend gegen Wind", "tokens": ["Ein", "Seg\u00b7ler", "kreu\u00b7zend", "ge\u00b7gen", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist immer etwas Schiefes.", "tokens": ["Ist", "im\u00b7mer", "et\u00b7was", "Schie\u00b7fes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob sie aus Anstand, aus Mitleid gibt,", "tokens": ["Ob", "sie", "aus", "An\u00b7stand", ",", "aus", "Mit\u00b7leid", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sich hinter der Kunst will sch\u00fctzen,", "tokens": ["Sich", "hin\u00b7ter", "der", "Kunst", "will", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn sie nicht innerst sich selber liebt,", "tokens": ["Wenn", "sie", "nicht", "in\u00b7nerst", "sich", "sel\u00b7ber", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wird L\u00fcge niemandem n\u00fctzen.", "tokens": ["Wird", "L\u00fc\u00b7ge", "nie\u00b7man\u00b7dem", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIS", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es gibt eine L\u00fcge, politisch und k\u00fchn,", "tokens": ["Es", "gibt", "ei\u00b7ne", "L\u00fc\u00b7ge", ",", "po\u00b7li\u00b7tisch", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und die ist auch noch zu r\u00fcgen.", "tokens": ["Und", "die", "ist", "auch", "noch", "zu", "r\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich meine: Wir sollten uns alle bem\u00fchn,", "tokens": ["Ich", "mei\u00b7ne", ":", "Wir", "soll\u00b7ten", "uns", "al\u00b7le", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "M\u00f6glichst wenig zu l\u00fcgen.", "tokens": ["M\u00f6g\u00b7lichst", "we\u00b7nig", "zu", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}