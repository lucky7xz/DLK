{"dta.poem.2522": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff den  72.  Ejusdem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es schlo\u00df das deutsche Volck im Alterthum der Zeiten/", "tokens": ["Es", "schlo\u00df", "das", "deut\u00b7sche", "Volck", "im", "Al\u00b7ter\u00b7thum", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df etwas Heiliges in tieffen W\u00e4ldern sey:", "tokens": ["Da\u00df", "et\u00b7was", "Hei\u00b7li\u00b7ges", "in", "tief\u00b7fen", "W\u00e4l\u00b7dern", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie pflegten manchem Baum fast Opffer zubereiten/", "tokens": ["Sie", "pfleg\u00b7ten", "man\u00b7chem", "Baum", "fast", "Opf\u00b7fer", "zu\u00b7be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der geweyhte Stamm war von Verw\u00fcstung frey", "tokens": ["Und", "der", "ge\u00b7weyh\u00b7te", "Stamm", "war", "von", "Ver\u00b7w\u00fcs\u00b7tung", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie fielen auff die Knie/ die G\u00f6tter da zu ehren/", "tokens": ["Sie", "fie\u00b7len", "auff", "die", "Knie", "/", "die", "G\u00f6t\u00b7ter", "da", "zu", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo selbst die gr\u00fcne Nacht ein gr\u00f6sser Schrecken gab:", "tokens": ["Wo", "selbst", "die", "gr\u00fc\u00b7ne", "Nacht", "ein", "gr\u00f6s\u00b7ser", "Schre\u00b7cken", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein unbesonnen Beil dorfft einen Ast versehren/", "tokens": ["Kein", "un\u00b7be\u00b7son\u00b7nen", "Beil", "dorfft", "ei\u00b7nen", "Ast", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein sterblich Menschen Arm die Reiser brechen ab.", "tokens": ["Kein", "sterb\u00b7lich", "Men\u00b7schen", "Arm", "die", "Rei\u00b7ser", "bre\u00b7chen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In andrer Ehren-Furcht und heiligerm Beginnen", "tokens": ["In", "an\u00b7drer", "Eh\u00b7ren\u00b7Furcht", "und", "hei\u00b7li\u00b7germ", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wil ich durch einen Wald der hohen W\u00fcrden gehn/", "tokens": ["Wil", "ich", "durch", "ei\u00b7nen", "Wald", "der", "ho\u00b7hen", "W\u00fcr\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Wo sich der Ceder-Baum bi\u00df an der Wolcken Zinnen", "tokens": ["Wo", "sich", "der", "Ce\u00b7der\u00b7Baum", "bi\u00df", "an", "der", "Wol\u00b7cken", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Siegpr\u00e4chtig hebt empor/ und kan bey Sternen stehn.", "tokens": ["Sieg\u00b7pr\u00e4ch\u00b7tig", "hebt", "em\u00b7por", "/", "und", "kan", "bey", "Ster\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$(", "KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hochwerthester Patron/ indem sein Tag erschienen/", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7ter", "Pat\u00b7ron", "/", "in\u00b7dem", "sein", "Tag", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Und sich ob dessen Heil die gantze Stadt ergetzt/", "tokens": ["Und", "sich", "ob", "des\u00b7sen", "Heil", "die", "gant\u00b7ze", "Stadt", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOUS", "PDS", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat meine Poesie sich sollen di\u00df erk\u00fchnen/", "tokens": ["Hat", "mei\u00b7ne", "Poe\u00b7sie", "sich", "sol\u00b7len", "di\u00df", "er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "VMFIN", "PDS", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da\u00df zum Ged\u00e4chtn\u00fc\u00df sie JHM eine Ceder setzt.", "tokens": ["Da\u00df", "zum", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "sie", "JhM", "ei\u00b7ne", "Ce\u00b7der", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auch gar de\u00df H\u00f6chsten Mund wil die Regenten gleichen", "tokens": ["Auch", "gar", "de\u00df", "H\u00f6chs\u00b7ten", "Mund", "wil", "die", "Re\u00b7gen\u00b7ten", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Mit diesem edlen Baum/ und seiner W\u00fcrdigkeit;", "tokens": ["Mit", "die\u00b7sem", "ed\u00b7len", "Baum", "/", "und", "sei\u00b7ner", "W\u00fcr\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kan das hohe Ampt den wahren Ruhm erreichen/", "tokens": ["So", "kan", "das", "ho\u00b7he", "Ampt", "den", "wah\u00b7ren", "Ruhm", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den der Verdienste Krafft schon l\u00e4ngst hat zubereit.", "tokens": ["Den", "der", "Ver\u00b7diens\u00b7te", "Krafft", "schon", "l\u00e4ngst", "hat", "zu\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und die in seinem Weg\u2019 und Wandel einher gehen/", "tokens": ["Und", "die", "in", "sei\u00b7nem", "Weg'", "und", "Wan\u00b7del", "ein\u00b7her", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die sollen fort f\u00fcr fort im sch\u00f6nsten Wachsthum bl\u00fchn/", "tokens": ["Die", "sol\u00b7len", "fort", "f\u00fcr", "fort", "im", "sch\u00f6ns\u00b7ten", "Wach\u00b7sthum", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PTKVZ", "APPR", "PTKVZ", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wie in Libanon die hohen Cedern stehen/", "tokens": ["Und", "wie", "in", "Li\u00b7ba\u00b7non", "die", "ho\u00b7hen", "Ce\u00b7dern", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die aller Menschen Aug und Hertzen an sich ziehn.", "tokens": ["Die", "al\u00b7ler", "Men\u00b7schen", "Aug", "und", "Hert\u00b7zen", "an", "sich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gewi\u00df der Segen hat hier reichlich eingetroffen/", "tokens": ["Ge\u00b7wi\u00df", "der", "Se\u00b7gen", "hat", "hier", "reich\u00b7lich", "ein\u00b7ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und Jhn/ ", "tokens": ["Und", "Jhn", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Auff seinen Schultern ruht der B\u00fcrger Heil und Hoffen/", "tokens": ["Auff", "sei\u00b7nen", "Schul\u00b7tern", "ruht", "der", "B\u00fcr\u00b7ger", "Heil", "und", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ist/ was Titus war/ die Liebe dieser Welt.", "tokens": ["Und", "ist", "/", "was", "Ti\u00b7tus", "war", "/", "die", "Lie\u00b7be", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "PWS", "NE", "VAFIN", "$(", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja wie der Cedern Baum/ an Sch\u00f6nheit hoch erhaben/", "tokens": ["Ja", "wie", "der", "Ce\u00b7dern", "Baum", "/", "an", "Sch\u00f6n\u00b7heit", "hoch", "er\u00b7ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ART", "NN", "NE", "$(", "APPR", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Aest\u2019 in sch\u00f6ner Reyh und holder Bl\u00fcthe zeigt/", "tokens": ["Die", "A\u00b7est'", "in", "sch\u00f6\u00b7ner", "Reyh", "und", "hol\u00b7der", "Bl\u00fc\u00b7the", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "So gl\u00e4ntzt auch sein Gem\u00fcth von solchen grossen Gaben/", "tokens": ["So", "gl\u00e4ntzt", "auch", "sein", "Ge\u00b7m\u00fcth", "von", "sol\u00b7chen", "gros\u00b7sen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df selbst der Erden Gott/ der ", "tokens": ["Da\u00df", "selbst", "der", "Er\u00b7den", "Gott", "/", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Und wie die Zweige sich in rechter Gleichheit breiten/", "tokens": ["Und", "wie", "die", "Zwei\u00b7ge", "sich", "in", "rech\u00b7ter", "Gleich\u00b7heit", "brei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word"