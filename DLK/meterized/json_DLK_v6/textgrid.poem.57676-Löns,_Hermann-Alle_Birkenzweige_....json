{"textgrid.poem.57676": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Alle Birkenzweige ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Birkenzweige schwenken", "tokens": ["Al\u00b7le", "Bir\u00b7ken\u00b7zwei\u00b7ge", "schwen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6hlich jetzt ihr Maiengr\u00fcn,", "tokens": ["Fr\u00f6h\u00b7lich", "jetzt", "ihr", "Mai\u00b7en\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in vollen gelben Dolden", "tokens": ["Und", "in", "vol\u00b7len", "gel\u00b7ben", "Dol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Schl\u00fcsselblumen bl\u00fchn.", "tokens": ["Al\u00b7le", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singt das Lied von goldenen Blumen,", "tokens": ["Singt", "das", "Lied", "von", "gol\u00b7de\u00b7nen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Singt das Lied vom jungen Gr\u00fcn.", "tokens": ["Singt", "das", "Lied", "vom", "jun\u00b7gen", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt das Lied von unserer Liebe", "tokens": ["Singt", "das", "Lied", "von", "un\u00b7se\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und von unserer Herzen Bl\u00fchn.", "tokens": ["Und", "von", "un\u00b7se\u00b7rer", "Her\u00b7zen", "Bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Unser Lied, das ich gefunden,", "tokens": ["Un\u00b7ser", "Lied", ",", "das", "ich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Lied, das in mir klang,", "tokens": ["Un\u00b7ser", "Lied", ",", "das", "in", "mir", "klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Sonne deiner Liebe", "tokens": ["Als", "die", "Son\u00b7ne", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das kalte Herz bezwang.", "tokens": ["Mir", "das", "kal\u00b7te", "Herz", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}