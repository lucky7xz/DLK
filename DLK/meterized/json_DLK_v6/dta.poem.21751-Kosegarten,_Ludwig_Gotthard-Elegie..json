{"dta.poem.21751": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freundinn, der Fr&#252;hling ist da! Ich sah ihn in", "tokens": ["Freun\u00b7dinn", ",", "der", "Fr", "&#252;", "hling", "ist", "da", "!", "Ich", "sah", "ihn", "in"], "token_info": ["word", "punct", "word", "word", "XML_entity", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$(", "VVFIN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "APPR"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "r\u00f6thlichen Wolken", "tokens": ["r\u00f6th\u00b7li\u00b7chen", "Wol\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00dcber den blumigen H\u00f6h'n schweben. Die H\u00f6-", "tokens": ["\u00dc\u00b7ber", "den", "blu\u00b7mi\u00b7gen", "H\u00f6h'n", "schwe\u00b7ben", ".", "Die", "H\u00f6"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$.", "ART", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sah ich ihn wandeln tanzenden Schrittes. Ein buh-", "tokens": ["Sah", "ich", "ihn", "wan\u00b7deln", "tan\u00b7zen\u00b7den", "Schrit\u00b7tes", ".", "Ein", "buh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "ADJA", "NN", "$.", "ART", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Haucht' um den rosigen Hals sein hyacinthenes", "tokens": ["Haucht'", "um", "den", "ro\u00b7si\u00b7gen", "Hals", "sein", "hya\u00b7cin\u00b7the\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Haar.", "tokens": ["Haar", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Gr\u00e4schen und Bl\u00fcmchen entsprossen des Schreiten-", "tokens": ["Gr\u00e4sc\u00b7hen", "und", "Bl\u00fcm\u00b7chen", "ent\u00b7spros\u00b7sen", "des", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Schnee und Reif zerschmolz seinem erlauenden", "tokens": ["Schnee", "und", "Reif", "zer\u00b7schmolz", "sei\u00b7nem", "er\u00b7lau\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "PPOSAT", "ADJA"], "meter": "+-+-+---++-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Hanch.", "tokens": ["Hanch", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Silberstiebende B\u00e4chlein durchrieseln schon schwat-", "tokens": ["Sil\u00b7ber\u00b7stie\u00b7ben\u00b7de", "B\u00e4ch\u00b7lein", "durc\u00b7hrie\u00b7seln", "schon", "schwat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Blitzen im Sonnenstrahl, spiegeln die Sonne", "tokens": ["Blit\u00b7zen", "im", "Son\u00b7nen\u00b7strahl", ",", "spie\u00b7geln", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Rascher schon zirkelt das Blut, beschleuniget schla-", "tokens": ["Ra\u00b7scher", "schon", "zir\u00b7kelt", "das", "Blut", ",", "be\u00b7schleu\u00b7ni\u00b7get", "schla"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Freundlicher lacht uns die Welt, heitrer das", "tokens": ["Freund\u00b7li\u00b7cher", "lacht", "uns", "die", "Welt", ",", "hei\u00b7trer", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADJA", "ART"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Leben uns an.", "tokens": ["Le\u00b7ben", "uns", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Denn der Fr\u00fchling beginnt. Wer wollte des keh-", "tokens": ["Denn", "der", "Fr\u00fch\u00b7ling", "be\u00b7ginnt", ".", "Wer", "woll\u00b7te", "des", "keh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PWS", "VMFIN", "ART", "TRUNC"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Sich nicht erfreuen, verj\u00fcngt mit der verj\u00fcngten", "tokens": ["Sich", "nicht", "er\u00b7freu\u00b7en", ",", "ver\u00b7j\u00fcngt", "mit", "der", "ver\u00b7j\u00fcng\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "VVINF", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Nicht die ergr\u00fcnende Flur beschreiten erweiterten", "tokens": ["Nicht", "die", "er\u00b7gr\u00fc\u00b7nen\u00b7de", "Flur", "be\u00b7schrei\u00b7ten", "er\u00b7wei\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Herzens,", "tokens": ["Her\u00b7zens", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Nicht mit Gesang und Tanz gr\u00fcssen den keh-", "tokens": ["Nicht", "mit", "Ge\u00b7sang", "und", "Tanz", "gr\u00fcs\u00b7sen", "den", "keh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ida, der Lenz beginnt. Komm, Tochter sanfterer", "tokens": ["I\u00b7da", ",", "der", "Lenz", "be\u00b7ginnt", ".", "Komm", ",", "Toch\u00b7ter", "sanf\u00b7te\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$.", "VVFIN", "$,", "NN", "ADJD"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Freuden,", "tokens": ["Freu\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Komm' mit mir in das Feld. Siehe, der Frost", "tokens": ["Komm'", "mit", "mir", "in", "das", "Feld", ".", "Sie\u00b7he", ",", "der", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$.", "VVIMP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "ist dahin,", "tokens": ["ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Und der Schnee ist zerschmolzen. Es r\u00f6theln die", "tokens": ["Und", "der", "Schnee", "ist", "zer\u00b7schmol\u00b7zen", ".", "Es", "r\u00f6\u00b7theln", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$.", "PPER", "VVFIN", "ART"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.27": {"text": "\u00c4ste der Haseln,", "tokens": ["\u00c4s\u00b7te", "der", "Ha\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Fr\u00f6hlicher sieht des Geb\u00fcrgs alterergrauetes", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "sieht", "des", "Ge\u00b7b\u00fcrgs", "al\u00b7ter\u00b7er\u00b7grau\u00b7e\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA"], "meter": "+--+---+--+--", "measure": "dactylic.di.plus"}, "line.29": {"text": "Moos.", "tokens": ["Moos", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Was den Schlaf der Erstarrung geschlafen, in Ta-", "tokens": ["Was", "den", "Schlaf", "der", "Er\u00b7star\u00b7rung", "ge\u00b7schla\u00b7fen", ",", "in", "Ta"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVPP", "$,", "APPR", "TRUNC"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.31": {"text": "F\u00fchlt sich ins Leben geweckt, dr\u00e4ngt an die", "tokens": ["F\u00fchlt", "sich", "ins", "Le\u00b7ben", "ge\u00b7weckt", ",", "dr\u00e4ngt", "an", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "VVPP", "$,", "VVFIN", "APPR", "ART"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "W\u00e4rm' und das Licht.", "tokens": ["W\u00e4rm'", "und", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Schau, es wimmelt im Sumpf. Es beseelt sich die", "tokens": ["Schau", ",", "es", "wim\u00b7melt", "im", "Sumpf", ".", "Es", "be\u00b7seelt", "sich", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$.", "PPER", "VVFIN", "PRF", "ART"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Scholle. Auf schlanken", "tokens": ["Schol\u00b7le", ".", "Auf", "schlan\u00b7ken"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "APPR", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Halmen wieget sich sanft schillernder K\u00e4fer Ge-", "tokens": ["Hal\u00b7men", "wie\u00b7get", "sich", "sanft", "schil\u00b7lern\u00b7der", "K\u00e4\u00b7fer", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADJD", "ADJA", "NN", "TRUNC"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Hoch in den L\u00fcften erschallt das Lied der frohlok-", "tokens": ["Hoch", "in", "den", "L\u00fcf\u00b7ten", "er\u00b7schallt", "das", "Lied", "der", "froh\u00b7lo\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Mit des werdenden Tags spriessendem blasse-", "tokens": ["Mit", "des", "wer\u00b7den\u00b7den", "Tags", "spries\u00b7sen\u00b7dem", "blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Schwingt sich die S\u00e4ngerinn himmelempor, und", "tokens": ["Schwingt", "sich", "die", "S\u00e4n\u00b7ge\u00b7rinn", "him\u00b7mel\u00b7em\u00b7por", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "KON"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.39": {"text": "singt, bis die D\u00e4mmrung", "tokens": ["singt", ",", "bis", "die", "D\u00e4mm\u00b7rung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Berg' und Thale verh\u00fcllt, fr\u00f6hlich das fr\u00f6hliche", "tokens": ["Ber\u00b7g'", "und", "Tha\u00b7le", "ver\u00b7h\u00fcllt", ",", "fr\u00f6h\u00b7lich", "das", "fr\u00f6h\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVPP", "$,", "ADJD", "ART", "ADJA"], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.41": {"text": "Lied.", "tokens": ["Lied", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Komm, Holdselige dann, der Natur zartf\u00fchlende", "tokens": ["Komm", ",", "Hold\u00b7se\u00b7li\u00b7ge", "dann", ",", "der", "Na\u00b7tur", "zart\u00b7f\u00fch\u00b7len\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "ADV", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Freundinn,", "tokens": ["Freun\u00b7dinn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Komm mit mir ins Feld. Lass an des rieseln-", "tokens": ["Komm", "mit", "mir", "ins", "Feld", ".", "Lass", "an", "des", "rie\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "NE", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Saum uns lagern. Ihn stickt die tausendbl\u00e4ttrige", "tokens": ["Saum", "uns", "la\u00b7gern", ".", "Ihn", "stickt", "die", "tau\u00b7send\u00b7bl\u00e4tt\u00b7ri\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.46": {"text": "Bellis,", "tokens": ["Bel\u00b7lis", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.47": {"text": "Welche das werdende Jahr, welche das schei-", "tokens": ["Wel\u00b7che", "das", "wer\u00b7den\u00b7de", "Jahr", ",", "wel\u00b7che", "das", "schei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.48": {"text": "Horch' es locket die bl\u00f6dere Sie der fl\u00f6tende", "tokens": ["Horch'", "es", "lo\u00b7cket", "die", "bl\u00f6\u00b7de\u00b7re", "Sie", "der", "fl\u00f6\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "PPER", "ART", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.49": {"text": "Sprosser.", "tokens": ["Spros\u00b7ser", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.50": {"text": "Schau, im Haselgeb\u00fcsch polstert die Amsel ihr", "tokens": ["Schau", ",", "im", "Ha\u00b7sel\u00b7ge\u00b7b\u00fcsch", "pols\u00b7tert", "die", "Am\u00b7sel", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Nest.", "tokens": ["Nest", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.52": {"text": "Hoch auf dem Halmdach dr\u00fcben sitzt neben dem", "tokens": ["Hoch", "auf", "dem", "Halm\u00b7dach", "dr\u00fc\u00b7ben", "sitzt", "ne\u00b7ben", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Tauber das T\u00e4ubchen,", "tokens": ["Tau\u00b7ber", "das", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Girret sein z\u00e4rtliches Lied, liebeverlohren, ihr", "tokens": ["Gir\u00b7ret", "sein", "z\u00e4rt\u00b7li\u00b7ches", "Lied", ",", "lie\u00b7be\u00b7ver\u00b7loh\u00b7ren", ",", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "PPOSAT"], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.55": {"text": "vor \u2014", "tokens": ["vor"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.56": {"text": "Lenz und Liebe f\u00fcrwahr, gebarst du als Zwillings-", "tokens": ["Lenz", "und", "Lie\u00b7be", "f\u00fcr\u00b7wahr", ",", "ge\u00b7barst", "du", "als", "Zwil\u00b7lings"], "token_info": ["word", "word", "word", "