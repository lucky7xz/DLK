{"textgrid.poem.49245": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird die Welt doch durch und durch verkehret!", "tokens": ["Wie", "wird", "die", "Welt", "doch", "durch", "und", "durch", "ver\u00b7keh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "APPR", "KON", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier hat ein Koch im Grabe seine Ruh,", "tokens": ["Hier", "hat", "ein", "Koch", "im", "Gra\u00b7be", "sei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mancherlei von Speisen richte zu;", "tokens": ["Der", "man\u00b7cher\u00b7lei", "von", "Spei\u00b7sen", "rich\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt haben ihn die W\u00fcrme roh verzehret.", "tokens": ["Jetzt", "ha\u00b7ben", "ihn", "die", "W\u00fcr\u00b7me", "roh", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}