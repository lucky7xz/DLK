, "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201eihm, ihm, du Vielbegabter,", "tokens": ["\u201e", "ihm", ",", "ihm", ",", "du", "Viel\u00b7be\u00b7gab\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eentflammst du, ihm allein!", "tokens": ["\u201e", "ent\u00b7flammst", "du", ",", "ihm", "al\u00b7lein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eihn meint dein heisses Schmachten!", "tokens": ["\u201e", "ihn", "meint", "dein", "heis\u00b7ses", "Schmach\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eihn ruft dein lautes Schreyn!", "tokens": ["\u201e", "ihn", "ruft", "dein", "lau\u00b7tes", "Schreyn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201edein Geist, den Ursprung ahnend,", "tokens": ["\u201e", "dein", "Geist", ",", "den", "Ur\u00b7sprung", "ah\u00b7nend", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201everachtend Erdenleid,", "tokens": ["\u201e", "ver\u00b7ach\u00b7tend", "Er\u00b7den\u00b7leid", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201everschm\u00e4hend Erdenwol