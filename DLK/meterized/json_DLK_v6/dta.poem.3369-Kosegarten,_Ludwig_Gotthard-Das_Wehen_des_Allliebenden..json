{"dta.poem.3369": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Wehen des Allliebenden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was st&#252;rmst du, meine Seele?", "tokens": ["Was", "st", "&#252;", "rmst", "du", ",", "mei\u00b7ne", "See\u00b7le", "?"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was flammst du, Ewige?", "tokens": ["Was", "flammst", "du", ",", "E\u00b7wi\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Woher dein durstig Sehnen?", "tokens": ["Wo\u00b7her", "dein", "durs\u00b7tig", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Woher dein folternd Weh?", "tokens": ["Wo\u00b7her", "dein", "fol\u00b7ternd", "Weh", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die grauen H\u00fcgel d\u00e4mmern,", "tokens": ["Die", "grau\u00b7en", "H\u00fc\u00b7gel", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Fluren duften Ruh \u2014", "tokens": ["Die", "Flu\u00b7ren", "duf\u00b7ten", "Ruh"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch unausl\u00f6schlich schmachtest,", "tokens": ["Doch", "un\u00b7aus\u00b7l\u00f6schlich", "schmach\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und rastlos schw\u00e4rmest du!", "tokens": ["Und", "rast\u00b7los", "schw\u00e4r\u00b7mest", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist Thatendurst dein Dursten;", "tokens": ["Ist", "Tha\u00b7ten\u00b7durst", "dein", "Durs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durst nach Unsterblichkeit?", "tokens": ["Durst", "nach", "U\u00b7nsterb\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf, g\u00fcrte dich, und handle!", "tokens": ["Auf", ",", "g\u00fcr\u00b7te", "dich", ",", "und", "hand\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bahn ist gross und weit!", "tokens": ["Die", "Bahn", "ist", "gross", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ists Durst nach schn\u00f6dem Golde?", "tokens": ["Ists", "Durst", "nach", "schn\u00f6\u00b7dem", "Gol\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vergib! Ich wusst' es, nie,", "tokens": ["Ver\u00b7gib", "!", "Ich", "wusst'", "es", ",", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nie beugtest du ", "tokens": ["Nie", "beug\u00b7test", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Unsterbliche, das Knie.", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", ",", "das", "Knie", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Bist du es, Auserw\u00e4hlte,", "tokens": ["Bist", "du", "es", ",", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dieses Schmachten meint?", "tokens": ["Die", "die\u00b7ses", "Schmach\u00b7ten", "meint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, die des Nachts dem Tr\u00e4umer,", "tokens": ["Du", ",", "die", "des", "Nachts", "dem", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Tr\u00e4umer Tags erscheint?", "tokens": ["Dem", "Tr\u00e4u\u00b7mer", "Tags", "er\u00b7scheint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein, nicht dem Staub' entflammet,", "tokens": ["Nein", ",", "nicht", "dem", "Staub'", "ent\u00b7flam\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was diese Leere f\u00fcllt.", "tokens": ["Was", "die\u00b7se", "Lee\u00b7re", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O, sagt mir an, o nennt mir,", "tokens": ["O", ",", "sagt", "mir", "an", ",", "o", "nennt", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was dieses Lechzen stillt! \u2014 \u2014", "tokens": ["Was", "die\u00b7ses", "Lech\u00b7zen", "stillt", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So rufend, sank ich nieder,", "tokens": ["So", "ru\u00b7fend", ",", "sank", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stiller ward das Still.", "tokens": ["Und", "stil\u00b7ler", "ward", "das", "Still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schwieg das Hayngeschmetter;", "tokens": ["Es", "schwieg", "das", "Hayn\u00b7ge\u00b7schmet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schwieg das Sumpfgeschrill.", "tokens": ["Es", "schwieg", "das", "Sumpf\u00b7ge\u00b7schrill", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Leishorchend hingegossen", "tokens": ["Leis\u00b7hor\u00b7chend", "hin\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lag ich am Blumenrand", "tokens": ["Lag", "ich", "am", "Blu\u00b7men\u00b7rand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Bachs. Des Halbmonds Sichel", "tokens": ["Des", "Bachs", ".", "Des", "Halb\u00b7monds", "Si\u00b7chel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erblinkt', erblasst', und schwand.", "tokens": ["Er\u00b7blinkt'", ",", "erb\u00b7lasst'", ",", "und", "schwand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dunkler ward das Dunkel", "tokens": ["Und", "dunk\u00b7ler", "ward", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein leises Flistern rann", "tokens": ["Ein", "lei\u00b7ses", "Flis\u00b7tern", "rann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Schilf und Busch und Saaten;", "tokens": ["Durch", "Schilf", "und", "Busch", "und", "Saa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich wehten Schauer an", "tokens": ["Mich", "weh\u00b7ten", "Schau\u00b7er", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es klang in meine Ohren,", "tokens": ["Es", "klang", "in", "mei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "S\u00fcss, s\u00fcss, wie Lautenklang \u2014", "tokens": ["S\u00fcss", ",", "s\u00fcss", ",", "wie", "Lau\u00b7ten\u00b7klang"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWAV", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Woher, woher, o Stimme,", "tokens": ["Wo\u00b7her", ",", "wo\u00b7her", ",", "o", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die mir das Herz durchdrang? \u2014", "tokens": ["Die", "mir", "das", "Herz", "durch\u00b7drang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201enicht ist es Durst nach Sch\u00e4tzen,", "tokens": ["\u201e", "nicht", "ist", "es", "Durst", "nach", "Sch\u00e4t\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201enicht nach dem Lob der Welt,", "tokens": ["\u201e", "nicht", "nach", "dem", "Lob", "der", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u201enicht nach der Auserw\u00e4hlten,", "tokens": ["\u201e", "nicht", "nach", "der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewas dir den Busen schwellt.", "tokens": ["\u201e", "was", "dir", "den", "Bu\u00b7sen", "schwellt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ees ist \u2014 Hinweg vom Auge", "tokens": ["\u201e", "es", "ist", "Hin\u00b7weg", "vom", "Au\u00b7ge"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "$(", "ADV", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "\u201edie Binde! Weg vom Ohr", "tokens": ["\u201e", "die", "Bin\u00b7de", "!", "Weg", "vom", "Ohr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$.", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201edie dicke Haut! \u2014 O Bl\u00f6der,", "tokens": ["\u201e", "die", "di\u00b7cke", "Haut", "!", "O", "Bl\u00f6\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "$(", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u201eschau um dich! schau empor!", "tokens": ["\u201e", "schau", "um", "dich", "!", "schau", "em\u00b7por", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "PPER", "$.", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u201esiehst du des Sp\u00e4troths Gluthen?", "tokens": ["\u201e", "siehst", "du", "des", "Sp\u00e4t\u00b7roths", "Glu\u00b7then", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edes Waldes D\u00fcstergr\u00fcn?", "tokens": ["\u201e", "des", "Wal\u00b7des", "D\u00fcs\u00b7ter\u00b7gr\u00fcn", "?"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edie Gr\u00e4ser und die Blumen,", "tokens": ["\u201e", "die", "Gr\u00e4\u00b7ser", "und", "die", "Blu\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edie duftend um dich bl\u00fchn?", "tokens": ["\u201e", "die", "duf\u00b7tend", "um", "dich", "bl\u00fchn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eh\u00f6rst du der Wachtel Schmettern?", "tokens": ["\u201e", "h\u00f6rst", "du", "der", "Wach\u00b7tel", "Schmet\u00b7tern", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "\u201esiehst du im Gras, im Moos", "tokens": ["\u201e", "siehst", "du", "im", "Gras", ",", "im", "Moos"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "\u201edas fr\u00f6hliche Gewimmel", "tokens": ["\u201e", "das", "fr\u00f6h\u00b7li\u00b7che", "Ge\u00b7wim\u00b7mel"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201eder Leben klein und gross?", "tokens": ["\u201e", "der", "Le\u00b7ben", "klein", "und", "gross", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201eo Bl\u00f6der, o Bet\u00e4ubter,", "tokens": ["\u201e", "o", "Bl\u00f6\u00b7der", ",", "o", "Be\u00b7t\u00e4ub\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edich formt' aus feinerm Thon", "tokens": ["\u201e", "dich", "formt'", "aus", "fei\u00b7nerm", "Thon"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edie Hand, die alles formte;", "tokens": ["\u201e", "die", "Hand", ",", "die", "al\u00b7les", "form\u00b7te", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eund du verkennst sie schon!", "tokens": ["\u201e"