{"dta.poem.3870": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund/ dessen Vater in der Grube  \n von einer Wand erschlagen worden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da die Morgen-Glocke schl\u00e4gt,", "tokens": ["Da", "die", "Mor\u00b7gen\u00b7Glo\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Da es kaum drey Uhr geschlagen,", "tokens": ["Da", "es", "kaum", "drey", "Uhr", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da es anf\u00e4ngt recht zu tagen,", "tokens": ["Da", "es", "an\u00b7f\u00e4ngt", "recht", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der muntre Hahn sich regt,", "tokens": ["Da", "der", "mun\u00b7tre", "Hahn", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Muste, o wer solt es dencken!", "tokens": ["Mus\u00b7te", ",", "o", "wer", "solt", "es", "den\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen Leib in Schacht versencken,", "tokens": ["Sei\u00b7nen", "Leib", "in", "Schacht", "ver\u00b7sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den dein Hertz als Vater liebt,", "tokens": ["Den", "dein", "Hertz", "als", "Va\u00b7ter", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dessen Tod dich itzt betr\u00fcbt.", "tokens": ["Des\u00b7sen", "Tod", "dich", "itzt", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhn erdr\u00fccket eine Wand,", "tokens": ["Jhn", "er\u00b7dr\u00fc\u00b7cket", "ei\u00b7ne", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und da wie\u00df sein Todes-Zeiger,", "tokens": ["Und", "da", "wie\u00df", "sein", "To\u00b7des\u00b7Zei\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Fahre aus, geliebter Steiger!", "tokens": ["Fah\u00b7re", "aus", ",", "ge\u00b7lieb\u00b7ter", "Stei\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suche nun, statt Silber, Sand,", "tokens": ["Su\u00b7che", "nun", ",", "statt", "Sil\u00b7ber", ",", "Sand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUI", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brich des C\u00f6rpers matte Schw\u00e4che,", "tokens": ["Brich", "des", "C\u00f6r\u00b7pers", "mat\u00b7te", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehe ab von deiner Zeche,", "tokens": ["Ste\u00b7he", "ab", "von", "dei\u00b7ner", "Ze\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df von Bau und Arbeit ab,", "tokens": ["La\u00df", "von", "Bau", "und", "Ar\u00b7beit", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Suche nun das d\u00fcstre Grab.", "tokens": ["Su\u00b7che", "nun", "das", "d\u00fcst\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fasse dich, geliebter Sohn!", "tokens": ["Fas\u00b7se", "dich", ",", "ge\u00b7lieb\u00b7ter", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df den Vater dorten leben,", "tokens": ["La\u00df", "den", "Va\u00b7ter", "dor\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo er kan die Ausbeuth heben,", "tokens": ["Wo", "er", "kan", "die", "Aus\u00b7beuth", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Glaubens \u00e4chten Lohn.", "tokens": ["Und", "des", "Glau\u00b7bens", "\u00e4ch\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nunmehr steiget er noch weiter", "tokens": ["Nun\u00b7mehr", "stei\u00b7get", "er", "noch", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jacobs frohe Himmels-Leiter,", "tokens": ["Ja\u00b7cobs", "fro\u00b7he", "Him\u00b7mels\u00b7Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die befl\u00fcgelt seinen Lauff.", "tokens": ["Die", "be\u00b7fl\u00fc\u00b7gelt", "sei\u00b7nen", "Lauff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fasse dich, ", "tokens": ["Fas\u00b7se", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}}}}}