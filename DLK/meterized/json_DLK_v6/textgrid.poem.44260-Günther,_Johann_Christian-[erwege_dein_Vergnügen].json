{"textgrid.poem.44260": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[erwege dein Vergn\u00fcgen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwege dein Vergn\u00fcgen,", "tokens": ["Er\u00b7we\u00b7ge", "dein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels Seegenshand", "tokens": ["Des", "Him\u00b7mels", "See\u00b7gens\u00b7hand"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will dich auf Rosen wiegen,", "tokens": ["Will", "dich", "auf", "Ro\u00b7sen", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Unstern ist verband;", "tokens": ["Dein", "Uns\u00b7tern", "ist", "ver\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was dich bisher gekr\u00e4ncket,", "tokens": ["Was", "dich", "bis\u00b7her", "ge\u00b7kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird nun ins Grab gesencket,", "tokens": ["Wird", "nun", "ins", "Grab", "ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Europa wird ersch\u00fcttert,", "tokens": ["Eu\u00b7ro\u00b7pa", "wird", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rheinstrom schwillt von Blut,", "tokens": ["Der", "Rhein\u00b7strom", "schwillt", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil sich des Mavors Wut", "tokens": ["Weil", "sich", "des", "Ma\u00b7vors", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An allen Enden wittert.", "tokens": ["An", "al\u00b7len", "En\u00b7den", "wit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur uns verschont sein Brand;", "tokens": ["Nur", "uns", "ver\u00b7schont", "sein", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Man sieht in diesen Gr\u00e4nzen", "tokens": ["Man", "sieht", "in", "die\u00b7sen", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein blanckes Schwerd mehr gl\u00e4nzen,", "tokens": ["Kein", "blan\u00b7ckes", "Schwerd", "mehr", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Flieht, bange Mordcometen!", "tokens": ["Flieht", ",", "ban\u00b7ge", "Mord\u00b7co\u00b7me\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns ist die Ruh bescheert,", "tokens": ["Uns", "ist", "die", "Ruh", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Franckreichs W\u00fcrgeschwerd", "tokens": ["Und", "Fran\u00b7ck\u00b7reichs", "W\u00fcr\u00b7ge\u00b7schwerd"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erreicht nicht die Sudeten.", "tokens": ["Er\u00b7reicht", "nicht", "die", "Su\u00b7de\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Der Harnisch ziert die Wand,", "tokens": ["Der", "Har\u00b7nisch", "ziert", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Degen f\u00fcllt die Scheide,", "tokens": ["Der", "De\u00b7gen", "f\u00fcllt", "die", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum sprechen alle beide:", "tokens": ["Drum", "spre\u00b7chen", "al\u00b7le", "bei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Musen kommen wieder,", "tokens": ["Die", "Mu\u00b7sen", "kom\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die freien K\u00fcnste bl\u00fchn,", "tokens": ["Die", "frei\u00b7en", "K\u00fcns\u00b7te", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mars wirft die Trommel hin", "tokens": ["Mars", "wirft", "die", "Trom\u00b7mel", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und singet Klagelieder.", "tokens": ["Und", "sin\u00b7get", "Kla\u00b7ge\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da, wo man Palmen fand,", "tokens": ["Da", ",", "wo", "man", "Pal\u00b7men", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df jezt der Lorbeer gr\u00fcnen", "tokens": ["Mu\u00df", "jezt", "der", "Lor\u00b7beer", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dir zu Kr\u00e4nzen dienen,", "tokens": ["Und", "dir", "zu", "Kr\u00e4n\u00b7zen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dich, Schweidniz, nimmt das Gl\u00fccke", "tokens": ["Dich", ",", "Schweid\u00b7niz", ",", "nimmt", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor andern in den Schoos,", "tokens": ["Vor", "an\u00b7dern", "in", "den", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels Gnadenlos", "tokens": ["Des", "Him\u00b7mels", "Gna\u00b7den\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Baut deiner Wohlfahrt Br\u00fccke.", "tokens": ["Baut", "dei\u00b7ner", "Wohl\u00b7fahrt", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du tr\u00e4gst der Wei\u00dfheit Pfand", "tokens": ["Du", "tr\u00e4gst", "der", "Wei\u00df\u00b7heit", "Pfand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner Huld zu Lehne,", "tokens": ["Von", "sei\u00b7ner", "Huld", "zu", "Leh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum rufen deine S\u00f6hne:", "tokens": ["Drum", "ru\u00b7fen", "dei\u00b7ne", "S\u00f6h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein Wachsthum soll bekleiben,", "tokens": ["Dein", "Wach\u00b7sthum", "soll", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Seegen ewig seyn,", "tokens": ["Dein", "See\u00b7gen", "e\u00b7wig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Gl\u00fcckes Sonnenschein", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Soll stets dein Leitstern bleiben!", "tokens": ["Soll", "stets", "dein", "Leits\u00b7tern", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein festes Liebesband", "tokens": ["Dein", "fes\u00b7tes", "Lie\u00b7bes\u00b7band"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat uns bisher gezogen,", "tokens": ["Hat", "uns", "bis\u00b7her", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum sind wir dir gewogen,", "tokens": ["Drum", "sind", "wir", "dir", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fccktes Vaterland!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}