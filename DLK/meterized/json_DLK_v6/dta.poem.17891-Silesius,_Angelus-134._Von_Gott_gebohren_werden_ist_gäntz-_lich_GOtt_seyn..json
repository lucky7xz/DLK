{"dta.poem.17891": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "134.  Von Gott gebohren werden ist g\u00e4ntz-  \n lich GOtt seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott zeuget nichts als GOTT: zeugt er dich seinen", "tokens": ["Gott", "zeu\u00b7get", "nichts", "als", "GoTT", ":", "zeugt", "er", "dich", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "KOKOM", "NE", "$.", "VVFIN", "PPER", "PRF", "PPOSAT"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sohn/", "tokens": ["Sohn", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So wirstu GOtt in GOtt/ Herr auf de\u00df Herren Thron.", "tokens": ["So", "wirs\u00b7tu", "Gott", "in", "Gott", "/", "Herr", "auf", "de\u00df", "Her\u00b7ren", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "NN", "$(", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}