{"textgrid.poem.55578": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies wird die letzte Tr\u00e4n nicht sein,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies wird die letzte Tr\u00e4n nicht sein,", "tokens": ["Dies", "wird", "die", "letz\u00b7te", "Tr\u00e4n", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gl\u00fchend Herz aufquillet,", "tokens": ["Die", "gl\u00fc\u00b7hend", "Herz", "auf\u00b7quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das mit uns\u00e4glich neuer Pein", "tokens": ["Das", "mit", "un\u00b7s\u00e4g\u00b7lich", "neu\u00b7er", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich schmerzvermehrend stillet.", "tokens": ["Sich", "schmerz\u00b7ver\u00b7meh\u00b7rend", "stil\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O la\u00df doch immer hier und dort", "tokens": ["O", "la\u00df", "doch", "im\u00b7mer", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich ewig Liebe f\u00fchlen,", "tokens": ["Mich", "e\u00b7wig", "Lie\u00b7be", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6cht der Schmerz auch also fort", "tokens": ["Und", "m\u00f6cht", "der", "Schmerz", "auch", "al\u00b7so", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Nerv' und Adern w\u00fchlen.", "tokens": ["Durch", "Ner\u00b7v'", "und", "A\u00b7dern", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt ich doch ausgef\u00fcllt einmal", "tokens": ["K\u00f6nnt", "ich", "doch", "aus\u00b7ge\u00b7f\u00fcllt", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dir, o Ew'ger, werden", "tokens": ["Von", "dir", ",", "o", "Ew'\u00b7ger", ",", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, diese lange, tiefe Qual,", "tokens": ["Ach", ",", "die\u00b7se", "lan\u00b7ge", ",", "tie\u00b7fe", "Qual", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie dauert sie auf Erden!", "tokens": ["Wie", "dau\u00b7ert", "sie", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}