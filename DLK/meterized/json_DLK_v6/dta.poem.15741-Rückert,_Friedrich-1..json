{"dta.poem.15741": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hauch Gottes, Poesie, o komm mich anzuhauchen,", "tokens": ["Hauch", "Got\u00b7tes", ",", "Poe\u00b7sie", ",", "o", "komm", "mich", "an\u00b7zu\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,", "FM", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "In deinen Rosenduft die kalte Welt zu tauchen.", "tokens": ["In", "dei\u00b7nen", "Ro\u00b7sen\u00b7duft", "die", "kal\u00b7te", "Welt", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du anl\u00e4chelst, lacht; was du anblickest, gl\u00e4nzt;", "tokens": ["Was", "du", "an\u00b7l\u00e4\u00b7chelst", ",", "lacht", ";", "was", "du", "an\u00b7bli\u00b7ckest", ",", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eng' erweitert sich, und Weites wird begr\u00e4nzt.", "tokens": ["Die", "Eng'", "er\u00b7wei\u00b7tert", "sich", ",", "und", "Wei\u00b7tes", "wird", "be\u00b7gr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Durch dich ist ewig, was im Augenblick geschwunden,", "tokens": ["Durch", "dich", "ist", "e\u00b7wig", ",", "was", "im", "Au\u00b7gen\u00b7blick", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich gelebt, gedacht, genossen und empfunden.", "tokens": ["Was", "ich", "ge\u00b7lebt", ",", "ge\u00b7dacht", ",", "ge\u00b7nos\u00b7sen", "und", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}