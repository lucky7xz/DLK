{"textgrid.poem.34853": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbzuweilen d\u00fcnkt es mich, als tr\u00fcbe", "tokens": ["\u00bb", "zu\u00b7wei\u00b7len", "d\u00fcnkt", "es", "mich", ",", "als", "tr\u00fc\u00b7be"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geheime Sehnsucht deinen Blick \u2013", "tokens": ["Ge\u00b7hei\u00b7me", "Sehn\u00b7sucht", "dei\u00b7nen", "Blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kenn es wohl, dein Mi\u00dfgeschick:", "tokens": ["Ich", "kenn", "es", "wohl", ",", "dein", "Mi\u00df\u00b7ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verfehltes Leben, verfehlte Liebel", "tokens": ["Ver\u00b7fehl\u00b7tes", "Le\u00b7ben", ",", "ver\u00b7fehl\u00b7te", "Lie\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du nickst so traurig! Wiedergeben", "tokens": ["Du", "nickst", "so", "trau\u00b7rig", "!", "Wie\u00b7der\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ich dir nicht die Jugendzeit \u2013", "tokens": ["Kann", "ich", "dir", "nicht", "die", "Ju\u00b7gend\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unheilbar ist dein Herzeleid:", "tokens": ["Un\u00b7heil\u00b7bar", "ist", "dein", "Her\u00b7ze\u00b7leid", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verfehlte Liebe, verfehltes Leben!\u00ab", "tokens": ["Ver\u00b7fehl\u00b7te", "Lie\u00b7be", ",", "ver\u00b7fehl\u00b7tes", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}