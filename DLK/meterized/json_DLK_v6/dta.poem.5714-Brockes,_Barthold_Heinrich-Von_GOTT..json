{"dta.poem.5714": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Epicurer Grund st\u00fcrtzt ebenfalls danieder.", "tokens": ["Der", "E\u00b7pi\u00b7cu\u00b7rer", "Grund", "st\u00fcrtzt", "e\u00b7ben\u00b7falls", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flieg\u2019t die Natur, durch ein blind ", "tokens": ["Flieg't", "die", "Na\u00b7tur", ",", "durch", "ein", "blind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Bald hin, bald her,", "tokens": ["Bald", "hin", ",", "bald", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Stets hin und wieder?", "tokens": ["Stets", "hin", "und", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was wir vorher gesagt, zeigt uns gar kl\u00e4rlich an,", "tokens": ["Was", "wir", "vor\u00b7her", "ge\u00b7sagt", ",", "zeigt", "uns", "gar", "kl\u00e4r\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df eine Ursach stets die andere regieret,", "tokens": ["Da\u00df", "ei\u00b7ne", "Ur\u00b7sach", "stets", "die", "an\u00b7de\u00b7re", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch eine erstere wird sie gef\u00fchret;", "tokens": ["Durch", "ei\u00b7ne", "ers\u00b7te\u00b7re", "wird", "sie", "ge\u00b7f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bi\u00df da\u00df der Geist nicht weiter kommen kan,", "tokens": ["Bi\u00df", "da\u00df", "der", "Geist", "nicht", "wei\u00b7ter", "kom\u00b7men", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als zu dem ersteren Gesetz, das blos allein", "tokens": ["Als", "zu", "dem", "ers\u00b7te\u00b7ren", "Ge\u00b7setz", ",", "das", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus dem geflossen ist, wodurch die ganze Welt", "tokens": ["Aus", "dem", "ge\u00b7flos\u00b7sen", "ist", ",", "wo\u00b7durch", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVPP", "VAFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erschaffen ist, und uns ist vorgestellt.", "tokens": ["Er\u00b7schaf\u00b7fen", "ist", ",", "und", "uns", "ist", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das was man insgemein pflegt ", "tokens": ["Das", "was", "man", "ins\u00b7ge\u00b7mein", "pflegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ist nichts, und kan nichts seyn,", "tokens": ["Ist", "nichts", ",", "und", "kan", "nichts", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "KON", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Als blo\u00df ein Nahme nur, der zeigt, was wir nicht kennen.", "tokens": ["Als", "blo\u00df", "ein", "Nah\u00b7me", "nur", ",", "der", "zeigt", ",", "was", "wir", "nicht", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von einer W\u00fcrckung mu\u00df man stets zur andern gehn,", "tokens": ["Von", "ei\u00b7ner", "W\u00fcr\u00b7ckung", "mu\u00df", "man", "stets", "zur", "an\u00b7dern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "ADV", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und, sonder jemals still\u2019 zu stehn,", "tokens": ["Und", ",", "son\u00b7der", "je\u00b7mals", "still'", "zu", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Zur ersten Ursach sich erh\u00f6hn.", "tokens": ["Zur", "ers\u00b7ten", "Ur\u00b7sach", "sich", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}