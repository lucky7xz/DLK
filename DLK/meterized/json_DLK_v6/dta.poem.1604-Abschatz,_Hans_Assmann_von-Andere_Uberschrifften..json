{"dta.poem.1604": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Andere Uberschrifften.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein junges Leben schien kaum in der Bl\u00fctte seyn/", "tokens": ["Mein", "jun\u00b7ges", "Le\u00b7ben", "schien", "kaum", "in", "der", "Bl\u00fct\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott aber lie\u00df mich reiff zur Lebens-Erndte schneiden.", "tokens": ["Gott", "a\u00b7ber", "lie\u00df", "mich", "reiff", "zur", "Le\u00b7bens\u00b7Ernd\u00b7te", "schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Eltern s\u00e4t mich hier mit bittren Thr\u00e4nen ein/", "tokens": ["Ihr", "El\u00b7tern", "s\u00e4t", "mich", "hier", "mit", "bit\u00b7tren", "Thr\u00e4\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort aber erndtet ihr mich wieder ein mit Freuden.", "tokens": ["Dort", "a\u00b7ber", "ernd\u00b7tet", "ihr", "mich", "wie\u00b7der", "ein", "mit", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wor