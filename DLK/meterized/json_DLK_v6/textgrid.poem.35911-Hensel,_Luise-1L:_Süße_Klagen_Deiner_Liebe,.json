{"textgrid.poem.35911": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfe Klagen Deiner Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Klagen Deiner Liebe,", "tokens": ["S\u00fc\u00b7\u00dfe", "Kla\u00b7gen", "Dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holde Lieder, zart und tr\u00fcbe,", "tokens": ["Hol\u00b7de", "Lie\u00b7der", ",", "zart", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haucht die Fr\u00fchlingsluft mir zu.", "tokens": ["Haucht", "die", "Fr\u00fch\u00b7lings\u00b7luft", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gute, die Du mich geboren,", "tokens": ["Gu\u00b7te", ",", "die", "Du", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer bin ich Dir verloren;", "tokens": ["Nim\u00b7mer", "bin", "ich", "Dir", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00f6re nicht des Herzens Ruh'.", "tokens": ["St\u00f6\u00b7re", "nicht", "des", "Her\u00b7zens", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, Du sollst, o Mutter! selig,", "tokens": ["Ja", ",", "Du", "sollst", ",", "o", "Mut\u00b7ter", "!", "se\u00b7lig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$,", "FM", "NN", "$.", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig jung und rein und fr\u00f6hlich", "tokens": ["E\u00b7wig", "jung", "und", "rein", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst die Tochter wiedersehn;", "tokens": ["Einst", "die", "Toch\u00b7ter", "wie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie will im Land der Thr\u00e4nen", "tokens": ["Denn", "sie", "will", "im", "Land", "der", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fromm sich nach dem Himmel sehnen,", "tokens": ["Fromm", "sich", "nach", "dem", "Him\u00b7mel", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treu auf Gottes Wegen gehn.", "tokens": ["Treu", "auf", "Got\u00b7tes", "We\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Dein Antlitz zu erblicken,", "tokens": ["Und", "Dein", "Ant\u00b7litz", "zu", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder Dich an's Herz zu dr\u00fccken", "tokens": ["Wie\u00b7der", "Dich", "an's", "Herz", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal noch auf dieser Welt:", "tokens": ["Ein\u00b7mal", "noch", "auf", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies auch ist mein Wunsch, mein Flehen.", "tokens": ["Dies", "auch", "ist", "mein", "Wunsch", ",", "mein", "Fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6cht' ich, Theure! Dich noch sehen,", "tokens": ["M\u00f6cht'", "ich", ",", "Theu\u00b7re", "!", "Dich", "noch", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$.", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' mein Herz in Staub zerf\u00e4llt!", "tokens": ["Eh'", "mein", "Herz", "in", "Staub", "zer\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber nimmer fordern, klagen,", "tokens": ["A\u00b7ber", "nim\u00b7mer", "for\u00b7dern", ",", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still und fromm mein Loos ertragen,", "tokens": ["Still", "und", "fromm", "mein", "Loos", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Friedlich wallen meinen Pfad:", "tokens": ["Fried\u00b7lich", "wal\u00b7len", "mei\u00b7nen", "Pfad", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist meine Pflicht hienieden,", "tokens": ["Das", "ist", "mei\u00b7ne", "Pflicht", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das nur giebt der Seele Frieden,", "tokens": ["Das", "nur", "giebt", "der", "See\u00b7le", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den Dein Herz mir fromm erbat.", "tokens": ["Den", "Dein", "Herz", "mir", "fromm", "er\u00b7bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}