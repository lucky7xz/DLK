{"textgrid.poem.35914": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Scheiden \u2013 o bitterer Kelch, bald wird meine Lippe dich kosten!", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scheiden \u2013 o bitterer Kelch, bald wird meine Lippe dich kosten!", "tokens": ["Schei\u00b7den", "\u2013", "o", "bit\u00b7te\u00b7rer", "Kelch", ",", "bald", "wird", "mei\u00b7ne", "Lip\u00b7pe", "dich", "kos\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Scheiden \u2013 o schmerzendes Wort, das durch die Seele mir dringt!", "tokens": ["Schei\u00b7den", "\u2013", "o", "schmer\u00b7zen\u00b7des", "Wort", ",", "das", "durch", "die", "See\u00b7le", "mir", "dringt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sichel, so schneidend und scharf, du kommst meine Blumen zu f\u00e4llen.", "tokens": ["Si\u00b7chel", ",", "so", "schnei\u00b7dend", "und", "scharf", ",", "du", "kommst", "mei\u00b7ne", "Blu\u00b7men", "zu", "f\u00e4l\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "K\u00f6cher voll t\u00f6dtender Pfeil', o wie erbeb' ich vor dir!", "tokens": ["K\u00f6\u00b7cher", "voll", "t\u00f6d\u00b7ten\u00b7der", "Pfeil'", ",", "o", "wie", "er\u00b7beb'", "ich", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,", "FM", "KOKOM", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Heute noch kann meine Hand die trautesten H\u00e4nde erfassen,", "tokens": ["Heu\u00b7te", "noch", "kann", "mei\u00b7ne", "Hand", "die", "trau\u00b7tes\u00b7ten", "H\u00e4n\u00b7de", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Heute noch findet mein Blick, Mutter! Dein freundliches Aug'.", "tokens": ["Heu\u00b7te", "noch", "fin\u00b7det", "mein", "Blick", ",", "Mut\u00b7ter", "!", "Dein", "freund\u00b7li\u00b7ches", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Zweimal noch seh' ich mit Dir im Sp\u00e4troth ergl\u00fchen die H\u00fcgel,", "tokens": ["Zwei\u00b7mal", "noch", "seh'", "ich", "mit", "Dir", "im", "Sp\u00e4t\u00b7roth", "er\u00b7gl\u00fc\u00b7hen", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zweimal noch schallt uns vereint fr\u00fche der V\u00f6gelein Sang.", "tokens": ["Zwei\u00b7mal", "noch", "schallt", "uns", "ver\u00b7eint", "fr\u00fc\u00b7he", "der", "V\u00f6\u00b7ge\u00b7lein", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "ADJA", "ART", "NN", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber dann f\u00fchrt Dich Dein Pfad dahin in die neblichte Ferne,", "tokens": ["A\u00b7ber", "dann", "f\u00fchrt", "Dich", "Dein", "Pfad", "da\u00b7hin", "in", "die", "neb\u00b7lich\u00b7te", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ach, und es f\u00fchrt mich mein Weg einsam und tr\u00fcbe daher. \u2013", "tokens": ["Ach", ",", "und", "es", "f\u00fchrt", "mich", "mein", "Weg", "ein\u00b7sam", "und", "tr\u00fc\u00b7be", "da\u00b7her", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "PAV", "$.", "$("], "meter": "+---+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Aber ich denke an Dich \u2013 Du kannst mir entrissen nicht werden, \u2013", "tokens": ["A\u00b7ber", "ich", "den\u00b7ke", "an", "Dich", "\u2013", "Du", "kannst", "mir", "ent\u00b7ris\u00b7sen", "nicht", "wer\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "PTKNEG", "VAINF", "$,", "$("], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und der Gedanke an Dich wird mir zum tr\u00f6stenden Freund.", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", "an", "Dich", "wird", "mir", "zum", "tr\u00f6s\u00b7ten\u00b7den", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "---+---+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Tr\u00fcbt dann die Sehnsucht mein Aug', so trinkt wohl der Sand meine Z\u00e4hre,", "tokens": ["Tr\u00fcbt", "dann", "die", "Sehn\u00b7sucht", "mein", "Aug'", ",", "so", "trinkt", "wohl", "der", "Sand", "mei\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber die Hoffnung, sie hebt muthig zum Himmel mein Herz. \u2013", "tokens": ["A\u00b7ber", "die", "Hoff\u00b7nung", ",", "sie", "hebt", "mut\u00b7hig", "zum", "Him\u00b7mel", "mein", "Herz", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Amen, o Amen, mein Gott! Hier bin ich \u2013 ich bin Dir ergeben.", "tokens": ["A\u00b7men", ",", "o", "A\u00b7men", ",", "mein", "Gott", "!", "Hier", "bin", "ich", "\u2013", "ich", "bin", "Dir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gieb mir den bitteren Kelch \u2013 Vater! Du giebst ihn mir ja.", "tokens": ["Gieb", "mir", "den", "bit\u00b7te\u00b7ren", "Kelch", "\u2013", "Va\u00b7ter", "!", "Du", "giebst", "ihn", "mir", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$(", "NN", "$.", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}}}}}