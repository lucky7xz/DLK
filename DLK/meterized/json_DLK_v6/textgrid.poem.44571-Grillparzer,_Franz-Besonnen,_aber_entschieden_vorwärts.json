{"textgrid.poem.44571": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Besonnen, aber entschieden vorw\u00e4rts", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Kopf von Sorgen m\u00fcde, ging neulich ich aufs Land;", "tokens": ["Den", "Kopf", "von", "Sor\u00b7gen", "m\u00fc\u00b7de", ",", "ging", "neu\u00b7lich", "ich", "aufs", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Freund am Linientore sich mir entgegen fand.", "tokens": ["Ein", "Freund", "am", "Li\u00b7ni\u00b7en\u00b7to\u00b7re", "sich", "mir", "ent\u00b7ge\u00b7gen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir gr\u00fc\u00dften uns gar freundlich, wir dr\u00fcckten uns die Hand,", "tokens": ["Wir", "gr\u00fc\u00df\u00b7ten", "uns", "gar", "freund\u00b7lich", ",", "wir", "dr\u00fcck\u00b7ten", "uns", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir schieden voneinander hart an des Grabens Rand,", "tokens": ["Wir", "schie\u00b7den", "von\u00b7ein\u00b7an\u00b7der", "hart", "an", "des", "Gra\u00b7bens", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der eine ging nach D\u00f6bling, der andre ging nach Wien,", "tokens": ["Der", "ei\u00b7ne", "ging", "nach", "D\u00f6b\u00b7ling", ",", "der", "and\u00b7re", "ging", "nach", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NE", "$,", "PRELS", "PIS", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wir beide gingen vorw\u00e4rts \u2013 nur im verschiednen Sinn.", "tokens": ["Wir", "bei\u00b7de", "gin\u00b7gen", "vor\u00b7w\u00e4rts", "\u2013", "nur", "im", "ver\u00b7schied\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "$(", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}