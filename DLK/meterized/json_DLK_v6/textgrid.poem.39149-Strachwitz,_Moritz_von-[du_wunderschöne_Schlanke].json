{"textgrid.poem.39149": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[du wundersch\u00f6ne Schlanke]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wundersch\u00f6ne Schlanke,", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir biet' ich all mein Herz,", "tokens": ["Dir", "biet'", "ich", "all", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies stolze, liebeskranke,", "tokens": ["Dies", "stol\u00b7ze", ",", "lie\u00b7bes\u00b7kran\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glutschmachtende Dichterherz.", "tokens": ["Glutschmach\u00b7ten\u00b7de", "Dich\u00b7ter\u00b7herz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wohl m\u00f6cht' ich es gern umschlingen", "tokens": ["Wohl", "m\u00f6cht'", "ich", "es", "gern", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Blumen aus Oft und S\u00fcd,", "tokens": ["Mit", "Blu\u00b7men", "aus", "Oft", "und", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu deinem Preise singen", "tokens": ["Zu", "dei\u00b7nem", "Prei\u00b7se", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein hohes, hehres Lied.", "tokens": ["Ein", "ho\u00b7hes", ",", "heh\u00b7res", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Lied, das unerme\u00dflich", "tokens": ["Ein", "Lied", ",", "das", "un\u00b7er\u00b7me\u00df\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Klang zu Klange schwebt,", "tokens": ["Von", "Klang", "zu", "Klan\u00b7ge", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lied, das unverge\u00dflich", "tokens": ["Ein", "Lied", ",", "das", "un\u00b7ver\u00b7ge\u00df\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Lippe zu Lippe bebt.", "tokens": ["Von", "Lip\u00b7pe", "zu", "Lip\u00b7pe", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Lied, drin Nebeld\u00fcster", "tokens": ["Ein", "Lied", ",", "drin", "Ne\u00b7bel\u00b7d\u00fcs\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Himmelsbl\u00e4ue sich eint,", "tokens": ["Mit", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "sich", "eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Lied, drin Blumengefl\u00fcster", "tokens": ["Ein", "Lied", ",", "drin", "Blu\u00b7men\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ins Waldesgebrause weint.", "tokens": ["Ins", "Wal\u00b7des\u00b7ge\u00b7brau\u00b7se", "weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Drein m\u00f6cht' ich verweben, verzweigen,", "tokens": ["Drein", "m\u00f6cht'", "ich", "ver\u00b7we\u00b7ben", ",", "ver\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVPP", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Den ganzen t\u00f6nenden Drang,", "tokens": ["Den", "gan\u00b7zen", "t\u00f6\u00b7nen\u00b7den", "Drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verstummen darauf und schweigen", "tokens": ["Ver\u00b7stum\u00b7men", "da\u00b7rauf", "und", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PAV", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "All, all mein Leben lang.", "tokens": ["All", ",", "all", "mein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wenn ich zum stolzen Vermessen", "tokens": ["Doch", "wenn", "ich", "zum", "stol\u00b7zen", "Ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Mich stolz zusammengerafft,", "tokens": ["Mich", "stolz", "zu\u00b7sam\u00b7men\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Entgaukelt mir unterdessen", "tokens": ["Ent\u00b7gau\u00b7kelt", "mir", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ganze Gesangeskraft", "tokens": ["Die", "gan\u00b7ze", "Ge\u00b7san\u00b7ges\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Es ist ein einz'ger Gedanke,", "tokens": ["Es", "ist", "ein", "einz'\u00b7ger", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der stiehlt mir Lied und Herz:", "tokens": ["Der", "stiehlt", "mir", "Lied", "und", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wundersch\u00f6ne Schlanke,", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir biet' ich all mein Herz.", "tokens": ["Dir", "biet'", "ich", "all", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}