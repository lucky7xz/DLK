{"textgrid.poem.32703": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, mein R\u00f6schen, der Fr\u00fchling ist da;", "tokens": ["Sie\u00b7he", ",", "mein", "R\u00f6\u00b7schen", ",", "der", "Fr\u00fch\u00b7ling", "ist", "da", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Freuden die F\u00fclle sind ferne, sind nah;", "tokens": ["Freu\u00b7den", "die", "F\u00fcl\u00b7le", "sind", "fer\u00b7ne", ",", "sind", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADV", "$,", "VAFIN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Blumen entspringen;", "tokens": ["Blu\u00b7men", "ent\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "V\u00f6gelein singen,", "tokens": ["V\u00f6\u00b7ge\u00b7lein", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df die Geb\u00fcrg' und die Th\u00e4ler erklingen.", "tokens": ["Da\u00df", "die", "Ge\u00b7b\u00fcr\u00b7g'", "und", "die", "Th\u00e4\u00b7ler", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "La\u00df uns besuchen den seligen Plan,", "tokens": ["La\u00df", "uns", "be\u00b7su\u00b7chen", "den", "se\u00b7li\u00b7gen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wo wir uns beide das erste Mal sahn:", "tokens": ["Wo", "wir", "uns", "bei\u00b7de", "das", "ers\u00b7te", "Mal", "sahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Blumen entsprangen;", "tokens": ["Blu\u00b7men", "ent\u00b7spran\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "V\u00f6gelein sangen,", "tokens": ["V\u00f6\u00b7ge\u00b7lein", "san\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df die Geb\u00fcrg' und die Th\u00e4ler erklangen.", "tokens": ["Da\u00df", "die", "Ge\u00b7b\u00fcr\u00b7g'", "und", "die", "Th\u00e4\u00b7ler", "er\u00b7klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Aber ich wandelte traurig einher,", "tokens": ["A\u00b7ber", "ich", "wan\u00b7del\u00b7te", "trau\u00b7rig", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "F\u00fchlte die Freuden des Maien nicht mehr,", "tokens": ["F\u00fchl\u00b7te", "die", "Freu\u00b7den", "des", "Mai\u00b7en", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Blickte danieder;", "tokens": ["Blick\u00b7te", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Blumen und Lieder", "tokens": ["Blu\u00b7men", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Waren dem liebenden J\u00fcngling zuwider.", "tokens": ["Wa\u00b7ren", "dem", "lie\u00b7ben\u00b7den", "J\u00fcng\u00b7ling", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis du mein einsames Klagen geh\u00f6rt,", "tokens": ["Bis", "du", "mein", "ein\u00b7sa\u00b7mes", "Kla\u00b7gen", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und mir die Thr\u00e4nen in Lachen verkehrt.", "tokens": ["Und", "mir", "die", "Thr\u00e4\u00b7nen", "in", "La\u00b7chen", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jetzo erfreuen", "tokens": ["Jet\u00b7zo", "er\u00b7freu\u00b7en"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wieder von neuen", "tokens": ["Wie\u00b7der", "von", "neu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mich die gesegneten Tage des Maien.", "tokens": ["Mich", "die", "ge\u00b7se\u00b7gne\u00b7ten", "Ta\u00b7ge", "des", "Mai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "----+-+--+-", "measure": "iambic.tri.relaxed"}}}}}