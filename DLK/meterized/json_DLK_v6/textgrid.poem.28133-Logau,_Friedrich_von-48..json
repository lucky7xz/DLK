{"textgrid.poem.28133": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wil Kirchen-Bilder wer zum Ergern\u00fc\u00df anziehn,", "tokens": ["Wil", "Kir\u00b7chen\u00b7Bil\u00b7der", "wer", "zum", "Er\u00b7ger\u00b7n\u00fc\u00df", "an\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PWS", "APPRART", "NN", "VVIZU", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Den \u00e4rgern Bilder nicht, die Augen \u00e4rgern ihn;", "tokens": ["Den", "\u00e4r\u00b7gern", "Bil\u00b7der", "nicht", ",", "die", "Au\u00b7gen", "\u00e4r\u00b7gern", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum la\u00df er jene stehn und reisse diese hin.", "tokens": ["Drum", "la\u00df", "er", "je\u00b7ne", "stehn", "und", "reis\u00b7se", "die\u00b7se", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDS", "VVINF", "KON", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}