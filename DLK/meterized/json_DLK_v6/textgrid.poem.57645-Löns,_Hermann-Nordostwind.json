{"textgrid.poem.57645": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nordostwind", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nordostwind pfeift im Moore,", "tokens": ["Nord\u00b7ost\u00b7wind", "pfeift", "im", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Scharf treibt der feine Schnee,", "tokens": ["Scharf", "treibt", "der", "fei\u00b7ne", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hungrig zieht den Moordamm", "tokens": ["Hung\u00b7rig", "zieht", "den", "Moor\u00b7damm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Entlang ein k\u00fcmmerndes Reh.", "tokens": ["Ent\u00b7lang", "ein", "k\u00fcm\u00b7mern\u00b7des", "Reh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Treibschnee zischelt und ruschelt,", "tokens": ["Der", "Treib\u00b7schnee", "zi\u00b7schelt", "und", "ru\u00b7schelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verloren die Wege sind,", "tokens": ["Ver\u00b7lo\u00b7ren", "die", "We\u00b7ge", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich friere in meiner Seele", "tokens": ["Ich", "frie\u00b7re", "in", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und eisig pfeift der Wind.", "tokens": ["Und", "ei\u00b7sig", "pfeift", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}