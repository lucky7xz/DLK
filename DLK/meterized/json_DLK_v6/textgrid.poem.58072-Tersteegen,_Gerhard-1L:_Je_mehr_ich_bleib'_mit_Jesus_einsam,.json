{"textgrid.poem.58072": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Je mehr ich bleib' mit Jesus einsam,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je mehr ich bleib' mit Jesus einsam,", "tokens": ["Je", "mehr", "ich", "bleib'", "mit", "Je\u00b7sus", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr er sich mir macht gemeinsam;", "tokens": ["Je", "mehr", "er", "sich", "mir", "macht", "ge\u00b7mein\u00b7sam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er und ich nur sind allein", "tokens": ["Wenn", "er", "und", "ich", "nur", "sind", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meines Herzens K\u00e4mmerlein,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hab' ich in ihm tiefen Frieden,", "tokens": ["So", "hab'", "ich", "in", "ihm", "tie\u00b7fen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum bleib' ich gerne abgeschieden.", "tokens": ["Drum", "bleib'", "ich", "ger\u00b7ne", "ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}