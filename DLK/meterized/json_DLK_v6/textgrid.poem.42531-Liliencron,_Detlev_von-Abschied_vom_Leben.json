{"textgrid.poem.42531": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied vom Leben", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins halb schon tote Herz, ins alte, gr\u00fc\u00dfen", "tokens": ["Ins", "halb", "schon", "to\u00b7te", "Herz", ",", "ins", "al\u00b7te", ",", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJD", "ADV", "ADJA", "NN", "$,", "APPRART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch einmal Vogelsang und Sommerranken.", "tokens": ["Noch", "ein\u00b7mal", "Vo\u00b7gel\u00b7sang", "und", "Som\u00b7mer\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie blau der Himmel; welch ein lustig Schwanken", "tokens": ["Wie", "blau", "der", "Him\u00b7mel", ";", "welch", "ein", "lus\u00b7tig", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "$.", "PWAT", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gr\u00fcnen Bl\u00e4tter, die sich neckend k\u00fcssen.", "tokens": ["Der", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", ",", "die", "sich", "ne\u00b7ckend", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und nun das herbe Abschiednehmenm\u00fcssen.", "tokens": ["Und", "nun", "das", "her\u00b7be", "Ab\u00b7schied\u00b7neh\u00b7men\u00b7m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei, wie z\u00f6gernd, gleiten in Gedanken", "tokens": ["Vor\u00b7bei", ",", "wie", "z\u00f6\u00b7gernd", ",", "glei\u00b7ten", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJD", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wenigen Stunden, die ins Herz mir sanken", "tokens": ["Die", "we\u00b7ni\u00b7gen", "Stun\u00b7den", ",", "die", "ins", "Herz", "mir", "san\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit reinen Seligkeiten und Gen\u00fcssen.", "tokens": ["Mit", "rei\u00b7nen", "Se\u00b7lig\u00b7kei\u00b7ten", "und", "Ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "G\u00f6nnt mir den letzten Trunk aus diesen Schalen,", "tokens": ["G\u00f6nnt", "mir", "den", "letz\u00b7ten", "Trunk", "aus", "die\u00b7sen", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "eh ich hinab mu\u00df in die grauen Gr\u00fcnde;", "tokens": ["eh", "ich", "hin\u00b7ab", "mu\u00df", "in", "die", "grau\u00b7en", "Gr\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O g\u00f6nnt ihn mir als letzte meiner Qualen!", "tokens": ["O", "g\u00f6nnt", "ihn", "mir", "als", "letz\u00b7te", "mei\u00b7ner", "Qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "KOUS", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lebt wohl! Klagt euerm Gott all meine S\u00fcnde!", "tokens": ["Lebt", "wohl", "!", "Klagt", "eu\u00b7erm", "Gott", "all", "mei\u00b7ne", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "VVFIN", "PPOSAT", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr kennt die Schmerzen nicht, die in mein Leben", "tokens": ["Ihr", "kennt", "die", "Schmer\u00b7zen", "nicht", ",", "die", "in", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich gruben; sonst \u2013 ihr w\u00fcrdet mir vergeben.", "tokens": ["Sich", "gru\u00b7ben", ";", "sonst", "\u2013", "ihr", "w\u00fcr\u00b7det", "mir", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$.", "ADV", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}