{"dta.poem.23806": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der 146. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ermunter dich mein Geist und mache dich bereit/", "tokens": ["Er\u00b7mun\u00b7ter", "dich", "mein", "Geist", "und", "ma\u00b7che", "dich", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu loben deinen GOtt die gantze Lebens-Zeit/", "tokens": ["Zu", "lo\u00b7ben", "dei\u00b7nen", "Gott", "die", "gant\u00b7ze", "Le\u00b7bens\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die F\u00fcrsten helffen nichts/ die G\u00f6tter dieser Erden/", "tokens": ["Die", "F\u00fcrs\u00b7ten", "helf\u00b7fen", "nichts", "/", "die", "G\u00f6t\u00b7ter", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie so leicht als ich zu Leichen k\u00f6nnen werden;", "tokens": ["Weil", "sie", "so", "leicht", "als", "ich", "zu", "Lei\u00b7chen", "k\u00f6n\u00b7nen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "APPR", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie bald verf\u00e4llt alsdenn was ihre Macht gebaut:", "tokens": ["Wie", "bald", "ver\u00b7f\u00e4llt", "als\u00b7denn", "was", "ih\u00b7re", "Macht", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "KON", "PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der aber fehlet nicht/ der solchem HErren traut.", "tokens": ["Der", "a\u00b7ber", "feh\u00b7let", "nicht", "/", "der", "sol\u00b7chem", "Her\u00b7ren", "traut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKNEG", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den/ was die Welt beg reift/ als ihren Sch\u00f6pffer ehrt/", "tokens": ["Den", "/", "was", "die", "Welt", "beg", "reift", "/", "als", "ih\u00b7ren", "Sch\u00f6pf\u00b7fer", "ehrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "ART", "NN", "NN", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ewig Glauben h\u00e4lt/ Gewalt und Unrecht st\u00f6hrt.", "tokens": ["Der", "e\u00b7wig", "Glau\u00b7ben", "h\u00e4lt", "/", "Ge\u00b7walt", "und", "Un\u00b7recht", "st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$(", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Arme wohl versorgt/ aus Band und Kercker rei\u00dft/", "tokens": ["Der", "Ar\u00b7me", "wohl", "ver\u00b7sorgt", "/", "aus", "Band", "und", "Ker\u00b7cker", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Blinden ihr Gesicht/ den S\u00fcndern Trost verhei\u00dft.", "tokens": ["Den", "Blin\u00b7den", "ihr", "Ge\u00b7sicht", "/", "den", "S\u00fcn\u00b7dern", "Trost", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der seine Frommen liebt/ den Fremdling sicher f\u00fchret/", "tokens": ["Der", "sei\u00b7ne", "From\u00b7men", "liebt", "/", "den", "Fremd\u00b7ling", "si\u00b7cher", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von dem die Wittwe Rath/ die W\u00e4ysen Schutz versp\u00fc-", "tokens": ["Von", "dem", "die", "Witt\u00b7we", "Rath", "/", "die", "W\u00e4y\u00b7sen", "Schutz", "ver\u00b7sp\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der in ihr eignes Netz mit Spott die B\u00f6sen treibt.", "tokens": ["Der", "in", "ihr", "eig\u00b7nes", "Netz", "mit", "Spott", "die", "B\u00f6\u00b7sen", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dis Zion thut dein GOtt/ der ewig K\u00f6nig bleibt.", "tokens": ["Dis", "Zi\u00b7on", "thut", "dein", "Gott", "/", "der", "e\u00b7wig", "K\u00f6\u00b7nig", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NE", "VVFIN", "PPOSAT", "NN", "$(", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}