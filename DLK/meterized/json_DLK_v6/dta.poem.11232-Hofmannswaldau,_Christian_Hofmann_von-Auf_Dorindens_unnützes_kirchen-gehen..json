{"dta.poem.11232": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Dorindens unn\u00fctzes kirchen-gehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde hat in tempel sich verliebt,", "tokens": ["Do\u00b7rin\u00b7de", "hat", "in", "tem\u00b7pel", "sich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und da den heilgen ort durch ihren sinn betr\u00fcbt.", "tokens": ["Und", "da", "den", "heil\u00b7gen", "ort", "durch", "ih\u00b7ren", "sinn", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun ist die strafe da:", "tokens": ["Nun", "ist", "die", "stra\u00b7fe", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn als ihr schatz die gunst erfuhr,", "tokens": ["Denn", "als", "ihr", "schatz", "die", "gunst", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So macht er sich auf die gebahnte spur,", "tokens": ["So", "macht", "er", "sich", "auf", "die", "ge\u00b7bahn\u00b7te", "spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und kam ihr so zu nah,", "tokens": ["Und", "kam", "ihr", "so", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df sie bald wird was junges h\u00f6ren schreyn.", "tokens": ["Da\u00df", "sie", "bald", "wird", "was", "jun\u00b7ges", "h\u00f6\u00b7ren", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "PIS", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jhr andern, wo ihr auch wolt so gesinnet seyn:", "tokens": ["Ihr", "an\u00b7dern", ",", "wo", "ihr", "auch", "wolt", "so", "ge\u00b7sin\u00b7net", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PWAV", "PPER", "ADV", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So stellt viel lieber gar das kirchen-gehen ein.", "tokens": ["So", "stellt", "viel", "lie\u00b7ber", "gar", "das", "kir\u00b7chen\u00b7ge\u00b7hen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr jungfern! nehmt euch ein exempel,", "tokens": ["Ihr", "jung\u00b7fern", "!", "nehmt", "euch", "ein", "ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und seht, wie es Dorinde macht", "tokens": ["Und", "seht", ",", "wie", "es", "Do\u00b7rin\u00b7de", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fein alle morgen in dem tempel,", "tokens": ["Fein", "al\u00b7le", "mor\u00b7gen", "in", "dem", "tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df auch bey euch das gl\u00fcck erwacht;", "tokens": ["Da\u00df", "auch", "bey", "euch", "das", "gl\u00fcck", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Doch dieses w\u00fcnscht euch nicht, was sie bereits betroffen:", "tokens": ["Doch", "die\u00b7ses", "w\u00fcnscht", "euch", "nicht", ",", "was", "sie", "be\u00b7reits", "be\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie kan, ohn einen mann, itzt auf was junges hoffen.", "tokens": ["Sie", "kan", ",", "ohn", "ei\u00b7nen", "mann", ",", "itzt", "auf", "was", "jun\u00b7ges", "hof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUI", "ART", "NN", "$,", "ADV", "APPR", "PRELS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}