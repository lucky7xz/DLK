{"textgrid.poem.43861": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Betr\u00fcgliches Gl\u00fccke!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcgliches Gl\u00fccke!", "tokens": ["Be\u00b7tr\u00fcg\u00b7li\u00b7ches", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die st\u00e4hlerne Br\u00fccke", "tokens": ["Die", "st\u00e4h\u00b7ler\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Hofnung zerf\u00e4llt;", "tokens": ["Der", "Hof\u00b7nung", "zer\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Becher der Freuden", "tokens": ["Der", "Be\u00b7cher", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wird mir durch dies Leiden", "tokens": ["Wird", "mir", "durch", "dies", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit Wermuth verg\u00e4llt.", "tokens": ["Mit", "Wer\u00b7muth", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne der Tugend,", "tokens": ["Die", "Son\u00b7ne", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Blume der Jugend,", "tokens": ["Die", "Blu\u00b7me", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Geht unter und ein;", "tokens": ["Geht", "un\u00b7ter", "und", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "KON", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Himmel wird tr\u00fcbe,", "tokens": ["Der", "Him\u00b7mel", "wird", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Flammen der Liebe", "tokens": ["Die", "Flam\u00b7men", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Verlieren den Schein.", "tokens": ["Ver\u00b7lie\u00b7ren", "den", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling der Jahre", "tokens": ["Der", "Fr\u00fch\u00b7ling", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erstirbt auf der Baare:", "tokens": ["Er\u00b7stirbt", "auf", "der", "Baa\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer wird mir den Ku\u00df", "tokens": ["Wer", "wird", "mir", "den", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie vormahls gew\u00e4hren?", "tokens": ["Wie", "vor\u00b7mahls", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ach langes Entbehren!", "tokens": ["Ach", "lan\u00b7ges", "Ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ach kurzer Genu\u00df!", "tokens": ["Ach", "kur\u00b7zer", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Erblaste Florette,", "tokens": ["Er\u00b7blas\u00b7te", "Flo\u00b7ret\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dein Tod rei\u00dft die Kette", "tokens": ["Dein", "Tod", "rei\u00dft", "die", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Eintracht entzwey;", "tokens": ["Der", "Ein\u00b7tracht", "ent\u00b7zwey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dein Leichenbeg\u00e4ngn\u00fc\u00df", "tokens": ["Dein", "Lei\u00b7chen\u00b7be\u00b7g\u00e4n\u00b7gn\u00fc\u00df"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zeigt, wie das Verh\u00e4ngn\u00fc\u00df", "tokens": ["Zeigt", ",", "wie", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mein Henckersknecht sey.", "tokens": ["Mein", "Hen\u00b7ckers\u00b7knecht", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Bedeckt mich, ihr Berge,", "tokens": ["Be\u00b7deckt", "mich", ",", "ihr", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Umfast mich, ihr S\u00e4rge,", "tokens": ["Um\u00b7fast", "mich", ",", "ihr", "S\u00e4r\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Versagt mir die Luft!", "tokens": ["Ver\u00b7sagt", "mir", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mein Geist mag zerfliegen,", "tokens": ["Mein", "Geist", "mag", "zer\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Des Leibes Vergn\u00fcgen", "tokens": ["Des", "Lei\u00b7bes", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ist Moder und Gruft.", "tokens": ["Ist", "Mo\u00b7der", "und", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ich sterbe vor Kummer,", "tokens": ["Ich", "ster\u00b7be", "vor", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der ewige Schlummer", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Schlum\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Entgeistert die Brust.", "tokens": ["Ent\u00b7geis\u00b7tert", "die", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich liebte von Herzen,", "tokens": ["Ich", "lieb\u00b7te", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich lebte mit Schmerzen,", "tokens": ["Ich", "leb\u00b7te", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich sterbe mit Lust.", "tokens": ["Ich", "ster\u00b7be", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}