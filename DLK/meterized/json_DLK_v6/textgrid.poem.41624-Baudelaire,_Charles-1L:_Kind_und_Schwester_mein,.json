{"textgrid.poem.41624": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kind und Schwester mein,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind und Schwester mein,", "tokens": ["Kind", "und", "Schwes\u00b7ter", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "K\u00f6nnten dort wir sein,", "tokens": ["K\u00f6nn\u00b7ten", "dort", "wir", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo das Leben s\u00fc\u00df uns und reich ist!", "tokens": ["Wo", "das", "Le\u00b7ben", "s\u00fc\u00df", "uns", "und", "reich", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Nichts als Liebe sehn,", "tokens": ["Nichts", "als", "Lie\u00b7be", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Lieben und Vergehn", "tokens": ["Lie\u00b7ben", "und", "Ver\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Im Lande, das dir gleich ist!", "tokens": ["Im", "Lan\u00b7de", ",", "das", "dir", "gleich", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Tr\u00fcber Sonnen Licht,", "tokens": ["Tr\u00fc\u00b7ber", "Son\u00b7nen", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das durch Schleier bricht,", "tokens": ["Das", "durch", "Schlei\u00b7er", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Gleicht meinem z\u00e4rtlichen Sehnen,", "tokens": ["Gleicht", "mei\u00b7nem", "z\u00e4rt\u00b7li\u00b7chen", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wann wunderbar", "tokens": ["Wann", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Dein Augenpaar", "tokens": ["Dein", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Verr\u00e4terisch leuchtet durch Tr\u00e4nen.", "tokens": ["Ver\u00b7r\u00e4\u00b7te\u00b7risch", "leuch\u00b7tet", "durch", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Dort schaust nur Lust und Sch\u00f6nheit du,", "tokens": ["Dort", "schaust", "nur", "Lust", "und", "Sch\u00f6n\u00b7heit", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anmut, Pracht und tiefe Ruh.", "tokens": ["An\u00b7mut", ",", "Pracht", "und", "tie\u00b7fe", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leuchtend Hausger\u00e4t", "tokens": ["Leuch\u00b7tend", "Haus\u00b7ge\u00b7r\u00e4t"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Uns im Saale steht,", "tokens": ["Uns", "im", "Saa\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Versch\u00f6nt von entschwundenen Jahren.", "tokens": ["Ver\u00b7sch\u00f6nt", "von", "ent\u00b7schwun\u00b7de\u00b7nen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Seltner Blumen Duft", "tokens": ["Selt\u00b7ner", "Blu\u00b7men", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Will der s\u00fc\u00dfen Luft", "tokens": ["Will", "der", "s\u00fc\u00b7\u00dfen", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der Ambrawolken sich paaren.", "tokens": ["Der", "Am\u00b7bra\u00b7wol\u00b7ken", "sich", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Gew\u00f6lbe Pracht,", "tokens": ["Der", "Ge\u00b7w\u00f6l\u00b7be", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Tiefer Spiegel Nacht,", "tokens": ["Tie\u00b7fer", "Spie\u00b7gel", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Des Ostens reiches Gepr\u00e4nge,", "tokens": ["Des", "Os\u00b7tens", "rei\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Alles spr\u00e4che dort", "tokens": ["Al\u00b7les", "spr\u00e4\u00b7che", "dort"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "In fl\u00fcsterndem Wort", "tokens": ["In", "fl\u00fcs\u00b7tern\u00b7dem", "Wort"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Seiner Heimat liebliche Kl\u00e4nge.", "tokens": ["Sei\u00b7ner", "Hei\u00b7mat", "lieb\u00b7li\u00b7che", "Kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort schaust nur Lust und Sch\u00f6nheit du,", "tokens": ["Dort", "schaust", "nur", "Lust", "und", "Sch\u00f6n\u00b7heit", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anmut, Pracht und tiefe Ruh.", "tokens": ["An\u00b7mut", ",", "Pracht", "und", "tie\u00b7fe", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, wie auf der Flut", "tokens": ["Sieh", ",", "wie", "auf", "der", "Flut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+---+", "measure": "dactylic.init"}, "line.2": {"text": "Schiff an Schiff dort ruht,", "tokens": ["Schiff", "an", "Schiff", "dort", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die rastlos fernher geschwommen.", "tokens": ["Die", "rast\u00b7los", "fern\u00b7her", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Zu erf\u00fcllen dir", "tokens": ["Zu", "er\u00b7f\u00fcl\u00b7len", "dir"], "token_info": ["word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jegliche Begier,", "tokens": ["Jeg\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.6": {"text": "Sind vom Ende der Welt sie gekommen.", "tokens": ["Sind", "vom", "En\u00b7de", "der", "Welt", "sie", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Des Abendlichts Glut", "tokens": ["Des", "A\u00b7bend\u00b7lichts", "Glut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ergie\u00dft auf die Flut,", "tokens": ["Er\u00b7gie\u00dft", "auf", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Auf die Stadt in dem Flurenkranze,", "tokens": ["Auf", "die", "Stadt", "in", "dem", "Flu\u00b7ren\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Hyazinthenen Schein;", "tokens": ["Hy\u00b7a\u00b7zin\u00b7the\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Die Welt schl\u00e4ft ein", "tokens": ["Die", "Welt", "schl\u00e4ft", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "In warmem goldenem Glanze.", "tokens": ["In", "war\u00b7mem", "gol\u00b7de\u00b7nem", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dort schaust nur Lust und Sch\u00f6nheit du,", "tokens": ["Dort", "schaust", "nur", "Lust", "und", "Sch\u00f6n\u00b7heit", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anmut, Pracht und tiefe Ruh.", "tokens": ["An\u00b7mut", ",", "Pracht", "und", "tie\u00b7fe", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}