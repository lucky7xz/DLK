{"textgrid.poem.48898": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Marforius", "genre": "verse", "period": "N.A.", "pub_year": 1754, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marforius fand allen Sachen M\u00e4ngel,", "tokens": ["Mar\u00b7fo\u00b7ri\u00b7us", "fand", "al\u00b7len", "Sa\u00b7chen", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er l\u00e4sterte Gott, Engel und Erzengel,", "tokens": ["Er", "l\u00e4s\u00b7ter\u00b7te", "Gott", ",", "En\u00b7gel", "und", "Er\u00b7zen\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schalt darauf mit leichter M\u00fch", "tokens": ["Und", "schalt", "da\u00b7rauf", "mit", "leich\u00b7ter", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das menschliche Geschlecht, und das Geschlecht vom Vieh.", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7schlecht", ",", "und", "das", "Ge\u00b7schlecht", "vom", "Vieh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Er schalt das Lamm, den Hund, das Crocodill \u2013 \u2013 \u2013 \u2013", "tokens": ["Er", "schalt", "das", "Lamm", ",", "den", "Hund", ",", "das", "Cro\u00b7co\u00b7dill", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Esel nur und Affen schwieg er still.", "tokens": ["Vom", "E\u00b7sel", "nur", "und", "Af\u00b7fen", "schwieg", "er", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}