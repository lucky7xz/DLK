{"dta.poem.4376": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mittel l\u00e4nger und vergn\u00fcgt  \n zu leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder Tag, jedwede Stunde,", "tokens": ["Ein", "je\u00b7der", "Tag", ",", "jed\u00b7we\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja eine jegliche Secunde,", "tokens": ["Ja", "ei\u00b7ne", "jeg\u00b7li\u00b7che", "Se\u00b7cun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind Theile meiner Lebens-Zeit.", "tokens": ["Sind", "Thei\u00b7le", "mei\u00b7ner", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nun, zum Beyspiel, unsre Augen,", "tokens": ["Wenn", "nun", ",", "zum", "Bey\u00b7spiel", ",", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit n\u00f6htiger Aufmerksamkeit,", "tokens": ["Mit", "n\u00f6h\u00b7ti\u00b7ger", "Auf\u00b7merk\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Dinge Vollenkommenheit", "tokens": ["Der", "Din\u00b7ge", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur einzeln zu betrachten taugen,", "tokens": ["Nur", "ein\u00b7zeln", "zu", "be\u00b7trach\u00b7ten", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wie ein jedes Ding dem Geist", "tokens": ["Und", "wie", "ein", "je\u00b7des", "Ding", "dem", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Am deutlichsten sich St\u00fcck-weis weist;", "tokens": ["Am", "deut\u00b7lichs\u00b7ten", "sich", "St\u00fc\u00b7ck\u00b7weis", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PRF", "NN", "VVFIN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "So la\u00dft uns, ehe sie vergehen,", "tokens": ["So", "la\u00dft", "uns", ",", "e\u00b7he", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von unsrer Daur, die Theile sehn,", "tokens": ["Von", "uns\u00b7rer", "Daur", ",", "die", "Thei\u00b7le", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und uns vern\u00fcnftig oft bem\u00fchn,", "tokens": ["Und", "uns", "ver\u00b7n\u00fcnf\u00b7tig", "oft", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den Aneinanderhang der Zeit,", "tokens": ["Den", "An\u00b7ein\u00b7an\u00b7der\u00b7hang", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In welchem wir von hinnen eilen,", "tokens": ["In", "wel\u00b7chem", "wir", "von", "hin\u00b7nen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Getheilet, in Betracht zu ziehn,", "tokens": ["Ge\u00b7thei\u00b7let", ",", "in", "Be\u00b7tracht", "zu", "ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und unsre Dauer einzutheilen.", "tokens": ["Und", "uns\u00b7re", "Dau\u00b7er", "ein\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zumahl wenn, voller Heiterkeit,", "tokens": ["Zu\u00b7mahl", "wenn", ",", "vol\u00b7ler", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein sch\u00f6ner Tag uns oft erfreut,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Tag", "uns", "oft", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Jmgleichen, mit gestirnter Pracht,", "tokens": ["Jm\u00b7glei\u00b7chen", ",", "mit", "ge\u00b7stirn\u00b7ter", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein\u2019 angenehme sch\u00f6ne Nacht;", "tokens": ["Ein'", "an\u00b7ge\u00b7neh\u00b7me", "sch\u00f6\u00b7ne", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So w\u00fcnsch ich, da\u00df man \u00f6fters denke:", "tokens": ["So", "w\u00fcnsch", "ich", ",", "da\u00df", "man", "\u00f6f\u00b7ters", "den\u00b7ke", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er (sie) ist ein Theil von meiner Zeit, er (sie) ist", "tokens": ["Er", "(", "sie", ")", "ist", "ein", "Theil", "von", "mei\u00b7ner", "Zeit", ",", "er", "(", "sie", ")", "ist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$(", "PPER", "$(", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "$(", "PPER", "$(", "VAFIN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.23": {"text": "ein g\u00f6ttliches Geschenke.", "tokens": ["ein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7schen\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Je \u00f6fter man nun die Gedanken,", "tokens": ["Je", "\u00f6f\u00b7ter", "man", "nun", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "In Stund- und Viertel-Stunden-Schranken,", "tokens": ["In", "Stun\u00b7d", "und", "Vier\u00b7tel\u00b7Stun\u00b7den\u00b7Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}