{"textgrid.poem.25861": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[das Laub, das im Sommer so rauschend sang]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Laub, das im Sommer so rauschend sang,", "tokens": ["Das", "Laub", ",", "das", "im", "Som\u00b7mer", "so", "rau\u00b7schend", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Laub ist von den B\u00e4umen gestiegen.", "tokens": ["Das", "Laub", "ist", "von", "den", "B\u00e4u\u00b7men", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Voll stiller Bl\u00e4tter, gelb und braun,", "tokens": ["Voll", "stil\u00b7ler", "Bl\u00e4t\u00b7ter", ",", "gelb", "und", "braun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegen noch stiller die stillen Wege.", "tokens": ["Lie\u00b7gen", "noch", "stil\u00b7ler", "die", "stil\u00b7len", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie Duft von tausend K\u00fcssen und Tr\u00e4nen", "tokens": ["Wie", "Duft", "von", "tau\u00b7send", "K\u00fcs\u00b7sen", "und", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "CARD", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schweben N\u00e4ssen \u00fcber den Bl\u00e4ttern,", "tokens": ["Schwe\u00b7ben", "N\u00e4s\u00b7sen", "\u00fc\u00b7ber", "den", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00dcber den tausend herben Bl\u00e4ttern,", "tokens": ["\u00dc\u00b7ber", "den", "tau\u00b7send", "her\u00b7ben", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die nun sterben.", "tokens": ["Die", "nun", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}