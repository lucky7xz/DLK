{"textgrid.poem.36584": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amors Irrtum", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor sah die Doris schlafen;", "tokens": ["A\u00b7mor", "sah", "die", "Do\u00b7ris", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehend unter ihren Schafen,", "tokens": ["Ste\u00b7hend", "un\u00b7ter", "ih\u00b7ren", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah er ihrem Schlafe zu!", "tokens": ["Sah", "er", "ih\u00b7rem", "Schla\u00b7fe", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dick Geb\u00fcsch hielt mich verborgen,", "tokens": ["Dick", "Ge\u00b7b\u00fcsch", "hielt", "mich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutter, sprach er, guten Morgen!", "tokens": ["Mut\u00b7ter", ",", "sprach", "er", ",", "gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie so ruhig schliefest du!", "tokens": ["Wie", "so", "ru\u00b7hig", "schlie\u00b7fest", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doris, wach, erblickt den Knaben,", "tokens": ["Do\u00b7ris", ",", "wach", ",", "er\u00b7blickt", "den", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind, spricht sie, was willst du haben?", "tokens": ["Kind", ",", "spricht", "sie", ",", "was", "willst", "du", "ha\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was hast du hier zu thun?", "tokens": ["Und", "was", "hast", "du", "hier", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier ist nichts f\u00fcr dich zu spielen;", "tokens": ["Hier", "ist", "nichts", "f\u00fcr", "dich", "zu", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich, Kindchen! hier im K\u00fchlen", "tokens": ["La\u00df", "mich", ",", "Kind\u00b7chen", "!", "hier", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$.", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df mich noch ein wenig ruhn!", "tokens": ["La\u00df", "mich", "noch", "ein", "we\u00b7nig", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Amor, n\u00e4her tretend, siehet", "tokens": ["A\u00b7mor", ",", "n\u00e4\u00b7her", "tre\u00b7tend", ",", "sie\u00b7het"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJD", "VVPP", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Irrtum, stutzt und fliehet", "tokens": ["Sei\u00b7nen", "Irr\u00b7tum", ",", "stutzt", "und", "flie\u00b7het"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit err\u00f6tetem Gesicht!", "tokens": ["Mit", "er\u00b7r\u00f6\u00b7te\u00b7tem", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich erg\u00f6tzte mich dar\u00fcber,", "tokens": ["Ich", "er\u00b7g\u00f6tz\u00b7te", "mich", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor, rief ich, kleiner Lieber,", "tokens": ["A\u00b7mor", ",", "rief", "ich", ",", "klei\u00b7ner", "Lie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fliehen willst du? fliehe nicht!", "tokens": ["Flie\u00b7hen", "willst", "du", "?", "flie\u00b7he", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}