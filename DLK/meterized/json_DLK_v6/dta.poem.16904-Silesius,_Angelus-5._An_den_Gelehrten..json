{"dta.poem.16904": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "5.  An den Gelehrten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00fcbelst in der Schrifft/ und meinst mit Kl\u00fcgeley", "tokens": ["Du", "gr\u00fc\u00b7belst", "in", "der", "Schrifft", "/", "und", "meinst", "mit", "Kl\u00fc\u00b7ge\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu finden GOttes Sohn: Ach mache dich doch frey", "tokens": ["Zu", "fin\u00b7den", "Got\u00b7tes", "Sohn", ":", "Ach", "ma\u00b7che", "dich", "doch", "frey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "NN", "NN", "$.", "ITJ", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von diser Sucht/ und kom\u0304 i", "tokens": ["Von", "di\u00b7ser", "Sucht", "/", "und", "kom\u0304", "i"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$(", "KON", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wirstu bald der Krafft de\u00df wehrten Kinds geniessen.", "tokens": ["So", "wirs\u00b7tu", "bald", "der", "Krafft", "de\u00df", "wehr\u00b7ten", "Kinds", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-