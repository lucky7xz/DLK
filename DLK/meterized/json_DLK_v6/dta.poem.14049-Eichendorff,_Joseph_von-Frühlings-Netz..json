{"dta.poem.14049": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-Netz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So s\u00fc\u00dfe Zauberei ist los,", "tokens": ["So", "s\u00fc\u00b7\u00dfe", "Zau\u00b7be\u00b7rei", "ist", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wunderbare Lieder", "tokens": ["Und", "wun\u00b7der\u00b7ba\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geh'n durch der Erde Fr\u00fchlingsschoo\u00df,", "tokens": ["Geh'n", "durch", "der", "Er\u00b7de", "Fr\u00fch\u00b7lings\u00b7schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lassen ihn nicht wieder.", "tokens": ["Die", "las\u00b7sen", "ihn", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}