{"dta.poem.11752": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ist der schlechteste des menschlichen Geschlechtes,", "tokens": ["Der", "ist", "der", "schlech\u00b7tes\u00b7te", "des", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer selbst nichts rechtes wei\u00df, noch lernen will was rechtes.", "tokens": ["Wer", "selbst", "nichts", "rech\u00b7tes", "wei\u00df", ",", "noch", "ler\u00b7nen", "will", "was", "rech\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "ADJA", "VVFIN", "$,", "ADV", "VVINF", "VMFIN", "PIS", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer ist der beste? der hervor das Gute bringt", "tokens": ["Wer", "ist", "der", "bes\u00b7te", "?", "der", "her\u00b7vor", "das", "Gu\u00b7te", "bringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "$.", "ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus eigner Kraft, und nicht von au\u00dfen es erringt.", "tokens": ["Aus", "eig\u00b7ner", "Kraft", ",", "und", "nicht", "von", "au\u00b7\u00dfen", "es", "er\u00b7ringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PTKNEG", "APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ist zu loben, wer, was er nicht selbst vermag", "tokens": ["Doch", "ist", "zu", "lo\u00b7ben", ",", "wer", ",", "was", "er", "nicht", "selbst", "ver\u00b7mag"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKZU", "VVINF", "$,", "PWS", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu tragen, das erwirbt von fremdem Fruchtertrag.", "tokens": ["Zu", "tra\u00b7gen", ",", "das", "er\u00b7wirbt", "von", "frem\u00b7dem", "Fruch\u00b7ter\u00b7trag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es steht ein Baum im Wald und tr\u00e4gt die eigne Frucht,", "tokens": ["Es", "steht", "ein", "Baum", "im", "Wald", "und", "tr\u00e4gt", "die", "eig\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die so ihm gn\u00fcgt, da\u00df er nach keiner fremden sucht.", "tokens": ["Die", "so", "ihm", "gn\u00fcgt", ",", "da\u00df", "er", "nach", "kei\u00b7ner", "frem\u00b7den", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Daneben steht ein Baum, der ist nicht eigenfr\u00fcchtig;", "tokens": ["Da\u00b7ne\u00b7ben", "steht", "ein", "Baum", ",", "der", "ist", "nicht", "ei\u00b7gen\u00b7fr\u00fcch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der reiche Nachbar macht den armen eifers\u00fcchtig.", "tokens": ["Der", "rei\u00b7che", "Nach\u00b7bar", "macht", "den", "ar\u00b7men", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Soll er die Frucht von ihm zu sich her\u00fcber nehmen?", "tokens": ["Soll", "er", "die", "Frucht", "von", "ihm", "zu", "sich", "her\u00b7\u00fc\u00b7ber", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPER", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ers auch k\u00f6nnte, m\u00fc\u00dft' er sich des Diebstals sch\u00e4men.", "tokens": ["Wenn", "ers", "auch", "k\u00f6nn\u00b7te", ",", "m\u00fc\u00dft'", "er", "sich", "des", "Diebs\u00b7tals", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "$,", "VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Glut der Eifersucht brennt ihm sein Innres hol,", "tokens": ["Die", "Glut", "der", "Ei\u00b7fer\u00b7sucht", "brennt", "ihm", "sein", "Inn\u00b7res", "hol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und desto minder tr\u00e4gt er aus sich Frucht nun wol.", "tokens": ["Und", "des\u00b7to", "min\u00b7der", "tr\u00e4gt", "er", "aus", "sich", "Frucht", "nun", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "PRF", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Seht, wie zu nutzen er den Schaden selber wei\u00df,", "tokens": ["Seht", ",", "wie", "zu", "nut\u00b7zen", "er", "den", "Scha\u00b7den", "sel\u00b7ber", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PTKZU", "VVINF", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er l\u00e4dt in seine Kluft des Bienenschwarmes Flei\u00df.", "tokens": ["Er", "l\u00e4dt", "in", "sei\u00b7ne", "Kluft", "des", "Bie\u00b7nen\u00b7schwar\u00b7mes", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sein Innres r\u00e4umet er zur Wohnung willig ihnen,", "tokens": ["Sein", "Inn\u00b7res", "r\u00e4u\u00b7met", "er", "zur", "Woh\u00b7nung", "wil\u00b7lig", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freudig lohnen's ihm die arbeitsamen Bienen.", "tokens": ["Und", "freu\u00b7dig", "loh\u00b7nen's", "ihm", "die", "ar\u00b7beit\u00b7sa\u00b7men", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie tragen Honig her, und nicht vom Nachbar nur,", "tokens": ["Sie", "tra\u00b7gen", "Ho\u00b7nig", "her", ",", "und", "nicht", "vom", "Nach\u00b7bar", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KON", "PTKNEG", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie tragen rings ihn bei aus Berg und Wald und Flur.", "tokens": ["Sie", "tra\u00b7gen", "rings", "ihn", "bei", "aus", "Berg", "und", "Wald", "und", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Des goldnen Seimes voll wird jeder leere Raum,", "tokens": ["Des", "gold\u00b7nen", "Sei\u00b7mes", "voll", "wird", "je\u00b7der", "lee\u00b7re", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer fruchtbar ist der unfruchtbare Baum.", "tokens": ["Und", "im\u00b7mer", "frucht\u00b7bar", "ist", "der", "un\u00b7frucht\u00b7ba\u00b7re", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}