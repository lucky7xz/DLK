{"textgrid.poem.59597": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df ist Gott!", "tokens": ["Gro\u00df", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Es klingt sein m\u00e4chtig Werde,", "tokens": ["Es", "klingt", "sein", "m\u00e4ch\u00b7tig", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Himmel w\u00f6lben sich,", "tokens": ["Die", "Him\u00b7mel", "w\u00f6l\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Blumen steigt empor die Erde,", "tokens": ["Mit", "Blu\u00b7men", "steigt", "em\u00b7por", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tiefe setzet sich!", "tokens": ["Die", "Tie\u00b7fe", "set\u00b7zet", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Jubelklang der Sterne,", "tokens": ["Der", "Ju\u00b7bel\u00b7klang", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Des Menschen froher Lobgesang", "tokens": ["Des", "Men\u00b7schen", "fro\u00b7her", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "T\u00f6nt durch die N\u00e4he, durch die Ferne", "tokens": ["T\u00f6nt", "durch", "die", "N\u00e4\u00b7he", ",", "durch", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den hohen Klang:", "tokens": ["Den", "ho\u00b7hen", "Klang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Gro\u00df ist Gott, unendlich gro\u00df!", "tokens": ["Gro\u00df", "ist", "Gott", ",", "un\u00b7end\u00b7lich", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allm\u00e4chtig ist Gott!", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Er hebet aus dem Staube,", "tokens": ["Er", "he\u00b7bet", "aus", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was niedrig ist und klein,", "tokens": ["Was", "nied\u00b7rig", "ist", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verwehet gleich dem Sand, dem Laube,", "tokens": ["Ver\u00b7we\u00b7het", "gleich", "dem", "Sand", ",", "dem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was waget frech zu sein,", "tokens": ["Was", "wa\u00b7get", "frech", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er schl\u00e4gt die schn\u00f6den Sp\u00f6tter,", "tokens": ["Er", "schl\u00e4gt", "die", "schn\u00f6\u00b7den", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr stolzer Mut wird eitel Nichts,", "tokens": ["Ihr", "stol\u00b7zer", "Mut", "wird", "ei\u00b7tel", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist der h\u00f6chste Gott der G\u00f6tter,", "tokens": ["Er", "ist", "der", "h\u00f6chs\u00b7te", "Gott", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Gott des Lichts.", "tokens": ["Der", "Gott", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Allm\u00e4chtig ist Gott, der starke Gott.", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "Gott", ",", "der", "star\u00b7ke", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Allm\u00e4chtig ist Gott!", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Es zog in wilden Haufen", "tokens": ["Es", "zog", "in", "wil\u00b7den", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00f6lle trotzig aus,", "tokens": ["Die", "H\u00f6l\u00b7le", "trot\u00b7zig", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie prahlte: Meine Rosse saufen", "tokens": ["Sie", "prahl\u00b7te", ":", "Mei\u00b7ne", "Ros\u00b7se", "sau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die tiefsten Str\u00f6me aus;", "tokens": ["Die", "tiefs\u00b7ten", "Str\u00f6\u00b7me", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie prahlte: Meinen Scharen", "tokens": ["Sie", "prahl\u00b7te", ":", "Mei\u00b7nen", "Scha\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind Meer und L\u00e4nder untertan.", "tokens": ["Sind", "Meer", "und", "L\u00e4n\u00b7der", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da kam der Himmelsheld gefahren \u2013", "tokens": ["Da", "kam", "der", "Him\u00b7mels\u00b7held", "ge\u00b7fah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es lag ihr Wahn.", "tokens": ["Es", "lag", "ihr", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Allm\u00e4chtig ist Gott, der starke Gott.", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "Gott", ",", "der", "star\u00b7ke", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Barmherzig ist Gott!", "tokens": ["Barm\u00b7her\u00b7zig", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Er hat den Feind geschlagen,", "tokens": ["Er", "hat", "den", "Feind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat mir Sieg verliehn;", "tokens": ["Er", "hat", "mir", "Sieg", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was trotzig war, das mu\u00df verzagen,", "tokens": ["Was", "trot\u00b7zig", "war", ",", "das", "mu\u00df", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was schwach war, das ist k\u00fchn;", "tokens": ["Was", "schwach", "war", ",", "das", "ist", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er bricht die schweren Bande,", "tokens": ["Er", "bricht", "die", "schwe\u00b7ren", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er tilgt die fremde Tyrannei,", "tokens": ["Er", "tilgt", "die", "frem\u00b7de", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er rettet mich von Trug und Schande,", "tokens": ["Er", "ret\u00b7tet", "mich", "von", "Trug", "und", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er macht mich frei.", "tokens": ["Er", "macht", "mich", "frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Barmherzig ist Gott, der fromme Gott.", "tokens": ["Barm\u00b7her\u00b7zig", "ist", "Gott", ",", "der", "from\u00b7me", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Danket Gott!", "tokens": ["Dan\u00b7ket", "Gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Er will sich gn\u00e4dig weisen,", "tokens": ["Er", "will", "sich", "gn\u00e4\u00b7dig", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchret unsern Krieg;", "tokens": ["Er", "f\u00fch\u00b7ret", "un\u00b7sern", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht unsre H\u00e4nde, unsre Eisen:", "tokens": ["Nicht", "uns\u00b7re", "H\u00e4n\u00b7de", ",", "uns\u00b7re", "Ei\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Herr gab uns den Sieg.", "tokens": ["Der", "Herr", "gab", "uns", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum fallet fr\u00f6hlich nieder", "tokens": ["Drum", "fal\u00b7let", "fr\u00f6h\u00b7lich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und singt ihm lauten Lobgesang!", "tokens": ["Und", "singt", "ihm", "lau\u00b7ten", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch alle Herzen klinge wieder", "tokens": ["Durch", "al\u00b7le", "Her\u00b7zen", "klin\u00b7ge", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der hohe Klang:", "tokens": ["Der", "ho\u00b7he", "Klang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Gro\u00df ist Gott, unendlich gro\u00df!", "tokens": ["Gro\u00df", "ist", "Gott", ",", "un\u00b7end\u00b7lich", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}