{"textgrid.poem.58835": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "54. Siehe zu, wie liebest du!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Herzens Lieb' sei Gott verschrieben,", "tokens": ["Des", "Her\u00b7zens", "Lieb'", "sei", "Gott", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein innrer Grund in Ewigkeit;", "tokens": ["Dein", "inn\u00b7rer", "Grund", "in", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was du sonst liebst, mu\u00dft du nur lieben", "tokens": ["Was", "du", "sonst", "liebst", ",", "mu\u00dft", "du", "nur", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In h\u00f6chster Abgeschiedenheit.", "tokens": ["In", "h\u00f6chs\u00b7ter", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}