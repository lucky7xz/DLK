{"textgrid.poem.24279": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Erde, liebe Erde ...", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Bl\u00fcte im Mai", "tokens": ["Wie", "ei\u00b7ne", "Bl\u00fc\u00b7te", "im", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bl\u00e4ttert sich auf der Tag,", "tokens": ["Bl\u00e4t\u00b7tert", "sich", "auf", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zeigt seine nackende Sch\u00f6nheit der Sonne.", "tokens": ["Zeigt", "sei\u00b7ne", "na\u00b7cken\u00b7de", "Sch\u00f6n\u00b7heit", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sehen, o zaubrisches Gl\u00fcck! Gottselige Wonne,", "tokens": ["Se\u00b7hen", ",", "o", "zaub\u00b7ri\u00b7sches", "Gl\u00fcck", "!", "Gott\u00b7se\u00b7li\u00b7ge", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "ADJA", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Dies Atmen! Der Herzensschlag!", "tokens": ["Dies", "At\u00b7men", "!", "Der", "Her\u00b7zens\u00b7schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schmerzen und L\u00fcste herbei!", "tokens": ["Schmer\u00b7zen", "und", "L\u00fcs\u00b7te", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will euch ans Herz nehmen, ans Herz dr\u00fccken;", "tokens": ["Ich", "will", "euch", "ans", "Herz", "neh\u00b7men", ",", "ans", "Herz", "dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+---+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dornen und Dolche sollen mich entz\u00fccken:", "tokens": ["Dor\u00b7nen", "und", "Dol\u00b7che", "sol\u00b7len", "mich", "ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Alles was ist, ist sch\u00f6n und recht.", "tokens": ["Al\u00b7les", "was", "ist", ",", "ist", "sch\u00f6n", "und", "recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VAFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Erde, liebe Erde, ich bin dein Knecht.", "tokens": ["Er\u00b7de", ",", "lie\u00b7be", "Er\u00b7de", ",", "ich", "bin", "dein", "Knecht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}