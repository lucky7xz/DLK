{"textgrid.poem.49090": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "49. Die drei Augen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Angesicht des Menschen stehn", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "des", "Men\u00b7schen", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Augenlichter zwei;", "tokens": ["Der", "Au\u00b7gen\u00b7lich\u00b7ter", "zwei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eins strahlet in der tiefen Brust,", "tokens": ["Eins", "strah\u00b7let", "in", "der", "tie\u00b7fen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so sind's ihrer drei.", "tokens": ["Und", "so", "sin\u00b7d's", "ih\u00b7rer", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erlischt eins von den \u00e4u\u00dfern, hilft", "tokens": ["Er\u00b7lischt", "eins", "von", "den", "\u00e4u\u00b7\u00dfern", ",", "hilft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das andre aus der Noth;", "tokens": ["Das", "and\u00b7re", "aus", "der", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch geht das innre aus, dann ist", "tokens": ["Doch", "geht", "das", "inn\u00b7re", "aus", ",", "dann", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mensch lebendig todt.", "tokens": ["Der", "Mensch", "le\u00b7ben\u00b7dig", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ihm helfen die zwei \u00e4u\u00dfern nicht,", "tokens": ["Ihm", "hel\u00b7fen", "die", "zwei", "\u00e4u\u00b7\u00dfern", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "CARD", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So hell sie auch und klar;", "tokens": ["So", "hell", "sie", "auch", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Blind bleibt er, reichte Argus auch", "tokens": ["Blind", "bleibt", "er", ",", "reich\u00b7te", "Ar\u00b7gus", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm hundert Augen dar!", "tokens": ["Ihm", "hun\u00b7dert", "Au\u00b7gen", "dar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}