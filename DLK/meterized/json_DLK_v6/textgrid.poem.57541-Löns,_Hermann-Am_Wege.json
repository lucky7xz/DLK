{"textgrid.poem.57541": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Am Wege", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blickst du so bittend und sch\u00fcchtern mich an,", "tokens": ["Was", "blickst", "du", "so", "bit\u00b7tend", "und", "sch\u00fcch\u00b7tern", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du rosige, bl\u00fchende Maid,", "tokens": ["Du", "ro\u00b7si\u00b7ge", ",", "bl\u00fc\u00b7hen\u00b7de", "Maid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gef\u00e4llt dir der bleiche, weltfahrende Mann,", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "der", "blei\u00b7che", ",", "welt\u00b7fah\u00b7ren\u00b7de", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "PWAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So komm' nur, ich r\u00fccke zur Seit!", "tokens": ["So", "komm'", "nur", ",", "ich", "r\u00fc\u00b7cke", "zur", "Seit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Nein bleib \u2013 was ich sagte, es war nur ein Scherz,", "tokens": ["Nein", "bleib", "\u2013", "was", "ich", "sag\u00b7te", ",", "es", "war", "nur", "ein", "Scherz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Zu schade auch w\u00fcrd's um dich sein \u2013", "tokens": ["Zu", "scha\u00b7de", "auch", "w\u00fcrd's", "um", "dich", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "VAFIN", "APPR", "PPER", "VAINF", "$("], "meter": "-+--+-++", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bin ein wilder Gesell, hab' ein treuloses Herz", "tokens": ["Bin", "ein", "wil\u00b7der", "Ge\u00b7sell", ",", "hab'", "ein", "treu\u00b7lo\u00b7ses", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und du bist so hold und so rein.", "tokens": ["Und", "du", "bist", "so", "hold", "und", "so", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du willst es nicht glauben, du sch\u00fcttelst dein Haupt", "tokens": ["Du", "willst", "es", "nicht", "glau\u00b7ben", ",", "du", "sch\u00fct\u00b7telst", "dein", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und l\u00e4chelst mir Glut in die Brust,", "tokens": ["Und", "l\u00e4\u00b7chelst", "mir", "Glut", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich habe schon mancher das Kr\u00e4nzchen geraubt,", "tokens": ["Ich", "ha\u00b7be", "schon", "man\u00b7cher", "das", "Kr\u00e4nz\u00b7chen", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hab' niemals von Reue gewu\u00dft.", "tokens": ["Hab'", "nie\u00b7mals", "von", "Reu\u00b7e", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Was blickst du so schmachtend, ich bin nicht von Erz,", "tokens": ["Was", "blickst", "du", "so", "schmach\u00b7tend", ",", "ich", "bin", "nicht", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hab' hei\u00dfes, unchristliches Blut \u2013", "tokens": ["Hab'", "hei\u00b7\u00dfes", ",", "un\u00b7christ\u00b7li\u00b7ches", "Blut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Bin ein wilder Gesell, hab' ein treuloses Herz", "tokens": ["Bin", "ein", "wil\u00b7der", "Ge\u00b7sell", ",", "hab'", "ein", "treu\u00b7lo\u00b7ses", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und du bist so fromm und so gut.", "tokens": ["Und", "du", "bist", "so", "fromm", "und", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Du blickst mir ins Auge so innig und warm,", "tokens": ["Du", "blickst", "mir", "ins", "Au\u00b7ge", "so", "in\u00b7nig", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein Puls jagt, h\u00e4mmert so laut,", "tokens": ["Mein", "Puls", "jagt", ",", "h\u00e4m\u00b7mert", "so", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich k\u00fcsse dir Lippen und Busen und Arm,", "tokens": ["Ich", "k\u00fcs\u00b7se", "dir", "Lip\u00b7pen", "und", "Bu\u00b7sen", "und", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du herzige, knospige Braut.", "tokens": ["Du", "her\u00b7zi\u00b7ge", ",", "knos\u00b7pi\u00b7ge", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Was blickst du so sch\u00fcchtern jetzt erdbodenw\u00e4rts,", "tokens": ["Was", "blickst", "du", "so", "sch\u00fcch\u00b7tern", "jetzt", "erd\u00b7bo\u00b7den\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zu sp\u00e4t ist's, mein Blut st\u00fcrmisch rollt \u2013", "tokens": ["Zu", "sp\u00e4t", "ist's", ",", "mein", "Blut", "st\u00fcr\u00b7misch", "rollt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bin ein wilder Gesell, hab' ein treuloses Herz,", "tokens": ["Bin", "ein", "wil\u00b7der", "Ge\u00b7sell", ",", "hab'", "ein", "treu\u00b7lo\u00b7ses", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Was weinst du? du hast's ja gewollt.", "tokens": ["Was", "weinst", "du", "?", "du", "hast's", "ja", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}