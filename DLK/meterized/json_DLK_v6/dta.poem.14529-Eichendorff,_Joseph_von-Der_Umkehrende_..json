{"dta.poem.14529": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was g\u00e4b' es doch auf Erden,", "tokens": ["Was", "g\u00e4b'", "es", "doch", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hielt' den Jammer aus,", "tokens": ["Wer", "hielt'", "den", "Jam\u00b7mer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer m\u00f6cht' geboren werden,", "tokens": ["Wer", "m\u00f6cht'", "ge\u00b7bo\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hielt'st Du nicht droben Haus!", "tokens": ["Hielt'st", "Du", "nicht", "dro\u00b7ben", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}