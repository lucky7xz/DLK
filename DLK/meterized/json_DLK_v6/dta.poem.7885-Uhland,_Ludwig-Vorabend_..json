{"dta.poem.7885": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorabend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was streift vorbei im D\u00e4mmerlicht?", "tokens": ["Was", "streift", "vor\u00b7bei", "im", "D\u00e4m\u00b7mer\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War\u2019s nicht mein holdes Kind?", "tokens": ["Wa\u00b7r's", "nicht", "mein", "hol\u00b7des", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wehten aus dem K\u00f6rbchen nicht", "tokens": ["Und", "weh\u00b7ten", "aus", "dem", "K\u00f6rb\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rosend\u00fcfte lind?", "tokens": ["Die", "Ro\u00b7sen\u00b7d\u00fcf\u00b7te", "lind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja! morgen ist das Maienfest,", "tokens": ["Ja", "!", "mor\u00b7gen", "ist", "das", "Mai\u00b7en\u00b7fest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O morgen welche Lust!", "tokens": ["O", "mor\u00b7gen", "wel\u00b7che", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sie sich gl\u00e4nzend schauen l\u00e4\u00dft,", "tokens": ["Wann", "sie", "sich", "gl\u00e4n\u00b7zend", "schau\u00b7en", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die R\u00f6slein an der Brust.", "tokens": ["Die", "R\u00f6s\u00b7lein", "an", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}