{"textgrid.poem.33047": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur d\u00fcstre Wolken seh' ich geh'n und kommen,", "tokens": ["Nur", "d\u00fcst\u00b7re", "Wol\u00b7ken", "seh'", "ich", "geh'n", "und", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVFIN", "PPER", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ewig droht der Winter fortzuw\u00e4hren \u2013", "tokens": ["Und", "e\u00b7wig", "droht", "der", "Win\u00b7ter", "fort\u00b7zu\u00b7w\u00e4h\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele war so tr\u00fcb mir und beklommen,", "tokens": ["Die", "See\u00b7le", "war", "so", "tr\u00fcb", "mir", "und", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich rief den Fr\u00fchling, ach! er will nicht kommen,", "tokens": ["Ich", "rief", "den", "Fr\u00fch\u00b7ling", ",", "ach", "!", "er", "will", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ITJ", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie und des Himmels Stirne aufzukl\u00e4ren.", "tokens": ["Sie", "und", "des", "Him\u00b7mels", "Stir\u00b7ne", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und durch des Gartens G\u00e4nge dichtverschlungen", "tokens": ["Und", "durch", "des", "Gar\u00b7tens", "G\u00e4n\u00b7ge", "dicht\u00b7ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging ich \u2013 doch sieh, was hat sich dort begeben!", "tokens": ["Ging", "ich", "\u2013", "doch", "sieh", ",", "was", "hat", "sich", "dort", "be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "PTKVZ", "$,", "PWS", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schneegl\u00f6cklein sind der kalten Erd' entsprungen,", "tokens": ["Schnee\u00b7gl\u00f6c\u00b7klein", "sind", "der", "kal\u00b7ten", "Erd'", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sie haben siegend sich hervorgerungen,", "tokens": ["Sie", "ha\u00b7ben", "sie\u00b7gend", "sich", "her\u00b7vor\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erweckt von ", "tokens": ["Er\u00b7weckt", "von"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Nun stillt ihr, Fr\u00fchlingsboten, mein Verlangen!", "tokens": ["Nun", "stillt", "ihr", ",", "Fr\u00fch\u00b7lings\u00b7bo\u00b7ten", ",", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr woll't in's Herz mir neues Leben senken!", "tokens": ["Ihr", "woll't", "in's", "Herz", "mir", "neu\u00b7es", "Le\u00b7ben", "sen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie gl\u00e4ubig euer Kelch ist aufgegangen,", "tokens": ["Wie", "gl\u00e4u\u00b7big", "eu\u00b7er", "Kelch", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er der Sonne einz'gen Ku\u00df empfangen,", "tokens": ["Weil", "er", "der", "Son\u00b7ne", "einz'\u00b7gen", "Ku\u00df", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So soll mir Fr\u00fchling euer Anblick schenken!", "tokens": ["So", "soll", "mir", "Fr\u00fch\u00b7ling", "eu\u00b7er", "An\u00b7blick", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}