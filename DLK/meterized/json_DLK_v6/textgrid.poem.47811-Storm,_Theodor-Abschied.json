{"textgrid.poem.47811": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Wort, auch nicht das kleinste, kann ich sagen,", "tokens": ["Kein", "Wort", ",", "auch", "nicht", "das", "kleins\u00b7te", ",", "kann", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PTKNEG", "ART", "ADJA", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wozu das Herz den vollen Schlag verwehrt;", "tokens": ["Wo\u00b7zu", "das", "Herz", "den", "vol\u00b7len", "Schlag", "ver\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Stunde dr\u00e4ngt, ger\u00fcstet steht der Wagen,", "tokens": ["Die", "Stun\u00b7de", "dr\u00e4ngt", ",", "ge\u00b7r\u00fcs\u00b7tet", "steht", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist die Fahrt der Heimat abgekehrt.", "tokens": ["Es", "ist", "die", "Fahrt", "der", "Hei\u00b7mat", "ab\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geht immerhin \u2013 denn eure Tat ist euer \u2013", "tokens": ["Geht", "im\u00b7mer\u00b7hin", "\u2013", "denn", "eu\u00b7re", "Tat", "ist", "eu\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und widerruft, was einst das Herz gebot;", "tokens": ["Und", "wi\u00b7der\u00b7ruft", ",", "was", "einst", "das", "Herz", "ge\u00b7bot", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kauft, wenn dieser Preis euch nicht zu teuer,", "tokens": ["Und", "kauft", ",", "wenn", "die\u00b7ser", "Preis", "euch", "nicht", "zu", "teu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PDAT", "NN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Daf\u00fcr euch in der Heimat euer Brot!", "tokens": ["Da\u00b7f\u00fcr", "euch", "in", "der", "Hei\u00b7mat", "eu\u00b7er", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber kann des Landes nicht, des eignen,", "tokens": ["Ich", "a\u00b7ber", "kann", "des", "Lan\u00b7des", "nicht", ",", "des", "eig\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "PTKNEG", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Schmerz verstummte Klagen mi\u00dfverstehn;", "tokens": ["In", "Schmerz", "ver\u00b7stumm\u00b7te", "Kla\u00b7gen", "mi\u00df\u00b7ver\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann die stillen Gr\u00e4ber nicht verleugnen,", "tokens": ["Ich", "kann", "die", "stil\u00b7len", "Gr\u00e4\u00b7ber", "nicht", "ver\u00b7leug\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie tief sie jetzt in Unkraut auch vergehn.-", "tokens": ["Wie", "tief", "sie", "jetzt", "in", "Un\u00b7kraut", "auch", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "APPR", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du, deren zarte Augen mich befragen \u2013", "tokens": ["Du", ",", "de\u00b7ren", "zar\u00b7te", "Au\u00b7gen", "mich", "be\u00b7fra\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dich mir gab, gesegnet sei der Tag!", "tokens": ["Der", "dich", "mir", "gab", ",", "ge\u00b7seg\u00b7net", "sei", "der", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df nur dein Herz an meinem Herzen schlagen,", "tokens": ["La\u00df", "nur", "dein", "Herz", "an", "mei\u00b7nem", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zage nicht! Es ist derselbe Schlag.", "tokens": ["Und", "za\u00b7ge", "nicht", "!", "Es", "ist", "der\u00b7sel\u00b7be", "Schlag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es str\u00f6mt die Luft \u2013 die Knaben stehn und lauschen,", "tokens": ["Es", "str\u00f6mt", "die", "Luft", "\u2013", "die", "Kna\u00b7ben", "stehn", "und", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Strand her\u00fcber dringt ein M\u00f6wenschrei;", "tokens": ["Vom", "Strand", "her\u00b7\u00fc\u00b7ber", "dringt", "ein", "M\u00f6\u00b7wen\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist die Flut! Das ist des Meeres Rauschen!", "tokens": ["Das", "ist", "die", "Flut", "!", "Das", "ist", "des", "Mee\u00b7res", "Rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr kennt es wohl; wir waren oft dabei.", "tokens": ["Ihr", "kennt", "es", "wohl", ";", "wir", "wa\u00b7ren", "oft", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Von meinem Arm in dieser letzten Stunde", "tokens": ["Von", "mei\u00b7nem", "Arm", "in", "die\u00b7ser", "letz\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blickt einmal noch ins weite Land hinaus,", "tokens": ["Blickt", "ein\u00b7mal", "noch", "ins", "wei\u00b7te", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und merkt es wohl, es steht auf diesem Grunde,", "tokens": ["Und", "merkt", "es", "wohl", ",", "es", "steht", "auf", "die\u00b7sem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo wir auch weilen, unser Vaterhaus.", "tokens": ["Wo", "wir", "auch", "wei\u00b7len", ",", "un\u00b7ser", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wir scheiden jetzt, bis dieser Zeit Beschwerde", "tokens": ["Wir", "schei\u00b7den", "jetzt", ",", "bis", "die\u00b7ser", "Zeit", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein andrer Tag, ein besserer, ges\u00fchnt;", "tokens": ["Ein", "an\u00b7drer", "Tag", ",", "ein", "bes\u00b7se\u00b7rer", ",", "ge\u00b7s\u00fchnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn Raum ist auf der heimatlichen Erde", "tokens": ["Denn", "Raum", "ist", "auf", "der", "hei\u00b7mat\u00b7li\u00b7chen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Fremde nur und was den Fremden dient.", "tokens": ["F\u00fcr", "Frem\u00b7de", "nur", "und", "was", "den", "Frem\u00b7den", "dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch ist's das flehendste von den Gebeten,", "tokens": ["Doch", "ist's", "das", "fle\u00b7hends\u00b7te", "von", "den", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr m\u00f6gt dereinst, wenn mir es nicht verg\u00f6nnt,", "tokens": ["Ihr", "m\u00f6gt", "de\u00b7reinst", ",", "wenn", "mir", "es", "nicht", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit festem Fu\u00df auf diese Scholle treten,", "tokens": ["Mit", "fes\u00b7tem", "Fu\u00df", "auf", "die\u00b7se", "Schol\u00b7le", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von der sich jetzt mein hei\u00dfes Auge trennt! \u2013", "tokens": ["Von", "der", "sich", "jetzt", "mein", "hei\u00b7\u00dfes", "Au\u00b7ge", "trennt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und du, mein Kind, mein j\u00fcngstes, dessen Wiege", "tokens": ["Und", "du", ",", "mein", "Kind", ",", "mein", "j\u00fcngs\u00b7tes", ",", "des\u00b7sen", "Wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch noch auf diesem teuren Boden stand,", "tokens": ["Auch", "noch", "auf", "die\u00b7sem", "teu\u00b7ren", "Bo\u00b7den", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r mich! \u2013 denn alles andere ist L\u00fcge \u2013", "tokens": ["H\u00f6r", "mich", "!", "\u2013", "denn", "al\u00b7les", "an\u00b7de\u00b7re", "ist", "L\u00fc\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$.", "$(", "KON", "PIS", "PIS", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Mann gedeihet ohne Vaterland!", "tokens": ["Kein", "Mann", "ge\u00b7dei\u00b7het", "oh\u00b7ne", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Kannst du den Sinn, den diese Worte f\u00fchren,", "tokens": ["Kannst", "du", "den", "Sinn", ",", "den", "die\u00b7se", "Wor\u00b7te", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deiner Kinderseele nicht verstehn,", "tokens": ["Mit", "dei\u00b7ner", "Kin\u00b7der\u00b7see\u00b7le", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So soll es wie ein Schauer dich ber\u00fchren", "tokens": ["So", "soll", "es", "wie", "ein", "Schau\u00b7er", "dich", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOKOM", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ein Pulsschlag in dein Leben gehn!", "tokens": ["Und", "wie", "ein", "Puls\u00b7schlag", "in", "dein", "Le\u00b7ben", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}