{"textgrid.poem.37047": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod Sr. Exzellenz des Herrn Premierministers, Gerlach Adolph von M\u00fcnchhausen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Wolke, die kein Sonnenstral durchblinkt,", "tokens": ["Welch", "ei\u00b7ne", "Wol\u00b7ke", ",", "die", "kein", "Son\u00b7nen\u00b7stral", "durch\u00b7blinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4ngt sich um jedes Aug?", "tokens": ["H\u00e4ngt", "sich", "um", "je\u00b7des", "Aug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wem flie\u00dft die Z\u00e4hre, die auf jeder Wange steht,", "tokens": ["Wem", "flie\u00dft", "die", "Z\u00e4h\u00b7re", ",", "die", "auf", "je\u00b7der", "Wan\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Trauer Heroldin?", "tokens": ["Der", "Trau\u00b7er", "He\u00b7rol\u00b7din", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Dem edeln Greise, dem das Loo\u00df des Todes fiel,", "tokens": ["Dem", "e\u00b7deln", "Grei\u00b7se", ",", "dem", "das", "Loo\u00df", "des", "To\u00b7des", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf seine Urne, wie der Thau vom Rosenstrauch,", "tokens": ["Auf", "sei\u00b7ne", "Ur\u00b7ne", ",", "wie", "der", "Thau", "vom", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist der Thr\u00e4nen werth!", "tokens": ["Er", "ist", "der", "Thr\u00e4\u00b7nen", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er, de\u00dfen Seele nie der Tugend sich verschlo\u00df,", "tokens": ["Er", ",", "de\u00b7\u00dfen", "See\u00b7le", "nie", "der", "Tu\u00b7gend", "sich", "ver\u00b7schlo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der wie ein Genius,", "tokens": ["Der", "wie", "ein", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf diese Erdwelt kam, Gl\u00fcck um sich auszustreun,", "tokens": ["Auf", "die\u00b7se", "Erd\u00b7welt", "kam", ",", "Gl\u00fcck", "um", "sich", "aus\u00b7zu\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Verdienste zu erh\u00f6hn.", "tokens": ["Ver\u00b7diens\u00b7te", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er, der mit Wachsamkeit im Blick, am Ruder sa\u00df,", "tokens": ["Er", ",", "der", "mit", "Wach\u00b7sam\u00b7keit", "im", "Blick", ",", "am", "Ru\u00b7der", "sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Landes Wohlfarth wog,", "tokens": ["Des", "Lan\u00b7des", "Wohl\u00b7farth", "wog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Flor der St\u00e4dte hob, und Wonn' und \u00dcberflu\u00df", "tokens": ["Den", "Flor", "der", "St\u00e4d\u00b7te", "hob", ",", "und", "Wonn'", "und", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf die Gefilde rief.", "tokens": ["Auf", "die", "Ge\u00b7fil\u00b7de", "rief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er, dem der Schlummer oft vom Augenliede wich,", "tokens": ["Er", ",", "dem", "der", "Schlum\u00b7mer", "oft", "vom", "Au\u00b7gen\u00b7lie\u00b7de", "wich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Er um Mitternacht,", "tokens": ["Wenn", "Er", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele auf das Wohl der Menschheit heftete,", "tokens": ["Die", "See\u00b7le", "auf", "das", "Wohl", "der", "Menschheit", "hef\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und manchen Plan entwarf.", "tokens": ["Und", "man\u00b7chen", "Plan", "ent\u00b7warf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er, der die Musen an die Leine winkte, Er,", "tokens": ["Er", ",", "der", "die", "Mu\u00b7sen", "an", "die", "Lei\u00b7ne", "wink\u00b7te", ",", "Er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, wie Georg gebot,", "tokens": ["Der", ",", "wie", "Ge\u00b7org", "ge\u00b7bot", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rosen ihren Pfad bestreute, manchen Kranz", "tokens": ["Mit", "Ro\u00b7sen", "ih\u00b7ren", "Pfad", "be\u00b7streu\u00b7te", ",", "man\u00b7chen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um ihre Scheiteln wand.", "tokens": ["Um", "ih\u00b7re", "Schei\u00b7teln", "wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Enkel, dankt es Ihm! Traur Ihn Georgia,", "tokens": ["Ihr", "En\u00b7kel", ",", "dankt", "es", "Ihm", "!", "Traur", "Ihn", "Ge\u00b7or\u00b7gia", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "$.", "NN", "PPER", "NE", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du, seine Lieblingin,", "tokens": ["Du", ",", "sei\u00b7ne", "Lieb\u00b7lin\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rolle einen Schleyr um dein gesenktes Haupt,", "tokens": ["Und", "rol\u00b7le", "ei\u00b7nen", "Schleyr", "um", "dein", "ge\u00b7senk\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er starb, dein Vater starb:", "tokens": ["Er", "starb", ",", "dein", "Va\u00b7ter", "starb", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Best\u00e4ndig dachte Er dein Wohl, bis sich sein Geist", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "dach\u00b7te", "Er", "dein", "Wohl", ",", "bis", "sich", "sein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Staube lo\u00dfwand, und", "tokens": ["Vom", "Stau\u00b7be", "lo\u00df\u00b7wand", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf Engelschwingen durch der Sterne goldne Reyhn", "tokens": ["Auf", "En\u00b7gel\u00b7schwin\u00b7gen", "durch", "der", "Ster\u00b7ne", "gold\u00b7ne", "Reyhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Sitz der Gottheit flog.", "tokens": ["Zum", "Sitz", "der", "Got\u00b7theit", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da erndtet Er die Frucht der gro\u00dfen Tugenden!", "tokens": ["Da", "ernd\u00b7tet", "Er", "die", "Frucht", "der", "gro\u00b7\u00dfen", "Tu\u00b7gen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Sterne unter sich,", "tokens": ["Die", "Ster\u00b7ne", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut Er in eine Welt, die Er beseeligte,", "tokens": ["Schaut", "Er", "in", "ei\u00b7ne", "Welt", ",", "die", "Er", "be\u00b7see\u00b7lig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit heiterm Blick herab.", "tokens": ["Mit", "hei\u00b7term", "Blick", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Nachwelt ehret Ihn, stellt Ihn zum Muster auf,", "tokens": ["Die", "Nach\u00b7welt", "eh\u00b7ret", "Ihn", ",", "stellt", "Ihn", "zum", "Mus\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pflantzt sein Ged\u00e4chtni\u00df fort,", "tokens": ["Pflantzt", "sein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Nachruhm schreibt sein Lob, mit goldner Flammenschrift,", "tokens": ["Der", "Nach\u00b7ruhm", "schreibt", "sein", "Lob", ",", "mit", "gold\u00b7ner", "Flam\u00b7men\u00b7schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ans Thor der Ewigkeit.", "tokens": ["Ans", "Thor", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Augusta liebet Ihn, gr\u00e4bt sein ehrw\u00fcrdig Bild", "tokens": ["Au\u00b7gus\u00b7ta", "lie\u00b7bet", "Ihn", ",", "gr\u00e4bt", "sein", "ehr\u00b7w\u00fcr\u00b7dig", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "In ihren Busen, wirft", "tokens": ["In", "ih\u00b7ren", "Bu\u00b7sen", ",", "wirft"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft einen feuchten Blick, in jene Tage hin,", "tokens": ["Oft", "ei\u00b7nen", "feuch\u00b7ten", "Blick", ",", "in", "je\u00b7ne", "Ta\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Er ihr Schutzfreund war. \u2013", "tokens": ["Da", "Er", "ihr", "Schutz\u00b7freund", "war", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Oft klagt noch eine Leyr, in Trauerton gestimmt,", "tokens": ["Oft", "klagt", "noch", "ei\u00b7ne", "Leyr", ",", "in", "Trau\u00b7er\u00b7ton", "ge\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um seine Urne, oft", "tokens": ["Um", "sei\u00b7ne", "Ur\u00b7ne", ",", "oft"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchfliegt sein Ruhm, vermischt mit Harfenklang, den Hayn,", "tokens": ["Durch\u00b7fliegt", "sein", "Ruhm", ",", "ver\u00b7mischt", "mit", "Har\u00b7fen\u00b7klang", ",", "den", "Hayn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jeder Wipfel horcht.", "tokens": ["Und", "je\u00b7der", "Wip\u00b7fel", "horcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}