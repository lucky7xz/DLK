{"textgrid.poem.31663": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was die St\u00e4rke nicht kan thun / bringt zu weg die List der Sinnen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was die St\u00e4rke nicht kan thun / bringt zu weg die List der Sinnen.", "tokens": ["Was", "die", "St\u00e4r\u00b7ke", "nicht", "kan", "thun", "/", "bringt", "zu", "weg", "die", "List", "der", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$(", "VVFIN", "APPR", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Darius die Persisch Kron / durch des Reitknechts Kunst-Beginnen /", "tokens": ["Da\u00b7ri\u00b7us", "die", "Per\u00b7sisch", "Kron", "/", "durch", "des", "Reit\u00b7knechts", "Kunst\u00b7Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$(", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "durch das Pferd / die Griechen / kriegten / Troja / die ber\u00fchmte Stadt.", "tokens": ["durch", "das", "Pferd", "/", "die", "Grie\u00b7chen", "/", "krieg\u00b7ten", "/", "Tro\u00b7ja", "/", "die", "be\u00b7r\u00fchm\u00b7te", "Stadt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$(", "NE", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Hannibal mit Feuer-Ochsen freyen Pa\u00df gewunnen hat.", "tokens": ["Han\u00b7ni\u00b7bal", "mit", "Feu\u00b7er\u00b7Och\u00b7sen", "frey\u00b7en", "Pa\u00df", "ge\u00b7wun\u00b7nen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Judith / hat mit List enthaubt Holofernen ohne St\u00e4rke /", "tokens": ["Ju\u00b7dith", "/", "hat", "mit", "List", "ent\u00b7haubt", "Ho\u00b7lo\u00b7fer\u00b7nen", "oh\u00b7ne", "St\u00e4r\u00b7ke", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "APPR", "NN", "VVPP", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "und ihr Vatterland befreyt. St\u00e4rke / wo nicht List / das merke!", "tokens": ["und", "ihr", "Vat\u00b7ter\u00b7land", "be\u00b7freyt", ".", "St\u00e4r\u00b7ke", "/", "wo", "nicht", "List", "/", "das", "mer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$.", "NN", "$(", "PWAV", "PTKNEG", "NN", "$(", "PDS", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}