{"textgrid.poem.52805": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht bei Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In das Zeitbuch mit Zinnober,", "tokens": ["In", "das", "Zeit\u00b7buch", "mit", "Zin\u00b7no\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, mit Golde schreibt ihn ein", "tokens": ["Nein", ",", "mit", "Gol\u00b7de", "schreibt", "ihn", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "VVFIN", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland's achtzehnten October,", "tokens": ["Deut\u00b7schlan\u00b7d's", "acht\u00b7zehn\u00b7ten", "Oc\u00b7to\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "La\u00dft uns ihm ein Feuer weih'n,", "tokens": ["La\u00dft", "uns", "ihm", "ein", "Feu\u00b7er", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das von freien St\u00fccken lodert,", "tokens": ["Das", "von", "frei\u00b7en", "St\u00fc\u00b7cken", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht von oben aufgefordert.", "tokens": ["Nicht", "von", "o\u00b7ben", "auf\u00b7ge\u00b7for\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie viel Wunden! wie viel Leichen!", "tokens": ["Wie", "viel", "Wun\u00b7den", "!", "wie", "viel", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$.", "PWAV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mensch, was l\u00e4\u00dft du doch gescheh'n!", "tokens": ["Mensch", ",", "was", "l\u00e4\u00dft", "du", "doch", "ge\u00b7scheh'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nntest du dich nicht vergleichen,", "tokens": ["K\u00f6nn\u00b7test", "du", "dich", "nicht", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' du geh'st zum Aeu\u00dfersten?", "tokens": ["Eh'", "du", "geh'st", "zum", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Solches mu\u00df uns stets betr\u00fcben,", "tokens": ["Sol\u00b7ches", "mu\u00df", "uns", "stets", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn der Mensch soll Menschen lieben.", "tokens": ["Denn", "der", "Mensch", "soll", "Men\u00b7schen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber dennoch soll man ehren,", "tokens": ["A\u00b7ber", "den\u00b7noch", "soll", "man", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen deutschen Siegestag,", "tokens": ["Die\u00b7sen", "deut\u00b7schen", "Sie\u00b7ge\u00b7stag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mit hei\u00dfen Freudenz\u00e4hren", "tokens": ["Wo", "mit", "hei\u00b7\u00dfen", "Freu\u00b7den\u00b7z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Monarch auf Knieen lag,", "tokens": ["Der", "Mon\u00b7arch", "auf", "Kni\u00b7e\u00b7en", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wilhelm, Franz und Alexander", "tokens": ["Wil\u00b7helm", ",", "Franz", "und", "A\u00b7lex\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Knieten selber bei einander.", "tokens": ["Knie\u00b7ten", "sel\u00b7ber", "bei", "ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Holder Frieden, s\u00fc\u00dfer Frieden,", "tokens": ["Hol\u00b7der", "Frie\u00b7den", ",", "s\u00fc\u00b7\u00dfer", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dem Volk die Nothdurft giebt,", "tokens": ["Der", "dem", "Volk", "die", "Noth\u00b7durft", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Tag hat dich beschieden,", "tokens": ["Je\u00b7ner", "Tag", "hat", "dich", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D'rum wird er mit Recht geliebt,", "tokens": ["D'\u00b7rum", "wird", "er", "mit", "Recht", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da man k\u00fchn das Joch zerschlagen,", "tokens": ["Da", "man", "k\u00fchn", "das", "Joch", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das wir f\u00fcnfzehn Jahr' getragen.", "tokens": ["Das", "wir", "f\u00fcnf\u00b7zehn", "Jahr'", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "CARD", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Warum sollten wir auch d\u00fcrsten,", "tokens": ["Wa\u00b7rum", "soll\u00b7ten", "wir", "auch", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch nach einem fremden Herrn,", "tokens": ["Noch", "nach", "ei\u00b7nem", "frem\u00b7den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fehlt es uns doch nicht an F\u00fcrsten,", "tokens": ["Fehlt", "es", "uns", "doch", "nicht", "an", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie f\u00fchren uns ja gern.", "tokens": ["Und", "sie", "f\u00fch\u00b7ren", "uns", "ja", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort mit den Napoleonen,", "tokens": ["Fort", "mit", "den", "Na\u00b7po\u00b7le\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vivat uns're Landeskronen!", "tokens": ["Vi\u00b7vat", "un\u00b7s'\u00b7re", "Lan\u00b7des\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}