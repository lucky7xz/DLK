{"dta.poem.3774": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Der garten den ich mir selber erbaut", "tokens": ["Der", "gar\u00b7ten", "den", "ich", "mir", "sel\u00b7ber", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und seiner v\u00f6gel leblose schw\u00e4rme", "tokens": ["Und", "sei\u00b7ner", "v\u00f6\u00b7gel", "leb\u00b7lo\u00b7se", "schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Haben noch nie einen fr\u00fchling geschaut", "tokens": ["Ha\u00b7ben", "noch", "nie", "ei\u00b7nen", "fr\u00fch\u00b7ling", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJD", "VVPP"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Von kohle die st\u00e4mme von kohle die \u00e4ste", "tokens": ["Von", "koh\u00b7le", "die", "st\u00e4m\u00b7me", "von", "koh\u00b7le", "die", "\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "APPR", "VVFIN", "ART", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und d\u00fcstere felder am d\u00fcsteren rain", "tokens": ["Und", "d\u00fcs\u00b7te\u00b7re", "fel\u00b7der", "am", "d\u00fcs\u00b7te\u00b7ren", "rain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der fr\u00fcchte nimmer gebrochene l\u00e4ste", "tokens": ["Der", "fr\u00fcch\u00b7te", "nim\u00b7mer", "ge\u00b7bro\u00b7che\u00b7ne", "l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADJA", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gl\u00e4nzen wie lava im pinien-hain", "tokens": ["Gl\u00e4n\u00b7zen", "wie", "la\u00b7va", "im", "pi\u00b7ni\u00b7en\u00b7hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NE", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ein grauer schein aus verborgener h\u00f6hle", "tokens": ["Ein", "grau\u00b7er", "schein", "aus", "ver\u00b7bor\u00b7ge\u00b7ner", "h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verr\u00e4t nicht wann morgen wann abend naht", "tokens": ["Ver\u00b7r\u00e4t", "nicht", "wann", "mor\u00b7gen", "wann", "a\u00b7bend", "naht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PWAV", "ADV", "PWAV", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und staubige d\u00fcnste der mandel-\u00f6le", "tokens": ["Und", "stau\u00b7bi\u00b7ge", "d\u00fcns\u00b7te", "der", "man\u00b7del\u00b7\u00f6le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schweben auf beeten und anger und saat", "tokens": ["Schwe\u00b7ben", "auf", "bee\u00b7ten", "und", "an\u00b7ger", "und", "saat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "KON", "ADJA", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie zeug ich Dich aber im heiligtume", "tokens": ["Wie", "zeug", "ich", "Dich", "a\u00b7ber", "im", "hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u2014 So fragt ich wenn ich es sinnend durchmass", "tokens": ["So", "fragt", "ich", "wenn", "ich", "es", "sin\u00b7nend", "durch\u00b7mass"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KOUS", "PPER", "PPER", "ADJD", "VVFIN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In k\u00fchnen gespinsten der sorge vergass \u2014", "tokens": ["In", "k\u00fch\u00b7nen", "ge\u00b7spins\u00b7ten", "der", "sor\u00b7ge", "ver\u00b7gass"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dunkle grosse schwarze blume?", "tokens": ["Dunk\u00b7le", "gros\u00b7se", "schwar\u00b7ze", "blu\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}