{"textgrid.poem.48257": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Alten und die Jungen", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbunverst\u00e4ndlich sind uns die Jungen\u00ab", "tokens": ["\u00bb", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "sind", "uns", "die", "Jun\u00b7gen", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wird von den Alten best\u00e4ndig gesungen;", "tokens": ["Wird", "von", "den", "Al\u00b7ten", "be\u00b7st\u00e4n\u00b7dig", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Meinerseits m\u00f6cht ich's damit halten:", "tokens": ["Mei\u00b7ner\u00b7seits", "m\u00f6cht", "ich's", "da\u00b7mit", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PAV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbunverst\u00e4ndlich sind mir die Alten.\u00ab", "tokens": ["\u00bb", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "sind", "mir", "die", "Al\u00b7ten", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dieses am Ruderbleibenwollen", "tokens": ["Die\u00b7ses", "am", "Ru\u00b7der\u00b7blei\u00b7ben\u00b7wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In allen St\u00fccken und allen Rollen,", "tokens": ["In", "al\u00b7len", "St\u00fc\u00b7cken", "und", "al\u00b7len", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dieses sich Unentbehrlichvermeinen", "tokens": ["Die\u00b7ses", "sich", "Un\u00b7ent\u00b7behr\u00b7lich\u00b7ver\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "PRF", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Samt ihrer \u00bbAugen stillem Weinen\u00ab,", "tokens": ["Samt", "ih\u00b7rer", "\u00bb", "Au\u00b7gen", "stil\u00b7lem", "Wei\u00b7nen", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "$(", "NN", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als w\u00e4re der Welt ein Weh getan \u2013", "tokens": ["Als", "w\u00e4\u00b7re", "der", "Welt", "ein", "Weh", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ach ich kann es nicht verstahn.", "tokens": ["Ach", "ich", "kann", "es", "nicht", "ver\u00b7stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ob unsre Jungen, in ihrem Erdreisten,", "tokens": ["Ob", "uns\u00b7re", "Jun\u00b7gen", ",", "in", "ih\u00b7rem", "Er\u00b7dreis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wirklich was Besseres schaffen und leisten,", "tokens": ["Wirk\u00b7lich", "was", "Bes\u00b7se\u00b7res", "schaf\u00b7fen", "und", "leis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PWS", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Ob dem Parnasse sie n\u00e4her gekommen", "tokens": ["Ob", "dem", "Par\u00b7nas\u00b7se", "sie", "n\u00e4\u00b7her", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Oder blo\u00df einen Maulwurfsh\u00fcgel erklommen,", "tokens": ["O\u00b7der", "blo\u00df", "ei\u00b7nen", "Maul\u00b7wurfs\u00b7h\u00fc\u00b7gel", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ob sie, mit andern Neusittenverfechtern,", "tokens": ["Ob", "sie", ",", "mit", "an\u00b7dern", "Neu\u00b7sit\u00b7ten\u00b7ver\u00b7fech\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die Menschheit bessern oder verschlechtern,", "tokens": ["Die", "Menschheit", "bes\u00b7sern", "o\u00b7der", "ver\u00b7schlech\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Ob sie Frieden s\u00e4'n oder Sturm entfachen,", "tokens": ["Ob", "sie", "Frie\u00b7den", "s\u00e4'n", "o\u00b7der", "Sturm", "ent\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Ob sie Himmel oder H\u00f6lle machen \u2013", "tokens": ["Ob", "sie", "Him\u00b7mel", "o\u00b7der", "H\u00f6l\u00b7le", "ma\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Sie haben den Tag, sie haben die Stunde;", "tokens": ["Sie", "ha\u00b7ben", "den", "Tag", ",", "sie", "ha\u00b7ben", "die", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Der Mohr kann gehn, neu Spiel hebt an,", "tokens": ["Der", "Mohr", "kann", "gehn", ",", "neu", "Spiel", "hebt", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ADJD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}