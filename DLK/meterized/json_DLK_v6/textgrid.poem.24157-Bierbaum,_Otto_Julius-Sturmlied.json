{"textgrid.poem.24157": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sturmlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wild stie\u00df der Sturm durch die Nacht.", "tokens": ["Wild", "stie\u00df", "der", "Sturm", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "In den schwarzen Aesten der alten Eiche", "tokens": ["In", "den", "schwar\u00b7zen", "A\u00b7es\u00b7ten", "der", "al\u00b7ten", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+-+---+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Harfte er gellend ein Tanzlied der Kraft:", "tokens": ["Harf\u00b7te", "er", "gel\u00b7lend", "ein", "Tanz\u00b7lied", "der", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber die Berge und W\u00e4sser und W\u00e4lder,", "tokens": ["Ue\u00b7ber", "die", "Ber\u00b7ge", "und", "W\u00e4s\u00b7ser", "und", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hussahojoh!", "tokens": ["Hus\u00b7sa\u00b7ho\u00b7joh", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schwing durch die Nacht ich mich, fl\u00fcgelfroh singend,", "tokens": ["Schwing", "durch", "die", "Nacht", "ich", "mich", ",", "fl\u00fc\u00b7gel\u00b7froh", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PRF", "$,", "ADJD", "VVPP", "$,"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Hussahojoh!", "tokens": ["Hus\u00b7sa\u00b7ho\u00b7joh", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Tannen zerknick ich wie d\u00fcrres Schilf,", "tokens": ["Tan\u00b7nen", "zer\u00b7knick", "ich", "wie", "d\u00fcr\u00b7res", "Schilf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Aecker zerw\u00fchl ich wie Haufen Sands,", "tokens": ["A\u00b7e\u00b7cker", "zer\u00b7w\u00fchl", "ich", "wie", "Hau\u00b7fen", "Sands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "NN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Fangeball spiel ich mit Felsgestein", "tokens": ["Fan\u00b7ge\u00b7ball", "spiel", "ich", "mit", "Fels\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Hussahojoh!", "tokens": ["Hus\u00b7sa\u00b7ho\u00b7joh", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ausl\u00f6sch ich die Lichter, anfach ich die Flammen,", "tokens": ["Aus\u00b7l\u00f6sch", "ich", "die", "Lich\u00b7ter", ",", "an\u00b7fach", "ich", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Wolken umball ich die blinkenden Sterne,", "tokens": ["Mit", "Wol\u00b7ken", "um\u00b7ball", "ich", "die", "blin\u00b7ken\u00b7den", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gebirge von Wogen aufth\u00fcrm ich im Meere,", "tokens": ["Ge\u00b7bir\u00b7ge", "von", "Wo\u00b7gen", "auft\u00b7h\u00fcrm", "ich", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu schlingenden Schl\u00fcnden hinblas ich die Schiffe,", "tokens": ["Zu", "schlin\u00b7gen\u00b7den", "Schl\u00fcn\u00b7den", "hinb\u00b7las", "ich", "die", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hussah!", "tokens": ["Hus\u00b7sah", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Dann spiel ich mit treibenden Tr\u00fcmmern gelinde,", "tokens": ["Dann", "spiel", "ich", "mit", "trei\u00b7ben\u00b7den", "Tr\u00fcm\u00b7mern", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und, m\u00fcde, werd ich zum s\u00e4uselnden Winde,", "tokens": ["Und", ",", "m\u00fc\u00b7de", ",", "werd", "ich", "zum", "s\u00e4u\u00b7seln\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und singe wie Wiegenlied leis und weich.", "tokens": ["Und", "sin\u00b7ge", "wie", "Wie\u00b7gen\u00b7lied", "leis", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich k\u00fcsse die blinkenden Bl\u00fcten am Baume,", "tokens": ["Ich", "k\u00fcs\u00b7se", "die", "blin\u00b7ken\u00b7den", "Bl\u00fc\u00b7ten", "am", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich t\u00e4ndle am wogenden Halmackersaume", "tokens": ["Ich", "t\u00e4nd\u00b7le", "am", "wo\u00b7gen\u00b7den", "Hal\u00b7ma\u00b7cker\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und gl\u00e4tte die Wiesen sammetgleich.", "tokens": ["Und", "gl\u00e4t\u00b7te", "die", "Wie\u00b7sen", "sam\u00b7met\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das ist meine Kraft, die ich l\u00f6se und binde;", "tokens": ["Das", "ist", "mei\u00b7ne", "Kraft", ",", "die", "ich", "l\u00f6\u00b7se", "und", "bin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Krieg kreisch ich im Sturme, \u2013 im schaukelnden Winde", "tokens": ["Krieg", "kreisch", "ich", "im", "Stur\u00b7me", ",", "\u2013", "im", "schau\u00b7keln\u00b7den", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "$(", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bin ich ein stillfroher Friedereich.", "tokens": ["Bin", "ich", "ein", "still\u00b7fro\u00b7her", "Frie\u00b7de\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}