{"textgrid.poem.54063": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kleines Operettenlied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " mit ihm schlafen ja, aber keine Intimit\u00e4ten", "tokens": ["mit", "ihm", "schla\u00b7fen", "ja", ",", "a\u00b7ber", "kei\u00b7ne", "In\u00b7ti\u00b7mi\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "$,", "ADV", "PIAT", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sei nicht b\u00f6se, wenn ich dich, du liebe Inge,", "tokens": ["Sei", "nicht", "b\u00f6\u00b7se", ",", "wenn", "ich", "dich", ",", "du", "lie\u00b7be", "In\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "hier leis besinge \u2013", "tokens": ["hier", "leis", "be\u00b7sin\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "h\u00f6r mich mal an:", "tokens": ["h\u00f6r", "mich", "mal", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In dem weiten Reich der schw\u00e4rmerischen Dinge", "tokens": ["In", "dem", "wei\u00b7ten", "Reich", "der", "schw\u00e4r\u00b7me\u00b7ri\u00b7schen", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "kn\u00fcpft eine Schlinge", "tokens": ["kn\u00fcpft", "ei\u00b7ne", "Schlin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "dir jeder Mann.", "tokens": ["dir", "je\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch die Nacht ist keineswegs des Werkes Kr\u00f6nung.", "tokens": ["Doch", "die", "Nacht", "ist", "kei\u00b7nes\u00b7wegs", "des", "Wer\u00b7kes", "Kr\u00f6\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sieh, erst nachher da beginnt das wahre Spiel;", "tokens": ["Sieh", ",", "erst", "nach\u00b7her", "da", "be\u00b7ginnt", "das", "wah\u00b7re", "Spiel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "denn das Schlimmste an der Liebe ist Gew\u00f6hnung . . .", "tokens": ["denn", "das", "Schlimms\u00b7te", "an", "der", "Lie\u00b7be", "ist", "Ge\u00b7w\u00f6h\u00b7nung", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "ein Mal ist kein Mal, aber acht Mal sind sehr viel.", "tokens": ["ein", "Mal", "ist", "kein", "Mal", ",", "a\u00b7ber", "acht", "Mal", "sind", "sehr", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,", "ADV", "CARD", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "La\u00df die Liebe aus dem Spiel, wenn du liebst.", "tokens": ["La\u00df", "die", "Lie\u00b7be", "aus", "dem", "Spiel", ",", "wenn", "du", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Weil du dir dabei zu viel", "tokens": ["Weil", "du", "dir", "da\u00b7bei", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PAV", "APPR", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "vergibst.", "tokens": ["ver\u00b7gibst", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "H\u00f6re nicht auf Schmeichelein!", "tokens": ["H\u00f6\u00b7re", "nicht", "auf", "Schmei\u00b7che\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mu\u00dft du stets die Dumme sein?", "tokens": ["Mu\u00dft", "du", "stets", "die", "Dum\u00b7me", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn du ehrlich bist, dann f\u00e4llst du rein!", "tokens": ["Wenn", "du", "ehr\u00b7lich", "bist", ",", "dann", "f\u00e4llst", "du", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Das Gesch\u00e4ft ist faul: er nimmt, und du gibst . . .", "tokens": ["Das", "Ge\u00b7sch\u00e4ft", "ist", "faul", ":", "er", "nimmt", ",", "und", "du", "gibst", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "$.", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "La\u00df die Liebe aus dem Spiel, wenn du liebst!", "tokens": ["La\u00df", "die", "Lie\u00b7be", "aus", "dem", "Spiel", ",", "wenn", "du", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Steht nach K\u00fcssen dir der Sinn,", "tokens": ["Steht", "nach", "K\u00fcs\u00b7sen", "dir", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "na, dann geh nur ruhig hin \u2013", "tokens": ["na", ",", "dann", "geh", "nur", "ru\u00b7hig", "hin", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch von Liebe, doch von Liebe steht nichts drin!", "tokens": ["Doch", "von", "Lie\u00b7be", ",", "doch", "von", "Lie\u00b7be", "steht", "nichts", "drin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ich wei\u00df, wie das mal wird, du liebe Inge,", "tokens": ["Und", "ich", "wei\u00df", ",", "wie", "das", "mal", "wird", ",", "du", "lie\u00b7be", "In\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PDS", "ADV", "VAFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "wenn ich einst hinge", "tokens": ["wenn", "ich", "einst", "hin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "an deiner Brust:", "tokens": ["an", "dei\u00b7ner", "Brust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Um die Augen hast du dunkelblaue Ringe,", "tokens": ["Um", "die", "Au\u00b7gen", "hast", "du", "dun\u00b7kel\u00b7blau\u00b7e", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "doch ach! ich bringe dich nicht zur Lust.", "tokens": ["doch", "ach", "!", "ich", "brin\u00b7ge", "dich", "nicht", "zur", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Warum kommts, da\u00df wir uns so verlieren m\u00fcssen?", "tokens": ["Wa\u00b7rum", "kommts", ",", "da\u00df", "wir", "uns", "so", "ver\u00b7lie\u00b7ren", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wer mehr liebt, der leidet noch und noch.", "tokens": ["Wer", "mehr", "liebt", ",", "der", "lei\u00b7det", "noch", "und", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und du siehst an mir vorbei, wenn wir uns k\u00fcssen,", "tokens": ["Und", "du", "siehst", "an", "mir", "vor\u00b7bei", ",", "wenn", "wir", "uns", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "und du hast Furcht. Und liebst ja doch . . .", "tokens": ["und", "du", "hast", "Furcht", ".", "Und", "liebst", "ja", "doch", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$.", "KON", "VVFIN", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df die Liebe aus dem Spiel, wenn du liebst.", "tokens": ["La\u00df", "die", "Lie\u00b7be", "aus", "dem", "Spiel", ",", "wenn", "du", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Weil du dir dabei zu viel", "tokens": ["Weil", "du", "dir", "da\u00b7bei", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PAV", "APPR", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "vergibst.", "tokens": ["ver\u00b7gibst", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Erst schenkst du dein sch\u00f6nes Bein,", "tokens": ["Erst", "schenkst", "du", "dein", "sch\u00f6\u00b7nes", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "und du sagst: \u00bbMehr solls nicht sein!\u00ab", "tokens": ["und", "du", "sagst", ":", "\u00bb", "Mehr", "solls", "nicht", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PIS", "VMFIN", "PTKNEG", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und das Herz, das folgt dann hinterdrein . . .", "tokens": ["Und", "das", "Herz", ",", "das", "folgt", "dann", "hin\u00b7ter\u00b7drein", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VVFIN", "ADV", "PTKVZ", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und ich rate dir vergebens, wenn du gibst:", "tokens": ["Und", "ich", "ra\u00b7te", "dir", "ver\u00b7ge\u00b7bens", ",", "wenn", "du", "gibst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "La\u00df die Liebe aus dem Spiel, wenn du liebst!", "tokens": ["La\u00df", "die", "Lie\u00b7be", "aus", "dem", "Spiel", ",", "wenn", "du", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Frau und Mann sind niemals frei.", "tokens": ["Frau", "und", "Mann", "sind", "nie\u00b7mals", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Stets ist ein Gef\u00fchl dabei.", "tokens": ["Stets", "ist", "ein", "Ge\u00b7f\u00fchl", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Dummen sind gew\u00f6hnlich alle zwei!", "tokens": ["Und", "die", "Dum\u00b7men", "sind", "ge\u00b7w\u00f6hn\u00b7lich", "al\u00b7le", "zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "PIAT", "CARD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}