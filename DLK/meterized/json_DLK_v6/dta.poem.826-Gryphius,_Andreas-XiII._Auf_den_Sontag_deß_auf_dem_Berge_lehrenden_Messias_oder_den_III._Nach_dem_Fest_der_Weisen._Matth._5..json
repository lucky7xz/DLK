{"dta.poem.826": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  Auf den Sontag de\u00df auf dem Berge  \n lehrenden Messias/ oder den  III.  Nach dem  \n Fest der Weisen. Matth. 5.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "O Wol dem hohen Geist/ der auf dem Berg anh\u00f6ret ", "tokens": ["O", "Wol", "dem", "ho\u00b7hen", "Geist", "/", "der", "auf", "dem", "Berg", "an\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df was du selig sch\u00e4tz\u2019st? wol dem der in dem Thal", "tokens": ["Di\u00df", "was", "du", "se\u00b7lig", "sch\u00e4tz'st", "?", "wol", "dem", "der", "in", "dem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "ADJD", "VVFIN", "$.", "ADV", "ART", "ART", "APPR", "ART", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dir seine Schmertzen klagt/ vnd seine Seelen qual", "tokens": ["Dir", "sei\u00b7ne", "Schmert\u00b7zen", "klagt", "/", "vnd", "sei\u00b7ne", "See\u00b7len", "qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn Aussatz/ wenn sich Pest vnd Angst v\u00f1 S\u00fcnde mehret.", "tokens": ["Wenn", "Aus\u00b7satz", "/", "wenn", "sich", "Pest", "vnd", "Angst", "v\u00f1", "S\u00fcn\u00b7de", "meh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "KOUS", "PRF", "NE", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald leb\u2019t in Jhm durch dich/ was auff den tod versehret", "tokens": ["Bald", "le\u00b7b't", "in", "Jhm", "durch", "dich", "/", "was", "auff", "den", "tod", "ver\u00b7seh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "PPER", "$(", "PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Ich bin so starck nicht mehr! vnd mu\u00df nach meinem fall", "tokens": ["Ich", "bin", "so", "starck", "nicht", "mehr", "!", "vnd", "mu\u00df", "nach", "mei\u00b7nem", "fall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKNEG", "ADV", "$.", "KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschmachten fern von dir in diesem Thr\u00e4nen Sal/", "tokens": ["Ver\u00b7schmach\u00b7ten", "fern", "von", "dir", "in", "die\u00b7sem", "Thr\u00e4\u00b7nen", "Sal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "APPR", "PDAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem die grimme Sucht den C\u00f6rper gantz zust\u00f6ret.", "tokens": ["In", "dem", "die", "grim\u00b7me", "Sucht", "den", "C\u00f6r\u00b7per", "gantz", "zu\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ich werde/ wenn du wilt/ der scharffen schmertzen lo\u00df", "tokens": ["Ich", "wer\u00b7de", "/", "wenn", "du", "wilt", "/", "der", "scharf\u00b7fen", "schmert\u00b7zen", "lo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "PPER", "VMFIN", "$(", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin nicht werth da\u00df du dich zu mir heim solt finden.", "tokens": ["Ich", "bin", "nicht", "werth", "da\u00df", "du", "dich", "zu", "mir", "heim", "solt", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KOUS", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sprich Heyland/ nur ein wortt/ bald wird was itzt mich", "tokens": ["Sprich", "Hey\u00b7land", "/", "nur", "ein", "wortt", "/", "bald", "wird", "was", "itzt", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$(", "ADV", "ART", "NN", "$(", "ADV", "VAFIN", "PIS", "ADV", "PPER"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "nagt/ ", "tokens": ["nagt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was mein Gewissen kr\u00e4nckt/ was meine Seel anklagt", "tokens": ["Was", "mein", "Ge\u00b7wis\u00b7sen", "kr\u00e4nckt", "/", "was", "mei\u00b7ne", "Seel", "an\u00b7klagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd aller Teuffel macht vnd stoltzer Trotz verschwinden.", "tokens": ["Vnd", "al\u00b7ler", "Teuf\u00b7fel", "macht", "vnd", "stolt\u00b7zer", "Trotz", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}