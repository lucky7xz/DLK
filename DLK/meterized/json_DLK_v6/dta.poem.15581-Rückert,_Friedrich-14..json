{"dta.poem.15581": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Recht steht h\u00fcben und das Unrecht stehet dr\u00fcben,", "tokens": ["Das", "Recht", "steht", "h\u00fc\u00b7ben", "und", "das", "Un\u00b7recht", "ste\u00b7het", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bestimmt geschieden und entschieden auszu\u00fcben.", "tokens": ["Be\u00b7stimmt", "ge\u00b7schie\u00b7den", "und", "ent\u00b7schie\u00b7den", "aus\u00b7zu\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch unentschieden steht dazwischen manches Dritte,", "tokens": ["Doch", "un\u00b7ent\u00b7schie\u00b7den", "steht", "da\u00b7zwi\u00b7schen", "man\u00b7ches", "Drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PAV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unsicher schwankend in des Rechts und Unrechts Mitte.", "tokens": ["Un\u00b7si\u00b7cher", "schwan\u00b7kend", "in", "des", "Rechts", "und", "Un\u00b7rechts", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wie dieses wird genannt, erkl\u00e4rt und angewandt,", "tokens": ["Wie", "die\u00b7ses", "wird", "ge\u00b7nannt", ",", "er\u00b7kl\u00e4rt", "und", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daran vor allem wird der bessre Mensch erkannt.", "tokens": ["Da\u00b7ran", "vor", "al\u00b7lem", "wird", "der", "bess\u00b7re", "Mensch", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}