{"dta.poem.23465": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00e4hrdetes Geleite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df auf einem Steine:", "tokens": ["Ich", "sa\u00df", "auf", "ei\u00b7nem", "Stei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da deckt' ich Bein mit Beine,", "tokens": ["Da", "deckt'", "ich", "Bein", "mit", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darauf der Ellenbogen stand;", "tokens": ["Da\u00b7rauf", "der", "El\u00b7len\u00b7bo\u00b7gen", "stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schmiegte sich in meine Hand", "tokens": ["Es", "schmieg\u00b7te", "sich", "in", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Kinn und eine Wange.", "tokens": ["Das", "Kinn", "und", "ei\u00b7ne", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da dacht' ich sorglich lange", "tokens": ["Da", "dacht'", "ich", "sorg\u00b7lich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Weltlauf nach und irdschem Heil;", "tokens": ["Dem", "Welt\u00b7lauf", "nach", "und", "ird\u00b7schem", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch wurde mir kein Rath zu Theil,", "tokens": ["Doch", "wur\u00b7de", "mir", "kein", "Rath", "zu", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie man drei Ding' erw\u00fcrbe,", "tokens": ["Wie", "man", "drei", "Ding'", "er\u00b7w\u00fcr\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ihrer keins verd\u00fcrbe.", "tokens": ["Da\u00df", "ih\u00b7rer", "keins", "ver\u00b7d\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die zwei sind Ehr' und weltlich Gut,", "tokens": ["Die", "zwei", "sind", "Ehr'", "und", "welt\u00b7lich", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "NN", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das oft einander Schaden thut,", "tokens": ["Das", "oft", "ein\u00b7an\u00b7der", "Scha\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das dritte Gottes Segen,", "tokens": ["Das", "drit\u00b7te", "Got\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "An dem ist mehr gelegen:", "tokens": ["An", "dem", "ist", "mehr", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die h\u00e4tt' ich gern in ", "tokens": ["Die", "h\u00e4tt'", "ich", "gern", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Ja leider mag es nimmer sein,", "tokens": ["Ja", "lei\u00b7der", "mag", "es", "nim\u00b7mer", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df Gottes Gnade kehre", "tokens": ["Da\u00df", "Got\u00b7tes", "Gna\u00b7de", "keh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}