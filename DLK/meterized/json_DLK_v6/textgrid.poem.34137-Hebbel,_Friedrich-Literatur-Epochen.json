{"textgrid.poem.34137": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Literatur-Epochen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lumpe giebt es best\u00e4ndig, doch scheiden sich darnach die Zeiten,", "tokens": ["Lum\u00b7pe", "giebt", "es", "be\u00b7st\u00e4n\u00b7dig", ",", "doch", "schei\u00b7den", "sich", "dar\u00b7nach", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PRF", "PAV", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ob man sie r\u00fchmt und beklatscht, oder sie n\u00f6thigt zur Schaam.", "tokens": ["Ob", "man", "sie", "r\u00fchmt", "und", "be\u00b7klatscht", ",", "o\u00b7der", "sie", "n\u00f6\u00b7thigt", "zur", "Schaam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+----+-+", "measure": "iambic.penta.relaxed"}}}}}