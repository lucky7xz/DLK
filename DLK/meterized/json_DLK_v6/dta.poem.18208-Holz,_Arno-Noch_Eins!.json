{"dta.poem.18208": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Noch Eins!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Merkt auf, ihr Herren im Frack!", "tokens": ["Merkt", "auf", ",", "ihr", "Her\u00b7ren", "im", "Frack", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ihr hohen Herrn! denn ich pfeif euch noch Eins,", "tokens": ["Ihr", "ho\u00b7hen", "Herrn", "!", "denn", "ich", "pfeif", "euch", "noch", "Eins", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch Eins auf dem Dudelsack!", "tokens": ["Noch", "Eins", "auf", "dem", "Du\u00b7del\u00b7sack", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ob ihr auch flucht und mich niederschreit,", "tokens": ["Und", "ob", "ihr", "auch", "flucht", "und", "mich", "nie\u00b7der\u00b7schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "KON", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mir Alles einerlei!", "tokens": ["Mir", "Al\u00b7les", "ei\u00b7ner\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Porzellan- und Reifrockzeit", "tokens": ["Die", "Por\u00b7zel\u00b7lan", "und", "Rei\u00b7frock\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist Gottseidank vorbei!", "tokens": ["Ist", "Gott\u00b7sei\u00b7dank", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "$."], "meter": "-----+", "measure": "unknown.measure.single"}}, "stanza.2": {"line.1": {"text": "Vor dem Drei-Stern, den ", "tokens": ["Vor", "dem", "Drei\u00b7S\u00b7tern", ",", "den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Verschlie\u00dft St. Peter die Th\u00fcr:", "tokens": ["Ver\u00b7schlie\u00dft", "St.", "Pe\u00b7ter", "die", "Th\u00fcr", ":"], "token_info": ["word", "abbreviation", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Garibaldi hei\u00dft er und Bolivar", "tokens": ["Ga\u00b7ri\u00b7bal\u00b7di", "hei\u00dft", "er", "und", "Bo\u00b7li\u00b7var"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und Toussaint L'Ouvert\u00fcre.", "tokens": ["Und", "Tous\u00b7saint", "L'\u00b7Ou\u00b7ver\u00b7t\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es wandelt der neue Jesus Christ", "tokens": ["Es", "wan\u00b7delt", "der", "neu\u00b7e", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Still durch die V\u00f6lker schon:", "tokens": ["Still", "durch", "die", "V\u00f6l\u00b7ker", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O glaubt mir, ", "tokens": ["O", "glaubt", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "++-", "measure": "unknown.measure.di"}, "line.8": {"text": "Das Jahrhundert der Revolution!", "tokens": ["Das", "Jahr\u00b7hun\u00b7dert", "der", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schaut hin, schon hat's an den Nagel geh\u00e4ngt", "tokens": ["Schaut", "hin", ",", "schon", "hat's", "an", "den", "Na\u00b7gel", "ge\u00b7h\u00e4ngt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "APPR", "ART", "NE", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Purpur und Hermelin", "tokens": ["Pur\u00b7pur", "und", "Her\u00b7me\u00b7lin"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sitzt am Studirtisch tief versenkt", "tokens": ["Und", "sitzt", "am", "Stu\u00b7dir\u00b7tisch", "tief", "ver\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In die heilige Schrift des Darwin.", "tokens": ["In", "die", "hei\u00b7li\u00b7ge", "Schrift", "des", "Dar\u00b7win", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NE", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ja die biblische Spottgeburt aus Lehm", "tokens": ["Ja", "die", "bib\u00b7li\u00b7sche", "Spott\u00b7ge\u00b7burt", "aus", "Lehm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und die Wahrheit entschleiert ihr Weltsystem", "tokens": ["Und", "die", "Wahr\u00b7heit", "ent\u00b7schlei\u00b7ert", "ihr", "Welt\u00b7sys\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Vor der K\u00f6ngin der Wissenschaft!", "tokens": ["Vor", "der", "K\u00f6n\u00b7gin", "der", "Wis\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ihr aber thut, als w\u00e4re die Welt", "tokens": ["Ihr", "a\u00b7ber", "thut", ",", "als", "w\u00e4\u00b7re", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Noch die Welt, die sie ehmals war;", "tokens": ["Noch", "die", "Welt", ",", "die", "sie", "eh\u00b7mals", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihr bucht eure Titel und z\u00e4hlt euer Geld", "tokens": ["Ihr", "bucht", "eu\u00b7re", "Ti\u00b7tel", "und", "z\u00e4hlt", "eu\u00b7er", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und faselt von Thron und Altar!", "tokens": ["Und", "fa\u00b7selt", "von", "Thron", "und", "Al\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ihr faselt im Wachen, ihr faselt im Traum", "tokens": ["Ihr", "fa\u00b7selt", "im", "Wa\u00b7chen", ",", "ihr", "fa\u00b7selt", "im", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und im Fr\u00fchling genirt euch der Wind", "tokens": ["Und", "im", "Fr\u00fch\u00b7ling", "ge\u00b7nirt", "euch", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "PPER", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und keiner merkt, wie im Freiheitsbaum", "tokens": ["Und", "kei\u00b7ner", "merkt", ",", "wie", "im", "Frei\u00b7heits\u00b7baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schon die Knospen gesprungen sind!", "tokens": ["Schon", "die", "Knos\u00b7pen", "ge\u00b7sprun\u00b7gen", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ihr spreizt euch und bl\u00e4ht euch und n\u00f6rgelt und mault", "tokens": ["Ihr", "spreizt", "euch", "und", "bl\u00e4ht", "euch", "und", "n\u00f6r\u00b7gelt", "und", "mault"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KON", "VVFIN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trotz Hunger und Dynamit", "tokens": ["Trotz", "Hun\u00b7ger", "und", "Dy\u00b7na\u00b7mit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und seid doch an K\u00f6rper und Geist verfault,", "tokens": ["Und", "seid", "doch", "an", "K\u00f6r\u00b7per", "und", "Geist", "ver\u00b7fault", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Verfault bis ins hundertste Glied!", "tokens": ["Ver\u00b7fault", "bis", "ins", "hun\u00b7derts\u00b7te", "Glied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihr ha\u00dft das Licht wie die Pestilenz,", "tokens": ["Ihr", "ha\u00dft", "das", "Licht", "wie", "die", "Pes\u00b7ti\u00b7lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und der Schuftigste br\u00fcllt: Ich riskir's!", "tokens": ["Und", "der", "Schuf\u00b7tigs\u00b7te", "br\u00fcllt", ":", "Ich", "ris\u00b7kir's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PPER", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und schneuzt sich und schw\u00f6rt auf die Intelligenz", "tokens": ["Und", "schneuzt", "sich", "und", "schw\u00f6rt", "auf", "die", "In\u00b7tel\u00b7li\u00b7genz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Der hinterpommerschen Peers!", "tokens": ["Der", "hin\u00b7ter\u00b7pom\u00b7mer\u00b7schen", "Peers", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Doch ein braver Fluch ist auch ein Gebet", "tokens": ["Doch", "ein", "bra\u00b7ver", "Fluch", "ist", "auch", "ein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die Marseillaise ein Lied,", "tokens": ["Und", "die", "Mar\u00b7seil\u00b7laise", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum wenn das noch lange so weitergeht,", "tokens": ["Drum", "wenn", "das", "noch", "lan\u00b7ge", "so", "wei\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PDS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dann wei\u00df ich, was geschieht!", "tokens": ["Dann", "wei\u00df", "ich", ",", "was", "ge\u00b7schieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann ruft das Volk: Vermaledeit!", "tokens": ["Dann", "ruft", "das", "Volk", ":", "Ver\u00b7ma\u00b7le\u00b7deit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "He Pulver her und Blei!", "tokens": ["He", "Pul\u00b7ver", "her", "und", "Blei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Porzellan- und Reifrockzeit", "tokens": ["Die", "Por\u00b7zel\u00b7lan", "und", "Rei\u00b7frock\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist Gottseidank vorbei!", "tokens": ["Ist", "Gott\u00b7sei\u00b7dank", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "$."], "meter": "-----+", "measure": "unknown.measure.single"}}}}}