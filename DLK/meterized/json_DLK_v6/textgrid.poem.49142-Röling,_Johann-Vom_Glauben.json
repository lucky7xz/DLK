{"textgrid.poem.49142": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Vom Glauben", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins, spricht Jesus, ist euch noth;", "tokens": ["Eins", ",", "spricht", "Je\u00b7sus", ",", "ist", "euch", "noth", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was will unser Heyl hie sagen?", "tokens": ["Was", "will", "un\u00b7ser", "Heyl", "hie", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen wir durch ein Gebot", "tokens": ["K\u00f6n\u00b7nen", "wir", "durch", "ein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm all' unsre Schuld abtragen?", "tokens": ["Ihm", "all'", "uns\u00b7re", "Schuld", "ab\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+++-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bistu, Meister, so erb\u00f6tig,", "tokens": ["Bis\u00b7tu", ",", "Meis\u00b7ter", ",", "so", "er\u00b7b\u00f6\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ein Einigs uns nur n\u00f6tig", "tokens": ["Da\u00df", "ein", "Ei\u00b7nigs", "uns", "nur", "n\u00f6\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und da\u00df wir durch dieses Ein", "tokens": ["Und", "da\u00df", "wir", "durch", "die\u00b7ses", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PDAT", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00f6nnen gantz versorget seyn?", "tokens": ["K\u00f6n\u00b7nen", "gantz", "ver\u00b7sor\u00b7get", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Glaube, denke, dies bist du;", "tokens": ["Glau\u00b7be", ",", "den\u00b7ke", ",", "dies", "bist", "du", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts und nichtig sind wir Alle,", "tokens": ["Nichts", "und", "nich\u00b7tig", "sind", "wir", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KON", "ADJD", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit in jener selgen Ruh", "tokens": ["Seit", "in", "je\u00b7ner", "sel\u00b7gen", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Vater kam zu Falle,", "tokens": ["Un\u00b7ser", "Va\u00b7ter", "kam", "zu", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch, wenn wir dich nur haben,", "tokens": ["Den\u00b7noch", ",", "wenn", "wir", "dich", "nur", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlt uns nichts an Gut und Gaben,", "tokens": ["Fehlt", "uns", "nichts", "an", "Gut", "und", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie arm wir gehn herf\u00fcr,", "tokens": ["Und", "wie", "arm", "wir", "gehn", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glauben wir, so reich sind wir.", "tokens": ["Glau\u00b7ben", "wir", ",", "so", "reich", "sind", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4r' im h\u00f6chsten Himmel was", "tokens": ["W\u00e4r'", "im", "h\u00f6chs\u00b7ten", "Him\u00b7mel", "was"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "PWS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Seele zu vergn\u00fcgen,", "tokens": ["Uns\u00b7re", "See\u00b7le", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder Gott selbst h\u00e4tte das", "tokens": ["O\u00b7der", "Gott", "selbst", "h\u00e4t\u00b7te", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VAFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter seinem Hertzen liegen,", "tokens": ["Un\u00b7ter", "sei\u00b7nem", "Hert\u00b7zen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrd' er solches auch auff Erden", "tokens": ["W\u00fcrd'", "er", "sol\u00b7ches", "auch", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIAT", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns ohn Zweiffel la\u00dfen werden,", "tokens": ["Uns", "ohn", "Zweif\u00b7fel", "la\u00b7\u00dfen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der unser Werber ist;", "tokens": ["Wenn", "der", "un\u00b7ser", "Wer\u00b7ber", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaube, denke, was du bist.", "tokens": ["Glau\u00b7be", ",", "den\u00b7ke", ",", "was", "du", "bist", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, du bist, der wunderbar", "tokens": ["Du", ",", "du", "bist", ",", "der", "wun\u00b7der\u00b7bar"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPER", "VAFIN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesum, eh wir sind geboren", "tokens": ["Je\u00b7sum", ",", "eh", "wir", "sind", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dies Gantze selber war,", "tokens": ["Und", "dies", "Gant\u00b7ze", "sel\u00b7ber", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat f\u00fcr Aller Heyl erkoren,", "tokens": ["Hat", "f\u00fcr", "Al\u00b7ler", "Heyl", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist, durch den wir umfa\u00dfen,", "tokens": ["Du", "bist", ",", "durch", "den", "wir", "um\u00b7fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den nicht alle Himmel la\u00dfen", "tokens": ["Den", "nicht", "al\u00b7le", "Him\u00b7mel", "la\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der F\u00fcrst der Herrligkeit", "tokens": ["Und", "der", "F\u00fcrst", "der", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich an unser Hertz befreit.", "tokens": ["Sich", "an", "un\u00b7ser", "Hertz", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist die geheime Sprach,", "tokens": ["Du", "bist", "die", "ge\u00b7hei\u00b7me", "Sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drinnen Gott mit uns sich letzet,", "tokens": ["Drin\u00b7nen", "Gott", "mit", "uns", "sich", "let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist, bistu noch so schwach,", "tokens": ["Du", "bist", ",", "bis\u00b7tu", "noch", "so", "schwach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher gro\u00dfe Berg' umsetzet,", "tokens": ["Wel\u00b7cher", "gro\u00b7\u00dfe", "Ber\u00b7g'", "um\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Du bist die hochheilge Leiter,", "tokens": ["Du", "bist", "die", "hoch\u00b7heil\u00b7ge", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Welche jener Glaubens-Streiter", "tokens": ["Wel\u00b7che", "je\u00b7ner", "Glau\u00b7bens\u00b7Strei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "An dem Himmel sahe stehn,", "tokens": ["An", "dem", "Him\u00b7mel", "sa\u00b7he", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Drauff wir auff- und abwerts gehn.", "tokens": ["Drauff", "wir", "auf\u00b7f", "und", "ab\u00b7werts", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "TRUNC", "KON", "ADV", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Du warst, der den Gnaden-Schein", "tokens": ["Du", "warst", ",", "der", "den", "Gna\u00b7den\u00b7Schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcber Abeln hat bewogen,", "tokens": ["\u00dc\u00b7ber", "A\u00b7beln", "hat", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Henoch lebend Sternen-ein,", "tokens": ["He\u00b7noch", "le\u00b7bend", "Ster\u00b7nen\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noah in die Arch gezogen.", "tokens": ["Noah", "in", "die", "Arch", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du machtst Abraham zum Vater,", "tokens": ["Du", "machtst", "Ab\u00b7ra\u00b7ham", "zum", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du warst Josephs sein Berather,", "tokens": ["Du", "warst", "Jo\u00b7se\u00b7phs", "sein", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du gabst f\u00fcr Israels Noth", "tokens": ["Du", "gabst", "f\u00fcr", "Is\u00b7raels", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So viel Jahr das Himmels-Brodt.", "tokens": ["So", "viel", "Jahr", "das", "Him\u00b7mels\u00b7Brodt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Du warst, welcher so erfreut", "tokens": ["Du", "warst", ",", "wel\u00b7cher", "so", "er\u00b7freut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsers Heylands theure Zeugen", "tokens": ["Un\u00b7sers", "Hey\u00b7lands", "theu\u00b7re", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Erd-Krey\u00df hat begleitet", "tokens": ["Durch", "den", "Erd\u00b7Krey\u00df", "hat", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und durch kein Beschwer lie\u00df beugen,", "tokens": ["Und", "durch", "kein", "Be\u00b7schwer", "lie\u00df", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Straffen Ungeheuer,", "tokens": ["Al\u00b7ler", "Straf\u00b7fen", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hunger, Bl\u00f6\u00dfe, Schwerdt und Feuer", "tokens": ["Hun\u00b7ger", ",", "Bl\u00f6\u00b7\u00dfe", ",", "Schwerdt", "und", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Waren ihre gr\u00f6ste Lust,", "tokens": ["Wa\u00b7ren", "ih\u00b7re", "gr\u00f6s\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaube, denke, was du thust.", "tokens": ["Glau\u00b7be", ",", "den\u00b7ke", ",", "was", "du", "thust", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tugend-F\u00fcrstin, Engeln gleich,", "tokens": ["Tu\u00b7gen\u00b7dF\u00fcrs\u00b7tin", ",", "En\u00b7geln", "gleich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo du dich erwehlst zu setzen,", "tokens": ["Wo", "du", "dich", "er\u00b7wehlst", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Setzt das gantze Tugend-Reich", "tokens": ["Setzt", "das", "gant\u00b7ze", "Tu\u00b7gen\u00b7dReich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit allen seinen Sch\u00e4tzen;", "tokens": ["Sich", "mit", "al\u00b7len", "sei\u00b7nen", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6ttlich machstu, was wir sinnen,", "tokens": ["G\u00f6tt\u00b7lich", "machs\u00b7tu", ",", "was", "wir", "sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reden, wirken und beginnen,", "tokens": ["Re\u00b7den", ",", "wir\u00b7ken", "und", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es wird des H\u00f6chsten Bild", "tokens": ["Und", "es", "wird", "des", "H\u00f6chs\u00b7ten", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wiederum in uns erf\u00fcllt.", "tokens": ["Wie\u00b7de\u00b7rum", "in", "uns", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O, wie leicht kan so mein Geist", "tokens": ["O", ",", "wie", "leicht", "kan", "so", "mein", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff und ab nach Willen reysen", "tokens": ["Auff", "und", "ab", "nach", "Wil\u00b7len", "rey\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "PTKVZ", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da, wo sein Sch\u00f6pffer speist,", "tokens": ["Und", "da", ",", "wo", "sein", "Sch\u00f6pf\u00b7fer", "speist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich betr\u00e4nken und bespeisen,", "tokens": ["Sich", "be\u00b7tr\u00e4n\u00b7ken", "und", "be\u00b7spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wachen, ruhen, lachen, schertzen;", "tokens": ["Wa\u00b7chen", ",", "ru\u00b7hen", ",", "la\u00b7chen", ",", "schert\u00b7zen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich so bin mit dem Hertzen,", "tokens": ["Wo", "ich", "so", "bin", "mit", "dem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Werd' ich gantz ins K\u00fcnfftig' seyn,", "tokens": ["Werd'", "ich", "gantz", "ins", "K\u00fcnf\u00b7ftig'", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaube, dies machst du allein.", "tokens": ["Glau\u00b7be", ",", "dies", "machst", "du", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dr\u00fccket mich der S\u00fcnden Noth,", "tokens": ["Dr\u00fc\u00b7cket", "mich", "der", "S\u00fcn\u00b7den", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaub' ich, sind sie mir vergeben,", "tokens": ["Glaub'", "ich", ",", "sind", "sie", "mir", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreckt mich Satan, H\u00f6ll' und Todt,", "tokens": ["Schreckt", "mich", "Sa\u00b7tan", ",", "H\u00f6ll'", "und", "Todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaub' ich, macht mich solches leben.", "tokens": ["Glaub'", "ich", ",", "macht", "mich", "sol\u00b7ches", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Scheint der Himmel mir zu dr\u00e4uen,", "tokens": ["Scheint", "der", "Him\u00b7mel", "mir", "zu", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glaub' ich, so kan ich mich freuen,", "tokens": ["Glaub'", "ich", ",", "so", "kan", "ich", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und mir fehl' auch, was es sey,", "tokens": ["Und", "mir", "fehl'", "auch", ",", "was", "es", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaub' ich, f\u00e4llt mir Alles bey.", "tokens": ["Glaub'", "ich", ",", "f\u00e4llt", "mir", "Al\u00b7les", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach Herr, gib mir diesen Schatz,", "tokens": ["Ach", "Herr", ",", "gib", "mir", "die\u00b7sen", "Schatz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVIMP", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Reichthum, diese F\u00fclle,", "tokens": ["Die\u00b7sen", "Reicht\u00b7hum", ",", "die\u00b7se", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mach hiedurch bey mir Platz,", "tokens": ["Und", "mach", "hie\u00b7durch", "bey", "mir", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPER", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo ich mich mit dir verh\u00fclle,", "tokens": ["Wo", "ich", "mich", "mit", "dir", "ver\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebster Gott, in meiner Seelen,", "tokens": ["Liebs\u00b7ter", "Gott", ",", "in", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Kerker ihrer H\u00f6hlen", "tokens": ["Die", "im", "Ker\u00b7ker", "ih\u00b7rer", "H\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlecht zwar wohnt, doch dir gef\u00e4llt,", "tokens": ["Schlecht", "zwar", "wohnt", ",", "doch", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "KON", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie diesen Schmuck beh\u00e4lt.", "tokens": ["Wenn", "sie", "die\u00b7sen", "Schmuck", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Bin ich ein zerknirschtes Rohr,", "tokens": ["Bin", "ich", "ein", "zer\u00b7knirschtes", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "La\u00df