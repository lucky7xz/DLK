oken_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ach, wie selig sind wir hier,", "tokens": ["Ach", ",", "wie", "se\u00b7lig", "sind", "wir", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr noch, wenn wir dorthin kommen,", "tokens": ["Mehr", "noch", ",", "wenn", "wir", "dor\u00b7thin", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du, Jesu, uns, und wir", "tokens": ["Wenn", "du", ",", "Je\u00b7su", ",", "uns", ",", "und", "wir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "PPER", "$,", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich so haben eingenommen,", "tokens": ["Dich", "so", "ha\u00b7ben", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichtes ist, das dich uns raube;", "tokens": ["Nich\u00b7tes", "ist", ",", "das", "dich", "uns", "rau\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses macht allein der Glaube.", "tokens": ["Die\u00b7ses", "macht", "al\u00b7lein", "der", "Glau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Glaube, denke, was du bist,", "tokens": ["Glau\u00b7be", ",", "den\u00b7ke", ",", "was", "du", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr noch, was ein Gl\u00e4ubger ist.", "tokens": ["Mehr", "noch", ",", "was", "ein", "Gl\u00e4ub\u00b7ger", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}