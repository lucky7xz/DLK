{"dta.poem.13071": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas? oder h\u00f6ren wir der K\u00fcnste Gutbefinden;", "tokens": ["\u201e", "was", "?", "o\u00b7der", "h\u00f6\u00b7ren", "wir", "der", "K\u00fcns\u00b7te", "Gut\u00b7be\u00b7fin\u00b7den", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201evielleicht ist dessen Ruhm viel pr\u00e4chtiger zu gr\u00fcnden", "tokens": ["\u201e", "viel\u00b7leicht", "ist", "des\u00b7sen", "Ruhm", "viel", "pr\u00e4ch\u00b7ti\u00b7ger", "zu", "gr\u00fcn\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PDS", "NN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eals nur in diesem Stein, der mit dem Bau vergeht,", "tokens": ["\u201e", "als", "nur", "in", "die\u00b7sem", "Stein", ",", "der", "mit", "dem", "Bau", "ver\u00b7geht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund nicht so lang, als Ehr und Ruhm des F\u00fcrstens, steht?", "tokens": ["\u201e", "und", "nicht", "so", "lang", ",", "als", "Ehr", "und", "Ruhm", "des", "F\u00fcrs\u00b7tens", ",", "steht", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "695\u201dWas Tugenden erbaun, und Wissenschaften zieren,", "tokens": ["\"", "Was", "Tu\u00b7gen\u00b7den", "er\u00b7baun", ",", "und", "Wis\u00b7sen\u00b7schaf\u00b7ten", "zie\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "VVINF", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ekann seinen Prei\u00df nicht eh, als mit der Welt verliehren.", "tokens": ["\u201e", "kann", "sei\u00b7nen", "Prei\u00df", "nicht", "eh", ",", "als", "mit", "der", "Welt", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PTKNEG", "KOUS", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}