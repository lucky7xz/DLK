{"textgrid.poem.65085": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als uns'rer Seelen Aeolsharfensaiten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als uns'rer Seelen Aeolsharfensaiten", "tokens": ["Als", "un\u00b7s'\u00b7rer", "See\u00b7len", "A\u00b7e\u00b7ols\u00b7har\u00b7fen\u00b7sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Gotteshauch der Liebe laut erklangen,", "tokens": ["Vom", "Got\u00b7tes\u00b7hauch", "der", "Lie\u00b7be", "laut", "er\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als uns're Geister gl\u00fchend sich durchdrangen,", "tokens": ["Als", "un\u00b7s'\u00b7re", "Geis\u00b7ter", "gl\u00fc\u00b7hend", "sich", "durch\u00b7dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht wahr, mein Freund! Das waren sch\u00f6ne Zeiten?", "tokens": ["Nicht", "wahr", ",", "mein", "Freund", "!", "Das", "wa\u00b7ren", "sch\u00f6\u00b7ne", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist vorbei, und jene Seligkeiten,", "tokens": ["Das", "ist", "vor\u00b7bei", ",", "und", "je\u00b7ne", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu s\u00fc\u00df in ird'schem Gefild' zu prangen,", "tokens": ["Zu", "s\u00fc\u00df", "in", "ird'\u00b7schem", "Ge\u00b7fild'", "zu", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ADJA", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sind in Nacht und Tod dahingegangen", "tokens": ["Sie", "sind", "in", "Nacht", "und", "Tod", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ich dein schwankend Herz sah von mir gleiten.", "tokens": ["Als", "ich", "dein", "schwan\u00b7kend", "Herz", "sah", "von", "mir", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, ob auch liebeleer nun deine Brust;", "tokens": ["Doch", ",", "ob", "auch", "lie\u00b7be\u00b7leer", "nun", "dei\u00b7ne", "Brust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein starkes Band wird ewig uns verm\u00e4hlen,", "tokens": ["Ein", "star\u00b7kes", "Band", "wird", "e\u00b7wig", "uns", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Innersten ist's trostvoll mir bewu\u00dft:", "tokens": ["Im", "In\u00b7ners\u00b7ten", "ist's", "trost\u00b7voll", "mir", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "PPER", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Denn ewig werden uns're d\u00fcstern Seelen,", "tokens": ["Denn", "e\u00b7wig", "wer\u00b7den", "un\u00b7s'\u00b7re", "d\u00fcs\u00b7tern", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gefall'nen Engeln \u00e4hnlich, von der Lust", "tokens": ["Ge\u00b7fall'\u00b7nen", "En\u00b7geln", "\u00e4hn\u00b7lich", ",", "von", "der", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verlornen Edens trauernd sich erz\u00e4hlen.", "tokens": ["Ver\u00b7lor\u00b7nen", "E\u00b7dens", "trau\u00b7ernd", "sich", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}