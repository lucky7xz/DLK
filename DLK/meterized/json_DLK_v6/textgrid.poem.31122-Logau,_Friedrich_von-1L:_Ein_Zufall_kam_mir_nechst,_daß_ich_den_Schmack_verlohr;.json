{"textgrid.poem.31122": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Zufall kam mir nechst, da\u00df ich den Schmack verlohr;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zufall kam mir nechst, da\u00df ich den Schmack verlohr;", "tokens": ["Ein", "Zu\u00b7fall", "kam", "mir", "nechst", ",", "da\u00df", "ich", "den", "Schmack", "ver\u00b7lohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schm\u00e4ckte mir wie Koth, was lieblich schmackte vor,", "tokens": ["Es", "schm\u00e4ck\u00b7te", "mir", "wie", "Koth", ",", "was", "lieb\u00b7lich", "schmack\u00b7te", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NE", "$,", "PRELS", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch meistens Fisch und Fleisch; o, wann nur meine Sachen", "tokens": ["Doch", "meis\u00b7tens", "Fisch", "und", "Fleisch", ";", "o", ",", "wann", "nur", "mei\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "NN", "$.", "FM", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Leser allen Schmack nicht wolten graulich machen!", "tokens": ["Dem", "Le\u00b7ser", "al\u00b7len", "Schmack", "nicht", "wol\u00b7ten", "grau\u00b7lich", "ma\u00b7chen", "!"], "token_info": ["word",