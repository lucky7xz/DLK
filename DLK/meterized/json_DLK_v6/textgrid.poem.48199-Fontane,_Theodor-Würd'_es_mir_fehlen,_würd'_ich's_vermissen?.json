{"textgrid.poem.48199": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcrd' es mir fehlen, w\u00fcrd' ich's vermissen?", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute fr\u00fch, nach gut durchschlafener Nacht,", "tokens": ["Heu\u00b7te", "fr\u00fch", ",", "nach", "gut", "durch\u00b7schla\u00b7fe\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bin ich wieder aufgewacht.", "tokens": ["Bin", "ich", "wie\u00b7der", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich setzte mich an den Fr\u00fchst\u00fcckstisch,", "tokens": ["Ich", "setz\u00b7te", "mich", "an", "den", "Fr\u00fch\u00b7st\u00fccks\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Kaffee war warm, die Semmel war frisch,", "tokens": ["Der", "Kaf\u00b7fee", "war", "warm", ",", "die", "Sem\u00b7mel", "war", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich habe die Morgenzeitung gelesen,", "tokens": ["Ich", "ha\u00b7be", "die", "Mor\u00b7gen\u00b7zei\u00b7tung", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "(es sind wieder Avancements gewesen).", "tokens": ["(", "es", "sind", "wie\u00b7der", "A\u00b7van\u00b7ce\u00b7ments", "ge\u00b7we\u00b7sen", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "NN", "VAPP", "$(", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ich trat ans Fenster, ich sah hinunter,", "tokens": ["Ich", "trat", "ans", "Fens\u00b7ter", ",", "ich", "sah", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Es trabte wieder, es klingelte munter,", "tokens": ["Es", "trab\u00b7te", "wie\u00b7der", ",", "es", "klin\u00b7gel\u00b7te", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Eine Sch\u00fcrze (beim Schl\u00e4chter) hing \u00fcber dem Stuhle,", "tokens": ["Ei\u00b7ne", "Sch\u00fcr\u00b7ze", "(", "beim", "Schl\u00e4ch\u00b7ter", ")", "hing", "\u00fc\u00b7ber", "dem", "Stuh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPRART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Kleine M\u00e4dchen gingen nach der Schule, \u2013", "tokens": ["Klei\u00b7ne", "M\u00e4d\u00b7chen", "gin\u00b7gen", "nach", "der", "Schu\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Alles war freundlich, alles war nett,", "tokens": ["Al\u00b7les", "war", "freund\u00b7lich", ",", "al\u00b7les", "war", "nett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$,", "PIS", "VAFIN", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Aber wenn ich weiter geschlafen h\u00e4tt'", "tokens": ["A\u00b7ber", "wenn", "ich", "wei\u00b7ter", "ge\u00b7schla\u00b7fen", "h\u00e4tt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Und t\u00e4t' von alledem nichts wissen,", "tokens": ["Und", "t\u00e4t'", "von", "al\u00b7le\u00b7dem", "nichts", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "W\u00fcrd' es mir fehlen, w\u00fcrd' ich's vermissen?", "tokens": ["W\u00fcrd'", "es", "mir", "feh\u00b7len", ",", "w\u00fcrd'", "ich's", "ver\u00b7mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}