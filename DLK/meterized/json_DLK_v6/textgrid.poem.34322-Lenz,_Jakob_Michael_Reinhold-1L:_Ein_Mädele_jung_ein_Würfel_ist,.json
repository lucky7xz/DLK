{"textgrid.poem.34322": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein M\u00e4dele jung ein W\u00fcrfel ist,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dele jung ein W\u00fcrfel ist,", "tokens": ["Ein", "M\u00e4\u00b7de\u00b7le", "jung", "ein", "W\u00fcr\u00b7fel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl auf den Tisch gelegen:", "tokens": ["Wohl", "auf", "den", "Tisch", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das kleine R\u00f6sel aus Hennegau", "tokens": ["Das", "klei\u00b7ne", "R\u00f6\u00b7sel", "aus", "Hen\u00b7ne\u00b7gau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird bald zu Gottes Tisch gehen.", "tokens": ["Wird", "bald", "zu", "Got\u00b7tes", "Tisch", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Was l\u00e4chelst so froh mein liebes Kind,", "tokens": ["Was", "l\u00e4\u00b7chelst", "so", "froh", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Kreuz wird dir'n schon kommen.", "tokens": ["Dein", "Kreuz", "wird", "dir'n", "schon", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenns hei\u00dft, das R\u00f6sel aus Hennegau", "tokens": ["Wenns", "hei\u00dft", ",", "das", "R\u00f6\u00b7sel", "aus", "Hen\u00b7ne\u00b7gau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hab nun einen Mann genommen.", "tokens": ["Hab", "nun", "ei\u00b7nen", "Mann", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O Kindlein mein, wie thuts mir so weh,", "tokens": ["O", "Kin\u00b7dlein", "mein", ",", "wie", "thuts", "mir", "so", "weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie dir dein \u00c4ugelein lachen,", "tokens": ["Wie", "dir", "dein", "\u00c4u\u00b7ge\u00b7lein", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn ich die tausend Thr\u00e4nlein seh,", "tokens": ["Und", "wenn", "ich", "die", "tau\u00b7send", "Thr\u00e4n\u00b7lein", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die werden dein B\u00e4ckelein waschen.", "tokens": ["Die", "wer\u00b7den", "dein", "B\u00e4\u00b7ckel\u00b7ein", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}