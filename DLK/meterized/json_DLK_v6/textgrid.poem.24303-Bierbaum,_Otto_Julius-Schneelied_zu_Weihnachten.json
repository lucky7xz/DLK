{"textgrid.poem.24303": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Schneelied zu Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du trittst mich, singt der Schnee,", "tokens": ["Du", "trittst", "mich", ",", "singt", "der", "Schnee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir aber tuts nicht weh:", "tokens": ["Mir", "a\u00b7ber", "tuts", "nicht", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich knirsche nicht, ich singe;", "tokens": ["Ich", "knir\u00b7sche", "nicht", ",", "ich", "sin\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Fu\u00df ist wie der Bogenstrich,", "tokens": ["Dein", "Fu\u00df", "ist", "wie", "der", "Bo\u00b7gen\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df meine Seele klinge.", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", "klin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00f6r und verstehe mich \u2013:", "tokens": ["H\u00f6r", "und", "ver\u00b7ste\u00b7he", "mich", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "$(", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Getreten singe ich,", "tokens": ["Ge\u00b7tre\u00b7ten", "sin\u00b7ge", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und nichts als frohe Dinge.", "tokens": ["Und", "nichts", "als", "fro\u00b7he", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Denn, die getreten sind,", "tokens": ["Denn", ",", "die", "ge\u00b7tre\u00b7ten", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wissen, es kam ein Kind,", "tokens": ["Wis\u00b7sen", ",", "es", "kam", "ein", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Gar sehr geringe,", "tokens": ["Gar", "sehr", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "In einem Stall zur Welt:", "tokens": ["In", "ei\u00b7nem", "Stall", "zur", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Das hat sein Herz wie ein leuchtendes Licht", "tokens": ["Das", "hat", "sein", "Herz", "wie", "ein", "leuch\u00b7ten\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "In gro\u00dfe Finsternis gestellt.", "tokens": ["In", "gro\u00b7\u00dfe", "Fins\u00b7ter\u00b7nis", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wurde zerschlagen. Verloschen ists nicht.", "tokens": ["Es", "wur\u00b7de", "zer\u00b7schla\u00b7gen", ".", "Ver\u00b7lo\u00b7schen", "ists", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "NN", "VAFIN", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}