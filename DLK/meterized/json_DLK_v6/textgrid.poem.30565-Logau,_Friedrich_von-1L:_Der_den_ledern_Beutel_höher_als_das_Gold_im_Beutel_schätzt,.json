{"textgrid.poem.30565": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der den ledern Beutel h\u00f6her als das Gold im Beutel sch\u00e4tzt,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der den ledern Beutel h\u00f6her als das Gold im Beutel sch\u00e4tzt,", "tokens": ["Der", "den", "le\u00b7dern", "Beu\u00b7tel", "h\u00f6\u00b7her", "als", "das", "Gold", "im", "Beu\u00b7tel", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der taug hin, wo man nach Wurtzeln auff Antycir \u00fcbersetzt;", "tokens": ["Der", "taug", "hin", ",", "wo", "man", "nach", "Wurt\u00b7zeln", "auff", "An\u00b7ty\u00b7cir", "\u00fc\u00b7bers\u00b7etzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PWAV", "PIS", "APPR", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der de\u00df Leibes Zierden putzt, l\u00e4st den Sinn im Kote liegen,", "tokens": ["Der", "de\u00df", "Lei\u00b7bes", "Zier\u00b7den", "putzt", ",", "l\u00e4st", "den", "Sinn", "im", "Ko\u00b7te", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dieser kan zum K\u00f6nigreich unter allen Narren t\u00fcgen.", "tokens": ["Die\u