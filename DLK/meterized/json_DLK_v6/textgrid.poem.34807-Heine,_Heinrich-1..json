{"textgrid.poem.34807": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df Berlin, mit seinem dicken Sande", "tokens": ["Ver\u00b7la\u00df", "Ber\u00b7lin", ",", "mit", "sei\u00b7nem", "di\u00b7cken", "San\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und d\u00fcnnen Tee und \u00fcberwitz'gen Leuten,", "tokens": ["Und", "d\u00fcn\u00b7nen", "Tee", "und", "\u00fc\u00b7ber\u00b7witz'\u00b7gen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gott und Welt, und was sie selbst bedeuten,", "tokens": ["Die", "Gott", "und", "Welt", ",", "und", "was", "sie", "selbst", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "PWS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begriffen l\u00e4ngst mit Hegelschem Verstande.", "tokens": ["Be\u00b7grif\u00b7fen", "l\u00e4ngst", "mit", "He\u00b7gel\u00b7schem", "Ver\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm mit nach Indien, nach dem Sonnenlande,", "tokens": ["Komm", "mit", "nach", "In\u00b7di\u00b7en", ",", "nach", "dem", "Son\u00b7nen\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo Ambrabl\u00fcten ihren Duft verbreiten,", "tokens": ["Wo", "Am\u00b7bra\u00b7bl\u00fc\u00b7ten", "ih\u00b7ren", "Duft", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Pilgerscharen nach dem Ganges schreiten,", "tokens": ["Die", "Pil\u00b7ger\u00b7scha\u00b7ren", "nach", "dem", "Gan\u00b7ges", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "And\u00e4chtig und im wei\u00dfen Festgewande.", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "und", "im", "wei\u00b7\u00dfen", "Fest\u00b7ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort, wo die Palmen wehn, die Wellen blinken,", "tokens": ["Dort", ",", "wo", "die", "Pal\u00b7men", "wehn", ",", "die", "Wel\u00b7len", "blin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am heil'gen Ufer Lotosblumen ragen", "tokens": ["Am", "heil'\u00b7gen", "U\u00b7fer", "Lo\u00b7tos\u00b7blu\u00b7men", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empor zu Indras Burg, der ewig blauen;", "tokens": ["Em\u00b7por", "zu", "I\u00b7ndras", "Burg", ",", "der", "e\u00b7wig", "blau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort will ich gl\u00e4ubig vor dir niedersinken,", "tokens": ["Dort", "will", "ich", "gl\u00e4u\u00b7big", "vor", "dir", "nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deine F\u00fc\u00dfe dr\u00fccken, und dir sagen:", "tokens": ["Und", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "dr\u00fc\u00b7cken", ",", "und", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbmadame! Sie sind die sch\u00f6nste aller Frauen!\u00ab", "tokens": ["\u00bb", "ma\u00b7da\u00b7me", "!", "Sie", "sind", "die", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Frau\u00b7en", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$.", "PPER", "VAFIN", "ART", "ADJA", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}