{"textgrid.poem.52663": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Traumgl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will am Tag mir nicht gelingen,", "tokens": ["Will", "am", "Tag", "mir", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiter sch\u00f6n und mild,", "tokens": ["Hei\u00b7ter", "sch\u00f6n", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vor das Auge herzuzwingen", "tokens": ["Vor", "das", "Au\u00b7ge", "her\u00b7zu\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Geliebten Bild.", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", "Bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Uebers Antlitz ausgegossen", "tokens": ["Ue\u00b7bers", "Ant\u00b7litz", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All die Lieblichkeit", "tokens": ["All", "die", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bleibt dem innern Blick verschlossen,", "tokens": ["Bleibt", "dem", "in\u00b7nern", "Blick", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin zerstreut.", "tokens": ["Und", "ich", "bin", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hasch ich auch nach allen Sch\u00e4tzen", "tokens": ["Hasch", "ich", "auch", "nach", "al\u00b7len", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Erinnerung,", "tokens": ["Der", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Ach, sie k\u00f6nnen nicht ersetzen", "tokens": ["Ach", ",", "sie", "k\u00f6n\u00b7nen", "nicht", "er\u00b7set\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Lebendgen Schwung!", "tokens": ["Des", "Le\u00b7bend\u00b7gen", "Schwung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Keine Dichtung gibt ihn wieder", "tokens": ["Kei\u00b7ne", "Dich\u00b7tung", "gibt", "ihn", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Formen Drang,", "tokens": ["Je\u00b7ner", "For\u00b7men", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jener aufgebl\u00fchten Glieder", "tokens": ["Je\u00b7ner", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ineinanderklang.", "tokens": ["In\u00b7ein\u00b7an\u00b7der\u00b7klang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Jene s\u00fc\u00dfen sanften Augen", "tokens": ["Je\u00b7ne", "s\u00fc\u00b7\u00dfen", "sanf\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den lieben Mund,", "tokens": ["Und", "den", "lie\u00b7ben", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Phantasien tauchen", "tokens": ["Al\u00b7le", "Phan\u00b7ta\u00b7si\u00b7en", "tau\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nirgends auf den Grund.", "tokens": ["Nir\u00b7gends", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kann die Perle nicht gewinnen", "tokens": ["Kann", "die", "Per\u00b7le", "nicht", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Reize Meer,", "tokens": ["Aus", "der", "Rei\u00b7ze", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "All Ged\u00e4chtni\u00df, alles Sinnen", "tokens": ["All", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ",", "al\u00b7les", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zaubert sie nicht her.", "tokens": ["Zau\u00b7bert", "sie", "nicht", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aber, aber wenn der stille", "tokens": ["A\u00b7ber", ",", "a\u00b7ber", "wenn", "der", "stil\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abend wiederkehrt,", "tokens": ["A\u00b7bend", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00e4mmert auf in pr\u00e4chtger F\u00fclle,", "tokens": ["D\u00e4m\u00b7mert", "auf", "in", "pr\u00e4cht\u00b7ger", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mein Herz begehrt.", "tokens": ["Was", "mein", "Herz", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn das Dunkel mich umfangen,", "tokens": ["Wenn", "das", "Dun\u00b7kel", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleichet s\u00fc\u00df und rein", "tokens": ["Schlei\u00b7chet", "s\u00fc\u00df", "und", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Traum, den herzensbangen,", "tokens": ["In", "den", "Traum", ",", "den", "her\u00b7zens\u00b7ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Geliebte ein.", "tokens": ["Die", "Ge\u00b7lieb\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und sie kommt, um mich zu k\u00fcssen,", "tokens": ["Und", "sie", "kommt", ",", "um", "mich", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr Ku\u00df berauscht,", "tokens": ["Und", "ihr", "Ku\u00df", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In des Wiedersehns Gen\u00fcssen", "tokens": ["In", "des", "Wie\u00b7der\u00b7sehns", "Ge\u00b7n\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwelg ich unbelauscht.", "tokens": ["Schwelg", "ich", "un\u00b7be\u00b7lauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Und sie ist so sch\u00f6n zu schauen,", "tokens": ["Und", "sie", "ist", "so", "sch\u00f6n", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So lebendig warm,", "tokens": ["So", "le\u00b7ben\u00b7dig", "warm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So voll Liebe voll Vertrauen", "tokens": ["So", "voll", "Lie\u00b7be", "voll", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht sie mir im Arm!", "tokens": ["Ruht", "sie", "mir", "im", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}