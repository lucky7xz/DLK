{"textgrid.poem.33344": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Schmauchlied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem edlen Schmaucherorden", "tokens": ["Dem", "ed\u00b7len", "Schmau\u00b7cher\u00b7or\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Ost, S\u00fcd, West und Norden", "tokens": ["In", "Ost", ",", "S\u00fcd", ",", "West", "und", "Nor\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stimm' ich dies Loblied an:", "tokens": ["Stimm'", "ich", "dies", "Lob\u00b7lied", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es soll den Schmaucher lehren,", "tokens": ["Es", "soll", "den", "Schmau\u00b7cher", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie er mit allen Ehren", "tokens": ["Wie", "er", "mit", "al\u00b7len", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Vortheil schmauchen kann.", "tokens": ["Und", "Vor\u00b7theil", "schmau\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die grosse Kunst zu schweigen", "tokens": ["Die", "gros\u00b7se", "Kunst", "zu", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sey nur euch, Schmauchern, eigen;", "tokens": ["Sey", "nur", "euch", ",", "Schmau\u00b7chern", ",", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schreit man das Ohr euch wund:", "tokens": ["Schreit", "man", "das", "Ohr", "euch", "wund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So steckt, anstatt zu zanken,", "tokens": ["So", "steckt", ",", "an\u00b7statt", "zu", "zan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit ruhigem Gedanken", "tokens": ["Mit", "ru\u00b7hi\u00b7gem", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Das Pfeifchen in den Mund.", "tokens": ["Das", "Pfei\u00b7fchen", "in", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Will euch in tr\u00fcben Tagen", "tokens": ["Will", "euch", "in", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Hypochonder plagen:", "tokens": ["Der", "Hy\u00b7po\u00b7chon\u00b7der", "pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So setzt euch zum Kamin,", "tokens": ["So", "setzt", "euch", "zum", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00dft in blauen W\u00f6lkchen", "tokens": ["Und", "la\u00dft", "in", "blau\u00b7en", "W\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sorgen banges V\u00f6lkchen", "tokens": ["Der", "Sor\u00b7gen", "ban\u00b7ges", "V\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von eu'ren Stirnen zieh'n.", "tokens": ["Von", "eu'\u00b7ren", "Stir\u00b7nen", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kommt ihr in Liebesfehde,", "tokens": ["Kommt", "ihr", "in", "Lie\u00b7bes\u00b7feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und machet eine Spr\u00f6de", "tokens": ["Und", "ma\u00b7chet", "ei\u00b7ne", "Spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch einen Korb Verdru\u00df:", "tokens": ["Durch", "ei\u00b7nen", "Korb", "Ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So braucht das Abschiedsbl\u00e4ttchen", "tokens": ["So", "braucht", "das", "Ab\u00b7schieds\u00b7bl\u00e4tt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von ihrem spr\u00f6den Pf\u00f6tchen", "tokens": ["Von", "ih\u00b7rem", "spr\u00f6\u00b7den", "Pf\u00f6t\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getrost zu Fidibus.", "tokens": ["Ge\u00b7trost", "zu", "Fi\u00b7di\u00b7bus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Macht euer treues Liebchen", "tokens": ["Macht", "eu\u00b7er", "treu\u00b7es", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrem Extrast\u00fcbchen", "tokens": ["In", "ih\u00b7rem", "Ext\u00b7ra\u00b7st\u00fcb\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es auch mit andern so:", "tokens": ["Es", "auch", "mit", "an\u00b7dern", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So d\u00fcrft ihr, statt zu grillen,", "tokens": ["So", "d\u00fcrft", "ihr", ",", "statt", "zu", "gril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur euer Pfeifchen f\u00fcllen,", "tokens": ["Nur", "eu\u00b7er", "Pfei\u00b7fchen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und denken: Fumigo!", "tokens": ["Und", "den\u00b7ken", ":", "Fu\u00b7mi\u00b7go", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn euch der Ehstand h\u00e4rmet,", "tokens": ["Wenn", "euch", "der", "Eh\u00b7stand", "h\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und euer Weibchen l\u00e4rmet,", "tokens": ["Und", "eu\u00b7er", "Weib\u00b7chen", "l\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tobt in eu'rem Haus:", "tokens": ["Und", "tobt", "in", "eu'\u00b7rem", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So steckt, anstatt zu keifen,", "tokens": ["So", "steckt", ",", "an\u00b7statt", "zu", "kei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Flugs in den Mund die Pfeifen,", "tokens": ["Flugs", "in", "den", "Mund", "die", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und spuckt dabei brav aus.", "tokens": ["Und", "spuckt", "da\u00b7bei", "brav", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn Hofgunst euch verf\u00fchret,", "tokens": ["Wenn", "Hof\u00b7gunst", "euch", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schmauchet und studieret", "tokens": ["So", "schmau\u00b7chet", "und", "stu\u00b7die\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dabei der Grossen Gunst:", "tokens": ["Da\u00b7bei", "der", "Gros\u00b7sen", "Gunst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was sie mit vollem Munde", "tokens": ["Was", "sie", "mit", "vol\u00b7lem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Euch geben, ist im Grunde", "tokens": ["Euch", "ge\u00b7ben", ",", "ist", "im", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nichts als ein Maul voll Dunst.", "tokens": ["Nichts", "als", "ein", "Maul", "voll", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn nasenweise Gecken", "tokens": ["Wenn", "na\u00b7sen\u00b7wei\u00b7se", "Ge\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und eitle Narr'n euch necken", "tokens": ["Und", "eit\u00b7le", "Narr'n", "euch", "ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrem Saus und Braus:", "tokens": ["Mit", "ih\u00b7rem", "Saus", "und", "Braus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So setzt euch in den Winkel", "tokens": ["So", "setzt", "euch", "in", "den", "Win\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und pfeift den Eigend\u00fcnkel", "tokens": ["Und", "pfeift", "den", "Ei\u00b7gen\u00b7d\u00fcn\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus vollen Pfeifen aus.", "tokens": ["Aus", "vol\u00b7len", "Pfei\u00b7fen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Umnebelt eu're Geister", "tokens": ["Um\u00b7ne\u00b7belt", "eu'\u00b7re", "Geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ein dicker Seelenkleister;", "tokens": ["Ein", "di\u00b7cker", "See\u00b7len\u00b7kleis\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So brauchet nur daf\u00fcr,", "tokens": ["So", "brau\u00b7chet", "nur", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Um wieder zu genesen,", "tokens": ["Um", "wie\u00b7der", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Und all den Qualm zu l\u00f6sen,", "tokens": ["Und", "all", "den", "Qualm", "zu", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das edle Mundklystier.", "tokens": ["Das", "ed\u00b7le", "Mundk\u00b7lys\u00b7tier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn endlich euch als Greisen,", "tokens": ["Wenn", "end\u00b7lich", "euch", "als", "Grei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich Salomon dem Weisen,", "tokens": ["Gleich", "Sa\u00b7lo\u00b7mon", "dem", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben auch verdrie\u00dft:", "tokens": ["Das", "Le\u00b7ben", "auch", "ver\u00b7drie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So denkt: es ist hienieden", "tokens": ["So", "denkt", ":", "es", "ist", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Euch doch ein Rausch beschieden", "tokens": ["Euch", "doch", "ein", "Rausch", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der nicht ganz eitel ist.", "tokens": ["Der", "nicht", "ganz", "ei\u00b7tel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}