{"textgrid.poem.65080": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als mich des Kampfes Wetterschein umspr\u00fchte,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als mich des Kampfes Wetterschein umspr\u00fchte,", "tokens": ["Als", "mich", "des", "Kamp\u00b7fes", "Wet\u00b7ter\u00b7schein", "um\u00b7spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da war ich stark!", "tokens": ["Da", "war", "ich", "stark", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gerechten Zornes Flammenhauch durchgl\u00fchte", "tokens": ["Ge\u00b7rech\u00b7ten", "Zor\u00b7nes", "Flam\u00b7men\u00b7hauch", "durch\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein innerst' Mark,", "tokens": ["Mein", "in\u00b7ner\u00b7st'", "Mark", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Entr\u00fcstung lieh mir ihre scharfe Wehre", "tokens": ["Ent\u00b7r\u00fcs\u00b7tung", "lieh", "mir", "ih\u00b7re", "schar\u00b7fe", "Weh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich zu befrei'n;", "tokens": ["Mich", "zu", "be\u00b7frei'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Das Gl\u00fcck war hin, so sollte doch die Ehre", "tokens": ["Das", "Gl\u00fcck", "war", "hin", ",", "so", "soll\u00b7te", "doch", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gerettet sein.", "tokens": ["Ge\u00b7ret\u00b7tet", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Jetzt, da der Kampf vorbei und ausgerungen,", "tokens": ["Jetzt", ",", "da", "der", "Kampf", "vor\u00b7bei", "und", "aus\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PTKVZ", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getilgt die Schmach,", "tokens": ["Ge\u00b7tilgt", "die", "Schmach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jetzt f\u00fchl' ich, da\u00df die Kraft, die es durchdrungen,", "tokens": ["Jetzt", "f\u00fchl'", "ich", ",", "da\u00df", "die", "Kraft", ",", "die", "es", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz mir brach.", "tokens": ["Das", "Herz", "mir", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Aufschreit in meiner Brust die Qual, die herbe,", "tokens": ["Auf\u00b7schreit", "in", "mei\u00b7ner", "Brust", "die", "Qual", ",", "die", "her\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die vordem schwieg;", "tokens": ["Die", "vor\u00b7dem", "schwieg", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den hei\u00dfen Kampf bestand ich, ach! und sterbe", "tokens": ["Den", "hei\u00b7\u00dfen", "Kampf", "be\u00b7stand", "ich", ",", "ach", "!", "und", "ster\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ITJ", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An meinem Sieg!", "tokens": ["An", "mei\u00b7nem", "Sieg", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}