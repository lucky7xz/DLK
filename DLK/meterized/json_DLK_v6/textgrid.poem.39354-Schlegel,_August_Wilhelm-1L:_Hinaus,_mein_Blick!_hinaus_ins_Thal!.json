{"textgrid.poem.39354": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinaus, mein Blick! hinaus ins Thal!", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus, mein Blick! hinaus ins Thal!", "tokens": ["Hin\u00b7aus", ",", "mein", "Blick", "!", "hin\u00b7aus", "ins", "Thal", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wohnt noch Lebensf\u00fclle;", "tokens": ["Da", "wohnt", "noch", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da labe dich im Mondenstrahl", "tokens": ["Da", "la\u00b7be", "dich", "im", "Mon\u00b7dens\u00b7trahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und an der heil'gen Stille.", "tokens": ["Und", "an", "der", "heil'\u00b7gen", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da horch nun ungest\u00f6rt, mein Herz,", "tokens": ["Da", "horch", "nun", "un\u00b7ge\u00b7st\u00f6rt", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da horch den leisen Kl\u00e4ngen,", "tokens": ["Da", "horch", "den", "lei\u00b7sen", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die, wie von fern, zu Wonn' und Schmerz", "tokens": ["Die", ",", "wie", "von", "fern", ",", "zu", "Wonn'", "und", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "APPR", "ADJD", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich dir entgegen dr\u00e4ngen.", "tokens": ["Sich", "dir", "ent\u00b7ge\u00b7gen", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie dr\u00e4ngen sich so wunderbar,", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie regen all mein Sehnen.", "tokens": ["Sie", "re\u00b7gen", "all", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O sag' mir, Ahndung, bist du wahr?", "tokens": ["O", "sag'", "mir", ",", "Ahn\u00b7dung", ",", "bist", "du", "wahr", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du ein eitles W\u00e4hnen?", "tokens": ["Bist", "du", "ein", "eit\u00b7les", "W\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird einst mein Aug' in heller Lust,", "tokens": ["Wird", "einst", "mein", "Aug'", "in", "hel\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jetzt in Thr\u00e4nen, l\u00e4cheln?", "tokens": ["Wie", "jetzt", "in", "Thr\u00e4\u00b7nen", ",", "l\u00e4\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird einst die oft emp\u00f6rte Brust", "tokens": ["Wird", "einst", "die", "oft", "em\u00b7p\u00f6r\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir sel'ge Ruh umf\u00e4cheln?", "tokens": ["Mir", "sel'\u00b7ge", "Ruh", "um\u00b7f\u00e4\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und rief' auch die Vernunft mir zu:", "tokens": ["Und", "rie\u00b7f'", "auch", "die", "Ver\u00b7nunft", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbdu mu\u00dft der Ahndung z\u00fcrnen,", "tokens": ["\u00bb", "du", "mu\u00dft", "der", "Ahn\u00b7dung", "z\u00fcr\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es wohnt entz\u00fcckte Seelenruh", "tokens": ["Es", "wohnt", "ent\u00b7z\u00fcck\u00b7te", "See\u00b7len\u00b7ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u00fcber den Gestirnen;\u00ab", "tokens": ["Nur", "\u00fc\u00b7ber", "den", "Ge\u00b7stir\u00b7nen", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch k\u00f6nnt' ich nicht die Schmeichlerin", "tokens": ["Doch", "k\u00f6nnt'", "ich", "nicht", "die", "Schmeich\u00b7le\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus meinem Busen jagen:", "tokens": ["Aus", "mei\u00b7nem", "Bu\u00b7sen", "ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Oft hat sie meinen irren Sinn", "tokens": ["Oft", "hat", "sie", "mei\u00b7nen", "ir\u00b7ren", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gest\u00e4rkt empor getragen.", "tokens": ["Ge\u00b7st\u00e4rkt", "em\u00b7por", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn Ahndung und Erinnerung", "tokens": ["Wenn", "Ahn\u00b7dung", "und", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vor unserm Blick sich gatten,", "tokens": ["Vor", "un\u00b7serm", "Blick", "sich", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann mildert sich zur D\u00e4mmerung", "tokens": ["Dann", "mil\u00b7dert", "sich", "zur", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der Seele tiefster Schatten.", "tokens": ["Der", "See\u00b7le", "tiefs\u00b7ter", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, d\u00fcrften wir mit Tr\u00e4umen nicht", "tokens": ["Ach", ",", "d\u00fcrf\u00b7ten", "wir", "mit", "Tr\u00e4u\u00b7men", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wirklichkeit verweben,", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", "ver\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie arm an Farbe, Glanz und Licht", "tokens": ["Wie", "arm", "an", "Far\u00b7be", ",", "Glanz", "und", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4rst dann du Menschenleben!", "tokens": ["W\u00e4rst", "dann", "du", "Men\u00b7schen\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So hoffet treulich und beharrt", "tokens": ["So", "hof\u00b7fet", "treu\u00b7lich", "und", "be\u00b7harrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz bis hin zum Grabe;", "tokens": ["Das", "Herz", "bis", "hin", "zum", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Lieb' umfa\u00dft's die Gegenwart,", "tokens": ["Mit", "Lieb'", "um\u00b7fa\u00dft's", "die", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcnkt sich reich an Habe.", "tokens": ["Und", "d\u00fcnkt", "sich", "reich", "an", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Habe, die es selbst sich schafft,", "tokens": ["Die", "Ha\u00b7be", ",", "die", "es", "selbst", "sich", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag ihm kein Schicksal rauben:", "tokens": ["Mag", "ihm", "kein", "Schick\u00b7sal", "rau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es lebt und webt in W\u00e4rm' und Kraft,", "tokens": ["Es", "lebt", "und", "webt", "in", "W\u00e4rm'", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Zuversicht und Glauben.", "tokens": ["Durch", "Zu\u00b7ver\u00b7sicht", "und", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und w\u00e4r' in Nacht und Nebeldampf", "tokens": ["Und", "w\u00e4r'", "in", "Nacht", "und", "Ne\u00b7bel\u00b7dampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Alles rings erstorben,", "tokens": ["Auch", "Al\u00b7les", "rings", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df Herz hat l\u00e4ngst f\u00fcr jeden Kampf", "tokens": ["Die\u00df", "Herz", "hat", "l\u00e4ngst", "f\u00fcr", "je\u00b7den", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich einen Schild erworben.", "tokens": ["Sich", "ei\u00b7nen", "Schild", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit hohem Trotz im Ungemach", "tokens": ["Mit", "ho\u00b7hem", "Trotz", "im", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tr\u00e4gt es, was ihm beschieden.", "tokens": ["Tr\u00e4gt", "es", ",", "was", "ihm", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "So schlummr' ich ein, so werd' ich wach,", "tokens": ["So", "schlumm\u00b7r'", "ich", "ein", ",", "so", "werd'", "ich", "wach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "In Lust nicht, doch in Frieden.", "tokens": ["In", "Lust", "nicht", ",", "doch", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}