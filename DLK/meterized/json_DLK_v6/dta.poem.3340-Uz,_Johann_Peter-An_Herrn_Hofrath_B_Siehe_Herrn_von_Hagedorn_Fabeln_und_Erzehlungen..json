{"dta.poem.3340": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr armen Sterblichen, die Wahn und Stolz beth\u00f6ren,", "tokens": ["Ihr", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", ",", "die", "Wahn", "und", "Stolz", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Habt Augen, die nicht sehn, und Ohren, die nicht h\u00f6ren.", "tokens": ["Habt", "Au\u00b7gen", ",", "die", "nicht", "sehn", ",", "und", "Oh\u00b7ren", ",", "die", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,", "KON", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gestehts, der Wahrheit blo\u00df zu Ehren,", "tokens": ["Ge\u00b7stehts", ",", "der", "Wahr\u00b7heit", "blo\u00df", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel dem sch\u00e4rfsten Aug entflieht,", "tokens": ["Wie", "viel", "dem", "sch\u00e4rfs\u00b7ten", "Aug", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das nur ein Dichter sieht.", "tokens": ["Das", "nur", "ein", "Dich\u00b7ter", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seht ihr den Zephyr? Seht ihr Floren,", "tokens": ["Seht", "ihr", "den", "Ze\u00b7phyr", "?", "Seht", "ihr", "Flo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Bluhmen, die sie selbst gebohren?", "tokens": ["Auf", "Bluh\u00b7men", ",", "die", "sie", "selbst", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Soviele nackende Najaden,", "tokens": ["So\u00b7vie\u00b7le", "na\u00b7cken\u00b7de", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Die sich in k\u00fchlen Fluthen baden?", "tokens": ["Die", "sich", "in", "k\u00fch\u00b7len", "Flut\u00b7hen", "ba\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dryaden und Hamadryaden?", "tokens": ["Dry\u00b7a\u00b7den", "und", "Ha\u00b7mad\u00b7ry\u00b7a\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Seht ihr den Gott verliebter Pein", "tokens": ["Seht", "ihr", "den", "Gott", "ver\u00b7lieb\u00b7ter", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf sch\u00f6nen Wangen, sch\u00f6nen Busen?", "tokens": ["Auf", "sch\u00f6\u00b7nen", "Wan\u00b7gen", ",", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Grazien beym Mondenschein?", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "beym", "Mon\u00b7den\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Pegasus und unsre Musen", "tokens": ["Den", "Pe\u00b7ga\u00b7sus", "und", "uns\u00b7re", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und ihren gr\u00fcnen Lorbeerhayn?", "tokens": ["Und", "ih\u00b7ren", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7hayn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gebt Antwort meiner k\u00fchnen Frage:", "tokens": ["Gebt", "Ant\u00b7wort", "mei\u00b7ner", "k\u00fch\u00b7nen", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Seht ihr sie? Nein!", "tokens": ["Seht", "ihr", "sie", "?", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Wir Dichter sehn sie alle Tage.", "tokens": ["Wir", "Dich\u00b7ter", "sehn", "sie", "al\u00b7le", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}