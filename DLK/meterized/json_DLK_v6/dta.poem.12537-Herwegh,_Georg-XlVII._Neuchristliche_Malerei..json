{"dta.poem.12537": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.  \n  Neuchristliche Malerei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr dein heilig Gepinsel empfang' die Palme", "tokens": ["F\u00fcr", "dein", "hei\u00b7lig", "Ge\u00b7pin\u00b7sel", "emp\u00b7fang'", "die", "Pal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "des Jenseits!", "tokens": ["des", "Jen\u00b7seits", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch diesseitigen Kranz hat dir die Muse", "tokens": ["Doch", "dies\u00b7sei\u00b7ti\u00b7gen", "Kranz", "hat", "dir", "die", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "versagt:", "tokens": ["ver\u00b7sagt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Denn du spucktest ins Antlitz der G\u00f6ttlichen,", "tokens": ["Denn", "du", "spuck\u00b7test", "ins", "Ant\u00b7litz", "der", "G\u00f6tt\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.6": {"text": "setzest im Knechtssinn", "tokens": ["set\u00b7zest", "im", "Knechts\u00b7sinn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ihr selbstleuchtend Gestirn frech zum Tra-", "tokens": ["Ihr", "selbst\u00b7leuch\u00b7tend", "Ge\u00b7stirn", "frech", "zum", "Tra"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "ADJD", "APPRART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "banten herab!", "tokens": ["ban\u00b7ten", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}