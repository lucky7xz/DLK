{"textgrid.poem.31642": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "14. Der Herr ist des Armen Schutz", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann des H\u00f6chsten Schirmungs Schilde schwebet ob der Armen Haupt /", "tokens": ["Wann", "des", "H\u00f6chs\u00b7ten", "Schir\u00b7mungs", "Schil\u00b7de", "schwe\u00b7bet", "ob", "der", "Ar\u00b7men", "Haupt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "VVFIN", "KOUS", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "sind sie (ob sie auch schon sonsten alles Erden-Trosts beraubt)", "tokens": ["sind", "sie", "(", "ob", "sie", "auch", "schon", "sons\u00b7ten", "al\u00b7les", "Er\u00b7den\u00b7Trosts", "be\u00b7raubt", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "seelig: dann / wen Gott beschutzt / dem kan alle Welt nit schaden.", "tokens": ["see\u00b7lig", ":", "dann", "/", "wen", "Gott", "be\u00b7schutzt", "/", "dem", "kan", "al\u00b7le", "Welt", "nit", "scha\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "$(", "PWS", "NN", "VVPP", "$(", "PDS", "VMFIN", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Vnumst\u00f6\u00dflich ist die Maur / der umringten Gottes-Gnaden.", "tokens": ["Vnum\u00b7st\u00f6\u00df\u00b7lich", "ist", "die", "Maur", "/", "der", "um\u00b7ring\u00b7ten", "Got\u00b7tes\u00b7Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Seyd daher getrost / ihr Armen! eure Noht / hat keine Noht:", "tokens": ["Seyd", "da\u00b7her", "ge\u00b7trost", "/", "ihr", "Ar\u00b7men", "!", "eu\u00b7re", "Noht", "/", "hat", "kei\u00b7ne", "Noht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PAV", "VVPP", "$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$(", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Vrsach / dieser / der euch schutzet / ist der Allbeherrschend Gott.", "tokens": ["Vr\u00b7sach", "/", "die\u00b7ser", "/", "der", "euch", "schut\u00b7zet", "/", "ist", "der", "All\u00b7be\u00b7herr\u00b7schend", "Gott", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDAT", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}