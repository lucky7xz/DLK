{"dta.poem.14093": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tanzmeister.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlger\u00fcstet war ich kommen;", "tokens": ["Wohl\u00b7ge\u00b7r\u00fcs\u00b7tet", "war", "ich", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siegsgewi\u00df doch, wie zum Scherz,", "tokens": ["Siegs\u00b7ge\u00b7wi\u00df", "doch", ",", "wie", "zum", "Scherz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat ein Blick mein Herz genommen \u2014", "tokens": ["Hat", "ein", "Blick", "mein", "Herz", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer kann k\u00e4mpfen ohne Herz?", "tokens": ["Wer", "kann", "k\u00e4mp\u00b7fen", "oh\u00b7ne", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}