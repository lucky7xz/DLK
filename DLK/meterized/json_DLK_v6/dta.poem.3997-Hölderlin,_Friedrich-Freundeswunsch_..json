{"dta.poem.3997": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Freundeswunsch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn vom Fr\u00fchling rund umschlungen,", "tokens": ["Wenn", "vom", "Fr\u00fch\u00b7ling", "rund", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Morgens Hauch umweht,", "tokens": ["Von", "des", "Mor\u00b7gens", "Hauch", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trunken nach Erinnerungen", "tokens": ["Trun\u00b7ken", "nach", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine wache Seele sp\u00e4ht;", "tokens": ["Mei\u00b7ne", "wa\u00b7che", "See\u00b7le", "sp\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn, wie einst am fernen Herde,", "tokens": ["Wenn", ",", "wie", "einst", "am", "fer\u00b7nen", "Her\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mir so s\u00fc\u00df die Sonne blinkt,", "tokens": ["Mir", "so", "s\u00fc\u00df", "die", "Son\u00b7ne", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr Stral in's Herz der Erde", "tokens": ["Und", "ihr", "Stral", "in's", "Herz", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Erdenkinder dringt;", "tokens": ["Und", "der", "Er\u00b7den\u00b7kin\u00b7der", "dringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn, umd\u00e4mmert von der Weide,", "tokens": ["Wenn", ",", "um\u00b7d\u00e4m\u00b7mert", "von", "der", "Wei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Bach vor\u00fcber rinnt,", "tokens": ["Wo", "der", "Bach", "vor\u00b7\u00fc\u00b7ber", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief bewegt von Leid und Freude,", "tokens": ["Tief", "be\u00b7wegt", "von", "Leid", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele tr\u00e4umt und sinnt;", "tokens": ["Mei\u00b7ne", "See\u00b7le", "tr\u00e4umt", "und", "sinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn im Haine Geister s\u00e4useln,", "tokens": ["Wenn", "im", "Hai\u00b7ne", "Geis\u00b7ter", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn im Mondenschimmer sich", "tokens": ["Wenn", "im", "Mon\u00b7den\u00b7schim\u00b7mer", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kaum die stillen Teiche kr\u00e4useln:", "tokens": ["Kaum", "die", "stil\u00b7len", "Tei\u00b7che", "kr\u00e4u\u00b7seln", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schau ich oft und gr\u00fc\u00dfe dich.", "tokens": ["Schau", "ich", "oft", "und", "gr\u00fc\u00b7\u00dfe", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Edles Herz, du bist der Sterne", "tokens": ["Ed\u00b7les", "Herz", ",", "du", "bist", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der sch\u00f6nen Erde werth,", "tokens": ["Und", "der", "sch\u00f6\u00b7nen", "Er\u00b7de", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist des werth, so viel die ferne", "tokens": ["Bist", "des", "werth", ",", "so", "viel", "die", "fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "$,", "ADV", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahe Mutter Dir beschert.", "tokens": ["Na\u00b7he", "Mut\u00b7ter", "Dir", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh', mit Deiner Liebe lieben", "tokens": ["Sieh'", ",", "mit", "Dei\u00b7ner", "Lie\u00b7be", "lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6nes die Erw\u00e4hlten nur;", "tokens": ["Sch\u00f6\u00b7nes", "die", "Er\u00b7w\u00e4hl\u00b7ten", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Du bist ihr treu geblieben,", "tokens": ["Denn", "Du", "bist", "ihr", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Mutter, der Natur.", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", ",", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Gesang der Haine schalle", "tokens": ["Der", "Ge\u00b7sang", "der", "Hai\u00b7ne", "schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh, wie Du, um Deinen Pfad;", "tokens": ["Froh", ",", "wie", "Du", ",", "um", "Dei\u00b7nen", "Pfad", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft bewegt vom Weste, walle,", "tokens": ["Sanft", "be\u00b7wegt", "vom", "Wes\u00b7te", ",", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Dein friedlich Herz, die Saat!", "tokens": ["Wie", "Dein", "fried\u00b7lich", "Herz", ",", "die", "Saat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine liebste Bl\u00fcthe regne,", "tokens": ["Dei\u00b7ne", "liebs\u00b7te", "Bl\u00fc\u00b7the", "reg\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo Du wandelst, auf die Flur,", "tokens": ["Wo", "Du", "wan\u00b7delst", ",", "auf", "die", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo Dein Auge weilt, begegne,", "tokens": ["Wo", "Dein", "Au\u00b7ge", "weilt", ",", "be\u00b7geg\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir das L\u00e4cheln der Natur!", "tokens": ["Dir", "das", "L\u00e4\u00b7cheln", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft im stillen Tannenhaine", "tokens": ["Oft", "im", "stil\u00b7len", "Tan\u00b7nen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Webe Dir um's Angesicht", "tokens": ["We\u00b7be", "Dir", "um's", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine zauberische, reine", "tokens": ["Sei\u00b7ne", "zau\u00b7be\u00b7ri\u00b7sche", ",", "rei\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glorie das Abendlicht!", "tokens": ["Glo\u00b7rie", "das", "A\u00b7bend\u00b7licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Deines Herzens Sorge wiege", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "Sor\u00b7ge", "wie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drauf die Nacht in s\u00fc\u00dfe Ruh'", "tokens": ["Drauf", "die", "Nacht", "in", "s\u00fc\u00b7\u00dfe", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die freie Seele fliege", "tokens": ["Und", "die", "frei\u00b7e", "See\u00b7le", "flie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebend den Gestirnen zu!", "tokens": ["Lie\u00b7bend", "den", "Ge\u00b7stir\u00b7nen", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}