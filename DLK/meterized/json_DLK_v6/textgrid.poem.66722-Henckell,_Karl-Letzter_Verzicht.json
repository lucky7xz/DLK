{"textgrid.poem.66722": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Verzicht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann es nicht in Worten sagen,", "tokens": ["Ich", "kann", "es", "nicht", "in", "Wor\u00b7ten", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich im Innersten erf\u00fcllt:", "tokens": ["Was", "mich", "im", "In\u00b7ners\u00b7ten", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Worte sind wie abgetragen", "tokens": ["Wor\u00b7te", "sind", "wie", "ab\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "KOKOM", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bettlergewand, das einen Gott verh\u00fcllt.", "tokens": ["Bett\u00b7ler\u00b7ge\u00b7wand", ",", "das", "ei\u00b7nen", "Gott", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Von meinem Gott kann ich nicht prahlen", "tokens": ["Von", "mei\u00b7nem", "Gott", "kann", "ich", "nicht", "prah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit eitel Klangger\u00e4usch und Ruhm,", "tokens": ["Mit", "ei\u00b7tel", "Klang\u00b7ge\u00b7r\u00e4usch", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit armem Sprachgem\u00fcnz bezahlen", "tokens": ["Mit", "ar\u00b7mem", "Sprach\u00b7ge\u00b7m\u00fcnz", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Zutritt in mein tiefstes Heiligtum.", "tokens": ["Den", "Zu\u00b7tritt", "in", "mein", "tiefs\u00b7tes", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schweigend mu\u00df ich der Kraft vertrauen,", "tokens": ["Schwei\u00b7gend", "mu\u00df", "ich", "der", "Kraft", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die k\u00fcndet jeder Atemzug,", "tokens": ["Die", "k\u00fcn\u00b7det", "je\u00b7der", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die aus dem Staub mit Adlerklauen", "tokens": ["Die", "aus", "dem", "Staub", "mit", "Ad\u00b7ler\u00b7klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich zu des Lebens reinen H\u00f6hen trug.", "tokens": ["Mich", "zu", "des", "Le\u00b7bens", "rei\u00b7nen", "H\u00f6\u00b7hen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}