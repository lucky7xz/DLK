{"dta.poem.7316": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Was redet ihr so viel von Angst und Noth,", "tokens": ["Was", "re\u00b7det", "ihr", "so", "viel", "von", "Angst", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In eurem tadellosen Treiben?", "tokens": ["In", "eu\u00b7rem", "ta\u00b7del\u00b7lo\u00b7sen", "Trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr frommen Leute, schlagt die Sorge todt,", "tokens": ["Ihr", "from\u00b7men", "Leu\u00b7te", ",", "schlagt", "die", "Sor\u00b7ge", "todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie will ja doch nicht bei euch bleiben!", "tokens": ["Sie", "will", "ja", "doch", "nicht", "bei", "euch", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wo die Noth, um die das Mitleid weint,", "tokens": ["Doch", "wo", "die", "Noth", ",", "um", "die", "das", "Mit\u00b7leid", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur wie der Tropfen an des Trinkers Hand,", "tokens": ["Nur", "wie", "der", "Trop\u00b7fen", "an", "des", "Trin\u00b7kers", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df die dunkle Fluth, die Keiner meint,", "tokens": ["In\u00b7de\u00df", "die", "dunk\u00b7le", "Fluth", ",", "die", "Kei\u00b7ner", "meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verborgen steht bis an der Seele Rand \u2014", "tokens": ["Ver\u00b7bor\u00b7gen", "steht", "bis", "an", "der", "See\u00b7le", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr frommen Leute wollt' die Sorge kennen,", "tokens": ["Ihr", "from\u00b7men", "Leu\u00b7te", "wollt'", "die", "Sor\u00b7ge", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und habt doch nie die Schuld gesehn!", "tokens": ["Und", "habt", "doch", "nie", "die", "Schuld", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sie, sie d\u00fcrfen schon das Leben nennen", "tokens": ["Doch", "sie", ",", "sie", "d\u00fcr\u00b7fen", "schon", "das", "Le\u00b7ben", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine grauenvollen H\u00f6hn;", "tokens": ["Und", "sei\u00b7ne", "grau\u00b7en\u00b7vol\u00b7len", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hinauf schallt's wie Gesang und Loben,", "tokens": ["Hin\u00b7auf", "schallt's", "wie", "Ge\u00b7sang", "und", "Lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und um die Blumen spielt der Strahl,", "tokens": ["Und", "um", "die", "Blu\u00b7men", "spielt", "der", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Menschen wohnen still im Thal,", "tokens": ["Die", "Men\u00b7schen", "woh\u00b7nen", "still", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dunklen Geyer horsten droben.", "tokens": ["Die", "dunk\u00b7len", "Ge\u00b7yer", "hors\u00b7ten", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}