{"textgrid.poem.40682": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwem geb\u00fchre Kranz und Preis,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwem geb\u00fchre Kranz und Preis,", "tokens": ["\u00bb", "wem", "ge\u00b7b\u00fch\u00b7re", "Kranz", "und", "Preis", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem verneige sich mein Haupt:", "tokens": ["Wem", "ver\u00b7nei\u00b7ge", "sich", "mein", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenem, der zu glauben wei\u00df,", "tokens": ["Je\u00b7nem", ",", "der", "zu", "glau\u00b7ben", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesem, der zu wissen glaubt?\u00ab", "tokens": ["Die\u00b7sem", ",", "der", "zu", "wis\u00b7sen", "glaubt", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft die Palme dem nicht rauben,", "tokens": ["La\u00dft", "die", "Pal\u00b7me", "dem", "nicht", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem sein Glauben ward ein Wissen;", "tokens": ["Wem", "sein", "Glau\u00b7ben", "ward", "ein", "Wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft den Lorbeer den nicht missen,", "tokens": ["La\u00dft", "den", "Lor\u00b7beer", "den", "nicht", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wem das Wissen ward zum Glauben.", "tokens": ["Wem", "das", "Wis\u00b7sen", "ward", "zum", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt nicht diesen Kampf beklagen,", "tokens": ["Wollt", "nicht", "die\u00b7sen", "Kampf", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenen Frieden nicht beneiden;", "tokens": ["Je\u00b7nen", "Frie\u00b7den", "nicht", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Merkt nur, da\u00df in Lagern beiden", "tokens": ["Merkt", "nur", ",", "da\u00df", "in", "La\u00b7gern", "bei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "APPR", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Helden stolze Kronen tragen.", "tokens": ["Hel\u00b7den", "stol\u00b7ze", "Kro\u00b7nen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}