{"textgrid.poem.37621": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Belehrende Entschuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du z\u00fcrnst, weil ich dir um den Hals gefallen,", "tokens": ["Du", "z\u00fcrnst", ",", "weil", "ich", "dir", "um", "den", "Hals", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als heut dein Mund so freudig zu mir sprach,", "tokens": ["Als", "heut", "dein", "Mund", "so", "freu\u00b7dig", "zu", "mir", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df meine Freude dir im Ku\u00df erschallen,", "tokens": ["La\u00df", "mei\u00b7ne", "Freu\u00b7de", "dir", "im", "Ku\u00df", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein L\u00e4cheln suchte sich ein freundlich Dach:", "tokens": ["Mein", "L\u00e4\u00b7cheln", "such\u00b7te", "sich", "ein", "freund\u00b7lich", "Dach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein solcher Ku\u00df, er deutet sich nicht weiter,", "tokens": ["Ein", "sol\u00b7cher", "Ku\u00df", ",", "er", "deu\u00b7tet", "sich", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er l\u00f6scht sich wie ein hellgefallner Stern,", "tokens": ["Er", "l\u00f6scht", "sich", "wie", "ein", "hell\u00b7ge\u00b7fall\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Himmel scheint dahinter ewig heiter,", "tokens": ["Der", "Him\u00b7mel", "scheint", "da\u00b7hin\u00b7ter", "e\u00b7wig", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im tiefen Blau verliert er sich so gern.", "tokens": ["Im", "tie\u00b7fen", "Blau", "ver\u00b7liert", "er", "sich", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Gl\u00fccke ist ein h\u00f6heres Ber\u00fchren,", "tokens": ["Im", "Gl\u00fc\u00b7cke", "ist", "ein", "h\u00f6\u00b7he\u00b7res", "Be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind vereint von seiner Wunderkraft,", "tokens": ["Wir", "sind", "ver\u00b7eint", "von", "sei\u00b7ner", "Wun\u00b7der\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sollten wir um Zeichen uns noch zieren,", "tokens": ["Was", "soll\u00b7ten", "wir", "um", "Zei\u00b7chen", "uns", "noch", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir hatten uns so lange angegafft:", "tokens": ["Wir", "hat\u00b7ten", "uns", "so", "lan\u00b7ge", "an\u00b7ge\u00b7gafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie macht's die Rebe, will sie sich erheben?", "tokens": ["Wie", "macht's", "die", "Re\u00b7be", ",", "will", "sie", "sich", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit sich allein, sie hat doch keine Ruh!", "tokens": ["Mit", "sich", "al\u00b7lein", ",", "sie", "hat", "doch", "kei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "$,", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O h\u00e4ng' dich an die Welt wie diese Reben,", "tokens": ["O", "h\u00e4ng'", "dich", "an", "die", "Welt", "wie", "die\u00b7se", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und deck ihr dennoch deine Trauben zu!", "tokens": ["Und", "deck", "ihr", "den\u00b7noch", "dei\u00b7ne", "Trau\u00b7ben", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}