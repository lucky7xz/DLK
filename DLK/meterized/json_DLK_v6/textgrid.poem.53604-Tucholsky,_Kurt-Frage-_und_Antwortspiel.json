{"textgrid.poem.53604": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frage- und Antwortspiel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo holder Knabe, sag an! sag an!", "tokens": ["\u00bb", "o", "hol\u00b7der", "Kna\u00b7be", ",", "sag", "an", "!", "sag", "an", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Braucht Deutschland denn noch einen gro\u00dfen Mann?", "tokens": ["Braucht", "Deutschland", "denn", "noch", "ei\u00b7nen", "gro\u00b7\u00dfen", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hat W. T. B. dich nicht eingelullt?", "tokens": ["Hat", "W.", "T.", "B.", "dich", "nicht", "ein\u00b7ge\u00b7lullt", "?"], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Wetter ist an allem schuld \u2013", "tokens": ["Das", "Wet\u00b7ter", "ist", "an", "al\u00b7lem", "schuld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Regierung aber beileibe nicht,", "tokens": ["die", "Re\u00b7gie\u00b7rung", "a\u00b7ber", "bei\u00b7lei\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "sondern tut dieselbe treu ihre Pflicht.", "tokens": ["son\u00b7dern", "tut", "die\u00b7sel\u00b7be", "treu", "ih\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Die Noske-Rose entsendet nur Duft . . . \u00ab", "tokens": ["Die", "Nos\u00b7ke\u00b7Ro\u00b7se", "ent\u00b7sen\u00b7det", "nur", "Duft", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$.", "$.", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u00bbdir ham se woll mit de Muffe jebufft?\u00ab", "tokens": ["\u00bb", "dir", "ham", "se", "woll", "mit", "de", "Muf\u00b7fe", "je\u00b7bufft", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "FM", "FM", "ADV", "APPR", "NE", "NN", "ADV", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbo holder Knabe, sag an! sag an!", "tokens": ["\u00bb", "o", "hol\u00b7der", "Kna\u00b7be", ",", "sag", "an", "!", "sag", "an", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was hat man mit Nicolai getan?", "tokens": ["Was", "hat", "man", "mit", "Ni\u00b7co\u00b7lai", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der brave deutsche Studiker l\u00e4rmt,", "tokens": ["Der", "bra\u00b7ve", "deut\u00b7sche", "Stu\u00b7di\u00b7ker", "l\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "weil ihn des Professors Gesinnung h\u00e4rmt.", "tokens": ["weil", "ihn", "des", "Pro\u00b7fes\u00b7sors", "Ge\u00b7sin\u00b7nung", "h\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Den Deserteur von Amerongen", "tokens": ["Den", "De\u00b7ser\u00b7teur", "von", "A\u00b7me\u00b7ron\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "hat der Student nur angesongen.", "tokens": ["hat", "der", "Stu\u00b7dent", "nur", "an\u00b7ge\u00b7son\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Er ist national. Er ist nicht verludert . . . \u00ab", "tokens": ["Er", "ist", "na\u00b7ti\u00b7o\u00b7nal", ".", "Er", "ist", "nicht", "ver\u00b7lu\u00b7dert", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "$.", "$.", "$("], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u00bbdir ham se woll mitn Klammerbeutel gepudert?\u00ab", "tokens": ["\u00bb", "dir", "ham", "se", "woll", "mitn", "Klam\u00b7mer\u00b7beu\u00b7tel", "ge\u00b7pu\u00b7dert", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "FM", "FM", "ADV", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbo holder Knabe, sag an! sag an!", "tokens": ["\u00bb", "o", "hol\u00b7der", "Kna\u00b7be", ",", "sag", "an", "!", "sag", "an", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wor\u00fcber ich oft schon vergeblich sann:", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ich", "oft", "schon", "ver\u00b7geb\u00b7lich", "sann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann man nicht, um nach links zu beschwichtigen,", "tokens": ["Kann", "man", "nicht", ",", "um", "nach", "links", "zu", "be\u00b7schwich\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "$,", "KOUI", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "den Kurs ein wenig, ein wenig berichtigen?", "tokens": ["den", "Kurs", "ein", "we\u00b7nig", ",", "ein", "we\u00b7nig", "be\u00b7rich\u00b7ti\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "$,", "ART", "PIS", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Schlie\u00dflich, pardon (ich bin nicht spartakistisch!),", "tokens": ["Schlie\u00df\u00b7lich", ",", "par\u00b7don", "(", "ich", "bin", "nicht", "spar\u00b7ta\u00b7kis\u00b7tisch", "!", ")", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$,", "NN", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "$(", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "man munkelt, wir seien nicht mehr monarchistisch . . .", "tokens": ["man", "mun\u00b7kelt", ",", "wir", "sei\u00b7en", "nicht", "mehr", "mon\u00b7ar\u00b7chis\u00b7tisch", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Aber ich bin des Vertrauens voll . . . \u00ab", "tokens": ["A\u00b7ber", "ich", "bin", "des", "Ver\u00b7trau\u00b7ens", "voll", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "$.", "$.", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "\u00bbmanoli linksrum! Dir pipt er woll?\u00ab", "tokens": ["\u00bb", "ma\u00b7no\u00b7li", "links\u00b7rum", "!", "Dir", "pipt", "er", "woll", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "$.", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbo holder Knabe, sag an! sag an!", "tokens": ["\u00bb", "o", "hol\u00b7der", "Kna\u00b7be", ",", "sag", "an", "!", "sag", "an", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ich durchaus nicht verstehen kann:", "tokens": ["Was", "ich", "durc\u00b7haus", "nicht", "ver\u00b7ste\u00b7hen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Links k\u00f6nnen sie nichts als alarmieren", "tokens": ["Links", "k\u00f6n\u00b7nen", "sie", "nichts", "als", "a\u00b7larm\u00b7ie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "KOKOM", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und, wenn das Blut spritzt, retirieren.", "tokens": ["und", ",", "wenn", "das", "Blut", "spritzt", ",", "re\u00b7ti\u00b7rie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Rechts warten sie auf die Konjunktur.", "tokens": ["Rechts", "war\u00b7ten", "sie", "auf", "die", "Kon\u00b7junk\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Was meinst du, tut die Regierung nur?", "tokens": ["Was", "meinst", "du", ",", "tut", "die", "Re\u00b7gie\u00b7rung", "nur", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie steht zwischen Vorw\u00e4rts und Klerisei", "tokens": ["Sie", "steht", "zwi\u00b7schen", "Vor\u00b7w\u00e4rts", "und", "Kle\u00b7ri\u00b7sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "\u00fcber (oder unter) jeder Partei . . . \u00ab", "tokens": ["\u00fc\u00b7ber", "(", "o\u00b7der", "un\u00b7ter", ")", "je\u00b7der", "Par\u00b7tei", ".", ".", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "$(", "KON", "APPR", "$(", "PIAT", "NN", "$.", "$.", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "\u00bbmensch, ich glaube, du bist aus die Reichskanzlei \u2013!\u00ab", "tokens": ["\u00bb", "mensch", ",", "ich", "glau\u00b7be", ",", "du", "bist", "aus", "die", "Reichs\u00b7kanz\u00b7lei", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "$(", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}