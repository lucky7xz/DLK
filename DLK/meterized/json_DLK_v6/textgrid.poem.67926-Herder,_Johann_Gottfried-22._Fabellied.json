{"textgrid.poem.67926": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22. Fabellied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal in einem tiefen Thal", "tokens": ["Ein\u00b7mal", "in", "ei\u00b7nem", "tie\u00b7fen", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kukuk und die Nachtigal", "tokens": ["Der", "Ku\u00b7kuk", "und", "die", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eine Wett' th\u00e4ten anschlagen,", "tokens": ["Ei\u00b7ne", "Wett'", "th\u00e4\u00b7ten", "an\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Zu singen um das Meisterst\u00fcck:", "tokens": ["Zu", "sin\u00b7gen", "um", "das", "Meis\u00b7ter\u00b7st\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wers gew\u00f6nn' aus Kunst oder aus Gl\u00fcck;", "tokens": ["Wers", "ge\u00b7w\u00f6nn'", "aus", "Kunst", "o\u00b7der", "aus", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Dank sollt' er davon tragen.", "tokens": ["Dank", "sollt'", "er", "da\u00b7von", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kukuk sprach: \u00bbso dirs gef\u00e4llt,", "tokens": ["Der", "Ku\u00b7kuk", "sprach", ":", "\u00bb", "so", "dirs", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab der Sach einen Richter erw\u00e4hlt.\u00ab", "tokens": ["Hab", "der", "Sach", "ei\u00b7nen", "Rich\u00b7ter", "er\u00b7w\u00e4hlt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und th\u00e4t den Esel nennen.", "tokens": ["Und", "th\u00e4t", "den", "E\u00b7sel", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-----", "measure": "dactylic.init"}, "line.4": {"text": "\u00bbdenn weil der hat zwei Ohren gro\u00df,", "tokens": ["\u00bb", "denn", "weil", "der", "hat", "zwei", "Oh\u00b7ren", "gro\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "ART", "VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kann er h\u00f6ren desto ba\u00df", "tokens": ["So", "kann", "er", "h\u00f6\u00b7ren", "des\u00b7to", "ba\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was recht ist, erkennen!\u00ab", "tokens": ["Und", "was", "recht", "ist", ",", "er\u00b7ken\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als ihm die Sach nun ward erz\u00e4hlt,", "tokens": ["Als", "ihm", "die", "Sach", "nun", "ward", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und er zu richten hat Gewalt,", "tokens": ["Und", "er", "zu", "rich\u00b7ten", "hat", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schuf er: sie sollen singen!", "tokens": ["Schuf", "er", ":", "sie", "sol\u00b7len", "sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Nachtigal sang lieblich aus:", "tokens": ["Die", "Nach\u00b7ti\u00b7gal", "sang", "lieb\u00b7lich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Esel sprach: \u00bbdu machst mirs kraus;", "tokens": ["Der", "E\u00b7sel", "sprach", ":", "\u00bb", "du", "machst", "mirs", "kraus", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kanns in Kopf nicht bringen.\u00ab", "tokens": ["Ich", "kanns", "in", "Kopf", "nicht", "brin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Kukuk fing auch an und sang,", "tokens": ["Der", "Ku\u00b7kuk", "fing", "auch", "an", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er denn pflegt zu singen:", "tokens": ["Wie", "er", "denn", "pflegt", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kukuk, Kukuk! lacht fein darein,", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", "!", "lacht", "fein", "da\u00b7rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "ADJD", "PAV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das gefiel dem Esel im Sinne sein,", "tokens": ["Das", "ge\u00b7fiel", "dem", "E\u00b7sel", "im", "Sin\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er sprach: \u00bbin allen Rechten", "tokens": ["Er", "sprach", ":", "\u00bb", "in", "al\u00b7len", "Rech\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will ich ein Urtheil sprechen.", "tokens": ["Will", "ich", "ein", "Ur\u00b7theil", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hast wohl gesungen, Nachtigal!", "tokens": ["Hast", "wohl", "ge\u00b7sun\u00b7gen", ",", "Nach\u00b7ti\u00b7gal", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber Kukuk singt gut Choral,", "tokens": ["A\u00b7ber", "Ku\u00b7kuk", "singt", "gut", "Cho\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00e4lt den Tackt fein innen.", "tokens": ["Und", "h\u00e4lt", "den", "Tackt", "fein", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sprech ich nach meinem hohen Verstand,", "tokens": ["Das", "sprech", "ich", "nach", "mei\u00b7nem", "ho\u00b7hen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ob es g\u00f6lt ein ganzes Land,", "tokens": ["Und", "ob", "es", "g\u00f6lt", "ein", "gan\u00b7zes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df ichs dich gewinnen.\u00ab", "tokens": ["So", "la\u00df", "ichs", "dich", "ge\u00b7win\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}