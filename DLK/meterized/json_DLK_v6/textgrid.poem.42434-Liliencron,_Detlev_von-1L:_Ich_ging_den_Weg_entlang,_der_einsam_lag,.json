{"textgrid.poem.42434": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging den Weg entlang, der einsam lag,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging den Weg entlang, der einsam lag,", "tokens": ["Ich", "ging", "den", "Weg", "ent\u00b7lang", ",", "der", "ein\u00b7sam", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den stets allein ich gehe jeden Tag.", "tokens": ["Den", "stets", "al\u00b7lein", "ich", "ge\u00b7he", "je\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Heide schweigt, das Feld ist menschenleer,", "tokens": ["Die", "Hei\u00b7de", "schweigt", ",", "das", "Feld", "ist", "men\u00b7schen\u00b7leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Wind nur webt im Knickbusch um mich her.", "tokens": ["Der", "Wind", "nur", "webt", "im", "Knick\u00b7busch", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weit liegt vor mir die Stra\u00dfe ausgedehnt,", "tokens": ["Weit", "liegt", "vor", "mir", "die", "Stra\u00b7\u00dfe", "aus\u00b7ge\u00b7dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es hat mein Herz nur dich, nur dich ersehnt.", "tokens": ["Es", "hat", "mein", "Herz", "nur", "dich", ",", "nur", "dich", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PPER", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und k\u00e4mest du, ein Wunder w\u00e4r's f\u00fcr mich,", "tokens": ["Und", "k\u00e4\u00b7mest", "du", ",", "ein", "Wun\u00b7der", "w\u00e4r's", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich neigte mich vor dir: ich liebe dich.", "tokens": ["Ich", "neig\u00b7te", "mich", "vor", "dir", ":", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und im Begegnen, nur ein einziger Blick,", "tokens": ["Und", "im", "Be\u00b7geg\u00b7nen", ",", "nur", "ein", "ein\u00b7zi\u00b7ger", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Des ganzen Lebens w\u00e4r' es mein Geschick.", "tokens": ["Des", "gan\u00b7zen", "Le\u00b7bens", "w\u00e4r'", "es", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und richtest du dein Auge kalt auf mich,", "tokens": ["Und", "rich\u00b7test", "du", "dein", "Au\u00b7ge", "kalt", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich trotze, M\u00e4dchen, dir: ich liebe dich.", "tokens": ["Ich", "trot\u00b7ze", ",", "M\u00e4d\u00b7chen", ",", "dir", ":", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch wenn dein sch\u00f6nes Auge gr\u00fc\u00dft und lacht", "tokens": ["Doch", "wenn", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "gr\u00fc\u00dft", "und", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie eine Sonne mir in schwerer Nacht,", "tokens": ["Wie", "ei\u00b7ne", "Son\u00b7ne", "mir", "in", "schwe\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich z\u00f6ge rasch dein s\u00fc\u00dfes Herz an mich", "tokens": ["Ich", "z\u00f6\u00b7ge", "rasch", "dein", "s\u00fc\u00b7\u00dfes", "Herz", "an", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und fl\u00fcstre leise dir: ich liebe dich.", "tokens": ["Und", "fl\u00fcst\u00b7re", "lei\u00b7se", "dir", ":", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}