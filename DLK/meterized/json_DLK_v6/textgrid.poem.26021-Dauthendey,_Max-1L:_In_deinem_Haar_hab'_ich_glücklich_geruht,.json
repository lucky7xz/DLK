{"textgrid.poem.26021": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: In deinem Haar hab' ich gl\u00fccklich geruht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deinem Haar hab' ich gl\u00fccklich geruht,", "tokens": ["In", "dei\u00b7nem", "Haar", "hab'", "ich", "gl\u00fcck\u00b7lich", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lieg' nun in Gedanken, wie weh das tut.", "tokens": ["Lieg'", "nun", "in", "Ge\u00b7dan\u00b7ken", ",", "wie", "weh", "das", "tut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Blut schl\u00e4ft ein in meiner Hand;", "tokens": ["Das", "Blut", "schl\u00e4ft", "ein", "in", "mei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie in Spiegel starr' ich in jede Wand.", "tokens": ["Wie", "in", "Spie\u00b7gel", "starr'", "ich", "in", "je\u00b7de", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mein Herz erdr\u00fcckt mich und ist doch so leer,", "tokens": ["Mein", "Herz", "er\u00b7dr\u00fcckt", "mich", "und", "ist", "doch", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mehr als Erde ist oft ein Gedanke schwer.", "tokens": ["Mehr", "als", "Er\u00b7de", "ist", "oft", "ein", "Ge\u00b7dan\u00b7ke", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}}}}