{"dta.poem.21256": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig.  \n 1766.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Friedrich! du, dem ein Gott das\nf&#252;r die Sterblichen", "tokens": ["Fried\u00b7rich", "!", "du", ",", "dem", "ein", "Gott", "das", "f", "&#252;", "r", "die", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zu gef\u00e4hrliche Loos eines Monarchen gab,", "tokens": ["Zu", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Loos", "ei\u00b7nes", "Mon\u00b7ar\u00b7chen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und, o Wunder! der du glorreich dein", "tokens": ["Und", ",", "o", "Wun\u00b7der", "!", "der", "du", "glor\u00b7reich", "dein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$.", "ART", "PPER", "ADJD", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Loos erf\u00fcllst,", "tokens": ["Loos", "er\u00b7f\u00fcllst", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Siehe! deiner von Ruhm trunkenen Tage", "tokens": ["Sie\u00b7he", "!", "dei\u00b7ner", "von", "Ruhm", "trun\u00b7ke\u00b7nen", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PPOSAT", "APPR", "NN", "ADJA", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "sind", "tokens": ["sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Zwanzig tausend entflohn! Ihnen folgt", "tokens": ["Zwan\u00b7zig", "tau\u00b7send", "ent\u00b7flohn", "!", "Ih\u00b7nen", "folgt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "CARD", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "allzubald", "tokens": ["all\u00b7zu\u00b7bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Jedes Denkmaal von dir; alle die Tem-", "tokens": ["Je\u00b7des", "Denk\u00b7maal", "von", "dir", ";", "al\u00b7le", "die", "Tem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "$.", "PIS", "ART", "TRUNC"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Pallas, und dem Apoll, und dem verwundeten", "tokens": ["Pal\u00b7las", ",", "und", "dem", "A\u00b7poll", ",", "und", "dem", "ver\u00b7wun\u00b7de\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ART", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Kriegesgotte geweiht, werden Ruinen seyn.", "tokens": ["Krie\u00b7ges\u00b7got\u00b7te", "ge\u00b7weiht", ",", "wer\u00b7den", "Ru\u00b7i\u00b7nen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VAFIN", "NN", "VAINF", "$."], "meter": "+-+--+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Zwar das Jahrbuch der Welt nennt, wann", "tokens": ["Zwar", "das", "Jahr\u00b7buch", "der", "Welt", "nennt", ",", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PWAV"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.13": {"text": "Stolzer K\u00f6nige schl\u00e4st, dich den Eroberer,", "tokens": ["Stol\u00b7zer", "K\u00f6\u00b7ni\u00b7ge", "schl\u00e4st", ",", "dich", "den", "Er\u00b7o\u00b7be\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Dich den Grossen: doch ach! heisst diess", "tokens": ["Dich", "den", "Gros\u00b7sen", ":", "doch", "ach", "!", "heisst", "diess"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$.", "ADV", "ADV", "$.", "VVFIN", "PDS"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Deine Tugenden? So lebt in Europens und", "tokens": ["Dei\u00b7ne", "Tu\u00b7gen\u00b7den", "?", "So", "lebt", "in", "Eu\u00b7ro\u00b7pens", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "APPR", "NE", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "In der \u00e4lteren Welt Asiens mancher F\u00fcrst,", "tokens": ["In", "der", "\u00e4l\u00b7te\u00b7ren", "Welt", "A\u00b7siens", "man\u00b7cher", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Dir an Weisheit nicht gleich. Selbst der", "tokens": ["Dir", "an", "Weis\u00b7heit", "nicht", "gleich", ".", "Selbst", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "NN", "PTKNEG", "ADV", "$.", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Macedonier, wie lebt er? bewundert, und", "tokens": ["Ma\u00b7ce\u00b7do\u00b7nier", ",", "wie", "lebt", "er", "?", "be\u00b7wun\u00b7dert", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "$.", "VVPP", "$,", "KON"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Nicht geliebt; denn er fand keinen Dir-", "tokens": ["Nicht", "ge\u00b7liebt", ";", "denn", "er", "fand", "kei\u00b7nen", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$.", "KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Herold, dessen Gesang weiter, als Phidias", "tokens": ["He\u00b7rold", ",", "des\u00b7sen", "Ge\u00b7sang", "wei\u00b7ter", ",", "als", "Phi\u00b7di\u00b7as"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELAT", "NN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.21": {"text": "Marmor, oder Apells athmende Farbe,", "tokens": ["Mar\u00b7mor", ",", "o\u00b7der", "A\u00b7pells", "ath\u00b7men\u00b7de", "Far\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Aber, siehe! wie lebt C\u00e4sar Oktavius", "tokens": ["A\u00b7ber", ",", "sie\u00b7he", "!", "wie", "lebt", "C\u00e4\u00b7sar", "Ok\u00b7ta\u00b7vius"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVIMP", "$.", "PWAV", "VVFIN", "NE", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "Durch den Edeln in Rom? (Edel im", "tokens": ["Durch", "den", "E\u00b7deln", "in", "Rom", "?", "(", "E\u00b7del", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$.", "$(", "NN", "APPRART"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Grossen G\u00f6tter, obgleich nicht auf der", "tokens": ["Gros\u00b7sen", "G\u00f6t\u00b7ter", ",", "ob\u00b7gleich", "nicht", "auf", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PTKNEG", "APPR", "ART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Censors:) ewig geliebt, ewig ein Muster der", "tokens": ["Cen\u00b7sors", ":)", "e\u00b7wig", "ge\u00b7liebt", ",", "e\u00b7wig", "ein", "Mus\u00b7ter", "der"], "token_info": ["word", "emoticon", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJD", "VVPP", "$,", "ADJD", "ART", "NN", "ART"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.26": {"text": "V\u00e4ter jegliches Volks. \u2014 Gl\u00fccklicher", "tokens": ["V\u00e4\u00b7ter", "jeg\u00b7li\u00b7ches", "Volks", ".", "Gl\u00fcck\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "ADJA", "NN", "$.", "$(", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Unverd\u00e4chtig, ein Lob, reiner als beider", "tokens": ["Un\u00b7ver\u00b7d\u00e4ch\u00b7tig", ",", "ein", "Lob", ",", "rei\u00b7ner", "als", "bei\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "ADJA", "KOKOM", "PIAT"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "In sein Saitenspiel singt! Gl\u00fccklicher", "tokens": ["In", "sein", "Sai\u00b7ten\u00b7spiel", "singt", "!", "Gl\u00fcck\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Nicht den Feldherrn allein, und den ge-", "tokens": ["Nicht", "den", "Feld\u00b7herrn", "al\u00b7lein", ",", "und", "den", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "KON", "ART", "TRUNC"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Landesf\u00fcrsten in dir; der auch den Vater des", "tokens": ["Lan\u00b7des\u00b7f\u00fcrs\u00b7ten", "in", "dir", ";", "der", "auch", "den", "Va\u00b7ter", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "ART", "ADV", "ART", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Hauses, der auch den Freund, der auch den", "tokens": ["Hau\u00b7ses", ",", "der", "auch", "den", "Freund", ",", "der", "auch", "den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Weisen, gross in der Kunst jeder Kam\u00f6ne,", "tokens": ["Wei\u00b7sen", ",", "gross", "in", "der", "Kunst", "je\u00b7der", "Ka\u00b7m\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "G\u00f6tter! w\u00e4re doch ich dieser beneidete", "tokens": ["G\u00f6t\u00b7ter", "!", "w\u00e4\u00b7re", "doch", "ich", "die\u00b7ser", "be\u00b7nei\u00b7de\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "ADV", "PPER", "PDS", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Barde! selber zu schwach, aber durch", "tokens": ["Bar\u00b7de", "!", "sel\u00b7ber", "zu", "schwach", ",", "a\u00b7ber", "durch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "PTKA", "ADJD", "$,", "ADV", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Und die Sprache gest\u00e4rkt, die wie Kalliopens", "tokens": ["Und", "die", "Spra\u00b7che", "ge\u00b7st\u00e4rkt", ",", "die", "wie", "Kal\u00b7li\u00b7o\u00b7pens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "PRELS", "KOKOM", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Tuba t\u00f6net: wie weit liess ich euch", "tokens": ["Tu\u00b7ba", "t\u00f6\u00b7net", ":", "wie", "weit", "liess", "ich", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "PPER"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "S\u00e4nger Heinrichs! und dich, ganze Zunft", "tokens": ["S\u00e4n\u00b7ger", "Hein\u00b7richs", "!", "und", "dich", ",", "gan\u00b7ze", "Zunft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$.", "KON", "PPER", "$,", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}