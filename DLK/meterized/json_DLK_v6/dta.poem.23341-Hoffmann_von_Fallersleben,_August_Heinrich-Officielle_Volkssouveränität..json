{"dta.poem.23341": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Officielle Volkssouver\u00e4nit\u00e4t.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Polizeilich ist erlaubt,", "tokens": ["Po\u00b7li\u00b7zei\u00b7lich", "ist", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles zu verschnapsen;", "tokens": ["Al\u00b7les", "zu", "ver\u00b7schnap\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keinem wehrt man \u00fcberhaupt,", "tokens": ["Kei\u00b7nem", "wehrt", "man", "\u00fc\u00b7ber\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Welt zu tapsen.", "tokens": ["Durch", "die", "Welt", "zu", "tap\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lieber hat man doch, da\u00df sie", "tokens": ["Lie\u00b7ber", "hat", "man", "doch", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PIS", "ADV", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Vieh verdummen,", "tokens": ["Wie", "das", "Vieh", "ver\u00b7dum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn es kann das liebe Vieh", "tokens": ["Denn", "es", "kann", "das", "lie\u00b7be", "Vieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6chstens etwas brummen.", "tokens": ["H\u00f6chs\u00b7tens", "et\u00b7was", "brum\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Legten Ochs und Esel sich", "tokens": ["Leg\u00b7ten", "Ochs", "und", "E\u00b7sel", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das R\u00e4sonnieren,", "tokens": ["Auf", "das", "R\u00e4\u00b7son\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lie\u00dfe man gelegentlich", "tokens": ["Lie\u00b7\u00dfe", "man", "ge\u00b7le\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sie auch arretieren.", "tokens": ["Sie", "auch", "ar\u00b7re\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}