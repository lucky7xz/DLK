{"textgrid.poem.30159": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo viel Fremde kummen hin, ist viel neues mite kummen;", "tokens": ["Wo", "viel", "Frem\u00b7de", "kum\u00b7men", "hin", ",", "ist", "viel", "neu\u00b7es", "mi\u00b7te", "kum\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PTKVZ", "$,", "VAFIN", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wo viel \u00c4rtzte kummen hin, gehn die Menschen weg mit summen;", "tokens": ["Wo", "viel", "\u00c4rtz\u00b7te", "kum\u00b7men", "hin", ",", "gehn", "die", "Men\u00b7schen", "weg", "mit", "sum\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ADV", "APPR", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo viel Advocaten sind, geht gerades nach dem krummen.", "tokens": ["Wo", "viel", "Ad\u00b7vo\u00b7ca\u00b7ten", "sind", ",", "geht", "ge\u00b7ra\u00b7des", "nach", "dem", "krum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}