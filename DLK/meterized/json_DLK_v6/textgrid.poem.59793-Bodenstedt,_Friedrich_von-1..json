{"textgrid.poem.59793": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mit Engeln im blauen Himmelszelt,", "tokens": ["Nicht", "mit", "En\u00b7geln", "im", "blau\u00b7en", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht mit Rosen auf duftigem Blumenfeld,", "tokens": ["Nicht", "mit", "Ro\u00b7sen", "auf", "duf\u00b7ti\u00b7gem", "Blu\u00b7men\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Selbst mit der ewigen Sonne Licht", "tokens": ["Selbst", "mit", "der", "e\u00b7wi\u00b7gen", "Son\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vergleich' ich Zul\u00e9ikha, mein M\u00e4dchen, nicht!", "tokens": ["Ver\u00b7gleich'", "ich", "Zul\u00e9ik\u00b7ha", ",", "mein", "M\u00e4d\u00b7chen", ",", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn der Engel Busen ist liebeleer,", "tokens": ["Denn", "der", "En\u00b7gel", "Bu\u00b7sen", "ist", "lie\u00b7be\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unter Rosen drohen die Dornen her,", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "dro\u00b7hen", "die", "Dor\u00b7nen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die Sonne verh\u00fcllt des Nachts ihr Licht:", "tokens": ["Und", "die", "Son\u00b7ne", "ver\u00b7h\u00fcllt", "des", "Nachts", "ihr", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADV", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie alle gleichen Zul\u00e9ikha nicht!", "tokens": ["Sie", "al\u00b7le", "glei\u00b7chen", "Zul\u00e9ik\u00b7ha", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nichts finden, so weit das Weltall reicht,", "tokens": ["Nichts", "fin\u00b7den", ",", "so", "weit", "das", "Wel\u00b7tall", "reicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Blicke, was meiner Zul\u00e9ikha gleicht \u2013", "tokens": ["Die", "Bli\u00b7cke", ",", "was", "mei\u00b7ner", "Zul\u00e9ik\u00b7ha", "gleicht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sch\u00f6n, dornlos, voll ewigem Liebesschein,", "tokens": ["Sch\u00f6n", ",", "dorn\u00b7los", ",", "voll", "e\u00b7wi\u00b7gem", "Lie\u00b7bes\u00b7schein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kann sie mit sich selbst nur verglichen sein!", "tokens": ["Kann", "sie", "mit", "sich", "selbst", "nur", "ver\u00b7gli\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PRF", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}