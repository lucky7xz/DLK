{"dta.poem.1087": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aber der zeit flucht so schnell", "tokens": ["A\u00b7ber", "der", "zeit", "flucht", "so", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Solt mir ehe dan die red fehlen", "tokens": ["Solt", "mir", "e\u00b7he", "dan", "die", "red", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KOUS", "ADV", "ART", "VVFIN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So ist auch dein Nam so hell", "tokens": ["So", "ist", "auch", "dein", "Nam", "so", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das jhn gar nichts kan verh\u00e4len:", "tokens": ["Das", "jhn", "gar", "nichts", "kan", "ver\u00b7h\u00e4\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vnd der worten \u00fcberflu\u00df", "tokens": ["Vnd", "der", "wor\u00b7ten", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist gemeinglich voll verdru\u00df.", "tokens": ["Ist", "ge\u00b7mein\u00b7glich", "voll", "ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darumb will ich nu beschliessen/", "tokens": ["Da\u00b7rumb", "will", "ich", "nu", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Damit nicht was ich gern wolt/", "tokens": ["Da\u00b7mit", "nicht", "was", "ich", "gern", "wolt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "PWS", "PPER", "ADV", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das dir wolgefallen solt/", "tokens": ["Das", "dir", "wol\u00b7ge\u00b7fal\u00b7len", "solt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dich villeicht m\u00f6chte verdriessen.", "tokens": ["Dich", "vil\u00b7leicht", "m\u00f6ch\u00b7te", "ver\u00b7dries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}