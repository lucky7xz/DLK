{"dta.poem.14479": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und \u00fcber mir Lerchenlieder", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und unter mir Blumen bunt,", "tokens": ["Und", "un\u00b7ter", "mir", "Blu\u00b7men", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So werf' ich im Grase mich nieder", "tokens": ["So", "wer\u00b7f'", "ich", "im", "Gra\u00b7se", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und weine aus Herzensgrund.", "tokens": ["Und", "wei\u00b7ne", "aus", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}