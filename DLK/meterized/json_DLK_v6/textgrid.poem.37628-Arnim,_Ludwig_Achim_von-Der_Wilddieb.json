{"textgrid.poem.37628": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wilddieb", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter hat schon lang' geschaut", "tokens": ["Die", "Mut\u00b7ter", "hat", "schon", "lang'", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihrem Giebelfenster,", "tokens": ["Von", "ih\u00b7rem", "Gie\u00b7bel\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als kaum der Morgen hat gegraut,", "tokens": ["Als", "kaum", "der", "Mor\u00b7gen", "hat", "ge\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es weckten sie Gespenster:", "tokens": ["Es", "weck\u00b7ten", "sie", "Ge\u00b7spens\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mann, der Sohn, sie blieben aus,", "tokens": ["Der", "Mann", ",", "der", "Sohn", ",", "sie", "blie\u00b7ben", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wollten Abends schon nach Haus.", "tokens": ["Sie", "woll\u00b7ten", "A\u00b7bends", "schon", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da naht der Sohn, sie lacht ihn an,", "tokens": ["Da", "naht", "der", "Sohn", ",", "sie", "lacht", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er keucht mit schwerem Ranzen;", "tokens": ["Er", "keucht", "mit", "schwe\u00b7rem", "Ran\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie r\u00e4th, was ihm so lasten kann,", "tokens": ["Sie", "r\u00e4\u00b7th", ",", "was", "ihm", "so", "las\u00b7ten", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Was nach der Pfeif' mu\u00df tanzen:", "tokens": ["Was", "nach", "der", "Pfeif'", "mu\u00df", "tan\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ob Hirsch, ob Reh im Tanze fiel?", "tokens": ["Ob", "Hirsch", ",", "ob", "Reh", "im", "Tan\u00b7ze", "fiel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie holet Wein zum Freudenspiel! \u2013", "tokens": ["Sie", "ho\u00b7let", "Wein", "zum", "Freu\u00b7den\u00b7spiel", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Sohn schleicht scheu und denkt der Noth,", "tokens": ["Der", "Sohn", "schleicht", "scheu", "und", "denkt", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachts von ihm bestanden,", "tokens": ["Die", "Nachts", "von", "ihm", "be\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie viele J\u00e4ger ihn bedroht,", "tokens": ["Wie", "vie\u00b7le", "J\u00e4\u00b7ger", "ihn", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dunkel ihn nicht fanden;", "tokens": ["Im", "Dun\u00b7kel", "ihn", "nicht", "fan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Vater nur, der konnt nicht mit,", "tokens": ["Der", "Va\u00b7ter", "nur", ",", "der", "konnt", "nicht", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "VMFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der rief zu ihm die letzte Bitt'.", "tokens": ["Der", "rief", "zu", "ihm", "die", "letz\u00b7te", "Bitt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Vater scheut die lange Haft,", "tokens": ["Der", "Va\u00b7ter", "scheut", "die", "lan\u00b7ge", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt er in J\u00e4ger-H\u00e4nde,", "tokens": ["F\u00e4llt", "er", "in", "J\u00e4\u00b7ger\u00b7H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erloschen war der F\u00fc\u00dfe Kraft,", "tokens": ["Er\u00b7lo\u00b7schen", "war", "der", "F\u00fc\u00b7\u00dfe", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Augen Feuer-Br\u00e4nde;", "tokens": ["Der", "Au\u00b7gen", "Feu\u00b7er\u00b7Br\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom Sohn erfleht er schnellen Tod,", "tokens": ["Vom", "Sohn", "er\u00b7fleht", "er", "schnel\u00b7len", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wartet bis zum Morgenroth.", "tokens": ["Der", "war\u00b7tet", "bis", "zum", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Sohn kann fliehen, doch er harrt,", "tokens": ["Der", "Sohn", "kann", "flie\u00b7hen", ",", "doch", "er", "harrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich der Vater st\u00e4rke,", "tokens": ["Da\u00df", "sich", "der", "Va\u00b7ter", "st\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Fu\u00df scharrt leis, sein Auge starrt,", "tokens": ["Sein", "Fu\u00df", "scharrt", "leis", ",", "sein", "Au\u00b7ge", "starrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es der Vater merke:", "tokens": ["Da\u00df", "es", "der", "Va\u00b7ter", "mer\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein J\u00e4ger weicht von seinem Ort,", "tokens": ["Kein", "J\u00e4\u00b7ger", "weicht", "von", "sei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst tr\u00fcge er den Vater fort.", "tokens": ["Sonst", "tr\u00fc\u00b7ge", "er", "den", "Va\u00b7ter", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Fuchs, wenn ihn das Eisen f\u00e4ngt,", "tokens": ["Der", "Fuchs", ",", "wenn", "ihn", "das", "Ei\u00b7sen", "f\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei\u00dft ab die eignen Glieder;", "tokens": ["Bei\u00dft", "ab", "die", "eig\u00b7nen", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die gleiche Noth ihn jetzt umdr\u00e4ngt", "tokens": ["Die", "glei\u00b7che", "Noth", "ihn", "jetzt", "um\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Gesetz der Br\u00fcder:", "tokens": ["Und", "das", "Ge\u00b7setz", "der", "Br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwer lebend f\u00e4llt in J\u00e4gers Hand,", "tokens": ["\u00bb", "wer", "le\u00b7bend", "f\u00e4llt", "in", "J\u00e4\u00b7gers", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJD", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Den t\u00f6dte, wer ihm noch verwandt.\u00ab", "tokens": ["Den", "t\u00f6d\u00b7te", ",", "wer", "ihm", "noch", "ver\u00b7wandt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PWS", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Kopf wird hei\u00df, kein Thau ihm sinkt,", "tokens": ["Sein", "Kopf", "wird", "hei\u00df", ",", "kein", "Thau", "ihm", "sinkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht ist so verflossen,", "tokens": ["Die", "Nacht", "ist", "so", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Vater kniet, als Morgen blinkt,", "tokens": ["Der", "Va\u00b7ter", "kniet", ",", "als", "Mor\u00b7gen", "blinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sohn hat abgeschossen,", "tokens": ["Der", "Sohn", "hat", "ab\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie der Vater niederf\u00e4llt,", "tokens": ["Und", "wie", "der", "Va\u00b7ter", "nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die J\u00e4ger fliehn, die ihn umstellt.", "tokens": ["Die", "J\u00e4\u00b7ger", "fliehn", ",", "die", "ihn", "um\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie meinen all ein J\u00e4ger, that's", "tokens": ["Sie", "mei\u00b7nen", "all", "ein", "J\u00e4\u00b7ger", ",", "that's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scheun des Sohnes Rache,", "tokens": ["Und", "scheun", "des", "Soh\u00b7nes", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Zeichen sind sie eines Raths,", "tokens": ["Durch", "Zei\u00b7chen", "sind", "sie", "ei\u00b7nes", "Raths", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fliehn, als ob ein Drache", "tokens": ["Sie", "fliehn", ",", "als", "ob", "ein", "Dra\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An ihre Fersen sei gebannt,", "tokens": ["An", "ih\u00b7re", "Fer\u00b7sen", "sei", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sind die J\u00e4ger fortgerannt.", "tokens": ["So", "sind", "die", "J\u00e4\u00b7ger", "fort\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Des Vater Ehr' bedenkt der Sohn,", "tokens": ["Des", "Va\u00b7ter", "Ehr'", "be\u00b7denkt", "der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihn nicht fressen Raben,", "tokens": ["Da\u00df", "ihn", "nicht", "fres\u00b7sen", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihn die Fremden nicht mit Hohn", "tokens": ["Da\u00df", "ihn", "die", "Frem\u00b7den", "nicht", "mit", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Kirchhofseck begraben:", "tokens": ["In", "Kirch\u00b7hof\u00b7seck", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sackt ihn ein und hebt ihn auf", "tokens": ["Er", "sackt", "ihn", "ein", "und", "hebt", "ihn", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und eilt nach Haus im schnellen Lauf.", "tokens": ["Und", "eilt", "nach", "Haus", "im", "schnel\u00b7len", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So tritt er zu der Th\u00fcre ein,", "tokens": ["So", "tritt", "er", "zu", "der", "Th\u00fc\u00b7re", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+