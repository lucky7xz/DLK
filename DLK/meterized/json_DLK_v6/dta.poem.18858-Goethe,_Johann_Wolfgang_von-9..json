{"dta.poem.18858": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Mit festem Sinn", "tokens": ["Mit", "fes\u00b7tem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Verfolgt\u2019 er sein Ziel", "tokens": ["Ver\u00b7folgt'", "er", "sein", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bis er ruhte;", "tokens": ["Bis", "er", "ruh\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da ruht auch der feste Sinn.", "tokens": ["Da", "ruht", "auch", "der", "fes\u00b7te", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}