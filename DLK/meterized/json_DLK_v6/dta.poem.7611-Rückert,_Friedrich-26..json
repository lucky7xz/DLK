{"dta.poem.7611": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Sch\u00f6pfungsbrunn, der Sch\u00f6pfer sa\u00df daran,", "tokens": ["Ich", "sah", "den", "Sch\u00f6p\u00b7fungs\u00b7brunn", ",", "der", "Sch\u00f6p\u00b7fer", "sa\u00df", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sch\u00f6pfte, da\u00df die Flut vom goldnen Eimer rann.", "tokens": ["Und", "sch\u00f6pf\u00b7te", ",", "da\u00df", "die", "Flut", "vom", "gold\u00b7nen", "Ei\u00b7mer", "rann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er sch\u00f6pft' und go\u00df den Thau rings in die W\u00fcste aus,", "tokens": ["Er", "sch\u00f6pft'", "und", "go\u00df", "den", "Thau", "rings", "in", "die", "W\u00fcs\u00b7te", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ward zur Lebensau mit Fr\u00fchlingsblumenstraus.", "tokens": ["Die", "ward", "zur", "Le\u00b7ben\u00b7sau", "mit", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men\u00b7straus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die B\u00e4ch' und B\u00e4chelchen, die Quell' und Quellchen rannen,", "tokens": ["Die", "B\u00e4ch'", "und", "B\u00e4\u00b7chel\u00b7chen", ",", "die", "Quell'", "und", "Quell\u00b7chen", "ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Gras und Kraut hinan, und schneller noch von dannen.", "tokens": ["Zu", "Gras", "und", "Kraut", "hi\u00b7nan", ",", "und", "schnel\u00b7ler", "noch", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,", "KON", "ADJD", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo eine Welle kam, bl\u00fcht' eine Fr\u00fchlingsbraut;", "tokens": ["Wo", "ei\u00b7ne", "Wel\u00b7le", "kam", ",", "bl\u00fcht'", "ei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7braut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo eine Abschied nahm, da war verwelkt ein Kraut.", "tokens": ["Wo", "ei\u00b7ne", "Ab\u00b7schied", "nahm", ",", "da", "war", "ver\u00b7welkt", "ein", "Kraut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wo in Asche war ein Pflanzenleib zerfallen,", "tokens": ["Und", "wo", "in", "A\u00b7sche", "war", "ein", "Pflan\u00b7zen\u00b7leib", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schnell ward er neu gebaut von rinnenden Kristallen.", "tokens": ["Schnell", "ward", "er", "neu", "ge\u00b7baut", "von", "rin\u00b7nen\u00b7den", "Kris\u00b7tal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Sch\u00f6pfer sch\u00f6pfte fort, der Brunnen ward nicht leer,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "sch\u00f6pf\u00b7te", "fort", ",", "der", "Brun\u00b7nen", "ward", "nicht", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wiewol ihm fort und fort entsch\u00f6pft ward Meer um Meer.", "tokens": ["Wie\u00b7wol", "ihm", "fort", "und", "fort", "ent\u00b7sch\u00f6pft", "ward", "Meer", "um", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "PTKVZ", "VVPP", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn was von oben go\u00df der goldne Eimer nieder,", "tokens": ["Denn", "was", "von", "o\u00b7ben", "go\u00df", "der", "gold\u00b7ne", "Ei\u00b7mer", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das alles unten flo\u00df zur\u00fcck zum Brunnen wieder.", "tokens": ["Das", "al\u00b7les", "un\u00b7ten", "flo\u00df", "zu\u00b7r\u00fcck", "zum", "Brun\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVFIN", "PTKVZ", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}