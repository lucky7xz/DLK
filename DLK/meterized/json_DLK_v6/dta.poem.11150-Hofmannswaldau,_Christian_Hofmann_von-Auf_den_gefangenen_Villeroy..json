{"dta.poem.11150": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den gefangenen Villeroy.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der adler nimmt den hahn aus dem gemachten neste,", "tokens": ["Der", "ad\u00b7ler", "nimmt", "den", "hahn", "aus", "dem", "ge\u00b7mach\u00b7ten", "nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein loch ist ihm zu klein, kein thor ist ihm zu feste.", "tokens": ["Kein", "loch", "ist", "ihm", "zu", "klein", ",", "kein", "thor", "ist", "ihm", "zu", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PIAT", "NN", "VAFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wer gar zu zeitig will auf fremdem miste kr\u00e4hn,", "tokens": ["Wer", "gar", "zu", "zei\u00b7tig", "will", "auf", "frem\u00b7dem", "mis\u00b7te", "kr\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKA", "ADJD", "VMFIN", "APPR", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan, eh man es versieht, sich in dem korbe sehn.", "tokens": ["Kan", ",", "eh", "man", "es", "ver\u00b7sieht", ",", "sich", "in", "dem", "kor\u00b7be", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "PRF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}