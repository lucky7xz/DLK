{"textgrid.poem.66326": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Yorik", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Yorik starb, da flog", "tokens": ["Als", "Y\u00b7o\u00b7rik", "starb", ",", "da", "flog"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NE", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sein Seelchen auf gen Himmel,", "tokens": ["Sein", "Seel\u00b7chen", "auf", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So leicht, wie ein Seufzerchen", "tokens": ["So", "leicht", ",", "wie", "ein", "Seuf\u00b7zer\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Der Liebe himmelan fliegt.", "tokens": ["Der", "Lie\u00b7be", "him\u00b7me\u00b7lan", "fliegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es standen schon in goldnen Kreisen", "tokens": ["Es", "stan\u00b7den", "schon", "in", "gold\u00b7nen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Engelein um ihn herum.", "tokens": ["Die", "En\u00b7ge\u00b7lein", "um", "ihn", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf ihren Wangen gl\u00e4nzte", "tokens": ["Auf", "ih\u00b7ren", "Wan\u00b7gen", "gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des himmlischen L\u00e4chelns Morgenroth.", "tokens": ["Des", "himm\u00b7li\u00b7schen", "L\u00e4\u00b7chelns", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u00bbkomm doch, du sanftes Seelchen du,", "tokens": ["\u00bb", "komm", "doch", ",", "du", "sanf\u00b7tes", "Seel\u00b7chen", "du", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erz\u00e4hl' uns! bringst du gute Botschaft?\u00ab", "tokens": ["Er\u00b7z\u00e4hl'", "uns", "!", "bringst", "du", "gu\u00b7te", "Bot\u00b7schaft", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Yoriks Seele hauchte: \u00bbAch,", "tokens": ["Und", "Y\u00b7o\u00b7riks", "See\u00b7le", "hauch\u00b7te", ":", "\u00bb", "Ach", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "$.", "$(", "ITJ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dort unten lie\u00df ich meine Br\u00fcder", "tokens": ["Dort", "un\u00b7ten", "lie\u00df", "ich", "mei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Im Staub zur\u00fcck. O s\u00e4ht ihr sie", "tokens": ["Im", "Staub", "zu\u00b7r\u00fcck", ".", "O", "s\u00e4ht", "ihr", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "NE", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Erziehungslos und ohne F\u00fchrer", "tokens": ["Er\u00b7zie\u00b7hungs\u00b7los", "und", "oh\u00b7ne", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "In B\u00fcschen wandeln, wo die Hyder,", "tokens": ["In", "B\u00fc\u00b7schen", "wan\u00b7deln", ",", "wo", "die", "Hy\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die S\u00fcnde zischt. Wie sie sich m\u00fchen,", "tokens": ["Die", "S\u00fcn\u00b7de", "zischt", ".", "Wie", "sie", "sich", "m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Schlange zu zerrei\u00dfen,", "tokens": ["Die", "Schlan\u00b7ge", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die sie umflicht! Es f\u00e4llt", "tokens": ["Die", "sie", "um\u00b7flicht", "!", "Es", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der Jugend Blume, ach! vom Sturm gebrochen!", "tokens": ["Der", "Ju\u00b7gend", "Blu\u00b7me", ",", "ach", "!", "vom", "Sturm", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ITJ", "$.", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Es schl\u00e4gt der graue S\u00fcnder", "tokens": ["Es", "schl\u00e4gt", "der", "grau\u00b7e", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Verzweiflungsvoll an seinen Sch\u00e4del, da\u00df es hallt!", "tokens": ["Ver\u00b7zwei\u00b7flungs\u00b7voll", "an", "sei\u00b7nen", "Sch\u00e4\u00b7del", ",", "da\u00df", "es", "hallt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Sie r\u00f6cheln, sehn mit wildverr\u00fcckten Mienen", "tokens": ["Sie", "r\u00f6\u00b7cheln", ",", "sehn", "mit", "wild\u00b7ver\u00b7r\u00fcck\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Gen Himmel. Ach, dann strecken ihre Glieder", "tokens": ["Gen", "Him\u00b7mel", ".", "Ach", ",", "dann", "stre\u00b7cken", "ih\u00b7re", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ITJ", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sich f\u00fcrchterlich dem Tode aus,", "tokens": ["Sich", "f\u00fcrch\u00b7ter\u00b7lich", "dem", "To\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Decke ihres Sarges breitet", "tokens": ["Die", "De\u00b7cke", "ih\u00b7res", "Sar\u00b7ges", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Graunvolle Nacht und Stille \u00fcber'n Leichnam.", "tokens": ["Graun\u00b7vol\u00b7le", "Nacht", "und", "Stil\u00b7le", "\u00fc\u00b7ber'n", "Leich\u00b7nam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.28": {"text": "Es poltert hinab der Sarg;", "tokens": ["Es", "pol\u00b7tert", "hin\u00b7ab", "der", "Sarg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Die Stricke schnellen unter ihm hinweg,", "tokens": ["Die", "Stri\u00b7cke", "schnel\u00b7len", "un\u00b7ter", "ihm", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und Sch\u00e4del und Gebein und Staub", "tokens": ["Und", "Sch\u00e4\u00b7del", "und", "Ge\u00b7bein", "und", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "F\u00e4llt von der Schaufel dumpf hinab.\u00ab", "tokens": ["F\u00e4llt", "von", "der", "Schau\u00b7fel", "dumpf", "hin\u00b7ab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Der Freundschaft und der Liebe Thr\u00e4nen", "tokens": ["Der", "Freund\u00b7schaft", "und", "der", "Lie\u00b7be", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Rieseln in den Sand; hinabgeb\u00fcckt", "tokens": ["Rie\u00b7seln", "in", "den", "Sand", ";", "hin\u00b7ab\u00b7ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "Erschallt der Heulenden entsetzenvolle Klage:", "tokens": ["Er\u00b7schallt", "der", "Heu\u00b7len\u00b7den", "ent\u00b7set\u00b7zen\u00b7vol\u00b7le", "Kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u00bbo Vater, Mutter, Bruder,", "tokens": ["\u00bb", "o", "Va\u00b7ter", ",", "Mut\u00b7ter", ",", "Bru\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "O Freund, Geliebte, gute Nacht!\u00ab", "tokens": ["O", "Freund", ",", "Ge\u00b7lieb\u00b7te", ",", "gu\u00b7te", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Sie aber liegen stumm", "tokens": ["Sie", "a\u00b7ber", "lie\u00b7gen", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Und f\u00fchllos, wenn der Wurm", "tokens": ["Und", "f\u00fchl\u00b7los", ",", "wenn", "der", "Wurm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Zu ihrer Leiche kriecht.", "tokens": ["Zu", "ih\u00b7rer", "Lei\u00b7che", "kriecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Und Yorik schwieg. \u2013 Es weinten", "tokens": ["Und", "Y\u00b7o\u00b7rik", "schwieg", ".", "\u2013", "Es", "wein\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Die Engelein; und ihre Thr\u00e4nen fielen", "tokens": ["Die", "En\u00b7ge\u00b7lein", ";", "und", "ih\u00b7re", "Thr\u00e4\u00b7nen", "fie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Im Morgenduft herab und zitterten", "tokens": ["Im", "Mor\u00b7gen\u00b7duft", "her\u00b7ab", "und", "zit\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.43": {"text": "Wie Thau auf junge Palmen", "tokens": ["Wie", "Thau", "auf", "jun\u00b7ge", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Und halb offne Veilchen,", "tokens": ["Und", "halb", "off\u00b7ne", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Die dem Scho\u00dfe", "tokens": ["Die", "dem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.46": {"text": "Des werdenden Fr\u00fchlings entstiegen.", "tokens": ["Des", "wer\u00b7den\u00b7den", "Fr\u00fch\u00b7lings", "ent\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}