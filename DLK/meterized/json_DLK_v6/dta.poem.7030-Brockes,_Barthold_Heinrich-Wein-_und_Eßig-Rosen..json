{"dta.poem.7030": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wein- und E\u00dfig-Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan doch mehr das Aug\u2019 und auch das Hertze r\u00fchren,", "tokens": ["Was", "kan", "doch", "mehr", "das", "Aug'", "und", "auch", "das", "Hert\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "KON", "ADV", "PDS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn auf einem Blat zugleich", "tokens": ["Als", "wenn", "auf", "ei\u00b7nem", "Blat", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir die verbundne Zier von wei\u00df und rothen sp\u00fcren?", "tokens": ["Wir", "die", "ver\u00b7bund\u00b7ne", "Zier", "von", "wei\u00df", "und", "ro\u00b7then", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "VVFIN", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es scheinet die Natur, die an Erfindung reich,", "tokens": ["Es", "schei\u00b7net", "die", "Na\u00b7tur", ",", "die", "an", "Er\u00b7fin\u00b7dung", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch nicht damit zu frieden,", "tokens": ["Noch", "nicht", "da\u00b7mit", "zu", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df sie die Farben unterschieden,", "tokens": ["Da\u00df", "sie", "die", "Far\u00b7ben", "un\u00b7ter\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn sie auf dieser wei\u00df, auf jener r\u00f6htlich gl\u00e4ntzt.", "tokens": ["Wenn", "sie", "auf", "die\u00b7ser", "wei\u00df", ",", "auf", "je\u00b7ner", "r\u00f6ht\u00b7lich", "gl\u00e4ntzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "VVFIN", "$,", "APPR", "PDAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf einer andern Art ist sie gedoppelt sch\u00f6n;", "tokens": ["Auf", "ei\u00b7ner", "an\u00b7dern", "Art", "ist", "sie", "ge\u00b7dop\u00b7pelt", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und l\u00e4sst zugleich uns weiss\u2019 und rothe Striche sehn,", "tokens": ["Und", "l\u00e4sst", "zu\u00b7gleich", "uns", "weiss'", "und", "ro\u00b7the", "Stri\u00b7che", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PTKVZ", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zwo Farben siehet man recht angenehm vermengt;", "tokens": ["Zwo", "Far\u00b7ben", "sie\u00b7het", "man", "recht", "an\u00b7ge\u00b7nehm", "ver\u00b7mengt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier sieht man sie gestricht, mit P\u00fcnetgen, dort besprengt.", "tokens": ["Hier", "sieht", "man", "sie", "ge\u00b7stricht", ",", "mit", "P\u00fc\u00b7net\u00b7gen", ",", "dort", "be\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "VVPP", "$,", "APPR", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Auf Tulpen und auf Nelcken Art,", "tokens": ["Auf", "Tul\u00b7pen", "und", "auf", "Nel\u00b7cken", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sind Farb und Formen drauf gepaart;", "tokens": ["Sind", "Farb", "und", "For\u00b7men", "drauf", "ge\u00b7paart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sie scheinen recht mit Flei\u00df gezeichnet und gemahlet", "tokens": ["Sie", "schei\u00b7nen", "recht", "mit", "Flei\u00df", "ge\u00b7zeich\u00b7net", "und", "ge\u00b7mah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vom Pinsel der Natur. In ihrem Schoosse strahlet", "tokens": ["Vom", "Pin\u00b7sel", "der", "Na\u00b7tur", ".", "In", "ih\u00b7rem", "Schoos\u00b7se", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein gelbes Heer von gleichsam g\u00fcldnen Spitzen,", "tokens": ["Ein", "gel\u00b7bes", "Heer", "von", "gleich\u00b7sam", "g\u00fcld\u00b7nen", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die \u00fcm den Mittelpunet in s\u00fcsser R\u00fcnde sitzen.", "tokens": ["Die", "\u00fcm", "den", "Mit\u00b7tel\u00b7pu\u00b7net", "in", "s\u00fcs\u00b7ser", "R\u00fcn\u00b7de", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Noch sieht man eine Art, die hohen Incarnat,", "tokens": ["Noch", "sieht", "man", "ei\u00b7ne", "Art", ",", "die", "ho\u00b7hen", "In\u00b7car\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zum Schmuck der sanfften Bl\u00e4tter hat.", "tokens": ["Zum", "Schmuck", "der", "sanff\u00b7ten", "Bl\u00e4t\u00b7ter", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Noch nicht genug, man sieht noch andre Rosen bl\u00fchen,", "tokens": ["Noch", "nicht", "ge\u00b7nug", ",", "man", "sieht", "noch", "and\u00b7re", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "PIS", "VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die einer Purpur Farb\u2019, an dunckel rothem Schein,", "tokens": ["Die", "ei\u00b7ner", "Pur\u00b7pur", "Fa\u00b7rb'", ",", "an", "dun\u00b7ckel", "ro\u00b7them", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Vollkommen \u00e4hnlich seyn.", "tokens": ["Voll\u00b7kom\u00b7men", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Sie scheinen recht zu brennen und zu gl\u00fchen:", "tokens": ["Sie", "schei\u00b7nen", "recht", "zu", "bren\u00b7nen", "und", "zu", "gl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Kein dunckel rother Sammt, kein Purpur-Schnecken-Blut", "tokens": ["Kein", "dun\u00b7ckel", "ro\u00b7ther", "Sammt", ",", "kein", "Pur\u00b7pur\u00b7Schne\u00b7cken\u00b7Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ist v\u00f6ller, kr\u00e4fftiger, gl\u00e4ntzt pr\u00e4chtiger und flammt", "tokens": ["Ist", "v\u00f6l\u00b7ler", ",", "kr\u00e4ff\u00b7ti\u00b7ger", ",", "gl\u00e4ntzt", "pr\u00e4ch\u00b7ti\u00b7ger", "und", "flammt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "$,", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In so gef\u00fcllter Zier. Die holde Dunckelheit", "tokens": ["In", "so", "ge\u00b7f\u00fcll\u00b7ter", "Zier", ".", "Die", "hol\u00b7de", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Um desto besser zu formiren;", "tokens": ["Um", "des\u00b7to", "bes\u00b7ser", "zu", "for\u00b7mi\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Besch\u00e4fftiget sie sich die R\u00f6the zu schattiren,", "tokens": ["Be\u00b7sch\u00e4ff\u00b7ti\u00b7get", "sie", "sich", "die", "R\u00f6\u00b7the", "zu", "schat\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Mit einer blauen Farb\u2019, in der was sanfftes ruht.", "tokens": ["Mit", "ei\u00b7ner", "blau\u00b7en", "Fa\u00b7rb'", ",", "in", "der", "was", "sanff\u00b7tes", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Ich sah\u2019 den Unterscheid der Farben und Figur,", "tokens": ["Ich", "sah'", "den", "Un\u00b7ter\u00b7scheid", "der", "Far\u00b7ben", "und", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die man nur blos allein an Rosen sieht,", "tokens": ["Die", "man", "nur", "blos", "al\u00b7lein", "an", "Ro\u00b7sen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mit einem froh- und danckbarem Gem\u00fcht,", "tokens": ["Mit", "ei\u00b7nem", "froh", "und", "dan\u00b7ck\u00b7ba\u00b7rem", "Ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "Dem Sch\u00f6pfer der Natur in dieser Bluhme,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "in", "die\u00b7ser", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Zum Preis und Ruhme,", "tokens": ["Zum", "Preis", "und", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "In einer frohen Andacht an.", "tokens": ["In", "ei\u00b7ner", "fro\u00b7hen", "An\u00b7dacht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Noch hab ich dir die wunderbare Krafft,", "tokens": ["Noch", "hab", "ich", "dir", "die", "wun\u00b7der\u00b7ba\u00b7re", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Die in den Rosen-Bl\u00e4ttern steckt,", "tokens": ["Die", "in", "den", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7tern", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Bisher mein Leser, nicht entdeckt.", "tokens": ["Bis\u00b7her", "mein", "Le\u00b7ser", ",", "nicht", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ist dieser Bluhme Eigenschafft,", "tokens": ["Ist", "die\u00b7ser", "Bluh\u00b7me", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Nicht Danckens, nicht Bewunderns wehrt,", "tokens": ["Nicht", "Dan\u00b7ckens", ",", "nicht", "Be\u00b7wun\u00b7derns", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Da selbst, wenn sie die Zeit entbl\u00e4ttert,", "tokens": ["Da", "selbst", ",", "wenn", "sie", "die", "Zeit", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Aus ihren Bl\u00e4ttern noch verschiedne Sachen,", "tokens": ["Aus", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "noch", "ver\u00b7schied\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Zum Labsal des Geruchs, ja des Geschmacks zu machen?", "tokens": ["Zum", "Lab\u00b7sal", "des", "Ge\u00b7ruchs", ",", "ja", "des", "Ge\u00b7schmacks", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wie angenehm schmeckt die Conserve nicht,", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "schmeckt", "die", "Con\u00b7ser\u00b7ve", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Die, wenn man sie mit Zucker mischt,", "tokens": ["Die", ",", "wenn", "man", "sie", "mit", "Zu\u00b7cker", "mischt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Aus Rosen-Bl\u00e4ttern zugericht\u2019t,", "tokens": ["Aus", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7tern", "zu\u00b7ge\u00b7richt't", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Die den Geschmack sowol, als unsre Brust erfrischt?", "tokens": ["Die", "den", "Ge\u00b7schmack", "so\u00b7wol", ",", "als", "uns\u00b7re", "Brust", "er\u00b7frischt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wie wird so unsre Zung\u2019 als unser Hirn ger\u00fchret,", "tokens": ["Wie", "wird", "so", "uns\u00b7re", "Zung'", "als", "un\u00b7ser", "Hirn", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wenn man das Balsam-reiche Na\u00df,", "tokens": ["Wenn", "man", "das", "Bal\u00b7sam\u00b7rei\u00b7che", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Aus Rosen-Bl\u00e4ttern distilliret,", "tokens": ["Aus", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7tern", "dis\u00b7til\u00b7li\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.51": {"text": "Zum lieblichen Gew\u00fcrtz auf unsern Essen?", "tokens": ["Zum", "lieb\u00b7li\u00b7chen", "Ge\u00b7w\u00fcrtz", "auf", "un\u00b7sern", "Es\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Nechst diesen m\u00fcssen wir,", "tokens": ["Nechst", "die\u00b7sen", "m\u00fcs\u00b7sen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VMFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Zusammt des Rosen E\u00dfigs, hier", "tokens": ["Zu\u00b7sammt", "des", "Ro\u00b7sen", "E\u00b7\u00dfigs", ",", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Des Rosen Honigs nicht vergessen.", "tokens": ["Des", "Ro\u00b7sen", "Ho\u00b7nigs", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}