{"textgrid.poem.41838": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Feiertag im holden Maienmond", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Feiertag im holden Maienmond", "tokens": ["Ein", "Fei\u00b7er\u00b7tag", "im", "hol\u00b7den", "Mai\u00b7en\u00b7mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird eingel\u00e4utet von den Kirchenglocken,", "tokens": ["Wird", "ein\u00b7ge\u00b7l\u00e4u\u00b7tet", "von", "den", "Kir\u00b7chen\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Blick zu dem, der hoch im Himmel thront,", "tokens": ["Den", "Blick", "zu", "dem", ",", "der", "hoch", "im", "Him\u00b7mel", "thront", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Andachtsschauern fromm empor zu locken.", "tokens": ["In", "An\u00b7dachts\u00b7schau\u00b7ern", "fromm", "em\u00b7por", "zu", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Erde tr\u00e4gt ihr sch\u00f6nes Festgewand,", "tokens": ["Die", "Er\u00b7de", "tr\u00e4gt", "ihr", "sch\u00f6\u00b7nes", "Fest\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "All \u00fcberall ein Bl\u00fchen und ein D\u00fcften!", "tokens": ["All", "\u00fc\u00b7be\u00b7rall", "ein", "Bl\u00fc\u00b7hen", "und", "ein", "D\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit Ostern st\u00fcrzte finstern Grabesrand", "tokens": ["Seit", "Os\u00b7tern", "st\u00fcrz\u00b7te", "fins\u00b7tern", "Gra\u00b7bes\u00b7rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erstand ein neues Leben aus den Gr\u00fcften.", "tokens": ["Er\u00b7stand", "ein", "neu\u00b7es", "Le\u00b7ben", "aus", "den", "Gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und neue Wunder \u00fcberall geschehn", "tokens": ["Und", "neu\u00b7e", "Wun\u00b7der", "\u00fc\u00b7be\u00b7rall", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Keime wachsen und wo V\u00f6glein singen \u2013", "tokens": ["Wo", "Kei\u00b7me", "wach\u00b7sen", "und", "wo", "V\u00f6\u00b7glein", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "KON", "PWAV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin wir h\u00f6ren und wohin wir sehn", "tokens": ["Wo\u00b7hin", "wir", "h\u00f6\u00b7ren", "und", "wo\u00b7hin", "wir", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "KON", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will Alles aufw\u00e4rts zu dem Himmel dringen.", "tokens": ["Will", "Al\u00b7les", "auf\u00b7w\u00e4rts", "zu", "dem", "Him\u00b7mel", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So winkt auch die Natur zur Himmelfahrt;", "tokens": ["So", "winkt", "auch", "die", "Na\u00b7tur", "zur", "Him\u00b7mel\u00b7fahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im blauen Aether wei\u00dfe Wolken schwimmen,", "tokens": ["Im", "blau\u00b7en", "A\u00b7e\u00b7ther", "wei\u00b7\u00dfe", "Wol\u00b7ken", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Aug, fast glanzgeblendet, doch gewahrt", "tokens": ["Das", "Aug", ",", "fast", "glanz\u00b7ge\u00b7blen\u00b7det", ",", "doch", "ge\u00b7wahrt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Gold und Purpur in einander glimmen.", "tokens": ["Wie", "Gold", "und", "Pur\u00b7pur", "in", "ein\u00b7an\u00b7der", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie Erd und Himmel durch den Wolkenflor", "tokens": ["Wie", "Erd", "und", "Him\u00b7mel", "durch", "den", "Wol\u00b7ken\u00b7flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Horizonte sanft zusammenflie\u00dfen,", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "sanft", "zu\u00b7sam\u00b7men\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Lerchen zwitschern zu dem Kirchenchor", "tokens": ["Wie", "Ler\u00b7chen", "zwit\u00b7schern", "zu", "dem", "Kir\u00b7chen\u00b7chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Alles l\u00e4d zu seligem Genie\u00dfen. \u2013", "tokens": ["Und", "Al\u00b7les", "l\u00e4d", "zu", "se\u00b7li\u00b7gem", "Ge\u00b7nie\u00b7\u00dfen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So feiern wir den wunderreichen Tag,", "tokens": ["So", "fei\u00b7ern", "wir", "den", "wun\u00b7der\u00b7rei\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der nach der Osterweihe uns gegeben", "tokens": ["Der", "nach", "der", "Os\u00b7ter\u00b7wei\u00b7he", "uns", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis ihm das hohe Pfingstfest folgen mag,", "tokens": ["Bis", "ihm", "das", "ho\u00b7he", "Pfingst\u00b7fest", "fol\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Himmel uns're Blicke zu erheben.", "tokens": ["Zum", "Him\u00b7mel", "un\u00b7s'\u00b7re", "Bli\u00b7cke", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "So feiern wir die wunderreiche Zeit", "tokens": ["So", "fei\u00b7ern", "wir", "die", "wun\u00b7der\u00b7rei\u00b7che", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In frohem Aufblick und im sel'gen Ahnen:", "tokens": ["In", "fro\u00b7hem", "Auf\u00b7blick", "und", "im", "sel'\u00b7gen", "Ah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Fest des heil'gen Geistes ist nicht weit,", "tokens": ["Das", "Fest", "des", "heil'\u00b7gen", "Geis\u00b7tes", "ist", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Weg zum Gottesreiche uns zu bahnen.", "tokens": ["Den", "Weg", "zum", "Got\u00b7tes\u00b7rei\u00b7che", "uns", "zu", "bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nicht nur allein f\u00fcr uns sind wir bestrebt", "tokens": ["Nicht", "nur", "al\u00b7lein", "f\u00fcr", "uns", "sind", "wir", "be\u00b7strebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Gottesreich auf Erden auszubreiten:", "tokens": ["Das", "Got\u00b7tes\u00b7reich", "auf", "Er\u00b7den", "aus\u00b7zu\u00b7brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur wer im Dienst der ganzen Menschheit lebt", "tokens": ["Nur", "wer", "im", "Dienst", "der", "gan\u00b7zen", "Menschheit", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dient sich und seiner Zeit und allen Zeiten.", "tokens": ["Dient", "sich", "und", "sei\u00b7ner", "Zeit", "und", "al\u00b7len", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "PPOSAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Drum alle, die wir solchen Dienst gew\u00e4hlt,", "tokens": ["Drum", "al\u00b7le", ",", "die", "wir", "sol\u00b7chen", "Dienst", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "$,", "PRELS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Geistes Streiter, M\u00e4nner oder Frauen,", "tokens": ["Des", "Geis\u00b7tes", "Strei\u00b7ter", ",", "M\u00e4n\u00b7ner", "o\u00b7der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob gl\u00fccklichlebend, ob von Leid gequ\u00e4lt,", "tokens": ["Ob", "gl\u00fcck\u00b7lich\u00b7le\u00b7bend", ",", "ob", "von", "Leid", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sind wir geweiht durch Mut und Gottvertrauen.", "tokens": ["Sind", "wir", "ge\u00b7weiht", "durch", "Mut", "und", "Gott\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Im Dienst der Menschheit k\u00e4mpfend, treugeschart", "tokens": ["Im", "Dienst", "der", "Menschheit", "k\u00e4mp\u00b7fend", ",", "treu\u00b7ge\u00b7schart"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind gleich den J\u00fcngern wir zum Werk verbunden \u2013", "tokens": ["Sind", "gleich", "den", "J\u00fcn\u00b7gern", "wir", "zum", "Werk", "ver\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Ahnungsschauer einer Himmelfahrt", "tokens": ["Und", "Ah\u00b7nungs\u00b7schau\u00b7er", "ei\u00b7ner", "Him\u00b7mel\u00b7fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will sich als Segen unserm Thun bekunden.", "tokens": ["Will", "sich", "als", "Se\u00b7gen", "un\u00b7serm", "Thun", "be\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "KOUS", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}