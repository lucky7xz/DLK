{"textgrid.poem.49253": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1. Gl\u00fcckw\u00fcndschungs-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Sonne steiget,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Son\u00b7ne", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lufft kriegt neue Zier,", "tokens": ["Die", "Lufft", "kriegt", "neu\u00b7e", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fast kein Gefl\u00fcgel schweiget,", "tokens": ["Fast", "kein", "Ge\u00b7fl\u00fc\u00b7gel", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen scheinen f\u00fcr,", "tokens": ["Die", "Blu\u00b7men", "schei\u00b7nen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die gr\u00fcnen H\u00fcgel lachen,", "tokens": ["Die", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Saate gehet auff,", "tokens": ["Die", "Saa\u00b7te", "ge\u00b7het", "auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wald, Wild und Feld erwachen,", "tokens": ["Wald", ",", "Wild", "und", "Feld", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Lentz kriegt seinen Lauff.", "tokens": ["Der", "Lentz", "kriegt", "sei\u00b7nen", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Vatter, Herr der Erden,", "tokens": ["O", "Vat\u00b7ter", ",", "Herr", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Di\u00df wird von dir gethan;", "tokens": ["Di\u00df", "wird", "von", "dir", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df doch uns auch froh werden,", "tokens": ["La\u00df", "doch", "uns", "auch", "froh", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieh' uns auch wider an,", "tokens": ["Sieh'", "uns", "auch", "wi\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df deines Segens Wonne", "tokens": ["Da\u00df", "dei\u00b7nes", "Se\u00b7gens", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald m\u00f6ge bey uns seyn,", "tokens": ["Bald", "m\u00f6\u00b7ge", "bey", "uns", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und deiner Gnaden Sonne", "tokens": ["Und", "dei\u00b7ner", "Gna\u00b7den", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns gebe neuen Schein.", "tokens": ["Uns", "ge\u00b7be", "neu\u00b7en", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "La\u00df' uns doch nicht mehr wallen", "tokens": ["La\u00df'", "uns", "doch", "nicht", "mehr", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In solcher Krieges-Last,", "tokens": ["In", "sol\u00b7cher", "Krie\u00b7ge\u00b7sLast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verpflantz' in Ruh' vor allen", "tokens": ["Ver\u00b7pflant\u00b7z'", "in", "Ruh'", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Aeste vom Piast,", "tokens": ["Die", "A\u00b7es\u00b7te", "vom", "Pi\u00b7ast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die K\u00f6niglichen Aeste,", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wider ihre Schuld", "tokens": ["Die", "wi\u00b7der", "ih\u00b7re", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ern\u00e4hren frembde G\u00e4ste,", "tokens": ["Er\u00b7n\u00e4h\u00b7ren", "fremb\u00b7de", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Hoffnung und Gedult.", "tokens": ["In", "Hoff\u00b7nung", "und", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Halt ob dem lieben Helden", "tokens": ["Halt", "ob", "dem", "lie\u00b7ben", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Georg-Rudolffen Hand,", "tokens": ["Ge\u00b7or\u00b7gRu\u00b7dolf\u00b7fen", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df bald ihn h\u00f6ren melden:", "tokens": ["La\u00df", "bald", "ihn", "h\u00f6\u00b7ren", "mel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm wider in dein Land;", "tokens": ["Komm", "wi\u00b7der", "in", "dein", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es m\u00fcsse vor dir fliehen", "tokens": ["Es", "m\u00fcs\u00b7se", "vor", "dir", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Waffen schwere Last,", "tokens": ["Der", "Waf\u00b7fen", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Feldt und St\u00e4tte bl\u00fchen,", "tokens": ["Das", "Feldt", "und", "St\u00e4t\u00b7te", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die du zu Diensten hast.", "tokens": ["Die", "du", "zu", "Diens\u00b7ten", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr, denen jetzt beliebet,", "tokens": ["Ihr", ",", "de\u00b7nen", "jetzt", "be\u00b7lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu trotzen auff die Macht,", "tokens": ["Zu", "trot\u00b7zen", "auff", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die euch der Krieg nur giebet,", "tokens": ["Die", "euch", "der", "Krieg", "nur", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seyt wol darauff bedacht,", "tokens": ["Seyt", "wol", "dar\u00b7auff", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Mars doch fort mu\u00df gehen;", "tokens": ["Da\u00df", "Mars", "doch", "fort", "mu\u00df", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der F\u00fcrsten hoher Stand", "tokens": ["Der", "F\u00fcrs\u00b7ten", "ho\u00b7her", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sie doch bleiben stehen:", "tokens": ["Und", "sie", "doch", "blei\u00b7ben", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott h\u00e4lt ob ihnen Hand.", "tokens": ["Gott", "h\u00e4lt", "ob", "ih\u00b7nen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}