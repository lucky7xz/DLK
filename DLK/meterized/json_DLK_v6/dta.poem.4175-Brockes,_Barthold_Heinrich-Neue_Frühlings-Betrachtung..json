{"dta.poem.4175": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neue Fr\u00fchlings-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein reges, allgemeines, pressendes, belebt Ge-", "tokens": ["Welch", "ein", "re\u00b7ges", ",", "all\u00b7ge\u00b7mei\u00b7nes", ",", "pres\u00b7sen\u00b7des", ",", "be\u00b7lebt", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,", "VVPP", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "dr\u00e4nge", "tokens": ["dr\u00e4n\u00b7ge"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mu\u00df, zur holden Lenzen-Zeit,", "tokens": ["Mu\u00df", ",", "zur", "hol\u00b7den", "Len\u00b7zen\u00b7Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In nie unterbrochener, unvermerkter Aemsigkeit,", "tokens": ["In", "nie", "un\u00b7ter\u00b7bro\u00b7che\u00b7ner", ",", "un\u00b7ver\u00b7merk\u00b7ter", "A\u00b7em\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Zu dem unsrer Erden Fl\u00e4che itzt bedeckenden Gepr\u00e4nge,", "tokens": ["Zu", "dem", "uns\u00b7rer", "Er\u00b7den", "Fl\u00e4\u00b7che", "itzt", "be\u00b7de\u00b7cken\u00b7den", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Luft und Land, und \u00fcberall alles, was man siehet, f\u00fcllen!", "tokens": ["Luft", "und", "Land", ",", "und", "\u00fc\u00b7be\u00b7rall", "al\u00b7les", ",", "was", "man", "sie\u00b7het", ",", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "ADV", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Alle S\u00e4fte g\u00e4hren gleichsam, allenthalben sieht man fast,", "tokens": ["Al\u00b7le", "S\u00e4f\u00b7te", "g\u00e4h\u00b7ren", "gleich\u00b7sam", ",", "al\u00b7len\u00b7thal\u00b7ben", "sieht", "man", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$,", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Bald aus ungeschlachter Erde, bald aus einem d\u00fcrren Ast,", "tokens": ["Bald", "aus", "un\u00b7ge\u00b7schlach\u00b7ter", "Er\u00b7de", ",", "bald", "aus", "ei\u00b7nem", "d\u00fcr\u00b7ren", "Ast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Gras und Kr\u00e4uter, Bluhmen, Knospen, Bl\u00fcht\u2019 und junge", "tokens": ["Gras", "und", "Kr\u00e4u\u00b7ter", ",", "Bluh\u00b7men", ",", "Knos\u00b7pen", ",", "Bl\u00fcht'", "und", "jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "KON", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Bl\u00e4tter quillen,", "tokens": ["Bl\u00e4t\u00b7ter", "quil\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Auch mit ihnen, junge Schatten. Was, vor einer Stunde,", "tokens": ["Auch", "mit", "ih\u00b7nen", ",", "jun\u00b7ge", "Schat\u00b7ten", ".", "Was", ",", "vor", "ei\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ADJA", "NN", "$.", "PWS", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "noch", "tokens": ["noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Ungeformt im Klumpen sa\u00df, trennet und entwickelt sich,", "tokens": ["Un\u00b7ge\u00b7formt", "im", "Klum\u00b7pen", "sa\u00df", ",", "tren\u00b7net", "und", "ent\u00b7wi\u00b7ckelt", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "F\u00e4rbt, formirt sich, und erscheinet. Wie geschiehet dieses", "tokens": ["F\u00e4rbt", ",", "for\u00b7mirt", "sich", ",", "und", "er\u00b7schei\u00b7net", ".", "Wie", "ge\u00b7schie\u00b7het", "die\u00b7ses"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "$,", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "doch?", "tokens": ["doch", "?"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Wird es denn aus den Beh\u00e4ltern mit Gewalt hervorgedrun-", "tokens": ["Wird", "es", "denn", "aus", "den", "Be\u00b7h\u00e4l\u00b7tern", "mit", "Ge\u00b7walt", "her\u00b7vor\u00b7ge\u00b7drun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.17": {"text": "gen?", "tokens": ["gen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Kommt es etwan von sich selber aus der Knosp\u2019 hervor-", "tokens": ["Kommt", "es", "et\u00b7wan", "von", "sich", "sel\u00b7ber", "aus", "der", "Knosp'", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PRF", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "gesprungen?", "tokens": ["ge\u00b7sprun\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Oder zieht von aussen etwas sie aus ihrem Sitz herf\u00fcr?", "tokens": ["O\u00b7der", "zieht", "von", "aus\u00b7sen", "et\u00b7was", "sie", "aus", "ih\u00b7rem", "Sitz", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADV", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Wie bereitet sich in ihnen ihrer Form- und Farben Zier?", "tokens": ["Wie", "be\u00b7rei\u00b7tet", "sich", "in", "ih\u00b7nen", "ih\u00b7rer", "For\u00b7m", "und", "Far\u00b7ben", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "APPR", "PPER", "PPOSAT", "TRUNC", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn wir etwas zierlichs bilden, so geschicht es durch", "tokens": ["Wenn", "wir", "et\u00b7was", "zier\u00b7lichs", "bil\u00b7den", ",", "so", "ge\u00b7schicht", "es", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF", "$,", "ADV", "VVPP", "PPER", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "die Hand,", "tokens": ["die", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Diese f\u00fchret und regieret der sie leitende Verstand,", "tokens": ["Die\u00b7se", "f\u00fch\u00b7ret", "und", "re\u00b7gie\u00b7ret", "der", "sie", "lei\u00b7ten\u00b7de", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und die\u00df nennen wir denn Kunst. Aber hier erblicken", "tokens": ["Und", "die\u00df", "nen\u00b7nen", "wir", "denn", "Kunst", ".", "A\u00b7ber", "hier", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "NN", "$.", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wir", "tokens": ["wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Etwas, welches, sonder Zuthun unsrer Seel und Hand, sich", "tokens": ["Et\u00b7was", ",", "wel\u00b7ches", ",", "son\u00b7der", "Zu\u00b7thun", "uns\u00b7rer", "Seel", "und", "Hand", ",", "sich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PRELS", "$,", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "$,", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "bildet,", "tokens": ["bil\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Sich bewundernsw\u00fcrdig f\u00e4rbt, sich versilbert, sich ver-", "tokens": ["Sich", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "f\u00e4rbt", ",", "sich", "ver\u00b7sil\u00b7bert", ",", "sich", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "PRF", "VVPP", "$,", "PRF", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "g\u00fcldet.", "tokens": ["g\u00fcl\u00b7det", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "L\u00e4\u00dft sich hier nicht deutlich sp\u00fcren, da\u00df ein anderer Ver-", "tokens": ["L\u00e4\u00dft", "sich", "hier", "nicht", "deut\u00b7lich", "sp\u00fc\u00b7ren", ",", "da\u00df", "ein", "an\u00b7de\u00b7rer", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "VVINF", "$,", "KOUS", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "stand", "tokens": ["stand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ausser uns vorhanden sey, eine weit geschicktre Hand", "tokens": ["Aus\u00b7ser", "uns", "vor\u00b7han\u00b7den", "sey", ",", "ei\u00b7ne", "weit", "ge\u00b7schickt\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "VAFIN", "$,", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ausser uns unsichtbar wirke? Ueberall sieht man die Spur,", "tokens": ["Aus\u00b7ser", "uns", "un\u00b7sicht\u00b7bar", "wir\u00b7ke", "?", "Ue\u00b7be\u00b7rall", "sieht", "man", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Da\u00df sie \u00fcberall vorhanden. Ist es denn nicht unsre Pflicht,", "tokens": ["Da\u00df", "sie", "\u00fc\u00b7be\u00b7rall", "vor\u00b7han\u00b7den", ".", "Ist", "es", "denn", "nicht", "uns\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Kunst und Weisheit zu bewundern, und der Wirkung der", "tokens": ["Kunst", "und", "Weis\u00b7heit", "zu", "be\u00b7wun\u00b7dern", ",", "und", "der", "Wir\u00b7kung", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,", "KON", "ART", "NN", "ART"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Natur", "tokens": ["Na\u00b7tur"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Ehrerbietig nachzusinnen? Unser c\u00f6rperlichs Gesicht", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "nach\u00b7zu\u00b7sin\u00b7nen", "?", "Un\u00b7ser", "c\u00f6r\u00b7per\u00b7lichs", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Siehet die gewirkten Wunder; doch Den, Der sie wirket,", "tokens": ["Sie\u00b7het", "die", "ge\u00b7wirk\u00b7ten", "Wun\u00b7der", ";", "doch", "Den", ",", "Der", "sie", "wir\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ART", "$,", "ART", "PPER", "VVFIN", "$,"], "meter": "----+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Aber unsers Geistes Auge mu\u00df ja billig weiter gehen,", "tokens": ["A\u00b7ber", "un\u00b7sers", "Geis\u00b7tes", "Au\u00b7ge", "mu\u00df", "ja", "bil\u00b7lig", "wei\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VMFIN", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Und in unleugbarer Weisheit, Schmuck und Kunst, Den", "tokens": ["Und", "in", "un\u00b7leug\u00b7ba\u00b7rer", "Weis\u00b7heit", ",", "Schmuck", "und", "Kunst", ",", "Den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "K\u00fcnstler sehen,", "tokens": ["K\u00fcnst\u00b7ler", "se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Der unwidersprechlich da. Ist Er es vielleicht nicht wehrt,", "tokens": ["Der", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "da", ".", "Ist", "Er", "es", "viel\u00b7leicht", "nicht", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "$.", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Da\u00df man Jhn zu kennen suche? Sind die herrlichen Ge-", "tokens": ["Da\u00df", "man", "Jhn", "zu", "ken\u00b7nen", "su\u00b7che", "?", "Sind", "die", "herr\u00b7li\u00b7chen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "schenke,", "tokens": ["schen\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "(da Er ja, f\u00fcr uns, nur wirkt, da Er uns erfreut und n\u00e4hrt,", "tokens": ["(", "da", "Er", "ja", ",", "f\u00fcr", "uns", ",", "nur", "wirkt", ",", "da", "Er", "uns", "er\u00b7freut", "und", "n\u00e4hrt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "$,", "APPR", "PPER", "$,", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "KON", "VVFIN", "$,"], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Nutz als Lust zugleich verbindet) nicht, da\u00df man an Jhn", "tokens": ["Nutz", "als", "Lust", "zu\u00b7gleich", "ver\u00b7bin\u00b7det", ")", "nicht", ",", "da\u00df", "man", "an", "Jhn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", 