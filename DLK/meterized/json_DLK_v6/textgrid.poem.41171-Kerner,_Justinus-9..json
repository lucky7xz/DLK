{"textgrid.poem.41171": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich mit Druckerschw\u00e4rze heut klecksographiert',", "tokens": ["Als", "ich", "mit", "Dru\u00b7cker\u00b7schw\u00e4r\u00b7ze", "heut", "kleck\u00b7so\u00b7gra\u00b7phiert'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wozu mich nur der Teufel hat verf\u00fchrt,", "tokens": ["Wo\u00b7zu", "mich", "nur", "der", "Teu\u00b7fel", "hat", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kam dieses Skandalum heraufspaziert.", "tokens": ["Kam", "die\u00b7ses", "Skan\u00b7da\u00b7lum", "her\u00b7auf\u00b7spa\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wei\u00df ich, wer der ist, noch wer der war,", "tokens": ["Nicht", "wei\u00df", "ich", ",", "wer", "der", "ist", ",", "noch", "wer", "der", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWS", "ART", "VAFIN", "$,", "KON", "PWS", "ART", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Faustus vielleicht, des Drucks Erfinder gar,", "tokens": ["Faus\u00b7tus", "viel\u00b7leicht", ",", "des", "Drucks", "Er\u00b7fin\u00b7der", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der nie war (wie bekannt) ein gl\u00e4ub'ger Christ", "tokens": ["Der", "nie", "war", "(", "wie", "be\u00b7kannt", ")", "ein", "gl\u00e4ub'\u00b7ger", "Christ"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "$(", "PWAV", "ADJD", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und als Schwarzk\u00fcnstler in der H\u00f6lle ist.", "tokens": ["Und", "als", "Schwarz\u00b7k\u00fcnst\u00b7ler", "in", "der", "H\u00f6l\u00b7le", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "M\u00f6g' solches wahr sein, oder sein nicht wahr,", "tokens": ["M\u00f6g'", "sol\u00b7ches", "wahr", "sein", ",", "o\u00b7der", "sein", "nicht", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "$,", "KON", "PPOSAT", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Kommt das bei mir heut nicht so in Betracht,", "tokens": ["Kommt", "das", "bei", "mir", "heut", "nicht", "so", "in", "Be\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "APPR", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als da\u00df dies Bild so schm\u00e4hlich sich gemacht,", "tokens": ["Als", "da\u00df", "dies", "Bild", "so", "schm\u00e4h\u00b7lich", "sich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PDS", "NN", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dieweil es ganz aus jener Schw\u00e4rze kam,", "tokens": ["Die\u00b7weil", "es", "ganz", "aus", "je\u00b7ner", "Schw\u00e4r\u00b7ze", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die manchen schon versetzt in schwarzen Gram.", "tokens": ["Die", "man\u00b7chen", "schon", "ver\u00b7setzt", "in", "schwar\u00b7zen", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Druckfehler druckt ", "tokens": ["Druck\u00b7feh\u00b7ler", "druckt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Indes der Autor stirbt voll Zorn und Scham,", "tokens": ["In\u00b7des", "der", "Au\u00b7tor", "stirbt", "voll", "Zorn", "und", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der armen Frau zuruft in stiller Nacht:", "tokens": ["Der", "ar\u00b7men", "Frau", "zu\u00b7ruft", "in", "stil\u00b7ler", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbdu Frau, und ihr, ihr lieben Kinder, wacht!", "tokens": ["\u00bb", "du", "Frau", ",", "und", "ihr", ",", "ihr", "lie\u00b7ben", "Kin\u00b7der", ",", "wacht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,", "KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wie eine Druckerpresse hat's gekracht\u00ab,", "tokens": ["Wie", "ei\u00b7ne", "Dru\u00b7cker\u00b7pres\u00b7se", "hat's", "ge\u00b7kracht", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Worauf in einem schwarzen Pfuhl ich schwamm.", "tokens": ["Wo\u00b7rauf", "in", "ei\u00b7nem", "schwar\u00b7zen", "Pfuhl", "ich", "schwamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und wieviel Kreuz ", "tokens": ["Und", "wie\u00b7viel", "Kreuz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Das wird von mir gesagt nicht, \u2013 nur ", "tokens": ["Das", "wird", "von", "mir", "ge\u00b7sagt", "nicht", ",", "\u2013", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "PTKNEG", "$,", "$(", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nur eines steh' noch da", "tokens": ["Nur", "ei\u00b7nes", "steh'", "noch", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Oft spricht zum Autor der Buchh\u00e4ndler klagend:", "tokens": ["Oft", "spricht", "zum", "Au\u00b7tor", "der", "Buch\u00b7h\u00e4nd\u00b7ler", "kla\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "\u00bbo wenn doch nichts gedruckt von Ihnen w\u00e4r'!", "tokens": ["\u00bb", "o", "wenn", "doch", "nichts", "ge\u00b7druckt", "von", "Ih\u00b7nen", "w\u00e4r'", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "ADV", "PIS", "VVPP", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ihr ind'sches Lexikon, auf meine Ehr',", "tokens": ["Ihr", "in\u00b7d'\u00b7sches", "Le\u00b7xi\u00b7kon", ",", "auf", "mei\u00b7ne", "Ehr'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Liegt zehen Jahre, v\u00f6llig nichts ertragend,", "tokens": ["Liegt", "ze\u00b7hen", "Jah\u00b7re", ",", "v\u00f6l\u00b7lig", "nichts", "er\u00b7tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$,", "ADJD", "PIS", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Und Ihre Streitschrift! \u2013 doch, ich bitte sehr!", "tokens": ["Und", "Ih\u00b7re", "Streit\u00b7schrift", "!", "\u2013", "doch", ",", "ich", "bit\u00b7te", "sehr", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "$(", "ADV", "$,", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Mit andern B\u00fcchern geht es auch sehr schwer,", "tokens": ["Mit", "an\u00b7dern", "B\u00fc\u00b7chern", "geht", "es", "auch", "sehr", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Es h\u00e4ufen sich die Krebse immer mehr.\u00ab", "tokens": ["Es", "h\u00e4u\u00b7fen", "sich", "die", "Kreb\u00b7se", "im\u00b7mer", "mehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So spricht er, \u2013 und w\u00e4r' all dies auch nur Finte.", "tokens": ["So", "spricht", "er", ",", "\u2013", "und", "w\u00e4r'", "all", "dies", "auch", "nur", "Fin\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "KON", "VAFIN", "PIAT", "PDS", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ja, Druckerschw\u00e4rze! deiner ganz entsagend,", "tokens": ["Ja", ",", "Dru\u00b7cker\u00b7schw\u00e4r\u00b7ze", "!", "dei\u00b7ner", "ganz", "ent\u00b7sa\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Nehm' zum Klecksographieren ich nur Tinte,", "tokens": ["Nehm'", "zum", "Kleck\u00b7so\u00b7gra\u00b7phie\u00b7ren", "ich", "nur", "Tin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ADV", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Mich nimmermehr mit deiner Schmiere plagend.", "tokens": ["Mich", "nim\u00b7mer\u00b7mehr", "mit", "dei\u00b7ner", "Schmie\u00b7re", "pla\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Kaum da\u00df ich dieses schreib', fuhr's mit Gekrach", "tokens": ["Kaum", "da\u00df", "ich", "die\u00b7ses", "schreib'", ",", "fuhr's", "mit", "Ge\u00b7krach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Durch das Kamin herauf bis unters Dach,", "tokens": ["Durch", "das", "Ka\u00b7min", "her\u00b7auf", "bis", "un\u00b7ters", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.35": {"text": "Und Steine st\u00fcrzten donnernd vom Kamin,", "tokens": ["Und", "Stei\u00b7ne", "st\u00fcrz\u00b7ten", "don\u00b7nernd", "vom", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ich wu\u00dfte nicht, wo ich nur sollte hin.", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "wo", "ich", "nur", "soll\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "\u00bbweh!\u00ab rief ich, \u00bbda\u00df ich unter Teufeln bin!\u00ab", "tokens": ["\u00bb", "weh", "!", "\u00ab", "rief", "ich", ",", "\u00bb", "da\u00df", "ich", "un\u00b7ter", "Teu\u00b7feln", "bin", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Das war im ersten Schrecken nur, doch pl\u00f6tzlich", "tokens": ["Das", "war", "im", "ers\u00b7ten", "Schre\u00b7cken", "nur", ",", "doch", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Sprang ich gefa\u00dft ans offne Ofenloch,", "tokens": ["Sprang", "ich", "ge\u00b7fa\u00dft", "ans", "off\u00b7ne", "O\u00b7fen\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Draus quoll ein Dampf durchs ganze Haus, der roch", "tokens": ["Draus", "quoll", "ein", "Dampf", "durchs", "gan\u00b7ze", "Haus", ",", "der", "roch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Nach Kreosot und Druckerschw\u00e4rz' entsetzlich;", "tokens": ["Nach", "Kre\u00b7o\u00b7sot", "und", "Dru\u00b7cker\u00b7schw\u00e4rz'", "ent\u00b7setz\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Ich sah durch ihn hinauf, da sah ich noch,", "tokens": ["Ich", "sah", "durch", "ihn", "hin\u00b7auf", ",", "da", "sah", "ich", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Wie die klecksographierte Unnatur,", "tokens": ["Wie", "die", "kleck\u00b7so\u00b7gra\u00b7phier\u00b7te", "Un\u00b7na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Aller Druckfehler schreckliche Figur,", "tokens": ["Al\u00b7ler", "Druck\u00b7feh\u00b7ler", "schreck\u00b7li\u00b7che", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "Hohnlachend meiner, hu! der Hexenschlingel", "tokens": ["Hohn\u00b7la\u00b7chend", "mei\u00b7ner", ",", "hu", "!", "der", "He\u00b7xen\u00b7schlin\u00b7gel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "ITJ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Zum Ritt sich schwingend auf den Pressebengel,", "tokens": ["Zum", "Ritt", "sich", "schwin\u00b7gend", "auf", "den", "Pres\u00b7se\u00b7ben\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Wie ein Weltk\u00f6nig stolz von dannen fuhr.", "tokens": ["Wie", "ein", "Welt\u00b7k\u00f6\u00b7nig", "stolz", "von", "dan\u00b7nen", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}