{"textgrid.poem.45608": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "\u00c4sthetik der Eitelkeit", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum euch die Mittelhochdeutschen so wert,", "tokens": ["Wa\u00b7rum", "euch", "die", "Mit\u00b7tel\u00b7hoch\u00b7deut\u00b7schen", "so", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kommt gleich der Grund mir entgegen:", "tokens": ["Kommt", "gleich", "der", "Grund", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Indem ihr das Kindergestammel ehrt,", "tokens": ["In\u00b7dem", "ihr", "das", "Kin\u00b7der\u00b7ge\u00b7stam\u00b7mel", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fchlt ihr euch zugleich \u00fcberlegen.", "tokens": ["F\u00fchlt", "ihr", "euch", "zu\u00b7gleich", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ists doch mit Shakespeare viel anders nicht,", "tokens": ["Ists", "doch", "mit", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "viel", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NE", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur halb gilt das Seine, das Wahre,", "tokens": ["Nur", "halb", "gilt", "das", "Sei\u00b7ne", ",", "das", "Wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihr sch\u00e4tzt ihn beleuchtet von eurem Licht,", "tokens": ["Ihr", "sch\u00e4tzt", "ihn", "be\u00b7leuch\u00b7tet", "von", "eu\u00b7rem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im Reflex eurer Kommentare.", "tokens": ["Im", "Re\u00b7flex", "eu\u00b7rer", "Kom\u00b7men\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}