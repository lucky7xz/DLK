{"dta.poem.3613": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "3.  Triumph des erw\u00e4hlten Volkes .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Triumph, es kommt die Stunde,               ", "tokens": ["Auf", "Tri\u00b7umph", ",", "es", "kommt", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sich Zion, die Geliebte, die Betr\u00fcbte hoch erfreut,", "tokens": ["Da", "sich", "Zi\u00b7on", ",", "die", "Ge\u00b7lieb\u00b7te", ",", "die", "Be\u00b7tr\u00fcb\u00b7te", "hoch", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Babel aber geht zu Grunde,", "tokens": ["Ba\u00b7bel", "a\u00b7ber", "geht", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie kl\u00e4glich \u00fcber Jammer, \u00fcber Angst und Kum-", "tokens": ["Da\u00df", "sie", "kl\u00e4g\u00b7lich", "\u00fc\u00b7ber", "Jam\u00b7mer", ",", "\u00fc\u00b7ber", "Angst", "und", "Kum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "$,", "APPR", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "mer schreit.", "tokens": ["mer", "schreit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Diese Dirne hat beflecket", "tokens": ["Die\u00b7se", "Dir\u00b7ne", "hat", "be\u00b7fle\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr geschenktes, sch\u00f6n geschm\u00fccktes jungfr\u00e4uliches Eh-", "tokens": ["Ihr", "ge\u00b7schenk\u00b7tes", ",", "sch\u00f6n", "ge\u00b7schm\u00fcck\u00b7tes", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Eh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "ADJA", "ADJA", "TRUNC"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "renkleid;", "tokens": ["ren\u00b7kleid", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und mit Schmach und Hohn bedecket,", "tokens": ["Und", "mit", "Schmach", "und", "Hohn", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die dem Lamme auf die Hochzeit ist zum Weibe zu-", "tokens": ["Die", "dem", "Lam\u00b7me", "auf", "die", "Hoch\u00b7zeit", "ist", "zum", "Wei\u00b7be", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "bereit.", "tokens": ["be\u00b7reit", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Stolze Dirne nicht verweile,", "tokens": ["Stol\u00b7ze", "Dir\u00b7ne", "nicht", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die da auf den vielen, vielen, vielen grossen Wassern", "tokens": ["Die", "da", "auf", "den", "vie\u00b7len", ",", "vie\u00b7len", ",", "vie\u00b7len", "gros\u00b7sen", "Was\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "PIAT", "$,", "PIAT", "$,", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "sitzt;", "tokens": ["sitzt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und mit Angeln und am Seile", "tokens": ["Und", "mit", "An\u00b7geln", "und", "am", "Sei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganze V\u00f6lker zu sich ziehet, und in schn\u00f6der Brunst", "tokens": ["Gan\u00b7ze", "V\u00f6l\u00b7ker", "zu", "sich", "zie\u00b7het", ",", "und", "in", "schn\u00f6\u00b7der", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "erhitzt.", "tokens": ["er\u00b7hitzt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Zion siehet auf den Stra\u00dfen", "tokens": ["Zi\u00b7on", "sie\u00b7het", "auf", "den", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die entbl\u00f6\u00dften und geschminkten stolzen T\u00f6chter Ba-", "tokens": ["Die", "ent\u00b7bl\u00f6\u00df\u00b7ten", "und", "ge\u00b7schmink\u00b7ten", "stol\u00b7zen", "T\u00f6ch\u00b7ter", "Ba"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "bels an;", "tokens": ["bels", "an", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wie sie sich beschauen lassen,", "tokens": ["Wie", "sie", "sich", "be\u00b7schau\u00b7en", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nig, Priester, hoch und niedrig haben ihre Lust", "tokens": ["K\u00f6\u00b7nig", ",", "Pries\u00b7ter", ",", "hoch", "und", "nied\u00b7rig", "ha\u00b7ben", "ih\u00b7re", "Lust"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "daran.", "tokens": ["da\u00b7ran", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Auf dem Lande, in den St\u00e4dten", "tokens": ["Auf", "dem", "Lan\u00b7de", ",", "in", "den", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Dirne mit dem Becher, alle Heyden toll ge-", "tokens": ["Hat", "die", "Dir\u00b7ne", "mit", "dem", "Be\u00b7cher", ",", "al\u00b7le", "Hey\u00b7den", "toll", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "PIAT", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "macht;", "tokens": ["macht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sie stolzieren in den Ketten,", "tokens": ["Sie", "stol\u00b7zie\u00b7ren", "in", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben sie als Schicksalsg\u00f6ttin, sich als G\u00f6tzen hoch", "tokens": ["Ha\u00b7ben", "sie", "als", "Schick\u00b7sals\u00b7g\u00f6t\u00b7tin", ",", "sich", "als", "G\u00f6t\u00b7zen", "hoch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "$,", "PRF", "KOUS", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "geacht.", "tokens": ["ge\u00b7acht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Zions Sch\u00f6pfer schaut vom Himmel", "tokens": ["Zi\u00b7ons", "Sch\u00f6p\u00b7fer", "schaut", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die vollen, tollen Heyden und sein heilig Herz", "tokens": ["Auf", "die", "vol\u00b7len", ",", "tol\u00b7len", "Hey\u00b7den", "und", "sein", "hei\u00b7lig", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "entbrennt;", "tokens": ["ent\u00b7brennt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Da\u00df das w\u00fcste Weltget\u00fcmmel", "tokens": ["Da\u00df", "das", "w\u00fcs\u00b7te", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich sein trautes Zion nennet, welches ihn doch nicht", "tokens": ["Sich", "sein", "trau\u00b7tes", "Zi\u00b7on", "nen\u00b7net", ",", "wel\u00b7ches", "ihn", "doch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "erkennt.", "tokens": ["er\u00b7kennt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Zion netzet ihre Wangen", "tokens": ["Zi\u00b7on", "net\u00b7zet", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit so vielen heissen Thr\u00e4nen \u00fcber den Verw\u00fcstungs-", "tokens": ["Mit", "so", "vie\u00b7len", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber", "den", "Ver\u00b7w\u00fcs\u00b7tungs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "gr\u00e4uel;", "tokens": ["gr\u00e4u\u00b7el", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und erwartet mit Verlangen", "tokens": ["Und", "er\u00b7war\u00b7tet", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Banden der Chald\u00e4er ihres Gottes Sieg und", "tokens": ["In", "den", "Ban\u00b7den", "der", "Chal\u00b7d\u00e4\u00b7er", "ih\u00b7res", "Got\u00b7tes", "Sieg", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Heil.", "tokens": ["Heil", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Amen, Zion ist erh\u00f6ret,", "tokens": ["A\u00b7men", ",", "Zi\u00b7on", "ist", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Thr\u00e4nen sind wie Wasser gegen Mittag aufge-", "tokens": ["Uns\u00b7re", "Thr\u00e4\u00b7nen", "sind", "wie", "Was\u00b7ser", "ge\u00b7gen", "Mit\u00b7tag", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "zehret;", "tokens": ["zeh\u00b7ret", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Seht, Chald\u00e4a ist zerst\u00f6ret,", "tokens": ["Seht", ",", "Chald\u00b7\u00e4a", "ist", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Unser Weinen ist in Jauchzen, unsre Last in Lust ver-", "tokens": ["Un\u00b7ser", "Wei\u00b7nen", "ist", "in", "Jauch\u00b7zen", ",", "uns\u00b7re", "Last", "in", "Lust", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "kehrt.", "tokens": ["kehrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.9": {"line.1": {"text": "Freue dich mit Herz und Munde,", "tokens": ["Freu\u00b7e", "dich", "mit", "Herz", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du erkauftes, auserw\u00e4hltes und erl\u00f6stes Israel;", "tokens": ["Du", "er\u00b7kauf\u00b7tes", ",", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "und", "er\u00b7l\u00f6s\u00b7tes", "Is\u00b7rael", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "KON", "ADJA", "NE", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Siehe Babels eigne Hunde,", "tokens": ["Sie\u00b7he", "Ba\u00b7bels", "eig\u00b7ne", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die die Frommen jagen mu\u00dften, fressen diese Jesabel.", "tokens": ["Die", "die", "From\u00b7men", "ja\u00b7gen", "mu\u00df\u00b7ten", ",", "fres\u00b7sen", "die\u00b7se", "Je\u00b7sa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Da wir noch an Babels Weiden", "tokens": ["Da", "wir", "noch", "an", "Ba\u00b7bels", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Harfen h\u00e4ngen m\u00fcsten, war ein Tag wie tausend", "tokens": ["Uns\u00b7re", "Har\u00b7fen", "h\u00e4n\u00b7gen", "m\u00fcs\u00b7ten", ",", "war", "ein", "Tag", "wie", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,", "VAFIN", "ART", "NN", "KOKOM", "CARD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Jahr;", "tokens": ["Jahr", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Aber nun in Zions Freuden", "tokens": ["A\u00b7ber", "nun", "in", "Zi\u00b7ons", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird f\u00fcr einen Tag gerechnet, was sonst tausend", "tokens": ["Wird", "f\u00fcr", "ei\u00b7nen", "Tag", "ge\u00b7rech\u00b7net", ",", "was", "sonst", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "PRELS", "ADV", "CARD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Jahre war.", "tokens": ["Jah\u00b7re", "war", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "O wie gro\u00df ist dein