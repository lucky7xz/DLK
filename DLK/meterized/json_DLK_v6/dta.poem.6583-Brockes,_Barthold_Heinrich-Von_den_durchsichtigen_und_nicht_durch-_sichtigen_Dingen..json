{"dta.poem.6583": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lufft, die heiter ist, das Wasser, wenn es rein,", "tokens": ["Die", "Lufft", ",", "die", "hei\u00b7ter", "ist", ",", "das", "Was\u00b7ser", ",", "wenn", "es", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat jedes abgetheilt\u2019 und rege C\u00f6rperlein,", "tokens": ["Hat", "je\u00b7des", "ab\u00b7ge\u00b7theilt'", "und", "re\u00b7ge", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch deren Oeffnungen des Tages Licht", "tokens": ["Durch", "de\u00b7ren", "Oeff\u00b7nun\u00b7gen", "des", "Ta\u00b7ges", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dessen heller Schein gantz ungehindert bricht", "tokens": ["Und", "des\u00b7sen", "hel\u00b7ler", "Schein", "gantz", "un\u00b7ge\u00b7hin\u00b7dert", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}