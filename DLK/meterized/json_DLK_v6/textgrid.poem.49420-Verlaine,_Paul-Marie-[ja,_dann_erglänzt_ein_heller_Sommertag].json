{"textgrid.poem.49420": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[ja, dann ergl\u00e4nzt ein heller Sommertag]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, dann ergl\u00e4nzt ein heller Sommertag,", "tokens": ["Ja", ",", "dann", "er\u00b7gl\u00e4nzt", "ein", "hel\u00b7ler", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es f\u00fchlt die ew'ge Sonne meine Freude,", "tokens": ["Es", "f\u00fchlt", "die", "ew'\u00b7ge", "Son\u00b7ne", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die dir in Atlas und in lichter Seide", "tokens": ["Die", "dir", "in", "At\u00b7las", "und", "in", "lich\u00b7ter", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NE", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die liebe Sch\u00f6nheit noch versch\u00f6nen mag.", "tokens": ["Die", "lie\u00b7be", "Sch\u00f6n\u00b7heit", "noch", "ver\u00b7sch\u00f6\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Blau leuchtend wird der Himmel sich erheben", "tokens": ["Blau", "leuch\u00b7tend", "wird", "der", "Him\u00b7mel", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "PRF", "VVINF"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Gleich einem Zelt, an \u00fcpp'gen Falten reich,", "tokens": ["Gleich", "ei\u00b7nem", "Zelt", ",", "an", "\u00fcpp'\u00b7gen", "Fal\u00b7ten", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hoch \u00fcber unsern Stirnen, welche bleich", "tokens": ["Hoch", "\u00fc\u00b7ber", "un\u00b7sern", "Stir\u00b7nen", ",", "wel\u00b7che", "bleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im bangen Gl\u00fccke der Erwartung beben.", "tokens": ["Im", "ban\u00b7gen", "Gl\u00fc\u00b7cke", "der", "Er\u00b7war\u00b7tung", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sanft wird das Spiel des Abendwindes sein,", "tokens": ["Sanft", "wird", "das", "Spiel", "des", "Ab\u00b7end\u00b7win\u00b7des", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der schmeichelnd dann in unsern Schleiern f\u00e4chelt,", "tokens": ["Der", "schmei\u00b7chelnd", "dann", "in", "un\u00b7sern", "Schlei\u00b7ern", "f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebesblick der Sterne aber l\u00e4chelt", "tokens": ["Der", "Lie\u00b7bes\u00b7blick", "der", "Ster\u00b7ne", "a\u00b7ber", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Gatten zu mit freundlich mildem Schein.", "tokens": ["Den", "Gat\u00b7ten", "zu", "mit", "freund\u00b7lich", "mil\u00b7dem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}