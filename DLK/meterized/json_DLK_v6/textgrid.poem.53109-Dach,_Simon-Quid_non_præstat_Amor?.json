{"textgrid.poem.53109": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Quid non pr\u00e6stat Amor?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein sch\u00f6nes Lieb verlie\u00df mit mir,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Lieb", "ver\u00b7lie\u00df", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich solt in diesem Garten", "tokens": ["Ich", "solt", "in", "die\u00b7sem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein wenig Ihrer warten,", "tokens": ["Ein", "we\u00b7nig", "Ih\u00b7rer", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sitz ich vnd verschmachte schier.", "tokens": ["So", "sitz", "ich", "vnd", "ver\u00b7schmach\u00b7te", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo bleibstu doch, mein s\u00fcsses Leben?", "tokens": ["Wo", "bleibs\u00b7tu", "doch", ",", "mein", "s\u00fcs\u00b7ses", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seum nicht, mein Sonnen-Schein,", "tokens": ["Seum", "nicht", ",", "mein", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit \u00f6pffeln wart ich dein,", "tokens": ["Mit", "\u00f6pf\u00b7feln", "wart", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vnd Trauben von dem besten Reben.", "tokens": ["Vnd", "Trau\u00b7ben", "von", "dem", "bes\u00b7ten", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hie, wo der Baum vns Schatten giebt,", "tokens": ["Hie", ",", "wo", "der", "Baum", "vns", "Schat\u00b7ten", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winde lieblich wehen,", "tokens": ["Die", "Win\u00b7de", "lieb\u00b7lich", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd meinen Kummer sehen,", "tokens": ["Vnd", "mei\u00b7nen", "Kum\u00b7mer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sol seyn, was mir vnd dir beliebt;", "tokens": ["Sol", "seyn", ",", "was", "mir", "vnd", "dir", "be\u00b7liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "PWS", "PPER", "KON", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe Gra\u00df hieher getragen", "tokens": ["Ich", "ha\u00b7be", "Gra\u00df", "hie\u00b7her", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd wei\u00df von keiner Ruh,", "tokens": ["Vnd", "wei\u00df", "von", "kei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es mangelt nichts, als Du,", "tokens": ["Es", "man\u00b7gelt", "nichts", ",", "als", "Du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Lass mich nicht vber Vntrew klagen!", "tokens": ["Lass", "mich", "nicht", "vber", "Vnt\u00b7rew", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach Mutter! haltet jhr Sie an,", "tokens": ["Ach", "Mut\u00b7ter", "!", "hal\u00b7tet", "jhr", "Sie", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wil ich euch beschwehren", "tokens": ["So", "wil", "ich", "euch", "be\u00b7schweh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey meiner Glut vnd Zehren,", "tokens": ["Bey", "mei\u00b7ner", "Glut", "vnd", "Zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey allem, was euch lieb seyn kan,", "tokens": ["Bey", "al\u00b7lem", ",", "was", "euch", "lieb", "seyn", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey Ihren sittsamen Geberden,", "tokens": ["Bey", "Ih\u00b7ren", "sitt\u00b7sa\u00b7men", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Bey jhrem jungen Blut", "tokens": ["Bey", "jhrem", "jun\u00b7gen", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Vnd Tugendthafften Muth,", "tokens": ["Vnd", "Tu\u00b7gendt\u00b7haff\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der alles zwingt, was lebt auff Erden,", "tokens": ["Der", "al\u00b7les", "zwingt", ",", "was", "lebt", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bi\u00df da\u00df Ihr lasst mein Trost vnd Licht.", "tokens": ["Bi\u00df", "da\u00df", "Ihr", "lasst", "mein", "Trost", "vnd", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber wil indessen", "tokens": ["Ich", "a\u00b7ber", "wil", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur Ihre Zier ermessen,", "tokens": ["Nur", "Ih\u00b7re", "Zier", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mein vnd mich dazu zerbricht.", "tokens": ["Die", "Mein", "vnd", "mich", "da\u00b7zu", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Betreugt mich aber mein Verlangen,", "tokens": ["Be\u00b7treugt", "mich", "a\u00b7ber", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sol nach langer Noht", "tokens": ["So", "sol", "nach", "lan\u00b7ger", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An diesem Orth der Todt", "tokens": ["An", "die\u00b7sem", "Orth", "der", "Todt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "An Ihre stat mich doch vmbfangen.", "tokens": ["An", "Ih\u00b7re", "stat", "mich", "doch", "vmb\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}