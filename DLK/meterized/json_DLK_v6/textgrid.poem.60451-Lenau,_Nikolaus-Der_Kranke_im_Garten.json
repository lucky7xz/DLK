{"textgrid.poem.60451": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch eine Nachtigall, so sp\u00e4t?", "tokens": ["Noch", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ",", "so", "sp\u00e4t", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon sind die Bl\u00fcten l\u00e4ngst verweht,", "tokens": ["Schon", "sind", "die", "Bl\u00fc\u00b7ten", "l\u00e4ngst", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sommer reift die Felder schon,", "tokens": ["Der", "Som\u00b7mer", "reift", "die", "Fel\u00b7der", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch ein Fr\u00fchlingston?", "tokens": ["Und", "noch", "ein", "Fr\u00fch\u00b7lings\u00b7ton", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Lenz, ward es dir offenbar,", "tokens": ["O", "Lenz", ",", "ward", "es", "dir", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich noch sterbe dieses Jahr?", "tokens": ["Da\u00df", "ich", "noch", "ster\u00b7be", "die\u00b7ses", "Jahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und riefest aus der Ferne du", "tokens": ["Und", "rie\u00b7fest", "aus", "der", "Fer\u00b7ne", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einen Gru\u00df mir zu? \u2013", "tokens": ["Noch", "ei\u00b7nen", "Gru\u00df", "mir", "zu", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}