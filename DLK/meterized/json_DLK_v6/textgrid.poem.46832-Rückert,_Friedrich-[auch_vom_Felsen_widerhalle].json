{"textgrid.poem.46832": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[auch vom Felsen widerhalle]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch vom Felsen widerhalle", "tokens": ["Auch", "vom", "Fel\u00b7sen", "wi\u00b7der\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Klage Tr\u00f6sten schon,", "tokens": ["Mei\u00b7ner", "Kla\u00b7ge", "Tr\u00f6s\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wievielmehr im Trauerfalle", "tokens": ["Wie\u00b7viel\u00b7mehr", "im", "Trau\u00b7er\u00b7fal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus der Fremd' ein Herzenston.", "tokens": ["Aus", "der", "Fremd'", "ein", "Her\u00b7zen\u00b7ston", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was der Freund von meinen Lieben", "tokens": ["Was", "der", "Freund", "von", "mei\u00b7nen", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir geschrieben an das Herz,", "tokens": ["Mir", "ge\u00b7schrie\u00b7ben", "an", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klingt als h\u00e4tten mirs geschrieben", "tokens": ["Klingt", "als", "h\u00e4t\u00b7ten", "mirs", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "VAFIN", "NE", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lieben erdenw\u00e4rts.", "tokens": ["Mei\u00b7ne", "Lie\u00b7ben", "er\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Nicht als wie aus Erdenferne,", "tokens": ["Nicht", "als", "wie", "aus", "Er\u00b7den\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie vom Himmel ist's ein Trost,", "tokens": ["Wie", "vom", "Him\u00b7mel", "ist's", "ein", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich als sei von Stern zu Sterne", "tokens": ["Gleich", "als", "sei", "von", "Stern", "zu", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingerichtet Engelspost.", "tokens": ["Ein\u00b7ge\u00b7rich\u00b7tet", "En\u00b7gel\u00b7spost", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}