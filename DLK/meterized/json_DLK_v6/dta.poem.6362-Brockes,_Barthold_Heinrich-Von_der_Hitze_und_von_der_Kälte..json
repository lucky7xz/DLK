{"dta.poem.6362": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.57", "nl:0.28", "et:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja von Natur der Marmor kalt, erstarrt,", "tokens": ["Es", "ist", "ja", "von", "Na\u00b7tur", "der", "Mar\u00b7mor", "kalt", ",", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ART", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Klumpen, welcher dicht und hart.", "tokens": ["Ein", "Klum\u00b7pen", ",", "wel\u00b7cher", "dicht", "und", "hart", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es liegt ein jeder Theil davon in steter Ruh.", "tokens": ["Es", "liegt", "ein", "je\u00b7der", "Theil", "da\u00b7von", "in", "ste\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Legt sich im Frost nicht eben auch,", "tokens": ["Legt", "sich", "im", "Frost", "nicht", "e\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch des beeisten Nordwinds Hauch,", "tokens": ["Durch", "des", "be\u00b7eis\u00b7ten", "Nord\u00b7winds", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In einer kalten Rast, das harte Wasser zu?", "tokens": ["In", "ei\u00b7ner", "kal\u00b7ten", "Rast", ",", "das", "har\u00b7te", "Was\u00b7ser", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}