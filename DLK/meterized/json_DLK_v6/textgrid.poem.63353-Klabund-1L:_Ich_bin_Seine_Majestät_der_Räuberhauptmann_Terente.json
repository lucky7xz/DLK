{"textgrid.poem.63353": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin Seine Majest\u00e4t der R\u00e4uberhauptmann Terente", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin Seine Majest\u00e4t der R\u00e4uberhauptmann Terente", "tokens": ["Ich", "bin", "Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "der", "R\u00e4u\u00b7ber\u00b7haupt\u00b7mann", "Te\u00b7ren\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+--+-+-+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und geruhe, im Donaudelta das Zepter eines knorrigen Eichenkn\u00fcttels zu schwingen.", "tokens": ["Und", "ge\u00b7ru\u00b7he", ",", "im", "Do\u00b7nau\u00b7del\u00b7ta", "das", "Zep\u00b7ter", "ei\u00b7nes", "knor\u00b7ri\u00b7gen", "Ei\u00b7chen\u00b7kn\u00fct\u00b7tels", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Ich bin der Herr der hundert Teiche", "tokens": ["Ich", "bin", "der", "Herr", "der", "hun\u00b7dert", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Sklave der tausend M\u00e4dchen.", "tokens": ["Und", "der", "Skla\u00b7ve", "der", "tau\u00b7send", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "CARD", "NN", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Eines Tages in Braila auf dem Markt", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "in", "Brai\u00b7la", "auf", "dem", "Markt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sah ich zwei sch\u00f6ne Schwestern vom Erker auf mich herniederl\u00e4cheln.", "tokens": ["Sah", "ich", "zwei", "sch\u00f6\u00b7ne", "Schwes\u00b7tern", "vom", "Er\u00b7ker", "auf", "mich", "her\u00b7nie\u00b7der\u00b7l\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Eines Nachts in Braila auf dem Markt", "tokens": ["Ei\u00b7nes", "Nachts", "in", "Brai\u00b7la", "auf", "dem", "Markt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Raubte ich sie zu ihren Geschwistern, den Wildenten, in den Donausumpf.", "tokens": ["Raub\u00b7te", "ich", "sie", "zu", "ih\u00b7ren", "Ge\u00b7schwis\u00b7tern", ",", "den", "Wil\u00b7den\u00b7ten", ",", "in", "den", "Do\u00b7naus\u00b7umpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-++-+--+--+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich liebe die armen Teufel, die armen Engel.", "tokens": ["Ich", "lie\u00b7be", "die", "ar\u00b7men", "Teu\u00b7fel", ",", "die", "ar\u00b7men", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+----+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich habe zehn kriegsinvaliden Bettlern Leierk\u00e4sten gekauft.", "tokens": ["Ich", "ha\u00b7be", "zehn", "kriegs\u00b7in\u00b7va\u00b7li\u00b7den", "Bett\u00b7lern", "Lei\u00b7er\u00b7k\u00e4s\u00b7ten", "ge\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sie spielen auf den H\u00f6fen in Bukarest und Konstanza", "tokens": ["Sie", "spie\u00b7len", "auf", "den", "H\u00f6\u00b7fen", "in", "Bu\u00b7ka\u00b7rest", "und", "Kons\u00b7tan\u00b7za"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "KON", "NE"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Das Lied vom R\u00e4uberhauptmann Terente.", "tokens": ["Das", "Lied", "vom", "R\u00e4u\u00b7ber\u00b7haupt\u00b7mann", "Te\u00b7ren\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Cojoccar und Cervusa sind Laffen gegen mich.", "tokens": ["Co\u00b7joc\u00b7car", "und", "Cer\u00b7vu\u00b7sa", "sind", "Laf\u00b7fen", "ge\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Man wird sie mit Recht oder Unrecht h\u00e4ngen.", "tokens": ["Man", "wird", "sie", "mit", "Recht", "o\u00b7der", "Un\u00b7recht", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Aber nicht h\u00e4ngen wird man mich, der ich h\u00e4nge wild", "tokens": ["A\u00b7ber", "nicht", "h\u00e4n\u00b7gen", "wird", "man", "mich", ",", "der", "ich", "h\u00e4n\u00b7ge", "wild"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVINF", "VAFIN", "PIS", "PPER", "$,", "PRELS", "PPER", "VVFIN", "ADJD"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Am Leben.", "tokens": ["Am", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}