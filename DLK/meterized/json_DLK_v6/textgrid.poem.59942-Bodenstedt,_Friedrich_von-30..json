{"textgrid.poem.59942": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl besser ist's, ohn' Anerkennung leben", "tokens": ["Wohl", "bes\u00b7ser", "ist's", ",", "ohn'", "An\u00b7er\u00b7ken\u00b7nung", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch Verdienst des H\u00f6chsten wert zu sein,", "tokens": ["Und", "durch", "Ver\u00b7dienst", "des", "H\u00f6chs\u00b7ten", "wert", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als unverdient zum H\u00f6chsten sich erheben,", "tokens": ["Als", "un\u00b7ver\u00b7di\u00b7ent", "zum", "H\u00f6chs\u00b7ten", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gro\u00df vor der Welt und vor sich selber klein.", "tokens": ["Gro\u00df", "vor", "der", "Welt", "und", "vor", "sich", "sel\u00b7ber", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}