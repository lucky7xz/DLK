{"dta.poem.23852": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Nach Absterben des Autoris ersten  \n Gemahlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sagte da mein Hertz mit Schmertz war angef\u00fcllt/", "tokens": ["Ich", "sag\u00b7te", "da", "mein", "Hertz", "mit", "Schmertz", "war", "an\u00b7ge\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin/ erbarm es GOtt! des Hiobs Ebenbild/", "tokens": ["Ich", "bin", "/", "er\u00b7barm", "es", "Gott", "!", "des", "Hiobs", "E\u00b7ben\u00b7bild", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ADJD", "PPER", "NN", "$.", "ART", "NE", "NE", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch dacht ich/ Hiob darf sich mehr als ich betr\u00fcben/", "tokens": ["Doch", "dacht", "ich", "/", "Hiob", "darf", "sich", "mehr", "als", "ich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "NE", "VMFIN", "PRF", "ADV", "KOUS", "PPER", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mir ist mein halbes Guth/ ihm keines \u00fcbrig blieben.", "tokens": ["Mir", "ist", "mein", "hal\u00b7bes", "Guth", "/", "ihm", "kei\u00b7nes", "\u00fcb\u00b7rig", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja aller Kinder Tod beweint der krancke Mann/", "tokens": ["Ja", "al\u00b7ler", "Kin\u00b7der", "Tod", "be\u00b7weint", "der", "kran\u00b7cke", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ich doch einen Sohn gesund noch k\u00fcssen kan/", "tokens": ["Da", "ich", "doch", "ei\u00b7nen", "Sohn", "ge\u00b7sund", "noch", "k\u00fcs\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nur in einem St\u00fcck sind wir uns zu vergleichen/", "tokens": ["Und", "nur", "in", "ei\u00b7nem", "St\u00fcck", "sind", "wir", "uns", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er sein Weib beh\u00e4lt/ und meines mu\u00df verbleichen.", "tokens": ["Da\u00df", "er", "sein", "Weib", "be\u00b7h\u00e4lt", "/", "und", "mei\u00b7nes", "mu\u00df", "ver\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}