{"dta.poem.10559": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Nachts in der Caj\u00fcte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "An die bretterne Schiffswand,", "tokens": ["An", "die", "bret\u00b7ter\u00b7ne", "Schiffs\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo mein tr\u00e4umendes Haupt liegt,", "tokens": ["Wo", "mein", "tr\u00e4u\u00b7men\u00b7des", "Haupt", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Branden die Wellen, die wilden Wellen.", "tokens": ["Bran\u00b7den", "die", "Wel\u00b7len", ",", "die", "wil\u00b7den", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie rauschen und murmeln", "tokens": ["Sie", "rau\u00b7schen", "und", "mur\u00b7meln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mir heimlich in's Ohr:", "tokens": ["Mir", "heim\u00b7lich", "in's", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "\u201ebeth\u00f6rter Geselle!", "tokens": ["\u201e", "be\u00b7th\u00f6r\u00b7ter", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dein Arm ist kurz, und der Himmel ist weit", "tokens": ["Dein", "Arm", "ist", "kurz", ",", "und", "der", "Him\u00b7mel", "ist", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und die Sterne droben sind festgenagelt,", "tokens": ["Und", "die", "Ster\u00b7ne", "dro\u00b7ben", "sind", "fest\u00b7ge\u00b7na\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vergebliches Sehnen, vergebliches Seufzen,", "tokens": ["Ver\u00b7geb\u00b7li\u00b7ches", "Seh\u00b7nen", ",", "ver\u00b7geb\u00b7li\u00b7ches", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Das Beste w\u00e4re, du schliefest ein.\u201c", "tokens": ["Das", "Bes\u00b7te", "w\u00e4\u00b7re", ",", "du", "schlie\u00b7fest", "ein", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}