{"textgrid.poem.42760": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Klimmzug", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Symbol f\u00fcr das Leben.", "tokens": ["Das", "ist", "ein", "Sym\u00b7bol", "f\u00fcr", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Immer aufw\u00e4rts, himmelanstreben!", "tokens": ["Im\u00b7mer", "auf\u00b7w\u00e4rts", ",", "him\u00b7me\u00b7lans\u00b7tre\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Feste zieh! Nicht nachgeben!", "tokens": ["Fes\u00b7te", "zieh", "!", "Nicht", "nach\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Stelle dir vor: Dort oben winken", "tokens": ["Stel\u00b7le", "dir", "vor", ":", "Dort", "o\u00b7ben", "win\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$.", "ADV", "ADV", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Schn\u00e4pse und Schinken.", "tokens": ["Schn\u00e4p\u00b7se", "und", "Schin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Trachte sie zu erreichen, die Schn\u00e4pse.", "tokens": ["Trach\u00b7te", "sie", "zu", "er\u00b7rei\u00b7chen", ",", "die", "Schn\u00e4p\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Spanne die Muskeln, die Bizepse.", "tokens": ["Span\u00b7ne", "die", "Mus\u00b7keln", ",", "die", "Bi\u00b7zep\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Achte ver die Beschwerden.", "tokens": ["Ach\u00b7te", "ver", "die", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Nicht einschlafen. Nicht m\u00fcde werden!", "tokens": ["Nicht", "ein\u00b7schla\u00b7fen", ".", "Nicht", "m\u00fc\u00b7de", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIZU", "$.", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Du mu\u00dft in Gedanken w\u00e4hnen:", "tokens": ["Du", "mu\u00dft", "in", "Ge\u00b7dan\u00b7ken", "w\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Du h\u00f6rtest unter dir einen Schlund g\u00e4hnen.", "tokens": ["Du", "h\u00f6r\u00b7test", "un\u00b7ter", "dir", "ei\u00b7nen", "Schlund", "g\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-++-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "In dem Schlund sind Igel und W\u00f6lfe versammelt.", "tokens": ["In", "dem", "Schlund", "sind", "I\u00b7gel", "und", "W\u00f6l\u00b7fe", "ver\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die freuen sich auf den Menschen, der oben bammelt.", "tokens": ["Die", "freu\u00b7en", "sich", "auf", "den", "Men\u00b7schen", ",", "der", "o\u00b7ben", "bam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zu! Zu! Tu nicht \u00fcberlegen.", "tokens": ["Zu", "!", "Zu", "!", "Tu", "nicht", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "$.", "NE", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Immer weiter, herrlichen Zielen entgegen.", "tokens": ["Im\u00b7mer", "wei\u00b7ter", ",", "herr\u00b7li\u00b7chen", "Zie\u00b7len", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Sollte dich ein Floh am Po kneifen,", "tokens": ["Soll\u00b7te", "dich", "ein", "Floh", "am", "Po", "knei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Nicht mit beiden H\u00e4nden zugleich danach greifen.", "tokens": ["Nicht", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", "zu\u00b7gleich", "da\u00b7nach", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PIAT", "NN", "ADV", "PAV", "VVINF", "$."], "meter": "+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.18": {"text": "Nicht so ruckweis hin und her schlenkern;", "tokens": ["Nicht", "so", "ruck\u00b7weis", "hin", "und", "her", "schlen\u00b7kern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Das pa\u00dft nicht f\u00fcr ein Volk von Turnern und Denkern.", "tokens": ["Das", "pa\u00dft", "nicht", "f\u00fcr", "ein", "Volk", "von", "Tur\u00b7nern", "und", "Den\u00b7kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Klimme wacker,", "tokens": ["Klim\u00b7me", "wa\u00b7cker", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Alter Knacker!", "tokens": ["Al\u00b7ter", "Kna\u00b7cker", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Klimme, klimb", "tokens": ["Klim\u00b7me", ",", "klimb"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Zum Olymp!", "tokens": ["Zum", "O\u00b7lymp", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "H\u00f6her hinauf!", "tokens": ["H\u00f6\u00b7her", "hin\u00b7auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Gl\u00fcckauf!", "tokens": ["Gl\u00fcc\u00b7kauf", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Kragen total durchweicht.", "tokens": ["Kra\u00b7gen", "to\u00b7tal", "durch\u00b7weicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.27": {"text": "\u00c4h \u2013 \u00e4h \u2013 \u00e4h \u2013 endlich erreicht.", "tokens": ["\u00c4h", "\u2013", "\u00e4h", "\u2013", "\u00e4h", "\u2013", "end\u00b7lich", "er\u00b7reicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "ADJD", "$(", "ADV", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.28": {"text": "Das Unbeschreibliche zieht uns hinan,", "tokens": ["Das", "Un\u00b7be\u00b7schreib\u00b7li\u00b7che", "zieht", "uns", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Der ewigweibliche Turnvater Jahn.", "tokens": ["Der", "e\u00b7wig\u00b7weib\u00b7li\u00b7che", "Turn\u00b7va\u00b7ter", "Jahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}