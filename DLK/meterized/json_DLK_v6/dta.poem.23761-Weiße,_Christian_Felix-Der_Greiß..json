{"dta.poem.23761": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Grei\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Dort fiel ein armer alter Grei\u00df!", "tokens": ["Dort", "fiel", "ein", "ar\u00b7mer", "al\u00b7ter", "Grei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Haupt war wie ein Silber", "tokens": ["Sein", "Haupt", "war", "wie", "ein", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wei\u00df,", "tokens": ["wei\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und ihm versagt sein zitternd Knie,", "tokens": ["Und", "ihm", "ver\u00b7sagt", "sein", "zit\u00b7ternd", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAINF", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ach \u2014 die b\u00f6sen Knaben die,", "tokens": ["Und", "ach", "die", "b\u00f6\u00b7sen", "Kna\u00b7ben", "die", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$(", "ART", "ADJA", "NN", "ART", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie lachten sie!", "tokens": ["Wie", "lach\u00b7ten", "sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mich dauert dieser gute Mann!", "tokens": ["Mich", "dau\u00b7ert", "die\u00b7ser", "gu\u00b7te", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer eines Alten spotten kann,", "tokens": ["Wer", "ei\u00b7nes", "Al\u00b7ten", "spot\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist der wohl werth, ietzt jung zu seyn?", "tokens": ["Ist", "der", "wohl", "werth", ",", "ietzt", "jung", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist der wohl werth, einst alt zu seyn?", "tokens": ["Ist", "der", "wohl", "werth", ",", "einst", "alt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wahrhaftig, nein!", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "nein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}