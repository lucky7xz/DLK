{"textgrid.poem.24572": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie man sich bey seinen Lobe verhalten soll", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein billig Lob nimmt uns mit Anmuth ein.", "tokens": ["Ein", "bil\u00b7lig", "Lob", "nimmt", "uns", "mit", "An\u00b7muth", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jedoch ein kluger Geist soll dieses zu verstehen/", "tokens": ["Je\u00b7doch", "ein", "klu\u00b7ger", "Geist", "soll", "die\u00b7ses", "zu", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Viel weniger auf das Vergn\u00fcgen sehen/", "tokens": ["Viel", "we\u00b7ni\u00b7ger", "auf", "das", "Ver\u00b7gn\u00fc\u00b7gen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als auf die M\u00fch' es k\u00fcnftig wehrt zu seyn.", "tokens": ["Als", "auf", "die", "M\u00fch'", "es", "k\u00fcnf\u00b7tig", "wehrt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}