{"textgrid.poem.52447": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "29. Das Mitleid", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitleid! Heil dir, du Geweihte!", "tokens": ["Mit\u00b7leid", "!", "Heil", "dir", ",", "du", "Ge\u00b7weih\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPER", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiches Herzens, milder Hand,", "tokens": ["Wei\u00b7ches", "Her\u00b7zens", ",", "mil\u00b7der", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wallst du an des Dulders Seite", "tokens": ["Wallst", "du", "an", "des", "Dul\u00b7ders", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Pr\u00fcfung rauhes Land;", "tokens": ["Durch", "der", "Pr\u00fc\u00b7fung", "rau\u00b7hes", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Taust wie Balsam, milde Z\u00e4hren,", "tokens": ["Taust", "wie", "Bal\u00b7sam", ",", "mil\u00b7de", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hebest das zerknickte Rohr.", "tokens": ["He\u00b7best", "das", "zer\u00b7knick\u00b7te", "Rohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie zu Hyllius Alt\u00e4ren", "tokens": ["Wie", "zu", "Hyl\u00b7lius", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Blickt die Not zu dir empor.", "tokens": ["Blickt", "die", "Not", "zu", "dir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Hilfe stillt ihr Flehen;", "tokens": ["Dei\u00b7ne", "Hil\u00b7fe", "stillt", "ihr", "Fle\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Erbarmen eilt zur That.", "tokens": ["Dein", "Er\u00b7bar\u00b7men", "eilt", "zur", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnsche brennst du auszusp\u00e4hen,", "tokens": ["W\u00fcn\u00b7sche", "brennst", "du", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spendest, wenn der Mangel bat:", "tokens": ["Spen\u00b7dest", ",", "wenn", "der", "Man\u00b7gel", "bat", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spendest Br\u00fcdern, welche darben,", "tokens": ["Spen\u00b7dest", "Br\u00fc\u00b7dern", ",", "wel\u00b7che", "dar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Tagewerks Gewinn;", "tokens": ["Dei\u00b7nes", "Ta\u00b7ge\u00b7werks", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bindest loser deine Garben", "tokens": ["Bin\u00b7dest", "lo\u00b7ser", "dei\u00b7ne", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor der \u00c4hrenleserin.", "tokens": ["Vor", "der", "\u00c4h\u00b7ren\u00b7le\u00b7se\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In verarmter Witwen Kr\u00fcge", "tokens": ["In", "ver\u00b7arm\u00b7ter", "Wit\u00b7wen", "Kr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttest du der St\u00e4rkung Wein,", "tokens": ["Sch\u00fct\u00b7test", "du", "der", "St\u00e4r\u00b7kung", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00e4gst des L\u00e4chelns heitre Z\u00fcge", "tokens": ["Pr\u00e4gst", "des", "L\u00e4\u00b7chelns", "heit\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgeh\u00e4rmten Wangen ein;", "tokens": ["Ab\u00b7ge\u00b7h\u00e4rm\u00b7ten", "Wan\u00b7gen", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebst erlegner Wandrer B\u00fcrde", "tokens": ["Hebst", "er\u00b7leg\u00b7ner", "Wand\u00b7rer", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem tiefbeschneiten Damm,", "tokens": ["Auf", "dem", "tief\u00b7be\u00b7schnei\u00b7ten", "Damm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verpflegst in sichrer H\u00fcrde", "tokens": ["Und", "ver\u00b7pflegst", "in", "sich\u00b7rer", "H\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deines Nachbars irres Lamm.", "tokens": ["Dei\u00b7nes", "Nach\u00b7bars", "ir\u00b7res", "Lamm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sorglich streust du vor die Scheuer", "tokens": ["Sorg\u00b7lich", "streust", "du", "vor", "die", "Scheu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6geln Korn im Winter aus;", "tokens": ["V\u00f6\u00b7geln", "Korn", "im", "Win\u00b7ter", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00f6tigst zu des Herdes Feuer", "tokens": ["N\u00f6\u00b7tigst", "zu", "des", "Her\u00b7des", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Pilger in dein wirtlich Haus;", "tokens": ["Pil\u00b7ger", "in", "dein", "wirt\u00b7lich", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herbergst an des Strohdachs Balken", "tokens": ["Her\u00b7bergst", "an", "des", "Stroh\u00b7dachs", "Bal\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Prognens federlose Brut;", "tokens": ["Prog\u00b7nens", "fe\u00b7der\u00b7lo\u00b7se", "Brut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schirmest T\u00e4ubchen vor des Falken,", "tokens": ["Schir\u00b7mest", "T\u00e4ub\u00b7chen", "vor", "des", "Fal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fcchlein vor des Geiers Wut.", "tokens": ["K\u00fcch\u00b7lein", "vor", "des", "Gei\u00b7ers", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du entf\u00fchrst die junge Waise", "tokens": ["Du", "ent\u00b7f\u00fchrst", "die", "jun\u00b7ge", "Wai\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Mutter Rasengruft;", "tokens": ["Ih\u00b7rer", "Mut\u00b7ter", "Ra\u00b7sen\u00b7gruft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeden Seufzer, noch so leise,", "tokens": ["Je\u00b7den", "Seuf\u00b7zer", ",", "noch", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Raubt dein Ohr der Abendluft;", "tokens": ["Raubt", "dein", "Ohr", "der", "A\u00b7ben\u00b7dluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sanft, wie tauige Hyaden,", "tokens": ["Sanft", ",", "wie", "tau\u00b7i\u00b7ge", "Hya\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Blickst du auf das Findelkind,", "tokens": ["Blickst", "du", "auf", "das", "Fin\u00b7del\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Reichst ihm Ariadnens Faden", "tokens": ["Reichst", "ihm", "A\u00b7riad\u00b7nens", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Durch des Lebens Labyrinth.", "tokens": ["Durch", "des", "Le\u00b7bens", "La\u00b7by\u00b7rinth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du erw\u00e4rmst in sanfter R\u00fchrung", "tokens": ["Du", "er\u00b7w\u00e4rmst", "in", "sanf\u00b7ter", "R\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der Selbstsucht starres Eis,", "tokens": ["Auch", "der", "Selbst\u00b7sucht", "star\u00b7res", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warnst vor lockender Verf\u00fchrung", "tokens": ["Warnst", "vor", "lo\u00b7cken\u00b7der", "Ver\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcten\u00fcberstreutem Gleis';", "tokens": ["Bl\u00fc\u00b7ten\u00b7\u00fc\u00b7bers\u00b7treu\u00b7tem", "Gleis'", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neigest dich mit leisem Tr\u00f6sten", "tokens": ["Nei\u00b7gest", "dich", "mit", "lei\u00b7sem", "Tr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An der Schwermut dumpfes Ohr;", "tokens": ["An", "der", "Schwer\u00b7mut", "dum\u00b7pfes", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hebst entfesselt den Erl\u00f6sten", "tokens": ["Hebst", "ent\u00b7fes\u00b7selt", "den", "Er\u00b7l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von des Kerkers Stroh empor.", "tokens": ["Von", "des", "Ker\u00b7kers", "Stroh", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Herzen, die der Harm zerrissen,", "tokens": ["Her\u00b7zen", ",", "die", "der", "Harm", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hegst du mit besorgter Treu';", "tokens": ["Hegst", "du", "mit", "be\u00b7sorg\u00b7ter", "Treu'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcckest der Geduld das Kissen", "tokens": ["R\u00fc\u00b7ckest", "der", "Ge\u00b7duld", "das", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Schmerzenlagers Streu;", "tokens": ["Auf", "des", "Schmer\u00b7zen\u00b7la\u00b7gers", "Streu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schonst des Schlummers, nahst auf Socken;", "tokens": ["Schonst", "des", "Schlum\u00b7mers", ",", "nahst", "auf", "So\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchlst mit deinem Palmenreis;", "tokens": ["K\u00fchlst", "mit", "dei\u00b7nem", "Pal\u00b7men\u00b7reis", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trocknest mit ergo\u00dfnen Locken", "tokens": ["Trock\u00b7nest", "mit", "er\u00b7go\u00df\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Banger Todesk\u00e4mpfe Schwei\u00df.", "tokens": ["Ban\u00b7ger", "To\u00b7des\u00b7k\u00e4mp\u00b7fe", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bleib bei uns, bis einst die Hefe", "tokens": ["Bleib", "bei", "uns", ",", "bis", "einst", "die", "He\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Thr\u00e4nenkelch versiegt;", "tokens": ["In", "dem", "Thr\u00e4\u00b7nen\u00b7kelch", "ver\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4nze bleicher Tr\u00fcbsal Schl\u00e4fe,", "tokens": ["Kr\u00e4n\u00b7ze", "blei\u00b7cher", "Tr\u00fcb\u00b7sal", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die an deinen Scho\u00df sich schmiegt;", "tokens": ["Die", "an", "dei\u00b7nen", "Scho\u00df", "sich", "schmiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text"