{"textgrid.poem.31719": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sehnt sich meine Seele", "tokens": ["Es", "sehnt", "sich", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach einem k\u00fchlen Trunk.", "tokens": ["Nach", "ei\u00b7nem", "k\u00fch\u00b7len", "Trunk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den besten, den ich w\u00e4hle,", "tokens": ["Den", "bes\u00b7ten", ",", "den", "ich", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ist nur gut genung.", "tokens": ["Der", "ist", "nur", "gut", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er steht so sch\u00f6n im Glase", "tokens": ["Er", "steht", "so", "sch\u00f6n", "im", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gibt so lichten Schein,", "tokens": ["Und", "gibt", "so", "lich\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Morgentau im Grase,", "tokens": ["Wie", "Mor\u00b7gen\u00b7tau", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Rosen auf dem Rain.", "tokens": ["Wie", "Ro\u00b7sen", "auf", "dem", "Rain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich fange an zu singen", "tokens": ["Ich", "fan\u00b7ge", "an", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom K\u00f6nig Salomo,", "tokens": ["Vom", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom F\u00fcrst zu Flachsenfingen \u2013", "tokens": ["Vom", "F\u00fcrst", "zu", "Flach\u00b7sen\u00b7fin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bin in dubio,", "tokens": ["Und", "bin", "in", "du\u00b7bio", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ob nicht die bl\u00fchnde Rebe", "tokens": ["Ob", "nicht", "die", "bl\u00fchn\u00b7de", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So jugendlich und hold", "tokens": ["So", "ju\u00b7gend\u00b7lich", "und", "hold"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel besser sei als St\u00e4be", "tokens": ["Viel", "bes\u00b7ser", "sei", "als", "St\u00e4\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Silber und von Gold;", "tokens": ["Von", "Sil\u00b7ber", "und", "von", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob man in jenen Welten,", "tokens": ["Ob", "man", "in", "je\u00b7nen", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind wir nicht fromm gewest,", "tokens": ["Sind", "wir", "nicht", "fromm", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das B\u00f6se zu vergelten", "tokens": ["Das", "B\u00f6\u00b7se", "zu", "ver\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns schrecklich d\u00fcrsten l\u00e4\u00dft;", "tokens": ["Uns", "schreck\u00b7lich", "d\u00fcrs\u00b7ten", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob oder arme Seelen", "tokens": ["Ob", "o\u00b7der", "ar\u00b7me", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man zu erfreuen denkt", "tokens": ["Man", "zu", "er\u00b7freu\u00b7en", "denkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die erschlafften Kehlen", "tokens": ["Und", "die", "er\u00b7schlaff\u00b7ten", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Geisenheimer tr\u00e4nkt?", "tokens": ["Mit", "Gei\u00b7sen\u00b7hei\u00b7mer", "tr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nicht \u2013 und es k\u00fcmmert", "tokens": ["Ich", "wei\u00df", "nicht", "\u2013", "und", "es", "k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich wenig auch; wenn gut", "tokens": ["Mich", "we\u00b7nig", "auch", ";", "wenn", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$.", "KOUS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur meine Flasche schimmert,", "tokens": ["Nur", "mei\u00b7ne", "Fla\u00b7sche", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da bin ich hochgemut.", "tokens": ["Da", "bin", "ich", "hoch\u00b7ge\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ist zum Paradeise", "tokens": ["Da", "ist", "zum", "Pa\u00b7ra\u00b7dei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir rings die Welt erbl\u00fcht,", "tokens": ["Mir", "rings", "die", "Welt", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sing ich leise, leise", "tokens": ["Da", "sing", "ich", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein alt verschollen Lied.", "tokens": ["Ein", "alt", "ver\u00b7schol\u00b7len", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}