{"dta.poem.21593": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Alte Zeiten \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Alte Zeiten sah der Erde Antlitz,", "tokens": ["Al\u00b7te", "Zei\u00b7ten", "sah", "der", "Er\u00b7de", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ungez\u00e4hlt durchlief des ewigen Tages", "tokens": ["Un\u00b7ge\u00b7z\u00e4hlt", "durch\u00b7lief", "des", "e\u00b7wi\u00b7gen", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Glanzesbahn das rollend Rad der Sonne \u2014", "tokens": ["Glan\u00b7zes\u00b7bahn", "das", "rol\u00b7lend", "Rad", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber dennoch", "tokens": ["A\u00b7ber", "den\u00b7noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Scheinet jung und frisch der Fr\u00fchlingsmorgen,", "tokens": ["Schei\u00b7net", "jung", "und", "frisch", "der", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann der Feind der N\u00e4chte, strahlgewappnet,", "tokens": ["Wann", "der", "Feind", "der", "N\u00e4ch\u00b7te", ",", "strahl\u00b7ge\u00b7wapp\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wirft des Lichtes Pfeil hin \u00fcber dunkler", "tokens": ["Wirft", "des", "Lich\u00b7tes", "Pfeil", "hin", "\u00fc\u00b7ber", "dunk\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wolken W\u00e4lle.", "tokens": ["Wol\u00b7ken", "W\u00e4l\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Denn das Herz, das menschlich reiche F\u00fchlen", "tokens": ["Denn", "das", "Herz", ",", "das", "menschlich", "rei\u00b7che", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Altert nie. Wie oft entz\u00fcckten Augen", "tokens": ["Al\u00b7tert", "nie", ".", "Wie", "oft", "ent\u00b7z\u00fcck\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "PWAV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch ge\u00f6ffnet sich der Rosengarten", "tokens": ["Auch", "ge\u00b7\u00f6ff\u00b7net", "sich", "der", "Ro\u00b7sen\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Erster Liebe,", "tokens": ["Ers\u00b7ter", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ewig bleibt sie jung die Lust der Liebe", "tokens": ["E\u00b7wig", "bleibt", "sie", "jung", "die", "Lust", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ewig jung des Mutterherzens Jubel,", "tokens": ["E\u00b7wig", "jung", "des", "Mut\u00b7ter\u00b7her\u00b7zens", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ewig jung der Schmerz am Grab des Vaters.", "tokens": ["E\u00b7wig", "jung", "der", "Schmerz", "am", "Grab", "des", "Va\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lieben, Leiden", "tokens": ["Lie\u00b7ben", ",", "Lei\u00b7den"], "token_info": ["word", "punct", "word"], "pos": ["ADJA", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ist des Menschen nieverge\u00dfnes Wollen,", "tokens": ["Ist", "des", "Men\u00b7schen", "nie\u00b7ver\u00b7ge\u00df\u00b7nes", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nimmer ehrt der Kampf mit solchem Zwange,", "tokens": ["Nim\u00b7mer", "ehrt", "der", "Kampf", "mit", "sol\u00b7chem", "Zwan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nimmer wird ein Mensch, wie sehr er strebt, den", "tokens": ["Nim\u00b7mer", "wird", "ein", "Mensch", ",", "wie", "sehr", "er", "strebt", ",", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$,", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kampf vollenden.", "tokens": ["Kampf", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}