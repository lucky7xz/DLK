{"textgrid.poem.54602": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "16. Auf das Hew in der Krippen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df Kindlein ligt auf Hew' es will dein Hertz zum k\u00fcssen/", "tokens": ["Di\u00df", "Kin\u00b7dlein", "ligt", "auf", "He\u00b7w'", "es", "will", "dein", "Hertz", "zum", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "NE", "PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das wie die Blum und Hew doch wird verschwinden m\u00fcssen.", "tokens": ["Das", "wie", "die", "Blum", "und", "Hew", "doch", "wird", "ver\u00b7schwin\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NE", "KON", "NE", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}