{"textgrid.poem.47704": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[komm, Geliebte, komm, Geliebte]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Geliebte, komm, Geliebte,", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7te", ",", "komm", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein zu mir in's Haus tritt!", "tokens": ["Ein", "zu", "mir", "in's", "Haus", "tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blicke meiner Stube Boden,", "tokens": ["Bli\u00b7cke", "mei\u00b7ner", "Stu\u00b7be", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blicke meinen Pf\u00fchl an!", "tokens": ["Bli\u00b7cke", "mei\u00b7nen", "Pf\u00fchl", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Deinetwegen ist der Boden,", "tokens": ["Dei\u00b7net\u00b7we\u00b7gen", "ist", "der", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Pf\u00fchl, worauf ich lagre,", "tokens": ["Ist", "der", "Pf\u00fchl", ",", "wo\u00b7rauf", "ich", "la\u00b7gre", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Thr\u00e4nen\u00fcberfluthet.", "tokens": ["Thr\u00e4\u00b7nen\u00b7\u00fc\u00b7berf\u00b7lut\u00b7het", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}