{"dta.poem.16163": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "217.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verstand ist zweierlei: der ein' ist angeboren,", "tokens": ["Ver\u00b7stand", "ist", "zwei\u00b7er\u00b7lei", ":", "der", "ein'", "ist", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$.", "ART", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Wiegeneingebind und Mahlschatz unverloren.", "tokens": ["Dein", "Wie\u00b7gen\u00b7ein\u00b7ge\u00b7bind", "und", "Mahl\u00b7schatz", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erst zu erwerben ist der andre, zu ersparen,", "tokens": ["Erst", "zu", "er\u00b7wer\u00b7ben", "ist", "der", "and\u00b7re", ",", "zu", "er\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "ART", "ADJA", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mit den Jahren w\u00e4chst durch Lernen und Erfahren.", "tokens": ["Der", "mit", "den", "Jah\u00b7ren", "w\u00e4chst", "durch", "Ler\u00b7nen", "und", "Er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der zwei Verst\u00e4nde kann ein Mann entbehren keinen,", "tokens": ["Der", "zwei", "Ver\u00b7st\u00e4n\u00b7de", "kann", "ein", "Mann", "ent\u00b7beh\u00b7ren", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VMFIN", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und erst ein ganzer wirds, wo beide sich vereinen.", "tokens": ["Und", "erst", "ein", "gan\u00b7zer", "wirds", ",", "wo", "bei\u00b7de", "sich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "VAFIN", "$,", "PWAV", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}