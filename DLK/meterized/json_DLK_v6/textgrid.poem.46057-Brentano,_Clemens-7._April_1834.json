{"textgrid.poem.46057": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "7. April 1834", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer Trost in hei\u00dfen Stunden,", "tokens": ["S\u00fc\u00b7\u00dfer", "Trost", "in", "hei\u00b7\u00dfen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die Liebste, die mir lebt,", "tokens": ["Da", "die", "Liebs\u00b7te", ",", "die", "mir", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Zitternd vor mir stand in Wunden", "tokens": ["Zit\u00b7ternd", "vor", "mir", "stand", "in", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und doch nicht vor mir erbebt.", "tokens": ["Und", "doch", "nicht", "vor", "mir", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sie mir mit hei\u00dfem Flehen,", "tokens": ["Da", "sie", "mir", "mit", "hei\u00b7\u00dfem", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dem\u00fctig sie umfieng,", "tokens": ["Der", "de\u00b7m\u00fc\u00b7tig", "sie", "um\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wahr ins bange Aug' gesehen,", "tokens": ["Wahr", "ins", "ban\u00b7ge", "Aug'", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mir's durch die Seele gieng.", "tokens": ["Da\u00df", "mir's", "durch", "die", "See\u00b7le", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich fleht', ach mir alleine", "tokens": ["Und", "ich", "fleht'", ",", "ach", "mir", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "XY", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rechne diese Glut nicht an,", "tokens": ["Rech\u00b7ne", "die\u00b7se", "Glut", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Flamme war die meine,", "tokens": ["Dei\u00b7ne", "Flam\u00b7me", "war", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide fa\u00dfte uns der Wahn.", "tokens": ["Bei\u00b7de", "fa\u00df\u00b7te", "uns", "der", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "S\u00fc\u00dfes Kind, in deinen Wunden", "tokens": ["S\u00fc\u00b7\u00dfes", "Kind", ",", "in", "dei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du so unendlich sch\u00f6n,", "tokens": ["Bist", "du", "so", "un\u00b7end\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Schmerz mu\u00df da gesunden", "tokens": ["All", "mein", "Schmerz", "mu\u00df", "da", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in sel'gem Wiedersehn.", "tokens": ["Wie", "in", "sel'\u00b7gem", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alles, was da je geschieden,", "tokens": ["Al\u00b7les", ",", "was", "da", "je", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig innig sich erkennt,", "tokens": ["E\u00b7wig", "in\u00b7nig", "sich", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein wonnetrunkner Frieden", "tokens": ["Und", "ein", "won\u00b7ne\u00b7trunk\u00b7ner", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir im Herzen jauchzt und brennt.", "tokens": ["Mir", "im", "Her\u00b7zen", "jauchzt", "und", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schlie\u00dfest Lippen du und Augen,", "tokens": ["Schlie\u00b7\u00dfest", "Lip\u00b7pen", "du", "und", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird ein Feuerblick dein Leib,", "tokens": ["Wird", "ein", "Feu\u00b7er\u00b7blick", "dein", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dein Mund ein Feuerhauchen,", "tokens": ["Wird", "dein", "Mund", "ein", "Feu\u00b7er\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirst du sch\u00f6ner als ein Weib.", "tokens": ["Wirst", "du", "sch\u00f6\u00b7ner", "als", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hungern kann ich, harren, missen,", "tokens": ["Hun\u00b7gern", "kann", "ich", ",", "har\u00b7ren", ",", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch, dich hingegeben sehn,", "tokens": ["Doch", ",", "dich", "hin\u00b7ge\u00b7ge\u00b7ben", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bedecken nicht mit K\u00fcssen", "tokens": ["Und", "be\u00b7de\u00b7cken", "nicht", "mit", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fc\u00dft' ich kalt im Feuer stehn.", "tokens": ["M\u00fc\u00dft'", "ich", "kalt", "im", "Feu\u00b7er", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Du bist wahr, wie nie im Leben", "tokens": ["Du", "bist", "wahr", ",", "wie", "nie", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrheit mir entgegentrat,", "tokens": ["Wahr\u00b7heit", "mir", "ent\u00b7ge\u00b7gent\u00b7rat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so wirst du mir vergeben,", "tokens": ["Und", "so", "wirst", "du", "mir", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dir selbst entgegentrat.", "tokens": ["Was", "dir", "selbst", "ent\u00b7ge\u00b7gent\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Soll ich arm mein Elend bauen,", "tokens": ["Soll", "ich", "arm", "mein", "E\u00b7lend", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann hab' Mitleid und gib mehr,", "tokens": ["Dann", "hab'", "Mit\u00b7leid", "und", "gib", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "VVIMP", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gib mir kindliches Vertrauen", "tokens": ["Gib", "mir", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann wird alles leicht, was schwer.", "tokens": ["Dann", "wird", "al\u00b7les", "leicht", ",", "was", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aus der Ferne schon gib Winke,", "tokens": ["Aus", "der", "Fer\u00b7ne", "schon", "gib", "Win\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVIMP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt das Herz in deiner Brust,", "tokens": ["Mahnt", "das", "Herz", "in", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich trinkend nicht ertrinke", "tokens": ["Da\u00df", "ich", "trin\u00b7kend", "nicht", "er\u00b7trin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib mir Innigkeit statt Lust.", "tokens": ["Gib", "mir", "In\u00b7nig\u00b7keit", "statt", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Kind, wie auch der Blitz der Wonne", "tokens": ["Kind", ",", "wie", "auch", "der", "Blitz", "der", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich an deiner Brust durchz\u00fcckt,", "tokens": ["Mich", "an", "dei\u00b7ner", "Brust", "durch\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schrei' ich doch nach einer Sonne,", "tokens": ["Schrei'", "ich", "doch", "nach", "ei\u00b7ner", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dein Blick mir hart entr\u00fcckt.", "tokens": ["Die", "dein", "Blick", "mir", "hart", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und ich fleh' zum bl\u00fchnden Munde:", "tokens": ["Und", "ich", "fleh'", "zum", "bl\u00fchn\u00b7den", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprich doch:", "tokens": ["Sprich", "doch", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u00bbarmer, schone mein!", "tokens": ["\u00bb", "ar\u00b7mer", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Soll sie heilen deine Wunde,", "tokens": ["Soll", "sie", "hei\u00b7len", "dei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Halte meine Hand auch rein!", "tokens": ["Hal\u00b7te", "mei\u00b7ne", "Hand", "auch", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "G\u00fctig will ich zu dir blicken,", "tokens": ["G\u00fc\u00b7tig", "will", "ich", "zu", "dir", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will dich tragen in Geduld,", "tokens": ["Will", "dich", "tra\u00b7gen", "in", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dir freundlich kindlich nicken,", "tokens": ["Will", "dir", "freund\u00b7lich", "kind\u00b7lich", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchlen dich mit meiner Huld!", "tokens": ["K\u00fch\u00b7len", "dich", "mit", "mei\u00b7ner", "Huld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ach! vielmehr noch will ich geben,", "tokens": ["Ach", "!", "viel\u00b7mehr", "noch", "will", "ich", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Armes mir verfallnes Herz!", "tokens": ["Ar\u00b7mes", "mir", "ver\u00b7fall\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich soll dich rein durchbeben", "tokens": ["T\u00e4g\u00b7lich", "soll", "dich", "rein", "durch\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Freude und mein Schmerz!", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "und", "mein", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00df uns sein doch wie die Kinder,", "tokens": ["La\u00df", "uns", "sein", "doch", "wie", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00fctig, heiter, s\u00fc\u00df und rein,", "tokens": ["G\u00fc\u00b7tig", ",", "hei\u00b7ter", ",", "s\u00fc\u00df", "und", "rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann will dir ich immer linder", "tokens": ["Dann", "will", "dir", "ich", "im\u00b7mer", "lin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine arme Linder sein.", "tokens": ["Dei\u00b7ne", "ar\u00b7me", "Lin\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Willst du still in meinem Garten", "tokens": ["Willst", "du", "still", "in", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen dir und Heilkraut baun,", "tokens": ["Blu\u00b7men", "dir", "und", "Heil\u00b7kraut", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dft du auch der Beete warten,", "tokens": ["Mu\u00dft", "du", "auch", "der", "Bee\u00b7te", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brechen nie den schwachen Zaun.", "tokens": ["Bre\u00b7chen", "nie", "den", "schwa\u00b7chen", "Zaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Willst du Rosenpfade bahnen,", "tokens": ["Willst", "du", "Ro\u00b7sen\u00b7pfa\u00b7de", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So verblute nicht im Dorn,", "tokens": ["So", "ver\u00b7blu\u00b7te", "nicht", "im", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die lockenden Zyanen", "tokens": ["Um", "die", "lo\u00b7cken\u00b7den", "Zy\u00b7a\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt mit F\u00fc\u00dfen nicht dein Korn.", "tokens": ["Tritt", "mit", "F\u00fc\u00b7\u00dfen", "nicht", "dein", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Sei fein still, in mir gefangen", "tokens": ["Sei", "fein", "still", ",", "in", "mir", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PTKVZ", "$,", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stirbst du nicht den Hungertod", "tokens": ["Stirbst", "du", "nicht", "den", "Hun\u00b7ger\u00b7tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin lind, du sollst empfangen,", "tokens": ["Ich", "bin", "lind", ",", "du", "sollst", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Blumen, reines Brot.", "tokens": ["S\u00fc\u00b7\u00dfe", "Blu\u00b7men", ",", "rei\u00b7nes", "Brot", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "H\u00fcte mir, ich will dir h\u00fcten,", "tokens": ["H\u00fc\u00b7te", "mir", ",", "ich", "will", "dir", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses feuertrunkne Blut", "tokens": ["Die\u00b7ses", "feu\u00b7er\u00b7trunk\u00b7ne", "Blut"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bittre Frucht nach s\u00fc\u00dfen Bl\u00fcten", "tokens": ["Bitt\u00b7re", "Frucht", "nach", "s\u00fc\u00b7\u00dfen", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4chst auf ungerechtem Gut.", "tokens": ["W\u00e4chst", "auf", "un\u00b7ge\u00b7rech\u00b7tem", "Gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Glaube fest doch an mein Wissen", "tokens": ["Glau\u00b7be", "fest", "doch", "an", "mein", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ich glaube, da\u00df du wei\u00dft", "tokens": ["Auch", "ich", "glau\u00b7be", ",", "da\u00df", "du", "wei\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dich meine Reu' zerrissen,", "tokens": ["Da\u00df", "dich", "mei\u00b7ne", "Reu'", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Reue mich zerrei\u00dft.", "tokens": ["Dei\u00b7ne", "Reu\u00b7e", "mich", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Wie soll ich dein Herz je nehmen,", "tokens": ["Wie", "soll", "ich", "dein", "Herz", "je", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das du mir so flehend bringst,", "tokens": ["Das", "du", "mir", "so", "fle\u00b7hend", "bringst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die H\u00e4nde mit Besch\u00e4men", "tokens": ["Da", "die", "H\u00e4n\u00b7de", "mit", "Be\u00b7sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du mir vor die Augen zwingst.", "tokens": ["Du", "mir", "vor", "die", "Au\u00b7gen", "zwingst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Willst du je mein Herz umfassen", "tokens": ["Willst", "du", "je", "mein", "Herz", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Liebe Bl\u00fctenreis,", "tokens": ["Mit", "der", "Lie\u00b7be", "Bl\u00fc\u00b7ten\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du nie es fallen lassen", "tokens": ["Willst", "du", "nie", "es", "fal\u00b7len", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mach es nicht so gl\u00fchend hei\u00df.", "tokens": ["Mach", "es", "nicht", "so", "gl\u00fc\u00b7hend", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Armer Freund, ach meine Gluten,", "tokens": ["Ar\u00b7mer", "Freund", ",", "ach", "mei\u00b7ne", "Glu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "XY", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nicht deines Herzens Glut,", "tokens": ["Sind", "nicht", "dei\u00b7nes", "Her\u00b7zens", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese s\u00fc\u00df entflammten Fluten", "tokens": ["Die\u00b7se", "s\u00fc\u00df", "ent\u00b7flamm\u00b7ten", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind mein leicht entz\u00fcndlich Blut.", "tokens": ["Sind", "mein", "leicht", "ent\u00b7z\u00fcnd\u00b7lich", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "H\u00fct' mein Feuer, h\u00fct' die Flammen,", "tokens": ["H\u00fct'", "mein", "Feu\u00b7er", ",", "h\u00fct'", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn dies freie Element", "tokens": ["Denn", "dies", "frei\u00b7e", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmilzt dir nimmermehr zusammen", "tokens": ["Schmilzt", "dir", "nim\u00b7mer\u00b7mehr", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was zur Asche es verbrennt.", "tokens": ["Was", "zur", "A\u00b7sche", "es", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "W\u00e4rme dich in meiner Sonne", "tokens": ["W\u00e4r\u00b7me", "dich", "in", "mei\u00b7ner", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchle dich in meinem Mond,", "tokens": ["K\u00fch\u00b7le", "dich", "in", "mei\u00b7nem", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinke meiner Sterne Wonne", "tokens": ["Trin\u00b7ke", "mei\u00b7ner", "Ster\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der auf meiner Erde wohnt.", "tokens": ["Der", "auf", "mei\u00b7ner", "Er\u00b7de", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Alle Blumen, s\u00fc\u00df und reine", "tokens": ["Al\u00b7le", "Blu\u00b7men", ",", "s\u00fc\u00df", "und", "rei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich treibe auf zum Licht", "tokens": ["Die", "ich", "trei\u00b7be", "auf", "zum", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4nk' mit Tr\u00e4nen, bis auf eine", "tokens": [