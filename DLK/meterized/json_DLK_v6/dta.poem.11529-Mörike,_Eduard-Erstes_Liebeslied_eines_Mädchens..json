{"dta.poem.11529": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Liebeslied eines M\u00e4dchens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Was im Netze? Schau einmal!", "tokens": ["Was", "im", "Net\u00b7ze", "?", "Schau", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "$.", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ich bin bange;", "tokens": ["A\u00b7ber", "ich", "bin", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Greif' ich einen s\u00fc\u00dfen Aal?", "tokens": ["Greif'", "ich", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Aal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Greif' ich eine Schlange?", "tokens": ["Greif'", "ich", "ei\u00b7ne", "Schlan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lieb' ist blinde", "tokens": ["Lieb'", "ist", "blin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fischerin;", "tokens": ["Fi\u00b7sche\u00b7rin", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sagt dem Kinde,", "tokens": ["Sagt", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wo greift's hin?", "tokens": ["Wo", "greift's", "hin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PTKVZ", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Schon schnellt mir's in H\u00e4nden!", "tokens": ["Schon", "schnellt", "mir's", "in", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ach Jammer! o Lust!", "tokens": ["Ach", "Jam\u00b7mer", "!", "o", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "FM", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit Schmiegen und Wenden", "tokens": ["Mit", "Schmie\u00b7gen", "und", "Wen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir schl\u00fcpft's an die Brust.", "tokens": ["Mir", "schl\u00fcpft's", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Es bei\u00dft sich, o Wunder!", "tokens": ["Es", "bei\u00dft", "sich", ",", "o", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mir keck durch die Haut,", "tokens": ["Mir", "keck", "durch", "die", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schie\u00dft's Herze hinunter,", "tokens": ["Schie\u00dft's", "Her\u00b7ze", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O Liebe! mir graut!", "tokens": ["O", "Lie\u00b7be", "!", "mir", "graut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Was thun, was beginnen?", "tokens": ["Was", "thun", ",", "was", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$,", "PWS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das schaurige Ding,", "tokens": ["Das", "schau\u00b7ri\u00b7ge", "Ding", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es schnalzet da drinnen,", "tokens": ["Es", "schnal\u00b7zet", "da", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es legt sich im Ring.", "tokens": ["Es", "legt", "sich", "im", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Gift mu\u00df ich haben!", "tokens": ["Gift", "mu\u00df", "ich", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hier schleicht es herum,", "tokens": ["Hier", "schleicht", "es", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Thut wonniglich graben", "tokens": ["Thut", "won\u00b7nig\u00b7lich", "gra\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und bringt mich noch um!", "tokens": ["Und", "bringt", "mich", "noch", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}