{"textgrid.poem.34523": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da nun du schlummerst, tasten Traumgestalten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da nun du schlummerst, tasten Traumgestalten", "tokens": ["Da", "nun", "du", "schlum\u00b7merst", ",", "tas\u00b7ten", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an deinen Schlaf: dass du die H\u00e4nde regen,", "tokens": ["an", "dei\u00b7nen", "Schlaf", ":", "dass", "du", "die", "H\u00e4n\u00b7de", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KOUS", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "unwillig deine Lippen musst bewegen", "tokens": ["un\u00b7wil\u00b7lig", "dei\u00b7ne", "Lip\u00b7pen", "musst", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und weiter ringen mit des Tags Gewalten.", "tokens": ["und", "wei\u00b7ter", "rin\u00b7gen", "mit", "des", "Tags", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst wirst du ruh in schutzesstarken, kalten", "tokens": ["Einst", "wirst", "du", "ruh", "in", "schut\u00b7zes\u00b7star\u00b7ken", ",", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "VVINF", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Armen der Nacht: traumlos wird sich ihr Segen", "tokens": ["Ar\u00b7men", "der", "Nacht", ":", "traum\u00b7los", "wird", "sich", "ihr", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ADJD", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "auf deine wunde, welke Stirne legen \u2013", "tokens": ["auf", "dei\u00b7ne", "wun\u00b7de", ",", "wel\u00b7ke", "Stir\u00b7ne", "le\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PWAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und starr und glatt sind deines Kleides Falten.", "tokens": ["und", "starr", "und", "glatt", "sind", "dei\u00b7nes", "Klei\u00b7des", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn sich, gequ\u00e4lt, in seiner Tr\u00e4ume Kreise", "tokens": ["Wenn", "sich", ",", "ge\u00b7qu\u00e4lt", ",", "in", "sei\u00b7ner", "Tr\u00e4u\u00b7me", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "$,", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Schl\u00e4fer windet, l\u00e4chelst du so weise:", "tokens": ["der", "Schl\u00e4\u00b7fer", "win\u00b7det", ",", "l\u00e4\u00b7chelst", "du", "so", "wei\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es kommt der Tag und l\u00e4sst sich nicht betr\u00fcgen.", "tokens": ["es", "kommt", "der", "Tag", "und", "l\u00e4sst", "sich", "nicht", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O lerne l\u00e4cheln auch ob dieser Zeiten,", "tokens": ["O", "ler\u00b7ne", "l\u00e4\u00b7cheln", "auch", "ob", "die\u00b7ser", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "ADV", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ob dieses Tages dreisten Wirklichkeiten \u2013", "tokens": ["ob", "die\u00b7ses", "Ta\u00b7ges", "dreis\u00b7ten", "Wirk\u00b7lich\u00b7kei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es kommt die Nacht und straft sie alle L\u00fcgen!", "tokens": ["es", "kommt", "die", "Nacht", "und", "straft", "sie", "al\u00b7le", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}