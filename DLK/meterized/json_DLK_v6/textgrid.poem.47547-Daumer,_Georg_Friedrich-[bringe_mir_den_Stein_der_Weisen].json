{"textgrid.poem.47547": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[bringe mir den Stein der Weisen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringe mir den Stein der Weisen,", "tokens": ["Brin\u00b7ge", "mir", "den", "Stein", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringe mir den Becher Dschemschid's,", "tokens": ["Brin\u00b7ge", "mir", "den", "Be\u00b7cher", "Dschem\u00b7schi\u00b7d's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mir den Spiegel Alexander's", "tokens": ["Mir", "den", "Spie\u00b7gel", "A\u00b7lex\u00b7an\u00b7der's"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und das Siegel Salomonis,", "tokens": ["Und", "das", "Sie\u00b7gel", "Sa\u00b7lo\u00b7mo\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Bringe mir mit ", "tokens": ["Brin\u00b7ge", "mir", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bring', o Schenke, bringe Wein! \u2013", "tokens": ["Bring'", ",", "o", "Schen\u00b7ke", ",", "brin\u00b7ge", "Wein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wein, da\u00df ich die Kutte wasche,", "tokens": ["Wein", ",", "da\u00df", "ich", "die", "Kut\u00b7te", "wa\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die befleckte von des Hochmuths", "tokens": ["Die", "be\u00b7fleck\u00b7te", "von", "des", "Hoch\u00b7muths"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Hasses schwarzem Makel,", "tokens": ["Und", "des", "Has\u00b7ses", "schwar\u00b7zem", "Ma\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wein, da\u00df ich das Garn des Unsinns,", "tokens": ["Wein", ",", "da\u00df", "ich", "das", "Garn", "des", "Un\u00b7sinns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches \u00fcber Welt und Leben", "tokens": ["Wel\u00b7ches", "\u00fc\u00b7ber", "Welt", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pf\u00e4ffischer Betrug gebreitet,", "tokens": ["Pf\u00e4f\u00b7fi\u00b7scher", "Be\u00b7trug", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit gest\u00e4rktem Arm zerrei\u00dfe,", "tokens": ["Mit", "ge\u00b7st\u00e4rk\u00b7tem", "Arm", "zer\u00b7rei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wein, da\u00df ich die Welt erobre,", "tokens": ["Wein", ",", "da\u00df", "ich", "die", "Welt", "e\u00b7rob\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wein, da\u00df ich den Himmel st\u00fcrme,", "tokens": ["Wein", ",", "da\u00df", "ich", "den", "Him\u00b7mel", "st\u00fcr\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wein, da\u00df ich mit ", "tokens": ["Wein", ",", "da\u00df", "ich", "mit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ueber beide Welten setze,", "tokens": ["Ue\u00b7ber", "bei\u00b7de", "Wel\u00b7ten", "set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bring', o Schenke, bringe Wein!", "tokens": ["Bring'", ",", "o", "Schen\u00b7ke", ",", "brin\u00b7ge", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}