{"dta.poem.21290": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiI .  \n  Die Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst du jene Rose bl\u00fchen,", "tokens": ["Siehst", "du", "je\u00b7ne", "Ro\u00b7se", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nste! so erkenne dich:", "tokens": ["Sch\u00f6ns\u00b7te", "!", "so", "er\u00b7ken\u00b7ne", "dich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du Bienen zu ihr fliehen,", "tokens": ["Siehst", "du", "Bie\u00b7nen", "zu", "ihr", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Phyllis! so gedenk an mich.", "tokens": ["Phyl\u00b7lis", "!", "so", "ge\u00b7denk", "an", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Deine Bl\u00fchte lockt die Triebe", "tokens": ["Dei\u00b7ne", "Bl\u00fch\u00b7te", "lockt", "die", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Reichthum der Natur,", "tokens": ["Auf", "den", "Reicht\u00b7hum", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Jugend s\u00fcsse Liebe", "tokens": ["Und", "der", "Ju\u00b7gend", "s\u00fcs\u00b7se", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Raubt dir nichts, und n\u00e4hrt sich nur.", "tokens": ["Raubt", "dir", "nichts", ",", "und", "n\u00e4hrt", "sich", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KON", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}