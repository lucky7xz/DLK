{"textgrid.poem.41495": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Chloris", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jenem zarten Alter,", "tokens": ["In", "je\u00b7nem", "zar\u00b7ten", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich mit meinem Sch\u00e4fchen", "tokens": ["Als", "ich", "mit", "mei\u00b7nem", "Sch\u00e4f\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich noch zu messen pflegte", "tokens": ["Mich", "noch", "zu", "mes\u00b7sen", "pfleg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00e4lter war, doch kleiner,", "tokens": ["Und", "\u00e4l\u00b7ter", "war", ",", "doch", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als mein getreues Sch\u00e4fchen,", "tokens": ["Als", "mein", "ge\u00b7treu\u00b7es", "Sch\u00e4f\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da folgt ich schon der Chloris,", "tokens": ["Da", "folgt", "ich", "schon", "der", "Chlo\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wie mir mein treues Sch\u00e4fchen.", "tokens": ["Wie", "mir", "mein", "treu\u00b7es", "Sch\u00e4f\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch schon in jenen Zeiten", "tokens": ["Auch", "schon", "in", "je\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "War sie in meinen Augen", "tokens": ["War", "sie", "in", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mehr als ein sterblich M\u00e4dchen,", "tokens": ["Mehr", "als", "ein", "sterb\u00b7lich", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und ist noch eine G\u00f6ttin,", "tokens": ["Und", "ist", "noch", "ei\u00b7ne", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und mir die sch\u00f6nste G\u00f6ttin,", "tokens": ["Und", "mir", "die", "sch\u00f6ns\u00b7te", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die jemals sichtbar worden.", "tokens": ["Die", "je\u00b7mals", "sicht\u00b7bar", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Einst sagt' ich ihr: ich liebe;", "tokens": ["Einst", "sagt'", "ich", "ihr", ":", "ich", "lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ich liebe dich, o Chloris.", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "o", "Chlo\u00b7ris", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "FM", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Dies war des Herzens Sprache,", "tokens": ["Dies", "war", "des", "Her\u00b7zens", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Dies sagten meine Seufzer;", "tokens": ["Dies", "sag\u00b7ten", "mei\u00b7ne", "Seuf\u00b7zer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die kindisch bl\u00f6de Zunge", "tokens": ["Die", "kin\u00b7disch", "bl\u00f6\u00b7de", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Lie\u00df Herz und Seufzer reden", "tokens": ["Lie\u00df", "Herz", "und", "Seuf\u00b7zer", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und fand sich keine Worte.", "tokens": ["Und", "fand", "sich", "kei\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Doch mich verstand die Sch\u00f6ne", "tokens": ["Doch", "mich", "ver\u00b7stand", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und schenkte mir ein M\u00e4ulchen,", "tokens": ["Und", "schenk\u00b7te", "mir", "ein", "M\u00e4ul\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ein unverge\u00dflich M\u00e4ulchen.", "tokens": ["Ein", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "M\u00e4ul\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und sprach zu mir: Du Kleiner,", "tokens": ["Und", "sprach", "zu", "mir", ":", "Du", "Klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Du kennst noch nicht die Liebe.", "tokens": ["Du", "kennst", "noch", "nicht", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Seitdem entbrannte Chloris,", "tokens": ["Seit\u00b7dem", "ent\u00b7brann\u00b7te", "Chlo\u00b7ris", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Jedoch f\u00fcr andre Sch\u00e4fer.", "tokens": ["Je\u00b7doch", "f\u00fcr", "and\u00b7re", "Sch\u00e4\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Seitdem fing mancher Sch\u00e4fer", "tokens": ["Seit\u00b7dem", "fing", "man\u00b7cher", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.29": {"text": "Aus Chloris Augen Feuer.", "tokens": ["Aus", "Chlo\u00b7ris", "Au\u00b7gen", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Seitdem kam ich ins Alter,", "tokens": ["Seit\u00b7dem", "kam", "ich", "ins", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "In dem wir Menschen lieben,", "tokens": ["In", "dem", "wir", "Men\u00b7schen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Wie unsre V\u00e4ter liebten.", "tokens": ["Wie", "uns\u00b7re", "V\u00e4\u00b7ter", "lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Es reiften meine Jahre,", "tokens": ["Es", "reif\u00b7ten", "mei\u00b7ne", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Es gab mir jeder Fr\u00fchling", "tokens": ["Es", "gab", "mir", "je\u00b7der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Mehr Z\u00e4rtlichkeit und W\u00fcnsche.", "tokens": ["Mehr", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "W\u00fcn\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch jetzt verehr' ich Chloris;", "tokens": ["Noch", "jetzt", "ver\u00b7ehr'", "ich", "Chlo\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mir aber ist sie spr\u00f6de", "tokens": ["Mir", "a\u00b7ber", "ist", "sie", "spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcnscht nicht zu erfahren,", "tokens": ["Und", "w\u00fcnscht", "nicht", "zu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob ich die Liebe kenne;", "tokens": ["Ob", "ich", "die", "Lie\u00b7be", "ken\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jener s\u00fc\u00dfen Stunde", "tokens": ["Und", "je\u00b7ner", "s\u00fc\u00b7\u00dfen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ihres kleinen Sch\u00e4fers", "tokens": ["Und", "ih\u00b7res", "klei\u00b7nen", "Sch\u00e4\u00b7fers"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ihres holden Kusses", "tokens": ["Und", "ih\u00b7res", "hol\u00b7den", "Kus\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergi\u00dft die stolze Sch\u00f6ne.", "tokens": ["Ver\u00b7gi\u00dft", "die", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nur ich kann ihrer Lippen,", "tokens": ["Nur", "ich", "kann", "ih\u00b7rer", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.10": {"text": "Die sie mir l\u00e4chelnd reichte,", "tokens": ["Die", "sie", "mir", "l\u00e4\u00b7chelnd", "reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nur ich kann ihres Kusses", "tokens": ["Nur", "ich", "kann", "ih\u00b7res", "Kus\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und ihrer nicht vergessen.", "tokens": ["Und", "ih\u00b7rer", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}