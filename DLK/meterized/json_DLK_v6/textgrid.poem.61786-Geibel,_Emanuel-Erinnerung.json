{"textgrid.poem.61786": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4t abends wohl, wenn drau\u00dfen sturmdurchschauert", "tokens": ["Sp\u00e4t", "a\u00b7bends", "wohl", ",", "wenn", "drau\u00b7\u00dfen", "sturm\u00b7durch\u00b7schau\u00b7ert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "ADV", "$,", "KOUS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Nacht und Schnee die \u00f6de Gasse trauert,", "tokens": ["In", "Nacht", "und", "Schnee", "die", "\u00f6\u00b7de", "Gas\u00b7se", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und um den Turm das Volk der Kr\u00e4hen l\u00e4rmt,", "tokens": ["Und", "um", "den", "Turm", "das", "Volk", "der", "Kr\u00e4\u00b7hen", "l\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00e4gt mich ein Traum zu jenen Fr\u00fchherbsttagen,", "tokens": ["Tr\u00e4gt", "mich", "ein", "Traum", "zu", "je\u00b7nen", "Fr\u00fch\u00b7herbst\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die ich mit jugendseligem Behagen", "tokens": ["Die", "ich", "mit", "ju\u00b7gend\u00b7se\u00b7li\u00b7gem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Attikas Gebirg' verschw\u00e4rmt.", "tokens": ["In", "At\u00b7ti\u00b7kas", "Ge\u00b7bir\u00b7g'", "ver\u00b7schw\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Da scheint des Alters tr\u00fcber Bann gebrochen,", "tokens": ["Da", "scheint", "des", "Al\u00b7ters", "tr\u00fc\u00b7ber", "Bann", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Blut hebt leicht und fr\u00f6hlich an zu pochen,", "tokens": ["Mein", "Blut", "hebt", "leicht", "und", "fr\u00f6h\u00b7lich", "an", "zu", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe wieder zweiundzwanzig Jahr'.", "tokens": ["Ich", "ha\u00b7be", "wie\u00b7der", "zwei\u00b7und\u00b7zwan\u00b7zig", "Jahr'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein sanfter Lichtstrom rieselt um mich nieder,", "tokens": ["Ein", "sanf\u00b7ter", "Licht\u00b7strom", "rie\u00b7selt", "um", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und trunknen Auges gr\u00fc\u00df' ich alles wieder,", "tokens": ["Und", "trunk\u00b7nen", "Au\u00b7ges", "gr\u00fc\u00df'", "ich", "al\u00b7les", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was damals mein Entz\u00fccken war:", "tokens": ["Was", "da\u00b7mals", "mein", "Ent\u00b7z\u00fc\u00b7cken", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das tiefe Blau durchrauscht vom Flug der Tauben,", "tokens": ["Das", "tie\u00b7fe", "Blau", "durc\u00b7hrauscht", "vom", "Flug", "der", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die luft'ge Villa, die aus Myrtenlauben", "tokens": ["Die", "luft'\u00b7ge", "Vil\u00b7la", ",", "die", "aus", "Myr\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Hang Pentelis nach dem Meere sah,", "tokens": ["Vom", "Hang", "Pen\u00b7te\u00b7lis", "nach", "dem", "Mee\u00b7re", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Pinienschlucht getaucht in Abendgluten", "tokens": ["Die", "Pi\u00b7ni\u00b7en\u00b7schlucht", "ge\u00b7taucht", "in", "A\u00b7bend\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und jene Grotte mit den Silberfluten", "tokens": ["Und", "je\u00b7ne", "Grot\u00b7te", "mit", "den", "Sil\u00b7ber\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im \u00d6lwald von Kephissia.", "tokens": ["Im", "\u00d6l\u00b7wald", "von", "Ke\u00b7phis\u00b7sia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da kommst auch du blau\u00e4ugig Kind, Agathe,", "tokens": ["Da", "kommst", "auch", "du", "blau\u00b7\u00e4u\u00b7gig", "Kind", ",", "A\u00b7gat\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADJD", "NN", "$,", "NE", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Im schwarzen Haar die Bl\u00fcte der Granate,", "tokens": ["Im", "schwar\u00b7zen", "Haar", "die", "Bl\u00fc\u00b7te", "der", "Gra\u00b7na\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herab den Felspfad, in der Hand den Krug;", "tokens": ["Her\u00b7ab", "den", "Fel\u00b7spfad", ",", "in", "der", "Hand", "den", "Krug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wandelt' Hebe wohl im G\u00f6tterreigen,", "tokens": ["So", "wan\u00b7delt'", "He\u00b7be", "wohl", "im", "G\u00f6t\u00b7ter\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch unbewu\u00dft des Zaubers, der dein eigen,", "tokens": ["Doch", "un\u00b7be\u00b7wu\u00dft", "des", "Zau\u00b7bers", ",", "der", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schwebst gr\u00fc\u00dfend du vorbei im Flug.", "tokens": ["Schwebst", "gr\u00fc\u00b7\u00dfend", "du", "vor\u00b7bei", "im", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}