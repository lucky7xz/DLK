{"textgrid.poem.33600": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meine Heimat kam ich wieder,", "tokens": ["In", "mei\u00b7ne", "Hei\u00b7mat", "kam", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war die alte Heimat noch,", "tokens": ["Es", "war", "die", "al\u00b7te", "Hei\u00b7mat", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dieselbe Luft, dieselben Lieder,", "tokens": ["Die\u00b7sel\u00b7be", "Luft", ",", "die\u00b7sel\u00b7ben", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles war ein andres doch.", "tokens": ["Und", "al\u00b7les", "war", "ein", "and\u00b7res", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Welle rauschte wie vorzeiten,", "tokens": ["Die", "Wel\u00b7le", "rauschte", "wie", "vor\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Waldweg sprang wie sonst das Reh,", "tokens": ["Am", "Wald\u00b7weg", "sprang", "wie", "sonst", "das", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von fern erklang ein Abendl\u00e4uten,", "tokens": ["Von", "fern", "er\u00b7klang", "ein", "A\u00b7bend\u00b7l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Berge gl\u00e4nzten aus dem See.", "tokens": ["Die", "Ber\u00b7ge", "gl\u00e4nz\u00b7ten", "aus", "dem", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch vor dem Haus, wo uns vor Jahren", "tokens": ["Doch", "vor", "dem", "Haus", ",", "wo", "uns", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter stets empfing, dort sah", "tokens": ["Die", "Mut\u00b7ter", "stets", "emp\u00b7fing", ",", "dort", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fremde Menschen fremd gebaren;", "tokens": ["Ich", "frem\u00b7de", "Men\u00b7schen", "fremd", "ge\u00b7ba\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weh, wie weh mir da geschah!", "tokens": ["Wie", "weh", ",", "wie", "weh", "mir", "da", "ge\u00b7schah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir war, als rief' es aus den Wogen:", "tokens": ["Mir", "war", ",", "als", "rie\u00b7f'", "es", "aus", "den", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Flieh, flieh, und ohne Wiederkehr!", "tokens": ["Flieh", ",", "flieh", ",", "und", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die du geliebt, sind fortgezogen,", "tokens": ["Die", "du", "ge\u00b7liebt", ",", "sind", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehren nimmer, nimmermehr.", "tokens": ["Und", "keh\u00b7ren", "nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}