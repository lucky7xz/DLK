{"textgrid.poem.39710": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein schmahler rauher Pfad schien sonst die Erde.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schmahler rauher Pfad schien sonst die Erde.", "tokens": ["Ein", "schmah\u00b7ler", "rau\u00b7her", "Pfad", "schien", "sonst", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf den Bergen gl\u00e4nzt der Himmel \u00fcber ihr,", "tokens": ["Und", "auf", "den", "Ber\u00b7gen", "gl\u00e4nzt", "der", "Him\u00b7mel", "\u00fc\u00b7ber", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Abgrund ihr zur Seite war die H\u00f6lle,", "tokens": ["Ein", "Ab\u00b7grund", "ihr", "zur", "Sei\u00b7te", "war", "die", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Pfade f\u00fchrten in den Himmel und zur H\u00f6lle.", "tokens": ["Und", "Pfa\u00b7de", "f\u00fchr\u00b7ten", "in", "den", "Him\u00b7mel", "und", "zur", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Himmel ist gest\u00fcrzt, der Abgrund ausgef\u00fcllt,", "tokens": ["Der", "Him\u00b7mel", "ist", "ge\u00b7st\u00fcrzt", ",", "der", "Ab\u00b7grund", "aus\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Vernunft bedeckt, und sehr bequem zum gehen.", "tokens": ["Und", "mit", "Ver\u00b7nunft", "be\u00b7deckt", ",", "und", "sehr", "be\u00b7quem", "zum", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,", "KON", "ADV", "ADJD", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Glaubens H\u00f6hen sind nun demolieret.", "tokens": ["Des", "Glau\u00b7bens", "H\u00f6\u00b7hen", "sind", "nun", "de\u00b7mo\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und auf der flachen Erde schreitet der Verstand,", "tokens": ["Und", "auf", "der", "fla\u00b7chen", "Er\u00b7de", "schrei\u00b7tet", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und misset alles aus, nach Klafter und nach Schuen.", "tokens": ["Und", "mis\u00b7set", "al\u00b7les", "aus", ",", "nach", "Klaf\u00b7ter", "und", "nach", "Schu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}