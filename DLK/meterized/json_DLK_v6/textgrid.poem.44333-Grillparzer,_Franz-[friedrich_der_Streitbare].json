{"textgrid.poem.44333": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[friedrich der Streitbare]", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Herzog war in \u00d6sterreich,", "tokens": ["Ein", "Her\u00b7zog", "war", "in", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Friederich genannt,", "tokens": ["Herr", "Frie\u00b7de\u00b7rich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem tat so leicht es keiner gleich", "tokens": ["Dem", "tat", "so", "leicht", "es", "kei\u00b7ner", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im ganzen deutschen Land.", "tokens": ["Im", "gan\u00b7zen", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn er erschien, da ward ringsum", "tokens": ["Wenn", "er", "er\u00b7schien", ",", "da", "ward", "ring\u00b7sum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ADV"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Soweit sein Fu\u00dftritt klang", "tokens": ["So\u00b7weit", "sein", "Fu\u00df\u00b7tritt", "klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit eins der lautste Prahler stumm", "tokens": ["Mit", "eins", "der", "lauts\u00b7te", "Prah\u00b7ler", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch der K\u00fchnste bang.", "tokens": ["Und", "auch", "der", "K\u00fchns\u00b7te", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch kannte auch der M\u00e4gdlein Schar", "tokens": ["Doch", "kann\u00b7te", "auch", "der", "M\u00e4gd\u00b7lein", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Starken Sporrenklang,", "tokens": ["Des", "Star\u00b7ken", "Spor\u00b7ren\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und manche, die fast trotzig war,", "tokens": ["Und", "man\u00b7che", ",", "die", "fast", "trot\u00b7zig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward, wenn er nahte, bang.", "tokens": ["Ward", ",", "wenn", "er", "nah\u00b7te", ",", "bang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von all den Fraun so zart und mild", "tokens": ["Von", "all", "den", "Fraun", "so", "zart", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mocht keine doch mit Recht", "tokens": ["Mocht", "kei\u00b7ne", "doch", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergleichen sich der Brunehild", "tokens": ["Ver\u00b7glei\u00b7chen", "sich", "der", "Bru\u00b7ne\u00b7hild"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Pottendorfs Geschlecht.", "tokens": ["Aus", "Pot\u00b7ten\u00b7dorfs", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}