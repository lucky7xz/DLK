], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Dem S\u00fcden wieder zugewendet pfeilgeschwind,", "tokens": ["Dem", "S\u00fc\u00b7den", "wie\u00b7der", "zu\u00b7ge\u00b7wen\u00b7det", "pfeil\u00b7ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Anconas hohen Strand vorbei,", "tokens": ["An\u00b7co\u00b7nas", "ho\u00b7hen", "Strand", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.71": {"text": "Und Rom sogar und Konradins Schlachtfeld vorbei,", "tokens": ["Und", "Rom", "so\u00b7gar", "und", "Kon\u00b7ra\u00b7dins", "Schlacht\u00b7feld", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "KON", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.72": {"text": "Zur\u00fcck in mein gelobtes Land,", "tokens": ["Zu\u00b7r\u00fcck", "in", "mein", "ge\u00b7lob\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Bis mich zuletzt absondere vom Gew\u00fchl des Tags", "tokens": ["Bis", "mich", "zu\u00b7letzt", "ab\u00b7son\u00b7de\u00b7re", "vom", "Ge\u00b7w\u00fchl", "des", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.74": {"text": "Der stillste Pomeranzenhain.", "tokens": ["Der", "stills\u00b7te", "Po\u00b7me\u00b7ran\u00b7zen\u00b7hain", "."], "t