{"textgrid.poem.60791": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Einladung nach der Insel Palmaria", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Spezias siebenbusiger Golf nach Westen hin", "tokens": ["Wo", "Spe\u00b7zias", "sie\u00b7ben\u00b7bu\u00b7si\u00b7ger", "Golf", "nach", "Wes\u00b7ten", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADJA", "NN", "APPR", "NN", "PTKVZ"], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Sich \u00f6ffnet gegen Korsika,", "tokens": ["Sich", "\u00f6ff\u00b7net", "ge\u00b7gen", "Kor\u00b7si\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stand ehedem ein Venustempel, jetzo ragt", "tokens": ["Stand", "e\u00b7he\u00b7dem", "ein", "Ve\u00b7nus\u00b7tem\u00b7pel", ",", "jet\u00b7zo", "ragt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Am Ufer eine kleine Stadt.", "tokens": ["Am", "U\u00b7fer", "ei\u00b7ne", "klei\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr dehnt ein Eiland gegen\u00fcber lang sich aus,", "tokens": ["Ihr", "dehnt", "ein", "Ei\u00b7land", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "lang", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schiffer nennt's Palmaria:", "tokens": ["Der", "Schif\u00b7fer", "nennt's", "Pal\u00b7ma\u00b7ria", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nur wenige H\u00fctten z\u00e4hlt es, hier und dort verstreut,", "tokens": ["Nur", "we\u00b7ni\u00b7ge", "H\u00fct\u00b7ten", "z\u00e4hlt", "es", ",", "hier", "und", "dort", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "$,", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bewohner z\u00e4hlt es wenige;", "tokens": ["Be\u00b7woh\u00b7ner", "z\u00e4hlt", "es", "we\u00b7ni\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00d6lb\u00e4ume stehn am minderschroffen Bergeshang,", "tokens": ["\u00d6l\u00b7b\u00e4u\u00b7me", "stehn", "am", "min\u00b7der\u00b7schrof\u00b7fen", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die meergewohnte Myrte bl\u00fcht", "tokens": ["Die", "meer\u00b7ge\u00b7wohn\u00b7te", "Myr\u00b7te", "bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nach allen Seiten, Rebe gedeiht und Feigenbaum,", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", ",", "Re\u00b7be", "ge\u00b7deiht", "und", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Den Gipfel kr\u00f6nen Pinien.", "tokens": ["Den", "Gip\u00b7fel", "kr\u00f6\u00b7nen", "Pi\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "In einer Bucht am Ufer aber locke dich", "tokens": ["In", "ei\u00b7ner", "Bucht", "am", "U\u00b7fer", "a\u00b7ber", "lo\u00b7cke", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die kleine Villa halbversteckt.", "tokens": ["Die", "klei\u00b7ne", "Vil\u00b7la", "halb\u00b7ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "F\u00fcr diesen Sommer ist sie mein, und jeden Tag", "tokens": ["F\u00fcr", "die\u00b7sen", "Som\u00b7mer", "ist", "sie", "mein", ",", "und", "je\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "PPOSAT", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erquicken hier des Morgenwinds,", "tokens": ["Er\u00b7qui\u00b7cken", "hier", "des", "Mor\u00b7gen\u00b7winds", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der reinen Luft, des salzigen Bades K\u00fchlungen,", "tokens": ["Der", "rei\u00b7nen", "Luft", ",", "des", "sal\u00b7zi\u00b7gen", "Ba\u00b7des", "K\u00fch\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und ungest\u00f6rte Mu\u00dfe mich.", "tokens": ["Und", "un\u00b7ge\u00b7st\u00f6r\u00b7te", "Mu\u00b7\u00dfe", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Carraras Marmorberge steigen fern empor,", "tokens": ["Car\u00b7ra\u00b7ras", "Mar\u00b7mor\u00b7ber\u00b7ge", "stei\u00b7gen", "fern", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Zu ihren F\u00fc\u00dfen Lerici,", "tokens": ["Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "Le\u00b7ri\u00b7ci", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "(wo jenes Dichters Freund ertrank, und dann von ihm", "tokens": ["(", "wo", "je\u00b7nes", "Dich\u00b7ters", "Freund", "er\u00b7trank", ",", "und", "dann", "von", "ihm"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PDAT", "NN", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bestattet ward im Aschenkrug.)", "tokens": ["Be\u00b7stat\u00b7tet", "ward", "im", "A\u00b7schen\u00b7krug", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mit kahler Stirne ragen dort des Apennins", "tokens": ["Mit", "kah\u00b7ler", "Stir\u00b7ne", "ra\u00b7gen", "dort", "des", "A\u00b7pen\u00b7nins"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bergr\u00fccken, w\u00e4hrend wohlgemut", "tokens": ["Berg\u00b7r\u00fc\u00b7cken", ",", "w\u00e4h\u00b7rend", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Vor\u00fcber leichte Schiffe ziehn, um hier und dort", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "leich\u00b7te", "Schif\u00b7fe", "ziehn", ",", "um", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,", "KOUI", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Kaufm\u00e4nnisch aufzustapeln, was", "tokens": ["Kauf\u00b7m\u00e4n\u00b7nisch", "auf\u00b7zu\u00b7sta\u00b7peln", ",", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VVINF", "$,", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "An Pomeranzen senden mag Sizilien,", "tokens": ["An", "Po\u00b7me\u00b7ran\u00b7zen", "sen\u00b7den", "mag", "Si\u00b7zi\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "NE", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "An fremden Weinen Genua.", "tokens": ["An", "frem\u00b7den", "Wei\u00b7nen", "Ge\u00b7nua", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Doch, wenn du dich einb\u00fcrgern wolltest hier vielleicht,", "tokens": ["Doch", ",", "wenn", "du", "dich", "ein\u00b7b\u00fcr\u00b7gern", "woll\u00b7test", "hier", "viel\u00b7leicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So sollst du wissen, was gebricht:", "tokens": ["So", "sollst", "du", "wis\u00b7sen", ",", "was", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Nichts fehlt zu dieses Aufenthalts Behaglichkeit", "tokens": ["Nichts", "fehlt", "zu", "die\u00b7ses", "Auf\u00b7ent\u00b7halts", "Be\u00b7hag\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als folgerechtere K\u00fcchenkunst;", "tokens": ["Als", "fol\u00b7ge\u00b7rech\u00b7te\u00b7re", "K\u00fc\u00b7chen\u00b7kunst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Ein rauher Seemann waltet mir am Herde jetzt,", "tokens": ["Ein", "rau\u00b7her", "See\u00b7mann", "wal\u00b7tet", "mir", "am", "Her\u00b7de", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der stets von Porto Venere", "tokens": ["Der", "stets", "von", "Por\u00b7to", "Ve\u00b7ne\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Des Morgens holt zu Schiffe meinen Hausbedarf,", "tokens": ["Des", "Mor\u00b7gens", "holt", "zu", "Schif\u00b7fe", "mei\u00b7nen", "Haus\u00b7be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als Koch und als Matrose dient.", "tokens": ["Als", "Koch", "und", "als", "Mat\u00b7ro\u00b7se", "dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Da dies Bekenntnis im voraus ich abgelegt,", "tokens": ["Da", "dies", "Be\u00b7kennt\u00b7nis", "im", "vo\u00b7raus", "ich", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "APPRART", "PTKVZ", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So darf ich immer sagen: Komm!", "tokens": ["So", "darf", "ich", "im\u00b7mer", "sa\u00b7gen", ":", "Komm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wofern die Schatten deines florentinischen", "tokens": ["Wo\u00b7fern", "die", "Schat\u00b7ten", "dei\u00b7nes", "flo\u00b7ren\u00b7ti\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Landhauses je du missen kannst,", "tokens": ["Land\u00b7hau\u00b7ses", "je", "du", "mis\u00b7sen", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Das oft als Gastfreund liebend mich und gern empfing,", "tokens": ["Das", "oft", "als", "Gast\u00b7freund", "lie\u00b7bend", "mich", "und", "gern", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KOUS", "NN", "ADJD", "PPER", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Zu wohlbestelltem Tische lud;", "tokens": ["Zu", "wohl\u00b7be\u00b7stell\u00b7tem", "Ti\u00b7sche", "lud", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wofern in einem Himmelsstrich du leben magst,", "tokens": ["Wo\u00b7fern", "in", "ei\u00b7nem", "Him\u00b7mels\u00b7strich", "du", "le\u00b7ben", "magst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der keinen Raffael gebar;", "tokens": ["Der", "kei\u00b7nen", "Raf\u00b7fael", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "(doch zeugten diese K\u00fcsten auch Unsterbliche,", "tokens": ["(", "doch", "zeug\u00b7ten", "die\u00b7se", "K\u00fcs\u00b7ten", "auch", "U\u00b7nsterb\u00b7li\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PDAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--++--", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Kolumbus und Napoleon!)", "tokens": ["Ko\u00b7lum\u00b7bus", "und", "Na\u00b7po\u00b7le\u00b7on", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Wofern du, dem so teuer ist toskanischer,", "tokens": ["Wo\u00b7fern", "du", ",", "dem", "so", "teu\u00b7er", "ist", "tos\u00b7ka\u00b7ni\u00b7scher", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Vibrierter Konsonantenhauch,", "tokens": ["Vi\u00b7brier\u00b7ter", "Kon\u00b7so\u00b7nan\u00b7ten\u00b7hauch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "An Genuesersprache dich, an gallische", "tokens": ["An", "Ge\u00b7nu\u00b7e\u00b7ser\u00b7spra\u00b7che", "dich", ",", "an", "gal\u00b7li\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PPER", "$,", "APPR", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.50": {"text": "Verweichlichung gew\u00f6hnen kannst:", "tokens": ["Ver\u00b7weich\u00b7li\u00b7chung", "ge\u00b7w\u00f6h\u00b7nen", "kannst", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "So komm! Wo nicht, so lebe wohl! An jedem Ort", "tokens": ["So", "komm", "!", "Wo", "nicht", ",", "so", "le\u00b7be", "wohl", "!", "An", "je\u00b7dem", "Ort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "ADV", "$.", "APPR", "PIAT", "NN"], 