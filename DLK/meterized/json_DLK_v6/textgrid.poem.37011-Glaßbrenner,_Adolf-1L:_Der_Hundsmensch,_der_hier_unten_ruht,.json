{"textgrid.poem.37011": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Hundsmensch, der hier unten ruht,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hundsmensch, der hier unten ruht,", "tokens": ["Der", "Hunds\u00b7mensch", ",", "der", "hier", "un\u00b7ten", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei trocknem Ostwind ziemlich gut,", "tokens": ["Bei", "trock\u00b7nem", "Ost\u00b7wind", "ziem\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War, schlug es um nach Nord und S\u00fcd,", "tokens": ["War", ",", "schlug", "es", "um", "nach", "Nord", "und", "S\u00fcd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "APPR", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von b\u00f6sem Sinne und Gem\u00fcth.", "tokens": ["Von", "b\u00f6\u00b7sem", "Sin\u00b7ne", "und", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald war er hitzig-liberal,", "tokens": ["Bald", "war", "er", "hit\u00b7zig\u00b7li\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald kalt und schmeidig, wie ein Aal,", "tokens": ["Bald", "kalt", "und", "schmei\u00b7dig", ",", "wie", "ein", "Aal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Selbsts\u00fcchtig-gier in allen Phasen,", "tokens": ["Selbst\u00b7s\u00fcch\u00b7tig\u00b7gier", "in", "al\u00b7len", "Pha\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Schnauze voll der sch\u00f6nsten Phrasen,", "tokens": ["Die", "Schnau\u00b7ze", "voll", "der", "sch\u00f6ns\u00b7ten", "Phra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dummstolz und eitel, zeigte er", "tokens": ["Dumm\u00b7stolz", "und", "ei\u00b7tel", ",", "zeig\u00b7te", "er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Z\u00e4hne oft und blaffte sehr,", "tokens": ["Die", "Z\u00e4h\u00b7ne", "oft", "und", "blaff\u00b7te", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch drohte irgend ihm Gefahr,", "tokens": ["Doch", "droh\u00b7te", "ir\u00b7gend", "ihm", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So str\u00e4ubte sich vor Furcht sein Haar;", "tokens": ["So", "str\u00e4ub\u00b7te", "sich", "vor", "Furcht", "sein", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er heult', krummbuckelte und kroch", "tokens": ["Er", "heult'", ",", "krumm\u00b7bu\u00b7ckel\u00b7te", "und", "kroch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Schwanzwedelnd in's gemeinste Loch!", "tokens": ["Schwanz\u00b7we\u00b7delnd", "in's", "ge\u00b7meins\u00b7te", "Loch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bei all' dem aber war ", "tokens": ["Bei", "all'", "dem", "a\u00b7ber", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "ADV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Charaktrisir'n ihn anders nicht", "tokens": ["Cha\u00b7rak\u00b7tri\u00b7sir'n", "ihn", "an\u00b7ders", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als durch: Charakterloser Wicht!", "tokens": ["Als", "durch", ":", "Cha\u00b7rak\u00b7ter\u00b7lo\u00b7ser", "Wicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}