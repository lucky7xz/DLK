{"textgrid.poem.65717": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Ma\u00fcsefeind (o Wunderding!)", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Ma\u00fcsefeind (o Wunderding!)", "tokens": ["Den", "Ma\u00fc\u00b7se\u00b7feind", "(", "o", "Wun\u00b7der\u00b7ding", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der manche Ratte k\u00fcnstlich fieng,", "tokens": ["Der", "man\u00b7che", "Rat\u00b7te", "k\u00fcnst\u00b7lich", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hat nun der Tod gefangen.", "tokens": ["Hat", "nun", "der", "Tod", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Schade, da\u00df der Mann erbleicht!", "tokens": ["Ach", "Scha\u00b7de", ",", "da\u00df", "der", "Mann", "er\u00b7bleicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der selbst dem Alexander gleicht,", "tokens": ["Der", "selbst", "dem", "A\u00b7lex\u00b7an\u00b7der", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem die Griechen prangen.", "tokens": ["Mit", "dem", "die", "Grie\u00b7chen", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn erstlich kam er auf die Welt", "tokens": ["Denn", "erst\u00b7lich", "kam", "er", "auf", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nat\u00fcrlich, als wie jener Held,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", ",", "als", "wie", "je\u00b7ner", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schry auf gleiche Weise.", "tokens": ["Und", "schry", "auf", "glei\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schliefen Beyde, wenn sie m\u00fcd;", "tokens": ["Sie", "schlie\u00b7fen", "Bey\u00b7de", ",", "wenn", "sie", "m\u00fcd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch lebten sie bey Krieg und Fried", "tokens": ["Auch", "leb\u00b7ten", "sie", "bey", "Krieg", "und", "Fried"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von nichts, als Trank und Speise.", "tokens": ["Von", "nichts", ",", "als", "Trank", "und", "Spei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man glaubt so gar mit gutem Recht,", "tokens": ["Man", "glaubt", "so", "gar", "mit", "gu\u00b7tem", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es z\u00e4hle Ma\u00fcsefeinds Geschlecht", "tokens": ["Es", "z\u00e4h\u00b7le", "Ma\u00fc\u00b7se\u00b7feinds", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch mehr an alten Ahnen.", "tokens": ["Noch", "mehr", "an", "al\u00b7ten", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und, traut man der gemeinen Red,", "tokens": ["Und", ",", "traut", "man", "der", "ge\u00b7mei\u00b7nen", "Red", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gl\u00fcckt es dem bey seiner Greth,", "tokens": ["So", "gl\u00fcckt", "es", "dem", "bey", "sei\u00b7ner", "Greth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Trutz Jenem bey Roxanen.", "tokens": ["Trutz", "Je\u00b7nem", "bey", "Ro\u00b7xa\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wi\u00dft ihr, worinn Ma\u00fcsefeind", "tokens": ["Doch", "wi\u00dft", "ihr", ",", "wo\u00b7rinn", "Ma\u00fc\u00b7se\u00b7feind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Alexander ungleich scheint?", "tokens": ["Dem", "A\u00b7lex\u00b7an\u00b7der", "un\u00b7gleich", "scheint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das will ich e\u00fcch vertrauen.", "tokens": ["Das", "will", "ich", "e\u00b7\u00fcch", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er w\u00fcrgte nichts, als Maus und Ratt.", "tokens": ["Er", "w\u00fcrg\u00b7te", "nichts", ",", "als", "Maus", "und", "Ratt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hingegen jener Nimmersatt", "tokens": ["Hin\u00b7ge\u00b7gen", "je\u00b7ner", "Nim\u00b7mer\u00b7satt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PDAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der w\u00fcrgte Mann und Frauen.", "tokens": ["Der", "w\u00fcrg\u00b7te", "Mann", "und", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seit aber unser Held erblich,", "tokens": ["Seit", "a\u00b7ber", "un\u00b7ser", "Held", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gleicht er erst recht meisterlich", "tokens": ["So", "gleicht", "er", "erst", "recht", "meis\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem alten Alexandern.", "tokens": ["Dem", "al\u00b7ten", "A\u00b7lex\u00b7an\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn jetzund, (merkt auf diese Lehr,)", "tokens": ["Denn", "je\u00b7tzund", ",", "(", "merkt", "auf", "die\u00b7se", "Lehr", ",", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "$(", "VVFIN", "APPR", "PDAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gibt niemand einen Kre\u00fczer mehr", "tokens": ["Gibt", "nie\u00b7mand", "ei\u00b7nen", "Kre\u00fc\u00b7zer", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um Einen, als den Andern.", "tokens": ["Um", "Ei\u00b7nen", ",", "als", "den", "An\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "$,", "KOUS", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}