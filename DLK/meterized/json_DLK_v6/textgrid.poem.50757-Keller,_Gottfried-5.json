{"textgrid.poem.50757": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele Wochen sind entflohn,", "tokens": ["Vie\u00b7le", "Wo\u00b7chen", "sind", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich dich gesehen;", "tokens": ["Seit", "ich", "dich", "ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hab auch lange Tage schon", "tokens": ["Hab", "auch", "lan\u00b7ge", "Ta\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Blum' gesehen!", "tokens": ["Kei\u00b7ne", "Blum'", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Keine Blumen und kein Lieb \u2013", "tokens": ["Kei\u00b7ne", "Blu\u00b7men", "und", "kein", "Lieb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach was soll das werden?", "tokens": ["Ach", "was", "soll", "das", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PDS", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was soll aus dem Fr\u00fchlingstrieb", "tokens": ["Was", "soll", "aus", "dem", "Fr\u00fch\u00b7ling\u00b7strieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In mir innen werden?", "tokens": ["In", "mir", "in\u00b7nen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar noch stets der Lenz erschien,", "tokens": ["Zwar", "noch", "stets", "der", "Lenz", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner bin ich sicher;", "tokens": ["Sei\u00b7ner", "bin", "ich", "si\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fc\u00dft ich nur, was ich dir bin,", "tokens": ["W\u00fc\u00dft", "ich", "nur", ",", "was", "ich", "dir", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r ich doppelt sicher!", "tokens": ["W\u00e4r", "ich", "dop\u00b7pelt", "si\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Deiner lieben Augen", "tokens": ["Dei\u00b7ner", "lie\u00b7ben", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00e4re wohl ein zartes Gl\u00fcck", "tokens": ["W\u00e4\u00b7re", "wohl", "ein", "zar\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir f\u00fcr Herz und Augen!", "tokens": ["Mir", "f\u00fcr", "Herz", "und", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}