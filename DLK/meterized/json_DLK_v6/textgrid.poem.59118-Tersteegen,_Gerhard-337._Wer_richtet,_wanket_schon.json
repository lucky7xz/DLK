{"textgrid.poem.59118": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "337. Wer richtet, wanket schon", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Gutes schau und lieb in allen;", "tokens": ["Nur", "Gu\u00b7tes", "schau", "und", "lieb", "in", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "KON", "ADJD", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt einer, bet und richte nicht!", "tokens": ["F\u00e4llt", "ei\u00b7ner", ",", "bet", "und", "rich\u00b7te", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kann aufstehn, und du kannst fallen;", "tokens": ["Er", "kann", "auf\u00b7stehn", ",", "und", "du", "kannst", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schau Gott an, halt dich, eh's geschicht!", "tokens": ["Schau", "Gott", "an", ",", "halt", "dich", ",", "eh's", "ge\u00b7schicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}