{"textgrid.poem.41542": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh dich nicht!", "tokens": ["Ich", "seh", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo bist du denn, o Hort, o Lebenshauch?", "tokens": ["Wo", "bist", "du", "denn", ",", "o", "Hort", ",", "o", "Le\u00b7bens\u00b7hauch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kannst du nicht wehen, da\u00df mein Ohr es h\u00f6rt?", "tokens": ["Kannst", "du", "nicht", "we\u00b7hen", ",", "da\u00df", "mein", "Ohr", "es", "h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was nebelst, was verflatterst du wie Rauch,", "tokens": ["Was", "ne\u00b7belst", ",", "was", "ver\u00b7flat\u00b7terst", "du", "wie", "Rauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn sich das Aug' nach deinen Zeichen kehrt?", "tokens": ["Wenn", "sich", "das", "Aug'", "nach", "dei\u00b7nen", "Zei\u00b7chen", "kehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein W\u00fcstenlicht,", "tokens": ["Mein", "W\u00fcs\u00b7ten\u00b7licht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein Aaronsstab, der lieblich k\u00f6nnte gr\u00fcnen,", "tokens": ["Mein", "Aa\u00b7rons\u00b7stab", ",", "der", "lieb\u00b7lich", "k\u00f6nn\u00b7te", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du tust es nicht;", "tokens": ["Du", "tust", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So mu\u00df ich eigne Schuld und Torheit s\u00fchnen!", "tokens": ["So", "mu\u00df", "ich", "eig\u00b7ne", "Schuld", "und", "Tor\u00b7heit", "s\u00fch\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hei\u00df ist der Tag;", "tokens": ["Hei\u00df", "ist", "der", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Sonne prallt von meiner Zelle Wand,", "tokens": ["Die", "Son\u00b7ne", "prallt", "von", "mei\u00b7ner", "Zel\u00b7le", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein traulich V\u00f6glein flattert ein und aus;", "tokens": ["Ein", "trau\u00b7lich", "V\u00f6\u00b7glein", "flat\u00b7tert", "ein", "und", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein gl\u00e4nzend Auge fragt mich unverwandt:", "tokens": ["Sein", "gl\u00e4n\u00b7zend", "Au\u00b7ge", "fragt", "mich", "un\u00b7ver\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schaut nicht der Herr zu diesen Fenstern aus?", "tokens": ["Schaut", "nicht", "der", "Herr", "zu", "die\u00b7sen", "Fens\u00b7tern", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was fragst du nach?", "tokens": ["Was", "fragst", "du", "nach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Stirne mu\u00df ich senken und err\u00f6ten.", "tokens": ["Die", "Stir\u00b7ne", "mu\u00df", "ich", "sen\u00b7ken", "und", "er\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O bittre Schmach!", "tokens": ["O", "bitt\u00b7re", "Schmach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Mein Wissen mu\u00dfte meinen Glauben t\u00f6ten.", "tokens": ["Mein", "Wis\u00b7sen", "mu\u00df\u00b7te", "mei\u00b7nen", "Glau\u00b7ben", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Wolke steigt,", "tokens": ["Die", "Wol\u00b7ke", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und langsam \u00fcber den azurnen Bau", "tokens": ["Und", "lang\u00b7sam", "\u00fc\u00b7ber", "den", "a\u00b7zur\u00b7nen", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat eine Schwefelh\u00fclle sich gelegt.", "tokens": ["Hat", "ei\u00b7ne", "Schwe\u00b7fel\u00b7h\u00fcl\u00b7le", "sich", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die L\u00fcfte wehn so seufzervoll und lau", "tokens": ["Die", "L\u00fcf\u00b7te", "wehn", "so", "seuf\u00b7zer\u00b7voll", "und", "lau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Angstgest\u00f6hn sich in den Zweigen regt;", "tokens": ["Und", "Angst\u00b7ge\u00b7st\u00f6hn", "sich", "in", "den", "Zwei\u00b7gen", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Herde keucht.", "tokens": ["Die", "Her\u00b7de", "keucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was f\u00fchlt das stumpfe Tier, ist's deine Schw\u00fcle?", "tokens": ["Was", "f\u00fchlt", "das", "stump\u00b7fe", "Tier", ",", "ist's", "dei\u00b7ne", "Schw\u00fc\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich steh gebeugt;", "tokens": ["Ich", "steh", "ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mein Herr ber\u00fchre mich, da\u00df ich dich f\u00fchle!", "tokens": ["Mein", "Herr", "be\u00b7r\u00fch\u00b7re", "mich", ",", "da\u00df", "ich", "dich", "f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Donnerschlag!", "tokens": ["Ein", "Don\u00b7ner\u00b7schlag", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Entsetzen hat den kranken Wald gepackt.", "tokens": ["Ent\u00b7set\u00b7zen", "hat", "den", "kran\u00b7ken", "Wald", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehe, wie im Nest mein Vogel duckt,", "tokens": ["Ich", "se\u00b7he", ",", "wie", "im", "Nest", "mein", "Vo\u00b7gel", "duckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Ast an Ast sich \u00e4chzend reibt und knackt,", "tokens": ["Wie", "Ast", "an", "Ast", "sich", "\u00e4ch\u00b7zend", "reibt", "und", "knackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Blitz an Blitz durch Schwefelgassen zuckt;", "tokens": ["Wie", "Blitz", "an", "Blitz", "durch", "Schwe\u00b7fel\u00b7gas\u00b7sen", "zuckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich schau ihm nach.", "tokens": ["Ich", "schau", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ist's deine Leuchte nicht, gewaltig Wesen?", "tokens": ["Ist's", "dei\u00b7ne", "Leuch\u00b7te", "nicht", ",", "ge\u00b7wal\u00b7tig", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Warum denn, ach!", "tokens": ["Wa\u00b7rum", "denn", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Warum nur f\u00e4llt mir ein was ich gelesen?", "tokens": ["Wa\u00b7rum", "nur", "f\u00e4llt", "mir", "ein", "was", "ich", "ge\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Dunkel weicht;", "tokens": ["Das", "Dun\u00b7kel", "weicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und wie ein leises Weinen f\u00e4llt herab", "tokens": ["Und", "wie", "ein", "lei\u00b7ses", "Wei\u00b7nen", "f\u00e4llt", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wolkentau; Gefl\u00fcster fern und nah.", "tokens": ["Der", "Wol\u00b7ken\u00b7tau", ";", "Ge\u00b7fl\u00fcs\u00b7ter", "fern", "und", "nah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$.", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne senkt den goldnen Gnadenstab,", "tokens": ["Die", "Son\u00b7ne", "senkt", "den", "gold\u00b7nen", "Gna\u00b7dens\u00b7tab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und pl\u00f6tzlich steht der Friedensbogen da.", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "steht", "der", "Frie\u00b7dens\u00b7bo\u00b7gen", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie? wird denn feucht", "tokens": ["Wie", "?", "wird", "denn", "feucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein Auge, ist nicht Dunstgebild der Regen?", "tokens": ["Mein", "Au\u00b7ge", ",", "ist", "nicht", "Dunst\u00b7ge\u00b7bild", "der", "Re\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir wird so leicht!", "tokens": ["Mir", "wird", "so", "leicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wie? kann denn Halmes Reibung mich bewegen?", "tokens": ["Wie", "?", "kann", "denn", "Hal\u00b7mes", "Rei\u00b7bung", "mich", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ADV", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf Bergesh\u00f6hn", "tokens": ["Auf", "Ber\u00b7ges\u00b7h\u00f6hn"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Stand ein Prophet und suchte dich wie ich:", "tokens": ["Stand", "ein", "Pro\u00b7phet", "und", "such\u00b7te", "dich", "wie", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da brach ein Sturm der Riesenfichte Ast,", "tokens": ["Da", "brach", "ein", "Sturm", "der", "Rie\u00b7sen\u00b7fich\u00b7te", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da fra\u00df ein Feuer durch die Wipfel sich;", "tokens": ["Da", "fra\u00df", "ein", "Feu\u00b7er", "durch", "die", "Wip\u00b7fel", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch unersch\u00fcttert stand der W\u00fcste Gast.", "tokens": ["Doch", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "stand", "der", "W\u00fcs\u00b7te", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da kam ein Wehn", "tokens": ["Da", "kam", "ein", "Wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie Gnadenhauch und zitternd \u00fcberwunden", "tokens": ["Wie", "Gna\u00b7den\u00b7hauch", "und", "zit\u00b7ternd", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sank der Prophet,", "tokens": ["Sank", "der", "Pro\u00b7phet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und weinte laut und hatte dich gefunden.", "tokens": ["Und", "wein\u00b7te", "laut", "und", "hat\u00b7te", "dich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": 