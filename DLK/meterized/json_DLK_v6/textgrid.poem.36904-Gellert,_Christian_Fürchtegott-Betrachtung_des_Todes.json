{"textgrid.poem.36904": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Todes", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sicher lebt der Mensch, der Staub!", "tokens": ["Wie", "si\u00b7cher", "lebt", "der", "Mensch", ",", "der", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Leben ist ein fallend Laub;", "tokens": ["Sein", "Le\u00b7ben", "ist", "ein", "fal\u00b7lend", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch schmeichelt er sich gern,", "tokens": ["Und", "den\u00b7noch", "schmei\u00b7chelt", "er", "sich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag des Todes sei noch fern.", "tokens": ["Der", "Tag", "des", "To\u00b7des", "sei", "noch", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der J\u00fcngling hofft des Greises Ziel,", "tokens": ["Der", "J\u00fcng\u00b7ling", "hofft", "des", "Grei\u00b7ses", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mann noch seiner Jahre viel,", "tokens": ["Der", "Mann", "noch", "sei\u00b7ner", "Jah\u00b7re", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Greis zu vielen noch ", "tokens": ["Der", "Greis", "zu", "vie\u00b7len", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner nimmt den Irrtum wahr.", "tokens": ["Und", "kei\u00b7ner", "nimmt", "den", "Irr\u00b7tum", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprich nicht: Ich denk in Gl\u00fcck und Not", "tokens": ["Sprich", "nicht", ":", "Ich", "denk", "in", "Gl\u00fcck", "und", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herzen oft an meinen Tod.", "tokens": ["Im", "Her\u00b7zen", "oft", "an", "mei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, den der Tod nicht weiser macht,", "tokens": ["Der", ",", "den", "der", "Tod", "nicht", "wei\u00b7ser", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat nie mit Ernst an ihn gedacht.", "tokens": ["Hat", "nie", "mit", "Ernst", "an", "ihn", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir leben hier zur Ewigkeit,", "tokens": ["Wir", "le\u00b7ben", "hier", "zur", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu tun, was uns der Herr gebeut,", "tokens": ["Zu", "tun", ",", "was", "uns", "der", "Herr", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsers Lebens kleinster Teil", "tokens": ["Und", "un\u00b7sers", "Le\u00b7bens", "kleins\u00b7ter", "Teil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist eine Frist zu unserm Heil.", "tokens": ["Ist", "ei\u00b7ne", "Frist", "zu", "un\u00b7serm", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Tod r\u00fcckt Seelen vor Gericht;", "tokens": ["Der", "Tod", "r\u00fcckt", "See\u00b7len", "vor", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da bringt Gott alles an das Licht,", "tokens": ["Da", "bringt", "Gott", "al\u00b7les", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht, was hier verborgen war,", "tokens": ["Und", "macht", ",", "was", "hier", "ver\u00b7bor\u00b7gen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Rat der Herzen offenbar.", "tokens": ["Den", "Rat", "der", "Her\u00b7zen", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum da dein Tod dir t\u00e4glich dr\u00e4ut,", "tokens": ["Drum", "da", "dein", "Tod", "dir", "t\u00e4g\u00b7lich", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sei doch wacker und bereit;", "tokens": ["So", "sei", "doch", "wa\u00b7cker", "und", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pr\u00fcf deinen Glauben, als ein Christ,", "tokens": ["Pr\u00fcf", "dei\u00b7nen", "Glau\u00b7ben", ",", "als", "ein", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er durch Liebe t\u00e4tig ist.", "tokens": ["Ob", "er", "durch", "Lie\u00b7be", "t\u00e4\u00b7tig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Seufzer in der letzten Not,", "tokens": ["Ein", "Seuf\u00b7zer", "in", "der", "letz\u00b7ten", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wunsch, durch des Erl\u00f6sers Tod", "tokens": ["Ein", "Wunsch", ",", "durch", "des", "Er\u00b7l\u00f6\u00b7sers", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Gottes Thron gerecht zu sein,", "tokens": ["Vor", "Got\u00b7tes", "Thron", "ge\u00b7recht", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies macht dich nicht von S\u00fcnden rein.", "tokens": ["Dies", "macht", "dich", "nicht", "von", "S\u00fcn\u00b7den", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Herz, das Gottes Stimme h\u00f6rt,", "tokens": ["Ein", "Herz", ",", "das", "Got\u00b7tes", "Stim\u00b7me", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr folgt, und sich vom B\u00f6sen kehrt;", "tokens": ["Ihr", "folgt", ",", "und", "sich", "vom", "B\u00f6\u00b7sen", "kehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gl\u00e4ubig Herz, von Lieb erf\u00fcllt,", "tokens": ["Ein", "gl\u00e4u\u00b7big", "Herz", ",", "von", "Lieb", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ist es, was in Christo gilt.", "tokens": ["Dies", "ist", "es", ",", "was", "in", "Chris\u00b7to", "gilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Heiligung erfordert M\u00fch;", "tokens": ["Die", "Hei\u00b7li\u00b7gung", "er\u00b7for\u00b7dert", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirkst sie nicht, Gott wirket sie.", "tokens": ["Du", "wirkst", "sie", "nicht", ",", "Gott", "wir\u00b7ket", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber ringe stets nach ihr,", "tokens": ["Du", "a\u00b7ber", "rin\u00b7ge", "stets", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4re sie ein Werk von dir.", "tokens": ["Als", "w\u00e4\u00b7re", "sie", "ein", "Werk", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Ruf des Lebens, das du lebst,", "tokens": ["Der", "Ruf", "des", "Le\u00b7bens", ",", "das", "du", "lebst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein h\u00f6chstes Ziel, nach dem du strebst,", "tokens": ["Dein", "h\u00f6chs\u00b7tes", "Ziel", ",", "nach", "dem", "du", "strebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Tage Rechenschaft", "tokens": ["Und", "dei\u00b7ner", "Ta\u00b7ge", "Re\u00b7chen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Tugend in des Glaubens Kraft.", "tokens": ["Ist", "Tu\u00b7gend", "in", "des", "Glau\u00b7bens", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihr alle seine Tage weihn,", "tokens": ["Ihr", "al\u00b7le", "sei\u00b7ne", "Ta\u00b7ge", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hei\u00dft eingedenk des Todes sein:", "tokens": ["Hei\u00dft", "ein\u00b7ge\u00b7denk", "des", "To\u00b7des", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wachsen in der Heiligung,", "tokens": ["Und", "wach\u00b7sen", "in", "der", "Hei\u00b7li\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist wahre Todserinnerung.", "tokens": ["Ist", "wah\u00b7re", "Tod\u00b7se\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wie oft verge\u00df ich diese Pflicht!", "tokens": ["Wie", "oft", "ver\u00b7ge\u00df", "ich", "die\u00b7se", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr, geh mit mir nicht ins Gericht;", "tokens": ["Herr", ",", "geh", "mit", "mir", "nicht", "ins", "Ge\u00b7richt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dr\u00fcck selbst des Todes Bild in mich,", "tokens": ["Dr\u00fcck", "selbst", "des", "To\u00b7des", "Bild", "in", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dir wandle w\u00fcrdiglich;", "tokens": ["Da\u00df", "ich", "dir", "wand\u00b7le", "w\u00fcr\u00b7dig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da\u00df ich mein Herz mit jedem Tag", "tokens": ["Da\u00df", "ich", "mein", "Herz", "mit", "je\u00b7dem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dir, o Gott! erforschen mag,", "tokens": ["Vor", "dir", ",", "o", "Gott", "!", "er\u00b7for\u00b7schen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$.", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Liebe, Demut, Fried und Treu,", "tokens": ["Ob", "Lie\u00b7be", ",", "De\u00b7mut", ",", "Fried", "und", "Treu", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frucht des Geistes, in ihm sei;", "tokens": ["Die", "Frucht", "des", "Geis\u00b7tes", ",", "in", "ihm", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da\u00df ich zu dir um Gnade fleh,", "tokens": ["Da\u00df", "ich", "zu", "dir", "um", "Gna\u00b7de", "fleh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets meiner Schwachheit widersteh,", "tokens": ["Stets", "mei\u00b7ner", "Schwach\u00b7heit", "wi\u00b7der\u00b7steh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einstens in des Glaubens Macht", "tokens": ["Und", "eins\u00b7tens", "in", "des", "Glau\u00b7bens", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freuden ruf: Es ist vollbracht!", "tokens": ["Mit", "Freu\u00b7den", "ruf", ":", "Es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}