{"dta.poem.7887": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schlimme Nachbarschaft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Nur selten komm\u2019 ich aus dem Zimmer,", "tokens": ["Nur", "sel\u00b7ten", "komm'", "ich", "aus", "dem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch will die Arbeit nicht vom Ort;", "tokens": ["Doch", "will", "die", "Ar\u00b7beit", "nicht", "vom", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ge\u00f6ffnet sind die B\u00fccher immer,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "sind", "die", "B\u00fc\u00b7cher", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keine Seite r\u00fcck\u2019 ich fort.", "tokens": ["Doch", "kei\u00b7ne", "Sei\u00b7te", "r\u00fcck", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Nachbars lieblich Fl\u00f6tenspielen", "tokens": ["Des", "Nach\u00b7bars", "lieb\u00b7lich", "Fl\u00f6\u00b7ten\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt jetzt mir die Gedanken hin,", "tokens": ["Nimmt", "jetzt", "mir", "die", "Ge\u00b7dan\u00b7ken", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jetzt mu\u00df ich hin\u00fcberschielen", "tokens": ["Und", "jetzt", "mu\u00df", "ich", "hin\u00b7\u00fc\u00b7bersc\u00b7hie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach meiner h\u00fcbschen Nachbarin.", "tokens": ["Nach", "mei\u00b7ner", "h\u00fcb\u00b7schen", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}