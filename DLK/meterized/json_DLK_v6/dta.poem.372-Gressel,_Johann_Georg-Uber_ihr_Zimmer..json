{"dta.poem.372": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihr Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Oh\u00f6chst begl\u00fcckter Ort/ viel mahl begl\u00fccktes Zimmer/", "tokens": ["O\u00b7h\u00f6chst", "be\u00b7gl\u00fcck\u00b7ter", "Ort", "/", "viel", "mahl", "be\u00b7gl\u00fcck\u00b7tes", "Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der du den netten Schein/ den angenehmen Schimmer", "tokens": ["Der", "du", "den", "net\u00b7ten", "Schein", "/", "den", "an\u00b7ge\u00b7neh\u00b7men", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und dich um diese Pracht so viel als nichts bem\u00fchst.", "tokens": ["Und", "dich", "um", "die\u00b7se", "Pracht", "so", "viel", "als", "nichts", "be\u00b7m\u00fchst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "ADV", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach m\u00f6cht\u2019 ich mich ein mahl in dich verwandeln k\u00f6nnen/", "tokens": ["Ach", "m\u00f6cht'", "ich", "mich", "ein", "mahl", "in", "dich", "ver\u00b7wan\u00b7deln", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ART", "ADV", "APPR", "PRF", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So w\u00fcrde ich mein Gl\u00fcck recht unvergleichlich nennen.", "tokens": ["So", "w\u00fcr\u00b7de", "ich", "mein", "Gl\u00fcck", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein/ wie gern sie auch belebte Sachen hat/", "tokens": ["Al\u00b7lein", "/", "wie", "gern", "sie", "auch", "be\u00b7leb\u00b7te", "Sa\u00b7chen", "hat", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "ADV", "PPER", "ADV", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So findt kein Flehen doch in deinen Ohren statt.", "tokens": ["So", "findt", "kein", "Fle\u00b7hen", "doch", "in", "dei\u00b7nen", "Oh\u00b7ren", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}