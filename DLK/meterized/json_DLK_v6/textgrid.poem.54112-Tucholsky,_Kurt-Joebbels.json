{"textgrid.poem.54112": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Joebbels", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wat w\u00e4rst du ohne deine M\u00f6belpacker!", "tokens": ["Wat", "w\u00e4rst", "du", "oh\u00b7ne", "dei\u00b7ne", "M\u00f6\u00b7bel\u00b7pa\u00b7cker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die stehn, bezahlt untreu, so um dir rum.", "tokens": ["Die", "stehn", ",", "be\u00b7zahlt", "un\u00b7treu", ",", "so", "um", "dir", "rum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "ADJD", "$,", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dahinter du: een arma Lauseknacker,", "tokens": ["Da\u00b7hin\u00b7ter", "du", ":", "e\u00b7en", "ar\u00b7ma", "Lau\u00b7se\u00b7kna\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "een Baritong fort Jachtenpublikum.", "tokens": ["e\u00b7en", "Ba\u00b7ri\u00b7tong", "fort", "Jach\u00b7ten\u00b7pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Die Weiber \u2013 hach \u2013 die bibbern dir entjejen", "tokens": ["Die", "Wei\u00b7ber", "\u2013", "hach", "\u2013", "die", "bib\u00b7bern", "dir", "ent\u00b7je\u00b7jen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVIMP", "$(", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "un m\u00f6chten sich am liebsten uffn Boden lejen!", "tokens": ["un", "m\u00f6ch\u00b7ten", "sich", "am", "liebs\u00b7ten", "uffn", "Bo\u00b7den", "le\u00b7jen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VMFIN", "PRF", "APPRART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du machst un tust und jippst da an . . .", "tokens": ["Du", "machst", "un", "tust", "und", "jippst", "da", "an", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "KON", "ADV", "ADV", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Josef, du bist 'n kleener Mann.", "tokens": ["Jo\u00b7sef", ",", "du", "bist", "'n", "klee\u00b7ner", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit dein Klumpfu\u00df \u2013 seh mal, bein andern", "tokens": ["Mit", "dein", "Klum\u00b7pfu\u00df", "\u2013", "seh", "mal", ",", "bein", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VVIMP", "ADV", "$,", "PIAT", "ADJA"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "da sacht ick nischt; det kann ja jeda ham.", "tokens": ["da", "sacht", "ick", "nischt", ";", "det", "kann", "ja", "je\u00b7da", "ham", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$.", "PDS", "VMFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wi\u00dft als Recke durch de Jejend wandern", "tokens": ["Du", "wi\u00dft", "als", "Re\u00b7cke", "durch", "de", "Je\u00b7jend", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "un pa\u00dft in keen Sch\u00fctzenjrahm?", "tokens": ["un", "pa\u00dft", "in", "ke\u00b7en", "Sch\u00fct\u00b7zen\u00b7jrahm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "In Sportpalast sowie in deine Presse,", "tokens": ["In", "Sport\u00b7pa\u00b7last", "so\u00b7wie", "in", "dei\u00b7ne", "Pres\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "da haste eine m\u00e4chtich jro\u00dfe Fresse.", "tokens": ["da", "has\u00b7te", "ei\u00b7ne", "m\u00e4ch\u00b7tich", "jro\u00b7\u00dfe", "Fres\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Riskierst du wat? \u2013 De Schnauze vornean.", "tokens": ["Ris\u00b7kierst", "du", "wat", "?", "\u2013", "De", "Schnau\u00b7ze", "vor\u00b7ne\u00b7an", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$.", "$(", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Josef, du bist 'n kleener Mann.", "tokens": ["Jo\u00b7sef", ",", "du", "bist", "'n", "klee\u00b7ner", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist mit irgendwat zu kurz gekomm.", "tokens": ["Du", "bist", "mit", "ir\u00b7gend\u00b7wat", "zu", "kurz", "ge\u00b7komm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nu r\u00e4chste dir, nu lechste los.", "tokens": ["Nu", "r\u00e4chs\u00b7te", "dir", ",", "nu", "lechs\u00b7te", "los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir hamm se woll zu fr\u00fch aus Nest jenomm!", "tokens": ["Dir", "hamm", "se", "woll", "zu", "fr\u00fch", "aus", "Nest", "je\u00b7nomm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "FM", "ADV", "PTKA", "ADJD", "APPR", "NN", "PDAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du bist keen Heros, det markierste blo\u00df.", "tokens": ["Du", "bist", "ke\u00b7en", "He\u00b7ros", ",", "det", "mar\u00b7kiers\u00b7te", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PDS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Du hast 'n Buckel, Mensch \u2013 du bist nich richtich!", "tokens": ["Du", "hast", "'n", "Bu\u00b7ckel", ",", "Mensch", "\u2013", "du", "bist", "nich", "rich\u00b7tich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Du bist blo\u00df laut \u2013 sonst biste jahnich wichtig!", "tokens": ["Du", "bist", "blo\u00df", "laut", "\u2013", "sonst", "bis\u00b7te", "jah\u00b7nich", "wich\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Keen Sch\u00fctze \u2013 een Porzellanzerschmei\u00dfer,", "tokens": ["Ke\u00b7en", "Sch\u00fct\u00b7ze", "\u2013", "e\u00b7en", "Por\u00b7zel\u00b7lan\u00b7zer\u00b7schmei\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "keen F\u00fchrer biste \u2013 blo\u00df 'n Rei\u00dfer,", "tokens": ["ke\u00b7en", "F\u00fch\u00b7rer", "bis\u00b7te", "\u2013", "blo\u00df", "'n", "Rei\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$(", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Josef,", "tokens": ["Jo\u00b7sef", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "du bist een jro\u00dfer Mann \u2013!", "tokens": ["du", "bist", "e\u00b7en", "jro\u00b7\u00dfer", "Mann", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}