{"dta.poem.1290": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Geburts-Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Eine lange Winter-Nacht", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "Win\u00b7ter\u00b7Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mich an das Licht gebracht/", "tokens": ["Hat", "mich", "an", "das", "Licht", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesu/ welchem Nacht und Licht", "tokens": ["Je\u00b7su", "/", "wel\u00b7chem", "Nacht", "und", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "PWAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu gehorchen ist verpflicht/", "tokens": ["Zu", "ge\u00b7hor\u00b7chen", "ist", "ver\u00b7pflicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mir deinen Gnaden-Schein", "tokens": ["La\u00df", "mir", "dei\u00b7nen", "Gna\u00b7den\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tag und Nacht f\u00fcr Augen seyn/", "tokens": ["Tag", "und", "Nacht", "f\u00fcr", "Au\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der finstren Wercke Dunst", "tokens": ["Wenn", "der", "finst\u00b7ren", "Wer\u00b7cke", "Dunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will vertuncklen deine Gunst;", "tokens": ["Will", "ver\u00b7tunck\u00b7len", "dei\u00b7ne", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die tr\u00fcbe Todes-Nacht", "tokens": ["Wenn", "die", "tr\u00fc\u00b7be", "To\u00b7des\u00b7Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir die Augen finster macht/", "tokens": ["Mir", "die", "Au\u00b7gen", "fins\u00b7ter", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bi\u00df ich dich/ den hellen Tag/", "tokens": ["Bi\u00df", "ich", "dich", "/", "den", "hel\u00b7len", "Tag", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$(", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sonder N\u00e4chte schauen mag.", "tokens": ["Son\u00b7der", "N\u00e4ch\u00b7te", "schau\u00b7en", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}