{"textgrid.poem.59465": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Parole", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie stand wohl am Fensterbogen", "tokens": ["Sie", "stand", "wohl", "am", "Fens\u00b7ter\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und flocht sich traurig ihr Haar,", "tokens": ["Und", "flocht", "sich", "trau\u00b7rig", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der J\u00e4ger war fortgezogen,", "tokens": ["Der", "J\u00e4\u00b7ger", "war", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der J\u00e4ger ihr Liebster war.", "tokens": ["Der", "J\u00e4\u00b7ger", "ihr", "Liebs\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als der Fr\u00fchling gekommen,", "tokens": ["Und", "als", "der", "Fr\u00fch\u00b7ling", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Welt war von Bl\u00fcten verschneit,", "tokens": ["Die", "Welt", "war", "von", "Bl\u00fc\u00b7ten", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da hat sie ein Herz sich genommen", "tokens": ["Da", "hat", "sie", "ein", "Herz", "sich", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PRF", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und ging in die gr\u00fcne Heid.", "tokens": ["Und", "ging", "in", "die", "gr\u00fc\u00b7ne", "Heid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie legt das Ohr an den Rasen,", "tokens": ["Sie", "legt", "das", "Ohr", "an", "den", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00f6rt ferner Hufe Klang \u2013", "tokens": ["H\u00f6rt", "fer\u00b7ner", "Hu\u00b7fe", "Klang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sind die Rehe, die grasen", "tokens": ["Das", "sind", "die", "Re\u00b7he", ",", "die", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am schattigen Bergeshang.", "tokens": ["Am", "schat\u00b7ti\u00b7gen", "Ber\u00b7ges\u00b7hang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und abends die W\u00e4lder rauschen,", "tokens": ["Und", "a\u00b7bends", "die", "W\u00e4l\u00b7der", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von fern nur f\u00e4llt noch ein Schu\u00df,", "tokens": ["Von", "fern", "nur", "f\u00e4llt", "noch", "ein", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da steht sie stille, zu lauschen:", "tokens": ["Da", "steht", "sie", "stil\u00b7le", ",", "zu", "lau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbdas war meines Liebsten Gru\u00df!\u00ab", "tokens": ["\u00bb", "das", "war", "mei\u00b7nes", "Liebs\u00b7ten", "Gru\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sprangen vom Fels die Quellen,", "tokens": ["Da", "spran\u00b7gen", "vom", "Fels", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da flogen die V\u00f6glein ins Tal.", "tokens": ["Da", "flo\u00b7gen", "die", "V\u00f6\u00b7glein", "ins", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbund wo ihr ihn trefft, ihr Gesellen,", "tokens": ["\u00bb", "und", "wo", "ihr", "ihn", "trefft", ",", "ihr", "Ge\u00b7sel\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gr\u00fc\u00dft mir ihn tausendmal!\u00ab", "tokens": ["Gr\u00fc\u00dft", "mir", "ihn", "tau\u00b7send\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "$("], "meter": "++-+-+", "measure": "iambic.tri"}}}}}