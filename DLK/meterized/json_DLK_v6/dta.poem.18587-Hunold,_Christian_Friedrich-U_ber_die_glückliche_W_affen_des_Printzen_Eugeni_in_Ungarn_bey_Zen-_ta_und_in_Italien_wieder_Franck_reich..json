{"dta.poem.18587": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber die gl\u00fcckliche  W affen des  \n Printzen Eugeni in Ungarn bey Zen-  \n ta/ und in Italien wieder Franck  \n reich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Held Austrasiens/ der Teutschen Tapfferkeit/", "tokens": ["Ein", "Held", "Aus\u00b7tra\u00b7si\u00b7ens", "/", "der", "Teut\u00b7schen", "Tapf\u00b7fer\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Musel-M\u00e4nner Todt/ der Ungarn Schutz und Leben", "tokens": ["Der", "Mu\u00b7sel\u00b7M\u00e4n\u00b7ner", "Todt", "/", "der", "Un\u00b7garn", "Schutz", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewinnet hier den Ruhm der Helden ist bereit/", "tokens": ["Ge\u00b7win\u00b7net", "hier", "den", "Ruhm", "der", "Hel\u00b7den", "ist", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Selbst Fama kan den Sieg nicht hoch genug erheben.", "tokens": ["Selbst", "Fa\u00b7ma", "kan", "den", "Sieg", "nicht", "hoch", "ge\u00b7nug", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df nun der Po der Thei\u00df an Purpur gleich will gehn.", "tokens": ["Da\u00df", "nun", "der", "Po", "der", "Thei\u00df", "an", "Pur\u00b7pur", "gleich", "will", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er schon einen Sieg/ der Feind die Flucht gewonnen:", "tokens": ["Da\u00df", "er", "schon", "ei\u00b7nen", "Sieg", "/", "der", "Feind", "die", "Flucht", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist/ weil in solcher Held den Sternen gleich mu\u00df stehn/", "tokens": ["Ist", "/", "weil", "in", "sol\u00b7cher", "Held", "den", "Ster\u00b7nen", "gleich", "mu\u00df", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "APPR", "PIAT", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum weil der Mond besiegt/ so geht er auch zur Sonnen.", "tokens": ["Drum", "weil", "der", "Mond", "be\u00b7siegt", "/", "so", "geht", "er", "auch", "zur", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}