{"textgrid.poem.53200": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Jugend sucht' einmal", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend sucht' einmal", "tokens": ["Die", "Ju\u00b7gend", "sucht'", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was n\u00fctzliches zu haben", "tokens": ["Was", "n\u00fctz\u00b7li\u00b7ches", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Venus, Sie befahl", "tokens": ["Von", "Ve\u00b7nus", ",", "Sie", "be\u00b7fahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es Amor ihrem Knaben:", "tokens": ["Es", "A\u00b7mor", "ih\u00b7rem", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dieser sinnet hin und her", "tokens": ["Die\u00b7ser", "sin\u00b7net", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was es seyn solt' ohngefehr,", "tokens": ["Was", "es", "seyn", "solt'", "ohn\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAINF", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Endlich f\u00e4llt der Tantz ihm ein", "tokens": ["End\u00b7lich", "f\u00e4llt", "der", "Tantz", "ihm", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sol das beste seyn.", "tokens": ["Der", "sol", "das", "bes\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er hat da Seiten-klang,", "tokens": ["Er", "hat", "da", "Sei\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lust, Anmuht, Gn\u00fcge, Leben,", "tokens": ["Lust", ",", "An\u00b7muht", ",", "Gn\u00fc\u00b7ge", ",", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gespr\u00e4che, Schertz, Gesang", "tokens": ["Ge\u00b7spr\u00e4\u00b7che", ",", "Schertz", ",", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd sich ihm mitgegeben:", "tokens": ["Vnd", "sich", "ihm", "mit\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wodurch unsre Schenckel sind", "tokens": ["Wo\u00b7durch", "uns\u00b7re", "Schen\u00b7ckel", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht als Federn, schnell als Wind,", "tokens": ["Leicht", "als", "Fe\u00b7dern", ",", "schnell", "als", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd wir springen wie ein Reh", "tokens": ["Vnd", "wir", "sprin\u00b7gen", "wie", "ein", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hoch auff der Berge H\u00f6h.", "tokens": ["Hoch", "auff", "der", "Ber\u00b7ge", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Von solchen Zeiten an", "tokens": ["Von", "sol\u00b7chen", "Zei\u00b7ten", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist tantzen jungen Hertzen,", "tokens": ["Ist", "tant\u00b7zen", "jun\u00b7gen", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was keine Lust seyn kan,", "tokens": ["Was", "kei\u00b7ne", "Lust", "seyn", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hie brechen sich die Schmertzen,", "tokens": ["Hie", "bre\u00b7chen", "sich", "die", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hie vergeht die Traurigkeit,", "tokens": ["Hie", "ver\u00b7geht", "die", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hie wohnt lauter g\u00fcldne Zeit,", "tokens": ["Hie", "wohnt", "lau\u00b7ter", "g\u00fcld\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wann man die im Reyen f\u00fchrt,", "tokens": ["Wann", "man", "die", "im", "Re\u00b7yen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die uns das Hertz ger\u00fchrt.", "tokens": ["Die", "uns", "das", "Hertz", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn wer verliebet ist", "tokens": ["Denn", "wer", "ver\u00b7lie\u00b7bet", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd geht mit der im Reyen", "tokens": ["Vnd", "geht", "mit", "der", "im", "Re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die er ihm hat erkiest,", "tokens": ["Die", "er", "ihm", "hat", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie meint ihn auch mit trewen,", "tokens": ["Sie", "meint", "ihn", "auch", "mit", "tre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der besitzt nach seinem Muth", "tokens": ["Der", "be\u00b7sitzt", "nach", "sei\u00b7nem", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr noch als ein F\u00fcrsten-Gut,", "tokens": ["Mehr", "noch", "als", "ein", "F\u00fcrs\u00b7ten\u00b7Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Seinen Tantz vertauscht er nicht", "tokens": ["Sei\u00b7nen", "Tantz", "ver\u00b7tauscht", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vielleicht umb dieses Licht.", "tokens": ["Viel\u00b7leicht", "umb", "die\u00b7ses", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vnd wer verdencket wol", "tokens": ["Vnd", "wer", "ver\u00b7den\u00b7cket", "wol"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es auch der gr\u00fcnen Jugend?", "tokens": ["Es", "auch", "der", "gr\u00fc\u00b7nen", "Ju\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lebt sie nur wie sie soll", "tokens": ["Lebt", "sie", "nur", "wie", "sie", "soll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "PPER", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd strebt nach Ehr und Tugend,", "tokens": ["Vnd", "strebt", "nach", "Ehr", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd verm\u00e4hlt die Ehrbarkeit", "tokens": ["Vnd", "ver\u00b7m\u00e4hlt", "die", "Ehr\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Reyen allezeit,", "tokens": ["Mit", "dem", "Re\u00b7yen", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was sie denn f\u00fcr Kurtzweil \u00fcbt,", "tokens": ["Was", "sie", "denn", "f\u00fcr", "Kurt\u00b7zweil", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das alles wird beliebt.", "tokens": ["Das", "al\u00b7les", "wird", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit dem Bescheid heran,", "tokens": ["Mit", "dem", "Be\u00b7scheid", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer F\u00fcsse hat zu springen!", "tokens": ["Wer", "F\u00fcs\u00b7se", "hat", "zu", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt zeig' er seinen Mann,", "tokens": ["Jetzt", "zeig'", "er", "sei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil Spiel und Saiten singen:", "tokens": ["Weil", "Spiel", "und", "Sai\u00b7ten", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "W\u00fcnschet diesem Edlen Paar", "tokens": ["W\u00fcn\u00b7schet", "die\u00b7sem", "Ed\u00b7len", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fcck und Segen-reiche Jahr,", "tokens": ["Gl\u00fcck", "und", "Se\u00b7gen\u00b7rei\u00b7che", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd gedencket stets dabey,", "tokens": ["Vnd", "ge\u00b7den\u00b7cket", "stets", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df alles eitel sey.", "tokens": ["Da\u00df", "al\u00b7les", "ei\u00b7tel", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}