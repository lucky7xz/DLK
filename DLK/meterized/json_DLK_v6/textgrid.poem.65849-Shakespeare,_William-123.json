{"textgrid.poem.65849": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "123", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, Zeit! nicht wandelbar sollst du mich schelten:", "tokens": ["Nein", ",", "Zeit", "!", "nicht", "wan\u00b7del\u00b7bar", "sollst", "du", "mich", "schel\u00b7ten", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PTKNEG", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir kann dein junger Pyramidenbau", "tokens": ["Mir", "kann", "dein", "jun\u00b7ger", "Py\u00b7ra\u00b7mi\u00b7den\u00b7bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht staunenswert, nicht f\u00fcr ein Wunder gelten;", "tokens": ["Nicht", "stau\u00b7nens\u00b7wert", ",", "nicht", "f\u00fcr", "ein", "Wun\u00b7der", "gel\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind nur Anputz einer \u00e4ltern Schau.", "tokens": ["Sie", "sind", "nur", "An\u00b7putz", "ei\u00b7ner", "\u00e4l\u00b7tern", "Schau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In enge Frist beschr\u00e4nkt, bewundern wir,", "tokens": ["In", "en\u00b7ge", "Frist", "be\u00b7schr\u00e4nkt", ",", "be\u00b7wun\u00b7dern", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was du f\u00fcr altersgrau uns willst verkaufen,", "tokens": ["Was", "du", "f\u00fcr", "al\u00b7ters\u00b7grau", "uns", "willst", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vergessen fr\u00fcherer Kunde, voll Begier", "tokens": ["Ver\u00b7ges\u00b7sen", "fr\u00fc\u00b7he\u00b7rer", "Kun\u00b7de", ",", "voll", "Be\u00b7gier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nach eignem Wunsch beliebig sie zu taufen.", "tokens": ["Nach", "eig\u00b7nem", "Wunsch", "be\u00b7lie\u00b7big", "sie", "zu", "tau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Hohn biet' ich dir und deinen Chroniken:", "tokens": ["Hohn", "biet'", "ich", "dir", "und", "dei\u00b7nen", "Chro\u00b7ni\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nicht Gegenw\u00e4rt'ges noch Vergangnes nimmt mich Wunder;", "tokens": ["Nicht", "Ge\u00b7gen\u00b7w\u00e4rt'\u00b7ges", "noch", "Ver\u00b7gang\u00b7nes", "nimmt", "mich", "Wun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn Lug ist dein Bericht, und was wir sehn:", "tokens": ["Denn", "Lug", "ist", "dein", "Be\u00b7richt", ",", "und", "was", "wir", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein ewig Eilen taucht es auf und unter.", "tokens": ["Ein", "e\u00b7wig", "Ei\u00b7len", "taucht", "es", "auf", "und", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dies eine schw\u00f6r' und halt' ich treulich mir:", "tokens": ["Dies", "ei\u00b7ne", "schw\u00f6r'", "und", "halt'", "ich", "treu\u00b7lich", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wahr will ich sein, trotz deiner Sens' und dir.", "tokens": ["Wahr", "will", "ich", "sein", ",", "trotz", "dei\u00b7ner", "Sens'", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,", "APPR", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}