{"textgrid.poem.37327": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ungen\u00fcgend", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei es freundlich, sei es b\u00f6se,", "tokens": ["Sei", "es", "freund\u00b7lich", ",", "sei", "es", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meist gen\u00fcgend klar und scharf", "tokens": ["Meist", "ge\u00b7n\u00fc\u00b7gend", "klar", "und", "scharf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klingt des Mundes Wortget\u00f6se", "tokens": ["Klingt", "des", "Mun\u00b7des", "Wort\u00b7ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den t\u00e4glichen Bedarf.", "tokens": ["F\u00fcr", "den", "t\u00e4g\u00b7li\u00b7chen", "Be\u00b7darf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch die H\u00f6chstgef\u00fchle heischen", "tokens": ["Doch", "die", "H\u00f6chst\u00b7ge\u00b7f\u00fch\u00b7le", "hei\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren ganz besondern Klang;", "tokens": ["Ih\u00b7ren", "ganz", "be\u00b7son\u00b7dern", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann sagt grunzen oder kreischen", "tokens": ["Dann", "sagt", "grun\u00b7zen", "o\u00b7der", "krei\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als Rede und Gesang.", "tokens": ["Mehr", "als", "Re\u00b7de", "und", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}