{"textgrid.poem.56790": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber der Erde Stirne,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber der Erde Stirne,", "tokens": ["\u00dc\u00b7ber", "der", "Er\u00b7de", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "durch Tag und Nacht,", "tokens": ["durch", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "pfl\u00fcgt ein hagres Weib", "tokens": ["pfl\u00fcgt", "ein", "hag\u00b7res", "Weib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "hin und her ...", "tokens": ["hin", "und", "her", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wilde Stiere,", "tokens": ["Wil\u00b7de", "Stie\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "kaum zu hemmen, ziehn,", "tokens": ["kaum", "zu", "hem\u00b7men", ",", "ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "rei\u00dfen ihre Pflugschar durch den Grund:", "tokens": ["rei\u00b7\u00dfen", "ih\u00b7re", "Pflug\u00b7schar", "durch", "den", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Doch je rasender die Nacken zerrn,", "tokens": ["Doch", "je", "ra\u00b7sen\u00b7der", "die", "Na\u00b7cken", "zerrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "nur so tiefer dr\u00fcckt den Baum sie ein.", "tokens": ["nur", "so", "tie\u00b7fer", "dr\u00fcckt", "den", "Baum", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00dcber der Erde Stirne,", "tokens": ["\u00dc\u00b7ber", "der", "Er\u00b7de", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "durch Tag und Nacht,", "tokens": ["durch", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "f\u00fchrt Frau Sorge", "tokens": ["f\u00fchrt", "Frau", "Sor\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Furche, Furche, Furche ...", "tokens": ["Fur\u00b7che", ",", "Fur\u00b7che", ",", "Fur\u00b7che", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Leidenschaften,", "tokens": ["Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "kaum zu z\u00e4hmen, ziehn,", "tokens": ["kaum", "zu", "z\u00e4h\u00b7men", ",", "ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "rei\u00dfen ihre Pflugschar durch den Grund:", "tokens": ["rei\u00b7\u00dfen", "ih\u00b7re", "Pflug\u00b7schar", "durch", "den", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Doch je wilder die D\u00e4monen zerrn,", "tokens": ["Doch", "je", "wil\u00b7der", "die", "D\u00e4\u00b7mo\u00b7nen", "zerrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "nur so tiefer gr\u00e4bt den Stahl sie ein.", "tokens": ["nur", "so", "tie\u00b7fer", "gr\u00e4bt", "den", "Stahl", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}