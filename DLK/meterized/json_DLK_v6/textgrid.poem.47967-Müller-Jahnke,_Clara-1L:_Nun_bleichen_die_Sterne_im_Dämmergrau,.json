{"textgrid.poem.47967": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun bleichen die Sterne im D\u00e4mmergrau,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bleichen die Sterne im D\u00e4mmergrau,", "tokens": ["Nun", "blei\u00b7chen", "die", "Ster\u00b7ne", "im", "D\u00e4m\u00b7mer\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und die Geister schweben von hinnen \u2013", "tokens": ["und", "die", "Geis\u00b7ter", "schwe\u00b7ben", "von", "hin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADV", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und ich m\u00f6chte dich halten, du bl\u00fchender Traum", "tokens": ["und", "ich", "m\u00f6ch\u00b7te", "dich", "hal\u00b7ten", ",", "du", "bl\u00fc\u00b7hen\u00b7der", "Traum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PRF", "VVINF", "$,", "PPER", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "und f\u00fchle dich schon zerrinnen!", "tokens": ["und", "f\u00fch\u00b7le", "dich", "schon", "zer\u00b7rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte dich malen als wonniges Lied,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "ma\u00b7len", "als", "won\u00b7ni\u00b7ges", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "mit gl\u00fchenden Reizen dich schm\u00fccken \u2013", "tokens": ["mit", "gl\u00fc\u00b7hen\u00b7den", "Rei\u00b7zen", "dich", "schm\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "die Farbe ist bla\u00df und die Form zerrinnt", "tokens": ["die", "Far\u00b7be", "ist", "bla\u00df", "und", "die", "Form", "zer\u00b7rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und es will kein Strich mir gl\u00fccken.", "tokens": ["und", "es", "will", "kein", "Strich", "mir", "gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte dich singen als jubelndes Lied", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "sin\u00b7gen", "als", "ju\u00b7beln\u00b7des", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "der kommenden Sonne entgegen \u2013", "tokens": ["der", "kom\u00b7men\u00b7den", "Son\u00b7ne", "ent\u00b7ge\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "das Wort versagt und die Stimme bricht", "tokens": ["das", "Wort", "ver\u00b7sagt", "und", "die", "Stim\u00b7me", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "vor des Herzens wogenden Schl\u00e4gen.", "tokens": ["vor", "des", "Her\u00b7zens", "wo\u00b7gen\u00b7den", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer fa\u00dft den spr\u00fchenden Schaum? Wer bannt", "tokens": ["Wer", "fa\u00dft", "den", "spr\u00fc\u00b7hen\u00b7den", "Schaum", "?", "Wer", "bannt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der Stunde fl\u00fcchtige Sohle?", "tokens": ["der", "Stun\u00b7de", "fl\u00fcch\u00b7ti\u00b7ge", "Soh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer f\u00e4ngt den Strahl und wer hascht den Duft", "tokens": ["Wer", "f\u00e4ngt", "den", "Strahl", "und", "wer", "hascht", "den", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "der tr\u00e4umenden Nachtviole?!", "tokens": ["der", "tr\u00e4u\u00b7men\u00b7den", "Nacht\u00b7vi\u00b7o\u00b7le", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}