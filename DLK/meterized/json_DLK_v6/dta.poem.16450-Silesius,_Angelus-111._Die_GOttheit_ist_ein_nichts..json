{"dta.poem.16450": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "111.  Die GOttheit ist ein nichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die zarte GOttheit ist ein nichts und \u00fcbernichts:", "tokens": ["Die", "zar\u00b7te", "Got\u00b7theit", "ist", "ein", "nichts", "und", "\u00fc\u00b7ber\u00b7nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "PIS", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nichts in allem sicht/ Mensch glaube/ dieser sichts.", "tokens": ["Wer", "nichts", "in", "al\u00b7lem", "sicht", "/", "Mensch", "glau\u00b7be", "/", "die\u00b7ser", "sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PIS", "VVFIN", "$(", "NN", "VVFIN", "$(", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}