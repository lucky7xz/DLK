{"dta.poem.10777": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft vff das Brautbeth H. Frantz  \n Cammeels vnd Jungfraw Vrsula Mar-  \n greta Eichelsteinin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ein zarte Jungfrawschaffthierinnen ligt beg raben/", "tokens": ["Ein", "zar\u00b7te", "Jung\u00b7fraw\u00b7schafft\u00b7hie\u00b7rin\u00b7nen", "ligt", "beg", "ra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hochzeit Gott ", "tokens": ["Der", "Hoch\u00b7zeit", "Gott"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr Stunde kommen war/ da\u00df sie jetz sterben solt/", "tokens": ["Ihr", "Stun\u00b7de", "kom\u00b7men", "war", "/", "da\u00df", "sie", "jetz", "ster\u00b7ben", "solt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der must sie bringen vmb/ dem sie doch ware holdt/", "tokens": ["Der", "must", "sie", "brin\u00b7gen", "vmb", "/", "dem", "sie", "doch", "wa\u00b7re", "holdt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "APPR", "$(", "PRELS", "PPER", "ADV", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der grausame Tyran\u0303 wolt sie nit lassen leben/", "tokens": ["Der", "grau\u00b7sa\u00b7me", "Ty\u00b7ra\u00f1", "wolt", "sie", "nit", "las\u00b7sen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In diesem Federwerck/ sie must den Geist auffgeben/", "tokens": ["In", "die\u00b7sem", "Fe\u00b7der\u00b7werck", "/", "sie", "must", "den", "Geist", "auff\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Venus verlie\u00dfe sie/ die sie vmb h\u00fclff rufft an", "tokens": ["Ve\u00b7nus", "ver\u00b7lie\u00b7\u00dfe", "sie", "/", "die", "sie", "vmb", "h\u00fclff", "rufft", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$(", "PRELS", "PPER", "APPR", "NE", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit manchen Threnen hei\u00df: ", "tokens": ["Mit", "man\u00b7chen", "Thre\u00b7nen", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der arge kleine Gott/ je mehr sie sich thet wehren/", "tokens": ["Der", "ar\u00b7ge", "klei\u00b7ne", "Gott", "/", "je", "mehr", "sie", "sich", "thet", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ADV", "ADV", "PPER", "PRF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Je mehr entz\u00fcndet er das hitzige begeren", "tokens": ["Je", "mehr", "ent\u00b7z\u00fcn\u00b7det", "er", "das", "hit\u00b7zi\u00b7ge", "be\u00b7ge\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Der M\u00f6rderischen Lieb de\u00df harten J\u00fcngelings/", "tokens": ["Der", "M\u00f6r\u00b7de\u00b7ri\u00b7schen", "Lieb", "de\u00df", "har\u00b7ten", "J\u00fcn\u00b7ge\u00b7lings", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie weinet inniglich/ er lachte nur de\u00df dings:", "tokens": ["Sie", "wei\u00b7net", "in\u00b7nig\u00b7lich", "/", "er", "lach\u00b7te", "nur", "de\u00df", "dings", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Blum/ die sch\u00f6ne Blum die must jhr Haubte neigen/", "tokens": ["Die", "Blum", "/", "die", "sch\u00f6\u00b7ne", "Blum", "die", "must", "jhr", "Haub\u00b7te", "nei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADJA", "NE", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Venus R\u00f6selein mit seinen gr\u00fcnen Zweigen", "tokens": ["Das", "Ve\u00b7nus", "R\u00f6\u00b7se\u00b7lein", "mit", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Must endtlichen daran/ von grosser Fewers gluth", "tokens": ["Must", "endt\u00b7li\u00b7chen", "da\u00b7ran", "/", "von", "gros\u00b7ser", "Fe\u00b7wers", "gluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PAV", "$(", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verwelcken vnd allhier verliern sein h\u00f6chstes Gut.", "tokens": ["Ver\u00b7wel\u00b7cken", "vnd", "all\u00b7hier", "ver\u00b7li\u00b7ern", "sein", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.17": {"text": "Nembt ein Exempel dran jhr andere Jungfrawen/", "tokens": ["Nembt", "ein", "Ex\u00b7em\u00b7pel", "dran", "jhr", "an\u00b7de\u00b7re", "Jung\u00b7fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Vnd sehet zu/ wem jhr ewr Jungfrawschafft vertrawen/", "tokens": ["Vnd", "se\u00b7het", "zu", "/", "wem", "jhr", "ewr", "Jung\u00b7fraw\u00b7schafft", "ver\u00b7tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "$(", "PWS", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Da\u00df euch nit auch so geh\u2019: es wehre dan\u0303 vieleicht/", "tokens": ["Da\u00df", "euch", "nit", "auch", "so", "geh'", ":", "es", "weh\u00b7re", "da\u00f1", "vie\u00b7leicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df jhr de\u00df Lebens m\u00fcd/ den Todt auch w\u00fcnschet euch.", "tokens": ["Da\u00df", "jhr", "de\u00df", "Le\u00b7bens", "m\u00fcd", "/", "den", "Todt", "auch", "w\u00fcn\u00b7schet", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$(", "ART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}