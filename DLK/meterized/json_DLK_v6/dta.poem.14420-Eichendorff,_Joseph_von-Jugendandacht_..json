{"dta.poem.14420": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl wei\u00df ich's, \u2014 dieser Farben heimlich Spreiten", "tokens": ["Wohl", "wei\u00df", "ich's", ",", "die\u00b7ser", "Far\u00b7ben", "heim\u00b7lich", "Sprei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "$(", "PDAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Deckt einer Jungfrau strahlend reine Glieder;", "tokens": ["Deckt", "ei\u00b7ner", "Jung\u00b7frau", "strah\u00b7lend", "rei\u00b7ne", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wogt der gro\u00dfe Schleier auf und nieder,", "tokens": ["Es", "wogt", "der", "gro\u00b7\u00dfe", "Schlei\u00b7er", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie schlummert drunten fort seit Ewigkeiten.", "tokens": ["Sie", "schlum\u00b7mert", "drun\u00b7ten", "fort", "seit", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}