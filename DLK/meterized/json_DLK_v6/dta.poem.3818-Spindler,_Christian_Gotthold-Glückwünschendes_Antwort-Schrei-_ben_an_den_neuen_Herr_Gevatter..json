{"dta.poem.3818": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschendes Antwort-Schrei-  \n ben/ an den neuen Herr Gevatter.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bin ich nicht erfreut gewesen,", "tokens": ["Wie", "bin", "ich", "nicht", "er\u00b7freut", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald ich deinen Brief gelesen,", "tokens": ["So", "bald", "ich", "dei\u00b7nen", "Brief", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mir die Nachricht kund gethan:", "tokens": ["Der", "mir", "die", "Nach\u00b7richt", "kund", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Es sey ein junger Sohn gekommen,", "tokens": ["Es", "sey", "ein", "jun\u00b7ger", "Sohn", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, was ich fast nicht glauben kan,", "tokens": ["Ja", ",", "was", "ich", "fast", "nicht", "glau\u00b7ben", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Freundschaffts-volle Brust, dein g\u00fctiges Er-", "tokens": ["Die", "Freund\u00b7schaffts\u00b7vol\u00b7le", "Brust", ",", "dein", "g\u00fc\u00b7ti\u00b7ges", "Er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "weisen,", "tokens": ["wei\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Will mich des Kindes Path, von dir Gevatter,", "tokens": ["Will", "mich", "des", "Kin\u00b7des", "Path", ",", "von", "dir", "Ge\u00b7vat\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "$,", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "heissen.", "tokens": ["heis\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Nun sp\u00fchr ich deine grosse Liebe,", "tokens": ["Nun", "sp\u00fchr", "ich", "dei\u00b7ne", "gros\u00b7se", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Bewundert meine treuste Brust.", "tokens": ["Be\u00b7wun\u00b7dert", "mei\u00b7ne", "treus\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gr\u00f6ster Sehnsucht und Verlangen", "tokens": ["Mit", "gr\u00f6s\u00b7ter", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will ich den kleinen Path umfangen,", "tokens": ["Will", "ich", "den", "klei\u00b7nen", "Path", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und er sey k\u00fcnfftig meine Lust.", "tokens": ["Und", "er", "sey", "k\u00fcnff\u00b7tig", "mei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will ihn h\u00f6chst erfreut zum Tauffstein hin be-", "tokens": ["Ich", "will", "ihn", "h\u00f6chst", "er\u00b7freut", "zum", "Tauffs\u00b7tein", "hin", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "gleiten,", "tokens": ["glei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und ihm den Weg zum Heil, durch mein Gebet,", "tokens": ["Und", "ihm", "den", "Weg", "zum", "Heil", ",", "durch", "mein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "bereiten.", "tokens": ["be\u00b7rei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Des sch\u00f6nsten Himmels fr\u00f6mmste G\u00fcte", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Him\u00b7mels", "fr\u00f6mms\u00b7te", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreue nun auch das Gem\u00fcthe", "tokens": ["Er\u00b7freu\u00b7e", "nun", "auch", "das", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Er gebe ihr verneute Kr\u00e4ffte,", "tokens": ["Er", "ge\u00b7be", "ihr", "ver\u00b7neu\u00b7te", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er gebe neue Lebens-S\u00e4ffte,", "tokens": ["Er", "ge\u00b7be", "neu\u00b7e", "Le\u00b7bens\u00b7S\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er segne auch das Auferziehn", "tokens": ["Er", "seg\u00b7ne", "auch", "das", "Auf\u00b7er\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von deinem ", "tokens": ["Von", "dei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "ten Erben,", "tokens": ["ten", "Er\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Der Jahre Wachsthum wird des Segens Quell", "tokens": ["Der", "Jah\u00b7re", "Wach\u00b7sthum", "wird", "des", "Se\u00b7gens", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "erwerben.", "tokens": ["er\u00b7wer\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Letzt solst du bey dem Kindtauff-Essen", "tokens": ["Letzt", "solst", "du", "bey", "dem", "Kind\u00b7tauff\u00b7Es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "All Ungelegenheit vergessen,", "tokens": ["All", "Un\u00b7ge\u00b7le\u00b7gen\u00b7heit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb nur nicht mehr als ein Gericht,", "tokens": ["Gieb", "nur", "nicht", "mehr", "als", "ein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die\u00df soll mir am besten schmecken,", "tokens": ["Und", "die\u00df", "soll", "mir", "am", "bes\u00b7ten", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst dich nicht in Kosten stecken,", "tokens": ["Du", "wirst", "dich", "nicht", "in", "Kos\u00b7ten", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, warrlich, das verlang ich nicht.", "tokens": ["Nein", ",", "warr\u00b7lich", ",", "das", "ver\u00b7lang", "ich", "nicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ein wohlgemeint Geschenck ins stille Wochen-", "tokens": ["Ein", "wohl\u00b7ge\u00b7meint", "Ge\u00b7schenck", "ins", "stil\u00b7le", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bette.", "tokens": ["Bet\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}