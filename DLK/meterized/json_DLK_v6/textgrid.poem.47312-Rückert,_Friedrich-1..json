{"textgrid.poem.47312": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, zum Himmel dich zu schwingen aus der Nacht!", "tokens": ["Auf", ",", "zum", "Him\u00b7mel", "dich", "zu", "schwin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Herz, empor zum Licht zu ringen aus der Nacht!", "tokens": ["Herz", ",", "em\u00b7por", "zum", "Licht", "zu", "rin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "APPRART", "NN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Sieh, wie Gottes Liebesboten leuchtende", "tokens": ["Sieh", ",", "wie", "Got\u00b7tes", "Lie\u00b7bes\u00b7bo\u00b7ten", "leuch\u00b7ten\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "NN", "NN", "ADJA"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Gr\u00fc\u00dfe dir entgegenbringen aus der Nacht!", "tokens": ["Gr\u00fc\u00b7\u00dfe", "dir", "ent\u00b7ge\u00b7gen\u00b7brin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wo im Westen sank die Sonne, bl\u00fchn ihr nach", "tokens": ["Wo", "im", "Wes\u00b7ten", "sank", "die", "Son\u00b7ne", ",", "bl\u00fchn", "ihr", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "R\u00f6ten, die noch nicht vergingen, aus der Nacht.", "tokens": ["R\u00f6\u00b7ten", ",", "die", "noch", "nicht", "ver\u00b7gin\u00b7gen", ",", "aus", "der", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Wo sie steigen wird im Osten, sieh, wie schon", "tokens": ["Wo", "sie", "stei\u00b7gen", "wird", "im", "Os\u00b7ten", ",", "sieh", ",", "wie", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "VAFIN", "APPRART", "NN", "$,", "VVFIN", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Rosen an zu keimen fingen aus der Nacht!", "tokens": ["Ro\u00b7sen", "an", "zu", "kei\u00b7men", "fin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PTKZU", "VVINF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Lichts Erinn'rungen und Lichtes Hoffnungen,", "tokens": ["Lichts", "Er\u00b7inn'\u00b7run\u00b7gen", "und", "Lich\u00b7tes", "Hoff\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Die sich dir zum Kranze schlingen aus der Nacht.", "tokens": ["Die", "sich", "dir", "zum", "Kran\u00b7ze", "schlin\u00b7gen", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Und dar\u00fcber schaun die ew'gen Stern' herein,", "tokens": ["Und", "da\u00b7r\u00fc\u00b7ber", "schaun", "die", "ew'\u00b7gen", "Stern'", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die hernieder tr\u00f6stend klingen aus der Nacht:", "tokens": ["Die", "her\u00b7nie\u00b7der", "tr\u00f6s\u00b7tend", "klin\u00b7gen", "aus", "der", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Eh' der Kranz von Doppelrosen dort verbl\u00fcht,", "tokens": ["Eh'", "der", "Kranz", "von", "Dop\u00b7pel\u00b7ro\u00b7sen", "dort", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Wird dein ew'ger Tag entspringen aus der Nacht!", "tokens": ["Wird", "dein", "ew'\u00b7ger", "Tag", "ent\u00b7sprin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Nachtigall der Himmelsrosen, ", "tokens": ["Nach\u00b7ti\u00b7gall", "der", "Him\u00b7mels\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Liebend dich empor zu singen aus der Nacht!", "tokens": ["Lie\u00b7bend", "dich", "em\u00b7por", "zu", "sin\u00b7gen", "aus", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}