{"textgrid.poem.41069": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Versperrte Aussicht", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel! Himmel! welchen Graus", "tokens": ["Him\u00b7mel", "!", "Him\u00b7mel", "!", "wel\u00b7chen", "Graus"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich durch die Fensterscheiben!", "tokens": ["Seh'", "ich", "durch", "die", "Fens\u00b7ter\u00b7schei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer baut mir vor das Haus", "tokens": ["Ei\u00b7ner", "baut", "mir", "vor", "das", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich wollte frei doch bleiben.", "tokens": ["Und", "ich", "woll\u00b7te", "frei", "doch", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alter! la\u00df nur immerzu", "tokens": ["Al\u00b7ter", "!", "la\u00df", "nur", "im\u00b7mer\u00b7zu"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Licht und Luft und Raum dir nehmen,", "tokens": ["Licht", "und", "Luft", "und", "Raum", "dir", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald in desto gr\u00f6\u00dfrer Ruh'", "tokens": ["Bald", "in", "des\u00b7to", "gr\u00f6\u00df\u00b7rer", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Sarg dich zu bequemen.", "tokens": ["Mit", "dem", "Sarg", "dich", "zu", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}