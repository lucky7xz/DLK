{"textgrid.poem.42049": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, der du schwer auf meiner Lippe ruhst,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, der du schwer auf meiner Lippe ruhst,", "tokens": ["Gott", ",", "der", "du", "schwer", "auf", "mei\u00b7ner", "Lip\u00b7pe", "ruhst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und ist kein Tun, das du nicht tust,", "tokens": ["Und", "ist", "kein", "Tun", ",", "das", "du", "nicht", "tust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schl\u00e4fst im Ohr mir diesen tiefen Schlaf.", "tokens": ["Du", "schl\u00e4fst", "im", "Ohr", "mir", "die\u00b7sen", "tie\u00b7fen", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ich dich unter Mandelb\u00e4umen traf,", "tokens": ["Da", "ich", "dich", "un\u00b7ter", "Man\u00b7del\u00b7b\u00e4u\u00b7men", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du gingst umh\u00fcllt von roten Bl\u00fcten ganz.", "tokens": ["Du", "gingst", "um\u00b7h\u00fcllt", "von", "ro\u00b7ten", "Bl\u00fc\u00b7ten", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich stand geb\u00fcckt, ich losch in deinem Glanz,", "tokens": ["Ich", "stand", "ge\u00b7b\u00fcckt", ",", "ich", "losch", "in", "dei\u00b7nem", "Glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sank beschwert von deiner Pracht dahin.", "tokens": ["Ich", "sank", "be\u00b7schwert", "von", "dei\u00b7ner", "Pracht", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und \u00e4ngste, Hoher, dem ich nahe bin,", "tokens": ["Und", "\u00e4ngs\u00b7te", ",", "Ho\u00b7her", ",", "dem", "ich", "na\u00b7he", "bin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df ich dir mehr entfernt und steinern werde \u2013", "tokens": ["Da\u00df", "ich", "dir", "mehr", "ent\u00b7fernt", "und", "stei\u00b7nern", "wer\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "KON", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nimm doch, o Gott, mit deiner Glanzgeb\u00e4rde", "tokens": ["Nimm", "doch", ",", "o", "Gott", ",", "mit", "dei\u00b7ner", "Glanz\u00b7ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zur\u00fcck den falschen Blick, der dich verkennt.", "tokens": ["Zu\u00b7r\u00fcck", "den", "fal\u00b7schen", "Blick", ",", "der", "dich", "ver\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus dieser bunten Ferne, die dich trennt,", "tokens": ["Aus", "die\u00b7ser", "bun\u00b7ten", "Fer\u00b7ne", ",", "die", "dich", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "O kehre zur erloschnen H\u00f6hle gn\u00e4dig ein!", "tokens": ["O", "keh\u00b7re", "zur", "er\u00b7lo\u00b7schnen", "H\u00f6h\u00b7le", "gn\u00e4\u00b7dig", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie dunkeln Krug erf\u00fcllend flie\u00dft ein Wein.", "tokens": ["Wie", "dun\u00b7keln", "Krug", "er\u00b7f\u00fcl\u00b7lend", "flie\u00dft", "ein", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}