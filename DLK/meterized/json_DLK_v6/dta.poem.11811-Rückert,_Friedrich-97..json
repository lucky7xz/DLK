{"dta.poem.11811": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie gegen Morgen, wann die Nacht die Macht verlor,", "tokens": ["Wie", "ge\u00b7gen", "Mor\u00b7gen", ",", "wann", "die", "Nacht", "die", "Macht", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allm\u00e4hlich d\u00fcnner um die Sinne wird der Flor", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "d\u00fcn\u00b7ner", "um", "die", "Sin\u00b7ne", "wird", "der", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Schlummers, der dir hat die Au\u00dfenwelt verh\u00e4ngt,", "tokens": ["Des", "Schlum\u00b7mers", ",", "der", "dir", "hat", "die", "Au\u00b7\u00dfen\u00b7welt", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie nun ein zu dir sich durch die Ritzen dr\u00e4ngt;", "tokens": ["Da\u00df", "sie", "nun", "ein", "zu", "dir", "sich", "durch", "die", "Rit\u00b7zen", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "APPR", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und heller hinterm Flor schon das Bewu\u00dftseyn d\u00e4mmert", "tokens": ["Und", "hel\u00b7ler", "hin\u00b7term", "Flor", "schon", "das", "Be\u00b7wu\u00df\u00b7tseyn", "d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem was gegen Ohr und Auge dumpf dir h\u00e4mmert;", "tokens": ["Von", "dem", "was", "ge\u00b7gen", "Ohr", "und", "Au\u00b7ge", "dumpf", "dir", "h\u00e4m\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "APPR", "NN", "KON", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Wachens Bildertanz dem Traumgestaltenchor", "tokens": ["Des", "Wa\u00b7chens", "Bil\u00b7der\u00b7tanz", "dem", "Traum\u00b7ge\u00b7stal\u00b7ten\u00b7chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich mischt, bis dieser ganz in jenem sich verlor:", "tokens": ["Sich", "mischt", ",", "bis", "die\u00b7ser", "ganz", "in", "je\u00b7nem", "sich", "ver\u00b7lor", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "KOUS", "PDS", "ADV", "APPR", "PDAT", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So gegen's Ende, wann die Macht verliert das Leben,", "tokens": ["So", "ge\u00b7gen's", "En\u00b7de", ",", "wann", "die", "Macht", "ver\u00b7liert", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich der Schleier will von einem Jenseits heben,", "tokens": ["Und", "sich", "der", "Schlei\u00b7er", "will", "von", "ei\u00b7nem", "Jen\u00b7seits", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Tritt in dies Traumgewirr, das schon verworrner kreist,", "tokens": ["Tritt", "in", "dies", "Traum\u00b7ge\u00b7wirr", ",", "das", "schon", "ver\u00b7worr\u00b7ner", "kreist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDS", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von h\u00f6hrem Wachen auch ein halbverh\u00fcllter Geist;", "tokens": ["Von", "h\u00f6h\u00b7rem", "Wa\u00b7chen", "auch", "ein", "halb\u00b7ver\u00b7h\u00fcll\u00b7ter", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da\u00df mit dem Seelenaug' und mit dem Herzensohr", "tokens": ["Da\u00df", "mit", "dem", "See\u00b7le\u00b7naug'", "und", "mit", "dem", "Her\u00b7zen\u00b7sohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du siehest, h\u00f6rst, was du nicht h\u00f6rtest, sahst zuvor.", "tokens": ["Du", "sie\u00b7hest", ",", "h\u00f6rst", ",", "was", "du", "nicht", "h\u00f6r\u00b7test", ",", "sahst", "zu\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dann \u00fcberh\u00f6re nicht die leisen Ahnungen,", "tokens": ["Dann", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7re", "nicht", "die", "lei\u00b7sen", "Ah\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Von reinerm Ton und Licht die fernen Mahnungen;", "tokens": ["Von", "rei\u00b7nerm", "Ton", "und", "Licht", "die", "fer\u00b7nen", "Mah\u00b7nun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "Von einem Licht, das sich mit diesem nicht vertr\u00e4gt,", "tokens": ["Von", "ei\u00b7nem", "Licht", ",", "das", "sich", "mit", "die\u00b7sem", "nicht", "ver\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PDAT", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem Hauch, wodurch sich dieser Rauch zerschl\u00e4gt;", "tokens": ["Von", "ei\u00b7nem", "Hauch", ",", "wo\u00b7durch", "sich", "die\u00b7ser", "Rauch", "zer\u00b7schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PRF", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Von Morgenluft, die macht den Duft der Nacht zerrinnen,", "tokens": ["Von", "Mor\u00b7gen\u00b7luft", ",", "die", "macht", "den", "Duft", "der", "Nacht", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Gru\u00df, da\u00df nun Verdru\u00df mu\u00df und Genu\u00df vonhinnen.", "tokens": ["Vom", "Gru\u00df", ",", "da\u00df", "nun", "Ver\u00b7dru\u00df", "mu\u00df", "und", "Ge\u00b7nu\u00df", "von\u00b7hin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "NN", "VMFIN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Dann tr\u00e4um noch aus geschwind den Traum, der dich ergetzt,", "tokens": ["Dann", "tr\u00e4um", "noch", "aus", "ge\u00b7schwind", "den", "Traum", ",", "der", "dich", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Froh, da\u00df er so gelind sich um ins Wachen setzt.", "tokens": ["Froh", ",", "da\u00df", "er", "so", "ge\u00b7lind", "sich", "um", "ins", "Wa\u00b7chen", "setzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "ADJD", "PRF", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}