{"dta.poem.2731": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Land des Friedens/  \n Bey Beerdigung Jfr. A. M. F. den 26. Febr.  \n 1679. entworffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jn deiner Todes-Noth/ in deiner letzten Stunde/", "tokens": ["In", "dei\u00b7ner", "To\u00b7des\u00b7Noth", "/", "in", "dei\u00b7ner", "letz\u00b7ten", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Leib und Seele ringt und scheiden soll von hier/", "tokens": ["Da", "Leib", "und", "See\u00b7le", "ringt", "und", "schei\u00b7den", "soll", "von", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "KON", "VVINF", "VMFIN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Rufft dir dein Heyland zu aus seinem wahren Munde:", "tokens": ["Rufft", "dir", "dein", "Hey\u00b7land", "zu", "aus", "sei\u00b7nem", "wah\u00b7ren", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh\u2019/ ", "tokens": ["Geh'", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Kein Harff- und Lauten-Klang klingt so in deinem Ohr.", "tokens": ["Kein", "Ha\u00b7rff", "und", "Lau\u00b7ten\u00b7Klang", "klingt", "so", "in", "dei\u00b7nem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "TRUNC", "KON", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Du Himmels-werthe Braut/ was gleicht sich deinen Ehren/", "tokens": ["Du", "Him\u00b7mels\u00b7wert\u00b7he", "Braut", "/", "was", "gleicht", "sich", "dei\u00b7nen", "Eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PWS", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun dir Jerusalem schleust auff sein Frieden-Thor?", "tokens": ["Nun", "dir", "Je\u00b7ru\u00b7sa\u00b7lem", "schleust", "auff", "sein", "Frie\u00b7den\u00b7Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Geh\u2019 ein/ als Siegerin/ in keuschen Lorbeer-Kronen/", "tokens": ["Geh'", "ein", "/", "als", "Sie\u00b7ge\u00b7rin", "/", "in", "keu\u00b7schen", "Lor\u00b7beer\u00b7Kro\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$(", "KOUS", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Tritt unter deinen Fu\u00df die Hecken dieser Welt.", "tokens": ["Tritt", "un\u00b7ter", "dei\u00b7nen", "Fu\u00df", "die", "He\u00b7cken", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir m\u00fcssen noch allhier in Dorn und Disteln wohnen/", "tokens": ["Wir", "m\u00fcs\u00b7sen", "noch", "all\u00b7hier", "in", "Dorn", "und", "Dis\u00b7teln", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da du als Schwester bist den Engeln zugesellt.", "tokens": ["Da", "du", "als", "Schwes\u00b7ter", "bist", "den", "En\u00b7geln", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man sage was man wil/ es ist doch hier kein Friede/", "tokens": ["Man", "sa\u00b7ge", "was", "man", "wil", "/", "es", "ist", "doch", "hier", "kein", "Frie\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PWS", "PIS", "VMFIN", "$(", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nun sich der Erden-Krei\u00df in nichts als Blute w\u00e4scht/", "tokens": ["Nun", "sich", "der", "Er\u00b7den\u00b7Krei\u00df", "in", "nichts", "als", "Blu\u00b7te", "w\u00e4scht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "APPR", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun auch die Sonne mehr ihr Licht zu leihen m\u00fcde/", "tokens": ["Nun", "auch", "die", "Son\u00b7ne", "mehr", "ihr", "Licht", "zu", "lei\u00b7hen", "m\u00fc\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und nicht die weite See des Krieges Flammen lescht.", "tokens": ["Und", "nicht", "die", "wei\u00b7te", "See", "des", "Krie\u00b7ges", "Flam\u00b7men", "lescht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo nur von Schwerdtern klingt Sud/ Westen/ Ost und Norden", "tokens": ["Wo", "nur", "von", "Schwerd\u00b7tern", "klingt", "Sud", "/", "Wes\u00b7ten", "/", "Ost", "und", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "VVFIN", "NE", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Eyd und B\u00fcndnis nicht die grimmen Waffen h\u00e4lt.", "tokens": ["Und", "Eyd", "und", "B\u00fcnd\u00b7nis", "nicht", "die", "grim\u00b7men", "Waf\u00b7fen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo nur ein Zeit-Vertreib Verw\u00fcsten/ Brennen/ Morden/", "tokens": ["Wo", "nur", "ein", "Zeit\u00b7Ver\u00b7treib", "Ver\u00b7w\u00fcs\u00b7ten", "/", "Bren\u00b7nen", "/", "Mor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und Kron und Scepter feil umb das verdammte Geld.", "tokens": ["Und", "Kron", "und", "Scep\u00b7ter", "feil", "umb", "das", "ver\u00b7damm\u00b7te", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo/ sag ich/ nicht genung/ da\u00df gantze L\u00e4nder Leichen/", "tokens": ["Wo", "/", "sag", "ich", "/", "nicht", "ge\u00b7nung", "/", "da\u00df", "gant\u00b7ze", "L\u00e4n\u00b7der", "Lei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVIMP", "PPER", "$(", "PTKNEG", "ADV", "$(", "KOUS", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df wie ein todtes Aa\u00df die St\u00e4dte sind verheert/", "tokens": ["Da\u00df", "wie", "ein", "tod\u00b7tes", "Aa\u00df", "die", "St\u00e4d\u00b7te", "sind", "ver\u00b7heert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und da\u00df kein Platz mehr leer von solchen Jammer-Zeichen/", "tokens": ["Und", "da\u00df", "kein", "Platz", "mehr", "leer", "von", "sol\u00b7chen", "Jam\u00b7mer\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wo nicht Bellonens Spie\u00df und Blut-Fahn eingekehrt.", "tokens": ["Wo", "nicht", "Bel\u00b7lo\u00b7nens", "Spie\u00df", "und", "Blut\u00b7Fahn", "ein\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nein/ was noch \u00e4rger ist das innre Seelen-Streiten/", "tokens": ["Nein", "/", "was", "noch", "\u00e4r\u00b7ger", "ist", "das", "inn\u00b7re", "See\u00b7len\u00b7Strei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Unzufriedeuheit/ die uns zu Boden schl\u00e4gt/", "tokens": ["Die", "Un\u00b7zu\u00b7frie\u00b7deu\u00b7heit", "/", "die", "uns", "zu", "Bo\u00b7den", "schl\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist noch viel hefftiger/ als die bedr\u00e4ngten Zeiten/", "tokens": ["Ist", "noch", "viel", "heff\u00b7ti\u00b7ger", "/", "als", "die", "be\u00b7dr\u00e4ng\u00b7ten", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit wie viel Martern sie auch immermehr belegt.", "tokens": ["Mit", "wie", "viel", "Mar\u00b7tern", "sie", "auch", "im\u00b7mer\u00b7mehr", "be\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Mensch ist stets im Streit/ mu\u00df mit Begierden k\u00e4mpffen/", "tokens": ["Der", "Mensch", "ist", "stets", "im", "Streit", "/", "mu\u00df", "mit", "Be\u00b7gier\u00b7den", "k\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "$(", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Umbschantzt mit Hoffnungen/ geschreckt von Furcht u\u00f1 Qual/", "tokens": ["U\u00b7mbschantzt", "mit", "Hoff\u00b7nun\u00b7gen", "/", "ge\u00b7schreckt", "von", "Furcht", "u\u00f1", "Qual", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Und eh\u2019 er einen Feind der Reitzungen wird d\u00e4mpffen/", "tokens": ["Und", "eh'", "er", "ei\u00b7nen", "Feind", "der", "Reit\u00b7zun\u00b7gen", "wird", "d\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "So f\u00fchrt der ander ihn schon in ein Trauer-Thal.", "tokens": ["So", "f\u00fchrt", "der", "an\u00b7der", "ihn", "schon", "in", "ein", "Trau\u00b7e\u00b7rThal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auch unser Fleisch und Blut wird nicht getreu verbleiben/", "tokens": ["Auch", "un\u00b7ser", "Fleisch", "und", "Blut", "wird", "nicht", "ge\u00b7treu", "ver\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VAFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie vielmal lockt es uns blo\u00df zum Verderben an.", "tokens": ["Wie", "viel\u00b7mal", "lockt", "es", "uns", "blo\u00df", "zum", "Ver\u00b7der\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie wird uns Schmeicheley nicht auff die H\u00f6hen treiben", "tokens": ["Wie", "wird", "uns", "Schmei\u00b7che\u00b7ley", "nicht", "auff", "die", "H\u00f6\u00b7hen", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo man vor Sturtz und Fall sich schwerlich h\u00fcten kan.", "tokens": ["Wo", "man", "vor", "Sturtz", "und", "Fall", "sich", "schwer\u00b7lich", "h\u00fc\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und was hegt doch die Welt/ die schn\u00f6de Welt/ vor Frieden?", "tokens": ["Und", "was", "hegt", "doch", "die", "Welt", "/", "die", "schn\u00f6\u00b7de", "Welt", "/", "vor", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "APPR", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Den blo\u00df der Eigen-Nutz zusammen hat geleimt.", "tokens": ["Den", "blo\u00df", "der", "Ei\u00b7gen\u00b7Nutz", "zu\u00b7sam\u00b7men", "hat", "ge\u00b7leimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat Bunds-Genossen nicht verborgner Ha\u00df geschieden/", "tokens": ["Hat", "Bunds\u00b7Ge\u00b7nos\u00b7sen", "nicht", "ver\u00b7borg\u00b7ner", "Ha\u00df", "ge\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn offt nur einem Theil was \u00fcber Nacht getr\u00e4umt.", "tokens": ["Wenn", "offt", "nur", "ei\u00b7nem", "Theil", "was", "\u00fc\u00b7ber", "Nacht", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "PRELS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drumb wohl dem! wer da kan aus diesen Landen eilen/", "tokens": ["Drumb", "wohl", "dem", "!", "wer", "da", "kan", "aus", "die\u00b7sen", "Lan\u00b7den", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "$.", "PWS", "ADV", "VMFIN", "APPR"