{"dta.poem.9643": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bauern alle vier zusammen thun einen  \n wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lassen wir den himmel walten/", "tokens": ["So", "las\u00b7sen", "wir", "den", "him\u00b7mel", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie seiner allmacht es gef\u00e4llt;", "tokens": ["Wie", "sei\u00b7ner", "all\u00b7macht", "es", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn wir die herrschafft nur behalten/", "tokens": ["Wenn", "wir", "die", "herr\u00b7schafft", "nur", "be\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind wir die gl\u00fccklichsten der welt.", "tokens": ["Sind", "wir", "die", "gl\u00fcck\u00b7lichs\u00b7ten", "der", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ART", "NN", "$."], "meter": "++-+---+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wir w\u00fcnschen ihnen langes leben/", "tokens": ["Wir", "w\u00fcn\u00b7schen", "ih\u00b7nen", "lan\u00b7ges", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihrem hohen werthen gast/", "tokens": ["Und", "ih\u00b7rem", "ho\u00b7hen", "wert\u00b7hen", "gast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df GOtt ihm ein Gemahl mag geben/", "tokens": ["Da\u00df", "Gott", "ihm", "ein", "Ge\u00b7mahl", "mag", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie du/ ", "tokens": ["Wie", "du", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}}}}}