{"textgrid.poem.60762": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief ins Herz mir Feuerbr\u00e4nde", "tokens": ["Tief", "ins", "Herz", "mir", "Feu\u00b7er\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werfen deine sch\u00f6nen H\u00e4nde!", "tokens": ["Wer\u00b7fen", "dei\u00b7ne", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen Erd und Himmel kenn ich", "tokens": ["Zwi\u00b7schen", "Erd", "und", "Him\u00b7mel", "kenn", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine liebern Gegenst\u00e4nde:", "tokens": ["Kei\u00b7ne", "lie\u00b7bern", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber diese k\u00f6nnten Dichter", "tokens": ["\u00dc\u00b7ber", "die\u00b7se", "k\u00f6nn\u00b7ten", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreiben hunderttausend B\u00e4nde!", "tokens": ["Schrei\u00b7ben", "hun\u00b7dert\u00b7tau\u00b7send", "B\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Pf\u00e4nder sind sie deiner N\u00e4he,", "tokens": ["Pf\u00e4n\u00b7der", "sind", "sie", "dei\u00b7ner", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denen ich das Herz verpf\u00e4nde.", "tokens": ["De\u00b7nen", "ich", "das", "Herz", "ver\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sie keusche Rosen pfl\u00fccken", "tokens": ["Wenn", "sie", "keu\u00b7sche", "Ro\u00b7sen", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "L\u00e4ngs der gr\u00fcnen Gartenw\u00e4nde,", "tokens": ["L\u00e4ngs", "der", "gr\u00fc\u00b7nen", "Gar\u00b7ten\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "M\u00f6cht ich selbst zur Rose werden,", "tokens": ["M\u00f6cht", "ich", "selbst", "zur", "Ro\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df ich ihren Druck empf\u00e4nde!", "tokens": ["Da\u00df", "ich", "ih\u00b7ren", "Druck", "em\u00b7pf\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}