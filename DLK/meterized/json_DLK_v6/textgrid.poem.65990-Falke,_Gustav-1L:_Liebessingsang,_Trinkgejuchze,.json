{"textgrid.poem.65990": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebessingsang, Trinkgejuchze,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebessingsang, Trinkgejuchze,", "tokens": ["Lie\u00b7bes\u00b7sings\u00b7ang", ",", "Trink\u00b7ge\u00b7juch\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ppische Poeterei!", "tokens": ["L\u00e4p\u00b7pi\u00b7sche", "Poe\u00b7te\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nicht dies Nachtigallgeschluchze", "tokens": ["Nicht", "dies", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7schluch\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, nur einen Adlerschrei!", "tokens": ["O", ",", "nur", "ei\u00b7nen", "Ad\u00b7ler\u00b7schrei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, nur einen vollen, wahren", "tokens": ["O", ",", "nur", "ei\u00b7nen", "vol\u00b7len", ",", "wah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ton aus tiefster Brust, davor", "tokens": ["Ton", "aus", "tiefs\u00b7ter", "Brust", ",", "da\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir erschreckt zusammenfahren,", "tokens": ["Wir", "er\u00b7schreckt", "zu\u00b7sam\u00b7men\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht den zahmen Gimpelchor.", "tokens": ["Nicht", "den", "zah\u00b7men", "Gim\u00b7pel\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch das zwitschert wie im Bauer", "tokens": ["Doch", "das", "zwit\u00b7schert", "wie", "im", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bl\u00f6de Dompfaffmelodei:", "tokens": ["Bl\u00f6\u00b7de", "Dom\u00b7pfaff\u00b7me\u00b7lo\u00b7dei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde Wehmut, s\u00fc\u00dfe Trauer,", "tokens": ["Hol\u00b7de", "Weh\u00b7mut", ",", "s\u00fc\u00b7\u00dfe", "Trau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, nur einen Adlerschrei!", "tokens": ["O", ",", "nur", "ei\u00b7nen", "Ad\u00b7ler\u00b7schrei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}