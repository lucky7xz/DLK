{"textgrid.poem.66071": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Meinem Kinde", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00e4fst, und sachte neig ich mich", "tokens": ["Du", "schl\u00e4fst", ",", "und", "sach\u00b7te", "neig", "ich", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcber dein Bettchen und segne dich.", "tokens": ["\u00dc\u00b7ber", "dein", "Bett\u00b7chen", "und", "seg\u00b7ne", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jeder behutsame Atemzug", "tokens": ["Je\u00b7der", "be\u00b7hut\u00b7sa\u00b7me", "A\u00b7tem\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ist ein schweifender Himmelsflug,", "tokens": ["Ist", "ein", "schwei\u00b7fen\u00b7der", "Him\u00b7mels\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ist ein Suchen weit umher,", "tokens": ["Ist", "ein", "Su\u00b7chen", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob nicht doch ein Sternlein w\u00e4r,", "tokens": ["Ob", "nicht", "doch", "ein", "Stern\u00b7lein", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo aus eitel Glanz und Licht", "tokens": ["Wo", "aus", "ei\u00b7tel", "Glanz", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebe sich ein Gl\u00fcckskraut bricht,", "tokens": ["Lie\u00b7be", "sich", "ein", "Gl\u00fccks\u00b7kraut", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das sie gefl\u00fcgelt herniedertr\u00e4gt", "tokens": ["Das", "sie", "ge\u00b7fl\u00fc\u00b7gelt", "her\u00b7nie\u00b7der\u00b7tr\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und dir aufs wei\u00dfe Deckchen legt.", "tokens": ["Und", "dir", "aufs", "wei\u00b7\u00dfe", "Deck\u00b7chen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}