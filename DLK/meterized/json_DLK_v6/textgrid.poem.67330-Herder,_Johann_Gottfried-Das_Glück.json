{"textgrid.poem.67330": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht knie' ich vor der blinden G\u00f6ttin Wagen,", "tokens": ["Nicht", "knie'", "ich", "vor", "der", "blin\u00b7den", "G\u00f6t\u00b7tin", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, Kronen streuend, dort mit schwarzen Rossen f\u00e4hrt;", "tokens": ["Die", ",", "Kro\u00b7nen", "streu\u00b7end", ",", "dort", "mit", "schwar\u00b7zen", "Ros\u00b7sen", "f\u00e4hrt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch Jene, die ein Rad und leichte Fl\u00fcgel tragen.", "tokens": ["Auch", "Je\u00b7ne", ",", "die", "ein", "Rad", "und", "leich\u00b7te", "Fl\u00fc\u00b7gel", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "ART", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist eines trauenden Gebets nicht werth.", "tokens": ["Ist", "ei\u00b7nes", "trau\u00b7en\u00b7den", "Ge\u00b7bets", "nicht", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Gl\u00fcck sei Sie, die mit der Weisheit thronet,", "tokens": ["Mein", "Gl\u00fcck", "sei", "Sie", ",", "die", "mit", "der", "Weis\u00b7heit", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Ruder th\u00e4tiger Vernunft in ihrer Hand,", "tokens": ["Das", "Ru\u00b7der", "th\u00e4\u00b7ti\u00b7ger", "Ver\u00b7nunft", "in", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie, die dem stillen Flei\u00df, der mit sich selber wohnet,", "tokens": ["Sie", ",", "die", "dem", "stil\u00b7len", "Flei\u00df", ",", "der", "mit", "sich", "sel\u00b7ber", "woh\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die trefflichsten der Gaben zuerkannt.", "tokens": ["Die", "treff\u00b7lichs\u00b7ten", "der", "Ga\u00b7ben", "zu\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Aus reichem F\u00fcllhorn schenket sie ihm Fr\u00fcchte,", "tokens": ["Aus", "rei\u00b7chem", "F\u00fcll\u00b7horn", "schen\u00b7ket", "sie", "ihm", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihm sein eigener gesunder Muth gew\u00e4hrt;", "tokens": ["Die", "ihm", "sein", "ei\u00b7ge\u00b7ner", "ge\u00b7sun\u00b7der", "Muth", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die sch\u00f6nste Perle blinkt auf seinem Angesichte,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Per\u00b7le", "blinkt", "auf", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der M\u00fche Lohn, o mehr als Kronen werth!", "tokens": ["Der", "M\u00fc\u00b7he", "Lohn", ",", "o", "mehr", "als", "Kro\u00b7nen", "werth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "FM", "ADV", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie ist's, die t\u00e4glich ihm auch Blumen streuet,", "tokens": ["Sie", "ist's", ",", "die", "t\u00e4g\u00b7lich", "ihm", "auch", "Blu\u00b7men", "streu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADJD", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seiner Kinder Schaar h\u00fcpft sammelnd um den Thron", "tokens": ["Und", "sei\u00b7ner", "Kin\u00b7der", "Schaar", "h\u00fcpft", "sam\u00b7melnd", "um", "den", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geberin; er nimmt aus ihrer Hand, erfreuet,", "tokens": ["Der", "Ge\u00b7be\u00b7rin", ";", "er", "nimmt", "aus", "ih\u00b7rer", "Hand", ",", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Blumen viel, zuletzt den sanften Mohn;", "tokens": ["Der", "Blu\u00b7men", "viel", ",", "zu\u00b7letzt", "den", "sanf\u00b7ten", "Mohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der bringt ambrosisch ihm gesunden Schlummer,", "tokens": ["Der", "bringt", "am\u00b7bro\u00b7sisch", "ihm", "ge\u00b7sun\u00b7den", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schlummer, den das Rad der Rastlosen nicht kennt.", "tokens": ["Den", "Schlum\u00b7mer", ",", "den", "das", "Rad", "der", "Rast\u00b7lo\u00b7sen", "nicht", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Statt Perlen streuet Die oft Thr\u00e4nen; Neid und Kummer", "tokens": ["Statt", "Per\u00b7len", "streu\u00b7et", "Die", "oft", "Thr\u00e4\u00b7nen", ";", "Neid", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ART", "ADV", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind von dem gelben Golde kaum getrennt.", "tokens": ["Sind", "von", "dem", "gel\u00b7ben", "Gol\u00b7de", "kaum", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Schwester Du der Klugheit und der Treue,", "tokens": ["O", "Schwes\u00b7ter", "Du", "der", "Klug\u00b7heit", "und", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du r\u00fcckw\u00e4rts Schauende, mein Jugendgl\u00fcck,", "tokens": ["Du", "r\u00fcck\u00b7w\u00e4rts", "Schau\u00b7en\u00b7de", ",", "mein", "Ju\u00b7gend\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, meine Zeit (Du siehst, Du siehst, wem ich sie weihe!),", "tokens": ["Ach", ",", "mei\u00b7ne", "Zeit", "(", "Du", "siehst", ",", "Du", "siehst", ",", "wem", "ich", "sie", "wei\u00b7he", "!", ")", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich selbst, o Gute, gieb mir nur zur\u00fcck!", "tokens": ["Mich", "selbst", ",", "o", "Gu\u00b7te", ",", "gieb", "mir", "nur", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}