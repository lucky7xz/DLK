{"textgrid.poem.66067": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft, ich hab dich lieb gehabt,", "tokens": ["Du", "wei\u00dft", ",", "ich", "hab", "dich", "lieb", "ge\u00b7habt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer gleich, an jedem Tag,", "tokens": ["Und", "im\u00b7mer", "gleich", ",", "an", "je\u00b7dem", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ich ein wenig Gl\u00fcck uns fing,", "tokens": ["Ob", "ich", "ein", "we\u00b7nig", "Gl\u00fcck", "uns", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob still in Sorgen abseits ging.", "tokens": ["Ob", "still", "in", "Sor\u00b7gen", "ab\u00b7seits", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kam ein Fr\u00fchlingssonnenschein", "tokens": ["Da", "kam", "ein", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kam ein junger Rosentag,", "tokens": ["Und", "kam", "ein", "jun\u00b7ger", "Ro\u00b7sen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stand in lauter Rausch und Traum", "tokens": ["Ich", "stand", "in", "lau\u00b7ter", "Rausch", "und", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An eines fremden Gartens Saum.", "tokens": ["An", "ei\u00b7nes", "frem\u00b7den", "Gar\u00b7tens", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus holder Morgenlieblichkeit", "tokens": ["Aus", "hol\u00b7der", "Mor\u00b7gen\u00b7lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klang da ein Lied, so s\u00fc\u00df, so s\u00fc\u00df,", "tokens": ["Klang", "da", "ein", "Lied", ",", "so", "s\u00fc\u00df", ",", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich im Lauschen mich verlor", "tokens": ["Da\u00df", "ich", "im", "Lau\u00b7schen", "mich", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hatt f\u00fcr deinen Ruf kein Ohr.", "tokens": ["Und", "hatt", "f\u00fcr", "dei\u00b7nen", "Ruf", "kein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch gab des Gartens T\u00fcr nicht nach,", "tokens": ["Doch", "gab", "des", "Gar\u00b7tens", "T\u00fcr", "nicht", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein zweifach Schl\u00f6\u00dflein lag davor,", "tokens": ["Ein", "zwei\u00b7fach", "Schl\u00f6\u00df\u00b7lein", "lag", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hat den Tr\u00e4umer aufgeweckt,", "tokens": ["Das", "hat", "den", "Tr\u00e4u\u00b7mer", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn auf sich selbst zur\u00fcckgeschreckt.", "tokens": ["Ihn", "auf", "sich", "selbst", "zu\u00b7r\u00fcck\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er ri\u00df sich los und kehrt nun heim", "tokens": ["Er", "ri\u00df", "sich", "los", "und", "kehrt", "nun", "heim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00e4ngt sein Herz an deines hin.", "tokens": ["Und", "dr\u00e4ngt", "sein", "Herz", "an", "dei\u00b7nes", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotz Rausch und Traum, du f\u00fchlst, es blieb", "tokens": ["Trotz", "Rausch", "und", "Traum", ",", "du", "f\u00fchlst", ",", "es", "blieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das alte Herz und hat dich lieb.", "tokens": ["Das", "al\u00b7te", "Herz", "und", "hat", "dich", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}