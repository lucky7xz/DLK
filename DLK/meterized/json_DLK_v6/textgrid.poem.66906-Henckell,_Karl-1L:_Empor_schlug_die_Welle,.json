{"textgrid.poem.66906": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Empor schlug die Welle,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Empor schlug die Welle,", "tokens": ["Em\u00b7por", "schlug", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Wogen der Wahrheit", "tokens": ["Die", "Wo\u00b7gen", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Erfassen die Menschen,", "tokens": ["Er\u00b7fas\u00b7sen", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Erobern das Reich.", "tokens": ["Er\u00b7o\u00b7bern", "das", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sie sind aus den Tiefen", "tokens": ["Sie", "sind", "aus", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Lebens gestiegen,", "tokens": ["Des", "Le\u00b7bens", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie brausen zusammen,", "tokens": ["Sie", "brau\u00b7sen", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gewaltig gestaut.", "tokens": ["Ge\u00b7wal\u00b7tig", "ge\u00b7staut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Es wanken die Sessel,", "tokens": ["Es", "wan\u00b7ken", "die", "Ses\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es brechen die S\u00e4ulen,", "tokens": ["Es", "bre\u00b7chen", "die", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Schl\u00e4fer erwachen,", "tokens": ["Die", "Schl\u00e4\u00b7fer", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die M\u00fcden stehn auf.", "tokens": ["Die", "M\u00fc\u00b7den", "stehn", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nun gilt es zu bauen", "tokens": ["Nun", "gilt", "es", "zu", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein starkes Geb\u00e4ude,", "tokens": ["Ein", "star\u00b7kes", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun gilt es zu bilden", "tokens": ["Nun", "gilt", "es", "zu", "bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein stattliches Werk.", "tokens": ["Ein", "statt\u00b7li\u00b7ches", "Werk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "So schlagt mit dem Hammer,", "tokens": ["So", "schlagt", "mit", "dem", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So haut mit dem Mei\u00dfel,", "tokens": ["So", "haut", "mit", "dem", "Mei\u00b7\u00dfel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit K\u00fchnheit besonnen", "tokens": ["Mit", "K\u00fchn\u00b7heit", "be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Errichtet den Bau!", "tokens": ["Er\u00b7rich\u00b7tet", "den", "Bau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Da\u00df weit allem Volke", "tokens": ["Da\u00df", "weit", "al\u00b7lem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PIS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Wohnstatt sich w\u00f6lbe,", "tokens": ["Die", "Wohn\u00b7statt", "sich", "w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf freierem Grunde", "tokens": ["Auf", "frei\u00b7e\u00b7rem", "Grun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gerechter erh\u00f6ht.", "tokens": ["Ge\u00b7rech\u00b7ter", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Seid eins in der Liebe,", "tokens": ["Seid", "eins", "in", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu schlichten den Hader,", "tokens": ["Zu", "schlich\u00b7ten", "den", "Ha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Durch Zwietracht untrennbar", "tokens": ["Durch", "Zwiet\u00b7racht", "un\u00b7trenn\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seid einig \u2013 und schafft!", "tokens": ["Seid", "ei\u00b7nig", "\u2013", "und", "schafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Gesetze zu schmieden,", "tokens": ["Ge\u00b7set\u00b7ze", "zu", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sei Weisheit beschieden,", "tokens": ["Sei", "Weis\u00b7heit", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch Sch\u00f6nheit durchschimmre", "tokens": ["Doch", "Sch\u00f6n\u00b7heit", "durch\u00b7schimm\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die S\u00e4ulen der Kraft!", "tokens": ["Die", "S\u00e4u\u00b7len", "der", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}