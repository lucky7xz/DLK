{"textgrid.poem.51185": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kennt ihr den Kleinkinderhimmel,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr den Kleinkinderhimmel,", "tokens": ["Kennt", "ihr", "den", "Klein\u00b7kin\u00b7der\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo als Gott der Zuckerb\u00e4cker", "tokens": ["Wo", "als", "Gott", "der", "Zu\u00b7cker\u00b7b\u00e4\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "KOKOM", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waltet s\u00fc\u00df und hoch und herrlich", "tokens": ["Wal\u00b7tet", "s\u00fc\u00df", "und", "hoch", "und", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Augen kleiner Schlecker?", "tokens": ["In", "den", "Au\u00b7gen", "klei\u00b7ner", "Schle\u00b7cker", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und zur Weihnachtszeit, wie flimmert,", "tokens": ["Und", "zur", "Weih\u00b7nachts\u00b7zeit", ",", "wie", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PWAV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Duftet es an allen W\u00e4nden!", "tokens": ["Duf\u00b7tet", "es", "an", "al\u00b7len", "W\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welchen Schatz von Seligkeiten", "tokens": ["Wel\u00b7chen", "Schatz", "von", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttet er aus m\u00e4cht'gen H\u00e4nden!", "tokens": ["Sch\u00fct\u00b7tet", "er", "aus", "m\u00e4cht'\u00b7gen", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4\u00dft erbl\u00fchen Wunderblumen,", "tokens": ["L\u00e4\u00dft", "er\u00b7bl\u00fc\u00b7hen", "Wun\u00b7der\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weise streut er die Gew\u00fcrze;", "tokens": ["Wei\u00b7se", "streut", "er", "die", "Ge\u00b7w\u00fcr\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n stehn ihm die hohe, wei\u00dfe", "tokens": ["Sch\u00f6n", "stehn", "ihm", "die", "ho\u00b7he", ",", "wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zipfelm\u00fctze, Wams und Sch\u00fcrze.", "tokens": ["Zip\u00b7fel\u00b7m\u00fct\u00b7ze", ",", "Wams", "und", "Sch\u00fcr\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wonach die guten Kinder", "tokens": ["Doch", "wo\u00b7nach", "die", "gu\u00b7ten", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schmachtend vor dem Laden stehen,", "tokens": ["Schmach\u00b7tend", "vor", "dem", "La\u00b7den", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df dem Reichen, Allgewalt'gen", "tokens": ["Mu\u00df", "dem", "Rei\u00b7chen", ",", "All\u00b7ge\u00b7walt'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reizlos durch die H\u00e4nde gehen.", "tokens": ["Reiz\u00b7los", "durch", "die", "H\u00e4n\u00b7de", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einmal kaum im Jahr genie\u00dft er", "tokens": ["Ein\u00b7mal", "kaum", "im", "Jahr", "ge\u00b7nie\u00dft", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus Zerstreuung in dem Handel", "tokens": ["Aus", "Zer\u00b7streu\u00b7ung", "in", "dem", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchtig ein gefehltes T\u00f6rtchen", "tokens": ["Fl\u00fcch\u00b7tig", "ein", "ge\u00b7fehl\u00b7tes", "T\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ver\u00e4chtlich eine Mandel.", "tokens": ["Und", "ver\u00b7\u00e4cht\u00b7lich", "ei\u00b7ne", "Man\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zipfelm\u00fctze, wei\u00dfe Sch\u00fcrze,", "tokens": ["Zip\u00b7fel\u00b7m\u00fct\u00b7ze", ",", "wei\u00b7\u00dfe", "Sch\u00fcr\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie n\u00fcchtern gl\u00e4nzet ihr,", "tokens": ["O", "wie", "n\u00fcch\u00b7tern", "gl\u00e4n\u00b7zet", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie mahnt ihr mich an wei\u00dfes,", "tokens": ["Und", "wie", "mahnt", "ihr", "mich", "an", "wei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reinliches Konzeptpapier!", "tokens": ["Rein\u00b7li\u00b7ches", "Kon\u00b7zept\u00b7pa\u00b7pier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}