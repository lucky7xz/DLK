{"dta.poem.11743": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ist ein schlechter Herr, wie gl\u00e4nzend auch er thront,", "tokens": ["Der", "ist", "ein", "schlech\u00b7ter", "Herr", ",", "wie", "gl\u00e4n\u00b7zend", "auch", "er", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der besser m\u00fcssigem als flei\u00df'gem Diener lohnt;", "tokens": ["Der", "bes\u00b7ser", "m\u00fcs\u00b7si\u00b7gem", "als", "flei\u00df'\u00b7gem", "Die\u00b7ner", "lohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der, wie die Sonne, sticht den der im Feld arbeitet,", "tokens": ["Der", ",", "wie", "die", "Son\u00b7ne", ",", "sticht", "den", "der", "im", "Feld", "ar\u00b7bei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freundlich scheinet dem, der sich im Schatten breitet.", "tokens": ["Und", "freund\u00b7lich", "schei\u00b7net", "dem", ",", "der", "sich", "im", "Schat\u00b7ten", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "