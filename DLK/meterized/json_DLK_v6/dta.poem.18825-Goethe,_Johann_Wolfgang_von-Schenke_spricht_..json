{"dta.poem.18825": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Schenke spricht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Du, mit deinen braunen Locken,", "tokens": ["Du", ",", "mit", "dei\u00b7nen", "brau\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh\u2019 mir weg verschmitzte Dirne!", "tokens": ["Geh'", "mir", "weg", "ver\u00b7schmitz\u00b7te", "Dir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schenk\u2019 ich meinem Herrn zu Danke,", "tokens": ["Schenk'", "ich", "mei\u00b7nem", "Herrn", "zu", "Dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PTKANT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so k\u00fcsst er mir die Stirne.", "tokens": ["Nun", "so", "k\u00fcsst", "er", "mir", "die", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber du, ich wollte wetten,", "tokens": ["A\u00b7ber", "du", ",", "ich", "woll\u00b7te", "wet\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist mir nicht damit zufrieden,", "tokens": ["Bist", "mir", "nicht", "da\u00b7mit", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Wangen, deine Br\u00fcste", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", ",", "dei\u00b7ne", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden meinen Freund erm\u00fcden.", "tokens": ["Wer\u00b7den", "mei\u00b7nen", "Freund", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Glaubst du wohl mich zu betr\u00fcgen", "tokens": ["Glaubst", "du", "wohl", "mich", "zu", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass du jetzt versch\u00e4mt entweichest?", "tokens": ["Dass", "du", "jetzt", "ver\u00b7sch\u00e4mt", "ent\u00b7wei\u00b7chest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Schwelle will ich liegen", "tokens": ["Auf", "der", "Schwel\u00b7le", "will", "ich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erwachen wenn du schleichest.", "tokens": ["Und", "er\u00b7wa\u00b7chen", "wenn", "du", "schlei\u00b7chest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}