{"dta.poem.7159": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hast Du Grosses ALL\u2019 auch mich", "tokens": ["Wie", "hast", "Du", "Gros\u00b7ses", "AlL'", "auch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJA", "NN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm abgewichnen Jahr absonderlich", "tokens": ["Jm", "ab\u00b7ge\u00b7wich\u00b7nen", "Jahr", "ab\u00b7son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So mancher Bitte doch gew\u00e4hret,", "tokens": ["So", "man\u00b7cher", "Bit\u00b7te", "doch", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und so viel Gutes mir bescheret.", "tokens": ["Und", "so", "viel", "Gu\u00b7tes", "mir", "be\u00b7sche\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ob mir gleich im vor\u2019gen Jahr", "tokens": ["Denn", "ob", "mir", "gleich", "im", "vor'\u00b7gen", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein pl\u00f6tzlich Ungl\u00fcck \u00fcberkam,", "tokens": ["Ein", "pl\u00f6tz\u00b7lich", "Un\u00b7gl\u00fcck", "\u00fc\u00b7ber\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da ich im Fallen Schaden nahm,", "tokens": ["Da", "ich", "im", "Fal\u00b7len", "Scha\u00b7den", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mir von meinem Arm so gar,", "tokens": ["Und", "mir", "von", "mei\u00b7nem", "Arm", "so", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie starck er gleich, der obre Knochen", "tokens": ["Wie", "starck", "er", "gleich", ",", "der", "ob\u00b7re", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dennoch entzwey gebrochen:", "tokens": ["Den\u00b7noch", "ent\u00b7zwey", "ge\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So hab\u2019 ich doch, o Sch\u00f6pffer! Deine Huld,", "tokens": ["So", "hab'", "ich", "doch", ",", "o", "Sch\u00f6pf\u00b7fer", "!", "Dei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auch selbst in diesem Stand\u2019, empfunden.", "tokens": ["Auch", "selbst", "in", "die\u00b7sem", "Stand'", ",", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Du schencktest mir Bequemlichkeit,", "tokens": ["Du", "schenck\u00b7test", "mir", "Be\u00b7quem\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gelassenheit, Geduld,", "tokens": ["Ge\u00b7las\u00b7sen\u00b7heit", ",", "Ge\u00b7duld", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Du lindertest, sowol wenn ich verbunden,", "tokens": ["Du", "lin\u00b7der\u00b7test", ",", "so\u00b7wol", "wenn", "ich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Als sonst, der Schmertzen Hefftigkeit:", "tokens": ["Als", "sonst", ",", "der", "Schmert\u00b7zen", "Heff\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So, da\u00df ich, weil der Bruch gantz rein;", "tokens": ["So", ",", "da\u00df", "ich", ",", "weil", "der", "Bruch", "gantz", "rein", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Fast keine Pein,", "tokens": ["Fast", "kei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wenn ich verbunden war, gef\u00fchlt. ", "tokens": ["Wenn", "ich", "ver\u00b7bun\u00b7den", "war", ",", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Liebreicher GOTT, Du bester Artzt, daf\u00fcr,", "tokens": ["Lieb\u00b7rei\u00b7cher", "GoTT", ",", "Du", "bes\u00b7ter", "Artzt", ",", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Da\u00df Du so v\u00e4terlich Dich meiner angenommen!", "tokens": ["Da\u00df", "Du", "so", "v\u00e4\u00b7ter\u00b7lich", "Dich", "mei\u00b7ner", "an\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}