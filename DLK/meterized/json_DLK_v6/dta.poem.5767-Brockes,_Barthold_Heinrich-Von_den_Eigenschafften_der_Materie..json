{"dta.poem.5767": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man theile sie darauf in tausend Theil aufs neu.", "tokens": ["Man", "thei\u00b7le", "sie", "da\u00b7rauf", "in", "tau\u00b7send", "Theil", "aufs", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "APPR", "CARD", "NN", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der allerkleinste Theil, den man erdencken kan,", "tokens": ["Der", "al\u00b7ler\u00b7kleins\u00b7te", "Theil", ",", "den", "man", "er\u00b7den\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist eben den Gesetzen unterthan;", "tokens": ["Ist", "e\u00b7ben", "den", "Ge\u00b7set\u00b7zen", "un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man merckt, da\u00df der auch fast unendlich theilbar sey.", "tokens": ["Man", "merckt", ",", "da\u00df", "der", "auch", "fast", "un\u00b7end\u00b7lich", "theil\u00b7bar", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "ADV", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von allen diesen Theilen nun", "tokens": ["Von", "al\u00b7len", "die\u00b7sen", "Thei\u00b7len", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entsteht ein Schlu\u00df,", "tokens": ["Ent\u00b7steht", "ein", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den jederman gestehen mu\u00df,", "tokens": ["Den", "je\u00b7der\u00b7man", "ge\u00b7ste\u00b7hen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df auch darinn so viel Figuren ruhn.", "tokens": ["Da\u00df", "auch", "da\u00b7rinn", "so", "viel", "Fi\u00b7gu\u00b7ren", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und ob man gleich nicht deutlich zeigen kan,", "tokens": ["Und", "ob", "man", "gleich", "nicht", "deut\u00b7lich", "zei\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie die Particuln die so klein,", "tokens": ["Wie", "die", "Par\u00b7ti\u00b7culn", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADV", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "In sich selbst unterschieden seyn;", "tokens": ["In", "sich", "selbst", "un\u00b7ter\u00b7schie\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So trifft jedennoch jedermann", "tokens": ["So", "trifft", "je\u00b7den\u00b7noch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIS"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Unleugbar die Figur in einer jeden an.", "tokens": ["Un\u00b7leug\u00b7bar", "die", "Fi\u00b7gur", "in", "ei\u00b7ner", "je\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "PIAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}