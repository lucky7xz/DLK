{"textgrid.poem.44095": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wird es doch nur weiter gehn?", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird es doch nur weiter gehn?", "tokens": ["Wie", "wird", "es", "doch", "nur", "wei\u00b7ter", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dieses mag bey Seite stehn,", "tokens": ["Doch", "die\u00b7ses", "mag", "bey", "Sei\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich w\u00fcntsch es warlich nicht zu wi\u00dfen;", "tokens": ["Ich", "w\u00fcnt\u00b7sch", "es", "war\u00b7lich", "nicht", "zu", "wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist Gott mein Gott und ich sein Knecht,", "tokens": ["Ist", "Gott", "mein", "Gott", "und", "ich", "sein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "KON", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kan ich zum Voraus mit Recht", "tokens": ["So", "kan", "ich", "zum", "Vo\u00b7raus", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ein immer heilsam Gl\u00fccke schlie\u00dfen.", "tokens": ["Ein", "im\u00b7mer", "heil\u00b7sam", "Gl\u00fc\u00b7cke", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es bl\u00fcht nicht stets ein Rosenfeld;", "tokens": ["Es", "bl\u00fcht", "nicht", "stets", "ein", "Ro\u00b7sen\u00b7feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warum w\u00e4r es auch die Welt?", "tokens": ["Und", "wa\u00b7rum", "w\u00e4r", "es", "auch", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Wesen kommt vom Unbestande.", "tokens": ["Ihr", "We\u00b7sen", "kommt", "vom", "Un\u00b7be\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Vorsicht theilt ja Lust und Pein", "tokens": ["Die", "Vor\u00b7sicht", "theilt", "ja", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu unserm eignen Vortheil ein,", "tokens": ["Zu", "un\u00b7serm", "eig\u00b7nen", "Vor\u00b7theil", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit der Mensch in keinem strande.", "tokens": ["Da\u00b7mit", "der", "Mensch", "in", "kei\u00b7nem", "stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur so viel bitt ich von der Macht,", "tokens": ["Nur", "so", "viel", "bitt", "ich", "von", "der", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen Zeug hervorgebracht,", "tokens": ["Die", "mei\u00b7nen", "Zeug", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Welt mit meiner Pflicht zu gr\u00fcnen;", "tokens": ["Der", "Welt", "mit", "mei\u00b7ner", "Pflicht", "zu", "gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gebe Mittel an die Hand,", "tokens": ["Sie", "ge\u00b7be", "Mit\u00b7tel", "an", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit mein Wi\u00dfen und Verstand", "tokens": ["Da\u00b7mit", "mein", "Wi\u00b7\u00dfen", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Nechsten einmahl gl\u00fccklich dienen.", "tokens": ["Dem", "Nechs\u00b7ten", "ein\u00b7mahl", "gl\u00fcck\u00b7lich", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin bisher gar scharf versucht;", "tokens": ["Ich", "bin", "bis\u00b7her", "gar", "scharf", "ver\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bittre Bl\u00fcthe, s\u00fc\u00dfe Frucht,", "tokens": ["Doch", "bitt\u00b7re", "Bl\u00fc\u00b7the", ",", "s\u00fc\u00b7\u00dfe", "Frucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Leiden kommt der Flei\u00df zu Ehren.", "tokens": ["Durch", "Lei\u00b7den", "kommt", "der", "Flei\u00df", "zu", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Glaubt Neider, die ihr jezt so schreyt,", "tokens": ["Glaubt", "Nei\u00b7der", ",", "die", "ihr", "jezt", "so", "schreyt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr werdet einmahl weit und breit", "tokens": ["Ihr", "wer\u00b7det", "ein\u00b7mahl", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Vortheil meines Ungl\u00fccks h\u00f6ren.", "tokens": ["Den", "Vor\u00b7theil", "mei\u00b7nes", "Un\u00b7gl\u00fccks", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf dich, mein Gott, kommt alles an,", "tokens": ["Auf", "dich", ",", "mein", "Gott", ",", "kommt", "al\u00b7les", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viel ich w\u00fcntschen soll und kan;", "tokens": ["So", "viel", "ich", "w\u00fcnt\u00b7schen", "soll", "und", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst der Noth bald Gr\u00e4nzen sezen.", "tokens": ["Du", "wirst", "der", "Noth", "bald", "Gr\u00e4n\u00b7zen", "se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekehre mich, hernach den Feind,", "tokens": ["Be\u00b7keh\u00b7re", "mich", ",", "her\u00b7nach", "den", "Feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und las mich, wo es rathsam scheint,", "tokens": ["Und", "las", "mich", ",", "wo", "es", "rath\u00b7sam", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von nun an Trost und Ruh erg\u00f6zen.", "tokens": ["Von", "nun", "an", "Trost", "und", "Ruh", "er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein t\u00e4glich Creuz verschm\u00e4h ich nicht,", "tokens": ["Ein", "t\u00e4g\u00b7lich", "Creuz", "ver\u00b7schm\u00e4h", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es weckt nur meine Christenpflicht;", "tokens": ["Es", "weckt", "nur", "mei\u00b7ne", "Chris\u00b7ten\u00b7pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur dieses bitt ich deine G\u00fcte:", "tokens": ["Nur", "die\u00b7ses", "bitt", "ich", "dei\u00b7ne", "G\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Errette mich aus dieser Schmach,", "tokens": ["Er\u00b7ret\u00b7te", "mich", "aus", "die\u00b7ser", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schlepp ich dann mein Creuz dir nach,", "tokens": ["Und", "schlepp", "ich", "dann", "mein", "Creuz", "dir", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gieb mir stets ein froh Gem\u00fcthe.", "tokens": ["So", "gieb", "mir", "stets", "ein", "froh", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So oft ich in Gedancken steh", "tokens": ["So", "oft", "ich", "in", "Ge\u00b7dan\u00b7cken", "steh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fcckw\u00e4rts in die Jahre geh,", "tokens": ["Und", "r\u00fcck\u00b7w\u00e4rts", "in", "die", "Jah\u00b7re", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir, ich weis nicht wie, vergangen,", "tokens": ["Die", "mir", ",", "ich", "weis", "nicht", "wie", ",", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "PTKVZ", "PTKNEG", "KOKOM", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So find ich zwar viel Ungedult,", "tokens": ["So", "find", "ich", "zwar", "viel", "Un\u00b7ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch auch viel Zeichen starcker Huld,", "tokens": ["Doch", "auch", "viel", "Zei\u00b7chen", "star\u00b7cker", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die mich im Fallen aufgefangen.", "tokens": ["Die", "mich", "im", "Fal\u00b7len", "auf\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So gern ich andern Gutes thu,", "tokens": ["So", "gern", "ich", "an\u00b7dern", "Gu\u00b7tes", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So feindlich sezt mir alles zu,", "tokens": ["So", "feind\u00b7lich", "sezt", "mir", "al\u00b7les", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja selbst die eignen Hausgeno\u00dfen;", "tokens": ["Ja", "selbst", "die", "eig\u00b7nen", "Haus\u00b7ge\u00b7no\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Vater giebt mir keinen Rath,", "tokens": ["Mein", "Va\u00b7ter", "giebt", "mir", "kei\u00b7nen", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber mich wird stets das Bad", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "wird", "stets", "das", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADV", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Neid und Misgunst ausgego\u00dfen.", "tokens": ["Von", "Neid", "und", "Mis\u00b7gunst", "aus\u00b7ge\u00b7go\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, f\u00fchre mich mit starcker Hand", "tokens": ["Ach", ",", "f\u00fch\u00b7re", "mich", "mit", "star\u00b7cker", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Jacob aus Egyptenland,", "tokens": ["Wie", "Ja\u00b7cob", "aus", "E\u00b7gyp\u00b7ten\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach, hilf mir, da\u00df mein Opfer brenne,", "tokens": ["Ach", ",", "hilf", "mir", ",", "da\u00df", "mein", "Op\u00b7fer", "bren\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gieb mir doch nur so viel Zeit", "tokens": ["Und", "gieb", "mir", "doch", "nur", "so", "viel", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "ADV", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Von Nothdurft, Gram und D\u00fcrftigkeit,", "tokens": ["Von", "Noth\u00b7durft", ",", "Gram", "und", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit ich mich recht be\u00dfern k\u00f6nne.", "tokens": ["Da\u00b7mit", "ich", "mich", "recht", "be\u00b7\u00dfern", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Ich habe grob und schwer gefehlt", "tokens": ["Ich", "ha\u00b7be", "grob", "und", "schwer", "ge\u00b7fehlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, wenn mich Freund und Feind gequ\u00e4lt,", "tokens": ["Und", ",", "wenn", "mich", "Freund", "und", "Feind", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Unmuth oft dein Licht verla\u00dfen;", "tokens": ["Aus", "Un\u00b7muth", "oft", "dein", "Licht", "ver\u00b7la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bleib ich allzeit treu gesinnt.", "tokens": ["Doch", "bleib", "ich", "all\u00b7zeit", "treu", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Vater wirst das schwache Kind", "tokens": ["Du", "Va\u00b7ter", "wirst", "das", "schwa\u00b7che", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht gar so grausam fliehn und ha\u00dfen.", "tokens": ["Nicht", "gar", "so", "grau\u00b7sam", "fliehn", "und", "ha\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Jezt geh ich an ein schweres Werck", "tokens": ["Jezt", "geh", "ich", "an", "ein", "schwe\u00b7res", "Werck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und habe nun den gro\u00dfen Berg", "tokens": ["Und", "ha\u00b7be", "nun", "den", "gro\u00b7\u00dfen", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meiner Kunst zur\u00fcckzulegen;", "tokens": ["Von", "mei\u00b7ner", "Kunst", "zu\u00b7r\u00fcck\u00b7zu\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb Kraft, sonst komm ich nirgends fort.", "tokens": ["Gieb", "Kraft", ",", "sonst", "komm", "ich", "nir\u00b7gends", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe deines Sohnes Wort,", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7nes", "Soh\u00b7nes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies ist mir schon so gut als Seegen.", "tokens": ["Dies", "ist", "mir", "schon", "so", "gut", "als", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}