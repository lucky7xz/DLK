{"textgrid.poem.23990": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Birke, wie warst du sch\u00f6n,", "tokens": ["Bir\u00b7ke", ",", "wie", "warst", "du", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Als du im gr\u00fcnen Kleid,", "tokens": ["Als", "du", "im", "gr\u00fc\u00b7nen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zierliche Jungfrau, standst", "tokens": ["Zier\u00b7li\u00b7che", "Jung\u00b7frau", ",", "standst"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und dir der Fr\u00fchlingswind", "tokens": ["Und", "dir", "der", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Leise durchs zage Gezweig", "tokens": ["Lei\u00b7se", "durchs", "za\u00b7ge", "Ge\u00b7zweig"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Strich, wie des Br\u00e4utigams Hand", "tokens": ["Strich", ",", "wie", "des", "Br\u00e4u\u00b7ti\u00b7gams", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NE", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Z\u00e4rtlich der Braut durch die schimmernden Locken streicht.", "tokens": ["Z\u00e4rt\u00b7lich", "der", "Braut", "durch", "die", "schim\u00b7mern\u00b7den", "Lo\u00b7cken", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Birke, wie bist du sch\u00f6n,", "tokens": ["Bir\u00b7ke", ",", "wie", "bist", "du", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die du im goldnen Kleid,", "tokens": ["Die", "du", "im", "gold\u00b7nen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6ne Matrone, stehst.", "tokens": ["Sch\u00f6\u00b7ne", "Mat\u00b7ro\u00b7ne", ",", "stehst", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ruhig in klarer Luft", "tokens": ["Ru\u00b7hig", "in", "kla\u00b7rer", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00e4ngt nun das fahle Gezweig,", "tokens": ["H\u00e4ngt", "nun", "das", "fah\u00b7le", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie die Arme der Frau", "tokens": ["Wie", "die", "Ar\u00b7me", "der", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "L\u00e4ssig herab im erm\u00fcdeten Schoo\u00dfe ruhn.", "tokens": ["L\u00e4s\u00b7sig", "her\u00b7ab", "im", "er\u00b7m\u00fc\u00b7de\u00b7ten", "Schoo\u00b7\u00dfe", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}