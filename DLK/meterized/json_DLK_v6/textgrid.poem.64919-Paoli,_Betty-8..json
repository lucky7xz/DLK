{"textgrid.poem.64919": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein's hab' ich heut gef\u00fchlt auf meinem Abendgang,", "tokens": ["Ein's", "hab'", "ich", "heut", "ge\u00b7f\u00fchlt", "auf", "mei\u00b7nem", "A\u00b7bend\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Fr\u00fchlingsahnung mich beseligend durchdrang.", "tokens": ["Als", "Fr\u00fch\u00b7lin\u00b7gsah\u00b7nung", "mich", "be\u00b7se\u00b7li\u00b7gend", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo ihren Scheidegru\u00df die Sonne niederstrahlte,", "tokens": ["Wo", "ih\u00b7ren", "Schei\u00b7de\u00b7gru\u00df", "die", "Son\u00b7ne", "nie\u00b7der\u00b7strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich im tiefen Strom des Himmels Klarheit malte,", "tokens": ["Wo", "sich", "im", "tie\u00b7fen", "Strom", "des", "Him\u00b7mels", "Klar\u00b7heit", "mal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo mir begegnete manch fr\u00f6hlich Angesicht,", "tokens": ["Wo", "mir", "be\u00b7ge\u00b7gne\u00b7te", "manch", "fr\u00f6h\u00b7lich", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Befried'gung auf der Stirn, im Auge heitres Licht.", "tokens": ["Be\u00b7frie\u00b7d'\u00b7gung", "auf", "der", "Stirn", ",", "im", "Au\u00b7ge", "heit\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Im L\u00e4cheln der Natur, im L\u00e4cheln jener Mienen", "tokens": ["Im", "L\u00e4\u00b7cheln", "der", "Na\u00b7tur", ",", "im", "L\u00e4\u00b7cheln", "je\u00b7ner", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist meiner Zukunft Bild hold tr\u00f6stend mir erschienen.", "tokens": ["Ist", "mei\u00b7ner", "Zu\u00b7kunft", "Bild", "hold", "tr\u00f6s\u00b7tend", "mir", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADJD", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn als so mild der West das Antlitz mir gek\u00fchlt", "tokens": ["Denn", "als", "so", "mild", "der", "West", "das", "Ant\u00b7litz", "mir", "ge\u00b7k\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fremde Lust mich hob, da habe ich gef\u00fchlt:", "tokens": ["Und", "frem\u00b7de", "Lust", "mich", "hob", ",", "da", "ha\u00b7be", "ich", "ge\u00b7f\u00fchlt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So lange noch ein Lenz mit seinem Blumenstabe", "tokens": ["So", "lan\u00b7ge", "noch", "ein", "Lenz", "mit", "sei\u00b7nem", "Blu\u00b7men\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben bl\u00fchen macht und Zier bringt selbst dem Grabe;", "tokens": ["Das", "Le\u00b7ben", "bl\u00fc\u00b7hen", "macht", "und", "Zier", "bringt", "selbst", "dem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So lang die Sonne noch ergl\u00e4nzt am Firmament,", "tokens": ["So", "lang", "die", "Son\u00b7ne", "noch", "er\u00b7gl\u00e4nzt", "am", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang am Horizont ein einz'ger Stern noch brennt;", "tokens": ["So", "lang", "am", "Ho\u00b7ri\u00b7zont", "ein", "einz'\u00b7ger", "Stern", "noch", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So lang ein Gr\u00fcn noch keimt, so lang noch Wasser rauschen", "tokens": ["So", "lang", "ein", "Gr\u00fcn", "noch", "keimt", ",", "so", "lang", "noch", "Was\u00b7ser", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,", "ADV", "ADJD", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und V\u00f6gel singen noch, auf deren Lied zu lauschen;", "tokens": ["Und", "V\u00f6\u00b7gel", "sin\u00b7gen", "noch", ",", "auf", "de\u00b7ren", "Lied", "zu", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "APPR", "PRELAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "So lang ein Menschenaug' noch strahlt im Freudenschein: \u2013", "tokens": ["So", "lang", "ein", "Men\u00b7sche\u00b7naug'", "noch", "strahlt", "im", "Freu\u00b7den\u00b7schein", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange wird mein Herz nicht g\u00e4nzlich elend sein.", "tokens": ["So", "lan\u00b7ge", "wird", "mein", "Herz", "nicht", "g\u00e4nz\u00b7lich", "e\u00b7lend", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}