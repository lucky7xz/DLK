{"dta.poem.2885": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt: ein s\u00e4ugend Kind, wonach zuerst es streckt", "tokens": ["Man", "sagt", ":", "ein", "s\u00e4u\u00b7gend", "Kind", ",", "wo\u00b7nach", "zu\u00b7erst", "es", "streckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "ART", "ADJD", "NN", "$,", "PWAV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00e4ndchen, daran wird sein k\u00fcnft'ger Sinn entdeckt.", "tokens": ["Die", "H\u00e4nd\u00b7chen", ",", "da\u00b7ran", "wird", "sein", "k\u00fcnft'\u00b7ger", "Sinn", "ent\u00b7deckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum Gutes, Sch\u00f6nes, soll man nur dem Kind vorhalten,", "tokens": ["Drum", "Gu\u00b7tes", ",", "Sch\u00f6\u00b7nes", ",", "soll", "man", "nur", "dem", "Kind", "vor\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "ADJA", "$,", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um schlechte Neigungen in ihm nicht zu entfalten.", "tokens": ["Um", "schlech\u00b7te", "Nei\u00b7gun\u00b7gen", "in", "ihm", "nicht", "zu", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}