{"textgrid.poem.65172": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas also der Lohn f\u00fcr stete Treu,", "tokens": ["\u00bb", "das", "al\u00b7so", "der", "Lohn", "f\u00fcr", "ste\u00b7te", "Treu", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr Hilfe in Todesn\u00f6ten,", "tokens": ["F\u00fcr", "Hil\u00b7fe", "in", "To\u00b7des\u00b7n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df, spottend jeglicher Scham und Scheu,", "tokens": ["Da\u00df", ",", "spot\u00b7tend", "jeg\u00b7li\u00b7cher", "Scham", "und", "Scheu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Recht sie mit F\u00fc\u00dfen treten?", "tokens": ["Mein", "Recht", "sie", "mit", "F\u00fc\u00b7\u00dfen", "tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Kaiser zu helfen, hab' ich mein Schlo\u00df,", "tokens": ["Dem", "Kai\u00b7ser", "zu", "hel\u00b7fen", ",", "hab'", "ich", "mein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "All' meine G\u00fcter verpf\u00e4ndet,", "tokens": ["All'", "mei\u00b7ne", "G\u00fc\u00b7ter", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dem Kaiser, de\u00df schuft'ger Schreibertro\u00df", "tokens": ["Dem", "Kai\u00b7ser", ",", "de\u00df", "schuft'\u00b7ger", "Schrei\u00b7ber\u00b7tro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mich t\u00fcckisch beraubt und sch\u00e4ndet!", "tokens": ["Mich", "t\u00fc\u00b7ckisch", "be\u00b7raubt", "und", "sch\u00e4n\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nachdem sie mich wie ein Wild gehetzt,", "tokens": ["Nach\u00b7dem", "sie", "mich", "wie", "ein", "Wild", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Genarrt mich h\u00fcben und dr\u00fcben,", "tokens": ["Ge\u00b7narrt", "mich", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erkl\u00e4ren sie meine Ford'rung jetzt,", "tokens": ["Er\u00b7kl\u00e4\u00b7ren", "sie", "mei\u00b7ne", "Ford'\u00b7rung", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schurken! f\u00fcr \u00fcbertrieben!", "tokens": ["Die", "Schur\u00b7ken", "!", "f\u00fcr", "\u00fc\u00b7bert\u00b7rie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zu deutsch besagt dieser Worte Sinn,", "tokens": ["Zu", "deutsch", "be\u00b7sagt", "die\u00b7ser", "Wor\u00b7te", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich habe den Kaiser betrogen,", "tokens": ["Ich", "ha\u00b7be", "den", "Kai\u00b7ser", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Aus seinem und des Landes Ruin", "tokens": ["Aus", "sei\u00b7nem", "und", "des", "Lan\u00b7des", "Ru\u00b7in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gewinn und Vorteil gezogen!", "tokens": ["Ge\u00b7winn", "und", "Vor\u00b7teil", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Als Retter aus der h\u00f6chsten Gefahr", "tokens": ["Als", "Ret\u00b7ter", "aus", "der", "h\u00f6chs\u00b7ten", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Begr\u00fc\u00dft' er mich einst in Hulden;", "tokens": ["Be\u00b7gr\u00fc\u00dft'", "er", "mich", "einst", "in", "Hul\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt l\u00e4\u00dft er mich mit ergrautem Haar", "tokens": ["Jetzt", "l\u00e4\u00dft", "er", "mich", "mit", "er\u00b7grau\u00b7tem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die schwerste Unbill erdulden!", "tokens": ["Die", "schwers\u00b7te", "Un\u00b7bill", "er\u00b7dul\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verlustig deiner irdischen Hab',", "tokens": ["Ver\u00b7lus\u00b7tig", "dei\u00b7ner", "ir\u00b7di\u00b7schen", "Hab'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "An deiner Ehre gesch\u00e4digt,", "tokens": ["An", "dei\u00b7ner", "Eh\u00b7re", "ge\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Baumkircher! lege dich nur ins Grab!", "tokens": ["Baum\u00b7kir\u00b7cher", "!", "le\u00b7ge", "dich", "nur", "ins", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "++----+-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Dein Tagewerk ist erledigt!\u00ab \u2013", "tokens": ["Dein", "Ta\u00b7ge\u00b7werk", "ist", "er\u00b7le\u00b7digt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit kaiserlichem Siegel den Brief", "tokens": ["Mit", "kai\u00b7ser\u00b7li\u00b7chem", "Sie\u00b7gel", "den", "Brief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wirft er zerkn\u00fcllt in die Ecke,", "tokens": ["Wirft", "er", "zer\u00b7kn\u00fcllt", "in", "die", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dann st\u00f6hnet er auf, so schwer, so tief,", "tokens": ["Dann", "st\u00f6h\u00b7net", "er", "auf", ",", "so", "schwer", ",", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und starret empor zur Decke.", "tokens": ["Und", "star\u00b7ret", "em\u00b7por", "zur", "De\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es fliegt sein Herz, es fiebert sein Hirn", "tokens": ["Es", "fliegt", "sein", "Herz", ",", "es", "fie\u00b7bert", "sein", "Hirn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Von finstrer Gedanken Schwalle;", "tokens": ["Von", "finst\u00b7rer", "Ge\u00b7dan\u00b7ken", "Schwal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Von hei\u00dfem Zorn ger\u00f6tet die Stirn,", "tokens": ["Von", "hei\u00b7\u00dfem", "Zorn", "ge\u00b7r\u00f6\u00b7tet", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Durchmi\u00dft er ruhlos die Halle.", "tokens": ["Durch\u00b7mi\u00dft", "er", "ruh\u00b7los", "die", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wie er in's stolze Herz zur\u00fcck", "tokens": ["Und", "wie", "er", "in's", "stol\u00b7ze", "Herz", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gewaltsam dr\u00e4nget die Klage,", "tokens": ["Ge\u00b7walt\u00b7sam", "dr\u00e4n\u00b7get", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da steigen empor vor seinem Blick", "tokens": ["Da", "stei\u00b7gen", "em\u00b7por", "vor", "sei\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Bilder vergang'ner Tage.", "tokens": ["Die", "Bil\u00b7der", "ver\u00b7gang'\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er denkt der Zeiten, in denen er", "tokens": ["Er", "denkt", "der", "Zei\u00b7ten", ",", "in", "de\u00b7nen", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit jugendlich k\u00fchnem Wagen,", "tokens": ["Mit", "ju\u00b7gend\u00b7lich", "k\u00fch\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Wetterstrahl, das Magyarenheer", "tokens": ["Ein", "Wet\u00b7ter\u00b7strahl", ",", "das", "Ma\u00b7gy\u00b7a\u00b7ren\u00b7heer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bei Neustadt zur\u00fcckgeschlagen.", "tokens": ["Bei", "Neu\u00b7stadt", "zu\u00b7r\u00fcck\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Er denkt, wie am Wienerthor er dort,", "tokens": ["Er", "denkt", ",", "wie", "am", "Wie\u00b7ner\u00b7thor", "er", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim Anprall der Feindesbanden,", "tokens": ["Beim", "An\u00b7prall", "der", "Fein\u00b7des\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur er der Stadt und des Kaisers Hort,", "tokens": ["Nur", "er", "der", "Stadt", "und", "des", "Kai\u00b7sers", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "KON", "ART", "NN", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Den blutigen Strau\u00df bestanden!", "tokens": ["Den", "blu\u00b7ti\u00b7gen", "Strau\u00df", "be\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er sieht sich, als, im Tode noch grimm,", "tokens": ["Er", "sieht", "sich", ",", "als", ",", "im", "To\u00b7de", "noch", "grimm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "$,", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die Seinen im Staube lagen,", "tokens": ["Die", "Sei\u00b7nen", "im", "Stau\u00b7be", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Allein noch k\u00e4mpfen, bis hinter ihm", "tokens": ["Al\u00b7lein", "noch", "k\u00e4mp\u00b7fen", ",", "bis", "hin\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "KOUS", "APPR", "PPER"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Br\u00fccke war abgetragen!", "tokens": ["Die", "Br\u00fc\u00b7cke", "war", "ab\u00b7ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "O wie die wechselnden Bilder ihn,", "tokens": ["O", "wie", "die", "wech\u00b7seln\u00b7den", "Bil\u00b7der", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein Zauberreigen, umschweben!", "tokens": ["Ein", "Zau\u00b7berr\u00b7ei\u00b7gen", ",", "um\u00b7schwe\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Kaiser in seiner Burg zu Wien", "tokens": ["Der", "Kai\u00b7ser", "in", "sei\u00b7ner", "Burg", "zu", "Wi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Belagert, von Feinden umgeben!", "tokens": ["Be\u00b7la\u00b7gert", ",", "von", "Fein\u00b7den", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mit seinen B\u00fcrgern in schwerem Streit,", "tokens": ["Mit", "sei\u00b7nen", "B\u00fcr\u00b7gern", "in", "schwe\u00b7rem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bedroht mit Speeren und Spie\u00dfen,", "tokens": ["Be\u00b7droht", "mit", "Spee\u00b7ren", "und", "Spie\u00b7\u00dfen", 