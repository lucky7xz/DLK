.relaxed"}, "line.4": {"text": "Und rei\u00dft es halb aus der Scheide.", "tokens": ["Und", "rei\u00dft", "es", "halb", "aus", "der", "Schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Des Greises Augen funkeln und gl\u00fch'n", "tokens": ["Des", "Grei\u00b7ses", "Au\u00b7gen", "fun\u00b7keln", "und", "gl\u00fch'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Gleich unheilk\u00fcndenden Sternen,", "tokens": ["Gleich", "un\u00b7heil\u00b7k\u00fcn\u00b7den\u00b7den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und finster murmelt er vor sich hin:", "tokens": ["Und", "fins\u00b7ter", "mur\u00b7melt", "er", "vor", "sich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u00bbsie sollen mich kennen lernen!\u00ab", "tokens": ["\u00bb", "sie", "sol\u00b7len", "mich", "ken\u00b7nen", "ler\u00b7nen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}