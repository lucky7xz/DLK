{"textgrid.poem.46334": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, dessen L\u00e4chlen mir die Thorheit oft verrieth,", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dessen L\u00e4chlen mir die Thorheit oft verrieth,", "tokens": ["Du", ",", "des\u00b7sen", "L\u00e4ch\u00b7len", "mir", "die", "Thor\u00b7heit", "oft", "ver\u00b7rieth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die itzt mein sch\u00e4rfrer Blick in mir und andern sieht;", "tokens": ["Die", "itzt", "mein", "sch\u00e4r\u00b7frer", "Blick", "in", "mir", "und", "an\u00b7dern", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Urbild, dessen Ernst mich Tugend f\u00fchlen lehrte,", "tokens": ["Mein", "Ur\u00b7bild", ",", "des\u00b7sen", "Ernst", "mich", "Tu\u00b7gend", "f\u00fch\u00b7len", "lehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "PPER", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ich zwar sp\u00e4t begriff, doch durch dich fr\u00fcher ehrte;", "tokens": ["Die", "ich", "zwar", "sp\u00e4t", "be\u00b7griff", ",", "doch", "durch", "dich", "fr\u00fc\u00b7her", "ehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "H\u00f6r an, wie dir mein Lied auf Satirs Fl\u00f6te klingt,", "tokens": ["H\u00f6r", "an", ",", "wie", "dir", "mein", "Lied", "auf", "Sa\u00b7tirs", "Fl\u00f6\u00b7te", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Kleist und Gleim geh\u00f6rt, und Thoren niedrig d\u00fcnckt.", "tokens": ["Da\u00df", "Kleist", "und", "Gleim", "ge\u00b7h\u00f6rt", ",", "und", "Tho\u00b7ren", "nied\u00b7rig", "d\u00fcnckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein erster Ruhm ist, wenn ich Vater dir gefalle.", "tokens": ["Mein", "ers\u00b7ter", "Ruhm", "ist", ",", "wenn", "ich", "Va\u00b7ter", "dir", "ge\u00b7fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du lobst mich ohne List, und schiltst mich ohne Galle.", "tokens": ["Du", "lobst", "mich", "oh\u00b7ne", "List", ",", "und", "schiltst", "mich", "oh\u00b7ne", "Gal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}