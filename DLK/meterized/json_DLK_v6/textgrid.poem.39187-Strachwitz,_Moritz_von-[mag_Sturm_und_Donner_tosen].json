{"textgrid.poem.39187": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[mag Sturm und Donner tosen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag Sturm und Donner tosen: ich wei\u00df, ich liebe Dich;", "tokens": ["Mag", "Sturm", "und", "Don\u00b7ner", "to\u00b7sen", ":", "ich", "wei\u00df", ",", "ich", "lie\u00b7be", "Dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mag West und Fr\u00fchling kosen: ich wei\u00df, ich liebe Dich;", "tokens": ["Mag", "West", "und", "Fr\u00fch\u00b7ling", "ko\u00b7sen", ":", "ich", "wei\u00df", ",", "ich", "lie\u00b7be", "Dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NN", "VVINF", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mag \u00fcber alle Lande hinrasen Wetterzorn", "tokens": ["Mag", "\u00fc\u00b7ber", "al\u00b7le", "Lan\u00b7de", "hin\u00b7ra\u00b7sen", "Wet\u00b7ter\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVPP", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und brechen alle Rosen: ich wei\u00df, ich liebe Dich;", "tokens": ["Und", "bre\u00b7chen", "al\u00b7le", "Ro\u00b7sen", ":", "ich", "wei\u00df", ",", "ich", "lie\u00b7be", "Dich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Das Weltrund mag zerflie\u00dfen als Duft im Meeresborn,", "tokens": ["Das", "Welt\u00b7rund", "mag", "zer\u00b7flie\u00b7\u00dfen", "als", "Duft", "im", "Mee\u00b7res\u00b7born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Vergehn im Wesenlosen: ich wei\u00df, ich liebe Dich.", "tokens": ["Ver\u00b7gehn", "im", "We\u00b7sen\u00b7lo\u00b7sen", ":", "ich", "wei\u00df", ",", "ich", "lie\u00b7be", "Dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}