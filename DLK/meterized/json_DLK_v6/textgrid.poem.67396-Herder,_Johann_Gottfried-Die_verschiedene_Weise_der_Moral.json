{"textgrid.poem.67396": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die verschiedene Weise der Moral", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf offnem Markte mit Gebieterton", "tokens": ["Auf", "off\u00b7nem", "Mark\u00b7te", "mit", "Ge\u00b7bie\u00b7ter\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erschien in Herrscherpracht der Gott Imperativus.", "tokens": ["Er\u00b7schien", "in", "Herr\u00b7scher\u00b7pracht", "der", "Gott", "Im\u00b7pe\u00b7ra\u00b7ti\u00b7vus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "\u00bbich bin das Ich, der \u00e4chten Weisheit ", "tokens": ["\u00bb", "ich", "bin", "das", "Ich", ",", "der", "\u00e4ch\u00b7ten", "Weis\u00b7heit"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vocativ der Pflicht, des Rechts Nominativus.", "tokens": ["Ein", "Vo\u00b7ca\u00b7tiv", "der", "Pflicht", ",", "des", "Rechts", "No\u00b7mi\u00b7na\u00b7ti\u00b7vus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wer von der W\u00fcrde wich, erzittre meinem Thron!", "tokens": ["Wer", "von", "der", "W\u00fcr\u00b7de", "wich", ",", "er\u00b7zitt\u00b7re", "mei\u00b7nem", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin der kleinsten Schuld Fiscal-Accusativus,", "tokens": ["Ich", "bin", "der", "kleins\u00b7ten", "Schuld", "Fis\u00b7ca\u00b7lAc\u00b7cu\u00b7sa\u00b7ti\u00b7vus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und hinter mir dort steht zu B\u00fcttelstraf' und Lohn", "tokens": ["Und", "hin\u00b7ter", "mir", "dort", "steht", "zu", "B\u00fct\u00b7tel\u00b7straf'", "und", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein dunkler Schlu\u00dfstein noch, der Gott Infinitivus. \u2013", "tokens": ["Ein", "dunk\u00b7ler", "Schlu\u00df\u00b7stein", "noch", ",", "der", "Gott", "In\u00b7fi\u00b7ni\u00b7ti\u00b7vus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Doch wer bist Du?\u00ab \u00bbIch bin der armen Menschheit Sohn,", "tokens": ["Doch", "wer", "bist", "Du", "?", "\u00ab", "\u00bb", "Ich", "bin", "der", "ar\u00b7men", "Menschheit", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$.", "$(", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Ein Flehender, der bl\u00f6de Optativus;", "tokens": ["Ein", "Fle\u00b7hen\u00b7der", ",", "der", "bl\u00f6\u00b7de", "Op\u00b7ta\u00b7ti\u00b7vus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Doch selbst mein Wunsch, mein Streben wird mir Lohn;", "tokens": ["Doch", "selbst", "mein", "Wunsch", ",", "mein", "Stre\u00b7ben", "wird", "mir", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn hier ist mein Geno\u00df, der helfende Dativus,", "tokens": ["Denn", "hier", "ist", "mein", "Ge\u00b7no\u00df", ",", "der", "hel\u00b7fen\u00b7de", "Da\u00b7ti\u00b7vus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ein guter Mann.\u00ab \u00bbIhm werd' ein Bettlerlohn!", "tokens": ["Ein", "gu\u00b7ter", "Mann", ".", "\u00ab", "\u00bb", "Ihm", "werd'", "ein", "Bett\u00b7ler\u00b7lohn", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und rufet lauter aus mit Pauken und Drommeten:", "tokens": ["Und", "ru\u00b7fet", "lau\u00b7ter", "aus", "mit", "Pau\u00b7ken", "und", "Drom\u00b7me\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber zog der L\u00e4rm; die sanfte Menschenliebe", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zog", "der", "L\u00e4rm", ";", "die", "sanf\u00b7te", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihren W\u00fcnschen, ihrer Hoffnung blieb", "tokens": ["Mit", "ih\u00b7ren", "W\u00fcn\u00b7schen", ",", "ih\u00b7rer", "Hoff\u00b7nung", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und feuert' an des Herzens zarte Triebe:", "tokens": ["Und", "feu\u00b7ert'", "an", "des", "Her\u00b7zens", "zar\u00b7te", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbihr Menschen, liebet Euch und seid einander lieb!", "tokens": ["\u00bb", "ihr", "Men\u00b7schen", ",", "lie\u00b7bet", "Euch", "und", "seid", "ein\u00b7an\u00b7der", "lieb", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verzeihet gern! wir m\u00fcssen Alle fehlen.", "tokens": ["Ver\u00b7zei\u00b7het", "gern", "!", "wir", "m\u00fcs\u00b7sen", "Al\u00b7le", "feh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VMFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hofft das Bessre stets! denn Hoffnung st\u00e4rkt die Seelen.", "tokens": ["Und", "hofft", "das", "Bess\u00b7re", "stets", "!", "denn", "Hoff\u00b7nung", "st\u00e4rkt", "die", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$.", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erwartet wenig, um so reichlicher zu geben!", "tokens": ["Er\u00b7war\u00b7tet", "we\u00b7nig", ",", "um", "so", "reich\u00b7li\u00b7cher", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUI", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aufs Werthsein rechnet nicht! der Menschen ganzes Leben,", "tokens": ["Aufs", "Werth\u00b7sein", "rech\u00b7net", "nicht", "!", "der", "Men\u00b7schen", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Statt Haben und Besitz, ist Streben.\u00ab", "tokens": ["Statt", "Ha\u00b7ben", "und", "Be\u00b7sitz", ",", "ist", "Stre\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entflohen war der L\u00e4rm, sein Trommeln war vor\u00fcber;", "tokens": ["Ent\u00b7flo\u00b7hen", "war", "der", "L\u00e4rm", ",", "sein", "Trom\u00b7meln", "war", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sanfte Stimme, zart und schwach,", "tokens": ["Die", "sanf\u00b7te", "Stim\u00b7me", ",", "zart", "und", "schwach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie t\u00f6nete in Aller Herzen nach.", "tokens": ["Sie", "t\u00f6\u00b7ne\u00b7te", "in", "Al\u00b7ler", "Her\u00b7zen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Hoffnungen gewann der Mensch das Leben lieber,", "tokens": ["Mit", "Hoff\u00b7nun\u00b7gen", "ge\u00b7wann", "der", "Mensch", "das", "Le\u00b7ben", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und jeder Wunsch, so leise man ihn sprach,", "tokens": ["Und", "je\u00b7der", "Wunsch", ",", "so", "lei\u00b7se", "man", "ihn", "sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ADV", "ADJD", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ging strebend auf die fernste Nachwelt \u00fcber.", "tokens": ["Ging", "stre\u00b7bend", "auf", "die", "ferns\u00b7te", "Nach\u00b7welt", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}