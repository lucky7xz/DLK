{"textgrid.poem.50409": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "67. An Vipsanius", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst dass du von Adel bist,", "tokens": ["Du", "sagst", "dass", "du", "von", "A\u00b7del", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dass du drum ein Weib, die deines Gleichen ist,", "tokens": ["Und", "dass", "du", "drum", "ein", "Weib", ",", "die", "dei\u00b7nes", "Glei\u00b7chen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob gleich an ihr kein gutes Haar,", "tokens": ["Ob", "gleich", "an", "ihr", "kein", "gu\u00b7tes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tugend vorzuziehn dich gar nicht k\u00f6ntst entbrechen:", "tokens": ["Der", "Tu\u00b7gend", "vor\u00b7zu\u00b7ziehn", "dich", "gar", "nicht", "k\u00f6ntst", "ent\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Der sich sein ", "tokens": ["Der", "sich", "sein"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}}}}