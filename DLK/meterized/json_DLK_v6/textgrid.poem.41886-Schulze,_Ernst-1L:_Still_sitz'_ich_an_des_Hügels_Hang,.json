{"textgrid.poem.41886": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still sitz' ich an des H\u00fcgels Hang,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still sitz' ich an des H\u00fcgels Hang,", "tokens": ["Still", "sitz'", "ich", "an", "des", "H\u00fc\u00b7gels", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel ist so klar,", "tokens": ["Der", "Him\u00b7mel", "ist", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das L\u00fcftchen spielt im gr\u00fcnen Thal,", "tokens": ["Das", "L\u00fcft\u00b7chen", "spielt", "im", "gr\u00fc\u00b7nen", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich beym ersten Fr\u00fchlingsstrahl", "tokens": ["Wo", "ich", "beym", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Einst, ach, so gl\u00fccklich war;", "tokens": ["Einst", ",", "ach", ",", "so", "gl\u00fcck\u00b7lich", "war", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo ich an ihrer Seite ging", "tokens": ["Wo", "ich", "an", "ih\u00b7rer", "Sei\u00b7te", "ging"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So traulich und so nah,", "tokens": ["So", "trau\u00b7lich", "und", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tief im dunkeln Felsenquell", "tokens": ["Und", "tief", "im", "dun\u00b7keln", "Fel\u00b7sen\u00b7quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den sch\u00f6nen Himmel blau und hell,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Him\u00b7mel", "blau", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sie im Himmel sah.", "tokens": ["Und", "sie", "im", "Him\u00b7mel", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sieh, wie der bunte Fr\u00fchling schon", "tokens": ["Sieh", ",", "wie", "der", "bun\u00b7te", "Fr\u00fch\u00b7ling", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Knosp' und Bl\u00fcthe blickt!", "tokens": ["Aus", "Knosp'", "und", "Bl\u00fc\u00b7the", "blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht alle Bl\u00fcthen sind mir gleich,", "tokens": ["Nicht", "al\u00b7le", "Bl\u00fc\u00b7then", "sind", "mir", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am liebsten pfl\u00fcckt' ich von dem Zweig,", "tokens": ["Am", "liebs\u00b7ten", "pfl\u00fcckt'", "ich", "von", "dem", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von welchem sie gepfl\u00fcckt.", "tokens": ["Von", "wel\u00b7chem", "sie", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn Alles ist wie damals noch,", "tokens": ["Denn", "Al\u00b7les", "ist", "wie", "da\u00b7mals", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "KOKOM", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen, das Gefild;", "tokens": ["Die", "Blu\u00b7men", ",", "das", "Ge\u00b7fild", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne scheint nicht minder hell,", "tokens": ["Die", "Son\u00b7ne", "scheint", "nicht", "min\u00b7der", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht minder freundlich schwimmt im Quell", "tokens": ["Nicht", "min\u00b7der", "freund\u00b7lich", "schwimmt", "im", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das blaue Himmelsbild.", "tokens": ["Das", "blau\u00b7e", "Him\u00b7mels\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es wandeln nur sich Will' und Wahn,", "tokens": ["Es", "wan\u00b7deln", "nur", "sich", "Will'", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wechseln Lust und Streit;", "tokens": ["Es", "wech\u00b7seln", "Lust", "und", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor\u00fcber flieht der Liebe Gl\u00fcck,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "flieht", "der", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur die Liebe bleibt zur\u00fcck,", "tokens": ["Und", "nur", "die", "Lie\u00b7be", "bleibt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lieb' und ach, das Leid!", "tokens": ["Die", "Lieb'", "und", "ach", ",", "das", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "XY", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O w\u00e4r' ich doch das V\u00f6glein nur", "tokens": ["O", "w\u00e4r'", "ich", "doch", "das", "V\u00f6\u00b7glein", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort an dem Wiesenhang,", "tokens": ["Dort", "an", "dem", "Wie\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann blieb' ich auf den Zweigen hier", "tokens": ["Dann", "blieb'", "ich", "auf", "den", "Zwei\u00b7gen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00e4ng' ein s\u00fc\u00dfes Lied von ihr", "tokens": ["Und", "s\u00e4ng'", "ein", "s\u00fc\u00b7\u00dfes", "Lied", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den ganzen Sommer lang.", "tokens": ["Den", "gan\u00b7zen", "Som\u00b7mer", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}