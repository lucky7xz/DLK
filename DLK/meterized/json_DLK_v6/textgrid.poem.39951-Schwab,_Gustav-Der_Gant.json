{"textgrid.poem.39951": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Gant", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leuchten die Lichter im Schlosse so helle!", "tokens": ["Wie", "leuch\u00b7ten", "die", "Lich\u00b7ter", "im", "Schlos\u00b7se", "so", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Herr Christoph von Ramstein, der frohe Geselle,", "tokens": ["Herr", "Chris\u00b7toph", "von", "Rams\u00b7tein", ",", "der", "fro\u00b7he", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er h\u00e4lt in dem Saale zum letztenmal Schmaus,", "tokens": ["Er", "h\u00e4lt", "in", "dem", "Saa\u00b7le", "zum", "letz\u00b7ten\u00b7mal", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADV", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Denn morgen verkauft man ihm G\u00fcter und Haus.", "tokens": ["Denn", "mor\u00b7gen", "ver\u00b7kauft", "man", "ihm", "G\u00fc\u00b7ter", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Ahnen verthaten's, er hat's nicht verschuldet,", "tokens": ["Die", "Ah\u00b7nen", "vert\u00b7ha\u00b7ten's", ",", "er", "hat's", "nicht", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was er nicht verbrochen, gelassen er duldet,", "tokens": ["Was", "er", "nicht", "ver\u00b7bro\u00b7chen", ",", "ge\u00b7las\u00b7sen", "er", "dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "$,", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Geht lustig in's Elend, das Leid, er verzecht's,", "tokens": ["Geht", "lus\u00b7tig", "in's", "E\u00b7lend", ",", "das", "Leid", ",", "er", "ver\u00b7zecht's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Leicht endet der Letzte des leichten Geschlechts.", "tokens": ["Leicht", "en\u00b7det", "der", "Letz\u00b7te", "des", "leich\u00b7ten", "Ge\u00b7schlechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df er so fr\u00f6hlich vom Gute kann scheiden,", "tokens": ["Doch", "da\u00df", "er", "so", "fr\u00f6h\u00b7lich", "vom", "Gu\u00b7te", "kann", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kein Kummer die Lust ihm des Lebens entleiden,", "tokens": ["Kein", "Kum\u00b7mer", "die", "Lust", "ihm", "des", "Le\u00b7bens", "ent\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das macht, ihn begleitet zur H\u00fctt' aus dem Saal", "tokens": ["Das", "macht", ",", "ihn", "be\u00b7glei\u00b7tet", "zur", "H\u00fctt'", "aus", "dem", "Saal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Engel des Himmels, ein lieblich Gemahl.", "tokens": ["Ein", "En\u00b7gel", "des", "Him\u00b7mels", ",", "ein", "lieb\u00b7lich", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Kein Gram ihr umschattet die blauenden Augen,", "tokens": ["Kein", "Gram", "ihr", "um\u00b7schat\u00b7tet", "die", "blau\u00b7en\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Draus mag er sich Stralen der Hoffnung entsaugen;", "tokens": ["Draus", "mag", "er", "sich", "Stra\u00b7len", "der", "Hoff\u00b7nung", "ent\u00b7sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr bleichet kein Schmerz auf der Wange das Rot,", "tokens": ["Ihr", "blei\u00b7chet", "kein", "Schmerz", "auf", "der", "Wan\u00b7ge", "das", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr schwellet den Busen kein Seufzer der Not.", "tokens": ["Ihr", "schwel\u00b7let", "den", "Bu\u00b7sen", "kein", "Seuf\u00b7zer", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Drum weil er den Schatz sich, den edlen, gerettet,", "tokens": ["Drum", "weil", "er", "den", "Schatz", "sich", ",", "den", "ed\u00b7len", ",", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ART", "NN", "PRF", "$,", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So f\u00fchlt er auf Stroh wie auf Flaum sich gebettet,", "tokens": ["So", "f\u00fchlt", "er", "auf", "Stroh", "wie", "auf", "Flaum", "sich", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wandelt am Morgen den traurigen Pfad,", "tokens": ["Und", "wan\u00b7delt", "am", "Mor\u00b7gen", "den", "trau\u00b7ri\u00b7gen", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als fl\u00f6g' er zum Tanze, gen Basel zum Rat.", "tokens": ["Als", "fl\u00f6g'", "er", "zum", "Tan\u00b7ze", ",", "gen", "Ba\u00b7sel", "zum", "Rat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPR", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Das Haus und die G\u00fcter, die sch\u00f6nsten im Lande,", "tokens": ["Das", "Haus", "und", "die", "G\u00fc\u00b7ter", ",", "die", "sch\u00f6ns\u00b7ten", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er gab sie schon lange den Herren zum Pfande,", "tokens": ["Er", "gab", "sie", "schon", "lan\u00b7ge", "den", "Her\u00b7ren", "zum", "Pfan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie sitzen mit Mantel und Kragen geschm\u00fcckt,", "tokens": ["Sie", "sit\u00b7zen", "mit", "Man\u00b7tel", "und", "Kra\u00b7gen", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Ritter vor ihnen entbl\u00f6\u00dft und geb\u00fcckt;", "tokens": ["Der", "Rit\u00b7ter", "vor", "ih\u00b7nen", "ent\u00b7bl\u00f6\u00dft", "und", "ge\u00b7b\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Und doch nicht gebeugt im zufriedenen Herzen;", "tokens": ["Und", "doch", "nicht", "ge\u00b7beugt", "im", "zu\u00b7frie\u00b7de\u00b7nen", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es schlie\u00dft sich der Kauf unter Lachen und Scherzen,", "tokens": ["Es", "schlie\u00dft", "sich", "der", "Kauf", "un\u00b7ter", "La\u00b7chen", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am Ende da spricht er: \u00bbIhr w\u00fcrdigen Herrn,", "tokens": ["Am", "En\u00b7de", "da", "spricht", "er", ":", "\u00bb", "Ihr", "w\u00fcr\u00b7di\u00b7gen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Eins gebet mir drein, und eins h\u00e4tt' ich so gern!", "tokens": ["Eins", "ge\u00b7bet", "mir", "drein", ",", "und", "eins", "h\u00e4tt'", "ich", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PIS", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Die blinkenden Thaler, sie m\u00fcssen ach! wandern;", "tokens": ["Die", "blin\u00b7ken\u00b7den", "Tha\u00b7ler", ",", "sie", "m\u00fcs\u00b7sen", "ach", "!", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "ITJ", "$.", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die goldenen G\u00fclden geh\u00f6ren schon Andern;", "tokens": ["Die", "gol\u00b7de\u00b7nen", "G\u00fcl\u00b7den", "ge\u00b7h\u00f6\u00b7ren", "schon", "An\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Euch liegt in den Buden viel Glanz und viel Glast,", "tokens": ["Euch", "liegt", "in", "den", "Bu\u00b7den", "viel", "Glanz", "und", "viel", "Glast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So schenkt mir ein einziges St\u00fccklein Damast.", "tokens": ["So", "schenkt", "mir", "ein", "ein\u00b7zi\u00b7ges", "St\u00fcck\u00b7lein", "Da\u00b7mast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich selber, ich will nichts von Sammt und von Seiden,", "tokens": ["Ich", "sel\u00b7ber", ",", "ich", "will", "nichts", "von", "Sammt", "und", "von", "Sei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "PIS", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch m\u00f6cht' ich mein ehlich Gemahl mit bekleiden,", "tokens": ["Doch", "m\u00f6cht'", "ich", "mein", "eh\u00b7lich", "Ge\u00b7mahl", "mit", "be\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "ADJD", "NN", "APPR", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie ist wie ein Engel aus himmlischen H\u00f6hn,", "tokens": ["Sie", "ist", "wie", "ein", "En\u00b7gel", "aus", "himm\u00b7li\u00b7schen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie ist f\u00fcr den Kittel der Armut zu sch\u00f6n.\u00ab", "tokens": ["Sie", "ist", "f\u00fcr", "den", "Kit\u00b7tel", "der", "Ar\u00b7mut", "zu", "sch\u00f6n", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKA", "ADJD", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Wohl r\u00fchret die M\u00e4nner des Rates die Bitte,", "tokens": ["Wohl", "r\u00fch\u00b7ret", "die", "M\u00e4n\u00b7ner", "des", "Ra\u00b7tes", "die", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei ehrlichen B\u00fcrgern ist g\u00fctige Sitte;", "tokens": ["Bei", "ehr\u00b7li\u00b7chen", "B\u00fcr\u00b7gern", "ist", "g\u00fc\u00b7ti\u00b7ge", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und flie\u00dfende Seide, gewichtig und echt,", "tokens": ["Und", "flie\u00b7\u00dfen\u00b7de", "Sei\u00b7de", ",", "ge\u00b7wich\u00b7tig", "und", "echt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die macht ihm ein Schneider von Basel zurecht.", "tokens": ["Die", "macht", "ihm", "ein", "Schnei\u00b7der", "von", "Ba\u00b7sel", "zu\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NE", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Und knapp an die quellenden Glieder sie fugend", "tokens": ["Und", "knapp", "an", "die", "quel\u00b7len\u00b7den", "Glie\u00b7der", "sie", "fu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bekleidet der Ritter das Weib seiner Jugend,", "tokens": ["Be\u00b7klei\u00b7det", "der", "Rit\u00b7ter", "das", "Weib", "sei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er f\u00fchret sie unter das niedrige Dach,", "tokens": ["Er", "f\u00fch\u00b7ret", "sie", "un\u00b7ter", "das", "nied\u00b7ri\u00b7ge", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als tret' er mit ihr in ein F\u00fcrstengemach.", "tokens": ["Als", "tret'", "er", "mit", "ihr", "in", "ein", "F\u00fcrs\u00b7ten\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.12": {"line.1": {"text": "Er pflanzt und er erntet, sie webet und spinnet,", "tokens": ["Er", "pflanzt", "und", "er", "ern\u00b7tet", ",", "sie", "we\u00b7bet", "und", "spin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie l\u00e4chelt so lieblich, er k\u00fc\u00dft und er minnet!", "tokens": ["Sie", "l\u00e4\u00b7chelt", "so", "lieb\u00b7lich", ",", "er", "k\u00fc\u00dft", "und", "er", "min\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wohl altert das Kleid, wohl verbl\u00fcht das Gesicht,", "tokens": ["Wohl", "al\u00b7tert", "das", "Kleid", ",", "wohl", "ver\u00b7bl\u00fcht", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch Liebe nicht weicht und Gen\u00fcge weicht nicht.", "tokens": ["Doch", "Lie\u00b7be", "nicht", "weich