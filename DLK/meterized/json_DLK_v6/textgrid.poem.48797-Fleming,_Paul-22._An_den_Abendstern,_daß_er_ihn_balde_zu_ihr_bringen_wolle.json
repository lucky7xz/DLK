{"textgrid.poem.48797": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "22. An den Abendstern, da\u00df er ihn balde zu ihr bringen wolle", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh' auf doch, geh' doch auf, du hellester der Sterne!", "tokens": ["Geh'", "auf", "doch", ",", "geh'", "doch", "auf", ",", "du", "hel\u00b7les\u00b7ter", "der", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Klytemnestern Sohn, der m\u00fcde sieht nach dir,", "tokens": ["Der", "Kly\u00b7tem\u00b7nes\u00b7tern", "Sohn", ",", "der", "m\u00fc\u00b7de", "sieht", "nach", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "komm, Ruhfreund, l\u00f6s' ihn ab. Diana scheint dir f\u00fcr,", "tokens": ["komm", ",", "Ruh\u00b7freund", ",", "l\u00f6s'", "ihn", "ab", ".", "Di\u00b7a\u00b7na", "scheint", "dir", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "NE", "VVFIN", "PPER", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ihr Volk seinen Gang von deinem Laufen lerne.", "tokens": ["da\u00df", "ihr", "Volk", "sei\u00b7nen", "Gang", "von", "dei\u00b7nem", "Lau\u00b7fen", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Trit, Hesperus, trit auf und stelle dich ins Ferne.", "tokens": ["Trit", ",", "Hes\u00b7pe\u00b7rus", ",", "trit", "auf", "und", "stel\u00b7le", "dich", "ins", "Fer\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir um deine Zeit gerufen hat zu ihr,", "tokens": ["Die", "mir", "um", "dei\u00b7ne", "Zeit", "ge\u00b7ru\u00b7fen", "hat", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "steht vor dem Fenster schon und w\u00fcndschet sehr nach mir.", "tokens": ["steht", "vor", "dem", "Fens\u00b7ter", "schon", "und", "w\u00fcnd\u00b7schet", "sehr", "nach", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm, f\u00fchre mich zu ihr, bei der ich bin so gerne.", "tokens": ["Komm", ",", "f\u00fch\u00b7re", "mich", "zu", "ihr", ",", "bei", "der", "ich", "bin", "so", "ger\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "APPR", "PRELS", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der sinkend' Abend fleucht, die dunkle Nacht f\u00e4hrt aus.", "tokens": ["Der", "sin\u00b7kend'", "A\u00b7bend", "fleucht", ",", "die", "dunk\u00b7le", "Nacht", "f\u00e4hrt", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der finstre Schatten schleicht um Thetis blindes Haus,", "tokens": ["Der", "finst\u00b7re", "Schat\u00b7ten", "schleicht", "um", "The\u00b7tis", "blin\u00b7des", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die m\u00fcde Welt schl\u00e4ft, ein, die muntern L\u00fcfte wachen.", "tokens": ["die", "m\u00fc\u00b7de", "Welt", "schl\u00e4ft", ",", "ein", ",", "die", "mun\u00b7tern", "L\u00fcf\u00b7te", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo bleibst du? Ja, du k\u00f6mmst. So leite mich denn hin.", "tokens": ["Wo", "bleibst", "du", "?", "Ja", ",", "du", "k\u00f6mmst", ".", "So", "lei\u00b7te", "mich", "denn", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PTKANT", "$,", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich werde nicht eh' froh, als bis ich bei ihr bin,", "tokens": ["Ich", "wer\u00b7de", "nicht", "eh'", "froh", ",", "als", "bis", "ich", "bei", "ihr", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "APPR", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die auch die Traurigkeit selbselbst kan fr\u00f6lich machen.", "tokens": ["die", "auch", "die", "Trau\u00b7rig\u00b7keit", "selb\u00b7selbst", "kan", "fr\u00f6\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}