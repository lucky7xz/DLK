{"textgrid.poem.64770": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "17. Die Tochter", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mama, da\u00df Sie mich liebreich h\u00fcten,", "tokens": ["Ma\u00b7ma", ",", "da\u00df", "Sie", "mich", "lieb\u00b7reich", "h\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das kann ich Ihnen nicht verbieten;", "tokens": ["Das", "kann", "ich", "Ih\u00b7nen", "nicht", "ver\u00b7bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, ist gleich die Gefahr noch weit,", "tokens": ["Und", ",", "ist", "gleich", "die", "Ge\u00b7fahr", "noch", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dank' ich doch Ihrer Z\u00e4rtlichkeit.", "tokens": ["Dank'", "ich", "doch", "Ih\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nehm' ich mich nicht selbst in Acht,", "tokens": ["Doch", "nehm'", "ich", "mich", "nicht", "selbst", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd' ich nur umsonst bewacht.", "tokens": ["So", "werd'", "ich", "nur", "um\u00b7sonst", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vielleicht, was ich sonst nie begehrte,", "tokens": ["Viel\u00b7leicht", ",", "was", "ich", "sonst", "nie", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Reizt mich, nur weil man es mir wehrte;", "tokens": ["Reizt", "mich", ",", "nur", "weil", "man", "es", "mir", "wehr\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "KOUS", "PIS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Frey soll mich sanfte Tugend ziehn,", "tokens": ["Frey", "soll", "mich", "sanf\u00b7te", "Tu\u00b7gend", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch Fesseln brech' ich, sie zu fliehn.", "tokens": ["Doch", "Fes\u00b7seln", "brech'", "ich", ",", "sie", "zu", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, nehm' ich mich nicht selbst in Acht,", "tokens": ["Drum", ",", "nehm'", "ich", "mich", "nicht", "selbst", "in", "Acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd' ich doch umsonst bewacht.", "tokens": ["So", "werd'", "ich", "doch", "um\u00b7sonst", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nie wird den M\u00fcttern Klugheit sagen,", "tokens": ["Nie", "wird", "den", "M\u00fct\u00b7tern", "Klug\u00b7heit", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was muntre M\u00e4dchen listig wagen;", "tokens": ["Was", "mun\u00b7tre", "M\u00e4d\u00b7chen", "lis\u00b7tig", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ich keine Thorheit thu',", "tokens": ["Da\u00b7mit", "ich", "kei\u00b7ne", "Thor\u00b7heit", "thu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So trauen Sie mir Weisheit zu.", "tokens": ["So", "trau\u00b7en", "Sie", "mir", "Weis\u00b7heit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, nehm' ich mich nicht selbst in Acht,", "tokens": ["Drum", ",", "nehm'", "ich", "mich", "nicht", "selbst", "in", "Acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd' ich ganz umsonst bewacht.", "tokens": ["So", "werd'", "ich", "ganz", "um\u00b7sonst", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}