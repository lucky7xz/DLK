{"textgrid.poem.64659": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "286. Dichterh\u00f6he", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Reimern, deren Schwung die Erde nie verlor,", "tokens": ["Aus", "Rei\u00b7mern", ",", "de\u00b7ren", "Schwung", "die", "Er\u00b7de", "nie", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stieg ", "tokens": ["Stieg"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da\u00df nun, hoch \u00fcber ihm, viel junge Dichter schweben,", "tokens": ["Da\u00df", "nun", ",", "hoch", "\u00fc\u00b7ber", "ihm", ",", "viel", "jun\u00b7ge", "Dich\u00b7ter", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ADJD", "APPR", "PPER", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht, weil die B\u00e4lle sich durch spreizend Gas erheben.", "tokens": ["Macht", ",", "weil", "die", "B\u00e4l\u00b7le", "sich", "durch", "sprei\u00b7zend", "Gas", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PRF", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}