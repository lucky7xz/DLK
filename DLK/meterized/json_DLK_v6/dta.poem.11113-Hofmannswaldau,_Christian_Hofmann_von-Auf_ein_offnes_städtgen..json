{"dta.poem.11113": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein offnes st\u00e4dtgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehest deinen feind mit offnem angesicht.", "tokens": ["Du", "sie\u00b7hest", "dei\u00b7nen", "feind", "mit", "off\u00b7nem", "an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mauer ist dein zaun, das rathhaus deine schencke,", "tokens": ["Die", "mau\u00b7er", "ist", "dein", "zaun", ",", "das", "rath\u00b7haus", "dei\u00b7ne", "schen\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVINF", "$,", "PRELS", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das k\u00fch-horn deine post, der r\u00f6hr-trog deine tr\u00e4ncke.", "tokens": ["Das", "k\u00fch\u00b7horn", "dei\u00b7ne", "post", ",", "der", "r\u00f6hr\u00b7trog", "dei\u00b7ne", "tr\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An thoren fehlts dir wohl, jedoch an thooren nicht.", "tokens": ["An", "tho\u00b7ren", "fehlts", "dir", "wohl", ",", "je\u00b7doch", "an", "thoo\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}