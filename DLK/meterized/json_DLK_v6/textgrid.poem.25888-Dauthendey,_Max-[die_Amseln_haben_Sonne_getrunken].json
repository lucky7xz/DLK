{"textgrid.poem.25888": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[die Amseln haben Sonne getrunken]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Amseln haben Sonne getrunken,", "tokens": ["Die", "Am\u00b7seln", "ha\u00b7ben", "Son\u00b7ne", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus allen G\u00e4rten strahlen die Lieder,", "tokens": ["Aus", "al\u00b7len", "G\u00e4r\u00b7ten", "strah\u00b7len", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In allen Herzen nisten die Amseln,", "tokens": ["In", "al\u00b7len", "Her\u00b7zen", "nis\u00b7ten", "die", "Am\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alle Herzen werden zu G\u00e4rten", "tokens": ["Und", "al\u00b7le", "Her\u00b7zen", "wer\u00b7den", "zu", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und bl\u00fchen wieder.", "tokens": ["Und", "bl\u00fc\u00b7hen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nun wachsen der Erde die gro\u00dfen Fl\u00fcgel,", "tokens": ["Nun", "wach\u00b7sen", "der", "Er\u00b7de", "die", "gro\u00b7\u00dfen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und allen Tr\u00e4umen neues Gefieder,", "tokens": ["Und", "al\u00b7len", "Tr\u00e4u\u00b7men", "neu\u00b7es", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Alle Menschen werden wie V\u00f6gel", "tokens": ["Al\u00b7le", "Men\u00b7schen", "wer\u00b7den", "wie", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAINF", "KOKOM", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und bauen Nester im Blauen.", "tokens": ["Und", "bau\u00b7en", "Nes\u00b7ter", "im", "Blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun sprechen die B\u00e4ume in gr\u00fcnem Gedr\u00e4nge,", "tokens": ["Nun", "spre\u00b7chen", "die", "B\u00e4u\u00b7me", "in", "gr\u00fc\u00b7nem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und rauschen Ges\u00e4nge zur hohen Sonne,", "tokens": ["Und", "rau\u00b7schen", "Ge\u00b7s\u00e4n\u00b7ge", "zur", "ho\u00b7hen", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In allen Seelen badet die Sonne,", "tokens": ["In", "al\u00b7len", "See\u00b7len", "ba\u00b7det", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Alle Wasser stehen in Flammen,", "tokens": ["Al\u00b7le", "Was\u00b7ser", "ste\u00b7hen", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Fr\u00fchling bringt Wasser und Feuer", "tokens": ["Fr\u00fch\u00b7ling", "bringt", "Was\u00b7ser", "und", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Liebend zusammen.", "tokens": ["Lie\u00b7bend", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}