{"dta.poem.717": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  Vber die Geburt Jesu.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht mehr denn lichte Nacht! Nacht lichter als der Tag/ ", "tokens": ["Nacht", "mehr", "denn", "lich\u00b7te", "Nacht", "!", "Nacht", "lich\u00b7ter", "als", "der", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN", "$.", "NN", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nacht heller als die ", "tokens": ["Nacht", "hel\u00b7ler", "als", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KOKOM", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Gott/ der licht/ in licht wohnhafftig/ jhm erkohren:", "tokens": ["Das", "Gott", "/", "der", "licht", "/", "in", "licht", "wohn\u00b7haff\u00b7tig", "/", "jhm", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "APPR", "NN", "ADJD", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Nacht/ die alle N\u00e4cht\u2019 vnd T\u00e4ge trotzen mag.", "tokens": ["O", "Nacht", "/", "die", "al\u00b7le", "N\u00e4cht'", "vnd", "T\u00e4\u00b7ge", "trot\u00b7zen", "mag."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "NN", "$(", "ART", "PIAT", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O frewdenreiche Nacht/ in welcher ach vnd klag/", "tokens": ["O", "frew\u00b7den\u00b7rei\u00b7che", "Nacht", "/", "in", "wel\u00b7cher", "ach", "vnd", "klag", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "APPR", "PRELS", "XY", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd finsterni\u00df vnd was sich auff die welt verschworen", "tokens": ["Vnd", "fins\u00b7ter\u00b7ni\u00df", "vnd", "was", "sich", "auff", "die", "welt", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "PWS", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd furcht vnd h\u00f6llen angst vnd schrecken ward verlohren", "tokens": ["Vnd", "furcht", "vnd", "h\u00f6l\u00b7len", "angst", "vnd", "schre\u00b7cken", "ward", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "KON", "VVFIN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel bricht! doch f\u00e4lt nuhmehr kein donnerschlag. ", "tokens": ["Der", "Him\u00b7mel", "bricht", "!", "doch", "f\u00e4lt", "nuh\u00b7mehr", "kein", "don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der zeit vnd n\u00e4chte schuff ist diese Nacht ankommen!", "tokens": ["Der", "zeit", "vnd", "n\u00e4ch\u00b7te", "schuff", "ist", "die\u00b7se", "Nacht", "an\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADJD", "VAFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd hat das recht der zeit/ vnd fleisch an sich genommen!", "tokens": ["Vnd", "hat", "das", "recht", "der", "zeit", "/", "vnd", "fleisch", "an", "sich", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$(", "KON", "ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd vnser fleisch vnd zeit der ewigkeit vermacht.", "tokens": ["Vnd", "vn\u00b7ser", "fleisch", "vnd", "zeit", "der", "e\u00b7wig\u00b7keit", "ver\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Jammer tr\u00fcbe nacht die schwartze nacht der S\u00fcnden", "tokens": ["Der", "Jam\u00b7mer", "tr\u00fc\u00b7be", "nacht", "die", "schwart\u00b7ze", "nacht", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Grabes dunckelheit/ mu\u00df durch die nacht verschwinden. ", "tokens": ["Des", "Gra\u00b7bes", "dun\u00b7ckel\u00b7heit", "/", "mu\u00df", "durch", "die", "nacht", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VMFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nacht lichter als der Tag; Nacht mehr denn lichte Nacht!", "tokens": ["Nacht", "lich\u00b7ter", "als", "der", "Tag", ";", "Nacht", "mehr", "denn", "lich\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ART", "NN", "$.", "NN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}}}}}