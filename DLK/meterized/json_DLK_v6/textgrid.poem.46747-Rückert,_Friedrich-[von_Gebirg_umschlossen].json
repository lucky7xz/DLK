{"textgrid.poem.46747": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von Gebirg umschlossen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Gebirg umschlossen,", "tokens": ["Von", "Ge\u00b7birg", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Rings von Flut umflossen,", "tokens": ["Rings", "von", "Flut", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Geht kein Weg von dir,", "tokens": ["Geht", "kein", "Weg", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Welt, herein zu mir.", "tokens": ["Welt", ",", "her\u00b7ein", "zu", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Keine Stimme klinget", "tokens": ["Kei\u00b7ne", "Stim\u00b7me", "klin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und kein Auge dringet", "tokens": ["Und", "kein", "Au\u00b7ge", "drin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In die stille Bucht,", "tokens": ["In", "die", "stil\u00b7le", "Bucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die mein Schmerz gesucht.", "tokens": ["Die", "mein", "Schmerz", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Keine fremden L\u00fcfte", "tokens": ["Kei\u00b7ne", "frem\u00b7den", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bringen fremde D\u00fcfte,", "tokens": ["Brin\u00b7gen", "frem\u00b7de", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oder fremden Schein,", "tokens": ["O\u00b7der", "frem\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aus der Flur herein.", "tokens": ["Aus", "der", "Flur", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schatten dicht gewoben!", "tokens": ["Schat\u00b7ten", "dicht", "ge\u00b7wo\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alles kommt von oben,", "tokens": ["Al\u00b7les", "kommt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Luft und Duft und Licht,", "tokens": ["Luft", "und", "Duft", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Was hier mit mir spricht.", "tokens": ["Was", "hier", "mit", "mir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Schatten dicht gewoben!", "tokens": ["Schat\u00b7ten", "dicht", "ge\u00b7wo\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alles geht nach oben,", "tokens": ["Al\u00b7les", "geht", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blumenglanz und Hauch,", "tokens": ["Blu\u00b7men\u00b7glanz", "und", "Hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Blick und Seufzer auch.", "tokens": ["Blick", "und", "Seuf\u00b7zer", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}