{"textgrid.poem.45585": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Systeme", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprecht ihr gelehrt von Epigonen,", "tokens": ["Sprecht", "ihr", "ge\u00b7lehrt", "von", "E\u00b7pi\u00b7go\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So verge\u00dft nicht die Diadochen,", "tokens": ["So", "ver\u00b7ge\u00dft", "nicht", "die", "Dia\u00b7do\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher w\u00fcrde uns mit Grillen verschonen,", "tokens": ["Man\u00b7cher", "w\u00fcr\u00b7de", "uns", "mit", "Gril\u00b7len", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "H\u00e4tte nicht ein Vorg\u00e4nger Unsinn gesprochen.", "tokens": ["H\u00e4t\u00b7te", "nicht", "ein", "Vor\u00b7g\u00e4n\u00b7ger", "Un\u00b7sinn", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}