{"textgrid.poem.45732": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Indirekter Liberalismus", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr strebt gar hei\u00df nach Freiheit,", "tokens": ["Ihr", "strebt", "gar", "hei\u00df", "nach", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich w\u00fcnsch euch gleichfalls frei;", "tokens": ["Ich", "w\u00fcnsch", "euch", "gleich\u00b7falls", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber nicht, damit ihr frei seid,", "tokens": ["A\u00b7ber", "nicht", ",", "da\u00b7mit", "ihr", "frei", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Damits die ", "tokens": ["Da\u00b7mits", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}