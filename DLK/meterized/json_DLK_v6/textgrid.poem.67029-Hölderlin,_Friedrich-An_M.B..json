{"textgrid.poem.67029": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An M.B.", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O l\u00e4chle fr\u00f6hlich unschuldsvolle Freuden,", "tokens": ["O", "l\u00e4ch\u00b7le", "fr\u00f6h\u00b7lich", "un\u00b7schulds\u00b7vol\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, muntrer Knabe, freue dich,", "tokens": ["Ja", ",", "mun\u00b7trer", "Kna\u00b7be", ",", "freu\u00b7e", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unbek\u00fcmmert, gleich dem Lamm auf Fr\u00fchlingsheiden,", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "gleich", "dem", "Lamm", "auf", "Fr\u00fch\u00b7lings\u00b7hei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entwickeln deine Keime sich.", "tokens": ["Ent\u00b7wi\u00b7ckeln", "dei\u00b7ne", "Kei\u00b7me", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht Sorgen und kein Heer von Leidenschaften", "tokens": ["Nicht", "Sor\u00b7gen", "und", "kein", "Heer", "von", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "KON", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Str\u00f6mt \u00fcber deine Seele hin;", "tokens": ["Str\u00f6mt", "\u00fc\u00b7ber", "dei\u00b7ne", "See\u00b7le", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sahst noch nicht, wie tolle Toren neidisch gafften,", "tokens": ["Du", "sahst", "noch", "nicht", ",", "wie", "tol\u00b7le", "To\u00b7ren", "nei\u00b7disch", "gaff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann sie die Tugend sehen bl\u00fchn.", "tokens": ["Wann", "sie", "die", "Tu\u00b7gend", "se\u00b7hen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich sucht noch nicht des k\u00fchnen L\u00e4strers Zunge:", "tokens": ["Dich", "sucht", "noch", "nicht", "des", "k\u00fch\u00b7nen", "L\u00e4st\u00b7rers", "Zun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst lobt sie, doch ihr Schlangengift", "tokens": ["Erst", "lobt", "sie", ",", "doch", "ihr", "Schlan\u00b7gen\u00b7gift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwandelt bald das Lob, das sie so gl\u00e4nzend sunge,", "tokens": ["Ver\u00b7wan\u00b7delt", "bald", "das", "Lob", ",", "das", "sie", "so", "gl\u00e4n\u00b7zend", "sun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Tadel, welcher t\u00f6dlich trifft.", "tokens": ["In", "Ta\u00b7del", ",", "wel\u00b7cher", "t\u00f6d\u00b7lich", "trifft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du glaubst mir nicht, da\u00df diese sch\u00f6ne Erde", "tokens": ["Du", "glaubst", "mir", "nicht", ",", "da\u00df", "die\u00b7se", "sch\u00f6\u00b7ne", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So viele Unzufriedne tr\u00e4gt,", "tokens": ["So", "vie\u00b7le", "Un\u00b7zu\u00b7fried\u00b7ne", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht der Welt, der dich der Sch\u00f6pfer gab, Beschwerde,", "tokens": ["Da\u00df", "nicht", "der", "Welt", ",", "der", "dich", "der", "Sch\u00f6p\u00b7fer", "gab", ",", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur eigner Kummer Seufzen regt.", "tokens": ["Nur", "eig\u00b7ner", "Kum\u00b7mer", "Seuf\u00b7zen", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJA", "NN", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So folge ihr, du edle gute Seele,", "tokens": ["So", "fol\u00b7ge", "ihr", ",", "du", "ed\u00b7le", "gu\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin dich nur die Tugend treibt,", "tokens": ["Wo\u00b7hin", "dich", "nur", "die", "Tu\u00b7gend", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich: Welt! kein leerer Schatten ists, das ich mir w\u00e4hle,", "tokens": ["Sprich", ":", "Welt", "!", "kein", "lee\u00b7rer", "Schat\u00b7ten", "ists", ",", "das", "ich", "mir", "w\u00e4h\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$.", "PIAT", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Weisheit, die mir ewig bleibt.", "tokens": ["Nur", "Weis\u00b7heit", ",", "die", "mir", "e\u00b7wig", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}