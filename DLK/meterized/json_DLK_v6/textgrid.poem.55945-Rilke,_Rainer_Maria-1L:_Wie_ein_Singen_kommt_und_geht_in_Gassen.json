{"textgrid.poem.55945": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ein Singen kommt und geht in Gassen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Singen kommt und geht in Gassen", "tokens": ["Wie", "ein", "Sin\u00b7gen", "kommt", "und", "geht", "in", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und sich n\u00e4hert und sich wieder scheut,", "tokens": ["und", "sich", "n\u00e4\u00b7hert", "und", "sich", "wie\u00b7der", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "KON", "PRF", "ADV", "VVFIN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "fl\u00fcgelschlagend, manchmal fast zu fassen", "tokens": ["fl\u00fc\u00b7gel\u00b7schla\u00b7gend", ",", "manch\u00b7mal", "fast", "zu", "fas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und dann wieder weit hinausgestreut:", "tokens": ["und", "dann", "wie\u00b7der", "weit", "hin\u00b7aus\u00b7ge\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "spielt mit der Genesenden das Leben;", "tokens": ["spielt", "mit", "der", "Ge\u00b7ne\u00b7sen\u00b7den", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "w\u00e4hrend sie, geschw\u00e4cht und ausgeruht,", "tokens": ["w\u00e4h\u00b7rend", "sie", ",", "ge\u00b7schw\u00e4cht", "und", "aus\u00b7ge\u00b7ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "unbeholfen, um sich hinzugeben,", "tokens": ["un\u00b7be\u00b7hol\u00b7fen", ",", "um", "sich", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "eine ungewohnte Geste tut.", "tokens": ["ei\u00b7ne", "un\u00b7ge\u00b7wohn\u00b7te", "Ges\u00b7te", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und sie f\u00fchlt es beinah wie Verf\u00fchrung,", "tokens": ["Und", "sie", "f\u00fchlt", "es", "bei\u00b7nah", "wie", "Ver\u00b7f\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wenn die hartgewordne Hand, darin", "tokens": ["wenn", "die", "hart\u00b7ge\u00b7word\u00b7ne", "Hand", ",", "da\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fieber waren voller Widersinn,", "tokens": ["Fie\u00b7ber", "wa\u00b7ren", "vol\u00b7ler", "Wi\u00b7der\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "fernher, wie mit bl\u00fchender Ber\u00fchrung,", "tokens": ["fern\u00b7her", ",", "wie", "mit", "bl\u00fc\u00b7hen\u00b7der", "Be\u00b7r\u00fch\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "zu liebkosen kommt ihr hartes Kinn.", "tokens": ["zu", "lieb\u00b7ko\u00b7sen", "kommt", "ihr", "har\u00b7tes", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}}}}}