{"textgrid.poem.42387": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du mir Kinder schenktest, Stern", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mir Kinder schenktest, Stern", "tokens": ["Der", "du", "mir", "Kin\u00b7der", "schenk\u00b7test", ",", "Stern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "PPER", "NN", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens, dass ich wandle", "tokens": ["Des", "Le\u00b7bens", ",", "dass", "ich", "wand\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie tr\u00e4umend, selig bis zum Kern \u2013", "tokens": ["Wie", "tr\u00e4u\u00b7mend", ",", "se\u00b7lig", "bis", "zum", "Kern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "ADJD", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allmacht, aus der mir F\u00fclle quillt,", "tokens": ["All\u00b7macht", ",", "aus", "der", "mir", "F\u00fcl\u00b7le", "quillt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dass mit jedem Tage", "tokens": ["Dass", "mit", "je\u00b7dem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ich neu entbrenne", "tokens": ["Ich", "neu", "ent\u00b7bren\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und Ehrfurcht trage", "tokens": ["Und", "Ehr\u00b7furcht", "tra\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In tiefster Brust \u2013 ich nenne", "tokens": ["In", "tiefs\u00b7ter", "Brust", "\u2013", "ich", "nen\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dich Gl\u00fcck, das mit geliebtem Munde", "tokens": ["Dich", "Gl\u00fcck", ",", "das", "mit", "ge\u00b7lieb\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mir l\u00e4chelt, ausersehen,", "tokens": ["Mir", "l\u00e4\u00b7chelt", ",", "au\u00b7ser\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und geweiht", "tokens": ["Und", "ge\u00b7weiht"], "token_info": ["word", "word"], "pos": ["KON", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Vom Genius der Liebe \u2013", "tokens": ["Vom", "Ge\u00b7nius", "der", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Und weiss: du bist im Grunde", "tokens": ["Und", "weiss", ":", "du", "bist", "im", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Unendliches Vergehen,", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "Ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Schmerz, Trauer, Ewigkeit.", "tokens": ["Schmerz", ",", "Trau\u00b7er", ",", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}