{"dta.poem.15931": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "136.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ehrgeiz gibt nicht Ruh noch Rast dem, der ihn hegt;", "tokens": ["Der", "Ehr\u00b7geiz", "gibt", "nicht", "Ruh", "noch", "Rast", "dem", ",", "der", "ihn", "hegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihm ist, wie vom Sturm die Flut, das Herz bewegt.", "tokens": ["Von", "ihm", "ist", ",", "wie", "vom", "Sturm", "die", "Flut", ",", "das", "Herz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bei einem Mann der That ist er villeicht zu loben;", "tokens": ["Bei", "ei\u00b7nem", "Mann", "der", "That", "ist", "er", "vil\u00b7leicht", "zu", "lo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sei davon gespornt, getragen und gehoben!", "tokens": ["Er", "sei", "da\u00b7von", "ge\u00b7spornt", ",", "ge\u00b7tra\u00b7gen", "und", "ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df er den innern Sturm durch \u00e4u\u00dfre St\u00fcrme d\u00e4mpfe;", "tokens": ["Da\u00df", "er", "den", "in\u00b7nern", "Sturm", "durch", "\u00e4u\u00df\u00b7re", "St\u00fcr\u00b7me", "d\u00e4mp\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie ihn nagt sein Wurm, bet\u00e4ub' er ihn durch K\u00e4mpfe!", "tokens": ["Und", "wie", "ihn", "nagt", "sein", "Wurm", ",", "be\u00b7t\u00e4ub'", "er", "ihn", "durch", "K\u00e4mp\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Allein bei Wissenschaft und Kunst ist ganz ein Fluch", "tokens": ["Al\u00b7lein", "bei", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", "ist", "ganz", "ein", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ehrgeiz, unstatthaft, ein innrer Widerspruch.", "tokens": ["Der", "Ehr\u00b7geiz", ",", "un\u00b7statt\u00b7haft", ",", "ein", "inn\u00b7rer", "Wi\u00b7der\u00b7spruch", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn mit der Ruh kann nicht die Unruh sich vertragen;", "tokens": ["Denn", "mit", "der", "Ruh", "kann", "nicht", "die", "Un\u00b7ruh", "sich", "ver\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PTKNEG", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh'r Geiz, als Ehrgeiz, l\u00e4\u00dft in Mu\u00dfe sich ertragen.", "tokens": ["Eh'r", "Geiz", ",", "als", "Ehr\u00b7geiz", ",", "l\u00e4\u00dft", "in", "Mu\u00b7\u00dfe", "sich", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "NN", "$,", "VVFIN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}