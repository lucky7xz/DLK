{"textgrid.poem.58101": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "139. Das Leben in Gott", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fisch wohl hundertmal mit Lust in einer Stunde", "tokens": ["Ein", "Fisch", "wohl", "hun\u00b7dert\u00b7mal", "mit", "Lust", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald in die H\u00f6he steigt, bald sinkt in 's Wassers Grunde,", "tokens": ["Bald", "in", "die", "H\u00f6\u00b7he", "steigt", ",", "bald", "sinkt", "in", "'s", "Was\u00b7sers", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er spielt gar sanft und still beim warmen Sonnenschein.", "tokens": ["Er", "spielt", "gar", "sanft", "und", "still", "beim", "war\u00b7men", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich auch im Element der Gottheit stets so lebe,", "tokens": ["Ich", "auch", "im", "E\u00b7le\u00b7ment", "der", "Got\u00b7theit", "stets", "so", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald kehr' ich ein in 'n Grund, bald mich im Geist erhebe", "tokens": ["Bald", "kehr'", "ich", "ein", "in", "'n", "Grund", ",", "bald", "mich", "im", "Geist", "er\u00b7he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "$,", "ADV", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und finde \u00fcberall mein'n Ruhpunkt, so ich mein'.", "tokens": ["Und", "fin\u00b7de", "\u00fc\u00b7be\u00b7rall", "mein'n", "Ruh\u00b7punkt", ",", "so", "ich", "mein'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}