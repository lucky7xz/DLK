{"textgrid.poem.65174": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Marthe, Baumkirchers einzig Kind,", "tokens": ["Frau", "Mar\u00b7the", ",", "Baum\u00b7kir\u00b7chers", "ein\u00b7zig", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mahnt ab, mit ahnendem Grauen:", "tokens": ["Mahnt", "ab", ",", "mit", "ah\u00b7nen\u00b7dem", "Grau\u00b7en", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbihr wi\u00dft, wie b\u00f6se sie Euch gesinnt,", "tokens": ["\u00bb", "ihr", "wi\u00dft", ",", "wie", "b\u00f6\u00b7se", "sie", "Euch", "ge\u00b7sinnt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wollt Euch ihnen vertrauen?", "tokens": ["Und", "wollt", "Euch", "ih\u00b7nen", "ver\u00b7trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich s\u00e4he Euch lieber von Priesterhand", "tokens": ["Ich", "s\u00e4\u00b7he", "Euch", "lie\u00b7ber", "von", "Pries\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gesalbt schon mit heil'gem Oele!", "tokens": ["Ge\u00b7salbt", "schon", "mit", "heil'\u00b7gem", "O\u00b7e\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ihr waget Euch an des Abgrund's Rand,", "tokens": ["Ihr", "wa\u00b7get", "Euch", "an", "des", "Ab\u00b7grun\u00b7d's", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ihr geht in des Drachen H\u00f6hle!\u00ab", "tokens": ["Ihr", "geht", "in", "des", "Dra\u00b7chen", "H\u00f6h\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Baumkircher blickt sie mit Strenge an:", "tokens": ["Baum\u00b7kir\u00b7cher", "blickt", "sie", "mit", "Stren\u00b7ge", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00bbwie magst du so th\u00f6richt sprechen?", "tokens": ["\u00bb", "wie", "magst", "du", "so", "th\u00f6\u00b7richt", "spre\u00b7chen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Wort h\u00e4lt jeder ehrliche Mann,", "tokens": ["Sein", "Wort", "h\u00e4lt", "je\u00b7der", "ehr\u00b7li\u00b7che", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wird seines der Kaiser brechen?", "tokens": ["Wird", "sei\u00b7nes", "der", "Kai\u00b7ser", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sein Schutzbrief sichert mir frei Geleit;", "tokens": ["Sein", "Schutz\u00b7brief", "si\u00b7chert", "mir", "frei", "Ge\u00b7leit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Was magst du noch mehr verlangen?", "tokens": ["Was", "magst", "du", "noch", "mehr", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn einer, gilt eines F\u00fcrsten Eid!", "tokens": ["Wenn", "ei\u00b7ner", ",", "gilt", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Eid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.8": {"text": "Drum la\u00df das Zagen und Bangen.\u00ab", "tokens": ["Drum", "la\u00df", "das", "Za\u00b7gen", "und", "Ban\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbschon einmal saht Ihr mit seinem Wort", "tokens": ["\u00bb", "schon", "ein\u00b7mal", "saht", "Ihr", "mit", "sei\u00b7nem", "Wort"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Nach Willk\u00fcr den Kaiser schalten!", "tokens": ["Nach", "Will\u00b7k\u00fcr", "den", "Kai\u00b7ser", "schal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz aller Mahnung ihn fort und fort", "tokens": ["Trotz", "al\u00b7ler", "Mah\u00b7nung", "ihn", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER", "PTKVZ", "KON", "PTKVZ"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das Eu're Euch vorenthalten!\u00ab", "tokens": ["Das", "Eu'\u00b7re", "Euch", "vor\u00b7ent\u00b7hal\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Baumkircher f\u00e4hrt auf voll Ungeduld:", "tokens": ["Baum\u00b7kir\u00b7cher", "f\u00e4hrt", "auf", "voll", "Un\u00b7ge\u00b7duld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "\u00bbda war er nur schlecht beraten!", "tokens": ["\u00bb", "da", "war", "er", "nur", "schlecht", "be\u00b7ra\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Schranzen war's und der Schreiber Schuld!", "tokens": ["Der", "Schran\u00b7zen", "wa\u00b7r's", "und", "der", "Schrei\u00b7ber", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Er war nicht Herr seiner Thaten!", "tokens": ["Er", "war", "nicht", "Herr", "sei\u00b7ner", "Tha\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Jetzt endlich hat er die Schliche erkannt", "tokens": ["Jetzt", "end\u00b7lich", "hat", "er", "die", "Schli\u00b7che", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der L\u00fcgenbrut, der gemeinen!", "tokens": ["Der", "L\u00fc\u00b7gen\u00b7brut", ",", "der", "ge\u00b7mei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich fasse die mir gebot'ne Hand", "tokens": ["Ich", "fas\u00b7se", "die", "mir", "ge\u00b7bot'\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PPER", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und halte sie fest in der meinen.", "tokens": ["Und", "hal\u00b7te", "sie", "fest", "in", "der", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O schwer und bitter hat mich's ged\u00fcnkt,", "tokens": ["O", "schwer", "und", "bit\u00b7ter", "hat", "mich's", "ge\u00b7d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mich gegen den Herrn zu wenden!", "tokens": ["Mich", "ge\u00b7gen", "den", "Herrn", "zu", "wen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch nun mir neu seine Gnade winkt,", "tokens": ["Doch", "nun", "mir", "neu", "sei\u00b7ne", "Gna\u00b7de", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wird all dieses Wirrsal enden!\u00ab", "tokens": ["Wird", "all", "die\u00b7ses", "Wirr\u00b7sal", "en\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "PDAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Frau Martha senket das Haupt im Harm,", "tokens": ["Frau", "Mar\u00b7tha", "sen\u00b7ket", "das", "Haupt", "im", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie kann die Sorge nicht bannen.", "tokens": ["Sie", "kann", "die", "Sor\u00b7ge", "nicht", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beschw\u00f6rend fa\u00dft sie des Vaters Arm,", "tokens": ["Be\u00b7schw\u00f6\u00b7rend", "fa\u00dft", "sie", "des", "Va\u00b7ters", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fleht: \u00bbO zieh nicht von dannen!", "tokens": ["Und", "fleht", ":", "\u00bb", "O", "zieh", "nicht", "von", "dan\u00b7nen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "VVFIN", "PTKNEG", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df t\u00fcckisch lauernd das Ungl\u00fcck wacht,", "tokens": ["Da\u00df", "t\u00fc\u00b7ckisch", "lau\u00b7ernd", "das", "Un\u00b7gl\u00fcck", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "De\u00df ward mir sichere Kunde:", "tokens": ["De\u00df", "ward", "mir", "si\u00b7che\u00b7re", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Es schrie das K\u00e4uzlein die ganze Nacht,", "tokens": ["Es", "schrie", "das", "K\u00e4uz\u00b7lein", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Im Hofe heulten die Hunde!\u00ab", "tokens": ["Im", "Ho\u00b7fe", "heul\u00b7ten", "die", "Hun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Ritter lacht. \u00bbDas arme Getier,", "tokens": ["Der", "Rit\u00b7ter", "lacht", ".", "\u00bb", "Das", "ar\u00b7me", "Ge\u00b7tier", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das also j\u00e4mmerlich klagte!", "tokens": ["Das", "al\u00b7so", "j\u00e4m\u00b7mer\u00b7lich", "klag\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Zeichen scheint mir's, untr\u00fcglich schier,", "tokens": ["Ein", "Zei\u00b7chen", "scheint", "mir's", ",", "un\u00b7tr\u00fcg\u00b7lich", "schier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df arger Hunger es plagte.", "tokens": ["Da\u00df", "ar\u00b7ger", "Hun\u00b7ger", "es", "plag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch nun lebe wohl! sei froh gefa\u00dft!", "tokens": ["Doch", "nun", "le\u00b7be", "wohl", "!", "sei", "froh", "ge\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$.", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bald siehst du, von hoher Warte,", "tokens": ["Bald", "siehst", "du", ",", "von", "ho\u00b7her", "War\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mich wiederkehren in freud'ger Hast!", "tokens": ["Mich", "wie\u00b7der\u00b7keh\u00b7ren", "in", "freu\u00b7d'\u00b7ger", "Hast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Leb wohl, meine traute Marthe!\u00ab", "tokens": ["Leb", "wohl", ",", "mei\u00b7ne", "trau\u00b7te", "Mar\u00b7the", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "VVFIN", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Er k\u00fc\u00dft sie z\u00e4rtlich auf Stirn und Wang',", "tokens": ["Er", "k\u00fc\u00dft", "sie", "z\u00e4rt\u00b7lich", "auf", "Stirn", "und", "Wang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er winket und gr\u00fc\u00dfet munter,", "tokens": ["Er", "win\u00b7ket", "und", "gr\u00fc\u00b7\u00dfet", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann sprengt er vom steilen Felsenhang", "tokens": ["Dann", "sprengt", "er", "vom", "stei\u00b7len", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Burg in d