{"dta.poem.18017": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "260.  Rein macht GOtt Gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts unreins komt zu Gott! bistu nicht f\u00fcnkel rein", "tokens": ["Nichts", "un\u00b7reins", "komt", "zu", "Gott", "!", "bis\u00b7tu", "nicht", "f\u00fcn\u00b7kel", "rein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADJA", "VVFIN", "APPR", "NN", "$.", "ADV", "PTKNEG", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von aller Creatur/ so wirst ihm nie gemein.", "tokens": ["Von", "al\u00b7ler", "Crea\u00b7tur", "/", "so", "wirst", "ihm", "nie", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}