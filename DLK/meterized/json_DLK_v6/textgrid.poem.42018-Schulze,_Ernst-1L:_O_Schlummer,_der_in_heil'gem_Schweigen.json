{"textgrid.poem.42018": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Schlummer, der in heil'gem Schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schlummer, der in heil'gem Schweigen", "tokens": ["O", "Schlum\u00b7mer", ",", "der", "in", "heil'\u00b7gem", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Rand der Quellen hingelehnt,", "tokens": ["Am", "Rand", "der", "Quel\u00b7len", "hin\u00b7ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich tr\u00e4umerisch mit Bl\u00fcthenzweigen", "tokens": ["Sich", "tr\u00e4u\u00b7me\u00b7risch", "mit", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ewig heitre Stirn umkr\u00f6nt,", "tokens": ["Die", "e\u00b7wig", "heit\u00b7re", "Stirn", "um\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du holder, goldgelockter Knabe,", "tokens": ["Du", "hol\u00b7der", ",", "gold\u00b7ge\u00b7lock\u00b7ter", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schutzengel \u00fcber Wieg' und Grab,", "tokens": ["Schut\u00b7zen\u00b7gel", "\u00fc\u00b7ber", "Wieg'", "und", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Komm zu der Leidenden herab", "tokens": ["Komm", "zu", "der", "Lei\u00b7den\u00b7den", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mit deinem linden Zauberstabe.", "tokens": ["Mit", "dei\u00b7nem", "lin\u00b7den", "Zau\u00b7ber\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehst du, wie er herniederschwebt,", "tokens": ["Siehst", "du", ",", "wie", "er", "her\u00b7nie\u00b7der\u00b7schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umgl\u00e4nzt von zarter Mondeshelle?", "tokens": ["Um\u00b7gl\u00e4nzt", "von", "zar\u00b7ter", "Mon\u00b7des\u00b7hel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Waldes dunkler Gipfel bebt,", "tokens": ["Des", "Wal\u00b7des", "dunk\u00b7ler", "Gip\u00b7fel", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leiser rauscht die fl\u00fccht'ge Quelle.", "tokens": ["Und", "lei\u00b7ser", "rauscht", "die", "fl\u00fccht'\u00b7ge", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Still wie der Thau von n\u00e4cht'gen H\u00f6hn,", "tokens": ["Still", "wie", "der", "Thau", "von", "n\u00e4cht'\u00b7gen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Harfenton aus finstrer Weite,", "tokens": ["Wie", "Har\u00b7fen\u00b7ton", "aus", "finst\u00b7rer", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Umschlingt er sie mit Liebeswehn", "tokens": ["Um\u00b7schlingt", "er", "sie", "mit", "Lie\u00b7bes\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und flieht mit seiner holden Beute.", "tokens": ["Und", "flieht", "mit", "sei\u00b7ner", "hol\u00b7den", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O bette sie auf weiches Wiesengr\u00fcn,", "tokens": ["O", "bet\u00b7te", "sie", "auf", "wei\u00b7ches", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df zart um sie ein duft'ger Flor sich schmiege;", "tokens": ["Da\u00df", "zart", "um", "sie", "ein", "duft'\u00b7ger", "Flor", "sich", "schmie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwachet rings des Waldes Harmonien,", "tokens": ["Er\u00b7wa\u00b7chet", "rings", "des", "Wal\u00b7des", "Har\u00b7mo\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sich ihr Herz auf leisen Kl\u00e4ngen wiege;", "tokens": ["Da\u00df", "sich", "ihr", "Herz", "auf", "lei\u00b7sen", "Kl\u00e4n\u00b7gen", "wie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ihr Tr\u00e4ume naht, den Zauberkreis zu ziehn,", "tokens": ["Ihr", "Tr\u00e4u\u00b7me", "naht", ",", "den", "Zau\u00b7ber\u00b7kreis", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Worin die Lust den wachen Schmerz betr\u00fcge;", "tokens": ["Wo\u00b7rin", "die", "Lust", "den", "wa\u00b7chen", "Schmerz", "be\u00b7tr\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Engel m\u00f6g' an ihrer Seite knien", "tokens": ["Ein", "En\u00b7gel", "m\u00f6g'", "an", "ih\u00b7rer", "Sei\u00b7te", "kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und auf die Stirn ihr k\u00fchle Lindrung hauchen;", "tokens": ["Und", "auf", "die", "Stirn", "ihr", "k\u00fch\u00b7le", "Lind\u00b7rung", "hau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Fern soll die Nacht der Wirklichkeit entfliehn,", "tokens": ["Fern", "soll", "die", "Nacht", "der", "Wirk\u00b7lich\u00b7keit", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Und gl\u00e4ubig sich auf s\u00fc\u00dfen Phantasien", "tokens": ["Und", "gl\u00e4u\u00b7big", "sich", "auf", "s\u00fc\u00b7\u00dfen", "Phan\u00b7ta\u00b7si\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ihr Geist in's Licht der ew'gen Klarheit tauchen.", "tokens": ["Ihr", "Geist", "in's", "Licht", "der", "ew'\u00b7gen", "Klar\u00b7heit", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, viel des Grams hat diese dunkle Welt,", "tokens": ["Ach", ",", "viel", "des", "Grams", "hat", "die\u00b7se", "dunk\u00b7le", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vergebens sucht der Geist sie zu verkl\u00e4ren;", "tokens": ["Ver\u00b7ge\u00b7bens", "sucht", "der", "Geist", "sie", "zu", "ver\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne steigt; vom goldnen Glanz erhellt", "tokens": ["Die", "Son\u00b7ne", "steigt", ";", "vom", "gold\u00b7nen", "Glanz", "er\u00b7hellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Prangt zauberisch Gebirg' und Thal und Feld:", "tokens": ["Prangt", "zau\u00b7be\u00b7risch", "Ge\u00b7bir\u00b7g'", "und", "Thal", "und", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch senkt auch rings der Thau die leisen Z\u00e4hren.", "tokens": ["Doch", "senkt", "auch", "rings", "der", "Thau", "die", "lei\u00b7sen", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Kelch der reinsten Freude wohnt der Schmerz,", "tokens": ["Im", "Kelch", "der", "reins\u00b7ten", "Freu\u00b7de", "wohnt", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Wehmuth sinkt vom heitern Himmel nieder;", "tokens": ["Die", "Weh\u00b7muth", "sinkt", "vom", "hei\u00b7tern", "Him\u00b7mel", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vergebens hofft und tr\u00e4umt das weiche Herz,", "tokens": ["Ver\u00b7ge\u00b7bens", "hofft", "und", "tr\u00e4umt", "das", "wei\u00b7che", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was einmal schwand giebt ihm die Welt nicht wieder.", "tokens": ["Was", "ein\u00b7mal", "schwand", "giebt", "ihm", "die", "Welt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, uns umf\u00e4ngt ein unbekanntes Land,", "tokens": ["Ach", ",", "uns", "um\u00b7f\u00e4ngt", "ein", "un\u00b7be\u00b7kann\u00b7tes", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind allein hier in dem weiten Raume;", "tokens": ["Wir", "sind", "al\u00b7lein", "hier", "in", "dem", "wei\u00b7ten", "Rau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was uns begl\u00fcckt, das bl\u00fchte nur im Traume,", "tokens": ["Was", "uns", "be\u00b7gl\u00fcckt", ",", "das", "bl\u00fch\u00b7te", "nur", "im",