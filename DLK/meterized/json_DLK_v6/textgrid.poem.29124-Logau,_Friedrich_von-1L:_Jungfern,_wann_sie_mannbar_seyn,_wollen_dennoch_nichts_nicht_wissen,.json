{"textgrid.poem.29124": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jungfern, wann sie mannbar seyn, wollen dennoch nichts nicht wissen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfern, wann sie mannbar seyn, wollen dennoch nichts nicht wissen,", "tokens": ["Jung\u00b7fern", ",", "wann", "sie", "mann\u00b7bar", "seyn", ",", "wol\u00b7len", "den\u00b7noch", "nichts", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VAINF", "$,", "VMFIN", "ADV", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Was ein Mann sey f\u00fcr ein Ding, wie ein Mann sey zu geniessen;", "tokens": ["Was", "ein", "Mann", "sey", "f\u00fcr", "ein", "Ding", ",", "wie", "ein", "Mann", "sey", "zu", "ge\u00b7nies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Weil sie aber meistens doch lieber jung als alte nemen,", "tokens": ["Weil", "sie", "a\u00b7ber", "meis\u00b7tens", "doch", "lie\u00b7ber", "jung", "als", "al\u00b7te", "ne\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "ADJA", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Fehlt es nicht, sie haben Wind, was dabey sey f\u00fcr bequemen.", "tokens": ["Fehlt", "es", "nicht", ",", "sie", "ha\u00b7ben", "Wind", ",", "was", "da\u00b7bey", "sey", "f\u00fcr", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "NN", "$,", "PRELS", "PAV", "VAFIN", "APPR", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}