{"textgrid.poem.26440": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlie\u00dft eure T\u00fcren nicht vor mir, ihr stolzen Bibliotheken!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00dft eure T\u00fcren nicht vor mir, ihr stolzen Bibliotheken!", "tokens": ["Schlie\u00dft", "eu\u00b7re", "T\u00fc\u00b7ren", "nicht", "vor", "mir", ",", "ihr", "stol\u00b7zen", "Bib\u00b7lio\u00b7the\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Denn was auf all euren wohlgef\u00fcllten Regalen fehlt und am meisten not tut, das bringe ich.", "tokens": ["Denn", "was", "auf", "all", "eu\u00b7ren", "wohl\u00b7ge\u00b7f\u00fcll\u00b7ten", "Re\u00b7ga\u00b7len", "fehlt", "und", "am", "meis\u00b7ten", "not", "tut", ",", "das", "brin\u00b7ge", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PIAT", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "APPRART", "ADJA", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+-+--+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Aus dem Kriege auftauchend hab' ich ein Buch geschrieben,", "tokens": ["Aus", "dem", "Krie\u00b7ge", "auf\u00b7tau\u00b7chend", "hab'", "ich", "ein", "Buch", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Worte meines Buches gar nichts, der Trieb darin alles,", "tokens": ["Die", "Wor\u00b7te", "mei\u00b7nes", "Bu\u00b7ches", "gar", "nichts", ",", "der", "Trieb", "da\u00b7rin", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "PIS", "$,", "ART", "NN", "PAV", "PIS", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein Buch f\u00fcr sich, nicht mit den andern verkn\u00fcpft, noch mit dem Verstand zu erfassen,", "tokens": ["Ein", "Buch", "f\u00fcr", "sich", ",", "nicht", "mit", "den", "an\u00b7dern", "ver\u00b7kn\u00fcpft", ",", "noch", "mit", "dem", "Ver\u00b7stand", "zu", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "$,", "PTKNEG", "APPR", "ART", "ADJA", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Doch das Geheime, Unausgesprochene wird auf jeder Seite zittern.", "tokens": ["Doch", "das", "Ge\u00b7hei\u00b7me", ",", "Un\u00b7aus\u00b7ge\u00b7spro\u00b7che\u00b7ne", "wird", "auf", "je\u00b7der", "Sei\u00b7te", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}