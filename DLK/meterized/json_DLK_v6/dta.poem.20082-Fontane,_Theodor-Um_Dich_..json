{"dta.poem.20082": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.85", "af:0.14"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Es kann die Ehre dieser Welt", "tokens": ["Es", "kann", "die", "Eh\u00b7re", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir keine Ehre geben,", "tokens": ["Dir", "kei\u00b7ne", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Dich in Wahrheit hebt und h\u00e4lt", "tokens": ["Was", "Dich", "in", "Wahr\u00b7heit", "hebt", "und", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df in Dir selber leben.", "tokens": ["Mu\u00df", "in", "Dir", "sel\u00b7ber", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn\u2019s Deinem Innersten gebricht", "tokens": ["Wenn's", "Dei\u00b7nem", "In\u00b7ners\u00b7ten", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "An \u00e4chten Stolzes St\u00fctze,", "tokens": ["An", "\u00e4ch\u00b7ten", "Stol\u00b7zes", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob dann die Welt Dir Beifall spricht", "tokens": ["Ob", "dann", "die", "Welt", "Dir", "Bei\u00b7fall", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all Dir wenig n\u00fctze.", "tokens": ["Ist", "all", "Dir", "we\u00b7nig", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das fl\u00fcchtge Lob, des Tages Ruhm", "tokens": ["Das", "fl\u00fccht\u00b7ge", "Lob", ",", "des", "Ta\u00b7ges", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Magst Du dem Eitlen g\u00f6nnen;", "tokens": ["Magst", "Du", "dem", "Eit\u00b7len", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das aber sei dein Heiligthum:", "tokens": ["Das", "a\u00b7ber", "sei", "dein", "Hei\u00b7lig\u00b7thum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}}}}