{"textgrid.poem.36440": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Jugendlust", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft den alten Ehrenmann", "tokens": ["La\u00dft", "den", "al\u00b7ten", "Eh\u00b7ren\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Jugend schelten!", "tokens": ["Uns\u00b7re", "Ju\u00b7gend", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weil er es nicht lassen kann,", "tokens": ["Weil", "er", "es", "nicht", "las\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ers nicht entgelten.", "tokens": ["Soll", "ers", "nicht", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wei\u00df er doch, worauf er schilt,", "tokens": ["Wei\u00df", "er", "doch", ",", "wo\u00b7rauf", "er", "schilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ihm ietzt so wenig gilt,", "tokens": ["Was", "ihm", "ietzt", "so", "we\u00b7nig", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "That er sonst nicht selten.", "tokens": ["That", "er", "sonst", "nicht", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist es denn nicht Zeit genung", "tokens": ["Ist", "es", "denn", "nicht", "Zeit", "ge\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den bittern Klagen?", "tokens": ["Zu", "den", "bit\u00b7tern", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alter! warum w\u00e4r ich iung?", "tokens": ["Al\u00b7ter", "!", "wa\u00b7rum", "w\u00e4r", "ich", "i\u00b7ung", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwa mich zu plagen?", "tokens": ["Et\u00b7wa", "mich", "zu", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Sprich nur, ob dein Herz nicht spricht:", "tokens": ["Sprich", "nur", ",", "ob", "dein", "Herz", "nicht", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Thu' es erst, wenn Muth gebricht,", "tokens": ["Thu'", "es", "erst", ",", "wenn", "Muth", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den alten Tagen.", "tokens": ["In", "den", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alter schweig! ich wei\u00df, ich bin", "tokens": ["Al\u00b7ter", "schweig", "!", "ich", "wei\u00df", ",", "ich", "bin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem rechten Wege.", "tokens": ["Auf", "dem", "rech\u00b7ten", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bruder sieh! mein froher Sinn", "tokens": ["Bru\u00b7der", "sieh", "!", "mein", "fro\u00b7her", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht faul noch tr\u00e4ge.", "tokens": ["Ist", "nicht", "faul", "noch", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sieh! es macht kein Kummerschwei\u00df,", "tokens": ["Sieh", "!", "es", "macht", "kein", "Kum\u00b7mer\u00b7schwei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, ein iugendlicher Flei\u00df", "tokens": ["Nein", ",", "ein", "i\u00b7u\u00b7gend\u00b7li\u00b7cher", "Flei\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Diese Herzensschl\u00e4ge.", "tokens": ["Die\u00b7se", "Her\u00b7zens\u00b7schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "M\u00fctter h\u00f6rt dem Vater zu,", "tokens": ["M\u00fct\u00b7ter", "h\u00f6rt", "dem", "Va\u00b7ter", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ret seine Lehren.", "tokens": ["H\u00f6\u00b7ret", "sei\u00b7ne", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bruder \u2013 \u2013 \u2013 \u2013 ich und du", "tokens": ["Bru\u00b7der", "\u2013", "\u2013", "\u2013", "\u2013", "ich", "und", "du"], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$(", "$(", "$(", "$(", "PPER", "KON", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "D\u00fcrfen sie nicht h\u00f6ren.", "tokens": ["D\u00fcr\u00b7fen", "sie", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nimm das Glas, das Doris h\u00e4lt.", "tokens": ["Nimm", "das", "Glas", ",", "das", "Do\u00b7ris", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4re dis die beste Welt,", "tokens": ["W\u00e4\u00b7re", "dis", "die", "bes\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn wir m\u00fc\u00dfig w\u00e4ren?", "tokens": ["Wenn", "wir", "m\u00fc\u00b7\u00dfig", "w\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bruder! ia, dich lehrt der Wein", "tokens": ["Bru\u00b7der", "!", "i\u00b7a", ",", "dich", "lehrt", "der", "Wein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Deine Weisheitsspr\u00fcche.", "tokens": ["Dei\u00b7ne", "Weis\u00b7heits\u00b7spr\u00fc\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnt ich wohl dein Bruder seyn,", "tokens": ["K\u00f6nnt", "ich", "wohl", "dein", "Bru\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich dir nicht gliche?", "tokens": ["Wenn", "ich", "dir", "nicht", "gli\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nein, wir w\u00e4ren nicht gescheut,", "tokens": ["Nein", ",", "wir", "w\u00e4\u00b7ren", "nicht", "ge\u00b7scheut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ein Tropfen Lebenszeit", "tokens": ["Wenn", "ein", "Trop\u00b7fen", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Lust verstriche.", "tokens": ["Oh\u00b7ne", "Lust", "ver\u00b7stri\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Br\u00fcder! ruft die Warheit aus", "tokens": ["Br\u00fc\u00b7der", "!", "ruft", "die", "War\u00b7heit", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Bacchusfesten.", "tokens": ["Auf", "den", "Bac\u00b7chus\u00b7fes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die bittet auf den Schmaus,", "tokens": ["Und", "die", "bit\u00b7tet", "auf", "den", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hlet die zu G\u00e4sten,", "tokens": ["W\u00e4h\u00b7let", "die", "zu", "G\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Welche l\u00e4ngst, wie wir, gewu\u00dft,", "tokens": ["Wel\u00b7che", "l\u00e4ngst", ",", "wie", "wir", ",", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welten voller Jugendlust", "tokens": ["Wel\u00b7ten", "vol\u00b7ler", "Ju\u00b7gend\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind die allerbesten.", "tokens": ["Sind", "die", "al\u00b7ler\u00b7bes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}