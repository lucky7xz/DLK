{"textgrid.poem.48909": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leander und Selin, zween Freunde, die", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leander und Selin, zween Freunde, die", "tokens": ["Le\u00b7an\u00b7der", "und", "Se\u00b7lin", ",", "zween", "Freun\u00b7de", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "KON", "NE", "$,", "VVFIN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstand und Edelmuth und gleicher Trieb", "tokens": ["Ver\u00b7stand", "und", "E\u00b7del\u00b7muth", "und", "glei\u00b7cher", "Trieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Tugend, fest verband, vertrauten sich,", "tokens": ["Zur", "Tu\u00b7gend", ",", "fest", "ver\u00b7band", ",", "ver\u00b7trau\u00b7ten", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVFIN", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einst in Gesch\u00e4ften, dem treulosen Meer.", "tokens": ["Einst", "in", "Ge\u00b7sch\u00e4f\u00b7ten", ",", "dem", "treu\u00b7lo\u00b7sen", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Die Winde wehten erst der Gegend zu,", "tokens": ["Die", "Win\u00b7de", "weh\u00b7ten", "erst", "der", "Ge\u00b7gend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die schon die Reisenden im Geiste sahn;", "tokens": ["Die", "schon", "die", "Rei\u00b7sen\u00b7den", "im", "Geis\u00b7te", "sahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Ufer floh, und bald erblickten sie", "tokens": ["Das", "U\u00b7fer", "floh", ",", "und", "bald", "er\u00b7blick\u00b7ten", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Rings um nur Luft und See. Das Firmament", "tokens": ["Rings", "um", "nur", "Luft", "und", "See", ".", "Das", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADV", "NN", "KON", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "War heiter und voll Glantz. Sie seegelten", "tokens": ["War", "hei\u00b7ter", "und", "voll", "Glantz", ".", "Sie", "see\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "In seinem Wiederschein geruhig fort,", "tokens": ["In", "sei\u00b7nem", "Wie\u00b7der\u00b7schein", "ge\u00b7ru\u00b7hig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und nahten sich bereits der Reise Ziel,", "tokens": ["Und", "nah\u00b7ten", "sich", "be\u00b7reits", "der", "Rei\u00b7se", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als schnell die Wellen sich emp\u00f6reten.", "tokens": ["Als", "schnell", "die", "Wel\u00b7len", "sich", "em\u00b7p\u00f6\u00b7re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Ein rei\u00dfender Orcan erwacht und schlug", "tokens": ["Ein", "rei\u00b7\u00dfen\u00b7der", "Or\u00b7can", "er\u00b7wacht", "und", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Das Schiff von seiner Bahn. Es scheiterte", "tokens": ["Das", "Schiff", "von", "sei\u00b7ner", "Bahn", ".", "Es", "schei\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Am Felsen. Jeder sucht den Tod zu fliehn;", "tokens": ["Am", "Fel\u00b7sen", ".", "Je\u00b7der", "sucht", "den", "Tod", "zu", "fliehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PIS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das kleinste St\u00fcck vom Schiff wird jetzt sein Schiff \u2013", "tokens": ["Das", "kleins\u00b7te", "St\u00fcck", "vom", "Schiff", "wird", "jetzt", "sein", "Schiff", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Den beyden Freunden ward ein Bret zu Theil;", "tokens": ["Den", "bey\u00b7den", "Freun\u00b7den", "ward", "ein", "Bret", "zu", "Theil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Allein, es war zu leicht f\u00fcr seine Last.", "tokens": ["Al\u00b7lein", ",", "es", "war", "zu", "leicht", "f\u00fcr", "sei\u00b7ne", "Last", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wir sincken, sprach Selin, das Bret ertr\u00e4gt", "tokens": ["Wir", "sin\u00b7cken", ",", "sprach", "Se\u00b7lin", ",", "das", "Bret", "er\u00b7tr\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "NE", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Uns beyde nicht, o Freund! Leb ewig wohl!", "tokens": ["Uns", "bey\u00b7de", "nicht", ",", "o", "Freund", "!", "Leb", "e\u00b7wig", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKNEG", "$,", "FM", "NN", "$.", "VVIMP", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Du must erhalten seyn, an dir verliehrt", "tokens": ["Du", "must", "er\u00b7hal\u00b7ten", "seyn", ",", "an", "dir", "ver\u00b7liehrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Wohl der Welt zu viel, und ohne dich", "tokens": ["Das", "Wohl", "der", "Welt", "zu", "viel", ",", "und", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIS", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "W\u00e4r mir das Leben doch nur eine Qvaal.", "tokens": ["W\u00e4r", "mir", "das", "Le\u00b7ben", "doch", "nur", "ei\u00b7ne", "Qvaal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Nein, sprach Leander, nein, ich sterb o Freund! \u2013", "tokens": ["Nein", ",", "sprach", "Le\u00b7an\u00b7der", ",", "nein", ",", "ich", "sterb", "o", "Freund", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "NE", "$,", "PTKANT", "$,", "PPER", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Allein Selin verlie\u00df zu schnell das Bret", "tokens": ["Al\u00b7lein", "Se\u00b7lin", "ver\u00b7lie\u00df", "zu", "schnell", "das", "Bret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "PTKA", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und \u00fcbergab getrost dem na\u00dfen Grab", "tokens": ["Und", "\u00fc\u00b7berg\u00b7ab", "ge\u00b7trost", "dem", "na\u00b7\u00dfen", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der Wa\u00dferwogen sich. Die Vorsehung", "tokens": ["Der", "Wa\u00b7\u00dfer\u00b7wo\u00b7gen", "sich", ".", "Die", "Vor\u00b7se\u00b7hung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die \u00fcber alles wacht, sah seine Treu", "tokens": ["Die", "\u00fc\u00b7ber", "al\u00b7les", "wacht", ",", "sah", "sei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und seine Gro\u00dfmuth an, und lie\u00df das Meer", "tokens": ["Und", "sei\u00b7ne", "Gro\u00df\u00b7muth", "an", ",", "und", "lie\u00df", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ihm nicht zum Grabe seyn. Mitleidig trugs", "tokens": ["Ihm", "nicht", "zum", "Gra\u00b7be", "seyn", ".", "Mit\u00b7lei\u00b7dig", "trugs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "VAINF", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Auf seinen Wellen ihn zum Ufer hin.", "tokens": ["Auf", "sei\u00b7nen", "Wel\u00b7len", "ihn", "zum", "U\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Er fand Leandern schon daselbst \u2013 O, wer", "tokens": ["Er", "fand", "Le\u00b7an\u00b7dern", "schon", "da\u00b7selbst", "\u2013", "O", ",", "wer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PAV", "$(", "NE", "$,", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Beschreibt die Regungen der Freude, die", "tokens": ["Be\u00b7schreibt", "die", "Re\u00b7gun\u00b7gen", "der", "Freu\u00b7de", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "Sie beyde f\u00fchlten! \u2013 Sie umarmten sich", "tokens": ["Sie", "bey\u00b7de", "f\u00fchl\u00b7ten", "!", "\u2013", "Sie", "um\u00b7arm\u00b7ten", "sich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Mit Z\u00e4hren in dem Aug. Leander sprach:", "tokens": ["Mit", "Z\u00e4h\u00b7ren", "in", "dem", "Aug.", "Le\u00b7an\u00b7der", "sprach", ":"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "O allzutreuer Freund, in was f\u00fcr Qvaal", "tokens": ["O", "all\u00b7zu\u00b7treu\u00b7er", "Freund", ",", "in", "was", "f\u00fcr", "Qvaal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Hat deine Freundschafft mich gest\u00fcrtzt! Ich hab", "tokens": ["Hat", "dei\u00b7ne", "Freund\u00b7schafft", "mich", "ge\u00b7st\u00fcrtzt", "!", "Ich", "hab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Um dich des Todes Angst zehnfach gef\u00fchlt.", "tokens": ["Um", "dich", "des", "To\u00b7des", "Angst", "zehn\u00b7fach", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Was du thatst wolt ich thun, denn ohne dich", "tokens": ["Was", "du", "thatst", "wolt", "ich", "thun", ",", "denn", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "PPER", "VVINF", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "W\u00fcnscht ich das Leben nicht \u2013 Geliebtester", "tokens": ["W\u00fcnscht", "ich", "das", "Le\u00b7ben", "nicht", "\u2013", "Ge\u00b7lieb\u00b7tes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Was w\u00e4r ich ohne dich! versetzt Selin;", "tokens": ["Was", "w\u00e4r", "ich", "oh\u00b7ne", "dich", "!", "ver\u00b7setzt", "Se\u00b7lin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$.", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der Himmel sey gelobt, der dich mir schenkt!", "tokens": ["Der", "Him\u00b7mel", "sey", "ge\u00b7lobt", ",", "der", "dich", "mir", "schenkt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Komm la\u00df uns ihn, der uns vom Tod befreyt,", "tokens": ["Komm", "la\u00df", "uns", "ihn", ",", "der", "uns", "vom", "Tod", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIMP", "PPER", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Verehren, und ihm ganz das Leben weyhn.", "tokens": ["Ver\u00b7eh\u00b7ren", ",", "und", "ihm", "ganz", "das", "Le\u00b7ben", "weyhn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Sie knieten weinend an das Ufer hin", "tokens": ["Sie", "knie\u00b7ten", "wei\u00b7nend", "an", "das", "U\u00b7fer", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und dankten dem, der sie errettete.", "tokens": ["Und", "dank\u00b7ten", "dem", ",", "der", "sie", "er\u00b7ret\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Und ihre Regung drang die Wolcken durch \u2013", "tokens": ["Und", "ih\u00b7re", "Re\u00b7gung", "drang", "die", "Wol\u00b7cken", "durch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Leander theilte mit Selin, der arm", "tokens": ["Le\u00b7an\u00b7der", "theil\u00b7te", "mit", "Se\u00b7lin", ",", "der", "arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NE", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "An G\u00fcthern und nur reich an Tugend war,", "tokens": ["An", "G\u00fc\u00b7thern", "und", "nur", "reich", "an", "Tu\u00b7gend", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "All seine Sch\u00e4tze, die Selin nur nahm", "tokens": ["All", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", ",", "die", "Se\u00b7lin", "nur", "nahm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PRELS", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Weil sich sein Freund dadurch gl\u00fcckseelig pries.", "tokens": ["Weil", "sich", "sein", "Freund", "da\u00b7durch", "gl\u00fcck\u00b7see\u00b7lig", "pries", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PAV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Und Seegen kam auf sie und auf ihr Haus,", "tokens": ["Und", "See\u00b7gen", "kam", "auf", "sie", "und", "auf", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Und lange waren sie das Wohl der Welt.", "tokens": ["Und", "lan\u00b7ge", "wa\u00b7ren", "sie", "das", "Wohl", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAF