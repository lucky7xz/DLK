{"textgrid.poem.34612": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihren abschied", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzeihe mir allhier/ mein widriges gestirne/", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "all\u00b7hier", "/", "mein", "wid\u00b7ri\u00b7ges", "ge\u00b7stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wofern ich mich zu sehr auff deinen lauff erz\u00fcrne/", "tokens": ["Wo\u00b7fern", "ich", "mich", "zu", "sehr", "auff", "dei\u00b7nen", "lauff", "er\u00b7z\u00fcr\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast mir allzuviel auff einen tag gethan.", "tokens": ["Du", "hast", "mir", "all\u00b7zu\u00b7viel", "auff", "ei\u00b7nen", "tag", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein leben reiset fort/ und dennoch soll ich leben:", "tokens": ["Mein", "le\u00b7ben", "rei\u00b7set", "fort", "/", "und", "den\u00b7noch", "soll", "ich", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "PTKVZ", "$(", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du nimmest/ was du doch mir niemahls hast gegeben/", "tokens": ["Du", "nim\u00b7mest", "/", "was", "du", "doch", "mir", "nie\u00b7mahls", "hast", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "ADV", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und legst mir unverh\u00f6rt die gr\u00f6ste martern an.", "tokens": ["Und", "legst", "mir", "un\u00b7ver\u00b7h\u00f6rt", "die", "gr\u00f6s\u00b7te", "mar\u00b7tern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "War es dann nicht genug/ da\u00df ich mit tausend thr\u00e4nen", "tokens": ["War", "es", "dann", "nicht", "ge\u00b7nug", "/", "da\u00df", "ich", "mit", "tau\u00b7send", "thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$(", "KOUS", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich t\u00e4glich/ doch umsonst/ nach k\u00fchlung muste sehnen?", "tokens": ["Mich", "t\u00e4g\u00b7lich", "/", "doch", "um\u00b7sonst", "/", "nach", "k\u00fch\u00b7lung", "mus\u00b7te", "seh\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "ADV", "ADV", "$(", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Soll ich von Sylvien auch noch geschieden sein?", "tokens": ["Soll", "ich", "von", "Syl\u00b7vi\u00b7en", "auch", "noch", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach armer Celadon! du hast zu viel erfahren/", "tokens": ["Ach", "ar\u00b7mer", "Ce\u00b7la\u00b7don", "!", "du", "hast", "zu", "viel", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PPER", "VAFIN", "PTKA", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch diese stunde mehr/ als vor in zwantzig jahren.", "tokens": ["Doch", "die\u00b7se", "stun\u00b7de", "mehr", "/", "als", "vor", "in", "zwant\u00b7zig", "jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "ADV", "$(", "KOKOM", "APPR", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drumb hoffe nur nicht erst auff neuen sonnenschein;", "tokens": ["Drumb", "hof\u00b7fe", "nur", "nicht", "erst", "auff", "neu\u00b7en", "son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du wirst/ du solst/ du must auff dieser auen sterben:", "tokens": ["Du", "wirst", "/", "du", "solst", "/", "du", "must", "auff", "die\u00b7ser", "au\u00b7en", "ster\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Umb/ wie es scheint/ den platz mit purpur anzuf\u00e4rben/", "tokens": ["Umb", "/", "wie", "es", "scheint", "/", "den", "platz", "mit", "pur\u00b7pur", "an\u00b7zu\u00b7f\u00e4r\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "NN", "APPR", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo ehmahls Sylvia so sch\u00f6ne blumen la\u00df.", "tokens": ["Wo", "eh\u00b7mahls", "Syl\u00b7via", "so", "sch\u00f6\u00b7ne", "blu\u00b7men", "la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Gedencke nur nicht mehr an die vergn\u00fcgten stunden;", "tokens": ["Ge\u00b7den\u00b7cke", "nur", "nicht", "mehr", "an", "die", "ver\u00b7gn\u00fcg\u00b7ten", "stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Wie sich ihr keuscher arm um deinen hals gewunden/", "tokens": ["Wie", "sich", "ihr", "keu\u00b7scher", "arm", "um", "dei\u00b7nen", "hals", "ge\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wie sie milch und brodt aus deinen h\u00e4nden a\u00df.", "tokens": ["Und", "wie", "sie", "milch", "und", "brodt", "aus", "dei\u00b7nen", "h\u00e4n\u00b7den", "a\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die lust ist schon vorbey. Itzt ist der todt erschienen.", "tokens": ["Die", "lust", "ist", "schon", "vor\u00b7bey", ".", "Itzt", "ist", "der", "todt", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der tod/ so dir allein kan zur ergetzung dienen;", "tokens": ["Der", "tod", "/", "so", "dir", "al\u00b7lein", "kan", "zur", "er\u00b7get\u00b7zung", "die\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Nachdem die gantze welt dir ihren trost versagt.", "tokens": ["Nach\u00b7dem", "die", "gant\u00b7ze", "welt", "dir", "ih\u00b7ren", "trost", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die zeit ver\u00e4ndert sich offt anders als man meynet.", "tokens": ["Die", "zeit", "ver\u00b7\u00e4n\u00b7dert", "sich", "offt", "an\u00b7ders", "als", "man", "mey\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Heut' hat dein treues hertz umb Sylvien geweinet:", "tokens": ["Heut'", "hat", "dein", "treu\u00b7es", "hertz", "umb", "Syl\u00b7vi\u00b7en", "ge\u00b7wei\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer wei\u00df/ wer morgen schon auch wieder dich beklagt.", "tokens": ["Wer", "wei\u00df", "/", "wer", "mor\u00b7gen", "schon", "auch", "wie\u00b7der", "dich", "be\u00b7klagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "ADV", "ADV", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O himmel/ wald und thal/ und ihr begr\u00fcnten auen/", "tokens": ["O", "him\u00b7mel", "/", "wald", "und", "thal", "/", "und", "ihr", "be\u00b7gr\u00fcn\u00b7ten", "au\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NN", "KON", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wo ich mich fr\u00fcher offt lie\u00df als Aurora schauen/", "tokens": ["Wo", "ich", "mich", "fr\u00fc\u00b7her", "offt", "lie\u00df", "als", "Au\u00b7ro\u00b7ra", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "ADV", "VVFIN", "KOKOM", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nehmt/ wo ihr mich noch kennt/ die letzten seuffzer hin/", "tokens": ["Nehmt", "/", "wo", "ihr", "mich", "noch", "kennt", "/", "die", "letz\u00b7ten", "seuff\u00b7zer", "hin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und saget Sylvien/ wenn man mich hat begraben/", "tokens": ["Und", "sa\u00b7get", "Syl\u00b7vi\u00b7en", "/", "wenn", "man", "mich", "hat", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$(", "KOUS", "PIS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und sie sich wieder wird an euren blumen laben/", "tokens": ["Und", "sie", "sich", "wie\u00b7der", "wird", "an", "eu\u00b7ren", "blu\u00b7men", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df ich f\u00fcr sie allein allhier gestorben bin.", "tokens": ["Da\u00df", "ich", "f\u00fcr", "sie", "al\u00b7lein", "all\u00b7hier", "ge\u00b7stor\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}