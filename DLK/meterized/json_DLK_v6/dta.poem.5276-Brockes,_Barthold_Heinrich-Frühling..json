{"dta.poem.5276": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gantze Luft war angef\u00fcllet", "tokens": ["Die", "gant\u00b7ze", "Luft", "war", "an\u00b7ge\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einer reinen Heiterkeit,", "tokens": ["Mit", "ei\u00b7ner", "rei\u00b7nen", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein junges Laub hatt\u2019 allbereit", "tokens": ["Ein", "jun\u00b7ges", "Laub", "hatt'", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der V\u00f6gel Nesterchen verh\u00fcllet", "tokens": ["Der", "V\u00f6\u00b7gel", "Nes\u00b7ter\u00b7chen", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die, da sie, mit so vieler Kunst,", "tokens": ["Die", ",", "da", "sie", ",", "mit", "so", "vie\u00b7ler", "Kunst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die s\u00fcsse Arbeit fertig hatten,", "tokens": ["Die", "s\u00fcs\u00b7se", "Ar\u00b7beit", "fer\u00b7tig", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In dem begr\u00fcnt- und zarten Schatten", "tokens": ["In", "dem", "be\u00b7gr\u00fcnt", "und", "zar\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll reger Trieb\u2019 und s\u00fcsser Brunst", "tokens": ["Voll", "re\u00b7ger", "Trieb'", "und", "s\u00fcs\u00b7ser", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Verliebet, und geliebt sich gatten,", "tokens": ["Ver\u00b7lie\u00b7bet", ",", "und", "ge\u00b7liebt", "sich", "gat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und nichts, vor lauter Lieb\u2019, als Liebe! Liebe! singen,", "tokens": ["Und", "nichts", ",", "vor", "lau\u00b7ter", "Lieb'", ",", "als", "Lie\u00b7be", "!", "Lie\u00b7be", "!", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "KOUS", "NN", "$.", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df Feld und Wald davon erklingen.", "tokens": ["Da\u00df", "Feld", "und", "Wald", "da\u00b7von", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wir sehen \u00fcberall ein w\u00fcrckliches Bewegen,", "tokens": ["Wir", "se\u00b7hen", "\u00fc\u00b7be\u00b7rall", "ein", "w\u00fcrck\u00b7li\u00b7ches", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Leben der Natur wird \u00fcberall versp\u00fchrt.", "tokens": ["Ein", "Le\u00b7ben", "der", "Na\u00b7tur", "wird", "\u00fc\u00b7be\u00b7rall", "ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach, m\u00f6chte die\u00df, wenn uns die holde Sch\u00f6nheit r\u00fchrt,", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "die\u00df", ",", "wenn", "uns", "die", "hol\u00b7de", "Sch\u00f6n\u00b7heit", "r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PDS", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein neues Leben auch in uns erregen!", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "auch", "in", "uns", "er\u00b7re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Es l\u00e4chelt gleichsam uns die g\u00fctige Natur", "tokens": ["Es", "l\u00e4\u00b7chelt", "gleich\u00b7sam", "uns", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit holden Blicken hier, aus jedem Vorwurf, an,", "tokens": ["Mit", "hol\u00b7den", "Bli\u00b7cken", "hier", ",", "aus", "je\u00b7dem", "Vor\u00b7wurf", ",", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So da\u00df man alle Pracht und Lieblichkeiten nur", "tokens": ["So", "da\u00df", "man", "al\u00b7le", "Pracht", "und", "Lieb\u00b7lich\u00b7kei\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PIAT", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Empfinden, nicht beschreiben, kann.", "tokens": ["Emp\u00b7fin\u00b7den", ",", "nicht", "be\u00b7schrei\u00b7ben", ",", "kann", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVINF", "$,", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ach m\u00f6gte GOtt in uns, da alle Ding\u2019 im Lentzen", "tokens": ["Ach", "m\u00f6g\u00b7te", "Gott", "in", "uns", ",", "da", "al\u00b7le", "Ding'", "im", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So lieblich und so wundersch\u00f6n", "tokens": ["So", "lieb\u00b7lich", "und", "so", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In tausendfacher Anmuth gl\u00e4ntzen,", "tokens": ["In", "tau\u00b7send\u00b7fa\u00b7cher", "An\u00b7muth", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Auch unsrer Seele Fr\u00fchling sehn;", "tokens": ["Auch", "uns\u00b7rer", "See\u00b7le", "Fr\u00fch\u00b7ling", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und in demselbigen, an unsern sanften Freuden,", "tokens": ["Und", "in", "dem\u00b7sel\u00b7bi\u00b7gen", ",", "an", "un\u00b7sern", "sanf\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die in ihr, blos aus seinem Werck, entstehn,", "tokens": ["Die", "in", "ihr", ",", "blos", "aus", "sei\u00b7nem", "Werck", ",", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Mit Lob und Danck geschm\u00fcckt, sein Vater Auge weiden!", "tokens": ["Mit", "Lob", "und", "Danck", "ge\u00b7schm\u00fcckt", ",", "sein", "Va\u00b7ter", "Au\u00b7ge", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}