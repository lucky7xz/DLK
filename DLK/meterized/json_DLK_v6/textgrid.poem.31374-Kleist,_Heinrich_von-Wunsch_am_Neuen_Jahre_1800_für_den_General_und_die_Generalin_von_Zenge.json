{"textgrid.poem.31374": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch am Neuen Jahre 1800 f\u00fcr den General und die Generalin von Zenge", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben gl\u00fccklicher Kinder gl\u00fcckliche Eltern! Das nenn ich", "tokens": ["Sie\u00b7ben", "gl\u00fcck\u00b7li\u00b7cher", "Kin\u00b7der", "gl\u00fcck\u00b7li\u00b7che", "El\u00b7tern", "!", "Das", "nenn", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "ADJA", "NN", "$.", "PDS", "VVFIN", "PPER"], "meter": "+-+--+-+--+-++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Doch noch ein Gl\u00fcck, an das, wahrlich, kein Neujahrswunsch reicht!", "tokens": ["Doch", "noch", "ein", "Gl\u00fcck", ",", "an", "das", ",", "wahr\u00b7lich", ",", "kein", "Neu\u00b7jahrs\u00b7wunsch", "reicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "APPR", "PDS", "$,", "ADV", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Soll ich euch doch etwas w\u00fcnschen, so sei es dies einzge: es finde", "tokens": ["Soll", "ich", "euch", "doch", "et\u00b7was", "w\u00fcn\u00b7schen", ",", "so", "sei", "es", "dies", "einz\u00b7ge", ":", "es", "fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PIS", "VVINF", "$,", "ADV", "VAFIN", "PPER", "PDS", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Euch ein Neujahr zu w\u00fcnschen niemals ein Dichter den Stoff.", "tokens": ["Euch", "ein", "Neu\u00b7jahr", "zu", "w\u00fcn\u00b7schen", "nie\u00b7mals", "ein", "Dich\u00b7ter", "den", "Stoff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}}}}}