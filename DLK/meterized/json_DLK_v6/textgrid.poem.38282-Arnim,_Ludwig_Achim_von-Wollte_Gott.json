{"textgrid.poem.38282": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wollte Gott", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meiner Frauen rother Mund,", "tokens": ["Mei\u00b7ner", "Frau\u00b7en", "ro\u00b7ther", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der brennt recht scharlachfarb;", "tokens": ["Der", "brennt", "recht", "schar\u00b7lach\u00b7farb", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er brennt recht wie ein rothe Ros',", "tokens": ["Er", "brennt", "recht", "wie", "ein", "ro\u00b7the", "Ros'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer ersten Bl\u00fcth.", "tokens": ["In", "ih\u00b7rer", "ers\u00b7ten", "Bl\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er brennt recht wie der roth Rubin,", "tokens": ["Er", "brennt", "recht", "wie", "der", "roth", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Goldes Farb;", "tokens": ["In", "Gol\u00b7des", "Farb", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er brennt recht, wie ein hei\u00dfe Kohl,", "tokens": ["Er", "brennt", "recht", ",", "wie", "ein", "hei\u00b7\u00dfe", "Kohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liegt in des Feuers Glut.", "tokens": ["Liegt", "in", "des", "Feu\u00b7ers", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr H\u00e4lslein wei\u00df, ihr schwarze Aeuglein klar,", "tokens": ["Ihr", "H\u00e4ls\u00b7lein", "wei\u00df", ",", "ihr", "schwar\u00b7ze", "A\u00b7e\u00b7u\u00b7glein", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darzu tr\u00e4gt sie ein goldfarb krauses Haar;", "tokens": ["Dar\u00b7zu", "tr\u00e4gt", "sie", "ein", "gold\u00b7farb", "krau\u00b7ses", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihr werther Leib ist wei\u00dfer als kein Hermelein,", "tokens": ["Ihr", "wert\u00b7her", "Leib", "ist", "wei\u00b7\u00dfer", "als", "kein", "Her\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Meister lebt auf dieser Erd,", "tokens": ["Kein", "Meis\u00b7ter", "lebt", "auf", "die\u00b7ser", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der mirs mahlen k\u00f6nnt so fein.", "tokens": ["Der", "mirs", "mah\u00b7len", "k\u00f6nnt", "so", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt Gott, w\u00e4r ich ein lauter Spiegelglas!", "tokens": ["Wollt", "Gott", ",", "w\u00e4r", "ich", "ein", "lau\u00b7ter", "Spie\u00b7gel\u00b7glas", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VAFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich die allersch\u00f6nste Frau", "tokens": ["Da\u00df", "sich", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All Morgen vor mir pflanzieret;", "tokens": ["All", "Mor\u00b7gen", "vor", "mir", "pflan\u00b7zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wollt Gott, w\u00e4r ich ein seiden Hemdlein wei\u00df,", "tokens": ["Wollt", "Gott", ",", "w\u00e4r", "ich", "ein", "sei\u00b7den", "Hemd\u00b7lein", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df mich die allersch\u00f6nste Frau", "tokens": ["Da\u00df", "mich", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An ihrem Leibe tr\u00fcge.", "tokens": ["An", "ih\u00b7rem", "Lei\u00b7be", "tr\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wollt Gott, w\u00e4r ich ein roth Goldringelein!", "tokens": ["Wollt", "Gott", ",", "w\u00e4r", "ich", "ein", "roth", "Gold\u00b7rin\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VAFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df mich die allersch\u00f6nste Frau", "tokens": ["Da\u00df", "mich", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ihre H\u00e4ndlein zwinge;", "tokens": ["An", "ih\u00b7re", "H\u00e4nd\u00b7lein", "zwin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollt Gott, w\u00e4r ich ein Eichhorn traun,", "tokens": ["Wollt", "Gott", ",", "w\u00e4r", "ich", "ein", "Eich\u00b7horn", "traun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spr\u00e4ng auf ihren Schoo\u00df,", "tokens": ["Und", "spr\u00e4ng", "auf", "ih\u00b7ren", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von rechter Liebe sie mich in ihr Aermlein schlo\u00df.", "tokens": ["Von", "rech\u00b7ter", "Lie\u00b7be", "sie", "mich", "in", "ihr", "A\u00b7erm\u00b7lein", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie k\u00fc\u00dft mich an mein rosenfarbes M\u00fcndlein,", "tokens": ["Sie", "k\u00fc\u00dft", "mich", "an", "mein", "ro\u00b7sen\u00b7far\u00b7bes", "M\u00fcnd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das nehm ich f\u00fcr des Kaisers Gut,", "tokens": ["Das", "nehm", "ich", "f\u00fcr", "des", "Kai\u00b7sers", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sollt ich drum desto \u00e4rmer seyn.", "tokens": ["Sollt", "ich", "drum", "des\u00b7to", "\u00e4r\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}