{"textgrid.poem.67200": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kommt der neue Tag aus fernen H\u00f6hn herunter,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt der neue Tag aus fernen H\u00f6hn herunter,", "tokens": ["Es", "kommt", "der", "neu\u00b7e", "Tag", "aus", "fer\u00b7nen", "H\u00f6hn", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Morgen, der erwacht ist aus den D\u00e4mmerungen,", "tokens": ["Der", "Mor\u00b7gen", ",", "der", "er\u00b7wacht", "ist", "aus", "den", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er lacht die Menschheit an, geschm\u00fcckt und munter,", "tokens": ["Er", "lacht", "die", "Menschheit", "an", ",", "ge\u00b7schm\u00fcckt", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Freuden ist die Menschheit sanft durchdrungen.", "tokens": ["Von", "Freu\u00b7den", "ist", "die", "Menschheit", "sanft", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein neues Leben will der Zukunft sich enth\u00fcllen,", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "will", "der", "Zu\u00b7kunft", "sich", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Bl\u00fcten scheint, dem Zeichen froher Tage,", "tokens": ["Mit", "Bl\u00fc\u00b7ten", "scheint", ",", "dem", "Zei\u00b7chen", "fro\u00b7her", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das gro\u00dfe Tal, die Erde sich zu f\u00fcllen,", "tokens": ["Das", "gro\u00b7\u00dfe", "Tal", ",", "die", "Er\u00b7de", "sich", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entfernt dagegen ist zur Fr\u00fchlingszeit die Klage.", "tokens": ["Ent\u00b7fernt", "da\u00b7ge\u00b7gen", "ist", "zur", "Fr\u00fch\u00b7lings\u00b7zeit", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}