{"textgrid.poem.35688": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "12. Trinklied f\u00fcr Freie", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Eichenlaub den Hut bekr\u00e4nzt!", "tokens": ["Mit", "Ei\u00b7chen\u00b7laub", "den", "Hut", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlauf! und trinkt den Wein,", "tokens": ["Wohl\u00b7auf", "!", "und", "trinkt", "den", "Wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der duftend uns entgegengl\u00e4nzt!", "tokens": ["Der", "duf\u00b7tend", "uns", "ent\u00b7ge\u00b7gen\u00b7gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn sandte Vater Rhein!", "tokens": ["Ihn", "sand\u00b7te", "Va\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist einem noch die Knechtschaft wert,", "tokens": ["Ist", "ei\u00b7nem", "noch", "die", "Knecht\u00b7schaft", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zittert ihm die Hand,", "tokens": ["Und", "zit\u00b7tert", "ihm", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu heben Kolbe, Lanz' und Schwert,", "tokens": ["Zu", "he\u00b7ben", "Kol\u00b7be", ",", "Lanz'", "und", "Schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's gilt f\u00fcrs Vaterland:", "tokens": ["Wenn's", "gilt", "f\u00fcrs", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weg mit dem Schurken, weg von hier!", "tokens": ["Weg", "mit", "dem", "Schur\u00b7ken", ",", "weg", "von", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Er kriech' um Schranzenbrot,", "tokens": ["Er", "kriech'", "um", "Schran\u00b7zen\u00b7brot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sauf' um F\u00fcrsten sich zum Tier,", "tokens": ["Und", "sauf'", "um", "F\u00fcrs\u00b7ten", "sich", "zum", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bub', und l\u00e4stre Gott!", "tokens": ["Und", "bub'", ",", "und", "l\u00e4st\u00b7re", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und putze seinem Herrn die Schuh,", "tokens": ["Und", "put\u00b7ze", "sei\u00b7nem", "Herrn", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchre seinem Herrn", "tokens": ["Und", "f\u00fch\u00b7re", "sei\u00b7nem", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Weib und seine Tochter zu;", "tokens": ["Sein", "Weib", "und", "sei\u00b7ne", "Toch\u00b7ter", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trage Band und Stern!", "tokens": ["Und", "tra\u00b7ge", "Band", "und", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "F\u00fcr uns, f\u00fcr uns ist diese Nacht!", "tokens": ["F\u00fcr", "uns", ",", "f\u00fcr", "uns", "ist", "die\u00b7se", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr uns der edle Trank!", "tokens": ["F\u00fcr", "uns", "der", "ed\u00b7le", "Trank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man keltert' ihn, als Frankreichs Macht", "tokens": ["Man", "kel\u00b7tert'", "ihn", ",", "als", "Fran\u00b7kreichs", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In H\u00f6chst\u00e4dts Th\u00e4lern sank.", "tokens": ["In", "H\u00f6chs\u00b7t\u00e4dts", "Th\u00e4\u00b7lern", "sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum, Br\u00fcder, auf! den Hut bekr\u00e4nzt!", "tokens": ["Drum", ",", "Br\u00fc\u00b7der", ",", "auf", "!", "den", "Hut", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "PTKVZ", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trinkt, und trinkt den Wein,", "tokens": ["Und", "trinkt", ",", "und", "trinkt", "den", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der duftend uns entgegengl\u00e4nzt!", "tokens": ["Der", "duf\u00b7tend", "uns", "ent\u00b7ge\u00b7gen\u00b7gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns sandt' ihn Vater Rhein!", "tokens": ["Uns", "sandt'", "ihn", "Va\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Uns r\u00f6tet hohe Freiheitsglut!", "tokens": ["Uns", "r\u00f6\u00b7tet", "ho\u00b7he", "Frei\u00b7heits\u00b7glut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns zittert nicht die Hand!", "tokens": ["Uns", "zit\u00b7tert", "nicht", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir scheuten nicht des Vaters Blut,", "tokens": ["Wir", "scheu\u00b7ten", "nicht", "des", "Va\u00b7ters", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geb\u00f6t's das Vaterland!", "tokens": ["Ge\u00b7b\u00f6t's", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Uns, uns geh\u00f6ret Hermann an,", "tokens": ["Uns", ",", "uns", "ge\u00b7h\u00f6\u00b7ret", "Her\u00b7mann", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Tell, der Schweizerheld!", "tokens": ["Und", "Tell", ",", "der", "Schwei\u00b7zer\u00b7held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder freie deutsche Mann!", "tokens": ["Und", "je\u00b7der", "frei\u00b7e", "deut\u00b7sche", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hat den Sand gez\u00e4hlt?", "tokens": ["Wer", "hat", "den", "Sand", "ge\u00b7z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Uns weckte l\u00e4ngst der Br\u00e4utigam", "tokens": ["Uns", "weck\u00b7te", "l\u00e4ngst", "der", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wildem Jammerlaut!", "tokens": ["Mit", "wil\u00b7dem", "Jam\u00b7mer\u00b7laut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des F\u00fcrsten frecher Kuppler nahm", "tokens": ["Des", "F\u00fcrs\u00b7ten", "fre\u00b7cher", "Kupp\u00b7ler", "nahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm seine junge Braut.", "tokens": ["Ihm", "sei\u00b7ne", "jun\u00b7ge", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Uns winselte bei stiller Nacht", "tokens": ["Uns", "win\u00b7sel\u00b7te", "bei", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Witwe Trauerton!", "tokens": ["Der", "Wit\u00b7we", "Trau\u00b7er\u00b7ton", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Raubsucht und des Haders Schlacht", "tokens": ["Der", "Raub\u00b7sucht", "und", "des", "Ha\u00b7ders", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschlug ihr Mann und Sohn.", "tokens": ["Er\u00b7schlug", "ihr", "Mann", "und", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Uns \u00e4chzte, nah dem Hungertod,", "tokens": ["Uns", "\u00e4chz\u00b7te", ",", "nah", "dem", "Hun\u00b7ger\u00b7tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Waise bleicher Mund!", "tokens": ["Der", "Wai\u00b7se", "blei\u00b7cher", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man nahm ihr letztes hartes Brot,", "tokens": ["Man", "nahm", "ihr", "letz\u00b7tes", "har\u00b7tes", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gab's des F\u00fcrsten Hund!", "tokens": ["Und", "gab's", "des", "F\u00fcrs\u00b7ten", "Hund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Zur Rach' erwacht! zur Rach' erwacht", "tokens": ["Zur", "Rach'", "er\u00b7wacht", "!", "zur", "Rach'", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der freie deutsche Mann!", "tokens": ["Der", "frei\u00b7e", "deut\u00b7sche", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trompet' und Trommel, ruft zur Schlacht!", "tokens": ["Trom\u00b7pet'", "und", "Trom\u00b7mel", ",", "ruft", "zur", "Schlacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weht, Fahnen, weht voran!", "tokens": ["Weht", ",", "Fah\u00b7nen", ",", "weht", "vo\u00b7ran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ob uns ein Meer entgegenrollt;", "tokens": ["Ob", "uns", "ein", "Meer", "ent\u00b7ge\u00b7gen\u00b7rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinein! sie sind entmannt,", "tokens": ["Hin\u00b7ein", "!", "sie", "sind", "ent\u00b7mannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Knecht'! und streiten nur um Sold,", "tokens": ["Die", "Knecht'", "!", "und", "strei\u00b7ten", "nur", "um", "Sold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht f\u00fcrs Vaterland!", "tokens": ["Und", "nicht", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Hinein! das Meer ist uns ein Spott!", "tokens": ["Hin\u00b7ein", "!", "das", "Meer", "ist", "uns", "ein", "Spott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt mit stolzem Klang:", "tokens": ["Und", "singt", "mit", "stol\u00b7zem", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbein' feste Burg ist unser Gott!\u00ab", "tokens": ["\u00bb", "ein'", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Klopstocks Schlachtgesang!", "tokens": ["Und", "Klops\u00b7tocks", "Schlacht\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Engel Gottes schwebt daher", "tokens": ["Der", "En\u00b7gel", "Got\u00b7tes", "schwebt", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Wolken Pulverdampf,", "tokens": ["Auf", "Wol\u00b7ken", "Pul\u00b7ver\u00b7dampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut zornig in der Feinde Heer,", "tokens": ["Schaut", "zor\u00b7nig", "in", "der", "Fein\u00b7de", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreckt sie aus dem Kampf!", "tokens": ["Und", "schreckt", "sie", "aus", "dem", "Kampf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sie fliehn! Der Fluch der L\u00e4nder f\u00e4hrt", "tokens": ["Sie", "fliehn", "!", "Der", "Fluch", "der", "L\u00e4n\u00b7der", "f\u00e4hrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blitzen, ihnen nach!", "tokens": ["Mit", "Blit\u00b7zen", ",", "ih\u00b7nen", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre R\u00fccken kerbt das Schwert", "tokens": ["Und", "ih\u00b7re", "R\u00fc\u00b7cken", "kerbt", "das", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit feiger Wunden Schmach!", "tokens": ["Mit", "fei\u00b7ger", "Wun\u00b7den", "Schmach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Auf roten Wogen w\u00e4lzt der Rhein", "tokens": ["Auf", "ro\u00b7ten", "Wo\u00b7gen", "w\u00e4lzt", "der", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sklaven\u00e4ser fort,", "tokens": ["Die", "Skla\u00b7ve\u00b7n\u00e4\u00b7ser", "fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und speit sie aus, und schluckt sie ein,", "tokens": ["Und", "speit", "sie", "aus", ",", "und", "schluckt", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jauchzt am Ufer fort!", "tokens": ["Und", "jauchzt", "am", "U\u00b7fer", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Der Rebenberg am Leichenthal", "tokens": ["Der", "Re\u00b7ben\u00b7berg", "am", "Lei\u00b7chen\u00b7thal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4nkt seinen Most mit Blut!", "tokens": ["Tr\u00e4nkt", "sei\u00b7nen", "Most", "mit", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann trinken wir beim Freudenmahl,", "tokens": ["Dann", "trin\u00b7ken", "wir", "beim", "Freu\u00b7den\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Triumph! Tyrannenblut!", "tokens": ["Tri\u00b7umph", "!", "Ty\u00b7ran\u00b7nen\u00b7blut", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}