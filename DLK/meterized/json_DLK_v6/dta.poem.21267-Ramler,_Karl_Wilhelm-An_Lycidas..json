{"dta.poem.21267": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Lycidas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Wen feine Mutter unter den z&#228;rtlichen", "tokens": ["Wen", "fei\u00b7ne", "Mut\u00b7ter", "un\u00b7ter", "den", "z", "&#228;", "rt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$(", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ges\u00e4ngen heller Nachtigallch\u00f6r\u2019 empfieng,", "tokens": ["Ge\u00b7s\u00e4n\u00b7gen", "hel\u00b7ler", "Nach\u00b7ti\u00b7gall\u00b7ch\u00f6r'", "emp\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ihr in ihren G\u00f6ttertr\u00e4umen", "tokens": ["Wer", "ihr", "in", "ih\u00b7ren", "G\u00f6t\u00b7ter\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4chtlich als Schwan sich vom Busen", "tokens": ["N\u00e4cht\u00b7lich", "als", "Schwan", "sich", "vom", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "PRF", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4ngt nicht erstrittne Fahnen, und Schl\u00fcssel", "tokens": ["H\u00e4ngt", "nicht", "er\u00b7stritt\u00b7ne", "Fah\u00b7nen", ",", "und", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bezwungner St\u00e4dte Thoren, und feindliche", "tokens": ["Be\u00b7zwung\u00b7ner", "St\u00e4d\u00b7te", "Tho\u00b7ren", ",", "und", "feind\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Galeerenschn\u00e4bel in Gradivens", "tokens": ["Ga\u00b7lee\u00b7ren\u00b7schn\u00e4\u00b7bel", "in", "Gra\u00b7di\u00b7vens"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blutige Tempel auf; keine Schiffe,", "tokens": ["Blu\u00b7ti\u00b7ge", "Tem\u00b7pel", "auf", ";", "kei\u00b7ne", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "PIAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Mit K\u00fcnsten aller V\u00f6lker, mit jeder", "tokens": ["Mit", "K\u00fcns\u00b7ten", "al\u00b7ler", "V\u00f6l\u00b7ker", ",", "mit", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "$,", "APPR", "PIAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sonnenrothen Berge, des kalten", "tokens": ["Der", "son\u00b7nen\u00b7ro\u00b7then", "Ber\u00b7ge", ",", "des", "kal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der aufgedeckten H\u00f6lle wuchernd,", "tokens": ["Der", "auf\u00b7ge\u00b7deck\u00b7ten", "H\u00f6l\u00b7le", "wu\u00b7chernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fliegen f\u00fcr ihn um die beiden Pole.", "tokens": ["Flie\u00b7gen", "f\u00fcr", "ihn", "um", "die", "bei\u00b7den", "Po\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ununterwiesen wird er als Knabe schon", "tokens": ["Un\u00b7un\u00b7ter\u00b7wie\u00b7sen", "wird", "er", "als", "Kna\u00b7be", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KOUS", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Fr\u00fchlingsbluhme singen, und froh", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7bluh\u00b7me", "sin\u00b7gen", ",", "und", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sich einen Dichter gr\u00fcssen h\u00f6ren.", "tokens": ["Sich", "ei\u00b7nen", "Dich\u00b7ter", "gr\u00fcs\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm wird die j\u00fcngste der Charitinnen,", "tokens": ["Ihm", "wird", "die", "j\u00fcngs\u00b7te", "der", "Cha\u00b7ri\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die wohlbewachte Scham, sich zur F\u00fch-", "tokens": ["Die", "wohl\u00b7be\u00b7wach\u00b7te", "Scham", ",", "sich", "zur", "F\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Entbieten. Ihm wird Pallas die Wolke von", "tokens": ["Ent\u00b7bie\u00b7ten", ".", "Ihm", "wird", "Pal\u00b7las", "die", "Wol\u00b7ke", "von"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "NN", "ART", "NN", "APPR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Augen nehmen, dass ihr J\u00fcnger", "tokens": ["Den", "Au\u00b7gen", "neh\u00b7men", ",", "dass", "ihr", "J\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In W\u00e4ldern wird er einsam den Vater", "tokens": ["In", "W\u00e4l\u00b7dern", "wird", "er", "ein\u00b7sam", "den", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Natur verehren. Endlich, o Lycidas,", "tokens": ["Na\u00b7tur", "ver\u00b7eh\u00b7ren", ".", "End\u00b7lich", ",", "o", "Ly\u00b7ci\u00b7das", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Erwartet er, gleich eines fremden", "tokens": ["Er\u00b7war\u00b7tet", "er", ",", "gleich", "ei\u00b7nes", "frem\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mannes Besuche den Tod mit Gleich-", "tokens": ["Man\u00b7nes", "Be\u00b7su\u00b7che", "den", "Tod", "mit", "Gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}