{"dta.poem.5417": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "GoTT  regiret alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gereichet' es dem grossen All zur Ehre,", "tokens": ["Ge\u00b7rei\u00b7chet'", "es", "dem", "gros\u00b7sen", "All", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu glauben, da\u00df mit dem, was klein,", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", "mit", "dem", ",", "was", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "APPR", "ART", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich zu befassen, ihm zu niedertr\u00e4chtig w\u00e4re;", "tokens": ["Sich", "zu", "be\u00b7fas\u00b7sen", ",", "ihm", "zu", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcrd\u2019 ich andrer Meinung seyn.", "tokens": ["So", "w\u00fcrd'", "ich", "an\u00b7drer", "Mei\u00b7nung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil aber, sonder allen Streit,", "tokens": ["Weil", "a\u00b7ber", ",", "son\u00b7der", "al\u00b7len", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit gr\u00f6ssere Vollkommenheit", "tokens": ["Weit", "gr\u00f6s\u00b7se\u00b7re", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erfodert wird, um alles zu regiren,", "tokens": ["Er\u00b7fo\u00b7dert", "wird", ",", "um", "al\u00b7les", "zu", "re\u00b7gi\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als etwas nur; wird es uns ja geb\u00fchren,", "tokens": ["Als", "et\u00b7was", "nur", ";", "wird", "es", "uns", "ja", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$.", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vielmehr das herrlichste vom Sch\u00f6pfer zu gedencken,", "tokens": ["Viel\u00b7mehr", "das", "herr\u00b7lichs\u00b7te", "vom", "Sch\u00f6p\u00b7fer", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als seine Macht und Weisheit einzuschrencken;", "tokens": ["Als", "sei\u00b7ne", "Macht", "und", "Weis\u00b7heit", "ein\u00b7zu\u00b7schren\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zumahl, wie es ja leichtlich zu erkennen,", "tokens": ["Zu\u00b7mahl", ",", "wie", "es", "ja", "leicht\u00b7lich", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nichts eigentlich f\u00fcr GOtt klein oder gro\u00df zu nennen.", "tokens": ["Nichts", "ei\u00b7gent\u00b7lich", "f\u00fcr", "Gott", "klein", "o\u00b7der", "gro\u00df", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+--+++-+-+-", "measure": "anapaest.di.plus"}}}}}