{"textgrid.poem.32576": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von feinerem Gef\u00fchl getrieben", "tokens": ["Von", "fei\u00b7ne\u00b7rem", "Ge\u00b7f\u00fchl", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertauschte mit dem Hirtenstand", "tokens": ["Ver\u00b7tauschte", "mit", "dem", "Hir\u00b7ten\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Apollo den Olymp. Er stieg herab, und fand", "tokens": ["A\u00b7pol\u00b7lo", "den", "O\u00b7lymp", ".", "Er", "stieg", "her\u00b7ab", ",", "und", "fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die Menschen, die man ihm bald gar zu gut beschrieben", "tokens": ["Die", "Men\u00b7schen", ",", "die", "man", "ihm", "bald", "gar", "zu", "gut", "be\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PPER", "ADV", "ADV", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald gar zu schlimm, wie's immer pflegt zu gehn,", "tokens": ["Bald", "gar", "zu", "schlimm", ",", "wie's", "im\u00b7mer", "pflegt", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "$,", "VVFIN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Leutchen, mu\u00dft er sich gestehn,", "tokens": ["Die", "Leut\u00b7chen", ",", "mu\u00dft", "er", "sich", "ge\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gew\u00e4nnen n\u00e4her angesehn;", "tokens": ["Ge\u00b7w\u00e4n\u00b7nen", "n\u00e4\u00b7her", "an\u00b7ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und setzte man sich nur auf ", "tokens": ["Und", "setz\u00b7te", "man", "sich", "nur", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PRF", "ADV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "So w\u00e4ren sie doch ganz was andres, als sie schienen,", "tokens": ["So", "w\u00e4\u00b7ren", "sie", "doch", "ganz", "was", "and\u00b7res", ",", "als", "sie", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PWS", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da er aus seinen Wolkenh\u00f6hn", "tokens": ["Da", "er", "aus", "sei\u00b7nen", "Wol\u00b7ken\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer wei\u00df wie schief auf sie herunter schielte.", "tokens": ["Wer", "wei\u00df", "wie", "schief", "auf", "sie", "her\u00b7un\u00b7ter", "schiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "ADJD", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Einem Wort: Apoll, so bald er ", "tokens": ["Mit", "Ei\u00b7nem", "Wort", ":", "A\u00b7poll", ",", "so", "bald", "er"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NE", "$,", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Entdeckte \u2013 was er nie als G\u00f6ttersohn gewu\u00dft \u2013", "tokens": ["Ent\u00b7deck\u00b7te", "\u2013", "was", "er", "nie", "als", "G\u00f6t\u00b7ter\u00b7sohn", "ge\u00b7wu\u00dft", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "PPER", "ADV", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es schlage was in seiner linken Brust;", "tokens": ["Es", "schla\u00b7ge", "was", "in", "sei\u00b7ner", "lin\u00b7ken", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und unvermerkt, mit lauter Scherz und Spielen,", "tokens": ["Und", "un\u00b7ver\u00b7merkt", ",", "mit", "lau\u00b7ter", "Scherz", "und", "Spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Lernt Seine Gottheit auch f\u00fcr arme Menschlein f\u00fchlen,", "tokens": ["Lernt", "Sei\u00b7ne", "Got\u00b7theit", "auch", "f\u00fcr", "ar\u00b7me", "Mensc\u00b7hlein", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nimmt fr\u00f6hlich Teil an ihrer Lust,", "tokens": ["Nimmt", "fr\u00f6h\u00b7lich", "Teil", "an", "ih\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Entdeckt sogar, auch das sei wahre Lust,", "tokens": ["Ent\u00b7deckt", "so\u00b7gar", ",", "auch", "das", "sei", "wah\u00b7re", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und von der besten Art, mit andern sich betr\u00fcben,", "tokens": ["Und", "von", "der", "bes\u00b7ten", "Art", ",", "mit", "an\u00b7dern", "sich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kurz, schmeckt die Wollust ", "tokens": ["Kurz", ",", "schmeckt", "die", "Wol\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Zum ersten Male ganz und rein,", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "ganz", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und merkt zuletzt \u2013 (was ihm bisher geheim geblieben)", "tokens": ["Und", "merkt", "zu\u00b7letzt", "\u2013", "(", "was", "ihm", "bis\u00b7her", "ge\u00b7heim", "ge\u00b7blie\u00b7ben", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "$(", "PWS", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Was von Thessaliens Volk Apoll", "tokens": ["Was", "von", "Thes\u00b7sa\u00b7li\u00b7ens", "Volk", "A\u00b7poll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht alles lernte! Tausend Sachen", "tokens": ["Nicht", "al\u00b7les", "lern\u00b7te", "!", "Tau\u00b7send", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PIS", "VVFIN", "$.", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wovon euch G\u00f6ttern nie ein W\u00f6rtchen tr\u00e4umen soll:", "tokens": ["Wo\u00b7von", "euch", "G\u00f6t\u00b7tern", "nie", "ein", "W\u00f6rt\u00b7chen", "tr\u00e4u\u00b7men", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den losen Scherz, das wohlgemute Lachen", "tokens": ["Den", "lo\u00b7sen", "Scherz", ",", "das", "wohl\u00b7ge\u00b7mu\u00b7te", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gedr\u00fcckt von keinem Zwanggesetz,", "tokens": ["Ge\u00b7dr\u00fcckt", "von", "kei\u00b7nem", "Zwang\u00b7ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ohne Absicht, ohne Schraube,", "tokens": ["Und", "oh\u00b7ne", "Ab\u00b7sicht", ",", "oh\u00b7ne", "Schrau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das trauliche, gutlaunige Geschw\u00e4tz", "tokens": ["Das", "trau\u00b7li\u00b7che", ",", "gut\u00b7lau\u00b7ni\u00b7ge", "Ge\u00b7schw\u00e4tz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Beim Abendstern in einer Sommerlaube,", "tokens": ["Beim", "A\u00b7bends\u00b7tern", "in", "ei\u00b7ner", "Som\u00b7mer\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Und, o! den gro\u00dfen ", "tokens": ["Und", ",", "o", "!", "den", "gro\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "$.", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Mehr freie Herzen zu gewinnen,", "tokens": ["Mehr", "frei\u00b7e", "Her\u00b7zen", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als Mahmud oder Dschingiskan", "tokens": ["Als", "Mah\u00b7mud", "o\u00b7der", "Dschin\u00b7gis\u00b7kan"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich Sklaven durch sein Schwert gewann,", "tokens": ["Sich", "Skla\u00b7ven", "durch", "sein", "Schwert", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den Zauber, den die Charitinnen", "tokens": ["Den", "Zau\u00b7ber", ",", "den", "die", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Cytherens G\u00fcrtel eingewebt,", "tokens": ["Cy\u00b7the\u00b7rens", "G\u00fcr\u00b7tel", "ein\u00b7ge\u00b7webt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Was jeden Mangel deckt und jeden Reiz erhebt,", "tokens": ["Was", "je\u00b7den", "Man\u00b7gel", "deckt", "und", "je\u00b7den", "Reiz", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verlange nichts voraus, \u2013 wir werden ", "tokens": ["Ver\u00b7lan\u00b7ge", "nichts", "vo\u00b7raus", ",", "\u2013", "wir", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So wie du ", "tokens": ["So", "wie", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Die, ohne da\u00df sie auf ihn ", "tokens": ["Die", ",", "oh\u00b7ne", "da\u00df", "sie", "auf", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUI", "KOUS", "PPER", "APPR", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "In frohem Mut und dumpfem Sinn", "tokens": ["In", "fro\u00b7hem", "Mut", "und", "dum\u00b7pfem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das Herz ihm aus dem Busen spielte,", "tokens": ["Das", "Herz", "ihm", "aus", "dem", "Bu\u00b7sen", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ward seine ", "tokens": ["Ward", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "\u00bbein blo\u00dfer Hirt \u2013 ist's m\u00f6glich? \u2013 ", "tokens": ["\u00bb", "ein", "blo\u00b7\u00dfer", "Hirt", "\u2013", "ist's", "m\u00f6g\u00b7lich", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Dem sch\u00f6nsten Gott?\u00ab \u2013 Das schrie um Rache! \u2013 Schon", "tokens": ["Dem", "sch\u00f6ns\u00b7ten", "Gott", "?", "\u00ab", "\u2013", "Das", "schrie", "um", "Ra\u00b7che", "!", "\u2013", "Schon"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "$(", "PDS", "VVFIN", "APPR", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ergriff sein Zorn den m\u00e4chtgen ", "tokens": ["Er\u00b7griff", "sein", "Zorn", "den", "m\u00e4cht\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Zu gutem Gl\u00fcck entfloh der Senn ein sanfter ", "tokens": ["Zu", "gu\u00b7tem", "Gl\u00fcck", "ent\u00b7floh", "der", "Senn", "ein", "sanf\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Er stutzt, und pl\u00f6tzlich kommt ein Einfall angeflogen,", "tokens": ["Er", "stutzt", ",", "und", "pl\u00f6tz\u00b7lich", "kommt", "ein", "Ein\u00b7fall", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der seinen Eifer k\u00fchlt und bald zum Mittel wird", "tokens": ["Der", "sei\u00b7nen", "Ei\u00b7fer", "k\u00fchlt", "und", "bald", "zum", "Mit\u00b7tel", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das Ziel, wornach er l\u00fcstet, zu erreichen.", "tokens": ["Das", "Ziel", ",", "wor\u00b7nach", "er", "l\u00fcs\u00b7tet", ",", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Halt! denkt er, bist du hier was anders als ein Hirt?", "tokens": ["Halt", "!", "denkt", "er", ",", "bist", "du", "hier", "was", "an\u00b7ders", "als", "ein", "Hirt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "PWS", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was foderst du voraus vor deines gleichen?", "tokens": ["Was", "fo\u00b7derst", "du", "vo\u00b7raus", "vor", "dei\u00b7nes", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Dem Hirten, der ", "tokens": ["Dem", "Hir\u00b7ten", ",", "der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Das Herz ist frei und Lieb erzwingt sich nie.", "tokens": ["Das", "Herz", "ist", "frei", "und", "Lieb", "er\u00b7zwingt", "sich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stracks geht er hin und macht aus seinem Bogen", "tokens": ["Stracks", "geht", "er", "hin", "und", "macht", "aus", "sei\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Werkzeug des Gef\u00fchls; der Dolmetsch s\u00fc\u00dfer Pein,", "tokens": ["Ein", "Werk\u00b7zeug", "des", "Ge\u00b7f\u00fchls", ";", "der", "Dol\u00b7metsch", "s\u00fc\u00b7\u00dfer", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die neue ", "tokens": ["Die", "neu\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In seinem Arm, und schwirret durch den Hain.", "tokens": ["In", "sei\u00b7nem", "Arm", ",", "und", "schwir\u00b7ret", "durch", "den", "Hain", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Herbei gelockt von ihren s\u00fc\u00dfen T\u00f6nen", "tokens": ["Her\u00b7bei", "ge\u00b7lockt", "von", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versammeln sich um ihn die Hirten und die Sch\u00f6nen,", "tokens": ["Ver\u00b7sam\u00b7meln", "sich", "um", "ihn", "die", "Hir\u00b7ten", "und", "die", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein jedes will des Wunders Zeuge sein.", "tokens": ["Ein", "je\u00b7des", "will", "des", "Wun\u00b7ders", "Zeu\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bald wirkt der Zauber, Arme schlingen", "tokens": ["Bald", "wirkt", "der", "Zau\u00b7ber", ",", "Ar\u00b7me", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In Arme sich, den F\u00fc\u00dfen wachsen Schwingen,", "tokens": ["In", "Ar\u00b7me", "sich", ",", "den", "F\u00fc\u00b7\u00dfen", "wach\u00b7sen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der ungelehrte Tanz dreht rasch sich um ihn her,", "tokens": ["Der", "un\u00b7ge\u00b7lehr\u00b7te", "Tanz", "dreht", "rasch", "sich", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wer war gl\u00fccklicher als er!", "tokens": ["Und", "wer", "war", "gl\u00fcck\u00b7li\u00b7cher", "als", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie lieben alle nun den Sch\u00f6pfer ihrer Freuden!", "tokens": ["Wie", "lie\u00b7ben", "al\u00b7le", "nun", "den", "Sch\u00f6p\u00b7fer", "ih\u00b7rer", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist, wiewohl in Sch\u00e4fertracht,", "tokens": ["Er", "ist", ",", "wie\u00b7wohl", "in", "Sch\u00e4\u00b7fer\u00b7tracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Gott f\u00fcr sie! Er hat sie gl\u00fccklicher gemacht.", "tokens": ["Ein", "Gott", "f\u00fcr", "sie", "!", "Er", "hat", "sie", "gl\u00fcck\u00b7li\u00b7cher", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie freundlich nun ihm jede Hirtin lacht!", "tokens": ["Wie", "freund\u00b7lich", "nun", "ihm", "je\u00b7de", "Hir\u00b7tin", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie dr\u00e4ngt man sich, um nah an ihm zu weiden!", "tokens": ["Wie", "dr\u00e4ngt", "man", "sich", ",", "um", "nah", "an", "ihm", "zu", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PRF", "$,", "KOUI", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wenn am warmen Abendglanz", "tokens": ["Und", "wenn", "am", "war\u00b7men", "A\u00b7bend\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Rosenbusch, zu Chloens F\u00fc\u00dfen \u2013", "tokens": ["Im", "Ro\u00b7sen\u00b7busch", ",", "zu", "Chloens", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NE", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Indes die Holde manchen s\u00fc\u00dfen", "tokens": ["In\u00b7des", "die", "Hol\u00b7de", "man\u00b7chen", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verstohlnen Blick am halb geflochtnen Kranz", "tokens": ["Ver\u00b7stohl\u00b7nen", "Blick", "am", "halb", "ge\u00b7flocht\u00b7nen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Herunter schl\u00fcpfen l\u00e4\u00dft \u2013 wenn dann die sanfte Leier", "tokens": ["Her\u00b7un\u00b7ter", "schl\u00fcp\u00b7fen", "l\u00e4\u00dft", "\u2013", "wenn", "dann", "die", "sanf\u00b7te", "Lei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "$(", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Liebe Schmerzen mit ged\u00e4mpftem Klang", "tokens": ["Der", "Lie\u00b7be", "Schmer\u00b7zen", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So z\u00e4rtlich klagt, stets n\u00e4her sein Gesang", "tokens": ["So", "z\u00e4rt\u00b7lich", "klagt", ",", "stets", "n\u00e4\u00b7her", "sein", "