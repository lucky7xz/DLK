{"textgrid.poem.59212": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4. Berliner Tafel", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele Lerchen hellerwacht,", "tokens": ["Vie\u00b7le", "Ler\u00b7chen", "hel\u00b7ler\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zum Himmel steigen,", "tokens": ["Die", "zum", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Viele Sterne in der Nacht,", "tokens": ["Vie\u00b7le", "Ster\u00b7ne", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieler Wipfel Neigen,", "tokens": ["Vie\u00b7ler", "Wip\u00b7fel", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Viele frische Herzen dann,", "tokens": ["Vie\u00b7le", "fri\u00b7sche", "Her\u00b7zen", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die begeistert lauschen \u2013", "tokens": ["Die", "be\u00b7geis\u00b7tert", "lau\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da bricht erst der Lenz recht an,", "tokens": ["Da", "bricht", "erst", "der", "Lenz", "recht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Klang und Waldesrauschen.", "tokens": ["Klang", "und", "Wal\u00b7des\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "So sind viele hier gesellt:", "tokens": ["So", "sind", "vie\u00b7le", "hier", "ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcstige Gesellen,", "tokens": ["R\u00fcs\u00b7ti\u00b7ge", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die ihr' Sach auf Klang gestellt,", "tokens": ["Die", "ih\u00b7r'", "Sach", "auf", "Klang", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schauspiel und Novellen,", "tokens": ["Schau\u00b7spiel", "und", "No\u00b7vel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Viele dann, die recht sich freun,", "tokens": ["Vie\u00b7le", "dann", ",", "die", "recht", "sich", "freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PRELS", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn wir's l\u00f6blich machen,", "tokens": ["Wenn", "wir's", "l\u00f6b\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und, greift einer falsch darein,", "tokens": ["Und", ",", "greift", "ei\u00b7ner", "falsch", "da\u00b7rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "ADJD", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auch von Herzen lachen.", "tokens": ["Auch", "von", "Her\u00b7zen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und wo ", "tokens": ["Und", "wo"], "token_info": ["word", "word"], "pos": ["KON", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Klingt das Lied erst helle,", "tokens": ["Klingt", "das", "Lied", "erst", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie wir hier vereint zum Kranz,", "tokens": ["Wie", "wir", "hier", "ver\u00b7eint", "zum", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht die sand'ge Schelle,", "tokens": ["Bl\u00fcht", "die", "san\u00b7d'\u00b7ge", "Schel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Kuckuck ruft und Nachtigall", "tokens": ["Ku\u00b7ckuck", "ruft", "und", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von Lust und Schmerzen", "tokens": ["Und", "von", "Lust", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weckt der Schall den Widerhall", "tokens": ["Weckt", "der", "Schall", "den", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rings in tausend Herzen.", "tokens": ["Rings", "in", "tau\u00b7send", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Land, das ihr schweigend meint", "tokens": ["Ein", "Land", ",", "das", "ihr", "schwei\u00b7gend", "meint"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wir freudig singen,", "tokens": ["Und", "wir", "freu\u00b7dig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein Meer, das uns vereint", "tokens": ["Und", "ein", "Meer", ",", "das", "uns", "ver\u00b7eint"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll hin\u00fcberbringen.", "tokens": ["Soll", "hin\u00b7\u00fc\u00b7berb\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Frische Fahrt denn, nah und fern,", "tokens": ["Fri\u00b7sche", "Fahrt", "denn", ",", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allen mut'gen Seglern,", "tokens": ["Al\u00b7len", "mut'\u00b7gen", "Seg\u00b7lern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die getreu dem rechten Stern,", "tokens": ["Die", "ge\u00b7treu", "dem", "rech\u00b7ten", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schleglern oder Heglern!", "tokens": ["Schleg\u00b7lern", "o\u00b7der", "Heg\u00b7lern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}