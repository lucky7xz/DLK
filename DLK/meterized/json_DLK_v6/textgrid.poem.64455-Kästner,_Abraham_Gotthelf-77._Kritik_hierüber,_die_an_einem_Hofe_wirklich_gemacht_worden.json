{"textgrid.poem.64455": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "77. Kritik hier\u00fcber, die an einem Hofe wirklich gemacht worden", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verbessern Sie das Sinngedicht,", "tokens": ["Ver\u00b7bes\u00b7sern", "Sie", "das", "Sinn\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schneider macht ja keine H\u00fcte.", "tokens": ["Der", "Schnei\u00b7der", "macht", "ja", "kei\u00b7ne", "H\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sprach ein Cavalier; ich dankt' ihm f\u00fcr die G\u00fcte,", "tokens": ["So", "sprach", "ein", "Ca\u00b7va\u00b7lier", ";", "ich", "dankt'", "ihm", "f\u00fcr", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und fragte: Kennen Sie denn Ihren Sch\u00f6pfer nicht?", "tokens": ["Und", "frag\u00b7te", ":", "Ken\u00b7nen", "Sie", "denn", "Ih\u00b7ren", "Sch\u00f6p\u00b7fer", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}