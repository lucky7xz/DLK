{"textgrid.poem.40127": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie mit zauberischen H\u00e4nden", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mit zauberischen H\u00e4nden", "tokens": ["Wie", "mit", "zau\u00b7be\u00b7ri\u00b7schen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "greifen Tr\u00e4ume in mein Leben,", "tokens": ["grei\u00b7fen", "Tr\u00e4u\u00b7me", "in", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "will ein Altes sich vollenden,", "tokens": ["will", "ein", "Al\u00b7tes", "sich", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "will ein Neues sich begeben ...", "tokens": ["will", "ein", "Neu\u00b7es", "sich", "be\u00b7ge\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Flamme sah ich lodern", "tokens": ["Ei\u00b7ne", "Flam\u00b7me", "sah", "ich", "lo\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hoch und rein aus goldner Schale,", "tokens": ["hoch", "und", "rein", "aus", "gold\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Flamme schien zu fodern:", "tokens": ["und", "die", "Flam\u00b7me", "schien", "zu", "fo\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wirf dein Leid in diese Schale!", "tokens": ["wirf", "dein", "Leid", "in", "die\u00b7se", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und anbetend hingezwungen", "tokens": ["Und", "an\u00b7be\u00b7tend", "hin\u00b7ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "f\u00fchlt' ich Gluten mich umfangen,", "tokens": ["f\u00fchlt'", "ich", "Glu\u00b7ten", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "rauschend k\u00fc\u00dften ihre Zungen", "tokens": ["rau\u00b7schend", "k\u00fc\u00df\u00b7ten", "ih\u00b7re", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auge mir und Stirn und Wangen.", "tokens": ["Au\u00b7ge", "mir", "und", "Stirn", "und", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich f\u00fchlte stumm vergehen", "tokens": ["Und", "ich", "f\u00fchl\u00b7te", "stumm", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "all mein Leid mit Einem Male,", "tokens": ["all", "mein", "Leid", "mit", "Ei\u00b7nem", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "rauschend mich als Flamme wehen", "tokens": ["rau\u00b7schend", "mich", "als", "Flam\u00b7me", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "selber in der goldnen Schale ...", "tokens": ["sel\u00b7ber", "in", "der", "gold\u00b7nen", "Scha\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie mit zauberischen H\u00e4nden", "tokens": ["Wie", "mit", "zau\u00b7be\u00b7ri\u00b7schen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "greifen Tr\u00e4ume in mein Leben.", "tokens": ["grei\u00b7fen", "Tr\u00e4u\u00b7me", "in", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ein Altes sich vollenden?", "tokens": ["Will", "ein", "Al\u00b7tes", "sich", "voll\u00b7en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "will ein Neues sich begeben?", "tokens": ["will", "ein", "Neu\u00b7es", "sich", "be\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}