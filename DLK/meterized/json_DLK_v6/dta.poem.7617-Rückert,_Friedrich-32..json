{"dta.poem.7617": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du am rechten Ort das rechte Wort zu sagen", "tokens": ["Wenn", "du", "am", "rech\u00b7ten", "Ort", "das", "rech\u00b7te", "Wort", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast unterlassen, bleibt es immer zu beklagen.", "tokens": ["Hast", "un\u00b7ter\u00b7las\u00b7sen", ",", "bleibt", "es", "im\u00b7mer", "zu", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn in Gedanken dann du's sagest hinterher,", "tokens": ["Wenn", "in", "Ge\u00b7dan\u00b7ken", "dann", "du's", "sa\u00b7gest", "hin\u00b7ter\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird die Vers\u00e4umnis dir nur f\u00fchlbar um so mehr.", "tokens": ["Wird", "die", "Ver\u00b7s\u00e4um\u00b7nis", "dir", "nur", "f\u00fchl\u00b7bar", "um", "so", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "APPR", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch unterla\u00df nur nicht, und sage dir es fein;", "tokens": ["Doch", "un\u00b7ter\u00b7la\u00df", "nur", "nicht", ",", "und", "sa\u00b7ge", "dir", "es", "fein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielleicht ein andermal wirst du dann kl\u00fcger seyn.", "tokens": ["Viel\u00b7leicht", "ein", "an\u00b7der\u00b7mal", "wirst", "du", "dann", "kl\u00fc\u00b7ger", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}