{"dta.poem.15940": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "145.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf hoher Alpe steht die Pflanze fest im Bodem,", "tokens": ["Auf", "ho\u00b7her", "Al\u00b7pe", "steht", "die", "Pflan\u00b7ze", "fest", "im", "Bo\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die freie Luft haucht sie den Bl\u00fctenodem.", "tokens": ["Und", "in", "die", "frei\u00b7e", "Luft", "haucht", "sie", "den", "Bl\u00fc\u00b7te\u00b7no\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst sie farbig bl\u00fchn und duftig, doch das Gr\u00fcn", "tokens": ["Du", "siehst", "sie", "far\u00b7big", "bl\u00fchn", "und", "duf\u00b7tig", ",", "doch", "das", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "KON", "ADJD", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Bl\u00e4tterwuchses mu\u00df erliegen dem Bem\u00fchn.", "tokens": ["Des", "Bl\u00e4t\u00b7ter\u00b7wuch\u00b7ses", "mu\u00df", "er\u00b7lie\u00b7gen", "dem", "Be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Pflanze gibt die Wucht der Bl\u00e4tter auf, und sucht", "tokens": ["Die", "Pflan\u00b7ze", "gibt", "die", "Wucht", "der", "Bl\u00e4t\u00b7ter", "auf", ",", "und", "sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bl\u00fct' entgegen nur zu retten ihrer Frucht.", "tokens": ["Die", "Bl\u00fct'", "ent\u00b7ge\u00b7gen", "nur", "zu", "ret\u00b7ten", "ih\u00b7rer", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib auf den Bl\u00e4tterschwall! du kannst ihn nicht erhalten;", "tokens": ["Gib", "auf", "den", "Bl\u00e4t\u00b7ter\u00b7schwall", "!", "du", "kannst", "ihn", "nicht", "er\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df die Bl\u00fcte sich in Himmelsluft entfalten!", "tokens": ["Und", "la\u00df", "die", "Bl\u00fc\u00b7te", "sich", "in", "Him\u00b7mel\u00b7sluft", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}