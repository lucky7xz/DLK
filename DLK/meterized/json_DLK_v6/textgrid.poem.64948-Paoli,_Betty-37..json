{"textgrid.poem.64948": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Auge, das dem Ruhm in's Antlitz durfte schauen,", "tokens": ["Das", "Au\u00b7ge", ",", "das", "dem", "Ruhm", "in's", "Ant\u00b7litz", "durf\u00b7te", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das schaut hinfort den Tod ohn' Angst und ohne Grauen.", "tokens": ["Das", "schaut", "hin\u00b7fort", "den", "Tod", "ohn'", "Angst", "und", "oh\u00b7ne", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Auge, das den Tod in n\u00e4chster N\u00e4h' geseh'n,", "tokens": ["Das", "Au\u00b7ge", ",", "das", "den", "Tod", "in", "n\u00e4chs\u00b7ter", "N\u00e4h'", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach Ruhmes Traumgesicht wird's nicht mehr streifend geh'n.", "tokens": ["Nach", "Ruh\u00b7mes", "Traum\u00b7ge\u00b7sicht", "wird's", "nicht", "mehr", "strei\u00b7fend", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wer sie beide sah, wie blieb ihm unverstanden,", "tokens": ["Und", "wer", "sie", "bei\u00b7de", "sah", ",", "wie", "blieb", "ihm", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PIS", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df beid' ein Schattenbild in zweierlei Gewanden?", "tokens": ["Da\u00df", "beid'", "ein", "Schat\u00b7ten\u00b7bild", "in", "zwei\u00b7er\u00b7lei", "Ge\u00b7wan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}