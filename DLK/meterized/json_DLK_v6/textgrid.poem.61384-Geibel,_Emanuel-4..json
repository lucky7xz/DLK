{"textgrid.poem.61384": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du der Sch\u00f6nheit F\u00fcrstin stolz und hoch,", "tokens": ["O", "du", "der", "Sch\u00f6n\u00b7heit", "F\u00fcrs\u00b7tin", "stolz", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du R\u00e4tselvolle, die kein Sinn erfa\u00dft,", "tokens": ["Du", "R\u00e4t\u00b7sel\u00b7vol\u00b7le", ",", "die", "kein", "Sinn", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist so kalt und z\u00fcndest Flammen doch,", "tokens": ["Du", "bist", "so", "kalt", "und", "z\u00fcn\u00b7dest", "Flam\u00b7men", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst so ruhig raubst du alle Rast;", "tokens": ["Und", "selbst", "so", "ru\u00b7hig", "raubst", "du", "al\u00b7le", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du machst mich irr an meines Herzens Schlag,", "tokens": ["Du", "machst", "mich", "irr", "an", "mei\u00b7nes", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich selbst verlor ich, seit ich dich gesehn;", "tokens": ["Mich", "selbst", "ver\u00b7lor", "ich", ",", "seit", "ich", "dich", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlaflose Nacht l\u00f6st ab vertr\u00e4umten Tag", "tokens": ["Schlaf\u00b7lo\u00b7se", "Nacht", "l\u00f6st", "ab", "ver\u00b7tr\u00e4um\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Mit Zweifeln, Gluten, Wehn \u2013", "tokens": ["Mit", "Zwei\u00b7feln", ",", "Glu\u00b7ten", ",", "Wehn", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Du aber l\u00e4chelst fort, als w\u00e4re nichts geschehn.", "tokens": ["Du", "a\u00b7ber", "l\u00e4\u00b7chelst", "fort", ",", "als", "w\u00e4\u00b7re", "nichts", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,", "KOKOM", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Oft zweifl' ich, da\u00df dir eine Seele ward,", "tokens": ["Oft", "zweifl'", "ich", ",", "da\u00df", "dir", "ei\u00b7ne", "See\u00b7le", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder mein' ich dann, sie schlafe nur,", "tokens": ["Und", "wie\u00b7der", "mein'", "ich", "dann", ",", "sie", "schla\u00b7fe", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wer sie weck' aus ihren Tr\u00e4umen zart:", "tokens": ["Und", "wer", "sie", "weck'", "aus", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "zart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr holdstes Wunder zeige ", "tokens": ["Ihr", "holds\u00b7tes", "Wun\u00b7der", "zei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Urpl\u00f6tzlich, wie der Lenz kommt \u00fcber Nacht,", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", ",", "wie", "der", "Lenz", "kommt", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So m\u00fcss' aufquellend einst in j\u00e4her Lust", "tokens": ["So", "m\u00fcss'", "auf\u00b7quel\u00b7lend", "einst", "in", "j\u00e4\u00b7her", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVPP", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Wesen all erbl\u00fchn in Fr\u00fchlingspracht,", "tokens": ["Dein", "We\u00b7sen", "all", "er\u00b7bl\u00fchn", "in", "Fr\u00fch\u00b7lings\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn deine junge Brust", "tokens": ["Wenn", "dei\u00b7ne", "jun\u00b7ge", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zum ersten Male f\u00fchlt, wovon sie nie gewu\u00dft.", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "f\u00fchlt", ",", "wo\u00b7von", "sie", "nie", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O d\u00fcrft' ich der gefeite Zaubrer sein,", "tokens": ["O", "d\u00fcrft'", "ich", "der", "ge\u00b7fei\u00b7te", "Zaub\u00b7rer", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der so den Frost in Maienwonne kehrt,", "tokens": ["Der", "so", "den", "Frost", "in", "Mai\u00b7en\u00b7won\u00b7ne", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der deine Wangen gl\u00fchn in hast'gem Schein,", "tokens": ["Der", "dei\u00b7ne", "Wan\u00b7gen", "gl\u00fchn", "in", "hast'\u00b7gem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Aug' in br\u00fcnst'gen Tr\u00e4nen fluten lehrt!", "tokens": ["Dein", "Aug'", "in", "br\u00fcnst'\u00b7gen", "Tr\u00e4\u00b7nen", "flu\u00b7ten", "lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "D\u00fcrft' ich der sein, der dir die Seele gibt,", "tokens": ["D\u00fcrft'", "ich", "der", "sein", ",", "der", "dir", "die", "See\u00b7le", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "VAINF", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die stummen R\u00e4tsel l\u00f6send deinem Sinn,", "tokens": ["Die", "stum\u00b7men", "R\u00e4t\u00b7sel", "l\u00f6\u00b7send", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Sel'ge, den du liebst, weil er dich liebt \u2013", "tokens": ["Der", "Sel'\u00b7ge", ",", "den", "du", "liebst", ",", "weil", "er", "dich", "liebt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O was ich hab' und bin,", "tokens": ["O", "was", "ich", "hab'", "und", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die eigne Seele halb, die ganze g\u00e4b' ich hin!", "tokens": ["Die", "eig\u00b7ne", "See\u00b7le", "halb", ",", "die", "gan\u00b7ze", "g\u00e4b'", "ich", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verwegner Traum! Doch wie du immer seist:", "tokens": ["Ver\u00b7weg\u00b7ner", "Traum", "!", "Doch", "wie", "du", "im\u00b7mer", "seist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich treibt zu dir allm\u00e4chtige Gewalt;", "tokens": ["Mich", "treibt", "zu", "dir", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebannt in deine Kreise liegt mein Geist,", "tokens": ["Ge\u00b7bannt", "in", "dei\u00b7ne", "Krei\u00b7se", "liegt", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich kann nicht los, und tust du noch so kalt.", "tokens": ["Ich", "kann", "nicht", "los", ",", "und", "tust", "du", "noch", "so", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du ziehst mich nach dir wie der Mond die Flut,", "tokens": ["Du", "ziehst", "mich", "nach", "dir", "wie", "der", "Mond", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie der Magnet das Eisen siegreich zieht;", "tokens": ["Wie", "der", "Mag\u00b7net", "das", "Ei\u00b7sen", "sieg\u00b7reich", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ob du harmlos spielst mit meiner Glut,", "tokens": ["Und", "ob", "du", "harm\u00b7los", "spielst", "mit", "mei\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ob streng dein Auge sieht:", "tokens": ["Ob", "streng", "dein", "Au\u00b7ge", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mein unstet Herz ist dein, und dein mein dunkles Lied.", "tokens": ["Mein", "un\u00b7stet", "Herz", "ist", "dein", ",", "und", "dein", "mein", "dunk\u00b7les", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "PPOSAT", "$,", "KON", "PPOSAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}