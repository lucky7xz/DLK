{"textgrid.poem.49580": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Anbetung der Hirten", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Bethlehem ging ein kalter Wind,", "tokens": ["Um", "Beth\u00b7le\u00b7hem", "ging", "ein", "kal\u00b7ter", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Stall war das arme Christuskind.", "tokens": ["Im", "Stall", "war", "das", "ar\u00b7me", "Chris\u00b7tus\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es lag auf zwei B\u00fcschel Grummetheu,", "tokens": ["Es", "lag", "auf", "zwei", "B\u00fc\u00b7schel", "Grum\u00b7me\u00b7theu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Ochs und ein Esel standen dabei.", "tokens": ["Ein", "Ochs", "und", "ein", "E\u00b7sel", "stan\u00b7den", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Hirten haben es schon gewi\u00dft,", "tokens": ["Die", "Hir\u00b7ten", "ha\u00b7ben", "es", "schon", "ge\u00b7wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df selbiges Kindlein der Heiland ist.", "tokens": ["Da\u00df", "sel\u00b7bi\u00b7ges", "Kin\u00b7dlein", "der", "Hei\u00b7land", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denn auf dem Felde und bei der Nacht", "tokens": ["Denn", "auf", "dem", "Fel\u00b7de", "und", "bei", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hat 's ihnen ein Engel zugebracht.", "tokens": ["Hat", "'s", "ih\u00b7nen", "ein", "En\u00b7gel", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie haben gebetet und sich gefreut,", "tokens": ["Sie", "ha\u00b7ben", "ge\u00b7be\u00b7tet", "und", "sich", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PRF", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und einer sagte: Ihr lieben Leut',", "tokens": ["Und", "ei\u00b7ner", "sag\u00b7te", ":", "Ihr", "lie\u00b7ben", "Leut'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich glaub 's wohl, da\u00df er bei Armen steht,", "tokens": ["Ich", "glaub", "'s", "wohl", ",", "da\u00df", "er", "bei", "Ar\u00b7men", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schon weil 's ihm selber so schlecht ergeht.", "tokens": ["Schon", "weil", "'s", "ihm", "sel\u00b7ber", "so", "schlecht", "er\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}