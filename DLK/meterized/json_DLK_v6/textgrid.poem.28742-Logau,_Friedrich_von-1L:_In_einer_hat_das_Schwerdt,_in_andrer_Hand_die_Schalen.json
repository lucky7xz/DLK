{"textgrid.poem.28742": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer hat das Schwerdt, in andrer Hand die Schalen", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer hat das Schwerdt, in andrer Hand die Schalen", "tokens": ["In", "ei\u00b7ner", "hat", "das", "Schwerdt", ",", "in", "an\u00b7drer", "Hand", "die", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gerechtigkeit; dann so siht man sie meistens mahlen.", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", ";", "dann", "so", "siht", "man", "sie", "meis\u00b7tens", "mah\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie so? Weil sich zur Wag ein Schwacher gerne kehrt,", "tokens": ["Wie", "so", "?", "Weil", "sich", "zur", "Wag", "ein", "Schwa\u00b7cher", "ger\u00b7ne", "kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "KOUS", "PRF", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Starcker aber nicht, der gerne fast das Schwerdt.", "tokens": ["Ein", "Star\u00b7cker", "a\u00b7ber", "nicht", ",", "der", "ger\u00b7ne", "fast", "das", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "p