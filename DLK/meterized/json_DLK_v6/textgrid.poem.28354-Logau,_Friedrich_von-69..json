{"textgrid.poem.28354": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treues Hertze, du zeuchst abe", "tokens": ["Treu\u00b7es", "Hert\u00b7ze", ",", "du", "zeuchst", "a\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Au\u00df der Welt und gehst zu Grabe,", "tokens": ["Au\u00df", "der", "Welt", "und", "gehst", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein zu nemen Freud und Ruh,", "tokens": ["Ein", "zu", "ne\u00b7men", "Freud", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Himmel richtet zu.", "tokens": ["Die", "der", "Him\u00b7mel", "rich\u00b7tet", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir und andren deinen lieben", "tokens": ["Mir", "und", "an\u00b7dren", "dei\u00b7nen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PIS", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist an deiner Stelle blieben", "tokens": ["Ist", "an", "dei\u00b7ner", "Stel\u00b7le", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bey so sonst geh\u00e4uffter Noth", "tokens": ["Bey", "so", "sonst", "ge\u00b7h\u00e4uff\u00b7ter", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hertzens Leid um deinen Tod.", "tokens": ["Hert\u00b7zens", "Leid", "um", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch die hier die Zeit verletzet,", "tokens": ["Doch", "die", "hier", "die", "Zeit", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wird bald haben dort ergetzet", "tokens": ["Wird", "bald", "ha\u00b7ben", "dort", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ewigkeit, die ohne Ziel", "tokens": ["E\u00b7wig\u00b7keit", ",", "die", "oh\u00b7ne", "Ziel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Uns auffs neue treuen wil.", "tokens": ["Uns", "auffs", "neu\u00b7e", "treu\u00b7en", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Mir wird seyn mein Sarck gemessen,", "tokens": ["Mir", "wird", "seyn", "mein", "Sarck", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eh dein Lob ich kan vergessen.", "tokens": ["Eh", "dein", "Lob", "ich", "kan", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VMFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "W\u00fcrdig bistu, da\u00df dein Ruhm", "tokens": ["W\u00fcr\u00b7dig", "bis\u00b7tu", ",", "da\u00df", "dein", "Ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Bleibt, weil bleibt das Menschenthum.", "tokens": ["Bleibt", ",", "weil", "bleibt", "das", "Men\u00b7schen\u00b7thum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Habe Danck f\u00fcr deine Liebe,", "tokens": ["Ha\u00b7be", "Danck", "f\u00fcr", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die best\u00e4ndig war, wanns tr\u00fcbe", "tokens": ["Die", "be\u00b7st\u00e4n\u00b7dig", "war", ",", "wanns", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "So, wie wann es helle war,", "tokens": ["So", ",", "wie", "wann", "es", "hel\u00b7le", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PWAV", "PPER", "ADJA", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "So in Gl\u00fcck als in Gefahr!", "tokens": ["So", "in", "Gl\u00fcck", "als", "in", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Habe Danck f\u00fcr deine Treue,", "tokens": ["Ha\u00b7be", "Danck", "f\u00fcr", "dei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die stets bliebe frisch und neue!", "tokens": ["Die", "stets", "blie\u00b7be", "frisch", "und", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Habe Danck f\u00fcrs werthe Pfand,", "tokens": ["Ha\u00b7be", "Danck", "f\u00fcrs", "wert\u00b7he", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Das du l\u00e4st in meiner Hand!", "tokens": ["Das", "du", "l\u00e4st", "in", "mei\u00b7ner", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Habe Danck f\u00fcr M\u00fch und Sorgen,", "tokens": ["Ha\u00b7be", "Danck", "f\u00fcr", "M\u00fch", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Die bi\u00df Abends an vom Morgen", "tokens": ["Die", "bi\u00df", "A\u00b7bends", "an", "vom", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Deine weisse Redligheit", "tokens": ["Dei\u00b7ne", "weis\u00b7se", "Red\u00b7lig\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Pfloge mir zur Nutzbarkeit!", "tokens": ["Pflo\u00b7ge", "mir", "zur", "Nutz\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Habe Danck, da\u00df deine Tugend,", "tokens": ["Ha\u00b7be", "Danck", ",", "da\u00df", "dei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Habe Danck, da\u00df deine Jugend,", "tokens": ["Ha\u00b7be", "Danck", ",", "da\u00df", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ob wol eine kurtze Zeit,", "tokens": ["Ob", "wol", "ei\u00b7ne", "kurt\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.32": {"text": "Mir so viel gab Gn\u00fcgligkeit!", "tokens": ["Mir", "so", "viel", "gab", "Gn\u00fcg\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Fahr im Friede! Gott wils haben;", "tokens": ["Fahr", "im", "Frie\u00b7de", "!", "Gott", "wils", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Aber lasse deine Gaben", "tokens": ["A\u00b7ber", "las\u00b7se", "dei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Deme, da\u00df zum Troste mir", "tokens": ["De\u00b7me", ",", "da\u00df", "zum", "Tros\u00b7te", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "APPRART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "\u00dcbrig blieben ist von dir.", "tokens": ["\u00dcb\u00b7rig", "blie\u00b7ben", "ist", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Fahr im Fried! ich kans nicht wenden,", "tokens": ["Fahr", "im", "Fried", "!", "ich", "kans", "nicht", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Bin zu schwach de\u00df Herren H\u00e4nden;", "tokens": ["Bin", "zu", "schwach", "de\u00df", "Her\u00b7ren", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Du zeuchst weg, wo ich ietzt bin,", "tokens": ["Du", "zeuchst", "weg", ",", "wo", "ich", "ietzt", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Ich, wo du bist, kumme hin.", "tokens": ["Ich", ",", "wo", "du", "bist", ",", "kum\u00b7me", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}