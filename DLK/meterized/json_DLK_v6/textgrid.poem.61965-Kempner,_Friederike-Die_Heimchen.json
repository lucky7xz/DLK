{"textgrid.poem.61965": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Die Heimchen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst Du, wie die Heimchen zirpen?", "tokens": ["H\u00f6rst", "Du", ",", "wie", "die", "Heim\u00b7chen", "zir\u00b7pen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird es Dir nicht heimlich so?", "tokens": ["Wird", "es", "Dir", "nicht", "heim\u00b7lich", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es nicht, als wenn Dir riefen", "tokens": ["Ist", "es", "nicht", ",", "als", "wenn", "Dir", "rie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundesstimmen irgendwo?", "tokens": ["Freun\u00b7dess\u00b7tim\u00b7men", "ir\u00b7gend\u00b7wo", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "D\u00fcst're Nacht im Krankenzimmer,", "tokens": ["D\u00fcst'\u00b7re", "Nacht", "im", "Kran\u00b7ken\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrme drau\u00dfen, St\u00fcrme drin,", "tokens": ["St\u00fcr\u00b7me", "drau\u00b7\u00dfen", ",", "St\u00fcr\u00b7me", "drin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuersbrunst am dunklen Himmel,", "tokens": ["Feu\u00b7ers\u00b7brunst", "am", "dunk\u00b7len", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfe Glut um Herz und Sinn.", "tokens": ["Hei\u00b7\u00dfe", "Glut", "um", "Herz", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sehnend mich nach neuer Sch\u00f6pfung,", "tokens": ["Seh\u00b7nend", "mich", "nach", "neu\u00b7er", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich nach ros'gem Morgenlicht,", "tokens": ["Mich", "nach", "ros'\u00b7gem", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00df ich still beim Lampenscheine,", "tokens": ["Sa\u00df", "ich", "still", "beim", "Lam\u00b7pen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kummer in dem Angesicht.", "tokens": ["Kum\u00b7mer", "in", "dem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Horch da, pl\u00f6tzlich Heimchen sangen,", "tokens": ["Horch", "da", ",", "pl\u00f6tz\u00b7lich", "Heim\u00b7chen", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traulich, heimlich ward es so,", "tokens": ["Trau\u00b7lich", ",", "heim\u00b7lich", "ward", "es", "so", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn Freundesstimmen riefen,", "tokens": ["Als", "wenn", "Freun\u00b7dess\u00b7tim\u00b7men", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stend, hoffend irgendwo!", "tokens": ["Tr\u00f6s\u00b7tend", ",", "hof\u00b7fend", "ir\u00b7gend\u00b7wo", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heimchent\u00f6ne, Heimchenworte,", "tokens": ["Heim\u00b7chen\u00b7t\u00f6\u00b7ne", ",", "Heim\u00b7chen\u00b7wor\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klangvoll fing's zu sprechen an:", "tokens": ["Klang\u00b7voll", "fing's", "zu", "spre\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwer die Kehlchen singen lehrte,", "tokens": ["\u00bb", "wer", "die", "Kehl\u00b7chen", "sin\u00b7gen", "lehr\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der auch Heilung schaffen kann!\u00ab", "tokens": ["Der", "auch", "Hei\u00b7lung", "schaf\u00b7fen", "kann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}