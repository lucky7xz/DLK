{"dta.poem.3897": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "17) Schertz-Schreiben an ein Frauen-  \n zimmer, von der Elecktricit\u00e4t.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Freundin! meine Sehnsucht spricht,", "tokens": ["Freun\u00b7din", "!", "mei\u00b7ne", "Sehn\u00b7sucht", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dein g\u00fctiges Erweisen,", "tokens": ["La\u00df", "dein", "g\u00fc\u00b7ti\u00b7ges", "Er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So die Nacht des Finstren bricht,", "tokens": ["So", "die", "Nacht", "des", "Finst\u00b7ren", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch voritzt Elecktrisch, heissen.", "tokens": ["Auch", "vo\u00b7ritzt", "E\u00b7leck\u00b7trisch", ",", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "NN", "$,", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mancher C\u00f6rper zeiget bald", "tokens": ["Man\u00b7cher", "C\u00f6r\u00b7per", "zei\u00b7get", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch Bewegung die Gewalt,", "tokens": ["Durch", "Be\u00b7we\u00b7gung", "die", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und durch wiederholtes Drehen", "tokens": ["Und", "durch", "wie\u00b7der\u00b7hol\u00b7tes", "Dre\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan man seine Kr\u00e4ffte sehen.", "tokens": ["Kan", "man", "sei\u00b7ne", "Kr\u00e4ff\u00b7te", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00f6\u00dfre C\u00f6rper ziehen ja", "tokens": ["Gr\u00f6\u00df\u00b7re", "C\u00f6r\u00b7per", "zie\u00b7hen", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleinre zu sich in die N\u00e4he;", "tokens": ["Klein\u00b7re", "zu", "sich", "in", "die", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist dein Anmuths-Feuer da,", "tokens": ["Ist", "dein", "An\u00b7muths\u00b7Feu\u00b7er", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So wei\u00df ich, wohin ich gehe.", "tokens": ["So", "wei\u00df", "ich", ",", "wo\u00b7hin", "ich", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Reichst du mir die zarte Hand,", "tokens": ["Reichst", "du", "mir", "die", "zar\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O! so ist mir l\u00e4ngst bekannt,", "tokens": ["O", "!", "so", "ist", "mir", "l\u00e4ngst", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df, obs gleich dein Hertz verlachet,", "tokens": ["Da\u00df", ",", "obs", "gleich", "dein", "Hertz", "ver\u00b7la\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mich die Krafft Elecktrisch machet.", "tokens": ["Mich", "die", "Krafft", "E\u00b7leck\u00b7trisch", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da die Elecktricit\u00e4t,", "tokens": ["Da", "die", "E\u00b7leck\u00b7tri\u00b7ci\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und ihr gantz besondres Wesen,", "tokens": ["Und", "ihr", "gantz", "be\u00b7sond\u00b7res", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast die halbe Welt versteht,", "tokens": ["Fast", "die", "hal\u00b7be", "Welt", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie es in Schrifften lesen,", "tokens": ["Weil", "sie", "es", "in", "Schriff\u00b7ten", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb doch mir von deiner Krafft,", "tokens": ["Gieb", "doch", "mir", "von", "dei\u00b7ner", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mir ein Elecktrum schafft,", "tokens": ["Die", "mir", "ein", "E\u00b7leck\u00b7trum", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundin! s\u00e4ume nicht zu eilen,", "tokens": ["Freun\u00b7din", "!", "s\u00e4u\u00b7me", "nicht", "zu", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir dasselbe mitzutheilen.", "tokens": ["Mir", "das\u00b7sel\u00b7be", "mit\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O! die Krafft ist wunderbar,", "tokens": ["O", "!", "die", "Krafft", "ist", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ist f\u00e4hig zum Entz\u00fcnden.", "tokens": ["Sie", "ist", "f\u00e4\u00b7hig", "zum", "Ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Di\u00df mu\u00df man bey dir f\u00fcrwar", "tokens": ["Di\u00df", "mu\u00df", "man", "bey", "dir", "f\u00fcr\u00b7war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "APPR", "PPER", "ADV"], "meter": "-+----+", "measure": "dactylic.init"}, "line.4": {"text": "In der Augen Strahlen finden.", "tokens": ["In", "der", "Au\u00b7gen", "Strah\u00b7len", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie ist schneller als ein Blitz;", "tokens": ["Sie", "ist", "schnel\u00b7ler", "als", "ein", "Blitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt sich doch des Feuers Sitz", "tokens": ["Zeigt", "sich", "doch", "des", "Feu\u00b7ers", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bey dem sch\u00f6nen Frauenzimmer,", "tokens": ["Bey", "dem", "sch\u00f6\u00b7nen", "Frau\u00b7en\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Augen hellsten Schimmer.", "tokens": ["In", "der", "Au\u00b7gen", "hells\u00b7ten", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu des Wercks Bef\u00f6rderung", "tokens": ["Zu", "des", "Wercks", "Be\u00b7f\u00f6r\u00b7de\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Braucht man sonsten blaue Seide;", "tokens": ["Braucht", "man", "sons\u00b7ten", "blau\u00b7e", "Sei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundin! du bist zart und jung,", "tokens": ["Freun\u00b7din", "!", "du", "bist", "zart", "und", "jung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du erweckest Lust und Freude,", "tokens": ["Du", "er\u00b7we\u00b7ckest", "Lust", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mache ein Elecktrisch Band,", "tokens": ["Ma\u00b7che", "ein", "E\u00b7leck\u00b7trisch", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn das Hertze ist entbrannt.", "tokens": ["Denn", "das", "Hert\u00b7ze", "ist", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df doch nicht die Sehnsucht schwitzen,", "tokens": ["La\u00df", "doch", "nicht", "die", "Sehn\u00b7sucht", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df so Licht als Flammen blitzen.", "tokens": ["La\u00df", "so", "Licht", "als", "Flam\u00b7men", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Di\u00df ist ihre Wissenschafft,", "tokens": ["Di\u00df", "ist", "ih\u00b7re", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die den Zoll der M\u00fche zahlet.", "tokens": ["Die", "den", "Zoll", "der", "M\u00fc\u00b7he", "zah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Di\u00df ist ihre seltne Krafft,", "tokens": ["Di\u00df", "ist", "ih\u00b7re", "selt\u00b7ne", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die hier dunckel abgemahlet.", "tokens": ["Die", "hier", "dun\u00b7ckel", "ab\u00b7ge\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df ist die Elecktricit\u00e4t,", "tokens": ["Di\u00df", "ist", "die", "E\u00b7leck\u00b7tri\u00b7ci\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die in unserm Sinn entsteht.", "tokens": ["Die", "in", "un\u00b7serm", "Sinn", "ent\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df mich deine Flammen sp\u00fchren,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Flam\u00b7men", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hilff mich nun elecktrisiren.", "tokens": ["Hilff", "mich", "nun", "e\u00b7leck\u00b7tri\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}}}}