{"dta.poem.11738": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hang' an einem Haar noch mit der Welt zusammen,", "tokens": ["Ich", "hang'", "an", "ei\u00b7nem", "Haar", "noch", "mit", "der", "Welt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unzerrei\u00dfbar war den St\u00fcrmen es, den Flammen.", "tokens": ["Und", "un\u00b7zer\u00b7rei\u00df\u00b7bar", "war", "den", "St\u00fcr\u00b7men", "es", ",", "den", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An einem Haare zieht die Welt mich, die ich ziehe;", "tokens": ["An", "ei\u00b7nem", "Haa\u00b7re", "zieht", "die", "Welt", "mich", ",", "die", "ich", "zie\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr folg' ich, die mich flieht, sie folgt mir, die ich fliehe.", "tokens": ["Ihr", "fol\u00b7g'", "ich", ",", "die", "mich", "flieht", ",", "sie", "folgt", "mir", ",", "die", "ich", "flie\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Mir folgt ihr Bildertanz, ihr folgt mein Liederchor,", "tokens": ["Mir", "folgt", "ihr", "Bil\u00b7der\u00b7tanz", ",", "ihr", "folgt", "mein", "Lie\u00b7der\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir ziehn uns ab und an; und ziehn uns beid' empor.", "tokens": ["Wir", "ziehn", "uns", "ab", "und", "an", ";", "und", "ziehn", "uns", "beid'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo sie empor nicht zog, w\u00e4r' ich in mir versunken;", "tokens": ["Wo", "sie", "em\u00b7por", "nicht", "zog", ",", "w\u00e4r'", "ich", "in", "mir", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ich nicht ihr entflog, w\u00e4r' ich nicht liebetrunken.", "tokens": ["Wo", "ich", "nicht", "ihr", "ent\u00b7flog", ",", "w\u00e4r'", "ich", "nicht", "lie\u00b7be\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So hat der Liebe Hand das leise Band gewebt,", "tokens": ["So", "hat", "der", "Lie\u00b7be", "Hand", "das", "lei\u00b7se", "Band", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lieb', an deren Band ewig das Ew'ge schwebt.", "tokens": ["Die", "Lieb'", ",", "an", "de\u00b7ren", "Band", "e\u00b7wig", "das", "Ew'\u00b7ge", "schwebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "ADJD", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}