{"dta.poem.2280": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Anla\u00df zum Schlafe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Zefirs sanftem S\u00e4useln", "tokens": ["Von", "Ze\u00b7firs", "sanf\u00b7tem", "S\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich oft eingeschlafen;", "tokens": ["Bin", "ich", "oft", "ein\u00b7ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Saft gepre\u00dfter Trauben", "tokens": ["Vom", "Saft", "ge\u00b7pre\u00df\u00b7ter", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin ich oft eingeschlafen;", "tokens": ["Bin", "ich", "oft", "ein\u00b7ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Schatten iunger B\u00e4ume,", "tokens": ["Im", "Schat\u00b7ten", "i\u00b7un\u00b7ger", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vom Schwarm der muntern Bienen,", "tokens": ["Vom", "Schwarm", "der", "mun\u00b7tern", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beim Sprudeln kleiner Quellen", "tokens": ["Beim", "Spru\u00b7deln", "klei\u00b7ner", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bin ich oft eingeschlafen;", "tokens": ["Bin", "ich", "oft", "ein\u00b7ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Doch, soll ich ietzo schlafen:", "tokens": ["Doch", ",", "soll", "ich", "iet\u00b7zo", "schla\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So m\u00fcssen K\u00fcsse rauschen.", "tokens": ["So", "m\u00fcs\u00b7sen", "K\u00fcs\u00b7se", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}