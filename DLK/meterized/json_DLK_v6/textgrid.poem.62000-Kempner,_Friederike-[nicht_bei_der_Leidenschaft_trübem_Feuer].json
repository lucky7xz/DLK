{"textgrid.poem.62000": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[nicht bei der Leidenschaft tr\u00fcbem Feuer]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht bei der Leidenschaft tr\u00fcbem Feuer,", "tokens": ["Nicht", "bei", "der", "Lei\u00b7den\u00b7schaft", "tr\u00fc\u00b7bem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nur bei der Weisheit hell strahlendem Licht,", "tokens": ["Nur", "bei", "der", "Weis\u00b7heit", "hell", "strah\u00b7len\u00b7dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schaue die Dinge, die Gott erschaffen,", "tokens": ["Schau\u00b7e", "die", "Din\u00b7ge", ",", "die", "Gott", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das Wie und das Wann, \u2013 das ergr\u00fcndest Du nicht.", "tokens": ["Das", "Wie", "und", "das", "Wann", ",", "\u2013", "das", "er\u00b7gr\u00fcn\u00b7dest", "Du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue die Wunder, \u2013 die gro\u00dfen und kleinen \u2013", "tokens": ["Schau\u00b7e", "die", "Wun\u00b7der", ",", "\u2013", "die", "gro\u00b7\u00dfen", "und", "klei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "ART", "ADJA", "KON", "ADJA", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Leuchtende Sterne in d\u00fcsterer Nacht!", "tokens": ["Leuch\u00b7ten\u00b7de", "Ster\u00b7ne", "in", "d\u00fcs\u00b7te\u00b7rer", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Doch verschm\u00e4h' ob des Glanzes von tausend Sonnen", "tokens": ["Doch", "ver\u00b7schm\u00e4h'", "ob", "des", "Glan\u00b7zes", "von", "tau\u00b7send", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "APPR", "CARD", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Keinerlei R\u00f6slein's bescheidene Pracht.", "tokens": ["Kei\u00b7ner\u00b7lei", "R\u00f6s\u00b7lein's", "be\u00b7schei\u00b7de\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "O Mensch, Du trittst mit F\u00fc\u00dfen tausend Wunder,", "tokens": ["O", "Mensch", ",", "Du", "trittst", "mit", "F\u00fc\u00b7\u00dfen", "tau\u00b7send", "Wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tausend Wunder, sie umgeben Dich.", "tokens": ["Und", "tau\u00b7send", "Wun\u00b7der", ",", "sie", "um\u00b7ge\u00b7ben", "Dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "PPER", "VVPP", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und tausend Wunder in den L\u00fcften fliegen,", "tokens": ["Und", "tau\u00b7send", "Wun\u00b7der", "in", "den", "L\u00fcf\u00b7ten", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Mensch, und Du beklagest Dich?", "tokens": ["O", "Mensch", ",", "und", "Du", "be\u00b7kla\u00b7gest", "Dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Knie nieder in dem weiten Welten-Raume,", "tokens": ["Knie", "nie\u00b7der", "in", "dem", "wei\u00b7ten", "Wel\u00b7ten\u00b7Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's Tag, so knie im gold'nen Sonnenschein,", "tokens": ["Ist's", "Tag", ",", "so", "knie", "im", "gold'\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist's Nacht, hoch \u00fcber Dir die Sterne leuchten,", "tokens": ["Ist's", "Nacht", ",", "hoch", "\u00fc\u00b7ber", "Dir", "die", "Ster\u00b7ne", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Dein Gebet sei Dank allein!", "tokens": ["Und", "Dein", "Ge\u00b7bet", "sei", "Dank", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}