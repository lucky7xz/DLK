{"textgrid.poem.40755": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Elbe flie\u00dft so still, so glatt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Elbe flie\u00dft so still, so glatt,", "tokens": ["Die", "El\u00b7be", "flie\u00dft", "so", "still", ",", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne scheint so helle,", "tokens": ["Die", "Son\u00b7ne", "scheint", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein L\u00fcftchen weht, es bebt kein Blatt,", "tokens": ["Kein", "L\u00fcft\u00b7chen", "weht", ",", "es", "bebt", "kein", "Blatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es regt sich keine Welle.", "tokens": ["Es", "regt", "sich", "kei\u00b7ne", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So liegt das Land seit Wochen schier", "tokens": ["So", "liegt", "das", "Land", "seit", "Wo\u00b7chen", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Sonnengluth und Ruhe,", "tokens": ["In", "Son\u00b7nen\u00b7gluth", "und", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch ist's, als l\u00e4g' ein Leichnam hier", "tokens": ["Doch", "ist's", ",", "als", "l\u00e4g'", "ein", "Leich\u00b7nam", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOKOM", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In einer gold'nen Truhe.", "tokens": ["In", "ei\u00b7ner", "gold'\u00b7nen", "Tru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Brunn versiegt, der Strom verrinnt,", "tokens": ["Der", "Brunn", "ver\u00b7siegt", ",", "der", "Strom", "ver\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df seine Spiegel sinken,", "tokens": ["Da\u00df", "sei\u00b7ne", "Spie\u00b7gel", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie das Wasser f\u00e4llt, beginnt", "tokens": ["Doch", "wie", "das", "Was\u00b7ser", "f\u00e4llt", ",", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gestein empor zu blinken,", "tokens": ["Ge\u00b7stein", "em\u00b7por", "zu", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Eiland steigt's, zum Fels versteint,", "tokens": ["Als", "Ei\u00b7land", "steigt's", ",", "zum", "Fels", "ver\u00b7steint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf alte Schrift zu schauen:", "tokens": ["Drauf", "al\u00b7te", "Schrift", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbwer einst mich sah, der hat geweint\u00ab", "tokens": ["\u00bb", "wer", "einst", "mich", "sah", ",", "der", "hat", "ge\u00b7weint", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Solch Wort ist d'rein gehauen.", "tokens": ["Solch", "Wort", "ist", "d'\u00b7rein", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "\u00bbei, hast so kl\u00e4glich du's gemeint,", "tokens": ["\u00bb", "ei", ",", "hast", "so", "kl\u00e4g\u00b7lich", "du's", "ge\u00b7meint", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "ADV", "ADJD", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen's lust'ger machen;", "tokens": ["Wir", "wol\u00b7len's", "lust'\u00b7ger", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer einst dich sah, der hat geweint,", "tokens": ["Wer", "einst", "dich", "sah", ",", "der", "hat", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sehn dich jetzt und lachen!", "tokens": ["Wir", "sehn", "dich", "jetzt", "und", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Versiegt der Brunn, so quillt doch frisch", "tokens": ["Ver\u00b7siegt", "der", "Brunn", ",", "so", "quillt", "doch", "frisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein edles Na\u00df im Keller;", "tokens": ["Ein", "ed\u00b7les", "Na\u00df", "im", "Kel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du Felsen trag' als Freudentisch", "tokens": ["Du", "Fel\u00b7sen", "trag'", "als", "Freu\u00b7den\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir Flaschenkorb und Teller!\u00ab", "tokens": ["Mir", "Fla\u00b7schen\u00b7korb", "und", "Tel\u00b7ler", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Landesf\u00fcrst im Uebermuth", "tokens": ["Der", "Lan\u00b7des\u00b7f\u00fcrst", "im", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sprach's und rief zum Feste;", "tokens": ["Er", "sprach's", "und", "rief", "zum", "Fes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Felsen mitten in der Fluth", "tokens": ["Zum", "Fel\u00b7sen", "mit\u00b7ten", "in", "der", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiegt schon sein Kahn die G\u00e4ste.", "tokens": ["Wiegt", "schon", "sein", "Kahn", "die", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Becher sch\u00e4umt, die Sch\u00fcssel dampft,", "tokens": ["Der", "Be\u00b7cher", "sch\u00e4umt", ",", "die", "Sch\u00fcs\u00b7sel", "dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Musik ert\u00f6nt im Runde,", "tokens": ["Mu\u00b7sik", "er\u00b7t\u00f6nt", "im", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df \u00fcpp'ger Tanz den Boden stampft", "tokens": ["Da\u00df", "\u00fcpp'\u00b7ger", "Tanz", "den", "Bo\u00b7den", "stampft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wohl bis zur Morgenstunde.", "tokens": ["Wohl", "bis", "zur", "Mor\u00b7gen\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Noch fiel der Strom, f\u00e4llt fort und fort", "tokens": ["Noch", "fiel", "der", "Strom", ",", "f\u00e4llt", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fels w\u00e4chst mittlerweile,", "tokens": ["Der", "Fels", "w\u00e4chst", "mitt\u00b7ler\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sichtbar unter jenem Wort", "tokens": ["Und", "sicht\u00b7bar", "un\u00b7ter", "je\u00b7nem", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird eine zweite Zeile.", "tokens": ["Wird", "ei\u00b7ne", "zwei\u00b7te", "Zei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Schaar, zur Heimfahrt jetzt vereint,", "tokens": ["Die", "Schaar", ",", "zur", "Heim\u00b7fahrt", "jetzt", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag's lesen auf den Steinen:", "tokens": ["Mag's", "le\u00b7sen", "auf", "den", "Stei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbwer einst mich sah, der hat geweint,", "tokens": ["\u00bb", "wer", "einst", "mich", "sah", ",", "der", "hat", "ge\u00b7weint", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer jetzt mich sieht, wird weinen.\u00ab", "tokens": ["Wer", "jetzt", "mich", "sieht", ",", "wird", "wei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie lassen an das Ufer sacht", "tokens": ["Sie", "las\u00b7sen", "an", "das", "U\u00b7fer", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den schmucken Nachen gleiten;", "tokens": ["Den", "schmu\u00b7cken", "Na\u00b7chen", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie zum ersten Wort gelacht,", "tokens": ["Wie", "sie", "zum", "ers\u00b7ten", "Wort", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lachen sie zum zweiten:", "tokens": ["So", "la\u00b7chen", "sie", "zum", "zwei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbals Pred'ger kamst du schon zu sp\u00e4t,", "tokens": ["\u00bb", "als", "Pre\u00b7d'\u00b7ger", "kamst", "du", "schon", "zu", "sp\u00e4t", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Spr\u00fcchlein halt' in Ehren;", "tokens": ["Dein", "Spr\u00fcch\u00b7lein", "halt'", "in", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df sehn, ob du dich als Prophet", "tokens": ["La\u00df", "sehn", ",", "ob", "du", "dich", "als", "Pro\u00b7phet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PPER", "PRF", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wohl besser magst bew\u00e4hren.\u00ab", "tokens": ["Wohl", "bes\u00b7ser", "magst", "be\u00b7w\u00e4h\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Strom doch flie\u00dft so still, so glatt,", "tokens": ["Der", "Strom", "doch", "flie\u00dft", "so", "still", ",", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne scheint so helle,", "tokens": ["Die", "Son\u00b7ne", "scheint", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein L\u00fcftchen weht, es bebt kein Blatt,", "tokens": ["Kein", "L\u00fcft\u00b7chen", "weht", ",", "es", "bebt", "kein", "Blatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es regt sich keine Welle. \u2013", "tokens": ["Es", "regt", "sich", "kei\u00b7ne", "Wel\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Solch Stillestehn ist schlimm'rer Sturm,", "tokens": ["Solch", "Stil\u00b7les\u00b7tehn", "ist", "schlim\u00b7m'\u00b7rer", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Solch Ruh'n ist langsam Sterben,", "tokens": ["Solch", "Ruh'n", "ist", "lang\u00b7sam", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Friede wird zum Nagewurm,", "tokens": ["Der", "Frie\u00b7de", "wird", "zum", "Na\u00b7ge\u00b7wurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Glanz wird zum Verderben.", "tokens": ["Der", "Glanz", "wird", "zum", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Sonne liegt, ein Gluthvampyr,", "tokens": ["Die", "Son\u00b7ne", "liegt", ",", "ein", "Gluth\u00b7vam\u00b7pyr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwer auf der Brust der Erde,", "tokens": ["Schwer", "auf", "der", "Brust", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Saugt ihrer Str\u00f6me Blut mit Gier,", "tokens": ["Saugt", "ih\u00b7rer", "Str\u00f6\u00b7me", "Blut", "mit", "Gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlingt ihr Saat und Heerde;", "tokens": ["Ver\u00b7schlingt", "ihr", "Saat", "und", "Heer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Hochwald sieht in K\u00fcmmerni\u00df", "tokens": ["Der", "Hoch\u00b7wald", "sieht", "in", "K\u00fcm\u00b7mer\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Haupt die Locken fallen,", "tokens": ["Vom", "Haupt", "die", "Lo\u00b7cken", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Trift zerbarst, als sei's ein Ri\u00df", "tokens": ["Die", "Trift", "zer\u00b7barst", ",", "als", "sei's", "ein", "Ri\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von jenen Feuerkrallen.", "tokens": ["Von", "je\u00b7nen", "Feu\u00b7er\u00b7kral\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gerippen gleich starrt Busch und Dorn,", "tokens": ["Ge\u00b7rip\u00b7pen", "gleich", "starrt", "Busch", "und", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den keine Regen streiften;", "tokens": ["Den", "kei\u00b7ne", "Re\u00b7gen", "streif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Baum die Frucht, vom Halm das Korn,", "tokens": ["Vom", "Baum", "die", "Frucht", ",", "vom", "Halm", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fallen, eh' sie reiften.", "tokens": ["Sie", "fal\u00b7len", ",", "eh'", "sie", "reif\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Hunger zieht durch Stadt und Land", "tokens": ["Der", "Hun\u00b7ger", "zieht", "durch", "Stadt", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sein Gefolg', die Seuchen,", "tokens": ["Und", "sein", "Ge\u00b7fol\u00b7g'", ",", "die", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da\u00df durch die Fluren kahlgebrannt", "tokens": ["Da\u00df", "durch", "die", "Flu\u00b7ren", "kahl\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Noth und Jammer schleichen.", "tokens": ["Nur", "Noth", "und", "Jam\u00b7mer", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gedeihn nur will ein einzig Na\u00df", "tokens": ["Ge\u00b7deihn", "nur", "will", "ein", "ein\u00b7zig", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am S\u00fcdhang in den Reben,", "tokens": ["Am", "S\u00fcd\u00b7hang", "in", "den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wird ein b\u00f6ser Tropfen das,", "tokens": ["Doch", "wird", "ein", "b\u00f6\u00b7ser", "Trop\u00b7fen", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird Gift statt Labung geben;", "tokens": ["Wird", "Gift", "statt", "La\u00b7bung", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das grimme Feuer, das ihn kocht,", "tokens": ["Das", "grim\u00b7me", "Feu\u00b7er", ",", "das", "ihn", "kocht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flie\u00dft in die Menschenader;", "tokens": ["Flie\u00dft", "in", "die", "Men\u00b7sche\u00b7na\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Hunger noch als Tollwuth pocht,", "tokens": ["Da\u00df", "Hun\u00b7ger", "noch", "als", "Toll\u00b7wuth", "pocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df Zorn entbrennt und Hader.", "tokens": ["Da\u00df", "Zorn", "ent\u00b7brennt", "und", "Ha\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Aufruhr st\u00fcrmt ans F\u00fcrstenthor;", "tokens": ["Der", "Auf\u00b7ruhr", "st\u00fcrmt", "ans", "F\u00fcrs\u00b7ten\u00b7thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar wei\u00df der Held zu siegen,", "tokens": ["Zwar", "wei\u00df", "der", "Held", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch will ein and'rer dunkler Flor", "tokens": ["Doch", "will", "ein", "an\u00b7d'\u00b7rer", "dunk\u00b7ler", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm nicht vom Auge fliegen:", "tokens": ["Ihm", "nicht", "vom", "Au\u00b7ge", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Volkes Elend unerreicht,", "tokens": ["Des", "Vol\u00b7kes", "E\u00b7lend", "un\u00b7er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo einst so reicher Segen!", "tokens": ["Wo", "einst", "so", "rei\u00b7cher", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird des F\u00fcrsten Auge feucht,", "tokens": ["Da", "wird", "des", "F\u00fcrs\u00b7ten", "Au\u00b7ge", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das war der erste Regen.", "tokens": ["Das", "war", "der", "ers\u00b7te", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wohl folgt dem auch der and're nach,", "tokens": ["Wohl", "folgt", "dem", "auch", "der", "an\u00b7d'\u00b7re", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sanft thauend aus der Wolke;", "tokens": ["Sanft", "thau\u00b7end", "aus", "der", "Wol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Es gr\u00fcnt der Wald, es rauscht der Bach,", "tokens": ["Es", "gr\u00fcnt", "der", "Wald", ",", "es", "rauscht", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gl\u00fcck erbl\u00fcht im Volke.", "tokens": ["Und", "Gl\u00fcck", "er\u00b7bl\u00fcht", "im", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch ob die Wasser Schrift und Stein", "tokens": ["Doch", "ob", "die", "Was\u00b7ser", "Schrift", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4ngst \u00fcberquollen haben,", "tokens": ["L\u00e4ngst", "\u00fc\u00b7berq\u00b7uol\u00b7len", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Felsenwort blieb fest und rein", "tokens": ["Das", "Fel\u00b7sen\u00b7wort", "blieb", "fest", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins F\u00fcrstenherz gegraben.", "tokens": ["Ins", "F\u00fcrs\u00b7ten\u00b7herz", "ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und flie\u00dft so glatt der Zeiten Fluth,", "tokens": ["Und", "flie\u00dft", "so", "glatt", "der", "Zei\u00b7ten", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So still als ob sie schliefe,", "tokens": ["So", "still", "als", "ob", "sie", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wei\u00df er: das Verh\u00e4ngni\u00df ruht", "tokens": ["Doch", "wei\u00df", "er", ":", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner dunklen Tiefe.", "tokens": ["In", "sei\u00b7ner", "dunk\u00b7len", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weh, wenn die Zeichen, die er meint,", "tokens": ["Weh", ",", "wenn", "die", "Zei\u00b7chen", ",", "die", "er", "meint", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Licht des Tags erscheinen!", "tokens": ["Am", "Licht", "des", "Tags", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer sie schon sah, der hat geweint,", "tokens": ["Wer", "sie", "schon", "sah", ",", "der", "hat", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer einst sie sieht, wird weinen.", "tokens": ["Wer", "einst", "sie", "sieht", ",", "wird", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}