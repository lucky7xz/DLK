{"textgrid.poem.28342": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Fried ist, wie man sagt, ietzunder in der Feder;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fried ist, wie man sagt, ietzunder in der Feder;", "tokens": ["Der", "Fried", "ist", ",", "wie", "man", "sagt", ",", "ietz\u00b7un\u00b7der", "in", "der", "Fe\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Krieg liegt aber noch dem Bauer auff dem Leder.", "tokens": ["Der", "Krieg", "liegt", "a\u00b7ber", "noch", "dem", "Bau\u00b7er", "auff", "dem", "Le\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Ohr wei\u00df nur vom Fried und sonst kein einig Sinn;", "tokens": ["Das", "Ohr", "wei\u00df", "nur", "vom", "Fried", "und", "sonst", "kein", "ei\u00b7nig", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "KON", "ADV", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df f\u00fchlen nichts davon, so ist es weit noch hin.", "tokens": ["Wei\u00df", "f\u00fch\u00b7len", "nichts", "da\u00b7von", ",", "so", "ist", "es", "weit", "noch", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "PAV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}