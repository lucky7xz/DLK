{"dta.poem.6940": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wofern man auch vermeinet:", "tokens": ["Al\u00b7lein", ",", "wo\u00b7fern", "man", "auch", "ver\u00b7mei\u00b7net", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es sey die Seele c\u00f6rperlich,", "tokens": ["Es", "sey", "die", "See\u00b7le", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blos darum, weil sie sich", "tokens": ["Blos", "da\u00b7rum", ",", "weil", "sie", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PAV", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem C\u00f6rper fest vereinet,", "tokens": ["Mit", "ei\u00b7nem", "C\u00f6r\u00b7per", "fest", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und weil man den Zusammenklang", "tokens": ["Und", "weil", "man", "den", "Zu\u00b7sam\u00b7men\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Der so geheim, und den Zusammenhang", "tokens": ["Der", "so", "ge\u00b7heim", ",", "und", "den", "Zu\u00b7sam\u00b7men\u00b7hang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der unbegreifflich ist, wodurch die Seele scheinet,", "tokens": ["Der", "un\u00b7be\u00b7greif\u00b7flich", "ist", ",", "wo\u00b7durch", "die", "See\u00b7le", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob sie gleich blos ein Geist, die Massa zu bewegen,", "tokens": ["Ob", "sie", "gleich", "blos", "ein", "Geist", ",", "die", "Mas\u00b7sa", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und was f\u00fcr B\u00e4nde sie verbinden", "tokens": ["Und", "was", "f\u00fcr", "B\u00e4n\u00b7de", "sie", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht wissen, nicht geschickt seyn zu ergr\u00fcnden;", "tokens": ["Nicht", "wis\u00b7sen", ",", "nicht", "ge\u00b7schickt", "seyn", "zu", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVPP", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dies sag ich ist, mit Epicurus wollen,", "tokens": ["Dies", "sag", "ich", "ist", ",", "mit", "E\u00b7pi\u00b7cu\u00b7rus", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VAFIN", "$,", "APPR", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df wir den hellsten Glantz des Geists verdunckeln sollen.", "tokens": ["Da\u00df", "wir", "den", "hells\u00b7ten", "Glantz", "des", "Geists", "ver\u00b7dun\u00b7ckeln", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}