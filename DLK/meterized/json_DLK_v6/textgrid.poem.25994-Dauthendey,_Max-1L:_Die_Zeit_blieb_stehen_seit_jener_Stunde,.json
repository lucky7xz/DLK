{"textgrid.poem.25994": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zeit blieb stehen seit jener Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit blieb stehen seit jener Stunde,", "tokens": ["Die", "Zeit", "blieb", "ste\u00b7hen", "seit", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Zeiger r\u00fcckte, keine Sekunde geschah,", "tokens": ["Kein", "Zei\u00b7ger", "r\u00fcck\u00b7te", ",", "kei\u00b7ne", "Se\u00b7kun\u00b7de", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Zeit blieb stehen, seit ich dich sah.", "tokens": ["Die", "Zeit", "blieb", "ste\u00b7hen", ",", "seit", "ich", "dich", "sah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mein Blut entfloh mir, da ging ich in Nacht,", "tokens": ["Mein", "Blut", "ent\u00b7floh", "mir", ",", "da", "ging", "ich", "in", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ging sacht meinem Blut nach,", "tokens": ["Ging", "sacht", "mei\u00b7nem", "Blut", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schwer fand ich mich wieder", "tokens": ["Schwer", "fand", "ich", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ADV"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Und seltsam des Mannes Geschick,", "tokens": ["Und", "selt\u00b7sam", "des", "Man\u00b7nes", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Ein Blick von dir knickt ihm die Glieder.", "tokens": ["Ein", "Blick", "von", "dir", "knickt", "ihm", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es flie\u00dft dunkel ein Laut: dein Blut und mein Blut.", "tokens": ["Es", "flie\u00dft", "dun\u00b7kel", "ein", "Laut", ":", "dein", "Blut", "und", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "APPR", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich lausche und f\u00fchle schwer, umher ist alles gro\u00df und gut.", "tokens": ["Ich", "lau\u00b7sche", "und", "f\u00fch\u00b7le", "schwer", ",", "um\u00b7her", "ist", "al\u00b7les", "gro\u00df", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,", "PTKVZ", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}