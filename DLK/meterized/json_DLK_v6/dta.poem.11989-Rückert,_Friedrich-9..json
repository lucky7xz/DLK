{"dta.poem.11989": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft durch ein Ungl\u00fcck wird ein gro\u00dfes Gl\u00fcck zu Theil,", "tokens": ["Oft", "durch", "ein", "Un\u00b7gl\u00fcck", "wird", "ein", "gro\u00b7\u00dfes", "Gl\u00fcck", "zu", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein das Ungl\u00fcck selbst wird durch das Gl\u00fcck nicht heil.", "tokens": ["Al\u00b7lein", "das", "Un\u00b7gl\u00fcck", "selbst", "wird", "durch", "das", "Gl\u00fcck", "nicht", "heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie jenes Bauern Kuh das Bein beim Ackern brach", "tokens": ["Wie", "je\u00b7nes", "Bau\u00b7ern", "Kuh", "das", "Bein", "beim", "A\u00b7ckern", "brach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "NN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Loch, darin er fand den Schatz hernach.", "tokens": ["In", "ei\u00b7nem", "Loch", ",", "da\u00b7rin", "er", "fand", "den", "Schatz", "her\u00b7nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da sprach die Kuh im Loch: An ist f\u00fcr dich gebrochen", "tokens": ["Da", "sprach", "die", "Kuh", "im", "Loch", ":", "An", "ist", "f\u00fcr", "dich", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "APPR", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tag des Gl\u00fcckes, doch mein Bein hab' ich gebrochen.", "tokens": ["Der", "Tag", "des", "Gl\u00fc\u00b7ckes", ",", "doch", "mein", "Bein", "hab'", "ich", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}