{"textgrid.poem.40882": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Schweiz", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was treibt euch wohl, ihr F\u00fcrsten, stets in die Schweizergaun?", "tokens": ["Was", "treibt", "euch", "wohl", ",", "ihr", "F\u00fcrs\u00b7ten", ",", "stets", "in", "die", "Schwei\u00b7zer\u00b7gaun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wollt einmal doch im Leben ein freies Land ihr schaun?", "tokens": ["Wollt", "ein\u00b7mal", "doch", "im", "Le\u00b7ben", "ein", "frei\u00b7es", "Land", "ihr", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wollt ihr das Zepter tauschen um einen Hirtenstab?", "tokens": ["Wollt", "ihr", "das", "Zep\u00b7ter", "tau\u00b7schen", "um", "ei\u00b7nen", "Hir\u00b7ten\u00b7stab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ha, oder wollt ihr finden in freier Erd' ein Grab?", "tokens": ["Ha", ",", "o\u00b7der", "wollt", "ihr", "fin\u00b7den", "in", "frei\u00b7er", "Erd'", "ein", "Grab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Seht auf das Land hernieder von hoher Alpenwand!", "tokens": ["Seht", "auf", "das", "Land", "her\u00b7nie\u00b7der", "von", "ho\u00b7her", "Al\u00b7pen\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da liegt's, gleich einem Buche, geschrieben von Gotteshand,", "tokens": ["Da", "liegt's", ",", "gleich", "ei\u00b7nem", "Bu\u00b7che", ",", "ge\u00b7schrie\u00b7ben", "von", "Got\u00b7tes\u00b7hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ART", "NN", "$,", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Berge sind die Lettern, das Blatt die gr\u00fcne Trift,", "tokens": ["Die", "Ber\u00b7ge", "sind", "die", "Let\u00b7tern", ",", "das", "Blatt", "die", "gr\u00fc\u00b7ne", "Trift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sankt Gotthard ist ein Punkt nur in dieser Riesenschrift.", "tokens": ["Sankt", "Got\u00b7thard", "ist", "ein", "Punkt", "nur", "in", "die\u00b7ser", "Rie\u00b7sen\u00b7schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VAFIN", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "--+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft ihr, was drin geschrieben? O seht, es strahlt so licht!", "tokens": ["Wi\u00dft", "ihr", ",", "was", "drin", "ge\u00b7schrie\u00b7ben", "?", "O", "seht", ",", "es", "strahlt", "so", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "VVPP", "$.", "NE", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es schrieb sie ja kein Kanzler, es ist kein Pergament,", "tokens": ["Es", "schrieb", "sie", "ja", "kein", "Kanz\u00b7ler", ",", "es", "ist", "kein", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drauf eines Volkes Herzblut als rothes Siegel brennt.", "tokens": ["Drauf", "ei\u00b7nes", "Vol\u00b7kes", "Herz\u00b7blut", "als", "ro\u00b7thes", "Sie\u00b7gel", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Seht dort den m\u00e4cht'gen Felsberg, der ", "tokens": ["Seht", "dort", "den", "m\u00e4cht'\u00b7gen", "Fels\u00b7berg", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der freie Aar umkreist ihm der kahlen Stirne Rand,", "tokens": ["Der", "frei\u00b7e", "Aar", "um\u00b7kreist", "ihm", "der", "kah\u00b7len", "Stir\u00b7ne", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Fels ist die graue Kutte, Schnee seiner Scheitel Zier,", "tokens": ["Fels", "ist", "die", "grau\u00b7e", "Kut\u00b7te", ",", "Schnee", "sei\u00b7ner", "Schei\u00b7tel", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Das Weltall seine Zelle, das Sternzelt sein Brevier.", "tokens": ["Das", "Wel\u00b7tall", "sei\u00b7ne", "Zel\u00b7le", ",", "das", "Stern\u00b7zelt", "sein", "Bre\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ist wo ein M\u00f6nch, bleibt sicher die Predigt auch nicht aus.", "tokens": ["Ist", "wo", "ein", "M\u00f6nch", ",", "bleibt", "si\u00b7cher", "die", "Pre\u00b7digt", "auch", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "ART", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der spricht im Lavinendonner, im rauschenden Quellengebraus;", "tokens": ["Der", "spricht", "im", "La\u00b7vi\u00b7nen\u00b7don\u00b7ner", ",", "im", "rau\u00b7schen\u00b7den", "Quel\u00b7len\u00b7ge\u00b7braus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Pater ist ein Ketzer, Zeit w\u00e4r's ihn einzusperrn!", "tokens": ["Der", "Pa\u00b7ter", "ist", "ein", "Ket\u00b7zer", ",", "Zeit", "w\u00e4r's", "ihn", "ein\u00b7zu\u00b7sperrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Seht dort im wei\u00dfen Schleier aufragt der ", "tokens": ["Seht", "dort", "im", "wei\u00b7\u00dfen", "Schlei\u00b7er", "auf\u00b7ragt", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Br\u00e4ut'gam hat ihr der Morgen mit Rosen die Stirn umlaubt,", "tokens": ["Als", "Br\u00e4ut'\u00b7gam", "hat", "ihr", "der", "Mor\u00b7gen", "mit", "Ro\u00b7sen", "die", "Stirn", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie hat mit bunten Blumen gestickt das gr\u00fcne Gewand,", "tokens": ["Sie", "hat", "mit", "bun\u00b7ten", "Blu\u00b7men", "ge\u00b7stickt", "das", "gr\u00fc\u00b7ne", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dran spielen rauschende Quellen, ein flatternd Silberband.", "tokens": ["Dran", "spie\u00b7len", "rau\u00b7schen\u00b7de", "Quel\u00b7len", ",", "ein", "flat\u00b7ternd", "Sil\u00b7ber\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Ob ihr w\u00f6lbt sich zur Kuppel der L\u00fcfte blauer Strom,", "tokens": ["Ob", "ihr", "w\u00f6lbt", "sich", "zur", "Kup\u00b7pel", "der", "L\u00fcf\u00b7te", "blau\u00b7er", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der spitzen Gletscher Reihe rings scheint die Orgel im Dom;", "tokens": ["Der", "spit\u00b7zen", "Glet\u00b7scher", "Rei\u00b7he", "rings", "scheint", "die", "Or\u00b7gel", "im", "Dom", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fcrwahr, mich d\u00e4ucht, wo Jungfrau und Orgel zusammenkam,", "tokens": ["F\u00fcr\u00b7wahr", ",", "mich", "d\u00e4ucht", ",", "wo", "Jung\u00b7frau", "und", "Or\u00b7gel", "zu\u00b7sam\u00b7men\u00b7kam", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Blieb da Musik und Sang aus, das w\u00e4re wundersam.", "tokens": ["Blieb", "da", "Mu\u00b7sik", "und", "Sang", "aus", ",", "das", "w\u00e4\u00b7re", "wun\u00b7der\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Horch, wie ihr Lied an Herzen so herrlich, kr\u00e4ftig pocht!", "tokens": ["Horch", ",", "wie", "ihr", "Lied", "an", "Her\u00b7zen", "so", "herr\u00b7lich", ",", "kr\u00e4f\u00b7tig", "pocht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "ADV", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Beim Himmel, niemals sangen der Erde T\u00f6chter so sch\u00f6n,", "tokens": ["Beim", "Him\u00b7mel", ",", "nie\u00b7mals", "san\u00b7gen", "der", "Er\u00b7de", "T\u00f6ch\u00b7ter", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mitsingen wohl Gottes Engel in Ch\u00f6ren auf den H\u00f6hn!", "tokens": ["Mit\u00b7sin\u00b7gen", "wohl", "Got\u00b7tes", "En\u00b7gel", "in", "Ch\u00f6\u00b7ren", "auf", "den", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "NE", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ihr Herrn, will's euch nicht munden? Ihr h\u00f6rt wohl keinen Klang,", "tokens": ["Ihr", "Herrn", ",", "will's", "euch", "nicht", "mun\u00b7den", "?", "Ihr", "h\u00f6rt", "wohl", "kei\u00b7nen", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil kein Kastrat, kein S\u00e4bel euch's um die Ohren sang,", "tokens": ["Weil", "kein", "Kast\u00b7rat", ",", "kein", "S\u00e4\u00b7bel", "euch's", "um", "die", "Oh\u00b7ren", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PIAT", "NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Im Schweizerland doch liest man gern jenes Riesenbuch", "tokens": ["Im", "Schwei\u00b7zer\u00b7land", "doch", "liest", "man", "gern", "je\u00b7nes", "Rie\u00b7sen\u00b7buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PIS", "ADV", "PDAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und horcht dem Lied der Jungfrau und merkt des Pred'gers Spruch.", "tokens": ["Und", "horcht", "dem", "Lied", "der", "Jung\u00b7frau", "und", "merkt", "des", "Pre\u00b7d'\u00b7gers", "Spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Im Schweizerland da springen die Quellen frei empor,", "tokens": ["Im", "Schwei\u00b7zer\u00b7land", "da", "sprin\u00b7gen", "die", "Quel\u00b7len", "frei", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Frei schweben die segelnden Wolken und singender V\u00f6gel Chor,", "tokens": ["Frei", "schwe\u00b7ben", "die", "se\u00b7geln\u00b7den", "Wol\u00b7ken", "und", "sin\u00b7gen\u00b7der", "V\u00f6\u00b7gel", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Frei blickt vom Firn die Gemse auf krachende Wetter herab,", "tokens": ["Frei", "blickt", "vom", "Firn", "die", "Gem\u00b7se", "auf", "kra\u00b7chen\u00b7de", "Wet\u00b7ter", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und freie Weste fl\u00fcstern um freier Helden Grab.", "tokens": ["Und", "frei\u00b7e", "Wes\u00b7te", "fl\u00fcs\u00b7tern", "um", "frei\u00b7er", "Hel\u00b7den", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Viel tausend Schweizer stehen auf hoher Alpenwand,", "tokens": ["Viel", "tau\u00b7send", "Schwei\u00b7zer", "ste\u00b7hen", "auf", "ho\u00b7her", "Al\u00b7pen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie schaun ins Land hernieder und dr\u00fccken Hand in Hand", "tokens": ["Sie", "schaun", "ins", "Land", "her\u00b7nie\