{"textgrid.poem.23885": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der melancholische Narr", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einer jungen Linde hab", "tokens": ["Aus", "ei\u00b7ner", "jun\u00b7gen", "Lin\u00b7de", "hab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschnitzt ich meinen Narrenstab;", "tokens": ["Ge\u00b7schnitzt", "ich", "mei\u00b7nen", "Nar\u00b7ren\u00b7stab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein eigener Sch\u00e4del wackelt drauf", "tokens": ["Mein", "ei\u00b7ge\u00b7ner", "Sch\u00e4\u00b7del", "wa\u00b7ckelt", "drauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwischen Schellen und B\u00e4ndern als bunter Knauf.", "tokens": ["Zwi\u00b7schen", "Schel\u00b7len", "und", "B\u00e4n\u00b7dern", "als", "bun\u00b7ter", "Knauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "KOKOM", "ADJD", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Lacht er?", "tokens": ["Lacht", "er", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "K\u00fc\u00df mich, k\u00fc\u00df mich, Klingelstock mein,", "tokens": ["K\u00fc\u00df", "mich", ",", "k\u00fc\u00df", "mich", ",", "Klin\u00b7gel\u00b7stock", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sei mein Lieb, und ich bin Dein.", "tokens": ["Sei", "mein", "Lieb", ",", "und", "ich", "bin", "Dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich armer Narre!", "tokens": ["Ach", ",", "ich", "ar\u00b7mer", "Nar\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Pst, pst, der Junker Lenz ist drau\u00df',", "tokens": ["Pst", ",", "pst", ",", "der", "Jun\u00b7ker", "Lenz", "ist", "drau\u00df'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Welt sieht bl\u00fchsam aus.", "tokens": ["Die", "gan\u00b7ze", "Welt", "sieht", "bl\u00fch\u00b7sam", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, Schellensch\u00e4del, r\u00fchr dich, sag:", "tokens": ["Du", ",", "Schel\u00b7len\u00b7sch\u00e4\u00b7del", ",", "r\u00fchr", "dich", ",", "sag", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lacht er uns auch, der Fr\u00fchlingstag?", "tokens": ["Lacht", "er", "uns", "auch", ",", "der", "Fr\u00fch\u00b7lings\u00b7tag", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sch\u00fcttelt.", "tokens": ["Er", "sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "K\u00fc\u00df mich, k\u00fc\u00df mich, Klingelstock mein,", "tokens": ["K\u00fc\u00df", "mich", ",", "k\u00fc\u00df", "mich", ",", "Klin\u00b7gel\u00b7stock", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sei mein Lieb, und ich bin dein.", "tokens": ["Sei", "mein", "Lieb", ",", "und", "ich", "bin", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, ich armer Narre!", "tokens": ["Ach", ",", "ich", "ar\u00b7mer", "Nar\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Weg! Alle Fenster dichte zu!", "tokens": ["Weg", "!", "Al\u00b7le", "Fens\u00b7ter", "dich\u00b7te", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir zwei alleine, ich und du,", "tokens": ["Wir", "zwei", "al\u00b7lei\u00b7ne", ",", "ich", "und", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADV", "$,", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wissen doch das Gl\u00fcck gewi\u00df;", "tokens": ["Wir", "wis\u00b7sen", "doch", "das", "Gl\u00fcck", "ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, gl\u00f6ckle in der Finsternis", "tokens": ["Du", ",", "gl\u00f6ck\u00b7le", "in", "der", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und grinse!", "tokens": ["Und", "grin\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "K\u00fc\u00df mich, k\u00fc\u00df mich, Klingelstock mein,", "tokens": ["K\u00fc\u00df", "mich", ",", "k\u00fc\u00df", "mich", ",", "Klin\u00b7gel\u00b7stock", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sei mein Lieb, und ich bin dein.", "tokens": ["Sei", "mein", "Lieb", ",", "und", "ich", "bin", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, ich armer Narre!", "tokens": ["Ach", ",", "ich", "ar\u00b7mer", "Nar\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}