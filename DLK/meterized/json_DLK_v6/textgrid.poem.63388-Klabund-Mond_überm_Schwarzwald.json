{"textgrid.poem.63388": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Mond \u00fcberm Schwarzwald", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Sichel des Monds!", "tokens": ["Gold\u00b7ne", "Si\u00b7chel", "des", "Monds", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dich schwingt der ewige Schnitter", "tokens": ["Dich", "schwingt", "der", "e\u00b7wi\u00b7ge", "Schnit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und m\u00e4ht Halme und Herzen.", "tokens": ["Und", "m\u00e4ht", "Hal\u00b7me", "und", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe, ich wandre auf steinichter H\u00f6he", "tokens": ["Sie\u00b7he", ",", "ich", "wand\u00b7re", "auf", "stei\u00b7nich\u00b7ter", "H\u00f6\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00dcber dem wolkigen Wald und neige", "tokens": ["\u00dc\u00b7ber", "dem", "wol\u00b7ki\u00b7gen", "Wald", "und", "nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Willig den Nacken", "tokens": ["Wil\u00b7lig", "den", "Na\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deinem erl\u00f6senden Streich.", "tokens": ["Dei\u00b7nem", "er\u00b7l\u00f6\u00b7sen\u00b7den", "Streich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}