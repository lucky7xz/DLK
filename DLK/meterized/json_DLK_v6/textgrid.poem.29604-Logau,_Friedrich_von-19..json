{"textgrid.poem.29604": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scotus ist ein guter Artzt; wer sich sehnt hinauff zu ziehn", "tokens": ["Sco\u00b7tus", "ist", "ein", "gu\u00b7ter", "Artzt", ";", "wer", "sich", "sehnt", "hin\u00b7auff", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PRF", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der Noth zu kummen ab, dieser schickt und ruffet ihn.", "tokens": ["Und", "der", "Noth", "zu", "kum\u00b7men", "ab", ",", "die\u00b7ser", "schickt", "und", "ruf\u00b7fet", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$,", "PDS", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}