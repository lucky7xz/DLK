{"textgrid.poem.57112": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur eine Seite deiner teuren Werke \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Seite deiner teuren Werke \u2013", "tokens": ["Nur", "ei\u00b7ne", "Sei\u00b7te", "dei\u00b7ner", "teu\u00b7ren", "Wer\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und sch\u00f6ner wird mein Wesen wie von Licht.", "tokens": ["und", "sch\u00f6\u00b7ner", "wird", "mein", "We\u00b7sen", "wie", "von", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du strahlst mich an. Wo blieb die eigne St\u00e4rke,", "tokens": ["Du", "strahlst", "mich", "an", ".", "Wo", "blieb", "die", "eig\u00b7ne", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du, mir zugleich Erf\u00fcllung und Gericht?", "tokens": ["Du", ",", "mir", "zu\u00b7gleich", "Er\u00b7f\u00fcl\u00b7lung", "und", "Ge\u00b7richt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kann der Lebende vor Dir bestehen?", "tokens": ["Wie", "kann", "der", "Le\u00b7ben\u00b7de", "vor", "Dir", "be\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und \u00fcber Wolken wandelt Antwort her:", "tokens": ["Und", "\u00fc\u00b7ber", "Wol\u00b7ken", "wan\u00b7delt", "Ant\u00b7wort", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist von denen, die doch immer ", "tokens": ["Du", "bist", "von", "de\u00b7nen", ",", "die", "doch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PRELS", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geh weiter denn, kein Sterblicher kann mehr.", "tokens": ["Geh", "wei\u00b7ter", "denn", ",", "kein", "Sterb\u00b7li\u00b7cher", "kann", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PIAT", "NN", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}