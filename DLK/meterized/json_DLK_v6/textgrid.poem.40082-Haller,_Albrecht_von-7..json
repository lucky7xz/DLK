{"textgrid.poem.40082": {"metadata": {"author": {"name": "Haller, Albrecht von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1752, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Russlands breites Reich sich mit der Erde schlie\u00dfet", "tokens": ["Wo", "Russ\u00b7lands", "brei\u00b7tes", "Reich", "sich", "mit", "der", "Er\u00b7de", "schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem letzten West des Morgens March zerflie\u00dfet,", "tokens": ["Und", "in", "dem", "letz\u00b7ten", "West", "des", "Mor\u00b7gens", "March", "zer\u00b7flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin kein Vorwitz drang, wo Thiere fremder Art", "tokens": ["Wo\u00b7hin", "kein", "Vor\u00b7witz", "drang", ",", "wo", "Thie\u00b7re", "frem\u00b7der", "Art"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,", "PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch ungenannten V\u00f6lkern dienten,", "tokens": ["Noch", "un\u00b7ge\u00b7nann\u00b7ten", "V\u00f6l\u00b7kern", "dien\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo unbekanntes Erzt sich f\u00fcr die Nachwelt spart", "tokens": ["Wo", "un\u00b7be\u00b7kann\u00b7tes", "Erzt", "sich", "f\u00fcr", "die", "Nach\u00b7welt", "spart"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nie gepfl\u00fcckte Kr\u00e4uter gr\u00fcnten,", "tokens": ["Und", "nie", "ge\u00b7pfl\u00fcck\u00b7te", "Kr\u00e4u\u00b7ter", "gr\u00fcn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lag eine neue Welt, von der Natur versteckt,", "tokens": ["Lag", "ei\u00b7ne", "neu\u00b7e", "Welt", ",", "von", "der", "Na\u00b7tur", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis ", "tokens": ["Bis"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}}}}