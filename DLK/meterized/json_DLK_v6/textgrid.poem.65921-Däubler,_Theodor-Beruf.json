{"textgrid.poem.65921": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Beruf", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Menschen, schwach durch alte Plagen,", "tokens": ["Ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "schwach", "durch", "al\u00b7te", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr seid von Gott entt\u00e4uscht und seiner Welt:", "tokens": ["Ihr", "seid", "von", "Gott", "ent\u00b7t\u00e4uscht", "und", "sei\u00b7ner", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie brauch ich euch und unser Himmelszelt!", "tokens": ["Wie", "brauch", "ich", "euch", "und", "un\u00b7ser", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Weib vernimmt nur des Entbl\u00f6\u00dften Klagen!", "tokens": ["Ein", "Weib", "ver\u00b7nimmt", "nur", "des", "Ent\u00b7bl\u00f6\u00df\u00b7ten", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft \u2013 gesamt \u2013 die Sternenherkunft tragen,", "tokens": ["Du", "mu\u00dft", "\u2013", "ge\u00b7samt", "\u2013", "die", "Ster\u00b7nen\u00b7her\u00b7kunft", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VVPP", "$(", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Mensch, du bist vor M\u00fchsal nie zerschellt:", "tokens": ["O", "Mensch", ",", "du", "bist", "vor", "M\u00fch\u00b7sal", "nie", "zer\u00b7schellt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrs Weib zu sorgen, bleibt dem Mann Entgelt,", "tokens": ["F\u00fcrs", "Weib", "zu", "sor\u00b7gen", ",", "bleibt", "dem", "Mann", "Ent\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Frieden freut ihn nicht, er hofft aufs Wagen!", "tokens": ["Denn", "Frie\u00b7den", "freut", "ihn", "nicht", ",", "er", "hofft", "aufs", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Menschtum fordern heile Leidenschaften.", "tokens": ["Ein", "Menschtum", "for\u00b7dern", "hei\u00b7le", "Lei\u00b7den\u00b7schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was Hingesonnenheit im N\u00e4chsten liebt,", "tokens": ["Was", "Hin\u00b7ge\u00b7son\u00b7nen\u00b7heit", "im", "N\u00e4chs\u00b7ten", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verlangt nach K\u00fcnstlertum, zu wunderhaften", "tokens": ["Ver\u00b7langt", "nach", "K\u00fcnst\u00b7ler\u00b7tum", ",", "zu", "wun\u00b7der\u00b7haf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erbauungen der Seele, die zerstiebt,", "tokens": ["Er\u00b7bau\u00b7un\u00b7gen", "der", "See\u00b7le", ",", "die", "zer\u00b7stiebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Triebe nicht um edle Mahnung haften:", "tokens": ["Wenn", "Trie\u00b7be", "nicht", "um", "ed\u00b7le", "Mah\u00b7nung", "haf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verbleib der Hilflosen, die Pflichten gibt!", "tokens": ["Ver\u00b7bleib", "der", "Hilf\u00b7lo\u00b7sen", ",", "die", "Pflich\u00b7ten", "gibt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}}}}