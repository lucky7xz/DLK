{"textgrid.poem.45111": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Literarische Besoldung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinds auch Brosamen, nur vom Tisch verzettelt,", "tokens": ["Sinds", "auch", "Bro\u00b7sa\u00b7men", ",", "nur", "vom", "Tisch", "ver\u00b7zet\u00b7telt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag Grund und Anla\u00df auf sich selbst beruhn \u2013", "tokens": ["Mag", "Grund", "und", "An\u00b7la\u00df", "auf", "sich", "selbst", "be\u00b7ruhn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "APPR", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da f\u00fcr mich keine Familie bettelt,", "tokens": ["Da", "f\u00fcr", "mich", "kei\u00b7ne", "Fa\u00b7mi\u00b7lie", "bet\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "So mu\u00df ich es schon selber tun.", "tokens": ["So", "mu\u00df", "ich", "es", "schon", "sel\u00b7ber", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}