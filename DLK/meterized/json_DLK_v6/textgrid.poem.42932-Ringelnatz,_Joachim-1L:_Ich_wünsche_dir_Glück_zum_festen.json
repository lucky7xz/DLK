{"textgrid.poem.42932": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich w\u00fcnsche dir Gl\u00fcck zum festen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcnsche dir Gl\u00fcck zum festen", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "Gl\u00fcck", "zum", "fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Boden. \u2013 War das dein erster Flug? \u2013", "tokens": ["Bo\u00b7den", ".", "\u2013", "War", "das", "dein", "ers\u00b7ter", "Flug", "?", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "VAFIN", "PDS", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ich glaube, du f\u00e4hrst am besten", "tokens": ["Ich", "glau\u00b7be", ",", "du", "f\u00e4hrst", "am", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das n\u00e4chstemal mit dem Eisenbahnzug.", "tokens": ["Das", "n\u00e4chs\u00b7te\u00b7mal", "mit", "dem", "Ei\u00b7sen\u00b7bahn\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ganz bleich siehst du aus. Und doch", "tokens": ["Ganz", "bleich", "siehst", "du", "aus", ".", "Und", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bist du so lebhaft und aussprudelnd froh.", "tokens": ["Bist", "du", "so", "leb\u00b7haft", "und", "aus\u00b7spru\u00b7delnd", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warst du unterwegs ebenso?", "tokens": ["Warst", "du", "un\u00b7ter\u00b7wegs", "e\u00b7ben\u00b7so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Du zitterst ja noch.", "tokens": ["Du", "zit\u00b7terst", "ja", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ja, die Luft hat keine Balken.", "tokens": ["Ja", ",", "die", "Luft", "hat", "kei\u00b7ne", "Bal\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber nun genie\u00dfe du das Nun.", "tokens": ["A\u00b7ber", "nun", "ge\u00b7nie\u00b7\u00dfe", "du", "das", "Nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDS", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcber Fl\u00fcgel denken Falken", "tokens": ["\u00dc\u00b7ber", "Fl\u00fc\u00b7gel", "den\u00b7ken", "Fal\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anders wie ein Huhn.", "tokens": ["An\u00b7ders", "wie", "ein", "Huhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Sind dir vier Wolkenstunden", "tokens": ["Sind", "dir", "vier", "Wol\u00b7ken\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu langsam verronnen,", "tokens": ["Zu", "lang\u00b7sam", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hast du die Erde nun wieder gefunden,", "tokens": ["Hast", "du", "die", "Er\u00b7de", "nun", "wie\u00b7der", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie neu liebgewonnen.", "tokens": ["Sie", "neu", "lieb\u00b7ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer oben unzufrieden,", "tokens": ["Wer", "o\u00b7ben", "un\u00b7zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00c4ngstlich oder auch krank war,", "tokens": ["\u00c4ngst\u00b7lich", "o\u00b7der", "auch", "krank", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei dann hienieden", "tokens": ["Sei", "dann", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wenigstens dankbar.", "tokens": ["We\u00b7nigs\u00b7tens", "dank\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+---+", "measure": "dactylic.init"}}}}}