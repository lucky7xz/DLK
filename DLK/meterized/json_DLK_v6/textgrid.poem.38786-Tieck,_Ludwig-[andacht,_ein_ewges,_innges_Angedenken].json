{"textgrid.poem.38786": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[andacht, ein ewges, innges Angedenken]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andacht, ein ewges, innges Angedenken,", "tokens": ["An\u00b7dacht", ",", "ein", "ew\u00b7ges", ",", "inn\u00b7ges", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anfang ist es vom lieblichsten Empfinden,", "tokens": ["An\u00b7fang", "ist", "es", "vom", "lieb\u00b7lichs\u00b7ten", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Aufschweben zu sich, um sich selbst zu finden,", "tokens": ["Auf\u00b7schwe\u00b7ben", "zu", "sich", ",", "um", "sich", "selbst", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$,", "KOUI", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allm\u00e4chtges Streben sich in sich zu senken.", "tokens": ["All\u00b7m\u00e4cht\u00b7ges", "Stre\u00b7ben", "sich", "in", "sich", "zu", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Liebe mu\u00df aus dem luftgen Duft sich lenken,", "tokens": ["Lie\u00b7be", "mu\u00df", "aus", "dem", "luft\u00b7gen", "Duft", "sich", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leben recht lind in Liebe ganz verschwinden,", "tokens": ["Le\u00b7ben", "recht", "lind", "in", "Lie\u00b7be", "ganz", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lichtheilig sich der Leib dem Geist verbinden,", "tokens": ["Licht\u00b7hei\u00b7lig", "sich", "der", "Leib", "dem", "Geist", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leid naht, lebendge Herzen uns zu schenken.", "tokens": ["Leid", "naht", ",", "le\u00b7bend\u00b7ge", "Her\u00b7zen", "uns", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Minne, so sangen, die das H\u00f6chste meinen,", "tokens": ["Min\u00b7ne", ",", "so", "san\u00b7gen", ",", "die", "das", "H\u00f6chs\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mild' innig Sinnen, Lust an Schmerz und Wunde,", "tokens": ["Mild'", "in\u00b7nig", "Sin\u00b7nen", ",", "Lust", "an", "Schmerz", "und", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Myrth' und Cypress' und Rosen sah' ich scheinen,", "tokens": ["Myr\u00b7th'", "und", "Cy\u00b7press'", "und", "Ro\u00b7sen", "sah'", "ich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Anfang und End' im s\u00fc\u00dfgeschm\u00fcckten Bunde,", "tokens": ["An\u00b7fang", "und", "End'", "im", "s\u00fc\u00df\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der reinsten Anmuth Licht, der Minne Allmacht,", "tokens": ["Der", "reins\u00b7ten", "An\u00b7muth", "Licht", ",", "der", "Min\u00b7ne", "All\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Aussprechen konnt' ich nun den Namen Alma.", "tokens": ["Aus\u00b7spre\u00b7chen", "konnt'", "ich", "nun", "den", "Na\u00b7men", "Al\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}