{"dta.poem.12020": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol \u00e4rgern dumpfen Sinn des Geistes Widerspr\u00fcche,", "tokens": ["Wol", "\u00e4r\u00b7gern", "dum\u00b7pfen", "Sinn", "des", "Geis\u00b7tes", "Wi\u00b7der\u00b7spr\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem feinern aber sind sie duft'ge Wohlger\u00fcche.", "tokens": ["Dem", "fei\u00b7nern", "a\u00b7ber", "sind", "sie", "duft'\u00b7ge", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn in der Endlichkeit thut nur durch Widerspruch", "tokens": ["Denn", "in", "der", "End\u00b7lich\u00b7keit", "thut", "nur", "durch", "Wi\u00b7der\u00b7spruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unendlichkeit sich kund, wie Segen in dem Fluch.", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "sich", "kund", ",", "wie", "Se\u00b7gen", "in", "dem", "Fluch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKVZ", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die h\u00f6chsten Dinge, die dein Denken nie kann denken,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Din\u00b7ge", ",", "die", "dein", "Den\u00b7ken", "nie", "kann", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gerad' auf diese mu\u00df sich stets dein Denken lenken.", "tokens": ["Ge\u00b7rad'", "auf", "die\u00b7se", "mu\u00df", "sich", "stets", "dein", "Den\u00b7ken", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDS", "VMFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was du erkennest als unwesenhaften Schein,", "tokens": ["Was", "du", "er\u00b7ken\u00b7nest", "als", "un\u00b7we\u00b7sen\u00b7haf\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekennest du zugleich als wesenhaft allein.", "tokens": ["Be\u00b7ken\u00b7nest", "du", "zu\u00b7gleich", "als", "we\u00b7sen\u00b7haft", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und was als Wirklichkeit dir steht vor allen Sinnen,", "tokens": ["Und", "was", "als", "Wirk\u00b7lich\u00b7keit", "dir", "steht", "vor", "al\u00b7len", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "NN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht in Unwirkliches der h\u00f6chste Sinn zerrinnen.", "tokens": ["Macht", "in", "Un\u00b7wirk\u00b7li\u00b7ches", "der", "h\u00f6chs\u00b7te", "Sinn", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Nur wenn du so zugleich bejahest und verneinest,", "tokens": ["Nur", "wenn", "du", "so", "zu\u00b7gleich", "be\u00b7ja\u00b7hest", "und", "ver\u00b7nei\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlst du, da\u00df im Gem\u00fct du Gott und Welt vereinest.", "tokens": ["F\u00fchlst", "du", ",", "da\u00df", "im", "Ge\u00b7m\u00fct", "du", "Gott", "und", "Welt", "ver\u00b7ei\u00b7nest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}