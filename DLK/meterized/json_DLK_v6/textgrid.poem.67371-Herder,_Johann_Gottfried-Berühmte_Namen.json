{"textgrid.poem.67371": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ber\u00fchmte Namen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4mpfet um Altar und Thronen,", "tokens": ["K\u00e4mp\u00b7fet", "um", "Al\u00b7tar", "und", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpft um Euer Heiligthum;", "tokens": ["K\u00e4mpft", "um", "Eu\u00b7er", "Hei\u00b7lig\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur la\u00dft H\u00fctten ruhig wohnen!", "tokens": ["Nur", "la\u00dft", "H\u00fct\u00b7ten", "ru\u00b7hig", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schonen ist des Kriegers Ruhm.", "tokens": ["Scho\u00b7nen", "ist", "des", "Krie\u00b7gers", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben Hirten viel verbrochen,", "tokens": ["Ha\u00b7ben", "Hir\u00b7ten", "viel", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat die Heerde Schuld daran?", "tokens": ["Hat", "die", "Heer\u00b7de", "Schuld", "da\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Grausam wird an ihr gerochen,", "tokens": ["Grau\u00b7sam", "wird", "an", "ihr", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was sie nicht gethan.", "tokens": ["Was", "sie", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Auf dem Throne herrsch' ein K\u00f6nig,", "tokens": ["Auf", "dem", "Thro\u00b7ne", "herr\u00b7sch'", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der sich selbst regieren kann;", "tokens": ["Der", "sich", "selbst", "re\u00b7gie\u00b7ren", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich hoff', es liegt ihm wenig,", "tokens": ["Und", "ich", "hoff'", ",", "es", "liegt", "ihm", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenig das Betr\u00fcgen an.", "tokens": ["We\u00b7nig", "das", "Be\u00b7tr\u00fc\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vater wird er seinen Kindern,", "tokens": ["Va\u00b7ter", "wird", "er", "sei\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unterdr\u00fcckten Retter sein,", "tokens": ["Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Ret\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird verh\u00fcten, er wird lindern,", "tokens": ["Wird", "ver\u00b7h\u00fc\u00b7ten", ",", "er", "wird", "lin\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn die Seufzer schrein.", "tokens": ["Wenn", "die", "Seuf\u00b7zer", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hellen Dank Euch, Ihr Neronen,", "tokens": ["Hel\u00b7len", "Dank", "Euch", ",", "Ihr", "Ne\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Claudius, Caligula,", "tokens": ["Clau\u00b7di\u00b7us", ",", "Ca\u00b7li\u00b7gu\u00b7la", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df Ihr zeigtet, was auf Thronen,", "tokens": ["Da\u00df", "Ihr", "zeig\u00b7tet", ",", "was", "auf", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem h\u00f6chsten Thron geschah.", "tokens": ["Auf", "dem", "h\u00f6chs\u00b7ten", "Thron", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kinder h\u00f6ren Eure Thaten", "tokens": ["Kin\u00b7der", "h\u00f6\u00b7ren", "Eu\u00b7re", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erz\u00e4hlen fort und fort:", "tokens": ["Und", "er\u00b7z\u00e4h\u00b7len", "fort", "und", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So gedeihen solche Staaten,", "tokens": ["So", "ge\u00b7dei\u00b7hen", "sol\u00b7che", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort und hier und dort.", "tokens": ["Dort", "und", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Heil'ger Tacitus, Du weiser", "tokens": ["Heil'\u00b7ger", "Ta\u00b7ci\u00b7tus", ",", "Du", "wei\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Afterredner, und Sueton,", "tokens": ["Af\u00b7ter\u00b7red\u00b7ner", ",", "und", "Su\u00b7e\u00b7ton", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ewig bl\u00fch' um Eurer Kaiser", "tokens": ["E\u00b7wig", "bl\u00fch'", "um", "Eu\u00b7rer", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Marmorsch\u00e4del ", "tokens": ["Mar\u00b7mor\u00b7sch\u00e4\u00b7del"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Drunten sagen die Tibere,", "tokens": ["Drun\u00b7ten", "sa\u00b7gen", "die", "Ti\u00b7be\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Was sie hier der Welt verhehlt,", "tokens": ["Was", "sie", "hier", "der", "Welt", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und was Ihr zu ew'ger Lehre", "tokens": ["Und", "was", "Ihr", "zu", "ew'\u00b7ger", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Frank und frei erz\u00e4hlt.", "tokens": ["Frank", "und", "frei", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aller Sterngiganten Namen", "tokens": ["Al\u00b7ler", "Stern\u00b7gi\u00b7gan\u00b7ten", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00df' ich ab vom Firmament,", "tokens": ["Rei\u00df'", "ich", "ab", "vom", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streue neuen goldnen Samen,", "tokens": ["Streu\u00b7e", "neu\u00b7en", "gold\u00b7nen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Boot-Orion brennt.", "tokens": ["Wo", "Boot\u00b7O\u00b7ri\u00b7on", "brennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Titus und die Antonine,", "tokens": ["Ti\u00b7tus", "und", "die", "An\u00b7to\u00b7ni\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gustav Adolph und Trajan \u2013", "tokens": ["Gus\u00b7tav", "A\u00b7dolph", "und", "Tra\u00b7jan", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$("], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Schau von Deiner Himmelsb\u00fchne,", "tokens": ["Schau", "von", "Dei\u00b7ner", "Him\u00b7mels\u00b7b\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Maximilian! \u2013 \u2013", "tokens": ["Ma\u00b7xi\u00b7mi\u00b7li\u00b7an", "!", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NE", "$.", "$(", "$("], "meter": "--+-+", "measure": "anapaest.init"}}}}}