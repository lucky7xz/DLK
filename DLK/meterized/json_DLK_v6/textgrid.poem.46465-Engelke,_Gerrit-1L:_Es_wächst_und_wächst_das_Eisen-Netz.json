{"textgrid.poem.46465": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es w\u00e4chst und w\u00e4chst das Eisen-Netz", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4chst und w\u00e4chst das Eisen-Netz", "tokens": ["Es", "w\u00e4chst", "und", "w\u00e4chst", "das", "Ei\u00b7sen\u00b7Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der parallelen Schienen,", "tokens": ["Der", "par\u00b7al\u00b7le\u00b7len", "Schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nach der Entwicklung Mu\u00df-Gesetz", "tokens": ["Nach", "der", "Ent\u00b7wick\u00b7lung", "Mu\u00df\u00b7Ge\u00b7setz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Kommt \u00fcber Br\u00fccken, Minen,", "tokens": ["Kommt", "\u00fc\u00b7ber", "Br\u00fc\u00b7cken", ",", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zug um Zug in Dampf!", "tokens": ["Zug", "um", "Zug", "in", "Dampf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Es pfl\u00fcgen Dampfer mehr und mehr", "tokens": ["Es", "pfl\u00fc\u00b7gen", "Damp\u00b7fer", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flutenwege offen.", "tokens": ["Die", "Flu\u00b7ten\u00b7we\u00b7ge", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Dock-Scho\u00df wird von Kielen leer:", "tokens": ["Der", "Dock\u00b7Scho\u00df", "wird", "von", "Kie\u00b7len", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Vorw\u00e4rts-Drang getroffen", "tokens": ["Vom", "Vor\u00b7w\u00e4rts\u00b7Drang", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt Schiff um Schiff zum Meer!", "tokens": ["Kommt", "Schiff", "um", "Schiff", "zum", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es w\u00e4chst vom Boden Turm um Turm:", "tokens": ["Es", "w\u00e4chst", "vom", "Bo\u00b7den", "Turm", "um", "Turm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fabriken, Haus, Kaserne,", "tokens": ["Fab\u00b7ri\u00b7ken", ",", "Haus", ",", "Ka\u00b7ser\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es w\u00fchlt und w\u00fchlt der Stra\u00dfenwurm", "tokens": ["Es", "w\u00fchlt", "und", "w\u00fchlt", "der", "Stra\u00b7\u00dfen\u00b7wurm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Asphaltweg zur Ferne:", "tokens": ["Den", "As\u00b7phalt\u00b7weg", "zur", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt langsam Stadt zu Stadt!", "tokens": ["Kommt", "lang\u00b7sam", "Stadt", "zu", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Menschenstrom w\u00e4lzt breit und breit,", "tokens": ["Ein", "Men\u00b7schen\u00b7strom", "w\u00e4lzt", "breit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewehrt mit Axt und Pfosten,", "tokens": ["Be\u00b7wehrt", "mit", "Axt", "und", "Pfos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Wasser, Land und Land und Zeit,", "tokens": ["Durch", "Was\u00b7ser", ",", "Land", "und", "Land", "und", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von S\u00fcd nach Nord, von West nach Osten:", "tokens": ["Von", "S\u00fcd", "nach", "Nord", ",", "von", "West", "nach", "Os\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$,", "APPR", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt einend Mensch zu Mensch!", "tokens": ["Kommt", "ei\u00b7nend", "Mensch", "zu", "Mensch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Quell wird Flu\u00df, und Flu\u00df wird Bord:", "tokens": ["Und", "Quell", "wird", "Flu\u00df", ",", "und", "Flu\u00df", "wird", "Bord", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,", "KON", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jede Stunde: Fahrzeit,", "tokens": ["Und", "je\u00b7de", "Stun\u00b7de", ":", "Fahr\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Meer wird Land, und Land wird Ort:", "tokens": ["Und", "Meer", "wird", "Land", ",", "und", "Land", "wird", "Ort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,", "KON", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kind wird Mann und Arbeit:", "tokens": ["Und", "Kind", "wird", "Mann", "und", "Ar\u00b7beit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt Alles und Alles zu Allem!", "tokens": ["Kommt", "Al\u00b7les", "und", "Al\u00b7les", "zu", "Al\u00b7lem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "PIS", "APPR", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}