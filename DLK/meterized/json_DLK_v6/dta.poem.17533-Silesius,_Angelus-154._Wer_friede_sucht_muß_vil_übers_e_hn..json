{"dta.poem.17533": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "154.  Wer friede sucht mu\u00df vil \u00fcbers e hn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch wenn du so genau das deine wilt besch\u00fctzen/", "tokens": ["Mensch", "wenn", "du", "so", "ge\u00b7nau", "das", "dei\u00b7ne", "wilt", "be\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "ADJD", "ART", "PPOSAT", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So wirstu nimmermehr im wahren friede sitzen.", "tokens": ["So", "wirs\u00b7tu", "nim\u00b7mer\u00b7mehr", "im", "wah\u00b7ren", "frie\u00b7de", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}