{"textgrid.poem.47976": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Friedhofszauber", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser stille Gottesacker,", "tokens": ["Die\u00b7ser", "stil\u00b7le", "Got\u00b7te\u00b7sa\u00b7cker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses gr\u00fcne Totenfeld,", "tokens": ["die\u00b7ses", "gr\u00fc\u00b7ne", "To\u00b7ten\u00b7feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie es wieder mich im Banne", "tokens": ["wie", "es", "wie\u00b7der", "mich", "im", "Ban\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seines tiefen Friedens h\u00e4lt!", "tokens": ["sei\u00b7nes", "tie\u00b7fen", "Frie\u00b7dens", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter diesen B\u00e4umen tr\u00e4umt ich", "tokens": ["Un\u00b7ter", "die\u00b7sen", "B\u00e4u\u00b7men", "tr\u00e4umt", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "einst mein Leben licht und sch\u00f6n \u2013", "tokens": ["einst", "mein", "Le\u00b7ben", "licht", "und", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sonnengoldne Zukunftsbilder", "tokens": ["son\u00b7nen\u00b7gold\u00b7ne", "Zu\u00b7kunfts\u00b7bil\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "winkten von den fernen H\u00f6hn.", "tokens": ["wink\u00b7ten", "von", "den", "fer\u00b7nen", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonnengoldne Zukunftsbilder", "tokens": ["Son\u00b7nen\u00b7gold\u00b7ne", "Zu\u00b7kunfts\u00b7bil\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lockten schmeichelnd mich hinaus", "tokens": ["lock\u00b7ten", "schmei\u00b7chelnd", "mich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aus der Heimat sicherm Frieden", "tokens": ["aus", "der", "Hei\u00b7mat", "si\u00b7cherm", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "in des Lebens Sturmgebraus.", "tokens": ["in", "des", "Le\u00b7bens", "Sturm\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen vollen Taumelbecher", "tokens": ["Ei\u00b7nen", "vol\u00b7len", "Tau\u00b7mel\u00b7be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "setzt ich d\u00fcrstend an den Mund \u2013", "tokens": ["setzt", "ich", "d\u00fcrs\u00b7tend", "an", "den", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und ich trank ihn bis zur Neige", "tokens": ["und", "ich", "trank", "ihn", "bis", "zur", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und ich leert ihn bis zum Grund.", "tokens": ["und", "ich", "leert", "ihn", "bis", "zum", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In die Heimat kehr ich wieder,", "tokens": ["In", "die", "Hei\u00b7mat", "kehr", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nun der Lenz die Fluren s\u00e4umt: \u2013", "tokens": ["nun", "der", "Lenz", "die", "Flu\u00b7ren", "s\u00e4umt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Schmerzen sind zerstoben,", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen", "sind", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "meine Wonnen sind vertr\u00e4umt.", "tokens": ["mei\u00b7ne", "Won\u00b7nen", "sind", "ver\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In geheimnisvolles Schweigen", "tokens": ["In", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00fcllt mich Lindendunkel ein;", "tokens": ["h\u00fcllt", "mich", "Lin\u00b7den\u00b7dun\u00b7kel", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durch die knospenschweren Zweige", "tokens": ["durch", "die", "knos\u00b7pen\u00b7schwe\u00b7ren", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "blickt der Maiensonnenschein.", "tokens": ["blickt", "der", "Mai\u00b7en\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und berauschend von den H\u00fcgeln", "tokens": ["Und", "be\u00b7rau\u00b7schend", "von", "den", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "steigt empor der Bl\u00fctenduft,", "tokens": ["steigt", "em\u00b7por", "der", "Bl\u00fc\u00b7ten\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber um die fernen H\u00f6hen", "tokens": ["a\u00b7ber", "um", "die", "fer\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weht's wie feuchte Nebelluft.", "tokens": ["weht's", "wie", "feuch\u00b7te", "Ne\u00b7bel\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieser stille Gottesacker,", "tokens": ["Die\u00b7ser", "stil\u00b7le", "Got\u00b7te\u00b7sa\u00b7cker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses weite Totenfeld,", "tokens": ["die\u00b7ses", "wei\u00b7te", "To\u00b7ten\u00b7feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie es mich im Zauberbanne", "tokens": ["wie", "es", "mich", "im", "Zau\u00b7ber\u00b7ban\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seines tiefen Friedens h\u00e4lt!", "tokens": ["sei\u00b7nes", "tie\u00b7fen", "Frie\u00b7dens", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}