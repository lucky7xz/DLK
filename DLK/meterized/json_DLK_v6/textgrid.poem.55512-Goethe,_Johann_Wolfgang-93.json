{"textgrid.poem.55512": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "93", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also lustig sah es aus,", "tokens": ["Al\u00b7so", "lus\u00b7tig", "sah", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Main vor\u00fcberflo\u00df,", "tokens": ["Wo", "der", "Main", "vor\u00b7\u00fc\u00b7ber\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als im schmucken Hain und Haus", "tokens": ["Als", "im", "schmu\u00b7cken", "Hain", "und", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Festlich Eilfer \u00fcberflo\u00df.", "tokens": ["Fest\u00b7lich", "Eil\u00b7fer", "\u00fc\u00b7ber\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ferner Freunde wart gedacht:", "tokens": ["Fer\u00b7ner", "Freun\u00b7de", "wart", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn das hei\u00dft genie\u00dfen,", "tokens": ["Denn", "das", "hei\u00dft", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn zu Fest- und Flusses Pracht", "tokens": ["Wenn", "zu", "Fest", "und", "Flus\u00b7ses", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "TRUNC", "KON", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Quellen flie\u00dfen.", "tokens": ["Tau\u00b7send", "Quel\u00b7len", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}