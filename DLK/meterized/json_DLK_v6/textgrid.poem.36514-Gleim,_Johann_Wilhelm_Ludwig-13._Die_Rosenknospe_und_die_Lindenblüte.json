{"textgrid.poem.36514": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "13. Die Rosenknospe und die Lindenbl\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Rosenknospe r\u00fchmte Lindenbl\u00fcten", "tokens": ["Ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "r\u00fchm\u00b7te", "Lin\u00b7den\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ihre Sch\u00f6nheit! \u2013 \u2013 Balsamduft", "tokens": ["Ih\u00b7re", "Sch\u00f6n\u00b7heit", "!", "\u2013", "\u2013", "Bal\u00b7sam\u00b7duft"], "token_info": ["word", "word", "punct", "punct", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "$(", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauchen wir in d\u00fcnne Luft!", "tokens": ["Hau\u00b7chen", "wir", "in", "d\u00fcn\u00b7ne", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagten all' auf einmal, und gerieten", "tokens": ["Sag\u00b7ten", "all'", "auf", "ein\u00b7mal", ",", "und", "ge\u00b7rie\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADV", "$,", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Fast in Zorn! die Knospe schwieg;", "tokens": ["Fast", "in", "Zorn", "!", "die", "Knos\u00b7pe", "schwieg", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zanken, denkt sie, will mir nicht geziemen!", "tokens": ["Zan\u00b7ken", ",", "denkt", "sie", ",", "will", "mir", "nicht", "ge\u00b7zie\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gegen Abend aber stieg", "tokens": ["Ge\u00b7gen", "A\u00b7bend", "a\u00b7ber", "stieg"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Geruch empor! \u2013 \u2013 Sie spricht:", "tokens": ["Ihr", "Ge\u00b7ruch", "em\u00b7por", "!", "\u2013", "\u2013", "Sie", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "$(", "$(", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}