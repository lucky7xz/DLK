{"dta.poem.7664": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein weiter Thorweg ist, ein Pf\u00f6rtchen eng zur Seiten,", "tokens": ["Ein", "wei\u00b7ter", "Thor\u00b7weg", "ist", ",", "ein", "Pf\u00f6rt\u00b7chen", "eng", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Gehn und Schreiten das, zum Fahren der und Reiten.", "tokens": ["Zum", "Gehn", "und", "Schrei\u00b7ten", "das", ",", "zum", "Fah\u00b7ren", "der", "und", "Rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PDS", "$,", "APPRART", "NN", "ART", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Fahrweg ist Gedr\u00e4ng, heut Staub und morgen Koth;", "tokens": ["Im", "Fahr\u00b7weg", "ist", "Ge\u00b7dr\u00e4ng", ",", "heut", "Staub", "und", "mor\u00b7gen", "Koth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$,", "ADV", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchs enge Pf\u00f6rtchen kommt man immer gut zur Noth.", "tokens": ["Durchs", "en\u00b7ge", "Pf\u00f6rt\u00b7chen", "kommt", "man", "im\u00b7mer", "gut", "zur", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein B\u00fccken darf dich nur und Dr\u00fccken nicht verdrie\u00dfen;", "tokens": ["Ein", "B\u00fc\u00b7cken", "darf", "dich", "nur", "und", "Dr\u00fc\u00b7cken", "nicht", "ver\u00b7drie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein zu Buck und Druck konnt' ich mich nie entschlie\u00dfen.", "tokens": ["Al\u00b7lein", "zu", "Buck", "und", "Druck", "konnt'", "ich", "mich", "nie", "ent\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wie ich t\u00e4glich dort geh' aus und ein das Thor,", "tokens": ["Und", "wie", "ich", "t\u00e4g\u00b7lich", "dort", "geh'", "aus", "und", "ein", "das", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVFIN", "PTKVZ", "KON", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieh' ich das weite stets dem engen th\u00f6richt vor.", "tokens": ["Zieh'", "ich", "das", "wei\u00b7te", "stets", "dem", "en\u00b7gen", "th\u00f6\u00b7richt", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mir ist, ein Ungl\u00fcck m\u00fc\u00dft' am Tage mich befallen,", "tokens": ["Mir", "ist", ",", "ein", "Un\u00b7gl\u00fcck", "m\u00fc\u00dft'", "am", "Ta\u00b7ge", "mich", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VMFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ich mich b\u00fcckte durch das enge Joch zu wallen.", "tokens": ["Wo", "ich", "mich", "b\u00fcck\u00b7te", "durch", "das", "en\u00b7ge", "Joch", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und jeder Fahr schein' ich mir f\u00fcr den Tag entgangen,", "tokens": ["Und", "je\u00b7der", "Fahr", "schein'", "ich", "mir", "f\u00fcr", "den", "Tag", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo meine Schritte durch den koth'gen Fahrweg drangen.", "tokens": ["Wo", "mei\u00b7ne", "Schrit\u00b7te", "durch", "den", "ko\u00b7th'\u00b7gen", "Fahr\u00b7weg", "dran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Du sprichst: ein Aberglaub' ist dis und Wahn ein toller.", "tokens": ["Du", "sprichst", ":", "ein", "A\u00b7ber\u00b7glaub'", "ist", "dis", "und", "Wahn", "ein", "tol\u00b7ler", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PDS", "KON", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja freilich, aber mir ein nicht unsegenvoller.", "tokens": ["Ja", "frei\u00b7lich", ",", "a\u00b7ber", "mir", "ein", "nicht", "un\u00b7se\u00b7gen\u00b7vol\u00b7ler", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "KON", "PPER", "ART", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Denn nie gedankenlos geh' ich nun aus und ein,", "tokens": ["Denn", "nie", "ge\u00b7dan\u00b7ken\u00b7los", "geh'", "ich", "nun", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets unterm Thore f\u00e4llt mir meine Thorheit ein.", "tokens": ["Stets", "un\u00b7term", "Tho\u00b7re", "f\u00e4llt", "mir", "mei\u00b7ne", "Thor\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Mann ist weise, der an seine Thorheit denkt,", "tokens": ["Der", "Mann", "ist", "wei\u00b7se", ",", "der", "an", "sei\u00b7ne", "Thor\u00b7heit", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df, mit welcher Macht sie unsre Schritte lenkt.", "tokens": ["Und", "wei\u00df", ",", "mit", "wel\u00b7cher", "Macht", "sie", "uns\u00b7re", "Schrit\u00b7te", "lenkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PWAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wo dir's unm\u00f6glich ist der Thorheit widerstreben,", "tokens": ["Wo", "dir's", "un\u00b7m\u00f6g\u00b7lich", "ist", "der", "Thor\u00b7heit", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Magst du ihr wenigstens der Weisheit Anstrich geben.", "tokens": ["Magst", "du", "ihr", "we\u00b7nigs\u00b7tens", "der", "Weis\u00b7heit", "An\u00b7strich", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}