{"textgrid.poem.24055": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die goldene W\u00e4rme schwand in die Nacht,", "tokens": ["Die", "gol\u00b7de\u00b7ne", "W\u00e4r\u00b7me", "schwand", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun ist der kalte Spott erwacht,", "tokens": ["Nun", "ist", "der", "kal\u00b7te", "Spott", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sich ins tiefste Erdenloch", "tokens": ["Der", "sich", "ins", "tiefs\u00b7te", "Er\u00b7den\u00b7loch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor der schenkenden G\u00fcte verkroch.", "tokens": ["Vor", "der", "schen\u00b7ken\u00b7den", "G\u00fc\u00b7te", "ver\u00b7kroch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Es glitzert frech,", "tokens": ["Es", "glit\u00b7zert", "frech", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Schild von Blech,", "tokens": ["Ein", "Schild", "von", "Blech", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der leere Mond \u00fcber des Tages Leiche.", "tokens": ["Der", "lee\u00b7re", "Mond", "\u00fc\u00b7ber", "des", "Ta\u00b7ges", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Seine Strahlen sind Seelen vom Schattenreiche.", "tokens": ["Sei\u00b7ne", "Strah\u00b7len", "sind", "See\u00b7len", "vom", "Schat\u00b7ten\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}}}}