{"dta.poem.20986": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "L.  \n Eines ungleichen Richters.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hier begraben liegt/ darff keiner recht bekennen/", "tokens": ["Wer", "hier", "be\u00b7gra\u00b7ben", "liegt", "/", "darff", "kei\u00b7ner", "recht", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VVFIN", "$(", "VMFIN", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ieder h\u00fcttet sich vor St\u00e4upen und vor Brennen.", "tokens": ["Ein", "ie\u00b7der", "h\u00fct\u00b7tet", "sich", "vor", "St\u00e4u\u00b7pen", "und", "vor", "Bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Wort geht noch wol hin/ doch dr\u00fcck ein Auge zu/", "tokens": ["Ein", "Wort", "geht", "noch", "wol", "hin", "/", "doch", "dr\u00fcck", "ein", "Au\u00b7ge", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$(", "ADV", "ADV", "ART", "NN", "PTKZU", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So sag ich: dieser Mann war eben so wie du.", "tokens": ["So", "sag", "ich", ":", "die\u00b7ser", "Mann", "war", "e\u00b7ben", "so", "wie", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PDAT", "NN", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}