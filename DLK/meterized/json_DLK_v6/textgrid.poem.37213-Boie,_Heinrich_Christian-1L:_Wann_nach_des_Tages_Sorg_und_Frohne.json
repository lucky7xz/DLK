{"textgrid.poem.37213": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann nach des Tages Sorg und Frohne", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann nach des Tages Sorg und Frohne", "tokens": ["Wann", "nach", "des", "Ta\u00b7ges", "Sorg", "und", "Froh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich Abends nun entl\u00e4\u00dft die Pflicht,", "tokens": ["Mich", "A\u00b7bends", "nun", "ent\u00b7l\u00e4\u00dft", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich der Chinarose nicht", "tokens": ["Und", "ich", "der", "Chi\u00b7na\u00b7ro\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht des Oleanders schone,", "tokens": ["Und", "nicht", "des", "O\u00b7lean\u00b7ders", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Hand zur stolzen Anemone", "tokens": ["Die", "Hand", "zur", "stol\u00b7zen", "A\u00b7ne\u00b7mo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bescheidenes Vergi\u00dfmeinnicht,", "tokens": ["Be\u00b7schei\u00b7de\u00b7nes", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schasmin, Resed und Myrthe bricht,", "tokens": ["Schas\u00b7min", ",", "Re\u00b7sed", "und", "Myr\u00b7the", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und der Begleiterin zur Krone", "tokens": ["Und", "der", "Be\u00b7glei\u00b7te\u00b7rin", "zur", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Sie unter Scherz und Liedern flicht;", "tokens": ["Sie", "un\u00b7ter", "Scherz", "und", "Lie\u00b7dern", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In holder Anmut wie Dione", "tokens": ["In", "hol\u00b7der", "An\u00b7mut", "wie", "Dio\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Lacht dann das M\u00e4dchen mir und spricht:", "tokens": ["Lacht", "dann", "das", "M\u00e4d\u00b7chen", "mir", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbdir Altem gleich gibts wo ich wohne", "tokens": ["\u00bb", "dir", "Al\u00b7tem", "gleich", "gibts", "wo", "ich", "woh\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "NN", "ADV", "VVFIN", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der S\u00e4nger und der G\u00e4rtner nicht.\u00ab", "tokens": ["Der", "S\u00e4n\u00b7ger", "und", "der", "G\u00e4rt\u00b7ner", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und mit dem freundlichsten Gesicht", "tokens": ["Und", "mit", "dem", "freund\u00b7lichs\u00b7ten", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.15": {"text": "Reicht sie mir einen Kuss zum Lohne.", "tokens": ["Reicht", "sie", "mir", "ei\u00b7nen", "Kuss", "zum", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Warum sprach man in solchem Tone", "tokens": ["Wa\u00b7rum", "sprach", "man", "in", "sol\u00b7chem", "To\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Vor zwanzig Jahren mir noch nicht?", "tokens": ["Vor", "zwan\u00b7zig", "Jah\u00b7ren", "mir", "noch", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}