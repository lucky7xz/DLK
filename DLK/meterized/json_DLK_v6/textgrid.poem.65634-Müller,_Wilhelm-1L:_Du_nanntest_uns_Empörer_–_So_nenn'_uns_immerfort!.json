{"textgrid.poem.65634": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du nanntest uns Emp\u00f6rer \u2013 So nenn' uns immerfort!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du nanntest uns Emp\u00f6rer \u2013 So nenn' uns immerfort!", "tokens": ["Du", "nann\u00b7test", "uns", "Em\u00b7p\u00f6\u00b7rer", "\u2013", "So", "nenn'", "uns", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Empor! Empor! so hei\u00dft es, der Griechen Losungswort.", "tokens": ["Em\u00b7por", "!", "Em\u00b7por", "!", "so", "hei\u00dft", "es", ",", "der", "Grie\u00b7chen", "Lo\u00b7sungs\u00b7wort", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Empor zu deinem Gotte, empor zu deinem Recht,", "tokens": ["Em\u00b7por", "zu", "dei\u00b7nem", "Got\u00b7te", ",", "em\u00b7por", "zu", "dei\u00b7nem", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Empor zu deinen V\u00e4tern, entw\u00fcrdigtes Geschlecht!", "tokens": ["Em\u00b7por", "zu", "dei\u00b7nen", "V\u00e4\u00b7tern", ",", "ent\u00b7w\u00fcr\u00b7dig\u00b7tes", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Empor aus Sklavenketten, aus dumpfem Kerkerduft,", "tokens": ["Em\u00b7por", "aus", "Skla\u00b7ven\u00b7ket\u00b7ten", ",", "aus", "dum\u00b7pfem", "Ker\u00b7ker\u00b7duft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Empor mit vollen Schwingen in freie Lebensluft!", "tokens": ["Em\u00b7por", "mit", "vol\u00b7len", "Schwin\u00b7gen", "in", "frei\u00b7e", "Le\u00b7bens\u00b7luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Empor, empor, ihr Schl\u00e4fer, aus tiefer Todesnacht!", "tokens": ["Em\u00b7por", ",", "em\u00b7por", ",", "ihr", "Schl\u00e4\u00b7fer", ",", "aus", "tie\u00b7fer", "To\u00b7des\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der Auferstehungsmorgen ist rosenroth erwacht.", "tokens": ["Der", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7mor\u00b7gen", "ist", "ro\u00b7sen\u00b7roth", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Du nanntest uns Emp\u00f6rer \u2013 So nenn' uns immerfort!", "tokens": ["Du", "nann\u00b7test", "uns", "Em\u00b7p\u00f6\u00b7rer", "\u2013", "So", "nenn'", "uns", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Empor, so hei\u00df' es ewig, der Griechen Losungswort!", "tokens": ["Em\u00b7por", ",", "so", "hei\u00df'", "es", "e\u00b7wig", ",", "der", "Grie\u00b7chen", "Lo\u00b7sungs\u00b7wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dir aber t\u00f6ne nimmer in's Herz der hohe Klang:", "tokens": ["Dir", "a\u00b7ber", "t\u00f6\u00b7ne", "nim\u00b7mer", "in's", "Herz", "der", "ho\u00b7he", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Beobacht' aus dem Staube die Welt dein Lebelang!", "tokens": ["Be\u00b7ob\u00b7acht'", "aus", "dem", "Stau\u00b7be", "die", "Welt", "dein", "Le\u00b7be\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}}}}