{"textgrid.poem.46091": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o Stunde, da der Schiffende bang lauert]", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Stunde, da der Schiffende bang lauert", "tokens": ["O", "Stun\u00b7de", ",", "da", "der", "Schif\u00b7fen\u00b7de", "bang", "lau\u00b7ert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sich zur Heimat sehnet an dem Tage,", "tokens": ["Und", "sich", "zur", "Hei\u00b7mat", "seh\u00b7net", "an", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da er von s\u00fc\u00dfen Freunden ist geschieden,", "tokens": ["Da", "er", "von", "s\u00fc\u00b7\u00dfen", "Freun\u00b7den", "ist", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da in des Pilgers Herz die Liebe trauert", "tokens": ["Da", "in", "des", "Pil\u00b7gers", "Herz", "die", "Lie\u00b7be", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf erster Fahrt, wenn ferner Glocken Klage", "tokens": ["Auf", "ers\u00b7ter", "Fahrt", ",", "wenn", "fer\u00b7ner", "Glo\u00b7cken", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Tag beweinet, der da stirbt in Frieden!", "tokens": ["Den", "Tag", "be\u00b7wei\u00b7net", ",", "der", "da", "stirbt", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}