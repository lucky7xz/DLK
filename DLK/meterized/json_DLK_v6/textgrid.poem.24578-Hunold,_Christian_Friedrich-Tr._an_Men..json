{"textgrid.poem.24578": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tr. an Men.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein H -- sage mir/wie mach ichs endlich recht?", "tokens": ["Mein", "H", "sa\u00b7ge", "mir", "/", "wie", "mach", "ichs", "end\u00b7lich", "recht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "XY", "$(", "VVFIN", "PPER", "$(", "PWAV", "VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+----+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schreib ich vom Lieben was/bin ich der Wollust Knecht;", "tokens": ["Schreib", "ich", "vom", "Lie\u00b7ben", "was", "/", "bin", "ich", "der", "Wol\u00b7lust", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "PWS", "$(", "VAFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und will ich dir so dann von wahrer Tugend schreiben/", "tokens": ["Und", "will", "ich", "dir", "so", "dann", "von", "wah\u00b7rer", "Tu\u00b7gend", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wilst du mich auch der Zahl der Heuchler einverleiben/", "tokens": ["Wilst", "du", "mich", "auch", "der", "Zahl", "der", "Heuch\u00b7ler", "ein\u00b7ver\u00b7lei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sagst/ich schicke mich nur in die falsche Welt/", "tokens": ["Und", "sagst", "/", "ich", "schi\u00b7cke", "mich", "nur", "in", "die", "fal\u00b7sche", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die bald von Tugend was/bald was vom lieben h\u00e4lt.", "tokens": ["Die", "bald", "von", "Tu\u00b7gend", "was", "/", "bald", "was", "vom", "lie\u00b7ben", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PWS", "$(", "ADV", "PWS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was mach ich endlich nun/wie werd ich dir gefallen?", "tokens": ["Was", "mach", "ich", "end\u00b7lich", "nun", "/", "wie", "werd", "ich", "dir", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOKOM", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du weist/geliebter Freund/ich folge dir in allen.", "tokens": ["Du", "weist", "/", "ge\u00b7lieb\u00b7ter", "Freund", "/", "ich", "fol\u00b7ge", "dir", "in", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn T -- noch bey dir in eintzger Gutheit ist/", "tokens": ["Wenn", "T", "noch", "bey", "dir", "in", "eintz\u00b7ger", "Gut\u00b7heit", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So mache/da\u00df er dich noch auf den Abend k\u00fc\u00dft.", "tokens": ["So", "ma\u00b7che", "/", "da\u00df", "er", "dich", "noch", "auf", "den", "A\u00b7bend", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du magst ihn itzo so/bald wieder so beschreiben/", "tokens": ["Du", "magst", "ihn", "it\u00b7zo", "so", "/", "bald", "wie\u00b7der", "so", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "$(", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird er doch hiervor dein treuer Diener bleiben.", "tokens": ["So", "wird", "er", "doch", "hier\u00b7vor", "dein", "treu\u00b7er", "Die\u00b7ner", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}