{"textgrid.poem.46267": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber ein spiegel zu schreiben", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du sch\u00f6n, so gebrauch auch flei\u00df,", "tokens": ["Bist", "du", "sch\u00f6n", ",", "so", "ge\u00b7brauch", "auch", "flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "mit lastern dich nicht zu beflecken:", "tokens": ["mit", "las\u00b7tern", "dich", "nicht", "zu", "be\u00b7fle\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bist du dan h\u00e4\u00dflich, so sei weis,", "tokens": ["bist", "du", "dan", "h\u00e4\u00df\u00b7lich", ",", "so", "sei", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit tugend den fehl zu bedecken.", "tokens": ["mit", "tu\u00b7gend", "den", "fehl", "zu", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}}}}