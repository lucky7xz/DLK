{"dta.poem.6168": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen k\u00f6nnen wir denselben Anfang finden.", "tokens": ["Von", "al\u00b7len", "k\u00f6n\u00b7nen", "wir", "den\u00b7sel\u00b7ben", "An\u00b7fang", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist allein der Drang,", "tokens": ["Es", "ist", "al\u00b7lein", "der", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sammt der Bewegung von dem Ort,", "tokens": ["Sammt", "der", "Be\u00b7we\u00b7gung", "von", "dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So kleine C\u00f6rpergen verbindet und entbindet.", "tokens": ["So", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7gen", "ver\u00b7bin\u00b7det", "und", "ent\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann sich des Staubes Heer zusammen findet,", "tokens": ["Wann", "sich", "des", "Stau\u00b7bes", "Heer", "zu\u00b7sam\u00b7men", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In rechter Maa\u00df, in recht beh\u00f6rigem Aceord;", "tokens": ["In", "rech\u00b7ter", "Maa\u00df", ",", "in", "recht", "be\u00b7h\u00f6\u00b7ri\u00b7gem", "A\u00b7ceord", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entstehen C\u00f6rper alsofort.", "tokens": ["Ent\u00b7ste\u00b7hen", "C\u00f6r\u00b7per", "al\u00b7so\u00b7fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}