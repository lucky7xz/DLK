text": "Und sie, und sie, die Religion,", "tokens": ["Und", "sie", ",", "und", "sie", ",", "die", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KON", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heilig sie, und erhaben,", "tokens": ["Hei\u00b7lig", "sie", ",", "und", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "PPER", "$,", "KON", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Furchtbar, und lieblich, und gross, und hehr,", "tokens": ["Furcht\u00b7bar", ",", "und", "lieb\u00b7lich", ",", "und", "gross", ",", "und", "hehr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Von Gott gesandt,", "tokens": ["Von", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Haben mein Maal errichtet. Nun stehet es da,", "tokens": ["Ha\u00b7ben", "mein", "Maal", "er\u00b7rich\u00b7tet", ".", "Nun", "ste\u00b7het", "es", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und spottet der Zeit, und spottet", "tokens": ["Und", "spot\u00b7tet", "der", "Zeit", ",", "und", "spot\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ewig gew\u00e4hnter Maale,", "tokens": ["E\u00b7wig", "ge\u00b7w\u00e4hn\u00b7ter", "Maa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Welche schon jetzt dem Auge, das sieht, Tr\u00fcmmern sind.", "tokens": ["Wel\u00b7che", "schon", "jetzt", "dem", "Au\u00b7ge", ",", "das", "sieht", ",", "Tr\u00fcm\u00b7mern", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}