"word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Suchte den Helden, fand ihn nicht; bis ich zuletzt", "tokens": ["Such\u00b7te", "den", "Hel\u00b7den", ",", "fand", "ihn", "nicht", ";", "bis", "ich", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "M\u00fcd' hinsank; dann wie aus Schlummer geweckt, auf Einmal", "tokens": ["M\u00fcd'", "hin\u00b7sank", ";", "dann", "wie", "aus", "Schlum\u00b7mer", "ge\u00b7weckt", ",", "auf", "Ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ADV", "KOKOM", "APPR", "NN", "VVPP", "$,", "APPR", "NN"], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Rings um mich her wie mit Donnerflammen es strahlen sah!", "tokens": ["Rings", "um", "mich", "her", "wie", "mit", "Don\u00b7ner\u00b7flam\u00b7men", "es", "strah\u00b7len", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "KOKOM", "APPR", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.14": {"line.1": {"text": "Welch Anschaun war es! Denn Ihn, den als Christ, ich liebte,", "tokens": ["Welch", "An\u00b7schaun", "war", "es", "!", "Denn", "Ihn", ",", "den", "als", "Christ", ",", "ich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$.", "KON", "PPER", "$,", "PRELS", "KOUS", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sah ich mit Einem schnellen begeisterten Blick,", "tokens": ["Sah", "ich", "mit", "Ei\u00b7nem", "schnel\u00b7len", "be\u00b7geis\u00b7ter\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als Dichter, und empfand: Es liebe mit Innigkeit", "tokens": ["Als", "Dich\u00b7ter", ",", "und", "emp\u00b7fand", ":", "Es", "lie\u00b7be", "mit", "In\u00b7nig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch der Dichter den G\u00f6ttlichen!", "tokens": ["Auch", "der", "Dich\u00b7ter", "den", "G\u00f6tt\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Erstaunt \u00fcber Seine so sp\u00e4te Wahl, dacht' ich nur Ihn!", "tokens": ["Er\u00b7staunt", "\u00fc\u00b7ber", "Sei\u00b7ne", "so", "sp\u00e4\u00b7te", "Wahl", ",", "dacht'", "ich", "nur", "Ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vergass selbst der ged\u00fcrsteten Unsterblichkeit,", "tokens": ["Ver\u00b7gass", "selbst", "der", "ge\u00b7d\u00fcrs\u00b7te\u00b7ten", "U\u