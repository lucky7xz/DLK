{"textgrid.poem.57319": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weiter hinab wallet mein Fuss, und der Stab wird", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiter hinab wallet mein Fuss, und der Stab wird", "tokens": ["Wei\u00b7ter", "hin\u00b7ab", "wal\u00b7let", "mein", "Fuss", ",", "und", "der", "Stab", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VAFIN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mir nicht allein von dem Staube, den der Weg st\u00e4ubt,", "tokens": ["Mir", "nicht", "al\u00b7lein", "von", "dem", "Stau\u00b7be", ",", "den", "der", "Weg", "st\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wird dem Wanderer auch von Asche", "tokens": ["Wird", "dem", "Wan\u00b7de\u00b7rer", "auch", "von", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "N\u00e4herer Todter bew\u00f6lkt.", "tokens": ["N\u00e4\u00b7he\u00b7rer", "Tod\u00b7ter", "be\u00b7w\u00f6lkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n wird mein Blick dort es gewahr. O der Aussicht", "tokens": ["Sch\u00f6n", "wird", "mein", "Blick", "dort", "es", "ge\u00b7wahr", ".", "O", "der", "Aus\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "PPER", "ADJD", "$.", "NE", "ART", "NN"], "meter": "+--+----+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dr\u00fcben! da strahlt's von dem Fr\u00fchling, der uns ewig", "tokens": ["Dr\u00fc\u00b7ben", "!", "da", "strahlt's", "von", "dem", "Fr\u00fch\u00b7ling", ",", "der", "uns", "e\u00b7wig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bl\u00fcht, und duftet, und weht. O Pfad, wo", "tokens": ["Bl\u00fcht", ",", "und", "duf\u00b7tet", ",", "und", "weht", ".", "O", "Pfad", ",", "wo"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$.", "NE", "NN", "$,", "PWAV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Staub nicht, und Asche bew\u00f6lkt.", "tokens": ["Staub", "nicht", ",", "und", "A\u00b7sche", "be\u00b7w\u00f6lkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KON", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Aber sondern muss ich mich, trennen mich, muss von den Freunden", "tokens": ["A\u00b7ber", "son\u00b7dern", "muss", "ich", "mich", ",", "tren\u00b7nen", "mich", ",", "muss", "von", "den", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KON", "VMFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Scheiden! Du bist ein tiefer bitterer Kelch!", "tokens": ["Schei\u00b7den", "!", "Du", "bist", "ein", "tie\u00b7fer", "bit\u00b7te\u00b7rer", "Kelch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+----+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ach tr\u00e4nk' ich dich nicht bey Tropfen!", "tokens": ["Ach", "tr\u00e4nk'", "ich", "dich", "nicht", "bey", "Trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Leert' ich mit Einem Zuge dich aus,", "tokens": ["Leert'", "ich", "mit", "Ei\u00b7nem", "Zu\u00b7ge", "dich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Ungest\u00fcm aus! wie, wer Durst lechzt,", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "aus", "!", "wie", ",", "wer", "Durst", "lechzt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PWAV", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell sich erk\u00fchlt, sich erlabet an dem Labsal!", "tokens": ["Schnell", "sich", "er\u00b7k\u00fchlt", ",", "sich", "er\u00b7la\u00b7bet", "an", "dem", "Lab\u00b7sal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weg vom Kelche, Gesang! Tiefsinnig", "tokens": ["Weg", "vom", "Kel\u00b7che", ",", "Ge\u00b7sang", "!", "Tief\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "$.", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hatt' ich geforscht,", "tokens": ["Hatt'", "ich", "ge\u00b7forscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Zweifelnd versenkt, ernster durchdacht: (O es galt da", "tokens": ["Zwei\u00b7felnd", "ver\u00b7senkt", ",", "erns\u00b7ter", "durch\u00b7dacht", ":(", "O", "es", "galt", "da"], "token_info": ["word", "word", "punct", "word", "word", "emoticon", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADJA", "NN", "$(", "NE", "PPER", "VVFIN", "ADV"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "T\u00e4uschung nicht mit, und kein Wahn mit) Was ihn mache,", "tokens": ["T\u00e4u\u00b7schung", "nicht", "mit", ",", "und", "kein", "Wahn", "mit", ")", "Was", "ihn", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "KON", "PIAT", "NN", "APPR", "$(", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Der, zu leben! entstand, zu sterben!", "tokens": ["Der", ",", "zu", "le\u00b7ben", "!", "ent\u00b7stand", ",", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PTKZU", "VVINF", "$.", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gl\u00fccklich den? Ich war es, und bins!", "tokens": ["Gl\u00fcck\u00b7lich", "den", "?", "Ich", "war", "es", ",", "und", "bins", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "$.", "PPER", "VAFIN", "PPER", "$,", "KON", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Viel Blumen bl\u00fchn in diesem heiligen Kranz. Unsterblichkeit", "tokens": ["Viel", "Blu\u00b7men", "bl\u00fchn", "in", "die\u00b7sem", "hei\u00b7li\u00b7gen", "Kranz", ".", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+--++--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ist der Blumen Eine. Der Weise durchschaut", "tokens": ["Ist", "der", "Blu\u00b7men", "Ei\u00b7ne", ".", "Der", "Wei\u00b7se", "durch\u00b7schaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "$.", "ART", "NN", "VVFIN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihrer Wirkung Kreis. Sie scheint der K\u00f6nige Loos;", "tokens": ["Ih\u00b7rer", "Wir\u00b7kung", "Kreis", ".", "Sie", "scheint", "der", "K\u00f6\u00b7ni\u00b7ge", "Loos", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Allein die werden in der Geschichte zu Mumien!", "tokens": ["Al\u00b7lein", "die", "wer\u00b7den", "in", "der", "Ge\u00b7schich\u00b7te", "zu", "Mu\u00b7mi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Geburtsrecht zu der Unsterblichkeit", "tokens": ["Ge\u00b7burts\u00b7recht", "zu", "der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist Unrecht bey der Nachwelt. So bald einst die Geschichte,", "tokens": ["Ist", "Un\u00b7recht", "bey", "der", "Nach\u00b7welt", ".", "So", "bald", "einst", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$.", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Was ihr obliegt, thut: so begr\u00e4bt sie durch Schweigen, und stellt", "tokens": ["Was", "ihr", "ob\u00b7liegt", ",", "thut", ":", "so", "be\u00b7gr\u00e4bt", "sie", "durch", "Schwei\u00b7gen", ",", "und", "stellt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die K\u00f6nige dann selbst nicht mehr als Mumien auf.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "dann", "selbst", "nicht", "mehr", "als", "Mu\u00b7mi\u00b7en", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "ADV", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie sind nach dem Tode, was wir sind.", "tokens": ["Sie", "sind", "nach", "dem", "To\u00b7de", ",", "was", "wir", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleibt, ihr Name; so rettet ihn nur Verdienst,", "tokens": ["Bleibt", ",", "ihr", "Na\u00b7me", ";", "so", "ret\u00b7tet", "ihn", "nur", "Ver\u00b7dienst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht die Krone: denn sie", "tokens": ["Nicht", "die", "Kro\u00b7ne", ":", "denn", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$.", "KON", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sank mit dem Haupte der sterbenden.", "tokens": ["Sank", "mit", "dem", "Haup\u00b7te", "der", "ster\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "$."], "meter": "+--+-+++-", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Voll Durstes war die heisse Seele des J\u00fcnglings", "tokens": ["Voll", "Durs\u00b7tes", "war", "die", "heis\u00b7se", "See\u00b7le", "des", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach der Unsterblichkeit!", "tokens": ["Nach", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wacht', und ich tr\u00e4umte", "tokens": ["Ich", "wacht'", ",", "und", "ich", "tr\u00e4um\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von der k\u00fchnen Fahrt auf der Zukunft Ozean!", "tokens": ["Von", "der", "k\u00fch\u00b7nen", "Fahrt", "auf", "der", "Zu\u00b7kunft", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Dank dir noch Einmal, mein fr\u00fcher Geleiter, dass du mir,", "tokens": ["Dank", "dir", "noch", "Ein\u00b7mal", ",", "mein", "fr\u00fc\u00b7her", "Ge\u00b7lei\u00b7ter", ",", "dass", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wie furchtbar es dort sey, mein Genius, zeigtest.", "tokens": ["Wie", "furcht\u00b7bar", "es", "dort", "sey", ",", "mein", "Ge\u00b7nius", ",", "zeig\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wies dein goldener Stab! Hochmastige, vollbesegelte Dichterwerke,", "tokens": ["Wie", "wies", "dein", "gol\u00b7de\u00b7ner", "Stab", "!", "Hoch\u00b7mas\u00b7ti\u00b7ge", ",", "voll\u00b7be\u00b7se\u00b7gel\u00b7te", "Dich\u00b7ter\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und dennoch gesunkene schreckten mich!", "tokens": ["Und", "den\u00b7noch", "ge\u00b7sun\u00b7ke\u00b7ne", "schreck\u00b7ten", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Weit hinab an dem brausenden Gestade", "tokens": ["Weit", "hin\u00b7ab", "an", "dem", "brau\u00b7sen\u00b7den", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lag's von der Scheiter umher.", "tokens": ["Lag's", "von", "der", "Schei\u00b7ter", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sie hatten sich hinaus auf die Woge gewagt, in den Sturm gewagt;", "tokens": ["Sie", "hat\u00b7ten", "sich", "hin\u00b7aus", "auf", "die", "Wo\u00b7ge", "ge\u00b7wagt", ",", "in", "den", "Sturm", "ge\u00b7wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APZR", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und Waren untergegangen!", "tokens": ["Und", "Wa\u00b7ren", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Bis zu der Schwermuth wurd' ich ernst, vertiefte mich", "tokens": ["Bis", "zu", "der", "Schwer\