{"textgrid.poem.42744": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[ein Nagel sa\u00df in einem St\u00fcck Holz]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Nagel sa\u00df in einem St\u00fcck Holz.", "tokens": ["Ein", "Na\u00b7gel", "sa\u00df", "in", "ei\u00b7nem", "St\u00fcck", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der war auf seine Gattin sehr stolz.", "tokens": ["Der", "war", "auf", "sei\u00b7ne", "Gat\u00b7tin", "sehr", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die trug eine goldene Haube", "tokens": ["Die", "trug", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Hau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und war eine Messingschraube.", "tokens": ["Und", "war", "ei\u00b7ne", "Mes\u00b7sing\u00b7schrau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie war etwas locker und etwas verschraubt,", "tokens": ["Sie", "war", "et\u00b7was", "lo\u00b7cker", "und", "et\u00b7was", "ver\u00b7schraubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sowohl in der Liebe, als auch \u00fcberhaupt.", "tokens": ["So\u00b7wohl", "in", "der", "Lie\u00b7be", ",", "als", "auch", "\u00fc\u00b7ber\u00b7haupt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie liebte ein H\u00e4kchen und traf sich mit ihm", "tokens": ["Sie", "lieb\u00b7te", "ein", "H\u00e4k\u00b7chen", "und", "traf", "sich", "mit", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "In einem Astloch. Sie wurden intim.", "tokens": ["In", "ei\u00b7nem", "Ast\u00b7loch", ".", "Sie", "wur\u00b7den", "in\u00b7tim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Kurz, eines Tages entfernten sie sich", "tokens": ["Kurz", ",", "ei\u00b7nes", "Ta\u00b7ges", "ent\u00b7fern\u00b7ten", "sie", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und lie\u00dfen den armen Nagel im Stich.", "tokens": ["Und", "lie\u00b7\u00dfen", "den", "ar\u00b7men", "Na\u00b7gel", "im", "Stich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der arme Nagel bog sich vor Schmerz.", "tokens": ["Der", "ar\u00b7me", "Na\u00b7gel", "bog", "sich", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Noch niemals hatte sein eisernes Herz", "tokens": ["Noch", "nie\u00b7mals", "hat\u00b7te", "sein", "ei\u00b7ser\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "So bittere Leiden gekostet.", "tokens": ["So", "bit\u00b7te\u00b7re", "Lei\u00b7den", "ge\u00b7kos\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Bald war er beinah verrostet.", "tokens": ["Bald", "war", "er", "bei\u00b7nah", "ver\u00b7ros\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Da aber kehrte sein fr\u00fcheres Gl\u00fcck,", "tokens": ["Da", "a\u00b7ber", "kehr\u00b7te", "sein", "fr\u00fc\u00b7he\u00b7res", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die alte Schraube, wieder zur\u00fcck.", "tokens": ["Die", "al\u00b7te", "Schrau\u00b7be", ",", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Sie gl\u00e4nzte \u00fcbers ganze Gesicht.", "tokens": ["Sie", "gl\u00e4nz\u00b7te", "\u00fc\u00b7bers", "gan\u00b7ze", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Ja, alte Liebe, die rostet nicht!", "tokens": ["Ja", ",", "al\u00b7te", "Lie\u00b7be", ",", "die", "ros\u00b7tet", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}