{"dta.poem.14456": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lieder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Lenze sitz' ich schon da oben,", "tokens": ["Viel", "Len\u00b7ze", "sitz'", "ich", "schon", "da", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Regenbogen steht im Land erhoben", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", "steht", "im", "Land", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die Th\u00e4ler, Wiesen, Wogen", "tokens": ["Und", "durch", "die", "Th\u00e4\u00b7ler", ",", "Wie\u00b7sen", ",", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Still, wie ein fernes Lied, gezogen,", "tokens": ["Still", ",", "wie", "ein", "fer\u00b7nes", "Lied", ",", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schifft immerfort Dein himmlisch Bild \u2014", "tokens": ["Schifft", "im\u00b7mer\u00b7fort", "Dein", "himm\u00b7lisch", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Strom und Schiff hielt niemals still.", "tokens": ["Doch", "Strom", "und", "Schiff", "hielt", "nie\u00b7mals", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}