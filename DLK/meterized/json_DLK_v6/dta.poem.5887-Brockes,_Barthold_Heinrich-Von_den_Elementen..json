{"dta.poem.5887": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Moschus kam Democritus,", "tokens": ["Nach", "Mo\u00b7schus", "kam", "De\u00b7mo\u00b7cri\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ahm\u2019te Moschus Lehre,", "tokens": ["Und", "ahm'\u00b7te", "Mo\u00b7schus", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "(in der annoch kein recht vollkommner Schlu\u00df)", "tokens": ["(", "in", "der", "an\u00b7noch", "kein", "recht", "voll\u00b7komm\u00b7ner", "Schlu\u00df", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADV", "PIAT", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In allen nach; es schien, als wenn er der Erfinder", "tokens": ["In", "al\u00b7len", "nach", ";", "es", "schien", ",", "als", "wenn", "er", "der", "Er\u00b7fin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "APPR", "$.", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der regen ", "tokens": ["Der", "re\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nicht minder", "tokens": ["Nicht", "min\u00b7der"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Schien ", "tokens": ["Schien"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Mit ihren Lehren", "tokens": ["Mit", "ih\u00b7ren", "Leh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Von kleinen C\u00f6rperlein und ihrer Form, bem\u00fcht,", "tokens": ["Von", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", "und", "ih\u00b7rer", "Form", ",", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Ordnungen der Welt gleichm\u00e4\u00dfig zu erkl\u00e4ren.", "tokens": ["Die", "Ord\u00b7nun\u00b7gen", "der", "Welt", "gleich\u00b7m\u00e4\u00b7\u00dfig", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}