{"textgrid.poem.50997": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber auch den F\u00f6hrenwald", "tokens": ["A\u00b7ber", "auch", "den", "F\u00f6h\u00b7ren\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df ich mir nicht schelten,", "tokens": ["La\u00df", "ich", "mir", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn mein Jauchzen widerhallt", "tokens": ["Wenn", "mein", "Jauch\u00b7zen", "wi\u00b7der\u00b7hallt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem sonnerhellten!", "tokens": ["In", "dem", "son\u00b7ner\u00b7hell\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Heiter ist's und aufger\u00e4umt", "tokens": ["Hei\u00b7ter", "ist's", "und", "auf\u00b7ge\u00b7r\u00e4umt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Wehn der F\u00f6hren,", "tokens": ["Und", "das", "Wehn", "der", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn die Luft in ihnen tr\u00e4umt,", "tokens": ["Wenn", "die", "Luft", "in", "ih\u00b7nen", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Angenehm zu h\u00f6ren!", "tokens": ["An\u00b7ge\u00b7nehm", "zu", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schlanken Riesenkindern gleich", "tokens": ["Schlan\u00b7ken", "Rie\u00b7sen\u00b7kin\u00b7dern", "gleich"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn sie da im Bunde,", "tokens": ["Stehn", "sie", "da", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jedes erbt ein kleines Reich", "tokens": ["Je\u00b7des", "erbt", "ein", "klei\u00b7nes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem gr\u00fcnen Grunde.", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber oben eng verwebt,", "tokens": ["A\u00b7ber", "o\u00b7ben", "eng", "ver\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Genossenschaft erhebt", "tokens": ["Die", "Ge\u00b7nos\u00b7sen\u00b7schaft", "er\u00b7hebt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolz zum Sonnenthrone.", "tokens": ["Stolz", "zum", "Son\u00b7nen\u00b7thro\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schmach und Gram umf\u00e4ngt sie nie,", "tokens": ["Schmach", "und", "Gram", "um\u00b7f\u00e4ngt", "sie", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer Lebensreue;", "tokens": ["Nim\u00b7mer", "Le\u00b7bens\u00b7reu\u00b7e", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schnell und mutig wachsen sie", "tokens": ["Schnell", "und", "mu\u00b7tig", "wach\u00b7sen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Himmels Bl\u00e4ue.", "tokens": ["In", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn ein Stamm im Sturme bricht,", "tokens": ["Wenn", "ein", "Stamm", "im", "Stur\u00b7me", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten ihn die Br\u00fcder;", "tokens": ["Hal\u00b7ten", "ihn", "die", "Br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er sinkt zur Erde nicht,", "tokens": ["Und", "er", "sinkt", "zur", "Er\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebend h\u00e4ngt er nieder.", "tokens": ["Schwe\u00b7bend", "h\u00e4ngt", "er", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Lieg ich so im Farrenkraut,", "tokens": ["Lieg", "ich", "so", "im", "Far\u00b7ren\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwindet jede Grille,", "tokens": ["Schwin\u00b7det", "je\u00b7de", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und es wird das Herz mir laut", "tokens": ["Und", "es", "wird", "das", "Herz", "mir", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der F\u00f6hrenstille.", "tokens": ["In", "der", "F\u00f6h\u00b7ren\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Weihrauchwolken ein und aus", "tokens": ["Weih\u00b7rauch\u00b7wol\u00b7ken", "ein", "und", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die R\u00e4ume wallen \u2013", "tokens": ["Durch", "die", "R\u00e4u\u00b7me", "wal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bin ich in ein Gotteshaus", "tokens": ["Bin", "ich", "in", "ein", "Got\u00b7tes\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwan eingefallen?", "tokens": ["Et\u00b7wan", "ein\u00b7ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Doch der Unsichtbare l\u00e4\u00dft", "tokens": ["Doch", "der", "Un\u00b7sicht\u00b7ba\u00b7re", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "L\u00e4chelnd es geschehen,", "tokens": ["L\u00e4\u00b7chelnd", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn mein wildes Kirchenfest", "tokens": ["Wenn", "mein", "wil\u00b7des", "Kir\u00b7chen\u00b7fest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier ich will begehen!", "tokens": ["Hier", "ich", "will", "be\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}