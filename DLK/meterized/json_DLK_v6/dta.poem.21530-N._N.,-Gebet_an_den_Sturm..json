{"dta.poem.21530": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gebet an den Sturm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Hinaus aus meines Zimmers dumpfer Schw\u00fcle!", "tokens": ["Hin\u00b7aus", "aus", "mei\u00b7nes", "Zim\u00b7mers", "dum\u00b7pfer", "Schw\u00fc\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh drau\u00dfen das Gewitter thronen!", "tokens": ["Sieh", "drau\u00b7\u00dfen", "das", "Ge\u00b7wit\u00b7ter", "thro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erbrause Sturm, die hei\u00dfe Stirn mir k\u00fchle,", "tokens": ["Er\u00b7brau\u00b7se", "Sturm", ",", "die", "hei\u00b7\u00dfe", "Stirn", "mir", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahinter rasen die D\u00e4monen.", "tokens": ["Da\u00b7hin\u00b7ter", "ra\u00b7sen", "die", "D\u00e4\u00b7mo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hei, wie die Blitze zucken durch die Nacht,", "tokens": ["Hei", ",", "wie", "die", "Blit\u00b7ze", "zu\u00b7cken", "durch", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wie pl\u00f6tzlich sie mit j\u00e4her Flammenpracht", "tokens": ["Wie", "pl\u00f6tz\u00b7lich", "sie", "mit", "j\u00e4\u00b7her", "Flam\u00b7men\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Zorn des Ew\u2019gen in die Wolken funkelnd schreiben", "tokens": ["Den", "Zorn", "des", "Ew'\u00b7gen", "in", "die", "Wol\u00b7ken", "fun\u00b7kelnd", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob seiner Menschheit nicht\u2019gem, blassem Treiben.", "tokens": ["Ob", "sei\u00b7ner", "Menschheit", "nicht'\u00b7gem", ",", "blas\u00b7sem", "Trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So k\u00fcsse mir vom Aug\u2019 die grauen Sorgen", "tokens": ["So", "k\u00fcs\u00b7se", "mir", "vom", "Aug'", "die", "grau\u00b7en", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kose mir die gl\u00fchenden Wangen,", "tokens": ["Und", "ko\u00b7se", "mir", "die", "gl\u00fc\u00b7hen\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und halte mich an deiner Brust geborgen", "tokens": ["Und", "hal\u00b7te", "mich", "an", "dei\u00b7ner", "Brust", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In wildem, brausendem Umfangen.", "tokens": ["In", "wil\u00b7dem", ",", "brau\u00b7sen\u00b7dem", "Um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus deiner Flammenschrift, da lass\u2019 mich lesen", "tokens": ["Aus", "dei\u00b7ner", "Flam\u00b7men\u00b7schrift", ",", "da", "lass'", "mich", "le\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das gro\u00dfe Weltgedicht von Uranfang gewesen,", "tokens": ["Das", "gro\u00b7\u00dfe", "Welt\u00b7ge\u00b7dicht", "von", "U\u00b7ran\u00b7fang", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die grimmen alten Lieder lass\u2019 im Donner hallen,", "tokens": ["Die", "grim\u00b7men", "al\u00b7ten", "Lie\u00b7der", "lass'", "im", "Don\u00b7ner", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die alle Sch\u00f6pfung singt und Menschenzungen lallen.", "tokens": ["Die", "al\u00b7le", "Sch\u00f6p\u00b7fung", "singt", "und", "Men\u00b7schen\u00b7zun\u00b7gen", "lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du der Locken wall\u2019nde Fluthen", "tokens": ["Wenn", "du", "der", "Lo\u00b7cken", "wall'n\u00b7de", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um\u2019s Haupt dir sch\u00fcttelst, hoch umwittert,", "tokens": ["Um's", "Haupt", "dir", "sch\u00fct\u00b7telst", ",", "hoch", "um\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Riesenfaust, in der die Blitze ruhten,", "tokens": ["Die", "Rie\u00b7sen\u00b7faust", ",", "in", "der", "die", "Blit\u00b7ze", "ruh\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ihr entgegentrat, voll Wuth zersplittert \u2014", "tokens": ["Was", "ihr", "ent\u00b7ge\u00b7gent\u00b7rat", ",", "voll", "Wuth", "zer\u00b7split\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann jauchzt dir zu mein hei\u00dfes, br\u00fcnstiges Beten;", "tokens": ["Dann", "jauchzt", "dir", "zu", "mein", "hei\u00b7\u00dfes", ",", "br\u00fcns\u00b7ti\u00b7ges", "Be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gieb mir von deiner Kraft, lass\u2019 mich zertreten,", "tokens": ["Gieb", "mir", "von", "dei\u00b7ner", "Kraft", ",", "lass'", "mich", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was meine Seele hemmt, die G\u00f6tzen mich zerschlagen!", "tokens": ["Was", "mei\u00b7ne", "See\u00b7le", "hemmt", ",", "die", "G\u00f6t\u00b7zen", "mich", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erh\u00f6r\u2019 mich, Geist des Sturms, bann\u2019 mir das kranke Zagen. \u2014", "tokens": ["Er\u00b7h\u00f6r'", "mich", ",", "Geist", "des", "Sturms", ",", "bann'", "mir", "das", "kran\u00b7ke", "Za\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der du mit schwarzem Fittig durch die N\u00e4chte", "tokens": ["Der", "du", "mit", "schwar\u00b7zem", "Fit\u00b7tig", "durch", "die", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahinjagst unter Donner und Zerst\u00f6rung,", "tokens": ["Da\u00b7hin\u00b7jagst", "un\u00b7ter", "Don\u00b7ner", "und", "Zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich, dessen einz\u2019ger Blick den Tod mir br\u00e4chte,", "tokens": ["Dich", ",", "des\u00b7sen", "einz'\u00b7ger", "Blick", "den", "Tod", "mir", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fleh\u2019 ich um gn\u00e4dige Erh\u00f6rung!", "tokens": ["Fleh'", "ich", "um", "gn\u00e4\u00b7di\u00b7ge", "Er\u00b7h\u00f6\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O gieb mir Frieden, s\u00fc\u00dfen Sturmesfrieden,", "tokens": ["O", "gieb", "mir", "Frie\u00b7den", ",", "s\u00fc\u00b7\u00dfen", "Stur\u00b7mes\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und banne mir des Herzens Eumeniden,", "tokens": ["Und", "ban\u00b7ne", "mir", "des", "Her\u00b7zens", "Eu\u00b7me\u00b7ni\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den wilden Schwarm der Furien, die noch keinen lie\u00dfen,", "tokens": ["Den", "wil\u00b7den", "Schwarm", "der", "Fu\u00b7ri\u00b7en", ",", "die", "noch", "kei\u00b7nen", "lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die Grauen vor mir selbst mir in die Seele gie\u00dfen.", "tokens": ["Die", "Grau\u00b7en", "vor", "mir", "selbst", "mir", "in", "die", "See\u00b7le", "gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "H\u00f6rst du des Herzens wildgepre\u00dftes Aechzen,", "tokens": ["H\u00f6rst", "du", "des", "Her\u00b7zens", "wild\u00b7ge\u00b7pre\u00df\u00b7tes", "A\u00b7ech\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie es erzuckt von alten Qualen?", "tokens": ["Wie", "es", "er\u00b7zuckt", "von", "al\u00b7ten", "Qua\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O lass\u2019 es l\u00e4nger nicht nach Liebe lechzen", "tokens": ["O", "lass'", "es", "l\u00e4n\u00b7ger", "nicht", "nach", "Lie\u00b7be", "lech\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bluten wie aus tausend Malen.", "tokens": ["Und", "blu\u00b7ten", "wie", "aus", "tau\u00b7send", "Ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nick\u2019 mir Gew\u00e4hrung mit den finstern Brauen,", "tokens": ["Nick'", "mir", "Ge\u00b7w\u00e4h\u00b7rung", "mit", "den", "fins\u00b7tern", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Lass\u2019 f\u00fchlen mich das wunderbare Grauen,", "tokens": ["Lass'", "f\u00fch\u00b7len", "mich", "das", "wun\u00b7der\u00b7ba\u00b7re", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Schmerzenslust am Sein bei deinem Wehen,", "tokens": ["Die", "Schmer\u00b7zens\u00b7lust", "am", "Sein", "bei", "dei\u00b7nem", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Seligkeit in deinem Athem zu vergehen.", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "in", "dei\u00b7nem", "A\u00b7them", "zu", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Sch\u00f6pfung hat erweckt aus ihren Tr\u00e4umen", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "hat", "er\u00b7weckt", "aus", "ih\u00b7ren", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein gro\u00dfer Ruf, dein schweres Grollen,", "tokens": ["Dein", "gro\u00b7\u00dfer", "Ruf", ",", "dein", "schwe\u00b7res", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl\u2019 sie sich in mir und um mich b\u00e4umen,", "tokens": ["Ich", "f\u00fchl'", "sie", "sich", "in", "mir", "und", "um", "mich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "PPER", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn deine Blitze zucken, Donner rollen.", "tokens": ["Wenn", "dei\u00b7ne", "Blit\u00b7ze", "zu\u00b7cken", ",", "Don\u00b7ner", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier stehe ich! Wie meine Haare fliegen", "tokens": ["Hier", "ste\u00b7he", "ich", "!", "Wie", "mei\u00b7ne", "Haa\u00b7re", "flie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWAV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und eng sich f\u00fcrchtend um die Schl\u00e4fe schmiegen!", "tokens": ["Und", "eng", "sich", "f\u00fcrch\u00b7tend", "um", "die", "Schl\u00e4\u00b7fe", "schmie\u00b7gen", "!"], "token_info":