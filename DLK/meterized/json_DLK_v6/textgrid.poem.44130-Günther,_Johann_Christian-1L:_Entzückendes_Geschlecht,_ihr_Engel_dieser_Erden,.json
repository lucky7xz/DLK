{"textgrid.poem.44130": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entz\u00fcckendes Geschlecht, ihr Engel dieser Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entz\u00fcckendes Geschlecht, ihr Engel dieser Erden,", "tokens": ["Ent\u00b7z\u00fc\u00b7cken\u00b7des", "Ge\u00b7schlecht", ",", "ihr", "En\u00b7gel", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die wir, was wir sind, ich meine M\u00e4nner, werden", "tokens": ["Durch", "die", "wir", ",", "was", "wir", "sind", ",", "ich", "mei\u00b7ne", "M\u00e4n\u00b7ner", ",", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "PPER", "PPOSAT", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und welchen die Natur, die nichts so k\u00fcnstlich baut,", "tokens": ["Und", "wel\u00b7chen", "die", "Na\u00b7tur", ",", "die", "nichts", "so", "k\u00fcnst\u00b7lich", "baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "$,", "PRELS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wafen wider uns durch Sch\u00f6nheit anvertraut,", "tokens": ["Die", "Wa\u00b7fen", "wi\u00b7der", "uns", "durch", "Sch\u00f6n\u00b7heit", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie, ist's euch nicht genug, da\u00df Regung, Mund und Mienen", "tokens": ["Wie", ",", "ist's", "euch", "nicht", "ge\u00b7nug", ",", "da\u00df", "Re\u00b7gung", ",", "Mund", "und", "Mie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Euch \u00fcber unser Herz zu Siegeskr\u00e4nzen dienen", "tokens": ["Euch", "\u00fc\u00b7ber", "un\u00b7ser", "Herz", "zu", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df ihr, wenn der Scherz aus Aug und Antliz brennt,", "tokens": ["Und", "da\u00df", "ihr", ",", "wenn", "der", "Scherz", "aus", "Aug", "und", "Ant\u00b7liz", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Weisen hintergehn und Helden werfen k\u00f6nt?", "tokens": ["Die", "Wei\u00b7sen", "hin\u00b7ter\u00b7gehn", "und", "Hel\u00b7den", "wer\u00b7fen", "k\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gedenckt ihr auch den Ruhm, worauf wir uns beflei\u00dfen,", "tokens": ["Ge\u00b7denckt", "ihr", "auch", "den", "Ruhm", ",", "wo\u00b7rauf", "wir", "uns", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch Wei\u00dfheit und Verstand noch vollends hinzurei\u00dfen?", "tokens": ["Durch", "Wei\u00df\u00b7heit", "und", "Ver\u00b7stand", "noch", "vol\u00b7lends", "hin\u00b7zu\u00b7rei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wollt ihr endlich gar mit Ehrsucht und Bem\u00fchn", "tokens": ["Und", "wollt", "ihr", "end\u00b7lich", "gar", "mit", "Ehr\u00b7sucht", "und", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Schmuck der Poesie uns von der Scheitel ziehn?", "tokens": ["Den", "Schmuck", "der", "Poe\u00b7sie", "uns", "von", "der", "Schei\u00b7tel", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ihr steigt ja sonst nicht gern, wenn M\u00e4nner folgen wollen,", "tokens": ["Ihr", "steigt", "ja", "sonst", "nicht", "gern", ",", "wenn", "M\u00e4n\u00b7ner", "fol\u00b7gen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "$,", "KOUS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Damit nur diese nicht ein Einsehn kriegen sollen;", "tokens": ["Da\u00b7mit", "nur", "die\u00b7se", "nicht", "ein", "Ein\u00b7sehn", "krie\u00b7gen", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PDS", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jezt aber klettert ihr am Helicon voran", "tokens": ["Jezt", "a\u00b7ber", "klet\u00b7tert", "ihr", "am", "He\u00b7li\u00b7con", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und lacht, wenn unser Fu\u00df nur hinckend folgen kan.", "tokens": ["Und", "lacht", ",", "wenn", "un\u00b7ser", "Fu\u00df", "nur", "hin\u00b7ckend", "fol\u00b7gen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Glaubt, fahrt ihr weiter fort, durch Wi\u00dfenschaft und Lehren", "tokens": ["Glaubt", ",", "fahrt", "ihr", "wei\u00b7ter", "fort", ",", "durch", "Wi\u00b7\u00dfen\u00b7schaft", "und", "Leh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die angebohrne List in eurer Brust zu mehren,", "tokens": ["Die", "an\u00b7ge\u00b7bohr\u00b7ne", "List", "in", "eu\u00b7rer", "Brust", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kehrt ihr ehstens gar im Musenheiligthum", "tokens": ["So", "kehrt", "ihr", "ehs\u00b7tens", "gar", "im", "Mu\u00b7sen\u00b7hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den eingef\u00fchrten Staat mitsamt der Herrschaft um.", "tokens": ["Den", "ein\u00b7ge\u00b7f\u00fchr\u00b7ten", "Staat", "mit\u00b7samt", "der", "Herr\u00b7schaft", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da wird der Phoebus euch nur Leyren puzen m\u00fc\u00dfen,", "tokens": ["Da", "wird", "der", "Phoe\u00b7bus", "euch", "nur", "Ley\u00b7ren", "pu\u00b7zen", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "PPER", "ADV", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da wird auch, wenn ihr nun das alte Recht zerri\u00dfen,", "tokens": ["Da", "wird", "auch", ",", "wenn", "ihr", "nun", "das", "al\u00b7te", "Recht", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Dichter m\u00e4nnlich Chor, geht's noch so bitter ein,", "tokens": ["Der", "Dich\u00b7ter", "m\u00e4nn\u00b7lich", "Chor", ",", "geht's", "noch", "so", "bit\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Euch blos um Pagenlohn zur Tafel dienstbahr seyn.", "tokens": ["Euch", "blos", "um", "Pa\u00b7gen\u00b7lohn", "zur", "Ta\u00b7fel", "dienst\u00b7bahr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So sprach ich, als ich las, mit was vor Geist und Leben", "tokens": ["So", "sprach", "ich", ",", "als", "ich", "las", ",", "mit", "was", "vor", "Geist", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die edle Brennerin den Versen Kraft gegeben;", "tokens": ["Die", "ed\u00b7le", "Bren\u00b7ne\u00b7rin", "den", "Ver\u00b7sen", "Kraft", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich sch\u00e4zte, lobt und pries das Ufer um den Belt,", "tokens": ["Ich", "sch\u00e4z\u00b7te", ",", "lobt", "und", "pries", "das", "U\u00b7fer", "um", "den", "Belt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der so ein seltnes Licht in Norden aufgestellt.", "tokens": ["Der", "so", "ein", "selt\u00b7nes", "Licht", "in", "Nor\u00b7den", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Apollo kam dazu und fing mich an zu schelten;", "tokens": ["A\u00b7pol\u00b7lo", "kam", "da\u00b7zu", "und", "fing", "mich", "an", "zu", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "KON", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So geht es, bracht er aus, was fremd ist, mu\u00df stets gelten.", "tokens": ["So", "geht", "es", ",", "bracht", "er", "aus", ",", "was", "fremd", "ist", ",", "mu\u00df", "stets", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du lobst dies Buch, und recht; doch hastu, wie ich seh,", "tokens": ["Du", "lobst", "dies", "Buch", ",", "und", "recht", ";", "doch", "has\u00b7tu", ",", "wie", "ich", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$,", "KON", "ADJD", "$.", "ADV", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dies, was du hier erhebst, viel be\u00dfer in der N\u00e4h:", "tokens": ["Dies", ",", "was", "du", "hier", "er\u00b7hebst", ",", "viel", "be\u00b7\u00dfer", "in", "der", "N\u00e4h", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Besinnstu dich denn nicht auf unsre Mariane?", "tokens": ["Be\u00b7sinns\u00b7tu", "dich", "denn", "nicht", "auf", "uns\u00b7re", "Ma\u00b7ri\u00b7a\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Tempel sehnt sich schon nach ihrer Ehrenfahne.", "tokens": ["Mein", "Tem\u00b7pel", "sehnt", "sich", "schon", "nach", "ih\u00b7rer", "Eh\u00b7ren\u00b7fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er schwieg; ich wurde roth, gerieth auf be\u00dfern Sinn", "tokens": ["Er", "schwieg", ";", "ich", "wur\u00b7de", "roth", ",", "ge\u00b7rieth", "auf", "be\u00b7\u00dfern", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und warf den netten Flei\u00df der klugen G\u00f6ttin hin.", "tokens": ["Und", "warf", "den", "net\u00b7ten", "Flei\u00df", "der", "klu\u00b7gen", "G\u00f6t\u00b7tin", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}