{"dta.poem.3082": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "127.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Flieh hier Leichtgl\u00e4ubigkeit, und dort die Zweifelsucht!", "tokens": ["Flieh", "hier", "Leicht\u00b7gl\u00e4u\u00b7big\u00b7keit", ",", "und", "dort", "die", "Zwei\u00b7fel\u00b7sucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch von der einen schlimm zur andern ist die Flucht.", "tokens": ["Doch", "von", "der", "ei\u00b7nen", "schlimm", "zur", "an\u00b7dern", "ist", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ART", "ADJD", "APPRART", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und doch, wer irgend naht der ersten oder letzten,", "tokens": ["Und", "doch", ",", "wer", "ir\u00b7gend", "naht", "der", "ers\u00b7ten", "o\u00b7der", "letz\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "ADV", "VVFIN", "ART", "ADJA", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sendet die zu der entgegen ihr gesetzten.", "tokens": ["Den", "sen\u00b7det", "die", "zu", "der", "ent\u00b7ge\u00b7gen", "ihr", "ge\u00b7setz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "APPR", "ART", "APPR", "PPER", "VVPP", "$."], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Kanst du den Mittelweg nicht treffen zwischen beiden,", "tokens": ["Kanst", "du", "den", "Mit\u00b7tel\u00b7weg", "nicht", "tref\u00b7fen", "zwi\u00b7schen", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So rath' ich diese mehr als jene dir zu meiden.", "tokens": ["So", "ra\u00b7th'", "ich", "die\u00b7se", "mehr", "als", "je\u00b7ne", "dir", "zu", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "PIAT", "KOKOM", "PDS", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Denn die Leichtgl\u00e4ubigkeit steht an des Glaubens Th\u00fcren,", "tokens": ["Denn", "die", "Leicht\u00b7gl\u00e4u\u00b7big\u00b7keit", "steht", "an", "des", "Glau\u00b7bens", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zweifel aber kann nur zur Verzweiflung f\u00fchren.", "tokens": ["Der", "Zwei\u00b7fel", "a\u00b7ber", "kann", "nur", "zur", "Ver\u00b7zwei\u00b7flung", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}