{"textgrid.poem.35167": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Sei weise!", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh nicht zu Denen, welche von sich reden;", "tokens": ["Geh", "nicht", "zu", "De\u00b7nen", ",", "wel\u00b7che", "von", "sich", "re\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKZU", "VVINF", "$,", "PRELS", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kennen nur das eigne, liebe Ich.", "tokens": ["Sie", "ken\u00b7nen", "nur", "das", "eig\u00b7ne", ",", "lie\u00b7be", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein feines Ohr vermeidet die Trompeten;", "tokens": ["Ein", "fei\u00b7nes", "Ohr", "ver\u00b7mei\u00b7det", "die", "Trom\u00b7pe\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Weise h\u00e4lt am liebsten sich f\u00fcr sich.", "tokens": ["Der", "Wei\u00b7se", "h\u00e4lt", "am", "liebs\u00b7ten", "sich", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "PRF", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geh nicht zu Denen, welche von sich schweigen;", "tokens": ["Geh", "nicht", "zu", "De\u00b7nen", ",", "wel\u00b7che", "von", "sich", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKZU", "VVINF", "$,", "PRELS", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch sie verehren nur ihr liebes Ich.", "tokens": ["Auch", "sie", "ver\u00b7eh\u00b7ren", "nur", "ihr", "lie\u00b7bes", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie wollen sich als gro\u00dfe Schweiger zeigen;", "tokens": ["Sie", "wol\u00b7len", "sich", "als", "gro\u00b7\u00dfe", "Schwei\u00b7ger", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Weise h\u00e4lt am liebsten sich f\u00fcr sich.", "tokens": ["Der", "Wei\u00b7se", "h\u00e4lt", "am", "liebs\u00b7ten", "sich", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "PRF", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und mu\u00dft du doch als Mensch zu Menschen gehen,", "tokens": ["Und", "mu\u00dft", "du", "doch", "als", "Mensch", "zu", "Men\u00b7schen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KOUS", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sprich und schweig, doch Beides nicht f\u00fcr dich.", "tokens": ["So", "sprich", "und", "schweig", ",", "doch", "Bei\u00b7des", "nicht", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "PIS", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Sprechen sei f\u00fcr die, die dich verstehen,", "tokens": ["Das", "Spre\u00b7chen", "sei", "f\u00fcr", "die", ",", "die", "dich", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Schweigen f\u00fcr der Andern liebes Ich.", "tokens": ["Das", "Schwei\u00b7gen", "f\u00fcr", "der", "An\u00b7dern", "lie\u00b7bes", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}