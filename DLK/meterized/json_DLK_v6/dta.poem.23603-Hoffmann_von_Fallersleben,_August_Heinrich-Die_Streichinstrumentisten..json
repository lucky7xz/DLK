{"dta.poem.23603": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Streichinstrumentisten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00f6chtet gerne streichen", "tokens": ["Ihr", "m\u00f6ch\u00b7tet", "ger\u00b7ne", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Geistes Freud' und Lust,", "tokens": ["Des", "Geis\u00b7tes", "Freud'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch k\u00f6nnt ihr niemals reichen", "tokens": ["Doch", "k\u00f6nnt", "ihr", "nie\u00b7mals", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In eine freie Brust;", "tokens": ["In", "ei\u00b7ne", "frei\u00b7e", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die wird euch nimmer fr\u00f6hnen", "tokens": ["Die", "wird", "euch", "nim\u00b7mer", "fr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie lumpiges Papier,", "tokens": ["Wie", "lum\u00b7pi\u00b7ges", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wird euch stets verh\u00f6hnen", "tokens": ["Die", "wird", "euch", "stets", "ver\u00b7h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In eurer Vampyrgier.", "tokens": ["In", "eu\u00b7rer", "Vam\u00b7pyr\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ihr den W\u00fctherichen", "tokens": ["Wenn", "ihr", "den", "W\u00fct\u00b7he\u00b7ri\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Gl\u00fcck und Ehren gleicht,", "tokens": ["An", "Gl\u00fcck", "und", "Eh\u00b7ren", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr werdet einst gestrichen,", "tokens": ["Ihr", "wer\u00b7det", "einst", "ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ihr die andern streicht.", "tokens": ["Wie", "ihr", "die", "an\u00b7dern", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum streichet nur die Geigen,", "tokens": ["Drum", "strei\u00b7chet", "nur", "die", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht ehrlich eure Hand!", "tokens": ["Macht", "ehr\u00b7lich", "eu\u00b7re", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spielt auf zum Geisterreigen", "tokens": ["Spielt", "auf", "zum", "Geis\u00b7ter\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr's deutsche Vaterland!", "tokens": ["F\u00fcr's", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}