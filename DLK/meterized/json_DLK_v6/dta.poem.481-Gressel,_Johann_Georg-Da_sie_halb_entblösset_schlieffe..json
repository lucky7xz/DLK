{"dta.poem.481": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie halb entbl\u00f6sset schlieffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wird Satyrus entz\u00fcckt von Amarillis Schein/", "tokens": ["Wird", "Sa\u00b7ty\u00b7rus", "ent\u00b7z\u00fcckt", "von", "A\u00b7ma\u00b7ril\u00b7lis", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "APPR", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er von ferne sieht die gelben Br\u00fcste spielen/", "tokens": ["Wenn", "er", "von", "fer\u00b7ne", "sieht", "die", "gel\u00b7ben", "Br\u00fcs\u00b7te", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie solte ich denn hart bey deiner Sch\u00f6nheit seyn?", "tokens": ["Wie", "sol\u00b7te", "ich", "denn", "hart", "bey", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich kan die Br\u00fcste sehn/ und mit den H\u00e4nden f\u00fchlen.", "tokens": ["Ich", "kan", "die", "Br\u00fcs\u00b7te", "sehn", "/", "und", "mit", "den", "H\u00e4n\u00b7den", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct