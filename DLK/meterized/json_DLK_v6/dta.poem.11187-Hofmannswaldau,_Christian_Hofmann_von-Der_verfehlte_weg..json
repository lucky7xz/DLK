{"dta.poem.11187": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verfehlte weg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der arme Titius ist gantz im kopff verwirrt,", "tokens": ["Der", "ar\u00b7me", "Ti\u00b7tius", "ist", "gantz", "im", "kopff", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil ihm Melisse hat ein k\u00f6rbgen angehangen.", "tokens": ["Weil", "ihm", "Me\u00b7lis\u00b7se", "hat", "ein", "k\u00f6rb\u00b7gen", "an\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat den weg verfehlt. Wie ist es zugegangen,", "tokens": ["Er", "hat", "den", "weg", "ver\u00b7fehlt", ".", "Wie", "ist", "es", "zu\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "VVPP", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er von Oschatz sich nach Eulenburg verirrt?", "tokens": ["Da\u00df", "er", "von", "O\u00b7schatz", "sich", "nach", "Eu\u00b7len\u00b7burg", "ver\u00b7irrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "PRF", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}