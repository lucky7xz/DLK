{"dta.poem.19502": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Seelchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag im Gras auf einer Alp,", "tokens": ["Ich", "lag", "im", "Gras", "auf", "ei\u00b7ner", "Alp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sel'ge Bl\u00e4uen starrt' ich auf \u2014", "tokens": ["In", "sel'\u00b7ge", "Bl\u00e4u\u00b7en", "starrt'", "ich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir war als ob auf meiner Brust", "tokens": ["Mir", "war", "als", "ob", "auf", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich etwas sacht betastete.", "tokens": ["Mich", "et\u00b7was", "sacht", "be\u00b7tas\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich blickte schr\u00e4g. Ein Falter sa\u00df", "tokens": ["Ich", "blick\u00b7te", "schr\u00e4g", ".", "Ein", "Fal\u00b7ter", "sa\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meinem grauen Wanderkleid.", "tokens": ["Auf", "mei\u00b7nem", "grau\u00b7en", "Wan\u00b7der\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Seelchen war's. So lernt' ich einst", "tokens": ["Mein", "Seel\u00b7chen", "wa\u00b7r'", "s.", "So", "lernt'", "ich", "einst"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVIMP", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In Rom an einem Basrelief.", "tokens": ["In", "Rom", "an", "ei\u00b7nem", "Bas\u00b7re\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie sieht es aus? Das w\u00fc\u00dft' ich gern,", "tokens": ["Wie", "sieht", "es", "aus", "?", "Das", "w\u00fc\u00dft'", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich blinzle mein Gewand entlang \u2014", "tokens": ["Ich", "blinz\u00b7le", "mein", "Ge\u00b7wand", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPO", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Blank war's, betupft mit Tropfen Bluts.", "tokens": ["Blank", "wa\u00b7r's", ",", "be\u00b7tupft", "mit", "Trop\u00b7fen", "Bluts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}