{"textgrid.poem.57330": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: B\u00fcrdet ihr nicht Satzungen auf dem geweihten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00fcrdet ihr nicht Satzungen auf dem geweihten", "tokens": ["B\u00fcr\u00b7det", "ihr", "nicht", "Sat\u00b7zun\u00b7gen", "auf", "dem", "ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dichter? erhebt zu Gesetz sie? und dem K\u00fcnstler", "tokens": ["Dich\u00b7ter", "?", "er\u00b7hebt", "zu", "Ge\u00b7setz", "sie", "?", "und", "dem", "K\u00fcnst\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "NN", "PPER", "$.", "KON", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ward doch selbst kein Gesetz gegeben,", "tokens": ["Ward", "doch", "selbst", "kein", "Ge\u00b7setz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's dem Gerechten nicht ward.", "tokens": ["Wie's", "dem", "Ge\u00b7rech\u00b7ten", "nicht", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Lernt: Die Natur schrieb in das Herz sein Gesetz ihm!", "tokens": ["Lernt", ":", "Die", "Na\u00b7tur", "schrieb", "in", "das", "Herz", "sein", "Ge\u00b7setz", "ihm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "PPER", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Thoren, er kent's, und sich selbst streng, ist er Th\u00e4ters;", "tokens": ["Tho\u00b7ren", ",", "er", "kent's", ",", "und", "sich", "selbst", "streng", ",", "ist", "er", "Th\u00e4\u00b7ters", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "KON", "PRF", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+--+---++-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "K\u00f6rnt zum Gipfel, wo ihr im Antritt,", "tokens": ["K\u00f6rnt", "zum", "Gip\u00b7fel", ",", "wo", "ihr", "im", "An\u00b7tritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gehet ihr einmal, schon sinkt.", "tokens": ["Ge\u00b7het", "ihr", "ein\u00b7mal", ",", "schon", "sinkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Regelt ihr gar lyrischen Flug: o so treft ihr", "tokens": ["Re\u00b7gelt", "ihr", "gar", "ly\u00b7ri\u00b7schen", "Flug", ":", "o", "so", "treft", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$.", "FM", "ADV", "VVFIN", "PPER"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "s' Aug' in den Stern dem Gesange der Alz\u00e4e,", "tokens": ["s'", "Aug'", "in", "den", "Stern", "dem", "Ge\u00b7san\u00b7ge", "der", "Al\u00b7z\u00e4e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Treft, je sch\u00f6ner es blicht, je st\u00e4rker", "tokens": ["Treft", ",", "je", "sch\u00f6\u00b7ner", "es", "blicht", ",", "je", "st\u00e4r\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihr's mit der passenden Faust.", "tokens": ["Ih\u00b7r's", "mit", "der", "pas\u00b7sen\u00b7den", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ist auch ein Lied, w\u00fcrdig Apolls, der Ach\u00e4er", "tokens": ["Ist", "auch", "ein", "Lied", ",", "w\u00fcr\u00b7dig", "A\u00b7polls", ",", "der", "A\u00b7ch\u00e4\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ADJD", "NE", "$,", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tr\u00fcmmern entflohn, der Quiriten, ein Melema,", "tokens": ["Tr\u00fcm\u00b7mern", "ent\u00b7flohn", ",", "der", "Qui\u00b7ri\u00b7ten", ",", "ein", "Me\u00b7le\u00b7ma", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Oder Eidos, nur eins der Ch\u00f6re", "tokens": ["O\u00b7der", "Ei\u00b7dos", ",", "nur", "eins", "der", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADV", "PIS", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sophokles, dem ihr nicht treft?", "tokens": ["So\u00b7phok\u00b7les", ",", "dem", "ihr", "nicht", "treft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}