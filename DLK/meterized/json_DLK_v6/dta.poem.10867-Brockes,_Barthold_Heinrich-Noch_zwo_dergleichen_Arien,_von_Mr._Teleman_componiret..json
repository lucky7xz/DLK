{"dta.poem.10867": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch zwo dergleichen  Arien,  \n von Mr. Teleman componiret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In den bunt bebl\u00fchmten Feldern,", "tokens": ["In", "den", "bunt", "be\u00b7bl\u00fchm\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den schattenreichen W\u00e4ldern,", "tokens": ["In", "den", "schat\u00b7ten\u00b7rei\u00b7chen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrscht, in stiller Einsamkeit,", "tokens": ["Herrscht", ",", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unschuld und Zufriedenheit.", "tokens": ["Un\u00b7schuld", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern vom st\u00e4dtischen Get\u00fcmmel,", "tokens": ["Fern", "vom", "st\u00e4d\u00b7ti\u00b7schen", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als in einem irdschen Himmel,", "tokens": ["Als", "in", "ei\u00b7nem", "ird\u00b7schen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Find ich hier die g\u00fcldne Zeit.", "tokens": ["Find", "ich", "hier", "die", "g\u00fcld\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}