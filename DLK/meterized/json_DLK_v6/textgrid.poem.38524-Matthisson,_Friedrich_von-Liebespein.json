{"textgrid.poem.38524": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Liebespein", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich die Langersehnte fand,", "tokens": ["Als", "ich", "die", "Lan\u00b7ger\u00b7sehn\u00b7te", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz sich an das ihre band,", "tokens": ["Mein", "Herz", "sich", "an", "das", "ih\u00b7re", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "PRELS", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, durch geheimen Zauberzug,", "tokens": ["Und", ",", "durch", "ge\u00b7hei\u00b7men", "Zau\u00b7ber\u00b7zug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Busen mir entgegenschlug:", "tokens": ["Ihr", "Bu\u00b7sen", "mir", "ent\u00b7ge\u00b7gen\u00b7schlug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da war ich froh in meinem Sinn!", "tokens": ["Da", "war", "ich", "froh", "in", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da tanzte Tag auf Tag mir hin,", "tokens": ["Da", "tanz\u00b7te", "Tag", "auf", "Tag", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie B\u00e4chlein hell im Sonnenschein,", "tokens": ["Wie", "B\u00e4ch\u00b7lein", "hell", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lauter und so silberrein!", "tokens": ["So", "lau\u00b7ter", "und", "so", "sil\u00b7ber\u00b7rein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da lachte Freud' und s\u00fcsse Ruh", "tokens": ["Da", "lach\u00b7te", "Freud'", "und", "s\u00fcs\u00b7se", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir stets ihr blaues Auge zu,", "tokens": ["Mir", "stets", "ihr", "blau\u00b7es", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ganze Welt vor mir vergieng,", "tokens": ["Die", "gan\u00b7ze", "Welt", "vor", "mir", "ver\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mich ihr Schwanenarm umfieng!", "tokens": ["Wenn", "mich", "ihr", "Schwa\u00b7nen\u00b7arm", "um\u00b7fi\u00b7eng", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Da war mir jede Stunde s\u00fc\u00df,", "tokens": ["Da", "war", "mir", "je\u00b7de", "Stun\u00b7de", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Lebenspfad ein Paradies,", "tokens": ["Mein", "Le\u00b7ben\u00b7spfad", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn alle Erdenseligkeit", "tokens": ["Denn", "al\u00b7le", "Er\u00b7den\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag, sonder Maas drauf ausgestreut.", "tokens": ["Lag", ",", "son\u00b7der", "Maas", "drauf", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich an ihrem Busen lag,", "tokens": ["Wenn", "ich", "an", "ih\u00b7rem", "Bu\u00b7sen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wiegt' ihres Herzens leiser Schlag", "tokens": ["Wiegt'", "ih\u00b7res", "Her\u00b7zens", "lei\u00b7ser", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich sanft zu Himmelstr\u00e4umen ein;", "tokens": ["Mich", "sanft", "zu", "Him\u00b7mel\u00b7str\u00e4u\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir schlug dieses Herz allein!", "tokens": ["Und", "mir", "schlug", "die\u00b7ses", "Herz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "ADV", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wenn uns im Laubdach, k\u00fchl und gr\u00fcn,", "tokens": ["Wenn", "uns", "im", "Laub\u00b7dach", ",", "k\u00fchl", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe, volle Mond beschien,", "tokens": ["Der", "lie\u00b7be", ",", "vol\u00b7le", "Mond", "be\u00b7schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sang Hain und Flur mir Sf\u00e4rensang,", "tokens": ["Sang", "Hain", "und", "Flur", "mir", "Sf\u00e4\u00b7ren\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jede Seelensait' erklang.", "tokens": ["Und", "je\u00b7de", "See\u00b7len\u00b7sait'", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bald wallten wir durch Blumenaun,", "tokens": ["Bald", "wall\u00b7ten", "wir", "durch", "Blu\u00b7me\u00b7naun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00fchlings Zauberpracht zu schaun;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Zau\u00b7ber\u00b7pracht", "zu", "schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch blikt' ich ihr ins Angesicht,", "tokens": ["Doch", "blikt'", "ich", "ihr", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sah' ich die Lenzgefilde nicht!", "tokens": ["Sah'", "ich", "die", "Lenz\u00b7ge\u00b7fil\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald ruhten wir auf Quellenmoos,", "tokens": ["Bald", "ruh\u00b7ten", "wir", "auf", "Quel\u00b7len\u00b7moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sanft der Abend niederflo\u00df,", "tokens": ["Wenn", "sanft", "der", "A\u00b7bend", "nie\u00b7der\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dr\u00fckte hei\u00df sich Mund an Mund,", "tokens": ["Da", "dr\u00fck\u00b7te", "hei\u00df", "sich", "Mund", "an", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu festen unsren Liebesbund!", "tokens": ["Zu", "fes\u00b7ten", "un\u00b7sren", "Lie\u00b7bes\u00b7bund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie Maienregen niederfleu\u00dft,", "tokens": ["Wie", "Mai\u00b7en\u00b7re\u00b7gen", "nie\u00b7der\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Bl\u00fctenb\u00e4ume sich ergeu\u00dft:", "tokens": ["Auf", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "sich", "er\u00b7geu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Flo\u00df jeder Flammenku\u00df von ihr", "tokens": ["Flo\u00df", "je\u00b7der", "Flam\u00b7men\u00b7ku\u00df", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erlabend in die Seele mir.", "tokens": ["Er\u00b7la\u00b7bend", "in", "die", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wir lebten Himmelswohnern gleich,", "tokens": ["Wir", "leb\u00b7ten", "Him\u00b7mels\u00b7woh\u00b7nern", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie an tausend Freuden reich,", "tokens": ["Wie", "sie", "an", "tau\u00b7send", "Freu\u00b7den", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wogt' und rauscht' ein Wonnemeer,", "tokens": ["Es", "wogt'", "und", "rauscht'", "ein", "Won\u00b7ne\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht abzusehn, rings um uns her!", "tokens": ["Nicht", "ab\u00b7zu\u00b7sehn", ",", "rings", "um", "uns", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIZU", "$,", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Genug der Freuden, o mein Lied,", "tokens": ["Ge\u00b7nug", "der", "Freu\u00b7den", ",", "o", "mein", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst mir Gl\u00fcklichem gebl\u00fcht!", "tokens": ["Die", "einst", "mir", "Gl\u00fck\u00b7li\u00b7chem", "ge\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinab! hinab! zum Trauerton,", "tokens": ["Hin\u00b7ab", "!", "hin\u00b7ab", "!", "zum", "Trau\u00b7er\u00b7ton", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freuden alle sind entflohn!", "tokens": ["Die", "Freu\u00b7den", "al\u00b7le", "sind", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie gab, in leichtem Flattersinn,", "tokens": ["Sie", "gab", ",", "in", "leich\u00b7tem", "Flat\u00b7ter\u00b7sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Herz an einen andern hin!", "tokens": ["Ihr", "Herz", "an", "ei\u00b7nen", "an\u00b7dern", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerri\u00df das goldne Himmelsband,", "tokens": ["Zer\u00b7ri\u00df", "das", "gold\u00b7ne", "Him\u00b7mels\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lieb' um unsre Seelen wand!", "tokens": ["Das", "Lieb'", "um", "uns\u00b7re", "See\u00b7len", "wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Das troknete, mit rascher Wuth,", "tokens": ["Das", "trok\u00b7ne\u00b7te", ",", "mit", "ra\u00b7scher", "Wuth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wilde Hundstagssonnenglut,", "tokens": ["Wie", "wil\u00b7de", "Hunds\u00b7tags\u00b7son\u00b7nen\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Quelle meiner Freuden leer,", "tokens": ["Die", "Quel\u00b7le", "mei\u00b7ner", "Freu\u00b7den", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Stund' an flo\u00df kein Tr\u00f6pfchen mehr!", "tokens": ["Von", "Stund'", "an", "flo\u00df", "kein", "Tr\u00f6pf\u00b7chen", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nun schleicht, bei wintertr\u00fcbem Sinn,", "tokens": ["Nun", "schleicht", ",", "bei", "win\u00b7ter\u00b7tr\u00fc\u00b7bem", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben langsamtraurig hin,", "tokens": ["Mein", "Le\u00b7ben", "langs\u00b7amt\u00b7rau\u00b7rig", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich irr' in d\u00fcstrer Mitternacht,", "tokens": ["Ich", "irr'", "in", "d\u00fcst\u00b7rer", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem Sternlein angelacht.", "tokens": ["Von", "kei\u00b7nem", "Stern\u00b7lein", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"tex