{"dta.poem.15844": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verschieden ist im Grund, und wie es ist so bleib' es,", "tokens": ["Ver\u00b7schie\u00b7den", "ist", "im", "Grund", ",", "und", "wie", "es", "ist", "so", "bleib'", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$,", "KON", "PWAV", "PPER", "VAFIN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden der Beruf des Mannes und des Weibes.", "tokens": ["Ver\u00b7schie\u00b7den", "der", "Be\u00b7ruf", "des", "Man\u00b7nes", "und", "des", "Wei\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was \u00e4u\u00dferlich der Mann, hat innerlich das Weib,", "tokens": ["Was", "\u00e4u\u00b7\u00dfer\u00b7lich", "der", "Mann", ",", "hat", "in\u00b7ner\u00b7lich", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "$,", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum zusammen erst sind sie ein ganzer Leib.", "tokens": ["Da\u00b7rum", "zu\u00b7sam\u00b7men", "erst", "sind", "sie", "ein", "gan\u00b7zer", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Geist des Mannes mag frei in die Welt sich regen,", "tokens": ["Der", "Geist", "des", "Man\u00b7nes", "mag", "frei", "in", "die", "Welt", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADJD", "APPR", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Weibes Seele soll den Haushalt still bewegen.", "tokens": ["Des", "Wei\u00b7bes", "See\u00b7le", "soll", "den", "Haus\u00b7halt", "still", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Haushalt ist die Welt, in die sie ist gestellt;", "tokens": ["Der", "Haus\u00b7halt", "ist", "die", "Welt", ",", "in", "die", "sie", "ist", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt bestellt sie, wenn den Haushalt sie bestellt.", "tokens": ["Die", "Welt", "be\u00b7stellt", "sie", ",", "wenn", "den", "Haus\u00b7halt", "sie", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und der es ist versagt, im Hause Haus zu halten,", "tokens": ["Und", "der", "es", "ist", "ver\u00b7sagt", ",", "im", "Hau\u00b7se", "Haus", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VAFIN", "VVPP", "$,", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als einen Haushalt soll sie ihr Gem\u00fct verwalten.", "tokens": ["Als", "ei\u00b7nen", "Haus\u00b7halt", "soll", "sie", "ihr", "Ge\u00b7m\u00fct", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sein Wissen mag der Mann an alle Welt verschwenden;", "tokens": ["Sein", "Wis\u00b7sen", "mag", "der", "Mann", "an", "al\u00b7le", "Welt", "ver\u00b7schwen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Weib soll, was sie wei\u00df, in ihr Gem\u00fct verwenden.", "tokens": ["Ein", "Weib", "soll", ",", "was", "sie", "wei\u00df", ",", "in", "ihr", "Ge\u00b7m\u00fct", "ver\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}