{"textgrid.poem.38680": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerungen", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Seegestad', in lauen Vollmondn\u00e4chten,", "tokens": ["Am", "See\u00b7ge\u00b7stad'", ",", "in", "lau\u00b7en", "Voll\u00b7mond\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denk' ich nur dich;", "tokens": ["Denk'", "ich", "nur", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu deines Namens goldnem Zug verflechten", "tokens": ["Zu", "dei\u00b7nes", "Na\u00b7mens", "gold\u00b7nem", "Zug", "ver\u00b7flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sterne sich.", "tokens": ["Die", "Ster\u00b7ne", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Wildni\u00df gl\u00e4nzt in ungewohnter Helle,", "tokens": ["Die", "Wild\u00b7ni\u00df", "gl\u00e4nzt", "in", "un\u00b7ge\u00b7wohn\u00b7ter", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dir erf\u00fcllt;", "tokens": ["Von", "dir", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf jedes Blatt, in jede Schattenquelle", "tokens": ["Auf", "je\u00b7des", "Blatt", ",", "in", "je\u00b7de", "Schat\u00b7ten\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Malt sich dein Bild.", "tokens": ["Malt", "sich", "dein", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Gern weil' ich, Grazie, wo du den H\u00fcgel", "tokens": ["Gern", "weil'", "ich", ",", "Gra\u00b7zie", ",", "wo", "du", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Hinabgeschwebt,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7schwebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Leicht wie ein Rosenblatt auf Zefyrs Fl\u00fcgel", "tokens": ["Leicht", "wie", "ein", "Ro\u00b7sen\u00b7blatt", "auf", "Ze\u00b7fyrs", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor\u00fcberbebt.", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7bebt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Am H\u00fcttchen dort bekr\u00e4nzt ich dir umflossen", "tokens": ["Am", "H\u00fctt\u00b7chen", "dort", "be\u00b7kr\u00e4nzt", "ich", "dir", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Abendglut,", "tokens": ["Von", "A\u00b7bend\u00b7glut", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Immergr\u00fcn und jungen Bl\u00fctensprossen", "tokens": ["Mit", "Im\u00b7mer\u00b7gr\u00fcn", "und", "jun\u00b7gen", "Bl\u00fc\u00b7tens\u00b7pros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Halmenhut.", "tokens": ["Den", "Hal\u00b7men\u00b7hut", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Bei jedem Lichtwurm, in den Felsenst\u00fccken,", "tokens": ["Bei", "je\u00b7dem", "Licht\u00b7wurm", ",", "in", "den", "Fel\u00b7sen\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob die Feen", "tokens": ["Als", "ob", "die", "Feen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da T\u00e4nze webten, riefst du voll Entz\u00fccken:", "tokens": ["Da", "T\u00e4n\u00b7ze", "web\u00b7ten", ",", "riefst", "du", "voll", "Ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sch\u00f6n! wie sch\u00f6n!", "tokens": ["Wie", "sch\u00f6n", "!", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wohin ich blick' und geh', erblick' ich immer", "tokens": ["Wo\u00b7hin", "ich", "blick'", "und", "geh'", ",", "er\u00b7blick'", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Den Wiesenplan,", "tokens": ["Den", "Wie\u00b7sen\u00b7plan", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo wir der Berge Schnee mit Purpurschimmer", "tokens": ["Wo", "wir", "der", "Ber\u00b7ge", "Schnee", "mit", "Pur\u00b7pur\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beleuchtet sahn.", "tokens": ["Be\u00b7leuch\u00b7tet", "sahn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ihr schmelzend Mailied weinte Filomele", "tokens": ["Ihr", "schmel\u00b7zend", "Mai\u00b7lied", "wein\u00b7te", "Fi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Uferhain;", "tokens": ["Im", "U\u00b7fer\u00b7hain", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da fleht' ich dir, im Blick die ganze Seele:", "tokens": ["Da", "fleht'", "ich", "dir", ",", "im", "Blick", "die", "gan\u00b7ze", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedenke mein!", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}