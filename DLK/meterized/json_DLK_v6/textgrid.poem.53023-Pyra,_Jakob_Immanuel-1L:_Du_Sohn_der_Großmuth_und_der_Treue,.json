{"textgrid.poem.53023": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Sohn der Gro\u00dfmuth und der Treue,", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Sohn der Gro\u00dfmuth und der Treue,", "tokens": ["Du", "Sohn", "der", "Gro\u00df\u00b7muth", "und", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Damon, meine Lust, und ewig meine Zier,", "tokens": ["O", "Da\u00b7mon", ",", "mei\u00b7ne", "Lust", ",", "und", "e\u00b7wig", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du w\u00fcrdiger Bewahrer meines Hertzens,", "tokens": ["Du", "w\u00fcr\u00b7di\u00b7ger", "Be\u00b7wah\u00b7rer", "mei\u00b7nes", "Hert\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du durch die Huld des Vaters aller Liebe", "tokens": ["Du", "durch", "die", "Huld", "des", "Va\u00b7ters", "al\u00b7ler", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr mich allein bestimmter Freund,", "tokens": ["F\u00fcr", "mich", "al\u00b7lein", "be\u00b7stimm\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh da das Bild des gantz entz\u00fcckten Geistes,", "tokens": ["Sieh", "da", "das", "Bild", "des", "gantz", "ent\u00b7z\u00fcck\u00b7ten", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durchschau das ofne Heiligste", "tokens": ["Durch\u00b7schau", "das", "of\u00b7ne", "Hei\u00b7ligs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Des dir gewiedmeten Gem\u00fctes.", "tokens": ["Des", "dir", "ge\u00b7wied\u00b7me\u00b7ten", "Ge\u00b7m\u00fc\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Bis in den stillen Grund der Seelen,", "tokens": ["Bis", "in", "den", "stil\u00b7len", "Grund", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom allerreinsten Licht erhabner Z\u00e4rtlichkeit", "tokens": ["Vom", "al\u00b7ler\u00b7reins\u00b7ten", "Licht", "er\u00b7hab\u00b7ner", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durchaus erhellt, verkl\u00e4ret und durchdrungen,", "tokens": ["Durc\u00b7haus", "er\u00b7hellt", ",", "ver\u00b7kl\u00e4\u00b7ret", "und", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcckst du mich, voll himmlischer Gedancken,", "tokens": ["Ent\u00b7z\u00fcckst", "du", "mich", ",", "voll", "himm\u00b7li\u00b7scher", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit dir von der unw\u00fcrdgen Welt,", "tokens": ["Mit", "dir", "von", "der", "un\u00b7w\u00fcrd\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Vom Schwarm des Staubs, in ewig heitre Sph\u00e4ren.", "tokens": ["Vom", "Schwarm", "des", "Staubs", ",", "in", "e\u00b7wig", "heit\u00b7re", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O g\u00f6ttlich sch\u00f6ne Einsamkeit!", "tokens": ["O", "g\u00f6tt\u00b7lich", "sch\u00f6\u00b7ne", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts ist um mich, als du und Doris.", "tokens": ["Nichts", "ist", "um", "mich", ",", "als", "du", "und", "Do\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "KON", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re dich, still, ruht ihr L\u00fcfte,", "tokens": ["Ich", "h\u00f6\u00b7re", "dich", ",", "still", ",", "ruht", "ihr", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Doris h\u00f6re drauf, du s\u00fcsse Freundlichkeit,", "tokens": ["O", "Do\u00b7ris", "h\u00f6\u00b7re", "drauf", ",", "du", "s\u00fcs\u00b7se", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Unschuld Bild, der Tugend reine Tochter,", "tokens": ["Der", "Un\u00b7schuld", "Bild", ",", "der", "Tu\u00b7gend", "rei\u00b7ne", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Damon singt von dein und meiner Liebe;", "tokens": ["Mein", "Da\u00b7mon", "singt", "von", "dein", "und", "mei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der gantze Himmel wird verkl\u00e4rt;", "tokens": ["Der", "gant\u00b7ze", "Him\u00b7mel", "wird", "ver\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Hertz, beklemmt von innigstem Vergn\u00fcgen,", "tokens": ["Mein", "Hertz", ",", "be\u00b7klemmt", "von", "in\u00b7nigs\u00b7tem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sch\u00f6pft Luft, bey deiner keuschen Lust,", "tokens": ["Sch\u00f6pft", "Luft", ",", "bey", "dei\u00b7ner", "keu\u00b7schen", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erleichtert sich, durch fremde Z\u00e4hren.", "tokens": ["Er\u00b7leich\u00b7tert", "sich", ",", "durch", "frem\u00b7de", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Freund, wer giebt dich meinem Arm?", "tokens": ["O", "Freund", ",", "wer", "giebt", "dich", "mei\u00b7nem", "Arm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was h\u00e4lt, was h\u00e4lt dich auf? was, bist du noch nicht da?", "tokens": ["Was", "h\u00e4lt", ",", "was", "h\u00e4lt", "dich", "auf", "?", "was", ",", "bist", "du", "noch", "nicht", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Zeit! warum verweigerst du so lange", "tokens": ["O", "Zeit", "!", "wa\u00b7rum", "ver\u00b7wei\u00b7gerst", "du", "so", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Brust den Trost, dem Wunsche die Vergn\u00fcgung?", "tokens": ["Der", "Brust", "den", "Trost", ",", "dem", "Wun\u00b7sche", "die", "Ver\u00b7gn\u00fc\u00b7gung", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Auge weicht nicht von der H\u00f6h,", "tokens": ["Mein", "Au\u00b7ge", "weicht", "nicht", "von", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wovon der krumme Weg sich zu uns niederdrehet.", "tokens": ["Wo\u00b7von", "der", "krum\u00b7me", "Weg", "sich", "zu", "uns", "nie\u00b7der\u00b7dre\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun, nunmehr kommen sie hervor.", "tokens": ["Nun", ",", "nun\u00b7mehr", "kom\u00b7men", "sie", "her\u00b7vor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! nicht mein Damon, meine Doris.", "tokens": ["Ach", "!", "nicht", "mein", "Da\u00b7mon", ",", "mei\u00b7ne", "Do\u00b7ris", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Kein Gang in noch entlaubten Schatten", "tokens": ["Kein", "Gang", "in", "noch", "ent\u00b7laub\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des rieselnden und schmahlen Schmerlenbachs", "tokens": ["Des", "rie\u00b7seln\u00b7den", "und", "schmah\u00b7len", "Schmer\u00b7len\u00b7bachs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kan meinen Fu\u00df in die begr\u00fcnten Th\u00e4ler", "tokens": ["Kan", "mei\u00b7nen", "Fu\u00df", "in", "die", "be\u00b7gr\u00fcn\u00b7ten", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu sich herab von eurem Wege locken,", "tokens": ["Zu", "sich", "her\u00b7ab", "von", "eu\u00b7rem", "We\u00b7ge", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der steil und voller Sand und Kies", "tokens": ["Der", "steil", "und", "vol\u00b7ler", "Sand", "und", "Kies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr meine Schenckel jetzt allein ein Lustgang bleibet,", "tokens": ["F\u00fcr", "mei\u00b7ne", "Schen\u00b7ckel", "jetzt", "al\u00b7lein", "ein", "Lust\u00b7gang", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo selbst mein niemals m\u00fcder Schritt", "tokens": ["Wo", "selbst", "mein", "nie\u00b7mals", "m\u00fc\u00b7der", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch einen neuen Fu\u00dfsteg zeichnet.", "tokens": ["Noch", "ei\u00b7nen", "neu\u00b7en", "Fu\u00df\u00b7steg", "zeich\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann steh ich einsam auf der H\u00f6he", "tokens": ["Dann", "steh", "ich", "ein\u00b7sam", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey gantzen Stunden still, voll sehnlicher Begier;", "tokens": ["Bey", "gant\u00b7zen", "Stun\u00b7den", "still", ",", "voll", "sehn\u00b7li\u00b7cher", "Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Wind pfeift mir durch die zerst\u00f6hrten Haare,", "tokens": ["Der", "Wind", "pfeift", "mir", "durch", "die", "zer\u00b7st\u00f6hr\u00b7ten", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch irrt mein Blick durch alle Weg und Felder", "tokens": ["Doch", "irrt", "mein", "Blick", "durch", "al\u00b7le", "Weg", "und", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcber Th\u00fcrm und Berge hin.", "tokens": ["Und", "\u00fc\u00b7ber", "Th\u00fcrm", "und", "Ber\u00b7ge", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Oft waffn ich auch die allzubl\u00f6den Augen;", "tokens": ["Oft", "waffn", "ich", "auch", "die", "all\u00b7zu\u00b7bl\u00f6\u00b7den", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch Damon, Doris, kommen nicht;", "tokens": ["Doch", "Da\u00b7mon", ",", "Do\u00b7ris", ",", "kom\u00b7men", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NE", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Abends kehr ich traurig wieder.", "tokens": ["Und", "A\u00b7bends", "kehr", "ich", "trau\u00b7rig", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die unverz\u00e4rtelt muntre 