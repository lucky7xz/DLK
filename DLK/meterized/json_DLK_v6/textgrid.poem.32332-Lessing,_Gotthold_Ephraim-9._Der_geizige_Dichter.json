{"textgrid.poem.32332": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "9. Der geizige Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, warum Semir ein reicher Geizhals ist?", "tokens": ["Du", "fragst", ",", "wa\u00b7rum", "Se\u00b7mir", "ein", "rei\u00b7cher", "Geiz\u00b7hals", "ist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Semir, der Dichter? er, den Welt und Nachwelt liest?", "tokens": ["Se\u00b7mir", ",", "der", "Dich\u00b7ter", "?", "er", ",", "den", "Welt", "und", "Nach\u00b7welt", "liest", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$.", "PPER", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Weil, nach des Schicksals ew'gem Schlu\u00df,", "tokens": ["Weil", ",", "nach", "des", "Schick\u00b7sals", "ew'\u00b7gem", "Schlu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Dichter darben mu\u00df.", "tokens": ["Ein", "je\u00b7der", "Dich\u00b7ter", "dar\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PAV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}