{"textgrid.poem.26036": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Schmetterlinge ziehen durch den Garten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schmetterlinge ziehen durch den Garten", "tokens": ["Die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "zie\u00b7hen", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Blumen, die von ihren Stengeln fliehen,", "tokens": ["Wie", "Blu\u00b7men", ",", "die", "von", "ih\u00b7ren", "Sten\u00b7geln", "flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Rosen, wie mein Herz erhitzt und schwer,", "tokens": ["Und", "Ro\u00b7sen", ",", "wie", "mein", "Herz", "er\u00b7hitzt", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOKOM", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gaben im Duft die volle Seele her.", "tokens": ["Ga\u00b7ben", "im", "Duft", "die", "vol\u00b7le", "See\u00b7le", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sie locken s\u00fc\u00df an allen hei\u00dfen Wegen,", "tokens": ["Sie", "lo\u00b7cken", "s\u00fc\u00df", "an", "al\u00b7len", "hei\u00b7\u00dfen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Sonne aber tr\u00e4gt mein Feuer dir entgegen.", "tokens": ["Die", "Son\u00b7ne", "a\u00b7ber", "tr\u00e4gt", "mein", "Feu\u00b7er", "dir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}