{"textgrid.poem.28618": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der mir funfftzig G\u00fclden soll, waget zwantzig G\u00fclden hin,", "tokens": ["Der", "mir", "funfft\u00b7zig", "G\u00fcl\u00b7den", "soll", ",", "wa\u00b7get", "zwant\u00b7zig", "G\u00fcl\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "CARD", "NN", "VMFIN", "$,", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df er meine Zahlung nur m\u00f6chte l\u00e4nger noch verziehn.", "tokens": ["Da\u00df", "er", "mei\u00b7ne", "Zah\u00b7lung", "nur", "m\u00f6ch\u00b7te", "l\u00e4n\u00b7ger", "noch", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VMFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Schaut! wie ist der gute Mann abgerichtet auff Gewin!", "tokens": ["Schaut", "!", "wie", "ist", "der", "gu\u00b7te", "Mann", "ab\u00b7ge\u00b7rich\u00b7tet", "auff", "Ge\u00b7win", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}}}}}