{"textgrid.poem.50977": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sooft die Sonne aufersteht,", "tokens": ["Sooft", "die", "Son\u00b7ne", "auf\u00b7er\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erneuet sich mein Hoffen", "tokens": ["Er\u00b7neu\u00b7et", "sich", "mein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bleibet, bis sie untergeht,", "tokens": ["Und", "blei\u00b7bet", ",", "bis", "sie", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Blume offen;", "tokens": ["Wie", "ei\u00b7ne", "Blu\u00b7me", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann schlummert es ermattet", "tokens": ["Dann", "schlum\u00b7mert", "es", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im dunklen Schatten ein,", "tokens": ["Im", "dunk\u00b7len", "Schat\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch eilig wacht es wieder auf", "tokens": ["Doch", "ei\u00b7lig", "wacht", "es", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrem ersten Schein.", "tokens": ["Mit", "ih\u00b7rem", "ers\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist die Kraft, die nimmer stirbt", "tokens": ["Das", "ist", "die", "Kraft", ",", "die", "nim\u00b7mer", "stirbt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer wieder streitet,", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das gute Blut, das nie verdirbt,", "tokens": ["Das", "gu\u00b7te", "Blut", ",", "das", "nie", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geheimnisvoll verbreitet!", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "ver\u00b7brei\u00b7tet", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Solang noch Morgenwinde", "tokens": ["So\u00b7lang", "noch", "Mor\u00b7gen\u00b7win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Voran der Sonne wehn,", "tokens": ["Vo\u00b7ran", "der", "Son\u00b7ne", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird nie der Freiheit Fechterschar", "tokens": ["Wird", "nie", "der", "Frei\u00b7heit", "Fech\u00b7ter\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Nacht und Schlaf vergehn!", "tokens": ["In", "Nacht", "und", "Schlaf", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}