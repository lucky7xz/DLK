{"dta.poem.15976": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sinne l\u00fcgen nicht, schwach aber sind die Sinne;", "tokens": ["Die", "Sin\u00b7ne", "l\u00fc\u00b7gen", "nicht", ",", "schwach", "a\u00b7ber", "sind", "die", "Sin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir werden nicht durch sie des Dinges Innres inne.", "tokens": ["Wir", "wer\u00b7den", "nicht", "durch", "sie", "des", "Din\u00b7ges", "Inn\u00b7res", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir sehn vom Aeu\u00dfern auch die eine nur der Seiten,", "tokens": ["Wir", "sehn", "vom", "A\u00b7e\u00b7u\u00b7\u00dfern", "auch", "die", "ei\u00b7ne", "nur", "der", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "ART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und die undeutlich selbst, wenn wir sie sehn vom weiten.", "tokens": ["Und", "die", "un\u00b7deut\u00b7lich", "selbst", ",", "wenn", "wir", "sie", "sehn", "vom", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei weitem sehen wir dem Ding nicht alles an,", "tokens": ["Bei", "wei\u00b7tem", "se\u00b7hen", "wir", "dem", "Ding", "nicht", "al\u00b7les", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch alles, was daran wir sehn, ist wirklich dran.", "tokens": ["Doch", "al\u00b7les", ",", "was", "da\u00b7ran", "wir", "sehn", ",", "ist", "wirk\u00b7lich", "dran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PAV", "PPER", "VVINF", "$,", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}