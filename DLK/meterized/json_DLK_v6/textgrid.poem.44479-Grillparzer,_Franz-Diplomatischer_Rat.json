{"textgrid.poem.44479": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Diplomatischer Rat", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Marder fra\u00df die H\u00fchner gern,", "tokens": ["Ein", "Mar\u00b7der", "fra\u00df", "die", "H\u00fch\u00b7ner", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wu\u00dft er nicht, wie sie erhaschen;", "tokens": ["Doch", "wu\u00dft", "er", "nicht", ",", "wie", "sie", "er\u00b7ha\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er fragt den Fuchs, 'nen alten Herrn,", "tokens": ["Er", "fragt", "den", "Fuchs", ",", "'nen", "al\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Steifheit schon verbot das Naschen.", "tokens": ["Dem", "Steif\u00b7heit", "schon", "ver\u00b7bot", "das", "Na\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sagt ihm: \u00bbFreund, der Rat ist alt,", "tokens": ["Der", "sagt", "ihm", ":", "\u00bb", "Freund", ",", "der", "Rat", "ist", "alt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was hilft zu z\u00f6gern, brauch Gewalt!\u00ab", "tokens": ["Was", "hilft", "zu", "z\u00f6\u00b7gern", ",", "brauch", "Ge\u00b7walt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PTKZU", "VVINF", "$,", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Marder st\u00fcrmt in vollem Lauf,", "tokens": ["Der", "Mar\u00b7der", "st\u00fcrmt", "in", "vol\u00b7lem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die H\u00fchner aber flattern auf,", "tokens": ["Die", "H\u00fch\u00b7ner", "a\u00b7ber", "flat\u00b7tern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die eine gackernd, kreischend jene,", "tokens": ["Die", "ei\u00b7ne", "ga\u00b7ckernd", ",", "krei\u00b7schend", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "VVPP", "$,", "ADJD", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gerade in des Fuchses Z\u00e4hne,", "tokens": ["Ge\u00b7ra\u00b7de", "in", "des", "Fuch\u00b7ses", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der gegen\u00fcber lauernd lag", "tokens": ["Der", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "lau\u00b7ernd", "lag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und m\u00fchlos hielt den Erntetag.", "tokens": ["Und", "m\u00fch\u00b7los", "hielt", "den", "Ern\u00b7te\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du nach H\u00fchnern l\u00fcstern bist,", "tokens": ["Wenn", "du", "nach", "H\u00fch\u00b7nern", "l\u00fcs\u00b7tern", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frag keinen, der sie selbst gern fri\u00dft.", "tokens": ["Frag", "kei\u00b7nen", ",", "der", "sie", "selbst", "gern", "fri\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}