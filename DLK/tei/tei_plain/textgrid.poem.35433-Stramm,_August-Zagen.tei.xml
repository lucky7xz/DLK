        <lg type="poem">
          <head>Zagen</head>
          <lg rhyme="unknown" type="stanza">
            <l>Die Himmel hangen</l>
            <l>Schatten haschen Wolken</l>
            <l>Ängste</l>
            <l>Hüpfen</l>
            <l>Ducken</l>
            <l>Recken</l>
            <l>Schaufeln schaufeln</l>
            <l>Müde</l>
            <l>Stumpf</l>
            <l>Versträubt</l>
            <l>Die</l>
            <l>Gehre</l>
            <l>Gruft.</l>
          </lg>
