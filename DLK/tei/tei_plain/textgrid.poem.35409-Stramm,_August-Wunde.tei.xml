  <text>
    <body>
      <div n="1">
        <lg type="poem">
          <head>Wunde</head>
          <lg rhyme="unknown" type="stanza">
            <l>Die Erde blutet unterm Helmkopf</l>
            <l>Sterne fallen</l>
            <l>Der Weltraum tastet.</l>
            <l>Schauder brausen</l>
            <l>Wirbeln</l>
            <l>Einsamkeiten.</l>
            <l>Nebel</l>
            <l>Weinen</l>
            <l>Ferne</l>
            <l>Deinen Blick.</l>
          </lg>
