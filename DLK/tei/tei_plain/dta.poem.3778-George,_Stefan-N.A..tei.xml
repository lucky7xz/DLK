  <text>
    <body>
      <div n="1">
        <lg type="poem">
          <head>N.A.</head>
          <lg rhyme="unknown" type="stanza">
            <l>Lose geschmeide ·</l>
            <l>Frauen dirnen</l>
            <l>Schlanke schenken</l>
            <l>Müde sich senken</l>
            <l>Ledig die lende</l>
            <l>Busen und hüfte</l>
            <l>Um die stirnen</l>
            <l>Der kränze rest</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Schläfernder broden</l>
            <l>Traufender düfte</l>
            <l>Weinkönig scheide!</l>
            <l>Aller ende</l>
            <l>Endet das fest</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Rosen regnen</l>
            <l>Purpurne satte</l>
            <l>Die liebkosen?</l>
            <l>Weisse matte</l>
            <l>Euch zu laben?</l>
            <l>Malvenrote</l>
            <l>Gelbe tote:</l>
            <l>Manen-küsse</l>
            <l>Euch zu segnen</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Auf die schleusen!</l>
            <l>Und aus reusen</l>
            <l>Regnen rosen</l>
            <l>Güsse flüsse</l>
            <l>Die begraben</l>
          </lg>
        </lg>
      </div>
    </body>
