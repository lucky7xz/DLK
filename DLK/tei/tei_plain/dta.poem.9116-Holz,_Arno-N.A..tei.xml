  <text>
    <body>
      <div n="1">
        <lg type="poem">
          <head>N.A.</head>
          <lg rhyme="unknown" type="stanza">
            <l>Draussen die Düne.</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Einsam das Haus,</l>
            <l>eintönig,</l>
            <l>ans Fenster,</l>
            <l>der Regen.</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Hinter mir,</l>
            <l>tictac,</l>
            <l>eine Uhr,</l>
            <l>meine Stirn</l>
            <l>gegen die Scheibe.</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Nichts.</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Alles vorbei.</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Grau der Himmel,</l>
            <l>grau die See</l>
            <l>und grau</l>
            <l>das Herz.</l>
          </lg>
        </lg>
      </div>
    </body>
